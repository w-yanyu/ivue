{
  "module": "v-realonlyForm-btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "controls": [
        {

          "flowCode": {
            "label": "规则流代码",
            "control": "text",
            "span": 8,
            "disabled": true
          },
          "flowName": {
            "label": "规则流名称",
            "control": "text",
            "span": 8,
            "disabled": true
          }
        },
        {
          "flowDescription": {
            "label": "规则流描述",
            "control": "textarea",
            "span": 24,
            "rows": 5,
            "maxlength": 500,
            "showWordLimit": 500,
            "disabled": true
          }
        }
      ]
    }
  }
}
