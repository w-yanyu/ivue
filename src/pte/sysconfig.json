{
  "initLanguage": "zh",
  "#### noLogin": "是否需要登录页面： false表示需要登录，true表示没有登录",
  "noLogin": false,
  "version": "1.0.0",
  "productCode": "PTE",
  "#### isProd": "生产模式开关： false开发模式，true生产模式。 开发模式将打印调试日志等",
  "isProd": false,
  "#### isShell": "是否接入Loader页面加载器： false正常登录不接入，true接入Loader",
  "isShell": false,
  "tagsMaxNumber": 20,
  "http": {
    "#### timeout": "HTTP请求响应最大时间（Axios配置）",
    "timeout": 60000,
    "successCode": 200
  },
  "dialogDrag": true,
  "extendFile": {
    "components": "pteregister.js",
    "#components": "自定义组件注册位置配置，默认存在于pte文件夹下",
    "httpintercept": "httpintercept.js",
    "#httpintercept": "自定义axios拦截器文件路径",
    "button": "ptebuttonregister.js",
    "control": "ptecontrolregister.js",
    "ptealias": "ptealias.json",
    "#ptealias": "组件别名路径，2.0已废弃",
    "i18ns": "i18ns",
    "#i18ns": "国际化语言包路径，默认public下",
    "store": "ptestoreregister.js",
    "##token": "tokenExtend.js",
    "getters": "ptegettersregister.js"
  },
  "#### router": "框架内置路由页面配置",
  "router": {
    "#### public": "公共内置跳转的路由（页面）地址",
    "whiteList": ["commonlogin", "/commonlogin"],
    "public": {
      "####login": "/SUMP/screen/mobileCall",
      "login": "/SUMP/screen/mobileCall",
      "layout": "/pte/json/layout/index2.json",
      "page401": "@page401",
      "page404": "@page404",
      "commonlogin": "@commonLogin"
    },
    "#### routerApi": "获取路由数据接口配置",
    "#### routerApi backend": "后台测试地址：/BAP/menu/getRouterList",
    "routerApi": {
      "doRequest": {
        "url": "/SUMP/rest/auth/menu",
        "#url": "/pte/json/layout/pl_menu.json",
        "method": "post",
        "resultSet": {
          "idField": "pageCode",
          "pIdField": "parentPageCode",
          "isLeafField": "leaf",
          "titleField": "title",
          "targetField": "menu_code",
          "externalField": "external",
          "toUrlField": "toUrl",
          "searchField": "menu_code",
          "#titleAliasField": "[id]title",
          "iconField": "icon"
        }
      }
    }
  },
  "### autoLock": "是否开启自动锁屏",
  "# autoLock": {
    "### timeout": "自动锁屏弹出期限（单位 分钟）",
    "timeOut": 1
  },
  "#### i18n": "国际化语言配置",
  "i18n": {
    "#### languagePackage": "获取国际化语言包后台接口地址。不配置以前端“/public/i18ns”目录下定义为准。",
    "languagePackage": ""
  },
  "#### login": "",
  "tokenAuth": {
    "#### updateApi": "更新token后台接口",
    "#### updateApi loader": "接入loader时认证地址：/loader/api/uiAuth/refresh-token",
    "#### updateApi backend": "后台测试接口/BAP/authentication/updateToken",
    "updateApi": "/SUMP/rest/auth/updateToken",
    "#######tokenInvalidIntervalTime": "剩余时间更新token/min",
    "tokenInvalidIntervalTime": 30,
    "############afterSessionInvalid": "过期时返回登录页面调用退出接口",
    "afterSessionInvalid": "/SUMP/rest/auth/logout",
    "### tokenDialog": "token弹出框模板",
    "# tokenDialog": {
      "toRequest": {
        "url": "@autoLock",
        "method": "get"
      },
      "title": "登录提示",
      "fullscreen": false,
      "wigth": "40%"
    }
  },
  "authority": {
    "doRequest": {
      "url": "/SUMP/rest/auth/authority",
      "method": "post",
      "params": {},
      "resultSet": {
        "authIdField": "resource_value",
        "authField": "control_auth_type"
      }
    }
  },
  "### systemDateFormat": "系统默认日期格式化配置",
  "systemDateFormat": {
    "### valueFormat": "提交日期的格式",
    "valueFormat": "yyyyMMdd",
    "### format": "显示的日期格式",
    "format": "yyyy-MM-dd"
  },
  "template": {
    "jsonDataApi": "/PTE/template/getTemplateData"
  },
  "#### pagination": "全局分页信息相关配置",
  "pagination": {
    "pageSizes": [10, 15, 20],
    "pageSize": 10,
    "show": true,
    "pageSizeField": "length",
    "currentPageField": "start",
    "hideOnSinglePage": false
  },
  "#### dict": "数据字典接口地址及参数配置",
  "dict": {
    "labelField": "dictName",
    "valueField": "dictId",
    "doRequest": {
      "url": "/SUMP/rest/dict",
      "method": "post"
    }
  },
  "#### message": "message弹出框配置",
  "#### showClose": "是否显示关闭按钮",
  "#### duration": "显示时间, 毫秒。设为 0 则不会自动关闭",
  "#### type": "对应Element-ui弹框类型",
  "message": [
    {
      "type": "success",
      "showClose": true,
      "duration": "5000"
    },
    {
      "type": "warning",
      "showClose": true,
      "duration": "5000"
    },
    {
      "type": "info",
      "showClose": true,
      "duration": "5000"
    },
    {
      "type": "error",
      "showClose": true,
      "duration": "15000"
    }
  ],
  "#### themes": "自定义肤色类型",
  "#### theme": "主题颜色",
  "#### value": "肤色的别名， 与布局中的肤色value对应",
  "#### 系统提供四种主题色配置": [
    {
      "theme": "368ee0",
      "alias": "default"
    },
    {
      "theme": "1458bd",
      "alias": "darkBlue"
    },
    {
      "theme": "081f59",
      "alias": "blue"
    },
    {
      "theme": "404359",
      "alias": "purple"
    }
  ],
  "themes": [
    {
      "theme": "368ee0",
      "alias": "default"
    },
    {
      "theme": "1458bd",
      "alias": "darkBlue"
    },
    {
      "theme": "081f59",
      "alias": "blue"
    },
    {
      "theme": "404359",
      "alias": "purple"
    }
  ],
  "###pswEncryption": "SHA",
  "pswEncryption": "SHA"
}
