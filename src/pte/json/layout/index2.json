{
  "module": "v-leftMenu-dynamic",
  "layout": {
    "appHeader": {
      "fullscreen": true,
      "logo": {
        "imgPath": "/logo/sunline_logo.png",
        "iconPath": "/logo/sunline_logo_mini.png"
      },

      "notice": {
        "enable": false,
        "icon": "icon-xiaoxi",
        "type": "danger",
        "max": 100,
        "rollPoling": 60,
        "value": 10,
        "toRequest": {
          "url": "/views/msap/aggs/aggs_wait_todo_view.json",
          "method": "POST"
        },

        
        "props": {
          "width": "70%",
          "title": "待办事项",
          "close": ""
        }
      },
      "themes": {
        "enable": true,
        "icon": "icon iconfont icon-huanfu",
        "options": [
          {
            "value": "default"
          },
          {
            "value": "darkBlue"
          },
          {
            "value": "blue"
          },
          {
            "value": "purple"
          }
        ],
        "doRequest": {
          "url": "/SUMP/icmcall/icmRPCCall",
          "method": "post",
          "params": {
            "servicecode": "ct2005"
          }
        }
      },
      "language": {
        "enable": true,
        "options": [
          {
            "key": "zh",
            "title": "simplifiedChinese",
            "icon": "flag-zh"
          },
          {
            "key": "en",
            "title": "english",
            "icon": "flag-en"
          }
        ],
        "doRequest": {
          "method": "post",
          "url": "/SUMP/language/change",
          "#url": "/pte/json/layout/language.json"
        }
      },
      "globalForm": {
        "role_id": {
          "control": "dropdownRole",
          "span": "1"
        }
      },
      "userInfo": {
        "type": "logout",
        "icon": "iconfont icon-tuichu",
        "label": " ",
        "tips": "退出登录",
        "doRequest": {
          "method": "post",
          "#url": "/pte/json/layout/logout.json",
          "url": "/SUMP/rest/auth/logout"
        }
        }
    },
    "appSidebar": {
      "search": {
        "enable": true
      }
    },
    "appMain": {},
    "appFooter": {
      "copyright": {
        "info": {
          "template": "$g{{user_info.userCode}} | $g{{user_info.userName}} | $g{{user_info.current_role_id}} - $g{{user_info.current_role_name}} | $g{{user_info.branch_id}} | $g{{user_info.trxn_date}}"
        }
      }
    }
  }
}