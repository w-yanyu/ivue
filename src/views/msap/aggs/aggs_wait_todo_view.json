{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "foldLineNumber": 3,
      "controls": [
        {
          "busi_type": {
            "control": "select",
            "label": "业务类型",
            "dict": {
              "dictType": "AGGS_BUSI_TYPE",
              "dictKey": [
                "AGGS_BUSI_TYPE"
              ],
              "format": "label"
            }
          },
          "source_system": {
            "control": "select",
            "label": "来源系统",
            "dict": {
              "dictType": "AGGS_SYSTEM_TYPE",
              "dictKey": [
                "AGGS_SYSTEM_TYPE"
              ],
              "format": "label"
            }
          }
        },
        {
          "appl_id": {
            "control": "text",
            "label": "申请编号"
          },
          "mobile_no": {
            "control": "text",
            "label": "手机号码"
          }
        },
        {
          "cust_no": {
            "control": "text",
            "label": "客户编号",
            "rules": [
              {
                "max": 18,
                "message": "长度不能超过18个字符"
              }
            ]
          },
          "cust_name": {
            "control": "text",
            "label": {
              "default": "申请人姓名",
              "i18n": "K0001.Applicant-name"
            },
            "rules": [
              {
                "max": 18,
                "message": "长度不能超过18个字符"
              }
            ]
          }
        },
        {
          "appl_start_date": {
            "control": "dateTimePicker",
            "label": {
              "default": "进件日期",
              "i18n": "K0001.Incoming-date"
            }
          },
          "appl_end_date": {
            "control": "dateTimePicker",
            "label": {
              "default": "进件截止日期",
              "i18n": "K0001.Incoming-deadline"
            }
          }
        },
        {
          "cert_type": {
            "control": "select",
            "label": "证件类型",
            "dict": {
              "dictType": "MS_E_CRET_TYPE",
              "dictKey": [
                "MS_E_CRET_TYPE"
              ],
              "format": "label"
            }
          },
          "cert_no": {
            "control": "text",
            "label": "证件号码"
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "aggs0001",
          "cur_user_id": "$g{{user_info.userCode}}",
          "busi_type": "${{params.busi_type}}",
          "source_system": "${{params.source_system}}",
          "appId": "aggsOnl"
        }
      },
      "thead": [
        {
          "label": "申请编号",
          "prop": "appl_id",
          "width": 200,
          "align": "center"
        },
        {
          "label": "业务类型",
          "prop": "busi_type",
          "width": 100,
          "align": "center",
          "dict": {
            "dictType": "AGGS_BUSI_TYPE",
            "dictKey": [
              "AGGS_BUSI_TYPE"
            ],
            "format": "label"
          }
        },
        {
          "label": "流程产品名称",
          "prop": "wf_prod_name",
          "width": 200,
          "align": "center"
        },
        {
          "label": "客户名称",
          "prop": "cust_name",
          "width": 150,
          "align": "center"
        },
        {
          "label": "手机号码",
          "prop": "mobile_no",
          "width": 150,
          "align": "center"
        },
        {
          "label": "证件号码",
          "prop": "cert_no",
          "width": 200,
          "align": "center"
        },
        {
          "label": "产品名称",
          "prop": "prod_name",
          "width": 200,
          "align": "center"
        },
        {
          "label": "进件日期",
          "prop": "system_date",
          "width": 100,
          "align": "center"
        },
        {
          "label": "进入节点时间",
          "prop": "start_time",
          "width": 100,
          "align": "center"
        },
        {
          "label": "当前节点",
          "prop": "task_define_name",
          "width": 150,
          "align": "center"
        },
        {
          "label": "处理状态",
          "prop": "appl_state",
          "dict": {
            "dictType": "CT_E_APPLST",
            "dictKey": [
              "CT_E_APPLST"
            ],
            "format": "label"
          },
          "align": "center"
        },
        {
          "label": "当前处理用户名称",
          "prop": "user_name",
          "value": "${{current.cur_user_id}}-${{current.user_name}}",
          "align": "center"
        },
        {
          "label": "当前处理角色名称",
          "prop": "role_name",
          "value": "${{current.cur_wfrl_id}}-${{current.role_name}}",
          "align": "center"
        },
        {
          "label": "停留时间",
          "prop": "stay_time",
          "width": 150,
          "align": "center"
        },
        {
          "label": "来源模块",
          "prop": "source_system",
          "width": 100,
          "align": "center",
          "dict": {
            "dictType": "AGGS_SYSTEM_TYPE",
            "dictKey": [
              "AGGS_SYSTEM_TYPE"
            ],
            "format": "label"
          }
        },
        {
          "label": "操作",
          "fixed": "right",
          "width": 240,
          "align": "center",
          "btns": [
            {
              "type": "edit",
              "label": "办理",
              "toRequest": {
                "url": "@CommWaitToDoDeal",
                "method": "GET",
                "target": "navTab",
                "noRefreshTab": true,
                "params": {
                  "frame_cd": "${{current.todo_frame}}",
                  "cust_name": "${{current.cust_name}}",
                  "appl_state": "${{current.appl_state}}",
                  "appl_date": "${{current.system_date}}",
                  "task_inst_id": "${{current.task_inst_id}}",
                  "cust_id": "${{current.cust_no}}",
                  "cust_no": "${{current.cust_no}}",
                  "appId": "${{current.source_system}}Onl"
                }
              },
              "props": {
                "title": "待办事项办理"
              }
            },
            {
              "type": "edit",
              "label": "流程跟踪",
              "toRequest": {
                "url": "@ProcessImgView",
                "method": "GET",
                "params": {
                  "from": "backlog",
                  "appId": "${{current.source_system}}Onl"
                }
              },
              "props": {
                "width": "1200px",
                "title": "流程跟踪图"
              }
            },
            {
              "type": "edit",
              "label": "转签",
              "show": {
                "prop": "sign_tran_flag",
                "is": [
                  "1"
                ]
              },
              "toRequest": {
                "url": "/views/msap/bpm/processManagement/design/process_transign_manager.json",
                "method": "GET",
                "params": {
                  "appId": "${{current.source_system}}Onl",
                  "task_inst_id": "${{current.task_inst_id}}",
                  "sign_mode": "0"
                }
              },
              "props": {
                "title": "流程转签"
              }
            },
            {
              "type": "edit",
              "label": "取消签收",
              "show": {
                "prop": "sign_cancel_flag",
                "is": [
                  "1"
                ]
              },
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "bpm1021",
                  "task_inst_id": "${{current.task_inst_id}}",
                  "appId": "${{form.appId}}"
                }
              },
              "confirm": {
                "title": "提示信息",
                "msg": "确认要取消签收吗？",
                "confirmButtonText": "确定",
                "cancelButtonText": "取消"
              }
            }
          ]
        }
      ]
    }
  }
}