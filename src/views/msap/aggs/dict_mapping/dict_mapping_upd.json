{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "aggs0103",
          "appId": "aggsOnl"

        }
      },
      "controls": [
        {
          "dict_type": {
            "control": "text",
            "label": "业务字典类型编号",
          "disabled":true,
            "rules": [
              {
                "required": true,
                "message": "请输入业务字典类型编号"
              },
              {
                "max": 32,
                "message": "长度不能超过32个字符"
              }
            ]
          },
          "dict_type_name": {
            "control": "text",
            "label": "业务字典类型名称",
            "rules": [
              {
                "required": true,
                "message": "请输入业务字典类型名称"
              },
              {
                "max": 20,
                "message": "长度不能超过20个字符"
              }
            ]
          }
        },
        {
          "dict_id": {
            "control": "text",
            "disabled":true,
            "label": "业务字典值编号",
            "rules": [
              {
                "required": true,
                "message": "请输入业务字典值编号"
              },
              {
                "max": 32,
                "message": "长度不能超过32个字符"
              }
            ]
          },
          "dict_name": {
            "control": "text",
            "label": "业务字典值名称",
            "rules": [
              {
                "required": true,
                "message": "请输入业务字典值名称"
              },
              {
                "max": 20,
                "message": "长度不能超过20个字符"
              }
            ]
          }
        },
        {
          "sort_no": {
            "control": "inputNumber",
            "label": "排序",
            "rules": [
              {
                "required": true,
                "message": "请输入业务字典排序序号"
              }
            ]
          }
        },
        {
          "mapp_dict_id": {
            "control": "text",
            "label": "映射业务字典值编号",
            "rules": [
              {
                "required": true,
