              },
              {
                "max": 32,
                "message": "长度不能超过32个字符"
              }
            ]
          },
          "mapp_dict_name": {
            "control": "text",
            "label": "映射业务字典值名称",
            "rules": [
              {
                "required": true,
                "message": "请输入映射业务字典值名称"
              },
              {
                "max": 20,
                "message": "长度不能超过20个字符"
              }
            ]
          }
        }
