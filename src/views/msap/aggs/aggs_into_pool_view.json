{
  "module": "v_search_btn_datagrid",
  "layout": {
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "target": "dialog",
          "label": "批量认领",
          "toRequest": {
            "method": "GET",
            "url": "/views/msap/aggs/aggs_into_pool_batch.json",
            "params": {
              "busi_type":"${{form.busi_type}}"
            }
          },
          "props": {
            "title": "批量认领",
            "width": "600px"
          }
        }
      ]
    },
    "form": {
      "controls": [
        {
          "busi_type": {
            "control": "remoteSelect",
            "label": "业务类型",
            "labelField": "dict_name",
            "valueField": "dict_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "aggs0105",
                "dict_type": "AGGS_BUSI_TYPE",
                "appId":"aggsOnl"
              }
            },
            "format": "label"
          
          },
          "source_system": {
            "control": "select",
            "label": "来源系统",
            "dict": {
              "dictType": "AGGS_SYSTEM_TYPE",
              "dictKey": [
                "AGGS_SYSTEM_TYPE"
              ],
              "format": "label"
            }
          }
        },
        {
          "appl_id": {
            "control": "text",
            "label": "申请编号"
          },
          "mobile_no": {
            "control": "text",
            "label": "手机号码"
          }
        },
        {
          "cust_no": {
            "control": "text",
            "label": "客户编号",
            "rules": [
              {
                "max": 18,
                "message": "长度不能超过18个字符"
              }
            ]
          },
          "cust_name": {
            "control": "text",
            "label": {
              "default": "申请人姓名",
              "i18n": "K0001.Applicant-name"
            },
            "rules": [
              {
                "max": 18,
                "message": "长度不能超过18个字符"
              }
            ]
          }
        },
        {
          "appl_start_date": {
            "control": "dateTimePicker",
            "label": {
              "default": "进件日期",
              "i18n": "K0001.Incoming-date"
            }
          },
          "appl_end_date": {
            "control": "dateTimePicker",
            "label": {
              "default": "进件截止日期",
              "i18n": "K0001.Incoming-deadline"
            }
          }
        },
        {
          "cert_type": {
            "control": "select",
            "label": "证件类型",
            "dict": {
              "dictType": "MS_E_CRET_TYPE",
              "dictKey": [
                "MS_E_CRET_TYPE"
              ],
              "format": "label"
            }
          },
          "cert_no": {
            "control": "text",
            "label": "证件号码"
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "aggs0002",
          "cur_user_id": "$g{{user_info.userCode}}",
          "appId": "aggsOnl"
        }
      },
      "thead": [
        {
          "label": "申请编号",
          "prop": "appl_id",
          "width": 200
        },
        {
          "label": "业务类型",
          "prop": "busi_type",
          "width": 100,
          "align": "center",
          "dict": {
            "dictType": "AGGS_BUSI_TYPE",
            "dictKey": [
              "AGGS_BUSI_TYPE"
            ],
            "format": "label"
          }
        },
        {
          "label": "流程产品名称",
          "prop": "wf_prod_name",
          "width": 200
        },
        {
          "label": "申请人姓名",
          "prop": "cust_name",
          "width": 150
        },
        {
          "label": "证件号码",
          "prop": "cert_no",
          "width": 160
        },
        {
          "label": "手机号",
          "prop": "mobile_no",
          "width": 120
        },
        {
          "label": "当前节点",
          "prop": "cur_name"
        },
        {
          "label": "进入节点时间",
          "prop": "start_time"
        },
        {
          "label": "停留时间",
          "prop": "stay_time",
          "align": "center"
        },
        {
          "label": "来源模块",
          "prop": "source_system",
          "width": 100,
          "align": "center",
          "dict": {
            "dictType": "AGGS_SYSTEM_TYPE",
            "dictKey": [
              "AGGS_SYSTEM_TYPE"
            ],
            "format": "label"
          }
        },
        {
          "label": "操作",
          "fixed": "right",
          "width": 80,
          "align": "center",
          "btns": [
            {
              "type": "delete",
              "label": "领取任务",
              "confirm": {
                "title": "提示信息",
                "msg": "确认要领取该任务吗",
                "confirmButtonText": "确定",
                "cancelButtonText": "取消"
              },
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "bpm1005",
                  "appId": "${{current.source_system}}Onl",
                  "task_id": "${{current.task_inst_id}}"
                }
              }
            }
          ]
        }
      ]
    }
  }
}