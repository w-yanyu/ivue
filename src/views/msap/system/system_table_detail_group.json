{
  "module": "v-realonlyForm-btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",

      "controlsGroup": [
        {
          "title": "表信息",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "accordion": false,
          "expand": true,
          "controls": [
            {
              "id1": {
                "control": "dynamic",
                "isShowChildForm": false,
                "span": 24,
                "toRequest": {
                  "url": "/views/msap/system/system_table.json",
                  "method": "get",
                  "params": { 
                    "table_name":"${{params.table_name}}",
                    "table_desc":"${{params.table_desc}}",
                    "busi_type":"${{params.busi_type}}"
                }
                 
                }
              }
            }
          ]
        },
        {
          "title": "索引信息",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "accordion": false,
          "expand": true,
          "controls": [
            {
              "id2": {
                "control": "dynamic",
                "isShowChildForm": false,
                "span": 24,
                "toRequest": {
                  "url": "/views/msap/system/system_table_odbs.json",
                  "method": "get",
                  "params": { 
                    "table_name":"${{params.table_name}}",
                    "table_desc":"${{params.table_desc}}",
                    "busi_type":"${{params.busi_type}}",
                    "appId":"${{params.appId}}"
                }
                 
                }
              }
            }
          ]
        },
        {
          "title": "字段信息",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "accordion": false,
          "expand": true,
          "controls": [
            {
              "id3": {
                "control": "dynamic",
                "isShowChildForm": false,
                "span": 24,
                "toRequest": {
                  "url": "/views/msap/system/system_table_colums.json",
                  "method": "get",
                  "params": { 
                    "table_name":"${{params.table_name}}",
                    "table_desc":"${{params.table_desc}}",
                    "busi_type":"${{params.busi_type}}",
                    "appId":"${{params.appId}}"
                }
                 
                }
              }
            }
          ]
        }
      ]
    }
  }
}