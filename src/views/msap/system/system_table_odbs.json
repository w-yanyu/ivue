{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ms0212",
          "table_name": "${{params.table_name}}",
          "appId": "${{params.appId}}"
        }
      },
      "pagination": {
        "show": false
      },
      "thead": [
        {
          "label": "索引编号",
          "prop": "odbindex_name",
          "align": "center"
        },
        {
          "label": "索引类型",
          "prop": "odbindex_type",
          "align": "center",
          "dict": {
            "dictType": "E_ODBINDEX_TYPE",
            "dictKey": [
              "E_ODBINDEX_TYPE"
            ],
            "format": "value-label"
          }
        },
        {
          "label": "索引字段",
          "prop": "column_list",
          "align": "center"
        }
      ]
    }
  }
}