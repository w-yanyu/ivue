{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "scope": "formData",
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": "",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "ms0205",
                            "appId":"${{params.appId}}"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "table_name": {
                        "control": "lookup",
                        "label": "表名",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入表名"
                            }
                        ],
                        "keyField":{
                            "table_name": "table_name",
                            "table_desc": "table_desc"
                          },
                          "toRequest": {
                            "url": "/views/msap/adp/project_table_lookup.json",
                            "method": "post",
                            "params": {
                              
                            }
                          },
                          "props": {
                            "title": "代码内表选择"
                          }
                    },
                    "table_desc": {
                        "control": "text",
                        "label": "表描述",
                        "width": "130px",
                        "rules": [
                            {
                                "message": "请输入表描述"
                            }
                        ]
                    }
                },
                {
                    "busi_type": {
                        "control": "text",
                        "label": "产品类别",
                        "width": "130px"
                    }
                }
            ]
        }
    }
}