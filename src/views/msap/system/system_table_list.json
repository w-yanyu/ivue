{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "appId": {
            "control": "select",
            "label": "业务模块",
            "selectFirstValue": true,
            "dict": {
              "dictType": "MS_APPID",
              "dictKey": [
                "MS_APPID"
              ],
              "format": "value-label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择业务模块"
              }
            ],
            "value": "${{params.appId}}"
          },
          "busi_type": {
            "control": "text",
            "label": "表类型"
          }
        },
        {
          "table_name": {
            "control": "text",
            "label": "数据表名"
          },
          "table_desc": {
            "control": "text",
            "label": "数据表中文名称"
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "icon": "el-icon-circle-plus",
          "target": "dialog",
          "label": "新增",
          "toRequest": {
            "method": "GET",
            "url": "/views/msap/system/system_table_add.json",
            "params": {
              "appId": "${{form.appId}}"              
            }
          },
          "props": {
            "title": "新增系统内表配置"
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "user_id": "$g{{user_info.userCode}}",
          "servicecode": "ms0207",
          "appId": "${{form.appId}}"
        }
      },
      "pagination": {
        "show": true,
        "pageSize": 10
      },
      "thead": [
        {
          "type": "index",
          "label": "序号"
        },
        {
          "label": "数据表名",
          "prop": "table_name"
        },
        {
          "label": "数据表中文名",
          "prop": "table_desc"
        },
        {
          "label": "表类型",
          "prop": "busi_type"
        },
        {
          "label": "操作",
          "align": "center",
          "fixed": "right",
          "btns": [
            {
              "type": "edit",
              "label": {
                "default": "表详情"
              },
              "toRequest": {
                "url": "/views/msap/system/system_table_detail_group.json",
                "method": "GET",
                "params": {
                  "appId": "${{form.appId}}",
                  "table_name": "${{current.table_name}}",
                  "table_desc": "${{current.table_desc}}",
                  "busi_type": "${{current.busi_type}}"
                }
              },
              "props": {
                "width": "1200px",
                "title": "表详情"
              }
            },
            {
              "type": "edit",
              "label": "删除",
             
              "doRequest": {
                  "url": "/SUMP/icmcall/icmRPCCall",
                  "method": "POST",
                  "params": {
                      "servicecode": "ms0206",
                      "table_name": "${{current.table_name}}",
                      "appId": "${{form.appId}}"
                  }
              },
              "confirm":{
                  "title":"提示信息",
                  "msg":"确认要删除表配置吗？",
                  "confirmButtonText":"确定",
                  "cancelButtonText":"取消"
              }
          }
          ]
        }
      ]
    }
  }
}