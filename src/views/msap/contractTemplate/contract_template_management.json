{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "controls": [
                {
                    "appId": {
                        "control": "select",
                        "label": "业务模块",
                        "selectFirstValue":true,
                        "dict": {
                            "dictType": "MS_APPID",
                            "dictKey": [
                                "MS_APPID"
                            ],
                            "format": "value-label"
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择业务模块"
                            }]
                    }
                  },
                {
                    "template_id": {
                        "label": "模板编号",
                        "control": "text",
                        "rules": [
                            {
                                "max": 32,
                                "message": "长度不能超过32个字符"
                            }
                        ]
                    },
                    
                    "template_name": {
                        "label": "模板名称",
                        "control": "text",
                        "rules": [
                            {
                                "max": 32,
                                "message": "长度不能超过32个字符"
                            }
                        ]
                    }
                }
            ],
            "foldLineNumber": 3
        },
        "toolbar": {
            "btns": [
                {
                    "type": "add",
                    "target": "dialog",
                    "label": "文件模板上传",
                    "toRequest": {
                        "method": "GET",
                        "url": "/views/msap/contractTemplate/contract_template_upload.json",
                        "params": {
                            "appId":"${{form.appId}}"
                          }
                    },
                    "props": {
                        "width": "65%",
                        "title": "方法定义新增"
                    }
                }
            ]
        },
        "datagrid": {
            "isInitSearch": false,
            "scope": "searchTable",
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "post",
                "params": {
                    "servicecode": "ms0103"
                }
            },
            "thead": [
                {
                    "label": "序号",
                    "type": "index"
                },
                {
                    "label": "模板编号",
                    "prop": "template_id",
                    "hidden":true,
                    "value":"${{current.templateId}}"
                },
                {
                    "label": "模板编号",
                    "prop": "templateId"
                },
                {
                    "label": "模板名称",
                    "prop": "templateName"
                },
                {
                    "label": "模板路径",
                    "prop": "templatePath"
                },
                {
                    "label": "模板类型",
                    "prop": "templateType"
                },
                {
                    "label": "模板描述",
                    "prop": "templateDesc"
                },
                {
                    "label": "文件类型",
                    "prop": "fileType"
                },
                
                {
                    "label": "操作",
                    "fixed": "right",
                    "width": 200,
                    "align":"center",
                    "btns": [
                        {
                            "type": "edit",
                            "label": "模板更新",
                            "toRequest": {
                                "url": "/views/msap/contractTemplate/contract_template_update.json",
                                "method": "GET",
                                "params": {
                                    "appId":"${{form.appId}}"
                                  }
                            },
                            "props": {
                                "width": "70%",
                                "title": "模板更新"
                            }
                        },
                        
                        {
                            "type": "delete",
                            "label": "删除",
                            "doRequest": {
                                "url": "/SUMP/icmcall/icmRPCCall",
                                "method": "post",
                                "params": {
                                    "servicecode": "ms0102",
                                    "trantp":"DELETE",
                                    "appId":"${{form.appId}}"
                                    
                                }
                            }
                        }
                    ]
                }
            ],
            "pagination": {
                "show": true
            }
        }
    }
}