{
    "module": "v_search_btn_datagrid",
    "layout": {
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": true,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "bpm1016",
                    "appId":"${{params.appId}}",
                    "wf_inst_id":"${{params.wf_inst_id}}"
                }
            },
            "pagination": {
                "show": false,
                "pageSize": 10
            },
            "thead": [
                {
                    "type": "index"
                },
                {
                    "label": "节点名称",
                    "prop": "task_define_name"
                },
                {
                    "label": "处理人编号",
                    "prop": "user_id"
                },
                {
                    "label": "处理人名称",
                    "prop": "user_name"
                },
                {
                    "label": "机构号",
                    "prop": "branch_id"
                },
                {
                    "label": "机构名称",
                    "prop": "branch_name"
                },
                {
                    "label": "处理时间",
                    "prop": "bpm_end_time"
                },
                {
                    "label": "耗时",
                    "prop": "cost_time"
                },                
                {
                    "label": "审批结果",
                    "prop": "appv_result"
                }
            ]
        }
    }
  }