{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "bpm1030",
          "appId":"${{params.appId}}"
        }
      },
      "controls": [
        {
          "target_user_id": {
            "control": "text",
            "disabled":true,
            "label": "申请用户编号",
            "rules": [
              {
                "required": true,
                "message": "请选择申请用户编号"
              }
            ]
          },
          "user_name": {
            "control": "text",
            "label": "申请用户名称",
            "disabled":true
          }
        },
        {
          "agent_cd": {
            "control": "text",
            "disabled":true,
            "label": "代理用户编号",
            "rules": [
              {
                "required": true,
                "message": "请选择代理用户编号"
              }
            ]
          },
          "agent_name": {
            "control": "text",
            "label": "代理用户名称",
            "disabled":true
          }
        },
        {
          "effect_date": {
            "control": "dateTimePicker",
            "label": "代理起始日期",
            "rules": [
              {
                "required": true,
                "message": "请选择代理起始日期"
              }
            ]
          },
          "invalid_date": {
            "control": "dateTimePicker",
            "label": "代理结束日期",
            "rules": [
              {
                "required": true,
                "message": "请选择代理结束日期"
              }
            ]
          }
           
         
        },{
          "valid_flag": {
            "control": "select",
            "label": "是否有效",
            "selectFirstValue":true,
            "dict": {
                "dictType": "MS_E_YESORNO",
                "dictKey": [
                    "MS_E_YESORNO"
                ],
                "format": "value-label"
            },
            "rules": [
                {
                    "required": true,
                    "message": "请选择是否有效"
                }]
        }
      
        }

      ]
    }
  }
}
