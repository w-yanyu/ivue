{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "appId": {
            "span": 8,
            "control": "select",
            "label": "业务模块",
            "selectFirstValue": true,
            "dict": {
              "dictType": "MS_APPID",
              "dictKey": [
                "MS_APPID"
              ],
              "format": "value-label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择业务模块"
              }
            ]
          }
        },
        {
          "target_user_id": {
            "span": 8,
            "control": "text",
            "label": "申请用户编号",
            "placeholder": "请选择申请用户编号"
          },
          "agent_cd": {
            "span": 8,
            "control": "text",
            "label": "代理用户编号",
            "placeholder": "请选择代理用户编号"
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "target": "dialog",
          "label": "新增",
          "toRequest": {
            "url": "/views/msap/bpm/workbench/agent_sett/agent-add.json",
            "method": "GET",
            "params": {
              "appId": "${{form.appId}}"
            }
          },
          "props": {
            "width": "65%",
            "title": "新增代理人配置"
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "bpm1032"
        }
      },
      "thead": [
        {
          "type": "index",
          "label": "序号"
        },
        {
          "label": "申请用户名称",
          "prop": "user_name",
          "align": "center"
        },
        {
          "label": "代理用户名称",
          "prop": "agent_name",
          "align": "center"
        },
        {
          "label": "代理起始日期",
          "prop": "effect_date",
          "align": "center"
        },
        {
          "label": "代理结束日期",
          "prop": "invalid_date",
          "align": "center"
        },
        {
          "label": "是否有效",
          "prop": "valid_flag",
          "align": "center",
          "dict": {
            "dictType": "MS_E_YESORNO",
            "dictKey": [
              "MS_E_YESORNO"
            ],
            "labelField": "dictName",
            "valueField": "dictId"
          }
        },
        {
          "label": "操作",
          "align": "center",
          "width": "180",
          "btns": [
            {
              "type": "edit",
              "label": "代理配置详情",
              "toRequest": {
                "url": "/views/msap/bpm/workbench/agent_sett/agent-veiw.json",
                "method": "POST",
                "params": {
                  "appId": "${{form.appId}}"
                }
              },
              "props": {
                "width": "65%",
                "title": "代理配置详情"
              }
            },
            {
              "type": "edit",
              "label": "维护",
              "toRequest": {
                "url": "/views/msap/bpm/workbench/agent_sett/agent-update.json",
                "method": "POST",
                "params": {
                  "appId": "${{form.appId}}"
                }
              },
              "props": {
                "width": "65%",
                "title": "代理维护"
              }
            },
            {
              "type": "delete",
              "label": "删除",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "bpm1031",
                  "appId": "${{form.appId}}"
                }
              }
            }
          ]
        }
      ]
    }
  }
}