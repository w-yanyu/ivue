{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "bpm1029",
          "appId": "${{params.appId}}"
        }
      },
      "controls": [
        {
          "target_user_id": {
            "control": "lookup",
            "label": "申请用户编号",
            "keyField": {
              "target_user_id": "user_id",
              "user_name": "user_name"
            },
            "toRequest": {
              "url": "/views/ce/paramManage/assign/collTeam/user_manager_lookup.json",
              "method": "get"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择申请用户编号"
              }
            ]
          },
          "user_name": {
            "control": "string",
            "label": "申请用户名称"
          }
        },
        {
          "agent_cd": {
            "control": "lookup",
            "label": "代理用户编号",
            "keyField": {
              "agent_cd": "user_id",
              "agent_name": "user_name"
            },
            "toRequest": {
              "url": "/views/ce/paramManage/assign/collTeam/user_manager_lookup.json",
              "method": "get"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择代理用户编号"
              }
            ]
          },
          "agent_name": {
            "control": "string",
            "label": "代理用户名称"
          }
        },
        {
          "effect_date": {
            "control": "dateTimePicker",
            "label": "代理起始日期",
            "rules": [
              {
                "required": true,
                "message": "请选择代理起始日期"
              }
            ]
          },
          "invalid_date": {
            "control": "dateTimePicker",
            "label": "代理结束日期",
            "rules": [
              {
                "required": true,
                "message": "请选择代理结束日期"
              }
            ]
          }
        },
        {
          "valid_flag": {
            "control": "select",
            "label": "是否有效",
            "selectFirstValue": true,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "value-label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择是否有效"
              }
            ]
          }
        }
      ]
    }
  }
}