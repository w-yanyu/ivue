{
  "module": "v-charts",
  "layout": {
      "rows":[
        [
          {
            "span": 24,
            "type": "ve-histogram",
            "props": {
              "settings" : {
                "#axisSite": { "right": ["OrderRate"] },
                "#yAxisType": ["KMB", "percent"],
                "yAxisName": ["单位：笔数"],
                "labelMap": {
                  "add_count": "新增业务数量",
                  "appv_count": "审批通过数量",
                  "rejt_count": "拒绝数据量",
                  "deal_count": "审批中数据量",
                  "cancel_count": "取消数量"
                }
              },
              "extend":{
                "xAxis.0.axisLabel.rotate": 45,
                "series": {
                  "label": {
                    "normal": {
                      "show": true
                    }
                  }
                }
              },
              "data": {
                "columns": ["system_date", "add_count", "appv_count", "rejt_count", "deal_count", "cancel_count"],
                "rows": {
                  "initRequest":{
                    "url": "/SUMP/icmcall/icmRPCCall",
                    "method": "post",
                    "params": {
                      "appIda":"${{form.appIda}}",
                        "servicecode": "bpm1022",
                        "busi_type": "DISLN",
                        "date_from":"19770101",
                        "date_end":"",
                        "appId":"icmsOnl"
                    }
                  }
                }
              }
            }
          }
        ]
      ]
  }
}
