
{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [],
      "btns":[]
    },
    "toolbar": {},
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "bpm5014",
          "appId":"${{params.appId}}",
          "route_cd":"${{params.route_cd}}",
          "wf_prod_no":"${{params.wf_prod_no}}",
          "bpm_version":"${{params.bpm_version}}",
          "route_id":"${{params.route_id}}"
        }
      },
      "pagination":{
        "show":false
      },
      "thead": [
        {
          "label": "流程产品编号",
          "align":"center",
          "prop": "wf_prod_no"
        },
        {
          "label": "路线编号",
          "align":"center",
          "prop": "route_cd"
        },
        {
          "label": "路线ID",
          "align":"center",
          "prop": "route_id"
        },
        {
          "label":"业务方法编号",
          "align":"center",
          "prop":"busi_id"
        },
        {
          "label":"业务方法名称",
          "align":"center",
          "prop":"service_name"
        },
        {
          "label":"执行序号",
          "align":"center",
          "prop":"sort_no"
        }
      ]
    }
  }
}
