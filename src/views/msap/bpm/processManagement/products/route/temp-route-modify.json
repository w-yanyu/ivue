{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "bpm4013",
          "bpm_version": "${{params.bpm_version}}",
          "wf_task_no": "${{params.wf_task_no}}",
          "route_cd":"${{params.route_cd}}",
          "appId":"${{params.appId}}"
        }
      },
      "btns": [
        {
          "type": "edit",
          "label": "保存",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "bpm4014",
              "bpm_version": "${{params.bpm_version}}",
              "appId":"${{params.appId}}"
            }
          }
        }
      ],
      "controls": [
        {
          "route_cd": {
            "control": "string",
            "label": "路线编号",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请选择路线编号"
              }
            ]
          },
          "route_id": {
            "control": "string",
            "label": "路线ID",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请选择路线ID"
              }
            ]
          },
          "route_name": {
            "control": "string",
            "label": "路线名称",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请选择路线名称"
              }
            ]
          },
          "wf_prod_no": {
            "control": "string",
            "label": "流程产品编号",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请选择流程产品编号"
              }
            ]
          },
          "wf_prod_name": {
            "control": "string",
            "label": "流程产品名称",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请选择流程产品名称"
              }
            ]
          },
          "route_type": {
            "control": "select",
            "label": "路线状态",
            "width": "130px",
            "dict": {
              "dictType": "CT_E_APPLST",
              "dictKey": [
                "CT_E_APPLST"
              ],
              "format": "value-label"
            }
          },
          "final_or_not": {
            "control": "select",
            "label": "是否终审",
            "width": "130px",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "value-label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择是否终审"
              }
            ]
          },
          "task_define_id": {
            "control": "string",
            "label": "起始节点ID",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请选择起始节点ID"
              }
            ]
          },
          "task_define_name": {
            "control": "string",
            "label": "起始节点名称",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请选择起始节点名称"
              }
            ]
          },
          "next_task_define_id": {
            "control": "string",
            "label": "目标节点ID",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请选择目标节点ID"
              }
            ]
          },
          "next_task_define_name": {
            "control": "string",
            "label": "目标节点名称",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请选择目标节点名称"
              }
            ]
          },
          "goback_ind": {
            "control": "select",
            "label": "是否退回",
            "width": "130px",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择是否退回"
              }
            ]
          },
          "seq_no":{
            "control":"inputNumber",
             
            "label": "路线序号",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请选择输入路线序号"
              }
            ]
          },
          "remark": {
            "control": "textarea",
            "label": "备注",
            "width": "130px",
            "maxlength": 500,
            "showWordLimit": true
          }
        }
      ]
    }
  }
}