{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest":"${{params}}",
      "btns": [
        
      ],
      "controls": [
        {
          "wf_prod_no": {
            "control": "string",
            "label": "流程产品编号",
            "width": "130px",
            "rules": [
              {
                "required": true
              }
            ]
          },
          "route_cd": {
            "control": "string",
            "label": "路线编号",
            "width": "130px",
            "rules": [
              {
                "required": true
              }
            ]
          },
          "route_id": {
            "control": "string",
            "label": "路线ID",
            "width": "130px",
            "rules": [
              {
                "required": true
              }
            ]
          },
          "busi_id":{
            "control": "string",
            "label": "业务方法编号",
            "width": "130px",
            "edit": false,
            "keyField": {
                "busi_id": "busi_id",
                "service_name":"service_name"
            },
            "toRequest": {
                "url": "/views/msap/bpm/processManagement/business/businessServiceComposition_lookup.json",
                "method": "post",
                "params":{
                  "servicecode": "bpm0065",
                  "appId":"${{params.appId}}"
                },
                "props": {
                  "width": "80%",
                  "title": "业务方法编号选择"
                }
                
            },
            "rules": [
              {
                "required": true,
                "message": "请输入业务编号"
              }
            ]
          },
          "service_name": {
            "control": "string",
            "label": "业务方法名称",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入业务名称"
              }
            ]
          },
          "sort_no": {
            "control": "string",
            "label": "执行序号",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入执行序号"
              }
            ]
          }
        }
      ]
    }
  }
}
