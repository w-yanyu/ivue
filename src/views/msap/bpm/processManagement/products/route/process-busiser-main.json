
{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [],
      "btns":[]
    },
    "toolbar": {
      "btns": [
        {
          "type": "edit",
          "label": "新增",
          "icon": "el-icon-tickets",
          "toRequest": {
            "url": "/views/msap/bpm/processManagement/products/route/process-busiser-add.json",
            "method": "POST",
            "params": {
              "bpm_version": "${{params.bpm_version}}",
              "wf_prod_no": "${{params.wf_prod_no}}",
              "route_cd": "${{params.route_cd}}",
              "route_id": "${{params.route_id}}",
              "appId":"${{params.appId}}"
            }
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "bpm4015",
          "appId":"${{params.appId}}",
          "route_cd":"${{params.route_cd}}",
          "wf_prod_no":"${{params.wf_prod_no}}",
          "bpm_version":"${{params.bpm_version}}"
        }
      },
      "pagination": {
        "show": false
      },
      "thead": [
        {
          "label": "流程产品编号",
          "align":"center",
          "prop": "wf_prod_no"
        },
        {
          "label": "路线编号",
          "align":"center",
          "prop": "route_cd"
        },
        {
          "label": "路线ID",
          "align":"center",
          "prop": "route_id"
        },
        {
          "label":"业务方法编号",
          "align":"center",
          "prop":"busi_id"
        },
        {
          "label":"业务方法名称",
          "align":"center",
          "prop":"service_name"
        },
        {
          "label":"执行序号",
          "align":"center",
          "prop":"sort_no"
        },
        {
          
          "label": "操作",
          "width": "280px",
          "align":"center",
          "btns": [
            {
              "type": "custom",
              "label": "查看",
              "width": "70px",
              "toRequest": {
                "url": "/views/msap/bpm/processManagement/products/route/process-busiser-look.json",
                "method": "POST",
                "params": {
                  "appId":"${{params.appId}}"
                }
              },
              "props": {
                "title": "查看"
              }
            },
            {
              "type": "custom",
              "label": "维护",
              "width": "70px",
              "toRequest": {
                "url": "/views/msap/bpm/processManagement/products/route/process-busiser-edit.json",
                "method": "POST",
                "params": {
                  "appId":"${{params.appId}}",
                  "route_deal_no":"${{current.route_deal_no}}"
                }
              },
              "props": {
                "title": "维护"
              }
            },
            {
              "type": "custom",
              "label": "删除",
              "width": "70px",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "bpm4018",
                  "appId":"${{params.appId}}",
                  "route_deal_no":"${{current.route_deal_no}}"
                }
              },
              "confirm": {
                "title": "提示信息",
                "msg": "请确认是否删除？ ",
                "confirmButtonText": "确定",
                "cancelButtonText": "取消"
              }
            }
          ]
        }
      ]
    }
  }
}
