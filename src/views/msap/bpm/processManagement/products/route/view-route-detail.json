{
  "module": "v-realonlyForm-btn",
  "layout": {
    "form": {
      "initRequest":"${{params}}",
      "scope": "searchTable",
      "controls": [
        {
          "route_id": {
            "control": "string",
            "label": "路线id",
            "width": "130px"
          },
          "route_cd": {
            "control": "string",
            "label": "路线编码",
            "width": "130px"
          }
        },{
          "route_name": {
            "control": "string",
            "label": "路线名称",
            "width": "130px"
          },
          "route_type": {
            "control": "string",
            "label": "路线类型",
            "width": "130px",
            "dict": {
              "dictType": "CT_E_APPLST",
              "dictKey": [
                "CT_E_APPLST"
              ],
              "format": "value-label"
            }
          }
        },{
          "wf_prod_no": {
            "control": "string",
            "label": "流程编号",
            "width": "130px"
          },
          "wf_prod_name": {
            "control": "string",
            "label": "流程名称",
            "width": "130px"
          }
        },{
          "task_define_id": {
            "control": "string",
            "label": "起始节点号",
            "width": "130px"
          },
          "task_define_name": {
            "control": "string",
            "label": "起始节点名称",
            "width": "130px"
          }
        },{
          "next_task_define_id": {
            "control": "string",
            "label": "目标节点号",
            "width": "130px"
          },
          "next_task_define_name": {
            "control": "string",
            "label": "目标节点名称",
            "width": "130px"
          }
        },{
          "goback_ind": {
            "control": "string",
            "label": "是否退回原处理人",
            "width": "130px",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "seq_no": {
            "control": "string",
            "label": "显示排序",
            "width": "130px"
          }
        },{
          "route_action": {
            "control": "string",
            "label": "路线ACTION",
            "width": "130px"
          },
          "data_version": {
            "control": "string",
            "label": "版本号",
            "width": "130px"
          }
        },{
          "data_update_time": {
            "control": "string",
            "label": "最近修改日期",
            "width": "130px"
          },
          "data_update_user": {
            "control": "string",
            "label": "最近修改人",
            "width": "130px"
          }
        },{
          "remark": {
            "control": "string",
            "label": "流程备注",
            "width": "130px"
          }
        }
      ]
    }
  }
}