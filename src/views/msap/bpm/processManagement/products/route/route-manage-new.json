{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "bpm5007",
          "bpm_version": "${{params.bpm_version}}",
          "wf_task_no": "${{params.wf_task_no}}",
          "route_cd":"${{params.route_cd}}",
          "appId":"${{params.appId}}"
        }
      },
      "btns": [
      {
        "type": "edit",
        "label": "生成模板",
        "doRequest": {
          "url": "/SUMP/icmcall/icmRPCCall",
          "method": "POST",
          "params": {
            "servicecode": "bpm3014",
            "wf_task_no": "${{params.wf_task_no}}",
            "route_cd":"${{params.route_cd}}",
            "bpm_version": "${{params.bpm_version}}",
            "appId":"${{params.appId}}"
          }
        }
      }
      ],
      "controls": [
        {
          "route_cd": {
            "control": "string",
            "label": "路线编号",
            "width": "130px",
            "rules": [
              {
                "required": true
              }
            ]
          },
          "route_id": {
            "control": "string",
            "label": "路线ID",
            "width": "130px",
            "rules": [
              {
                "required": true
              }
            ]
          },
          "route_name": {
            "control": "string",
            "label": "路线名称",
            "width": "130px",
            "rules": [
              {
                "required": true
              }
            ]
          },
          "wf_prod_no": {
            "control": "string",
            "label": "流程产品编号",
            "width": "130px",
            "rules": [
              {
                "required": true
              }
            ]
          },
          "wf_prod_name": {
            "control": "string",
            "label": "流程产品名称",
            "width": "130px",
            "rules": [
              {
                "required": true
              }
            ]
          },
          "task_define_id": {
            "control": "string",
            "label": "起始节点ID",
            "width": "130px",
            "rules": [
              {
                "required": true
              }
            ]
          },
          "task_define_name": {
            "control": "string",
            "label": "起始节点名称",
            "width": "130px",
            "rules": [
              {
                "required": true
              }
            ]
          },
          "next_task_define_id": {
            "control": "string",
            "label": "目标节点ID",
            "width": "130px",
            "rules": [
              {
                "required": true
              }
            ]
          },
          "next_task_define_name": {
            "control": "string",
            "label": "目标节点名称",
            "width": "130px",
            "rules": [
              {
                "required": true
              }
            ]
          },
          "route_type": {
            "control": "select",
            "label": "路线状态",
            "disabled":"true",
            "width": "130px",
            "dict": {
              "dictType": "CT_E_APPLST",
              "dictKey": [
                "CT_E_APPLST"
              ],
              "format": "value-label"
            }
          },
          "goback_ind": {
            "control": "select",
            "disabled":"true",
            "label": "是否退回",
            "width": "130px",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true
              }
            ]
          },
          "seq_no":{
            "control":"string",
            "disabled":"true",
            "label": "路线序号",
            "width": "130px",
            "rules": [
              {
                "required": true
              }
            ]
          },
          "remark": {
            "control": "textarea",
            "disabled":"true",
            "label": "备注",
            "width": "130px",
            "maxlength": 500,
            "showWordLimit": true
          }
        }
      ]
    }
  }
}