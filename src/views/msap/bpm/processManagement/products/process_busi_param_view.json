{
    "module": "v-inputDataForm-btn",
    "layout": {
        "form": {
            "name":"process_busi_param_view",
            "initRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "bpm0069",
                    "wfprod_cd": "${{params.wfprod_cd}}"
                }
            },
            "btns": [],
            "controls": [
                {
                    "wfprod_cd": {
                        "control": "text",
                        "label": "流程产品编号",
                        "disabled":true,
                        "span": 22,
                        "value":"${{params.wfprod_cd}}"
                    }
                },
                {
                    "wfprod_desc": {
                        "control": "textarea",
                        "label": "参数配置",
                        "span": 22,
                        "rows":50
                    }
                }
            ]
        }
    }
}