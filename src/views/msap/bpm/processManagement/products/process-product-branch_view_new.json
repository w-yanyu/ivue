
{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "btns": []
    },
    "toolbar": {
    },
    "datagrid": {
      "scope": "searchTable",
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "bpm5003",
          "appId":"${{params.appId}}",
          "wf_prod_no": "${{params.wf_prod_no}}",
          "bpm_version": "${{params.bpm_version}}"
        }
      },
      "thead": [
        {
          "label": "机构号",
          "prop": "branch_id",
          "align":"center"
        },
        {
          "label": "机构名称",
          "prop": "branch_name",
          "align":"center"
        }
      ]
    }
  }
}
