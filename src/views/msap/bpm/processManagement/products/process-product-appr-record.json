
{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "btns": []
    },
    "toolbar": {
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "${{params.servicecode}}",
          "appId":"${{params.appId}}",
          "wf_prod_no":"${{params.wf_prod_no}}",
          "bpm_version":"${{params.bpm_version}}"
        }
      },
      "pagination":{
        "show":false
      },
      "thead": [
        {
          "label": "流程产品编号",
          "prop": "wf_prod_no"
        },
        {
          "label": "流程产品名称",
          "prop": "wf_prod_name"
        },
        {
          "label": "流程名称",
          "prop": "wf_define_name"
        },
        {
          "label": "流程产品类型",
          "width": 120,
          "prop": "wf_prod_tp",
          "dict": {
            "dictType": "BPM_E_WFPDTP",
            "dictKey": [
              "BPM_E_WFPDTP"
            ],
            "format": "value-label"
          }
        },
        {
          "label":"版本号",
          "prop":"bpm_version"
        },
        {
          "label":"审批意见",
          "prop":"appv_desc"
        },
        {
          "label":"创建人",
          "prop":"data_update_user"
        },
        {
          "label":"创建时间",
          "prop":"data_create_time"
        },
        {
          "label":"审核人",
          "prop":"data_update_user"
        },
        {
          "label":"审核时间",
          "prop":"data_update_time"
        }
      ]
    }
  }
}
