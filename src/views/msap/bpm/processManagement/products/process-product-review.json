{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "btns": [
        {
          "type": "submit",
          "label": "提交审核",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "bpm4024",
              "appId": "${{params.appId}}",
              "bpm_version": "${{params.bpm_version}}",
              "wf_prod_no": "${{params.wf_prod_no}}"
            }
          }
        }
      ],
      "controls": [
        {
          "agree_flag": {
            "control": "select",
            "span": 16,
            "inputWidth":"250px",
            "align": "center",
            "label": "审核类型",
            "dict": {
              "dictType": "BPM_E_APPROVE_TYPE",
              "dictKey": [
                  "BPM_E_APPROVE_TYPE"
              ],
              "format": "label"
          },
            "rules": [
              {
                "required": true,
                "message": "请选择审核类型"
              }
            ]
          }
        },
        {
          "appv_desc": {
            "control": "textarea",
            "span": 16,
            "inputWidth":"250px",
            "align": "center",
            "label": "审核意见",
            "autosize": {
              "minRows": 3,
              "maxRows": 5
            },
            "maxlength": 200,
            "showWordLimit": true,
            "rules": [
              {
                "required": true,
                "message": "请输入审核意见"
              }
            ]
          }
        }
      ]
    }
  }
}
