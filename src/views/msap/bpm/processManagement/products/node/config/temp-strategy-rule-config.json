{
  "module": "v-form-editableDataGrid-btn",
  "layout": {
    "form": {

    },
    "toolbar": {

    },
    "datagrid": {
      "isInitSearch": true,
      "doRequest": {
        "url":"/SUMP/icmcall/icmRPCCall",
        
        "method":"POST",
        "params": {
          "wf_task_no": "${{params.wf_task_no}}",
          "bpm_version": "${{params.bpm_version}}",
          "wf_prod_no":"${{params.wf_prod_no}}",
          "task_define_id":"${{params.task_define_id}}",
          "servicecode": "bpm4011",
          "appId":"${{params.appId}}"
        }
      },
      "pagination": {
        "show": false
      },
      "allData":true,
      "thead": [
        {
          "type": "selection",
          "align": "center",
          "width": "55",
          "fixed": "left",
          "show": {
            "prop": "changkuan",
            "is": [
              "0"
            ]
          }
        },
        {
          "prop": "next_task_define_name",
          "type": "string",
          "label": "下一节点名称"
        },
        {
          "prop": "policy_rule_config",
          "type": "string",
          "label": "配置策略",
          "edit": true,
          "333keyField": {
            "policy_rule_config": "policy_id"
          },
          "toRequest": {
            "url": "/views/msap/bpm/processManagement/products/node/config/strategy-rule-lookup.json",
            "method": "post",
            "params": {
              "wf_task_no": "${{params.wf_task_no}}",
              "appId":"${{params.appId}}"
            }
          },
          "props": {
            "title": "选择规则"
          }
        },
        {
          "prop": "rule_desc",
          "type": "string",
          "label": "规则综合表述"
        },
        {
          "prop": "is_def_flow",
          "type": "select",
          "label": "是否默认路线",
          "dict": {
            "dictType": "MS_E_YESORNO",
            "dictKey": [
              "MS_E_YESORNO"
            ],
            "format": "value-label"
          }
        },
        {
          "prop": "check_seq",
          "type": "inputNumber",
          "label": "执行序号",
          "min": 0
        },
        {
          "label": "操作",
          "width": "200px",
          "align":"center",
          "btns": [{
            "type": "etEdit",
            "label": "规则配置",
            "#eventAfter": {
              "callback": "etEditCallback"
            },
            "toRequest": {
              "url": "@StrategyruletabNew",
              "method": "GET",
              "params": {
                "appId":"${{params.appId}}",
                "bpm_version": "${{params.bpm_version}}",
                "wf_task_no": "${{params.wf_task_no}}",
                "task_define_id": "${{params.task_define_id}}",
                "wf_prod_no": "${{params.wf_prod_no}}"
              }
            },
            "props": {
              "width": "65%",
              "title": "规则配置"
            }
          }]
        }
      ]
    },
    "footer": {
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "el-icon-tickets",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "wf_task_no": "${{params.wf_task_no}}",
              "bpm_version": "${{params.bpm_version}}",
              "wf_prod_no":"${{params.wf_prod_no}}",
              "task_define_id":"${{params.task_define_id}}",
              "servicecode": "bpm4027",
              "appId":"${{params.appId}}"
            }
          }
        }
      ]
    }
  }
}
