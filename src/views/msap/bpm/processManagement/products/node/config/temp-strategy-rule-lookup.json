{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [],
      "btns": []
    },
    "datagrid": {
      "doRequest": {
        "url":"/SUMP/icmcall/icmRPCCall",
        "method":"post",
        "params": {
          "wf_task_no": "${{params.wf_task_no}}",
          "bpm_version": "${{params.bpm_version}}",
          "servicecode": "bpm4009",
          "wf_prod_no": "${{params.wf_prod_no}}",
          "task_define_id": "${{params.task_define_id}}",
          "appId":"${{params.appId}}",
          "route_id":"${{params.route_id}}",
          "#process_flag":"1"
        }
      },
      "thead": [
        {
          "prop": "policy_name",
          "label":"策略名称"
        },
        {
          "prop": "policy_id",
          "label": "策略编号"
        },
        {
          "label": "操作",
          "align": "center",
          "btns": [
            {
              "type": "bringback",
              "label": "选择"
            }
          ]
        }
      ]
    }
  }
}
