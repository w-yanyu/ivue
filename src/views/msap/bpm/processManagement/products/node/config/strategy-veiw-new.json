{
  "module": "v-form-editableDataGrid-btn",
  "layout": {
    "form": {
      "controls": [
      ],
      "btns": [
      ]
    },
    "toolbar": {
      "btns": [
      ]
    },
    "datagrid": {
      "doRequest": {
        "url":"/SUMP/icmcall/icmRPCCall",
        
        "method":"POST",
        "params": {
          "wf_task_no": "${{params.wf_task_no}}",
          "bpm_version": "${{params.bpm_version}}",
          "task_define_id": "${{params.task_define_id}}",
          "wf_prod_no": "${{params.wf_prod_no}}",
          "servicecode": "bpm0011",
          "appId":"${{params.appId}}"
        }
      },
      "pagination": {
        "show": true
      },
      "thead": [
        {
          "prop": "policy_name",
          "type": "string",
          "label": "策略名称"
        },
        {
          "prop": "policy_id",
          "type": "string",
          "label": "策略编号"
        }
      ]
    }
  }
}
