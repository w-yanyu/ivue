{
  "module": "v-realonlyForm-btn",
  "layout": {
    "form": {
      "initRequest":"${{params}}",
      "scope": "searchTable",
      "controls": [
        {
          "wf_task_no": {
            "control": "string",
            "label": "任务编号",
            "width": "130px"
          },
          "wf_prod_no": {
            "control": "string",
            "label": "流程编号",
            "width": "130px"
          }
        },{
          "wf_prod_name": {
            "control": "string",
            "label": "流程名称",
            "width": "130px"
          },
          "task_define_id": {
            "control": "string",
            "label": "活动id",
            "width": "130px"
          }
        },{
          "task_define_name": {
            "control": "string",
            "label": "活动名称",
            "width": "130px"
          },
          "task_type": {
            "control": "string",
            "label": "任务类型",
            "width": "130px",
            "dict": {
              "dictType": "BPM_E_TASK_TYPE",
              "dictKey": [
                "BPM_E_TASK_TYPE"
              ],
              "format": "label"
            },
            "events" : {
              "showItem": {
                "TIMER" : ["task_time_rule","task_time_interval"]
              }
            }
          }
        },{"task_time_rule": {
            "control": "string",
            "label": "定时规则",
            "width": "130px",
            "dict": {
              "dictType": "BPM_E_TASK_TIME_RULE",
              "dictKey": [
                "BPM_E_TASK_TIME_RULE"
              ],
              "format": "label"
            }
          },
          "task_time_interval": {
            "control": "string",
            "label": "定时间隔",
            "width": "130px"
          }
        },{
          "task_url1": {
            "control": "string",
            "label": "编辑框架",
            "width": "130px"
          },
          "task_url2": {
            "control": "string",
            "label": "查看框架",
            "width": "130px"
          }
        },{
          "data_version": {
            "control": "string",
            "label": "版本号",
            "width": "130px"
          },
          "data_update_time": {
            "control": "string",
            "label": "最近修改日期",
            "width": "130px"
          }
        },{
          "data_update_user": {
            "control": "string",
            "label": "最近修改人",
            "width": "130px"
          },
          "remark": {
            "control": "string",
            "label": "流程备注",
            "width": "130px"
          }
        }
      ]
    }
  }
}
