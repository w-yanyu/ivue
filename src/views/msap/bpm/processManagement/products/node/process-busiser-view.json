{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [],
      "btns": []
    },
    "toolbar": {
      "btns": []
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "bpm4030",
          "appId": "${{params.appId}}",
          "task_define_id": "${{params.task_define_id}}",
          "wf_prod_no": "${{params.wf_prod_no}}",
          "bpm_version": "${{params.bpm_version}}",
          "task_deal_type": "0"
        }
      },
      "pagination": {
        "show": false
      },
      "thead": [
        {
          "label": "流程产品编号",
          "align": "center",
          "prop": "wf_prod_no"
        },
        {
          "label": "节点编号",
          "align": "center",
          "prop": "task_define_id"
        },
        {
          "label": "节点序号",
          "align": "center",
          "prop": "wf_task_no"
        },
        {
          "label": "业务方法编号",
          "align": "center",
          "prop": "busi_id"
        },
        {
          "label": "业务方法名称",
          "align": "center",
          "prop": "service_name"
        },
        {
          "label": "执行序号",
          "align": "center",
          "prop": "sort_no"
        }
      ]
    }
  }
}