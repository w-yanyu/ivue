{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "bpm0010",
          "wf_prod_no": "${{params.wf_prod_no}}",
          "trans_type": "SELECT",
          "appId":"${{params.appId}}"
        }
      },
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "bpm0010",
              "wf_part_no": "${{params.wf_part_no}}",
              "trans_type": "UPDATE",
              "appId":"${{params.appId}}"
            }
          }
        }
      ],
      "controls": [
        {
          "wf_part_name": {
            "control": "string",
            "label": "配置名称",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入配置名称"
              }
            ]
          },
          "wf_prod_no": {
            "control": "string",
            "label": "产品编号",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入产品编号"
              }
            ]
          },
          "wf_prod_name": {
            "control": "string",
            "label": "产品名称",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入产品名称"
              }
            ]
          },
          "task_define_id": {
            "control": "string",
            "label": "活动定义id",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入活动定义id"
              }
            ]
          },
          "task_define_name": {
            "control": "string",
            "label": "活动定义名称",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入活动定义名称"
              }
            ]
          },
          "select_type": {
            "initShow":true,
            "control": "select",
            "label": "处理方式",
            "width": "130px",
            "dict": {
              "dictType": "BPM_E_SELECT_TYPE",
              "dictKey": [
                "BPM_E_SELECT_TYPE"
              ],
              "format": "value-label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择处理方式"
              }
            ],
            "showItem": {
              "0": ["brch_type","wf_role_no","role_name"],
              "1": ["brch_type","wf_role_no","role_name"],
              "2": ["wf_role_no","brch_type","role_name","complete_type","complete_precent","pass_precent","reject_one"],
              "3": ["wf_role_no","brch_type","role_name","complete_type","complete_precent","pass_precent","reject_one"],
              "4": ["wf_role_no","brch_type","role_name","auto_select_rule"]
            },
            "events": {
              "condition": {
                  "AUTO": {
                      "control": {
                          "task_user_id": {
                              "disabled": true
                          }
                      },
                      "brch_type": {
                        "disabled": false,
                        "rules": [
                          {
                            "required": true
                          }
                        ]
                    }
                  },
                  "STARTER": {
                    "control": {
                        "brch_type": {
                            "disabled": true,
                            "rules": [
                              {
                                "required": false
                              }
                            ]
                        },
                        "task_user_id": {
                          "disabled": true,
                          "rules": [
                            {
                              "required": false
                            }
                          ]
                      }

                    }
                  },
                  "SELECT": {
                    "control": {
                        "wf_role_no": {
                            "disabled": false,
                            "rules": [
                              {
                                "required": true
                              }
                            ]
                        }
                    }
                  }
              }
            }
          },
          "brch_type": {
            "initShow":true,
            "control": "select",
            "label": "机构类型",
            "width": "130px",
            "dict": {
              "dictType": "BPM_E_BRCH_TYPE",
              "dictKey": [
                "BPM_E_BRCH_TYPE"
              ],
              "format": "value-label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择机构类型"
              }
            ],
            "showItem": {
              "APPOINT":["filter_brch_ind","branch_name"]
            }
          },
          "filter_brch_ind": {
            "initShow":false,
            "label": "处理机构",
            "width": "130px",
            "control": "lookup",
            "keyField": {
                "filter_brch_ind": "branch_id",
                "branch_name": "branch_name"
            },
            "toRequest": {
                "url": "/views/ismt/ct/busiAuth/authOperaLog/branch-info-manager_lookup.json",
                "method": "post"
            },
            "rules": [
              {
                "required": true,
                "message": "请输入处理机构"
              },
              {
                "max": 50,
                "message": "长度不能超过50个字符"
              }
            ]
          },
          "branch_name":{
            "initShow":false,
            "control": "string",
            "label": "处理机构名称",
            "width": "130px"
          },
          "wf_role_no":{
            "initShow":false,
            "control": "lookup",
            "label": "处理角色",
            "width": "130px",
            "edit": false,
            "keyField": {
                "wf_role_no": "role_id",
                "role_name": "role_name"
            },
            "toRequest": {
                "url": "/views/ismt/ct/roleManager/role-manager_lookup.json",
                "method": "post",
                "params":{
                  "appId":"${{params.appId}}"
                }

            },
            "rules": [
                {
                  "required": false
                }
            ]
          },
          "role_name":{
            "initShow":false,
            "control": "string",
            "label": "处理角色名称",
            "width": "130px"
          },
          "task_user_id": {
            "initShow":false,
            "control": "text",
            "label": "处理用户",
            "width": "130px",
            "rules": [
              {
                "required": false,
                "message": "请输入处理用户"
              },
              {
                "max": 50,
                "message": "长度不能超过50个字符"
              }
            ]
          },
          "opinion_view_type": {
            "control": "select",
            "label": "审批意见查看模式",
            "width": "130px",
            "dict": {
              "dictType": "BPM_E_OPINION_VIEW_TYPE",
              "dictKey": [
                "BPM_E_OPINION_VIEW_TYPE"
              ],
              "format": "value-label"
            }
          },
          "auto_select_rule": {
            "initShow":false,
            "control": "select",
            "label": "自动选人策略",
            "width": "130px",
            "dict": {
              "dictType": "BPM_E_AUTO_SELECT_RULE",
              "dictKey": [
                "BPM_E_AUTO_SELECT_RULE"
              ],
              "format": "value-label"
            },
            "events" : {
              "showItem": {
                "3" : ["service_impl"],
                "$default" : []
              }
            },
            "rules": [
              {
                "required": true,
                "message": "请选择自动选人策略"
              }
            ]
            
          },
          "service_impl": {
            "initShow":false,
            "control": "text",
            "label": "服务实现方法绝对路径",
            "width": "130px"
          },
          "complete_type": {
            "initShow":false,
            "control": "select",
            "label": "会签完成类型",
            "width": "130px",
            "dict": {
              "dictType": "BPM_E_COMPLETE_TYPE",
              "dictKey": [
                "BPM_E_COMPLETE_TYPE"
              ]
            }
          },
          "complete_precent": {
            "initShow":false,
            "control": "inputNumber",
            "label": "完成百分比(%)",
            "width": "130px",
            "min": 0,
            "max": 100
          },
          "pass_precent": {
            "initShow":false,
            "control": "inputNumber",
            "label": "通过百分比(%)",
            "width": "130px",
            "min": 0,
            "max": 100
          },
          "reject_one": {
            "initShow":false,
            "control": "select",
            "label": "是否一票否决",
            "width": "130px",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ]
            }
          },
          "data_version": {
            "control": "string",
            "label": "版本号",
            "width": "130px",
            "rules": [
              {
                "max": 50,
                "message": "长度不能超过50个字符"
              }
            ]
          },
          "remark": {
            "control": "textarea",
            "label": "备注",
            "width": "130px",
            "maxlength": 500,
            "showWordLimit": true
          }
        }
      ]
    }
  }
}