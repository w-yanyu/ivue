{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {          
          "id_": {
            "control": "text",
            "label": "流程编号",
            "rules": [
              {
                "max": 100,
                "message": "长度不能超过100个字符"
              }
            ]
          },
          "activity_name": {
            "control": "text",
            "label": "流程名称",
            "rules": [
              {
                "max": 100,
                "message": "长度不能超过100个字符"
              }
            ]
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "bpm0002",
          "appId": "${{params.appId}}"
        }
      },
      "thead": [
        {
          "label": "流程编号",
          "prop": "id_"
        },
        {
          "label": "流程名称",
          "prop": "name_"
        },
        {
          "label": "版本号",
          "prop": "version_"
        },
        {
          "label": "操作",
          "width": 70,
          "btns": [
            {
              "type": "bringback",
              "label": "选择"
            }
          ]
        }
      ]
    }
  }
}