{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest":"${{params}}",
      "btns": [],
      "controls": [
        
        {
          "bpm_version": {
            "control": "hidden",
            "label": "流程版本",
            "width": "130px"
          },
          "wf_prod_no": {
            "control": "string",
            "label": "流程产品编号",
            "width": "130px",
            "rules": [
              {
                "required": true
              }
            ]
          },
          "wf_prod_name": {
            "control": "string",
            "label": "流程产品名称",
            "width": "130px",
            "rules": [
              {
                "required": true
              }
            ]
          },
          "wf_prod_tp": {
            "control": "select",
            "disabled": true,
            "label": "流程产品类型",
            "width": "130px",
            "dict": {
              "dictType": "BPM_E_WFPDTP",
              "dictKey": [
                "BPM_E_WFPDTP"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true
              }
            ]
          },
          "start_mode": {
            "control": "select",
            "label": "流程启动模式",
            "width": "130px",
            "disabled": true,
            "dict": {
              "dictType": "BPM_E_STAR_MODE",
              "dictKey": [
                "BPM_E_STAR_MODE"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true 
                
              }
            ]
          },
          "wf_define_id": {
            "control": "lookup",
            "disabled": true,
            "label": "流程编号",
            "width": "130px",
            "prop": "actidd",
            "keyField":{
              "wf_define_id":"id_",
              "wf_define_name":"name_"
            },
            "toRequest": {
              "url": "/views/msap/bpm/processManagement/products/process-product-list.json",
              "method": "post",
              "params": {
                "appId":"${{params.appId}}"
              }
            },
            "props": {
              "title": "流程模板查询"
            },
            "rules": [
              {
                "required": true
              }
            ]
          },
          "wf_define_name": {
            "control": "string",
            "label": "流程名称",
            "width": "130px",
            "rules": [
              {
                "required": true
              }
            ]
          }
        }
      ]
    }
  }
}
