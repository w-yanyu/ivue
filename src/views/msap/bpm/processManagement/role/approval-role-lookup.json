{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "controls": [{
                "appr_role_id": {
                    "control": "text",
                    "label": "审批角色编号:"
                },
                "appr_role_name": {
                    "control": "text",
                    "label": "审批角色名称:"
                }
            }]
        },
        "toolbar": {
            "btns": []
        },
        "datagrid": {
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "bpm0053",
                    "appId":"${{params.appId}}"
                }
            },
            "thead": [{
                    "type": "selection",
                    "width": "55"
                }, {
                    "type": "index",
                    "label": "序号"
                }, {
                    "label": "审批角色编号",
                    "prop": "appr_role_id",
                    "align": "center"
                }, {
                    "label": "审批角色名称",
                    "prop": "appr_role_name",
                    "align": "center"
                },
                {
                    "label": "审批角色描述",
                    "prop": "appr_role_desc",
                    "align": "center"
                },
                
                {
                    "label": "操作",
                    "align": "center",
                    "width": "180",
                    "btns": [{
                        "type": "bringback",
                        "label": "选择"
                    }]
                }
            ]
        }
    }
}