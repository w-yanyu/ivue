{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "doRequest": {
                "url": "",
                "method": "POST",
                "params": {}
            },
            "controls": [
                {
                    "appId": {
                        "control": "select",
                        "label": "业务模块",
                        "selectFirstValue":true,
                        "dict": {
                            "dictType": "MS_APPID",
                            "dictKey": [
                                "MS_APPID"
                            ],
                            "format": "value-label"
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择业务模块"
                            }]
                    }
                },
                {
                    "wf_prod_no": {
                        "control": "text",
                          "label": "流程产品编号"
                    },
                    "wf_prod_name": {
                        "control": "text",
                        "label": "流程产品名称"
                    }
                }
            ]
        },
        "toolbar": {
            "btns": [
                {
                    "type": "add",
                    "target": "dialog",
                    "label": "新增",
                    "toRequest": {
                        "method": "GET",
                        "url": "/views/msap/bpm/processManagement/processQuick/processQuickProd/bpm_model_add.json",
                        "params":{
                            "appId":"${{form.appId}}"
                        }
                    },
                    "props": {
                    "title": "新增流程线路模板"
                    }
                }
            ]
          },
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": false,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "bpm3009"
                }
            },
            "thead": [
                {
                    "type": "index"
                },
                {
                    "label": "流程产品编号",
                    "prop": "wf_prod_no"
                },
                {
                    "label": "流程产品名称",
                    "prop": "wf_prod_name"
                },
                {
                    "prop": "wf_prod_tp",
                    "label": "流程产品类型",
                    "dict": {
                        "dictType": "BPM_E_WFPDTP",
                        "dictKey": [
                          "BPM_E_WFPDTP"
                        ]
                    }
                },
                {
                    "prop": "task_url1",
                    "label": "编辑框架"
                },
                {
                    "prop": "task_url2",
                    "label": "查看框架"
                },
                {
                    "label": "操作",
                    "fixed": "right",
                    "width": 200,
                    "align":"center",
                    "btns": [
                        {
                            "type": "delete",
                            "label": "删除",
                            "doRequest": {
                                "url": "/SUMP/icmcall/icmRPCCall",
                                "method": "post",
                                "params": {
                                    "servicecode": "bpm3011",
                                    "appId":"${{form.appId}}",
                                    "method":"del"
                                }
                            },
                            "confirm": {
                              "title": "提示信息",
                              "msg": "确定删除该信息吗？",
                              "confirmButtonText": "确定",
                              "cancelButtonText": "取消"
                            }
                        },
                        {
                            "type": "add",
                            "label": "维护",
                            "toRequest": {
                                "method": "GET",
                                "url": "/views/msap/bpm/processManagement/processQuick/processQuickProd/bpm_model_upd.json",
                                "params":{
                                    "appId":"${{form.appId}}"
                                }
                            },
                            "props": {
                                "title": "流程产品模板维护"
                            }
                        }
                    ]
                }
            ]
        }
    }
  }