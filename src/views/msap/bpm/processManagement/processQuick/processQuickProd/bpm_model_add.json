{
    "module": "v_inputDataForm_btn",
    "layout": {
      "form": {
        "btns": [
          {
            "type": "submit",
            "label": "提交",
            "icon": "",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "bpm3010",
                "appId":"${{params.appId}}"
              }
            }
          }
        ],
        "controls": [
          {
            "wf_prod_no": {
                "control": "text",
                "label": "流程产品编号",
                "width": "100px",
                "rules": [
                  {
                    "required": true,
                    "message": "请选择流程产品类型"
                  }
                ]
            },
            "wf_prod_name": {
                "control": "text",
                "label": "流程产品名称",
                "width": "100px",
                "rules": [
                  {
                    "required": true,
                    "message": "请选择流程产品类型"
                  }
                ]
            },
            "wfprod_tp_list": {
              "control": "select",
              "multiple":"true",
              "label": "流程产品类型",
              "width": "100px",
              "valueDataType":"string",
              "value":"01",
              "dict": {
                "dictType": "BPM_E_WFPDTP",
                "dictKey": [
                  "BPM_E_WFPDTP"
                ],
                "format": "label"
              },
              "rules": [
                {
                  "required": true,
                  "message": "请选择流程产品类型"
                }
              ]
            },
            "task_url1": {
              "control": "lookup",
              "label": "编辑框架",
              "width": "100px",
              "disabled": false,
              "edit": true,
              "value": "",
              "keyField": {
                "task_url1": "frame_cd"
              },
              "toRequest": {
                "url": "/views/msap/bpm/frameManagement/menu_list.json",
                "method": "get",
                "params": {
                  "appId":"${{params.appId}}"
                }
              },
              "props": {
                "title": "框架选择",
                "width": "80%"
              },
              "rules": [
                {
                  "required": true,
                  "message": "请选择框架编号"
                }
              ]
            },
            "task_url2": {
              "control": "lookup",
              "label": "查看框架",
              "width": "100px",
              "disabled": false,
              "edit": true,
              "value": "",
              "keyField": {
                "task_url2": "frame_cd"
              },
              "toRequest": {
                "url": "/views/msap/bpm/frameManagement/menu_list.json",
                "method": "get",
                "params": {
                  "appId":"${{params.appId}}"
                }
              },
              "props": {
                "title": "框架选择",
                "width": "80%"
              },
              "rules": [
                {
                  "required": true,
                  "message": "请选择框架编号"
                }
              ]
            }
          }
        ]
      }
    }
  }
  