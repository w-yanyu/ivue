{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "controls": [
                {
                    "task_define_id": {
                        "control": "text",
                          "label": "节点定义id"
                    },
                    "task_define_name": {
                        "control": "text",
                        "label": "节点定义名称"
                    }
                },
                {
                    "wf_prod_tp": {
                        "control": "select",
                          "label": "流程产品类型",
                          "dict": {
                            "dictType": "BPM_E_WFPDTP",
                            "dictKey": [
                              "BPM_E_WFPDTP"
                            ],
                            "format": "label"
                          }
                    },
                    "select_type": {
                        "control": "select",
                        "label": "处理方式",
                        "dict": {
                            "dictType": "BPM_E_SELECT_TYPE",
                            "dictKey": [
                              "BPM_E_SELECT_TYPE"
                            ],
                            "format": "label"
                          }
                    }
                }
            ]
        },
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": false,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "bpm3000",
                    "appId":"${{params.appId}}"
                }
            },
            "thead": [
                {
                    "label": "操作",
                    "align": "center",
                    "width": "200px",
                    "btns": [
                      {
                        "type": "bringback",
                        "label": "选择"
                      }
                    ]
                },
                {
                    "label": "节点定义id",
                    "prop": "task_define_id"
                },
                {
                    "label": "节点定义名称",
                    "prop": "task_define_name"
                },
                {
                  "label": "节点类型",
                  "prop": "task_mode",
                  "dict": {
                      "dictType": "BPM_E_TASK_MODE",
                      "dictKey": [
                        "BPM_E_TASK_MODE"
                      ],
                      "format": "label"
                    }
                },
                {
                    "label": "流程产品类型",
                    "prop": "wf_prod_tp",
                    "dict": {
                        "dictType": "BPM_E_WFPDTP",
                        "dictKey": [
                          "BPM_E_WFPDTP"
                        ],
                        "format": "label"
                      }
                },
                {
                  "label": "处理方式",
                  "prop": "select_type",
                  "dict": {
                    "dictType": "BPM_E_SELECT_TYPE",
                    "dictKey": [
                      "BPM_E_SELECT_TYPE"
                    ],
                    "format": "label"
                  }
                },
                {
                    "prop": "wf_role_no",
                    "label": "参与者角色编号"
                },
                {
                    "prop": "role_name",
                    "label": "参与者角色名称"
                },
                {
                    "prop": "opinion_view_type",
                    "label": "会签审批意见查看模式",
                    "dict": {
                        "dictType": "BPM_E_OPINION_VIEW_TYPE",
                        "dictKey": [
                          "BPM_E_OPINION_VIEW_TYPE"
                        ],
                        "format": "label"
                      }
                },
                {
                    "prop": "auto_select_rule",
                    "label": "自动选人策略",
                    "dict": {
                        "dictType": "BPM_E_AUTO_SELECT_RULE",
                        "dictKey": [
                          "BPM_E_AUTO_SELECT_RULE"
                        ],
                        "format": "label"
                      }
                },
                {
                    "prop": "service_impl",
                    "label": "服务实现方法绝对路径"
                }
            ]
        }
    }
  }