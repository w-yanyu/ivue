{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "doRequest": {
                "url": "",
                "method": "POST",
                "params": {}
            },
            "controls": [
                {
                    "appId": {
                        "control": "select",
                        "label": "业务模块",
                        "selectFirstValue":true,
                        "dict": {
                            "dictType": "MS_APPID",
                            "dictKey": [
                                "MS_APPID"
                            ],
                            "format": "value-label"
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择业务模块"
                            }]
                    }
                },
                {
                    "task_define_id": {
                        "control": "text",
                          "label": "节点定义编号"
                    },
                    "task_define_name": {
                        "control": "text",
                        "label": "节点定义名称"
                    }
                },
                {
                    "select_type": {
                        "control": "select",
                        "label": "处理方式",
                        "dict": {
                            "dictType": "BPM_E_SELECT_TYPE",
                            "dictKey": [
                              "BPM_E_SELECT_TYPE"
                            ],
                            "format": "label"
                          }
                    }
                }
            ]
        },
        "toolbar": {
            "btns": [
                {
                    "type": "add",
                    "target": "dialog",
                    "label": "新增",
                    "toRequest": {
                        "method": "GET",
                        "url": "/views/msap/bpm/processManagement/processQuick/processQuickPart/bpm_model_add.json",
                        "params":{
                            "appId":"${{form.appId}}"
                        }
                    },
                    "props": {
                    "title": "新增流程参与者模板"
                    }
                }
            ]
          },
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": false,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "bpm3000"
                }
            },
            "thead": [
                {
                    "type": "index"
                },
                {
                    "label": "节点定义编号",
                    "prop": "task_define_id"
                },
                {
                    "label": "节点定义名称",
                    "prop": "task_define_name"
                },
                {
                    "label": "节点模式",
                    "prop": "task_mode",
                    "dict": {
                        "dictType": "BPM_E_TASK_MODE",
                        "dictKey": [
                          "BPM_E_TASK_MODE"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "节点类型",
                    "prop": "task_type",
                    "dict": {
                        "dictType": "BPM_E_TASK_TYPE",
                        "dictKey": [
                          "BPM_E_TASK_TYPE"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "流程产品类型",
                    "prop": "wf_prod_tp"
                },
                {
                  "label": "处理方式",
                  "prop": "select_type",
                  "dict": {
                    "dictType": "BPM_E_SELECT_TYPE",
                    "dictKey": [
                      "BPM_E_SELECT_TYPE"
                    ],
                    "format": "label"
                  }
                },
                {
                    "prop": "wf_role_no",
                    "label": "参与者角色编号"
                },
                {
                    "prop": "role_name",
                    "label": "参与者角色名称"
                },
                {
                    "prop": "opinion_view_type",
                    "label": "会签审批意见查看模式",
                    "dict": {
                        "dictType": "BPM_E_OPINION_VIEW_TYPE",
                        "dictKey": [
                          "BPM_E_OPINION_VIEW_TYPE"
                        ],
                        "format": "label"
                      }
                },
                {
                    "prop": "auto_select_rule",
                    "label": "自动选人策略",
                    "dict": {
                        "dictType": "BPM_E_AUTO_SELECT_RULE",
                        "dictKey": [
                          "BPM_E_AUTO_SELECT_RULE"
                        ],
                        "format": "label"
                      }
                },
                {
                    "prop": "service_impl",
                    "label": "服务实现方法绝对路径"
                },
                {
                    "label": "操作",
                    "fixed": "right",
                    "width": 200,
                    "align":"center",
                    "btns": [
                        {
                            "type": "delete",
                            "label": "删除",
                            "doRequest": {
                                "url": "/SUMP/icmcall/icmRPCCall",
                                "method": "post",
                                "params": {
                                    "servicecode": "bpm3003",
                                    "appId":"${{form.appId}}",
                                    "method":"del"
                                }
                            },
                            "confirm": {
                              "title": "提示信息",
                              "msg": "确定删除该信息吗？",
                              "confirmButtonText": "确定",
                              "cancelButtonText": "取消"
                            }
                        },
                        {
                            "type": "add",
                            "label": "维护",
                            "toRequest": {
                                "method": "GET",
                                "url": "/views/msap/bpm/processManagement/processQuick/processQuickPart/bpm_model_upd.json",
                                "params":{
                                    "appId":"${{form.appId}}"
                                }
                            },
                            "props": {
                                "title": "参与者模板维护"
                              }
                        }
                    ]
                }
            ]
        }
    }
  }