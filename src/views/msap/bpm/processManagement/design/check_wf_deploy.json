{
    "module": "v-inputDataForm-btn",
    "layout": {
        "form": {
            "name":"check_wf",
            "initRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "chkact",
                    "proc_def_id": "${{params.id_}}"
                }
            },
            "btns": [],
            "controls": [
                {
                    "id_": {
                        "control": "text",
                        "label": "流程部署编号",
                        "disabled":true,
                        "span": 22,
                        "value":"${{params.id_}}"
                    }
                },
                {
                    "warn_msg": {
                        "control": "textarea",
                        "label": "校验结果",
                        "span": 22,
                        "rows":10
                    }
                }
            ]
        }
    }
}