{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "btns": [
        {
            "type": "submit",
            "label": "提交",
            "icon": "",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "post",
              "params": {
                  "servicecode": "bpm2011",
                  "appId":"${{params.appId}}",
                  "proc_def_id":"${{params.proc_def_id}}"
              }
            },
            "confirm":{
              "title":"提示信息",
              "msg":"将以此流程图为基础在【流程图编辑】中生成数据，成功后请转至【流程图编辑】中对流程图进行修改部署！",
              "confirmButtonText":"确定",
              "cancelButtonText":"取消"
            }
        }
      ],
      "controls": [
        {
          "model_name": {
            "control": "text",
            "label": "流程模板中文名称",
            "rules": [
              {
                "required": true,
                "message": "请输入流程模板中文名称"
              }
            ]
          },
          "model_key": {
            "control": "text",
            "label": "流程模板英文名称",
            "rules": [
              {
                "required": true,
                "message": "请输入流程模板英文名称"
              }
            ]
          }
        }
      ]
    }
  }
}
