{
    "module": "v_inputDataForm_btn",
    "layout": {
      "form": {
        "doRequest": {
          "url": "/SUMP/call/uploadForDP",
          "method": "post",
          "params": {
            "servicecode": "bpm2007",
            "appId":"${{params.appId}}"
          }
        },
        "btns":[
          {
            "type":"submit",
            "label":"提交"
          }
        ],
        "controls": [
          {
            "file": {
              "initShow": true,
              "width": "130px",
              "control": "file",
              "label": "部署文件",
              "btnLable": "请选择部署文件",
              "multiple": false,
              "rules": [
                {
                  "required": true,
                  "message": "请上传部署文件"
                }
              ]
            }
          }
        ]
      }
    }
  }
  