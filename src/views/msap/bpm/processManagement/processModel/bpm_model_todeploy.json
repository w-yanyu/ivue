{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest":"${{params}}",
      "btns": [
        {
            "type": "submit",
            "label": "提交",
            "icon": "",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "bpm2008",
                "appId": "${{params.appId}}"
              }
            }
        }
      ],
      "controls": [
        {
          "model_id": {
            "control": "string",
            "label": "流程模板编号"
          },
          "model_name": {
            "control": "string",
            "label": "流程模板中文名称"
          }
        },
        {
          "model_content": {
            "label": "本次修订内容",
            "control": "textarea",
            "width": "180px",
            "align": "left",
            "span": 24,
            "rows": 5,
            "maxlength": 500,
            "showWordLimit": 500
          }
        }
      ]
    }
  }
}
