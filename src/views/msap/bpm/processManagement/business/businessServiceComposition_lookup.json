{
  "module": "v_search_btn_datagrid",
  "layout": {
      "form": {
          "controls": [
            {
              "busi_id": {
                "control": "text",
                "label": "业务方法编号",
                "rules": [
                  {
                    "max": 500,
                    "message": "长度不能超过500个字符"
                  }
                ]
              },
              "busi_kind": {
                "control": "select",
                "label": "业务场景种类",
                "dict": {
                  "dictType": "BPM_E_BUSI_KIND",
                  "dictKey": [
                    "BPM_E_BUSI_KIND"
                  ],
                  "format": "value-label"

                }
              },
              "busi_method_call_type": {
                "control": "select",
                "label": "业务方法调用类型",
                "dict": {
                  "dictType": "BPM_E_CALL_BUSI_TYPE",
                  "dictKey": [
                    "BPM_E_CALL_BUSI_TYPE"
                  ],
                  "format": "value-label"
                }
              },
              "service_name": {
                "control": "text",
                "label": "服务名称",
                "rules": [
                  {
                    "max": 500,
                    "message": "长度不能超过500个字符"
                  }
                ]
              }
            }
          ]
      },
      "toolbar": {
          "btns": []
      },
      "datagrid": {
          "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                  "servicecode": "bpm0036",
                  "appId":"${{params.appId}}"
              }
          },
          "thead": [
              {
                  "type": "index",
                  "label": "序号"
              },
              {
                "label": "业务方法编号",
                "prop": "busi_id",
                "align": "center"
              },
              {
                "label": "业务场景种类",
                "prop": "busi_kind",
                "align": "center",
                "dict": {
                  "dictType": "BPM_E_BUSI_KIND",
                  "dictKey": [
                    "BPM_E_BUSI_KIND"
                  ]
                },
                "format": "value-label"
              },
              {
                "label": "业务方法调用类型",
                "prop": "busi_method_call_type",
                "align": "center",
                "dict": {
                  "dictType": "BPM_E_CALL_BUSI_TYPE",
                  "dictKey": [
                    "BPM_E_CALL_BUSI_TYPE"
                  ]

                }
              },
              {
                "label": "服务名称",
                "prop": "service_name",
                "align": "center"
              },
              {
                "label": "服务实现方法绝对路径",
                "prop": "service_impl",
                "align": "center"
              },
              {
                "label": "内部服务码",
                "prop": "inner_service_code",
                "align": "center"
              },
              {
                "label": "方法对应的伪代码描述",
                "prop": "remark",
                "align": "center"
              },

              {
                  "label": "操作",
                  "align": "center",
                  "width": "180",
                  "btns": [{
                      "type": "bringback",
                      "label": "选择"
                  }]
              }
          ]
      }
  }
}
