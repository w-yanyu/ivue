{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "scope": "formData",
      "initRequest":"${{params}}",
      "btns": [
        {
          "type": "submit",
          "label": "保存",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "bpm0034",
              "busi_id": "${{params.busi_id}}",
              "appId":"${{params.appId}}"
            }
          }
        }
      ],
      "controls": [

        {
          "busi_id": {
            "label": "业务编号",
            "control": "string",
            "width": "130px",
            "span":12
          },
          "busi_type": {
            "label": "业务类型",
            "control": "select",
            "width": "130px",
            "span":12,
            "disabled": false,
            "dict": {
              "dictType": "BPM_E_WFPDTP",
              "dictKey": [
                "BPM_E_WFPDTP"
              ]
            },
            "format": "value-label",
            "rules": [
              {
                "required": false,
                "message": "请输入业务编号"
              },
              {
                "max": 64,
                "message": "长度不能超过64个字符"
              }
            ]
          }
        },
        {
          "busi_kind": {
            "label": "业务场景种类",
            "control": "select",
            "width": "130px",
            "span": 12,
            "dict": {
              "dictType": "BPM_E_BUSI_KIND",
              "dictKey": [
                "BPM_E_BUSI_KIND"
              ],
              "format": "value-label"

            },
            "rules": [
              {
                "required": true,
                "message": "请选择业务场景种类"
              }
            ]
          },
          "busi_method_call_type": {
            "control": "select",
            "label": "业务方法调用类型",
            "width": "130px",
            "span": 12,
            "dict": {
              "dictType": "BPM_E_CALL_BUSI_TYPE",
              "dictKey": [
                "BPM_E_CALL_BUSI_TYPE"
              ],
              "format": "value-label"

            },
            "rules": [
              {
                "required": true,
                "message": "请选择业务方法调用类型"
              }
            ],
            "showItem": {
              "M": [
                "service_impl"
              ],
              "S": [
                "busi_service_call_id",
                "inner_service_code"
              ]
            }
          }
        },
        {
          "service_name": {
            "control": "text",
            "label": "服务名称",
            "width": "130px",
            "disabled": false,
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请输入服务名称"

              },
              {
                "max": 500,
                "message": "长度不能超过500个字符"
              }
            ]
          },
          "service_impl": {
            "control": "text",
            "initShow": false,
            "label": "服务实现方法绝对路径",
            "width": "130px",
            "disabled": false,
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请输入服务实现方法绝对路径"
              },
              {
                "max": 500,
                "message": "长度不能超过500个字符"
              }
            ]
          }
        },
        {
          "busi_service_call_id": {
            "control": "text",
            "initShow": false,
            "label": "业务服务调用标识",
            "width": "130px",
            "disabled": false,
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请输入业务服务调用标识"
              },
              {
                "max": 500,
                "message": "长度不能超过500个字符"
              }
            ]
          },
          "inner_service_code": {
            "control": "text",
            "initShow": false,
            "label": "内部服务码",
            "width": "130px",
            "disabled": false,
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请输入内部服务码"
              },
              {
                "max": 100,
                "message": "长度不能超过100个字符"
              }
            ]
          }
        },
        {
          "remark": {
            "label": "方法对应的伪代码描述",
            "control": "textarea",
            "width": "130px",
            "span":24,
            "autosize":{
              "minRows": 4
            },
            "rules": [
              {
                "required": true,
                "message": "请输入备注"
              },
              {
                "max": 100,
                "message": "长度不能超过100个字符"
              }
            ]
          }
        }
      ]
    }
  }
}

