{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "scope": "templateScope",
      "init": {
        "initRequest": "${{params}}"
      },
      "initDataOnce": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "ar0131"
        }
      },
      "btns": [
        {
          "type": "submit",
          "label": "提交"
        }
      ],
      "controls": [
        {
          "frame_cd": {
            "label": "框架编号",
            "control": "string",
            "value": "${{params.frame_cd}}",
            "width": "130px"
          },
          "func_cd": {
            "label": "功能编号",
            "control": "lookup",
            "width": "130px",
            "keyField":{
              "func_cd":"menu_id",
              "vue_path":"page_id",
              "tree_menu_name":"menu_desc"
            },
            "toRequest": {
              "url": "/views/ismt/ct/menu/lookup/menu_info_by_parent_lookup.json",
              "method": "post",
              "params": {
                "menu_upper_id":"KF1"

              }
            },
            "props": {
              "title": "菜单功能查询",
              "width": "65%"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择功能编号"
              }
            ]
          },
          "vue_path": {
            "label": "页面路径",
            "control": "text",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入页面路径"
              }
            ]
          },
          "tree_menu_name": {
            "label": "菜单名称",
            "control": "text",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入菜单名称"
              }
            ]
          },
          "tree_seq": {
            "label": "菜单序号",
            "control": "text",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入菜单名称"
              }
            ]
          },
          "is_enabled": {
            "control": "select",
            "label": "是否有效",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            },
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请选择是否有效"
              }
            ]
          },
          "channel_id": {
            "width": "130px",
            "dict": {
              "dictKey": [
                "E_CHANNEL_LIST"
              ],
              "dictType": "E_CHANNEL_LIST"
            },
            "control": "select",
            "format": "value-label",
            "label": "所属渠道",
            "rules": [
              {
                "required": true,
                "message": "请选择所属渠道"
              }]
          }
        }
      ]
    }
  }
}
