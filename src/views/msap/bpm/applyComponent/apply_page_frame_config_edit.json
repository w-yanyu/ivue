{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "scope": "templateScope",
      "initRequest": "${{params}}",
      "initDataOnce": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "ar0128"
        }
      },
      "btns": [
        {
          "type": "submit",
          "label": "提交"
        }
      ],
      "controls": [
        {
          "prod_name": {
            "control": "text",
            "label": "产品名称",
            "width": "130px",
            "disabled":true
          },
          "prod_cd": {
            "control": "text",
            "label": "产品编号",
            "width": "130px",
            "disabled":true
          },
          "frame_cd": {
            "label": "框架编号",
            "control": "string",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入框架编号"
              }
            ]
          },
          "frame_desc": {
            "label": "框架名称",
            "control": "text",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入框架描述"
              }
            ]
          },"apply_type": {
            "control": "text",
            "label": "申请类型",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入申请类型"
              }
            ]
        },
          "is_enabled": {
            "control": "select",
            "label": "是否生效",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            },
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请选择是否生效"
              }
            ]
          }
        }
      ]
    }
  }
}
