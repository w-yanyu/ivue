{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [],
      "btns": []
    },
    "toolbar": {
      "btns": []
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "bpm0070",
          "frame_cd": "${{params.frame_cd}}",
          "appId":"${{params.appId}}"
        }
      },
      "thead": [
        {
          "label": "流程产品编号",
          "prop": "wf_prod_no"
        },
        {
          "label": "流程产品名称",
          "prop": "wf_prod_name"
        },
        {
          "label":"流程节点名称",
          "prop":"task_define_name"
        }
      ]
    }
  }
}
