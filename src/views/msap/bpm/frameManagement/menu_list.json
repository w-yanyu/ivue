{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "frame_cd": {
            "label": "框架编号",
            "control": "text",
            "rules": [
              {
                "max": 30,
                "message": "长度不能超过30个字符"
              }
            ]
          },
          "frame_desc": {
            "label": "框架名称",
            "control": "text",
            "rules": [
              {
                "max": 50,
                "message": "长度不能超过50个字符"
              }
            ]
          }
        }
      ],
      "foldLineNumber": 3
    },
    "datagrid": {
      "isInitSearch": true,
      "scope": "searchTable",
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "bpm0044",
          "appId":"${{params.appId}}",
          "is_enabled":"1"
        }
      },
      "thead": [
        {
          "label": "操作",
          "fixed": "left",
          "btns": [
            {
              "type": "bringback",
              "label": "选择"
            }
          ]
        },
        {
          "label": "框架编号",
          "prop": "frame_cd",
          "align": "center"
        },
        {
          "label": "框架描述",
          "prop": "frame_desc",
          "align": "center"
        }
      ],
      "pagination": {
        "show": true
      }
    }
  }
}
