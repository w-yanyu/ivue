{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "controls": [
                {
                    "appId": {
                        "span": 8,
                        "control": "select",
                        "label": "业务模块",
                        "selectFirstValue":true,
                        "dict": {
                            "dictType": "MS_APPID",
                            "dictKey": [
                                "MS_APPID"
                            ],
                            "format": "value-label"
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择业务模块"
                            }]
                    }
                },
                {
                    "qry_user_id": {
                        "initShow":false,
                        "span": 8,
                        "control": "lookup",
                        "label": "用户编号",
                        "keyField": {
                            "qry_user_name": "user_name",
                            "qry_user_id": "user_id"
                          },
                          "toRequest": {
                            "url": "/views/ce/paramManage/assign/collTeam/user_manager_lookup.json",
                            "method": "get"
                          },
                          "props": {
                            "title": "用户编号"
                          }
                    },
                    "qry_user_name": {
                        "initShow":false,
                        "span": 8,
                        "control": "string",
                        "label": "用户名称"
                    },
                    "date_from": {
                        "span": 8,
                        "control": "dateTimePicker",
                        "label": "开始日期"
                    },
                    "date_end": {
                        "span": 8,
                        "control": "dateTimePicker",
                        "label": "结束日期"
                    }
                }

            ]
        },
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": false,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "bpm1024",
                    "appId":"${{form.appId}}"
                }
            },
            "thead": [
                {
                    "label": "序号",
                    "type": "index"
                },
                {
                    "label": "用户名称",
                    "prop": "user_name"
                },
                {
                    "label": "待办数量",
                    "prop": "to_do_count"
                },
                {
                    "label": "排名",
                    "prop": "cur_rank"
                }
            ]
        }
    }
}

