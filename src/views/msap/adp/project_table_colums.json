{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ms0210",
          "table_full_id": "${{params.table_full_id}}",
          "appId": "${{params.appId}}"
        }
      },
      "pagination": {
        "show": false
      },
      "thead": [
        {
          "label": "字段编号",
          "prop": "column_name",
          "align": "center"
        },
        {
          "label": "字段名称",
          "prop": "column_desc",
          "align": "center"
        },
        {
          "label": "字段类型",
          "prop": "column_type",
          "align": "center"
        },
        {
          "label": "字段长度",
          "prop": "column_length",
          "align": "center"
        }
      ]
    }
  }
}