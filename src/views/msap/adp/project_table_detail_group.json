{
  "module": "v-realonlyForm-btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/call/RPCCall",
        "method": "POST",
        "params": {
          "appId": "${{params.appId}}",
          "table_full_id": "${{params.table_full_id}}",
          "servicecode": "ms0202"
        }
      },
      "controlsGroup": [
        {
          "title": "表信息",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "accordion": false,
          "expand": true,
          "controls": [
            {
              "id4": {
                "control": "dynamic",
                "isShowChildForm": false,
                "span": 24,
                "toRequest": {
                  "url": "/views/msap/adp/project_table.json",
                  "method": "get",
                  "params": { 
                    "table_name":"${{params.table_name}}",
                    "table_desc":"${{params.table_desc}}",
                    "table_full_id":"${{params.table_full_id}}"
                }
                 
                }
              }
            }
          ]
        },
        {
          "title": "索引信息",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "accordion": false,
          "expand": true,
          "controls": [
            {
              "id2": {
                "control": "dynamic",
                "isShowChildForm": false,
                "span": 24,
                "toRequest": {
                  "url": "/views/msap/adp/project_table_odbs.json",
                  "method": "get",
                  "params": {
                    "table_full_id":"${{params.table_full_id}}",
                    "appId": "${{params.appId}}"
                  }
                }
              }
            }
          ]
        },
        {
          "title": "字段信息",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "accordion": false,
          "expand": true,
          "controls": [
            {
              "id3": {
                "control": "dynamic",
                "isShowChildForm": false,
                "span": 24,
                "toRequest": {
                  "url": "/views/msap/adp/project_table_colums.json",
                  "method": "get",
                  "params": {
                    "table_full_id":"${{params.table_full_id}}",
                    "appId": "${{params.appId}}"

                  }
                }
              }
            }
          ]
        }
      ]
    }
  }
}