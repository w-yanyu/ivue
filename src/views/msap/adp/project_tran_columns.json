{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ms0211",
          "flow_tran_id": "${{params.flow_tran_id}}",
          "query_type": "${{params.query_type}}",
          "appId": "${{params.appId}}"
        }
      },
      "pagination": {
        "show": false
      },
      "thead": [
        {
          "label": "字段编号",
          "prop": "column_name",
          "align": "center"
        },
        {
          "label": "字段描述",
          "prop": "column_desc",
          "align": "center"
        },
        {
          "label": "字段类型",
          "prop": "column_type",
          "align": "center"
        },
        {
          "label": "是否必输",
          "prop": "enable_ind",
          "align": "center",
          "dict": {
            "dictType": "MS_E_YESORNO",
            "dictKey": [
              "MS_E_YESORNO"
            ],
            "format": "value-label"
          }
        }
        
      ]
    }
  }
}