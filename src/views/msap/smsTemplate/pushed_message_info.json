{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "btns": [],
      "controls": [
        {
          "msg_seq_no": {
            "control": "string",
            "label": "消息流水号",
            "width": "100px",
            "span": 8
          },
          "msg_id": {
            "control": "string",
            "label": "返回消息编号",
            "width": "100px",
            "span": 8
          },
          "send_no": {
            "control": "string",
            "label": "发送编号",
            "width": "100px",
            "span": 8
          },
          "payload": {
            "control": "string",
            "label": "推送负载",
            "width": "100px",
            "span": 8
          },
          "activity_id": {
            "control": "string",
            "label": "活动编号",
            "width": "100px",
            "span": 8
          },
          "audience": {
            "control": "string",
            "label": "推送范围",
            "width": "100px",
            "span": 8
          },
          "content": {
            "control": "textarea",
            "label": "内容",
            "width": "100px",
            "span": 24,
            "disabled": true,
            "autosize": {
              "minRows": 4,
              "maxRows": 6
            }
          },
          "title": {
            "control": "string",
            "label": "标题",
            "width": "100px",
            "span": 8
          },
          "platform": {
            "control": "string",
            "label": "平台",
            "width": "100px",
            "span": 8,
            "dict": {
              "dictType": "CT_E_PLATFORM",
              "dictKey": [
                "CT_E_PLATFORM"
              ],
              "format": "label"
            }
          },
          "msg_type": {
            "control": "select",
            "label": "消息类型",
            "width": "100px",
            "span": 8,
            "disabled": true,
            "dict": {
              "dictType": "CT_E_MSG_TP",
              "dictKey": [
                "CT_E_MSG_TP"
              ],
              "format": "label"
            }
          },
          "task_type": {
            "control": "select",
            "label": "消息任务类型",
            "width": "100px",
            "span": 8,
            "disabled": true,
            "dict": {
              "dictType": "CT_E_MSG_TASK_TP",
              "dictKey": [
                "CT_E_MSG_TASK_TP"
              ],
              "format": "label"
            }
          },
          "extras": {
            "control": "textarea",
            "label": "附加信息",
            "width": "100px",
            "span": 24,
            "disabled": true,
            "autosize": {
              "minRows": 4,
              "maxRows": 6
            }
          },
          "intent": {
            "control": "string",
            "label": "跳转页面",
            "width": "100px",
            "span": 24
          },
          "channel": {
            "control": "string",
            "label": "下发渠道",
            "width": "100px",
            "span": 8
          },
          "schedule_id": {
            "control": "string",
            "label": "定时计划编号",
            "width": "100px",
            "span": 8
          },
          "schedule_start_date": {
            "control": "string",
            "label": "定时起始时间",
            "width": "100px",
            "span": 8
          },
          "schedule_end_date": {
            "control": "string",
            "label": "定时结束时间",
            "width": "100px",
            "span": 8
          },
          "schedule_time": {
            "control": "string",
            "label": "定时时间",
            "width": "100px",
            "span": 8
          },
          "msg_status": {
            "control": "select",
            "label": "消息状态",
            "width": "100px",
            "span": 8,
            "disabled": true,
            "dict": {
              "dictType": "CT_E_MSG_PUSU_STATUS",
              "dictKey": [
                "CT_E_MSG_PUSU_STATUS"
              ],
              "format": "label"
            }
          }
        }
      ]
    }
  }
}
