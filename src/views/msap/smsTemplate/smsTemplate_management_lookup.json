{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "scope": "formData",
      "controls": [
        {
          "template_desc": {
            "control": "text",
            "label": "模板描述"
          }
        },{
          "template_id": {
            "control": "text",
            "label": "模板编号"
          },
          "template_name": {
            "control": "text",
            "label": "模板名称"
          }
        }
      ]
    },

    "toolbar": {
      "btns": [
      ]
    },

    "datagrid": {
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "ms0094",
          "template_type":"SMS",
          "appId":"plOnl"
        }
      },
      "thead": [
        {
          "type":"string",
          "label": "模板编号",
          "align":"center",
          "prop": "template_id"
        },
        {
          "type":"string",
          "label": "模板名称",
          "align":"center",
          "prop": "template_name"
        },
        {
          "type":"string",
          "label": "模板描述",
          "align":"center",
          "prop": "template_desc"
        },
        {
          "type":"string",
          "label": "模板内容",
          "align":"center",
          "prop": "template_content"
        },
        {
          "type":"string",
          "label": "发送方式",
          "align":"center",
          "prop": "send_type",
          "dict":{
            "dictType": "MS_E_SEND_TYPE",
            "dictKey": [
              "MS_E_SEND_TYPE"
            ],
            "format": "value-label"

          }
        },
        {
          "label": "操作",
          "fixed": "right",
          "align": "center",
          "btns": [
            {
              "type": "bringback",
              "label": "选择"
            }
          ]
        }
      ]
    }
  }
}
