{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "msg_seq_no": {
            "control": "text",
            "label": "消息流水号",
            "span": 8
          },
          "platform": {
            "control": "select",
            "label": "平台",
            "span": 8,
            "dict": {
              "dictType": "CT_E_PLATFORM",
              "dictKey": [
                "CT_E_PLATFORM"
              ],
              "format": "label"
            }
          },
          "msg_type": {
            "control": "select",
            "label": "消息类型",
            "span": 8,
            "dict": {
              "dictType": "CT_E_MSG_TP",
              "dictKey": [
                "CT_E_MSG_TP"
              ],
              "format": "label"
            }
          },
          "task_type": {
            "control": "select",
            "label": "消息任务类型",
            "span": 8,
            "dict": {
              "dictType": "CT_E_MSG_TASK_TP",
              "dictKey": [
                "CT_E_MSG_TASK_TP"
              ],
              "format": "label"
            }
          },
          "channel": {
            "control": "hidden",
            "label": "下发渠道",
            "span": 8
          },
          "msg_status": {
            "control": "select",
            "label": "消息状态",
            "span": 8,
            "dict": {
              "dictType": "CT_E_MSG_PUSU_STATUS",
              "dictKey": [
                "CT_E_MSG_PUSU_STATUS"
              ],
              "format": "label"
            }
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ct2562"
        }
      },
      "thead": [
        {
          "type": "string",
          "label": "消息流水号",
          "prop": "msg_seq_no",
          "align": "center",
          "width": "250px"
        },
        {
          "type": "string",
          "label": "返回消息编号",
          "prop": "msg_id",
          "align": "center",
          "width": "180px"
        },
        {
          "type": "string",
          "label": "发送编号",
          "prop": "send_no",
          "align": "center",
          "width": "150px"
        },
        {
          "type": "string",
          "label": "内容",
          "prop": "content",
          "align": "center"
        },
        {
          "type": "string",
          "label": "标题",
          "prop": "title",
          "align": "center"
        },
        {
          "type": "string",
          "label": "平台",
          "prop": "platform",
          "align": "center",
          "width": "150px",
          "dict": {
            "dictType": "CT_E_PLATFORM",
            "dictKey": [
              "CT_E_PLATFORM"
            ],
            "format": "label"
          }
        },
        {
          "type": "string",
          "label": "消息类型",
          "prop": "msg_type",
          "align": "center",
          "dict": {
            "dictType": "CT_E_MSG_TP",
            "dictKey": [
              "CT_E_MSG_TP"
            ],
            "format": "label"
          }
        },
        {
          "type": "string",
          "label": "消息任务类型",
          "prop": "task_type",
          "align": "center",
          "dict": {
            "dictType": "CT_E_MSG_TASK_TP",
            "dictKey": [
              "CT_E_MSG_TASK_TP"
            ],
            "format": "label"
          }
        },
        {
          "type": "string",
          "label": "下发渠道",
          "prop": "channel",
          "align": "center"
        },
        {
          "type": "string",
          "label": "消息状态",
          "prop": "msg_status",
          "align": "center",
          "dict": {
            "dictType": "CT_E_MSG_PUSU_STATUS",
            "dictKey": [
              "CT_E_MSG_PUSU_STATUS"
            ],
            "format": "label"
          }
        },
        {
          "label": "操作",
          "align": "center",
          "width": "100px",
          "btns": [
            {
              "type": "look",
              "label": "详情",
              "toRequest": {
                "url": "/views/msap/smsTemplate/pushed_message_info.json",
                "method": "GET"
              },
              "props": {
                "width": "50%",
                "title": "推送消息详情"
              }
            }
          ]
        }
      ]
    }
  }
}
