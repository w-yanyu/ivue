{
    "module": "v-realonlyForm-btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "controls": [
                {
                  "template_type": {
                    "control": "select",
                    "disabled":true,
                    "label": "模板类型",
                    "default":"MAIN",
                    "width": "130px",
                    "dict": {
                      "dictType": "MS_E_TEMPLATE_TYPES",
                      "dictKey": [
                        "MS_E_TEMPLATE_TYPES"
                      ],
                      "format": "label"

                    },
                    "format": "value-label",
                    "rules": [
                      {
                        "required": false,
                        "message":"请输入"
                      },
                      {
                        "max":10,
                        "message":"最多可以输入 10 个字节"
                      }
                    ]
                  },
                    "template_id": {
                        "control": "string",
                        "disabled":true,
                        "label": "模板编号",
                        "inputWidth": "200px"
                    }
                },
                {
                    "template_name": {
                        "control": "string",
                        "label": "模板名称",
                        "width": "130px"
                    },
                    "template_desc": {
                        "control": "string",
                        "label": "模板描述",
                        "inputWidth": "200px"
                    }
                },
                {
                  "send_type": {
                    "control": "select",
                    "label": "发送方式",
                    "width": "130px",
                    "disabled":true,
                    "default":"TIME",
                    "dict": {
                      "dictType": "MS_E_SEND_TYPE",
                      "dictKey": [
                        "MS_E_SEND_TYPE"
                      ],
                      "format": "label"
                    },
                    "rules": [
                      {
                        "required": false,
                        "message":"请输入"
                      }
                    ]
                  }

                },
                {
                    "template_content": {
                        "control": "mce",
                        "disabled":true,
                        "label": "模板内容",
                        "labelWidth": "8",
                        "span":"20",
                        "autosize":"{ minRows: 2, maxRows: 4}",
                        "rules": [
                            {
                                "required": false
                            }
                        ]
                    }
                }
            ]
        }
    }
}
