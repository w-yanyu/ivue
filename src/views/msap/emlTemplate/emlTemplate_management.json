{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "scope": "formData",
            "controls": [
                {
                    "appId": {
                        "control": "select",
                        "label": "业务模块",
                        "selectFirstValue":true,
                        "dict": {
                            "dictType": "MS_APPID",
                            "dictKey": [
                                "MS_APPID"
                            ],
                            "format": "value-label"
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择业务模块"
                            }]
                    },
                    "template_desc": {
                        "control": "text",
                        "label": "模板描述"
                    }
                  },{
                    "template_id": {
                        "control": "text",
                        "label": "模板编号"
                    },
                    "template_name": {
                        "control": "text",
                        "label": "模板名称"
                    }
                }
            ]
        },

        "toolbar": {
            "btns": [
                {
                    "type": "add",
                    "icon": "el-icon-circle-plus",
                    "target": "dialog",
                    "label": "新增",
                    "toRequest": {
                        "method": "GET",
                        "url": "/views/msap/emlTemplate/emlTemplate_add.json",
                        "params": {
                          "appId":"${{formData.appId}}"
                        }
                    },
                    "props": {
                        "width": "80%",
                        "title": "新增信邮模板"
                    }
                }
            ]
        },

        "datagrid": {
            "isInitSearch": false,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "ms0094",
                    "template_type":"MAIN"
                }
            },
            "thead": [
                {
                    "type":"string",
                    "label": "模板编号",
                    "align":"center",
                    "prop": "template_id"
                },
                {
                    "type":"string",
                    "label": "模板名称",
                    "align":"center",
                    "prop": "template_name"
                },
                {
                    "type":"string",
                    "label": "模板描述",
                    "align":"center",
                    "prop": "template_desc"
                },
              {
                "type":"string",
                "label": "发送方式",
                "align":"center",
                "prop": "send_type",
                "dict":{
                  "dictType": "MS_E_SEND_TYPE",
                  "dictKey": [
                    "MS_E_SEND_TYPE"
                  ],
                  "format": "value-label"

                }
              },
                {
                    "label": "操作",
                    "fixed": "right",
                    "width":"200%",
                    "align":"center",
                    "btns": [
                        {
                            "type":"look",
                            "label":"详情",
                            "toRequest":{
                                "url":"/views/msap/emlTemplate/emlTemplate_info.json",
                                "method":"POST"
                            },
                            "props":{
                                "title":"信邮模板详情",
                                "width": "80%"
                            }
                        },
                        {
                            "type": "edit",
                            "label": "维护",
                            "toRequest": {
                                "url": "/views/msap/emlTemplate/emlTemplate_modify.json",
                                "method": "POST",
                                "params": {
                                    "appId":"${{formData.appId}}"
                                  }
                            },
                            "props": {
                                "title": "信邮模板维护",
                                "width": "80%"
                            }
                        },
                        {
                            "type": "delete",
                            "label": "删除",
                            "doRequest": {
                                "url": "/SUMP/icmcall/icmRPCCall",
                                "method": "POST",
                                "params": {
                                    "servicecode": "ms0091",
                                    "template_type": "MAIN",
                                    "appId":"${{formData.appId}}"
                                }
                            }
                        }
                    ]
                }
            ]
        }
    }
}
