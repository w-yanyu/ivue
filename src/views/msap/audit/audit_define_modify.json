{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "btns": [
                {
                    "type": "reviewBtn",
                    "label": "提交",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "ms0071",
                            "appId":"${{params.appId}}"
                        }
                    }
                }
            ],

            "controls": [
                {
                    "table_name": {
                        "control": "string",
                        "label": "数据库表名",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message":"请输入"
                            },
                            {
                                "max":30,
                                "message":"最多可以输入 30 个字节"
                            }
                        ]
                    },
                    "table_desc": {
                        "control": "text",
                        "label": "数据库表描述",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message":"请输入"
                            }
                        ]
                    }
                },
                {
                    "is_registered_changed_log": {
                        "control": "select",
                        "disabled":false,
                        "label": "登记变更日志标志",
                        "width": "130px",
                        "dict": {
                        "dictType": "MS_E_YESORNO",
                        "dictKey": [
                          "MS_E_YESORNO"
                        ],
                        "format": "label"

                        },

                        "rules": [
                            {
                                "required": true,
                                "message":"请输入"
                            },
                            {
                                "max":10,
                                "message":"最多可以输入 10 个字节"
                            }
                        ]
                    },
                    "audit_type": {
                        "control": "select",
                        "disabled":false,
                        "label": "审计类型",
                        "width": "130px",
                        "dict": {
                        "dictType": "MS_E_TABLETYPE",
                        "dictKey": [
                          "MS_E_TABLETYPE"
                        ],
                        "format": "value-label"

                        },

                        "rules": [
                            {
                                "required": true,
                                "message":"请输入"
                            },
                            {
                                "max":10,
                                "message":"最多可以输入 10 个字节"
                            }
                        ]
                    }
                }
               
            ]
        }
    }
}
