{
	"module": "v_search_btn_datagrid",
	"layout": {
		"form": {
			"controls": [{
				"appId": {
					"control": "select",
					"label": "业务模块",
					"selectFirstValue": true,
					"dict": {
						"dictType": "MS_APPID",
						"dictKey": [
							"MS_APPID"
						],
						"format": "value-label"
					},
					"rules": [{
						"required": true,
						"message": "请选择业务模块"
					}]
				}
			}, {
				"audit_seq": {
					"control": "text",
					"label": "审计流水"
				},
				"audit_type": {
					"control": "select",
					"label": "审计类型",
					"selectFirstValue": false,
					"dict": {
						"dictType": "MS_E_TABLETYPE",
						"dictKey": [
							"MS_E_TABLETYPE"
						],
						"format": "value-label"
					}
				}
			}, {
				"table_operator_type": {
					"control": "select",
					"label": "表数据操作类型",
					"selectFirstValue": false,
					"dict": {
						"dictType": "MS_E_TABLE_OPERATOR_TYPE",
						"dictKey": [
							"MS_E_TABLE_OPERATOR_TYPE"
						],
						"format": "value-label"
					}
				},
				"trxn_date": {
					"control": "dateTimePicker",
					"label": "交易日期",
					"valueFormat": "yyyyMMdd"
				}
			}, {
				"busi_seq": {
					"control": "text",
					"label": "业务流水"
				},
				"trxn_seq": {
					"control": "text",
					"label": "交易流水"
				}
			}, {
				"trxn_code": {
					"control": "text",
					"label": "交易码"
				},
				"trxn_desc": {
					"control": "text",
					"label": "交易描述"
				}
			}, {
				"table_name": {
					"control": "text",
					"label": "数据库表名"
				},
				"table_desc": {
					"control": "text",
					"label": "数据库表描述"
				}
			}, {
				"channel_id": {
					"control": "text",
					"label": "渠道编号"
				},
				"prim_key": {
					"control": "text",
					"label": "主键"
				}
			}, {
				"data_create_user": {
					"control": "text",
					"label": "数据创建用户"
				}
			}]
		},

		"toolbar": {
			"btns": [

			]
		},

		"datagrid": {
			"isInitSearch": false,
			"doRequest": {
				"url": "/SUMP/icmcall/icmRPCCall",
				"method": "post",
				"params": {
					"servicecode": "ms0073"
				}
			},
			"thead": [{
					"type": "string",
					"label": "审计流水",
					"align": "center",
					"prop": "audit_seq"
				},
				{
					"type": "string",
					"label": "审计类型",
					"align": "center",
					"prop": "audit_type",
					"dict": {
						"dictType": "MS_E_TABLETYPE",
						"dictKey": [
							"MS_E_TABLETYPE"
						],
						"format": "value-label"

					}
				},
				{
					"type": "string",
					"label": "表数据操作类型",
					"align": "center",
					"prop": "table_operator_type",
					"dict": {
						"dictType": "MS_E_TABLE_OPERATOR_TYPE",
						"dictKey": [
							"MS_E_TABLE_OPERATOR_TYPE"
						],
						"format": "value-label"

					}
				},
				{
					"type": "string",
					"label": "交易日期",
					"align": "center",
					"prop": "trxn_date"
				},
				{
					"type": "string",
					"label": "业务流水",
					"align": "center",
					"prop": "busi_seq"
				},
				{
					"type": "string",
					"label": "交易流水",
					"align": "center",
					"prop": "trxn_seq"
				},
				{
					"type": "string",
					"label": "交易码",
					"align": "center",
					"prop": "trxn_code"
				},
				{
					"type": "string",
					"label": "交易描述",
					"align": "center",
					"prop": "trxn_desc"
				},
				{
					"type": "string",
					"label": "数据库表名",
					"align": "center",
					"prop": "table_name"
				},
				{
					"type": "string",
					"label": "数据库表描述",
					"align": "center",
					"prop": "table_desc"
				},
				{
					"type": "string",
					"label": "渠道ID",
					"align": "center",
					"prop": "channel_id"
				},
				{
					"type": "string",
					"label": "主键",
					"align": "center",
					"prop": "prim_key"
				},
				{
					"type": "string",
					"label": "法人代码",
					"align": "center",
					"prop": "org_id"
				},
				{
					"type": "string",
					"label": "数据创建时间",
					"align": "center",
					"prop": "data_create_time"
				},
				{
					"type": "string",
					"label": "数据更新时间",
					"align": "center",
					"prop": "data_update_time"
				},
				{
					"type": "string",
					"label": "数据创建用户",
					"align": "center",
					"prop": "data_create_user"
				},
				{
					"type": "string",
					"label": "数据更新用户",
					"align": "center",
					"prop": "data_update_user"
				},
				{
					"type": "string",
					"label": "数据版本号",
					"align": "center",
					"prop": "data_version"
				},
				{
					"label": "操作",
					"fixed": "right",
					"width": "200%",
					"align": "center",
					"btns": [{
							"type": "look",
							"label": "明细",
							"toRequest": {
								"url": "/views/msap/audit/audit_register_detail.json",
								"method": "POST",
								"params": {
									"appId": "${{form.appId}}"
								}
							},
							"props": {
								"width": "70%",
								"title": "审计明细"
							}
						},
						{
							"type": "look",
							"label": "详情",
							"toRequest": {
								"url": "/views/msap/audit/audit_register_info.json",
								"method": "POST",
								"params": {
									"appId": "${{form.appId}}"
								}
							},
							"props": {
								"width": "70%",
								"title": "审计详情"
							}
						}
					]
				}
			]
		}
	}
}