{
    "module": "v_search_btn_datagrid",
    "layout": {
      "form": {
        "controls": [
            
            
            {
                "insuranceCode": {
                    "control": "text",
                    "label": "保险公司编码",
                    "rules": [
                        {
                            "max": 80,
                            "message": "长度不能超过20个字符"
                        }
                    ]
                },
                "insuranceName": {
                    "control": "text",
                    "label": "保险公司名称",
                    "rules": [
                        {
                            "max": 80,
                            "message": "长度不能超过80个字符"
                        }
                    ]
                }}
        ]
      },
      "toolbar": {
        "btns": [
          {
            "type": "add",
            "target": "dialog",
            "label": "新增",
            "toRequest": {
              "method": "GET",
              "url": "/views/qb/reginsurance/addInsurance.json"
            },
            "props": {
              "title": "新增保险公司"
            }
          }
  
        ]
      },
      "datagrid": {
        "scope": "searchTable",
        "isInitSearch": false,
        "doRequest": {
          "url": "/SUMP/icmcall/icmRPCCall",
          "method": "post",
          "params": {
            "servicecode": "selInsurance",
            "isparentnode": "Y"
          }
        },
        "thead": [
          {
            "label": "序号",
            "type": "index",
            "align": "center"
          },
          {
            "label": "保险公司主键",
            "prop": "insurId",
            "align": "center"
          },
          {
            "label": "保险公司编码",
            "prop": "insuranceCode",
            "align": "center"
          },
          {
            "label": "保险公司名称",
            "prop": "insuranceName",
            "align": "center"
          },
         
         
          {
            "label": "操作",
            "fixed": "right",
            "align": "center",
            "btns": [
              {
                "type": "delete",
                "label": "删除",
                "doRequest": {
                  "url": "/SUMP/icmcall/icmRPCCall",
                  "method": "POST",
                  "params": {
                    "servicecode": "delInsurance"
                  }
                }
              },
              {
                "type": "edit",
                "label": "修改",
                "toRequest": {
                  "url": "/views/qb/reginsurance/upInsurance.json",
                  "method": "post"
                },
                "props": {
                  "width": "65%",
                  "title": "修改"
                }
              }
            ]
          }
        ]
      }
    }
  }
  