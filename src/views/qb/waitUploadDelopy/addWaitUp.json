{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "scope": "formData",
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": "",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "addWaitUp"

                        }
                    }
                }
            ],
            "controls": [
                {

                    
                        "nodeCode": {
                          "control": "select",
                          "label": "节点编号",
                          "dict": {
                            "dictKey": [
                              "E_NODECODE"
                            ],
                            "dictType": "E_NODECODE",
                            "format": "label"
                          },
                          "rules": [
                            {
                              "required": true,
                              "message": "请选择节点编号"
                            }
                          ]
                         
                        },
                        "custGroupType": {
                            "control": "select",
                            "label": "客群级别",
                            "dict": {
                              "dictKey": [
                                "E_CUSTGROUPCODE"
                              ],
                              "dictType": "E_CUSTGROUPCODE",
                              "format": "label"
                            },
                            "rules": [
                              {
                                "required": true,
                                "message": "请选择客群级别"
                              }
                            ]
                           
                           
                          },
                          "fileTypeCode": {
                            "control": "select",
                            "label": "附件类型编号",
                            "dict": {
                              "dictKey": [
                                "E_FILETYPECODE"
                              ],
                              "dictType": "E_FILETYPECODE",
                              "format": "label"
                            },
                            "rules": [
                              {
                                "required": true,
                                "message": "请选择附件类型编号"
                              }
                            ]
                          },
                  
                  "fileCode": {
                    "label": "附件",
                    "control": "remoteSelect",
                    "valueField": "postid",
                    "labelField": "postspecies",
                    "doRequest": {
                      "url": "/SUMP/icmcall/icmRPCCall",
                      "method": "post",
                      "params": {
                        "servicecode": "selmateriaicf",
                        "isparentnode": "Y",
                        "typeofdate":"FJ",
                        "length": 100
                        
                      }
                    },
                    "rules": [
                      {
                        "required": true,
                        "message": "请选择附件"
                      }
                    ]
    
                  }
                }

            ]
        }
    }
}
