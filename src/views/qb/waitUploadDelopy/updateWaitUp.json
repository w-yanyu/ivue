{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
          "initRequest": "${{params}}",
            "scope": "formData",
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": "",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "updateWaitUp"

                        }
                    }
                }
            ],
            "controls": [
                {

                    "nodeCode": {
                        "span": 16,
                        "control": "select",
                        "label": "节点",   
                        "disabled": true,                    
                        "dict": {
                          "dictKey": [
                            "E_NODECODE"
                          ],
                          "dictType": "E_NODECODE",
                          "format": "label"
                        }
                       
                      },
                  "fileTypeCode": {
                    "label": "附件类型编号",
                    "control": "hidden",
                    "disabled": true
                  },
                  "fileTypeName": {
                    "span": 16,
                    "label": "附件类型名称",
                    "control": "text",
                    "disabled": true
                  },
                  "fileCode": {
                    "label": "附件编码",
                    "control": "hidden",
                    "disabled": true
                  },
                  "fileName": {
                    "span": 16,
                    "label": "附件名称",
                    "control": "text",
                    "disabled": true
                  },
                  "custGroupType": {
                    "span": 16,
                    "label": "客群级别",
                    "control": "select",
                    "dict": {
                      "dictKey": [
                        "E_CUSTGROUPCODE"
                      ],
                      "dictType": "E_CUSTGROUPCODE",
                      "format": "label"
                    }
                    ,
                     "rules": [
                      {
                       "required": true,
                       "message": "请选择客群级别"
                        }
                     ]
                  }
                }

            ]
        }
    }
}



