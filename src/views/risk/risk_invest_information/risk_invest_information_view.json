{
	"module": "v-search-btn-datagrid",
	"layout": {
		"datagrid": {
			"isInitSearch": true,
			"doRequest": {
				"url": "/API/risk-management-service/risk-invest-information/page-select",
				"method": "post",
				"params": {
					"investOwnerId": "${{params.investOwnerId}}"
				}
			},
			"thead": [
				{
					"label": "调查信息ID",
					"prop": "investId",
					"align": "center"
				},
				{
					"label": "调查日期",
					"prop": "investDate",
					"align": "center"
				},
				{
					"label": "调查动作",
					"prop": "investAction",
					"align": "center",
					"dict": {
						"dictType": "investActionSource",
						"dictKey": [],
						"format": "label",
						"doRequest": {
							"url": "/views/risk/dictdata/investAction.json",
							"method": "get"
						}
					}
				},
				{
					"label": "调查人员",
					"prop": "investOperator",
					"align": "center"
				},
				{
					"label": "调查地点",
					"prop": "investAddress",
					"align": "center"
				},
				{
					"label": "调查开始时间",
					"prop": "investStartTime",
					"align": "center",
					"type": "dateTimePicker",
					"format": "yyyy-MM-dd HH:mm:ss"
				},
				{
					"label": "调查结束时间",
					"prop": "investEndTime",
					"align": "center",
					"type": "dateTimePicker",
					"format": "yyyy-MM-dd HH:mm:ss"
				},
				{
					"label": "调查备注",
					"prop": "investRemark",
					"align": "center"
				}
			]
		}
	}
}