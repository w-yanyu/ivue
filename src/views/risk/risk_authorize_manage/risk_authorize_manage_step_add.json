{
  "module": "d-steps",
  "layout": {
    "steps": {
      "scope": "formObj",
      "alignCenter": true,
      "finishStatus": "success",
      "simpleType": false,
      "nextStepType": "submit",
      "skip": false,
      "list": [
        {
          "scope": "step1",
          "title": "授权基本信息",
          "icon": "el-icon-edit",
          "toRequest": {
            "url": "@Wrapper",
            "method": "get",
            "params": {
              "systemId": "${{formObj.step1.systemId}}"
            }
          },
          "btnsLable":{
            "submit":"下一步"
          },
          "props": {},
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "post",
            "params": {
              "servicecode": "deci090103",
              "isFirst":"${{formObj.step1.isFirst}}"
            }
          }
        },
        {
          "scope": "step2",
          "forbidBack": false,
          "title": "服务授权详情",
          "icon": "el-icon-edit",
          "toRequest": {
            "url": "@AuthorizeRelation",
            "method": "get",
            "params": {
              "systemId": "${{formObj.step1.systemId}}",
              "systemBranchId": "${{formObj.step1.systemBranchId}}"
            }
          },
          "props": {},
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "post",
            "params": {
              "servicecode": "deci090108",
              "systemId": "${{formObj.step1.systemId}}"
            }
          },
          "resultSet": {
            "systemId":"systemId",
            "systemName":"systemName",
            "systemBranchId":"systemBranchId"
          }
        }
      ]
    }
  }
}
