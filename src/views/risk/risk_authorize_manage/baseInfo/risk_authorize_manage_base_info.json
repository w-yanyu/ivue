{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "controls": [
        {
          "systemName": {
            "control": "text",
            "label": "系统名称",
            "width": "200px",
            "span": "18",
            "disabled": true,
            "rules": [
              {
                "required": true,
                "message": "请输入系统名称"
              }
            ]
          },
          "systemId": {
            "control": "text",
            "label": "系统编号标识",
            "width": "200px",
            "span": "18",
            "disabled": true,
            "rules": [
              {
                "required": true,
                "message": "请输入系统编号标识"
              }
            ]
          },
          "systemBranchId": {
            "control": "select",
            "label": "系统所属机构",
            "width": "200px",
            "span": "18",
            "filterable": true,
            "disabled": true,
            "dict": {
              "dictType": "E_RISK_DOCKING_SYSTEM",
              "dictKey": [
                "E_RISK_DOCKING_SYSTEM"
              ],
              "format": "label"
            }
          },
          "remark": {
            "control": "textarea",
            "showWordLimit":"200",
            "maxlength":"200",
            "label": "备注",
            "width": "200px",
            "align": "center",
            "span": "18",
            "rows": 6,
            "placeholder": "请输入",
            "disabled": true
          }
        }
      ]
    }
  }
}
