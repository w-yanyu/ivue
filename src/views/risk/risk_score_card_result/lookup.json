{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "score_card_type": {
            "label": "评分卡类型",
            "control": "select",
            "dict": {
              "dictType": "scoreCardType",
              "dictKey": [],
              "format": "label",
              "doRequest": {
                "url": "/views/risk/dictdata/scoreCardType.json",
                "method": "get"
              }
            },
            "span": 8
          },
          "score_card_code": {
            "label": "评分卡代码",
            "control": "text",
            "span": 8
          },
          "score_card_name": {
            "label": "评分卡名称",
            "control": "text",
            "span": 8
          }
        },
        {
          "branch_id": {
            "label": "机构名称",
            "control": "remoteSelect",
            "labelField": "branch_name",
            "valueField": "branch_id",
            "searchField":"branch_name",
            "span": 8,
            "value": "$g{{user_info.branch_id}}",
            
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "post",
              "params": {
                "servicecode": "ct2191"
              }
            },
            "rules": [
              {
                "required": true,
                "message": "请输入机构名称",
                "max": 125
              }
            ],
            "format": "label"
          },
          "busi_version": {
            "label": "版本号",
            "control": "inputNumber",
            "decimal": "2",
            "min": 0,
            "step": 0.1,
            "span": 8
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "scoreCardStatus":"",
          "servicecode": "deci030007"
        }
      },
      "pagination": {
        "hideOnSinglePage": false,
        "show": true,
        "pageSizes": [10, 15, 20],
        "pageSize": 10
      },
      "thead": [
        {
          "label": "评分卡类型",
          "prop": "scoreCardType",
          "dict": {
            "dictType": "scoreCardType",
            "dictKey": [],
            "format": "label",
            "doRequest": {
              "url": "/views/risk/dictdata/scoreCardType.json",
              "method": "get"
            }
          }
        },
        {
          "label": "评分卡代码",
          "prop": "scoreCardCode"
        },
        {
          "label": "评分卡名称",
          "prop": "scoreCardName"
        },
        {
          "label": "机构名称",
          "prop": "branch_id",
          "dict": {
            "dictType": "DECI_E_BRANCH_MARK",
            "dictKey": [
              "DECI_E_BRANCH_MARK"
            ],
            "format": "label"
          }
        },
        {
          "label": "版本号",
          "prop": "version"
        },
        {
          "label": "最后维护时间",
          "prop": "updateTime"
        },
        {
          "label": "操作",
          "width": "150px",
          "btns": [
            {
              "type": "bringback",
              "label": "选择"
            }
          ]
        }
      ]
    }
  }
}
