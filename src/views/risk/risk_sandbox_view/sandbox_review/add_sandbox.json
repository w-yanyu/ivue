{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/call/RPCCall",
            "method": "POST",
            "params": {
              "servicecode": "deci050071"
            }
          }
        }
      ],
      "controls": [
        {
          "sandbox_code": {
            "control": "text",
            "label": "沙箱代码",
            "width": "auto",
            "rules": [
              {
                "required": true,
                "message": "请输入沙箱代码"
              },
              {
                "max": 40,
                "message": "长度不能超过40个字符"
              }
            ]
          },
          "sandbox_name": {
            "control": "text",
            "label": "沙箱名称",
            "width": "auto",
            "rules": [
              {
                "required": true,
                "message": "请输入沙箱名称"
              },
              {
                "max": 100,
                "message": "长度不能超过100个字符"
              }
            ]
          }
        },{
          "sandbox_type": {
            "control": "select",
            "label": "沙箱类型",
            "width": "auto",
            "dict": {
              "dictType": "DECI_E_SANDBOX_TYPE",
              "dictKey": [
                "DECI_E_SANDBOX_TYPE"
              ],
              "format": "value - label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择沙箱类型"
              }
            ]
          },
          "sandbox_status": {
            "control": "select",
            "label": "沙箱状态",
            "width": "auto",
            "dict": {
              "dictType": "DECI_E_SANDBOX_STATUS",
              "dictKey": [
                "DECI_E_SANDBOX_STATUS"
              ],
              "format": "value - label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择沙箱状态"
              }
            ]
          }
        },
        {
          "sandbox_desc": {
            "control": "textarea",
            "label": "沙箱描述",
            "width": "auto",
            "span": 24,
            "autosize": {
              "minRows": 3,
              "maxRows": 5
            },
            "maxlength": 500,
            "showWordLimit": true,
            "rules": [
              {
                "required": true,
                "message": "请输入沙箱描述"
              },
              {
                "max": 500,
                "message": "长度不能超过500个字符"
              }
            ]
          }
        }
      ]
    }
  }
}
