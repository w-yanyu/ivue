{
  "module": "v-form-editableDataGrid-btn",
  "init": {
    "initRequest": "${{params}}"
  },
  "layout": {
    "#toolbar": {
      "#btnPlace": "right",
      "btns": [
        {
          "type": "add",
          "label": "新增",
          "icon": "el-icon-plus"
        }
      ]
    },
    "datagrid": {
      "name": "allData",
      "pagination": {
        "show": false
      },
      "initRequest": "${{params.allData}}",
      "thead": [
        {
          "prop": "contact_person_no",
          "label": "字段代码",
          "type": "text",
          "disabled": true,
          "value": "字段代码"
        },
        {
          "prop": "contact_person_name",
          "label": "字段名称",
          "type": "text",
          "disabled": true,
          "value": "字段名称"
        },
        {
          "prop": "contact_info",
          "label": "字段类型",
          "type": "text",
          "disabled": true,
          "value": "字段类型"
        },
        {
          "prop": "aaa",
          "label": "精度",
          "type": "text",
          "disabled": true,
          "value": "精度"
        },
        {
          "prop": "bbb",
          "label": "描述",
          "type": "text",
          "disabled": true,
          "value": "描述"
        },
        {
          "prop": "ccc",
          "label": "默认值",
          "type": "text",
          "disabled": "'${{params.pageType}}' === 'look'"
        },
        {
          "label": "操作",
          "width": "70px",
          "btns": [
            {
              "type": "delete",
              "label": "删除"
            }
          ],
          "align": "center"
        }
      ]
    }
  }
}
