{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "controls": [
        {
          "source_name": {
            "label": "数据源名称",
            "width": "auto",
            "control": "select",
            "dict": {
              "format": "label",
              "dictKey": [
                "DECI_E_DATA_SOURCE_NAME"
              ],
              "dictType": "DECI_E_DATA_SOURCE_NAME"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择数据源"
              }
            ]
          },
          "business_scene": {
            "label": "业务场景",
            "width": "auto",
            "control": "select",
            "dict": {
              "format": "label",
              "dictKey": [
                "DECI_E_BUSINESS_SCENE"
              ],
              "dictType": "DECI_E_BUSINESS_SCENE"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择业务场景"
              }
            ]
          }
        },
        {
          "field_group_code": {
            "label": "字段分组代码",
            "width": "auto",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入结果清单代码"
              },
              {
                "max": 40,
                "message": "长度不能超过40个字符"
              }
            ]
          },
          "field_group_name": {
            "label": "字段分组名称",
            "width": "auto",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入数据源结果清单名称"
              },
              {
                "max": 100,
                "message": "长度不能超过100个字符"
              }
            ]
          }
        }
      ],
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "deci050094",
          "sandbox_id": "${{params.sandbox_id}}"
        }
      }
    }
  }
}
