{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "scope":"formData",
      "initRequest": "${{params}}",
      "controls": [
        {
          "business_scene": {
            "label": "业务场景",
            "control": "select",
            "dict": {
              "dictType": "DECI_E_BUSINESS_SCENE",
              "dictKey": [
                "DECI_E_BUSINESS_SCENE"
              ],
              "format": "value - label"
            }
          },
          "result_code": {
            "label": "字段代码",
            "control": "text",
            "span": 12
          }
        },
        {
          "result_name": {
            "label": "字段名称",
            "control": "text",
            "span": 24
          }
        },
        {
          "allData": {
            "label": "返回结果字段",
            "initShow": true,
            "span": 24,
            "control": "dynamic",
            "toRequest": {
              "url": "/views/risk/risk_sandbox_view/sandbox_review/data_source_table_edit_table.json",
              "params": {
                "pageType": "edit"
              }
            },
            "props": {}
          }
        }
      ],
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": ""
        }
      }
    }
  }
}
