{
  "module": "v_search_btn_datagrid",
  "pageId": "risk_habit_model_parameter",
  "layout": {
    "form": {
      "controls": [
        {
          "habitModelId": {
            "label": "行为模型ID",
            "control": "text",
            "span": 8,
            "rules": [
              {
                "max": 20,
                "message": "请正确输入"
              }
            ]
          },
          "segmentId": {
            "label": "行为模型分段ID",
            "control": "text",
            "span": 8,
            "rules": [
              {
                "max": 2,
                "message": "请正确输入"
              }
            ]
          },
          "dataType": {
            "label": "数据类型",
            "control": "select",
            "span": 8,
            "disabledOptions":["Boolean"],
            "dict": {
              "dictType": "dataType",
              "dictKey": [],
              "format": "value - label",
              "doRequest": {
                "url": "/views/risk/dictdata/dataType.json",
                "method": "get"
              }
            }
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "authId": "habit_model_parameter_add_btn",
          "label": "新增",
          "toRequest": {
            "url": "/views/risk/risk_habit_model_parameter/risk_habit_model_parameter_add.json",
            "method": "get"
          },
          "props": {
            "width": "65%",
            "title": "新增"
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/API/risk-management-service/risk-habit-model-parameter/select-page",
        "method": "post"
      },
      "thead": [
        {
          "label": "行为模型ID",
          "prop": "habitModelId"
        },
        {
          "label": "行为模型分段ID",
          "prop": "segmentId"
        },
        {
          "label": "数据类型",
          "prop": "dataType",
          "dict": {
            "dictType": "dataType",
            "dictKey": [],
            "doRequest": {
              "url": "views/risk/dictdata/dataType.json"
            }
          }
        },
        {
          "label": "分段值1",
          "prop": "segmentValue1"
        },
        {
          "label": "分段值2",
          "prop": "segmentValue2"
        },
        {
          "label": "操作",
          "width": "150px",
          "btns": [
            {
              "type": "edit",
              "authId": "habit_model_parameter_upd_btn",
              "label": "编缉",
              "toRequest": {
                "url": "/views/risk/risk_habit_model_parameter/risk_habit_model_parameter_edit.json",
                "method": "get"
              },
              "props": {
                "width": "70%",
                "title": "编缉"
              }
            },
            {
              "type": "look",
              "label": "查看",
              "toRequest": {
                  "url": "/views/risk/risk_habit_model_parameter/risk_habit_model_parameter_view.json",
                "method": "get"
              },
              "props": {
                "width": "70%",
                "title": "查看"
              }
            },
            {
              "type": "delete",
              "authId": "habit_model_parameter_del_btn",
              "label": "删除",
              "doRequest": {
                "url": "/API/risk-management-service/risk-habit-model-parameter/delete",
                "method": "post"
              }
            }
          ]
        }
      ],
      "pagination": {
        "show": true,
        "pageSizes": [
          "10",
          "50",
          "100",
          "200"
        ],
        "pageSize": 10
      }
    }
  }
}
