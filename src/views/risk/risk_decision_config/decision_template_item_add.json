{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/call/RPCCall",
        "method": "post",
        "params": {
          "servicecode": "pd1101",
          "due_dili_template_no": "${{params.node_cd}}",
          "due_dili_template_name": "${{params.node_name}}"
        }
      },
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "#servicecode": "pd1102",
          "node_type": "${{params.node_type}}",
          "servicecode": "pd0214",
          "cust_class_list": "10|20|30|70|99",
          "prod_type": "CL",
          "sale_prod_cd": "0000001"
        }
      },
      "controls": [ 
        {
          "due_dili_template_no": {
            "width": "25px",
            "span": "8",
            "control": "select",
            "disabled": false,
            "label": "熔断触发字段",
            "rules": [
              {
                "required": true,
                "message": "必填项"
              }
            ]
          },
          "due_dili_template_name": {
            "width": "25px",
            "control": "select",
            "span": "8",
            "label": "条件符",
            "rules": [
              {
                "required": true,
                "message": "必填项"
              }
            ]
          },
          "is_effective": {
            "width": "25px",
            "control": "text",
            "span": "8",
            "label": "字段值",
            "rules": [
              {
                "required": true,
                "message": "必填项"
              }
            ]
          }
        },
        {
          "modify_path": {
            "width": "25px",
            "span": "6",
            "control": "text",
            "label": "熔断时长",
            "show":{
              "expression":"'${{params.is_leaf}}' === '1'"
            },
            "rules": [
              {
                "required": true,
                "message": "必填项"
              }
            ]
          }
        },
        {
          "due_dili_template_no": {
            "width": "25px",
            "span": "12",
            "control": "select",
            "label": "熔断结果字段",
            "show":{
              "expression":"'${{params.is_leaf}}' === '1'"
            },
            "rules": [
              {
                "required": true,
                "message": "必填项"
              }
            ]
          },
          "report_path": {
            "width": "25px",
            "span": "12",
            "control": "text",
            "label": "字段值",
            "show":{
              "expression":"'${{params.is_leaf}}' === '1'"
            },
            "rules": [
              {
                "required": true,
                "message": "必填项"
              }
            ]
          }
        }
      ]
    }
  }
}
