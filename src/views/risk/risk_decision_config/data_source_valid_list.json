{
    "module": "v-form-editableDataGrid-btn",
    "layout": { 
        
        "datagrid": {
            "name": "sourceValidList",
            "isInitSearch": false,
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "post",
              "params": {
                "servicecode": "deci090208",
                "model_id": "${{params.node_cd}}"
              }
            },
            "pagination": {
                "show": false
            },
            "allData":true,
            "thead": [
                {
                    "type": "index",
                    "label": "序号"
                },
                {
                    "label": "数据源代码",
                    "prop": "data_source_code",
                    "type": "text",
                    "disabled":true
                  },
                  {
                    "label": "数据源名称",
                    "prop": "data_source_name",
                    "type": "text",
                    "disabled":true
                  },
                  {
                    "prop": "data_source_invoke",
                    "type": "select",
                    "label": "数据源调用原则",
                    "options": [
                        {
                            "value": "C",
                            "label": "成本优先"
                        },
                        {
                            "value": "E",
                            "label": "效率优先"
                        },
                        {
                            "value": "A",
                            "label": "默认成本优先"
                        }
                    ],
                    "#rules": [
                        {
                          "required": true,
                          "message": "请输入"
                        }
                      ]
                },
                  {
                    "label": "默认数据有效期",
                    "prop": "default_validity_day",
                    "type": "text",
                    "#rules": [
                        {
                          "required": true,
                          "message": "请输入"
                        }
                      ]
                  }
            ]
        },
        "footer": {}
    }
}
  