{
  "module": "v-search-btn-datagrid",
  "##pageId": "risk_dict",
  "layout": {
    "form": {
      "controls": [
        {
          "dictCode": {
            "label": "字典代码",
            "control": "text",
            "span": 8
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "authId": "dict_add_btn",
          "show": true,
          "label": "新增",
          "toRequest": {
            "url": "@riskDictAdd",
            "method": "get"
          },
          "props": {
            "width": "70%",
            "title": "新增"
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/API/risk-management-service/risk-dict/page-select",
        "method": "post"
      },
      "thead": [
        {
          "label": "字典代码",
          "prop": "dictCode"
        },
        {
          "label": "字典名称",
          "prop": "dictName"
        },
        {
          "label": "数据类型",
          "prop": "dataType"
        },
        {
          "label": "操作",
          "width": "150px",
          "btns": [
            {
              "type": "edit",
              "##authId": "dict_upd_btn",
              "label": "编缉",
              "toRequest": {
                "url": "@riskDictEdit",
                "method": "get"
              },
              "props": {
                "width": "70%"
              }
            },
            {
              "type": "look",
              "label": "查看",
              "toRequest": {
                "url": "@riskDictView",
                "method": "get"
              },
              "props": {
                "width": "70%"
              }
            },
            {
              "type": "delete",
              "##authId": "dict_del_btn",
              "label": "删除",
              "doRequest": {
                "url": "/API/risk-management-service/risk-dict/delete",
                "method": "post"
              }
            }
          ]
        }
      ]
    }
  }
}
