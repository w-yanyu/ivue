{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "scope": "formData",
      "controls": [],
      "btns":[]
  },
  "toolbar":{
      "btns":[]
  },
    "datagrid": {
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ms0308",
          "appId":"${{params.appId}}",
          "deci_abut_code":"${{params.deci_abut_code}}"
        }
      },
      "thead": [
        {
          "align": "center",
          "label": "决策对接代码",
          "prop": "deci_abut_code"
        },
        {
          "align": "center",
          "label": "表名",
          "prop": "table_name"
        },
        {
          "align": "center",
          "label": "数据库表描述",
          "prop": "table_desc"
        },
        {
          "align": "center",
          "label": "odb索引代码",
          "prop": "odbindex_name"
        },
        {
          "align": "center",
          "label": "odb索引类型",
          "prop": "odbindex_type"
        },
        {
          "label": "操作",
          "width": "350px",
          "align": "center",
          "btns": [
            
            {
              "type": "look",
              "label": "维护",
              "toRequest": {
                "url": "@deciOdbUpd",
                "method": "POST",
                "params": {
                  "appId":"${{params.appId}}"
                }
              },
              "props": {
                "width": "65%",
                "title": "决策对接表维护"
              }
            }
          ]
        }
      ]
    }
  }
}