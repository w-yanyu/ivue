{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "btns": [
                {
                    "type": "edit",
                    "label": "提交",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "ms0306",
                            "appId":"${{params.appId}}"

                        }
                    }
                }
            ],

            "controls": [
                {
                    "deci_abut_code": {
                        "control": "string",
                        "label": "决策对接代码",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message":"请输入"
                            },
                            {
                                "max":30,
                                "message":"最多可以输入 30 个字节"
                            }
                        ]
                    },
                    "table_name": {
                        "control": "string",
                        "label": "数据库表名",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message":"请输入"
                            },
                            {
                                "max":30,
                                "message":"最多可以输入 30 个字节"
                            }
                        ]
                    }
                },
                {
                    "table_desc": {
                        "control": "string",
                        "label": "数据库表描述",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message":"请输入"
                            },
                            {
                                "max":30,
                                "message":"最多可以输入 30 个字节"
                            }
                        ]
                    },
                    "odbindex_name": {
                        "control": "remoteSelect",
                        "label": "odb索引代码",
                        "width": "130px",
                        "labelField": "odbindex_name",
                        "valueField": "odbindex_name",
                        "doRequest": {
                            "url": "/SUMP/icmcall/icmRPCCall",
                            "method": "POST",
                            "params": {
                                "servicecode": "ms0214",
                                "appId": "${{params.appId}}",
                                "table_name":"table_name"  
                         }
                        },
                        "format": "label"
                    },
                    "odbindex_type": {
                        "control": "select",
                        "label": "odb索引类型",
                        "width": "130px",
                        "dict": {
                            "dictType": "MS_DECI_ODB_TYPE",
                            "dictKey": [
                              "MS_DECI_ODB_TYPE"
                            ]
                          },
                        "format": "value-label"
                    }
                }
               
            ]
        }
    }
}
