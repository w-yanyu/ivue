{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "scope": "formData",
      "controls": [
        {
          "appId": {
              "control": "select",
              "label": "业务模块",
              "selectFirstValue":true,
              "dict": {
                  "dictType": "MS_APPID",
                  "dictKey": [
                      "MS_APPID"
                  ],
                  "format": "value-label"
              },
              "rules": [
                  {
                      "required": true,
                      "message": "请选择业务模块"
                  }]
          }
        },     
        {
          "deci_abut_code": {
            "control": "text",
            "label": "决策对接代码",
            "default": ""
          },
          "deci_abut_name": {
            "control": "text",
            "span": 12,
            "label": "决策对接名称"
          }
        }, 
        {
          "rule_id": {
            "control": "text",
            "label": "决策代码",
            "default": ""
          },
          "rule_name": {
            "control": "text",
            "span": 12,
            "label": "决策名称"
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "target": "dialog",
          "label": "新增",
          "toRequest": {
            "method": "POST",
            "url": "@deciAdd",
            "params": {
              "appId":"${{formData.appId}}"
            }
          },
          
          "props": {
            "width": "90%",
            "title": "决策对接配置"
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ms0302",
          "appId":"${{formData.appId}}"
        }
      },
      "thead": [
        {
          "type": "string",
          "align": "center",
          "label": "决策对接代码",
          "prop": "deci_abut_code"
        },
        {
          "type": "string",
          "align": "center",
          "label": "决策对接名称",
          "prop": "deci_abut_name"
        },
        {
          "type": "string",
          "align": "center",
          "label": "决策代码",
          "prop": "rule_id"
        },
        {
          "type": "string",
          "align": "center",
          "label": "决策名称",
          "prop": "rule_name"
        },
        {
          "type": "string",
          "align": "center",
          "label": "决策版本",
          "prop": "rule_version"
        },
        {
          "label": "操作",
          "width": "350px",
          "align": "center",
          "btns": [
            
            {
              "type": "look",
              "label": "维护",
              "toRequest": {
                "url": "@deciUpd",
                "method": "GET",
                "params": {
                  "appId":"${{formData.appId}}"
                }
              },
              "eventAfter": {
                "doParentCallback": true,
                "doLastCallback": true,
                "callback": "customCallback"
              },
              "props": {
                "width": "90%",
                "title": "决策对接配置"
              }
            },
            {
              "type": "custom",
              "label": "删除",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "ms0304",
                  "appId":"${{formData.appId}}"
                }
              },
              "confirm": {
                "title": "删除",
                "msg": "确定删除吗？",
                "confirmButtonText": "确定",
                "cancelButtonText": "取消"
              }
            },{
              "type": "edit",
              "label": "决策对接表维护",
              "toRequest": {
                "url": "/views/risk/risk_deci_manage/deciAbutmentTableList.json",
                "method": "GET",
                "params": {
                  "appId":"${{formData.appId}}"
                }
              },
              "eventAfter": {
                "doParentCallback": true,
                "doLastCallback": true,
                "callback": "customCallback"
              },
              "props": {
                "width": "65%",
                "title": "决策对接表"
              }
            }
          ]
        }
      ]
    }
  }
}