{
    "module": "v-search-btn-datagrid",
    "layout": {
        "datagrid": {
            "isInitSearch": true,
            "doRequest": {
                "url": "/API/risk-management-service/risk-queue-operator/page-select",
                "method": "post",
                "params": {
                    "queueId": "${{params.alertQueueId}}"
                }
            },
            "thead": [
                {
                    "label": "操作员",
                    "prop": "operatorId",
                    "align": "center"
                },
                {
                    "label": "操作员名称",
                    "prop": "operatorName",
                    "align": "center"
                },
                {
                    "label": "操作",
                    "width": "100px",
                    "align": "center",
                    "btns": [
                        {
                            "type": "customerOwner",
                            "label": "选择",
                            "doRequest": {
                                "url": "/API/risk-management-service/risk-alert-information/assignOperator",
                                "method": "post",
                                "params": {
                                    "alertId": "${{params.alertId}}",
                                    "assignOperator": "${{current.operatorId}}",
                                    "updateUser": "$g{{user_info.userCode}}"
                                }
                            },
                            "confirm": {
                                "title": "提示信息",
                                "msg": "确定选择该人员吗？",
                                "confirmButtonText": "确定",
                                "cancelButtonText": "取消"
                            }
                        }
                    ]
                }
            ]
        }
    }
}