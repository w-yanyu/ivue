{
    "module": "v-inputDataForm-btn",
    "layout": {
        "form": {
            "controls": [
                {
                    "reviewId": {
                        "label": "审核信息ID",
                        "control": "text",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请正确输入",
                                "max": 40
                            }
                        ]
                    },
                    "alertId": {
                        "label": "警报信息ID",
                        "control": "text",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请正确输入",
                                "max": 40
                            }
                        ]
                    }
                },
                {
                    "alertRiskLevel": {
                        "label": "警报风险级别",
                        "control": "text",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请正确填写",
                                "max": 1
                            }
                        ]
                    },
                    "alertTrxnSeq": {
                        "label": "警报交易流水号",
                        "control": "text",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请正确输入",
                                "max": 40
                            }
                        ]
                    }
                },
                {
                    "cardNumber": {
                        "label": "卡号",
                        "control": "text",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请正确输入",
                                "max": 19
                            }
                        ]
                    },
                    "accountId": {
                        "label": "账号",
                        "control": "text",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请正确输入",
                                "max": 40
                            }
                        ]
                    }
                },
                {
                    "customerId": {
                        "label": "客户号",
                        "control": "text",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请正确输入",
                                "max": 40
                            }
                        ]
                    },
                    "processingFlag": {
                        "label": "处理方式",
                        "control": "select",
                        "span": 12,
                        "dict": {
                            "dictType": "processingFlagSource",
                            "dictKey": [],
                            "format": "value-label",
                            "doRequest": {
                                "url": "/views/risk/dictdata/processingFlag.json",
                                "method": "get"
                            }
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "该控件必填"
                            }
                        ]
                    }
                },
                {
                    "applyOperator": {
                        "label": "申请人",
                        "control": "text",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请正确输入",
                                "max": 40
                            }
                        ]
                    },
                    "applyTime": {
                        "label": "申请时间",
                        "control": "dateTimePicker",
                        "valueFormat": "timestamp",
                        "format": "yyyy-MM-dd HH:mm:ss",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请正确输入"
                            }
                        ]
                    }
                },
                {
                    "status": {
                        "label": "审核状态",
                        "control": "select",
                        "span": 12,
                        "dict": {
                            "dictType": "alertReviewStatus",
                            "dictKey": [],
                            "format": "value-label",
                            "doRequest": {
                                "url": "/views/risk/dictdata/alertReviewStatus.json",
                                "method": "get"
                            }
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请正确输入",
                                "max": 1
                            }
                        ]
                    },
                    "reviewOperator": {
                        "label": "审核人",
                        "control": "text",
                        "span": 12,
                        "rules": [
                            {
                                "message": "请正确输入",
                                "max": 40
                            }
                        ]
                    }
                },
                {
                    "reviewTime": {
                        "label": "审核时间",
                        "control": "dateTimePicker",
                        "valueFormat": "timestamp",
                        "format": "yyyy-MM-dd HH:mm:ss",
                        "span": 12
                    },
                    "reviewRemark": {
                        "label": "审核意见",
                        "control": "text",
                        "span": 24,
                        "rules": [
                            {
                                "message": "请正确输入",
                                "max": 500
                            }
                        ]
                    }
                }
            ],
            "doRequest": {
                "url": "/API/risk-management-service/risk-alert-review/add",
                "method": "POST",
                "params": {
                    "createUser": "$g{{user_info.userCode}}",
                    "updateUser": "$g{{user_info.userCode}}"
                }
            }
        }
    }
}
