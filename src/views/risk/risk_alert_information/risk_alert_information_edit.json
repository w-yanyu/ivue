{
  "module": "v-search-btn-datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "alertId": {
            "label": "警报信息ID",
            "control": "string",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "该控件必填",
                "max": 40
              }
            ]
          },
          "alertDate": {
            "label": "警报日期",
            "control": "dateTimePicker",
            "format": "yyyyMMdd",
            "span": 12
          }
        },
        {
          "alertRiskLevel": {
            "label": "警报风险级别",
            "control": "select",
            "span": 12,
            "dict": {
              "dictType": "alertRiskLevelSource",
              "dictKey": [],
              "format": "value-label",
              "doRequest": {
                "url": "/views/risk/dictdata/alertRiskLevel.json",
                "method": "get"
              }
            },
            "rules": [
              {
                "required": true,
                "message": "请正确输入",
                "max": 1
              }
            ]
          },
          "alertTrxnSeq": {
            "label": "警报交易流水号",
            "control": "text",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请正确输入",
                "max": 40
              }
            ]
          }
        },
        {
          "cardNumber": {
            "label": "卡号",
            "control": "text",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请正确输入",
                "max": 19
              }
            ]
          },
          "accountId": {
            "label": "账号",
            "control": "text",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请正确输入",
                "max": 40
              }
            ]
          }
        },
        {
          "customerId": {
            "label": "客户号",
            "control": "text",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请正确输入",
                "max": 40
              }
            ]
          },
          "countryCode": {
            "label": "国家代码",
            "control": "text",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请正确输入",
                "max": 3
              }
            ]
          }
        },
        {
          "merchantId": {
            "label": "商户号",
            "control": "text",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请正确输入",
                "max": 40
              }
            ]
          },
          "merchantName": {
            "label": "商户名称",
            "control": "text",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请正确输入",
                "max": 40
              }
            ]
          }
        },
        {
          "terminalId": {
            "label": "终端号",
            "control": "text",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请正确输入",
                "max": 40
              }
            ]
          },
          "alertStatus": {
            "label": "警报状态",
            "control": "select",
            "span": 12,
            "dict": {
              "dictType": "alertStatusSource",
              "dictKey": [],
              "format": "value-label",
              "doRequest": {
                "url": "/views/risk/dictdata/alertStatus.json",
                "method": "get"
              }
            },
            "rules": [
              {
                "required": true,
                "message": "该控件必填"
              }
            ]
          }
        },
        {
          "processingFlag": {
            "label": "处理方式",
            "control": "select",
            "span": 12,
            "dict": {
              "dictType": "processingFlagSource",
              "dictKey": [],
              "format": "value-label",
              "doRequest": {
                "url": "/views/risk/dictdata/processingFlag.json",
                "method": "get"
              }
            },
            "rules": [
              {
                "required": true,
                "message": "该控件必填"
              }
            ]
          },
          "alertQueueId": {
            "label": "所属队列",
            "control": "text",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请正确输入",
                "max": 20
              }
            ]
          }
        },
        {
          "assignOperator": {
            "label": "分配人员",
            "control": "text",
            "span": 12,
            "rules": [
              {
                "message": "请正确输入",
                "max": 40
              }
            ]
          }
        },
        {
          "fraudFlag": {
            "label": "欺诈交易标识",
            "control": "select",
            "span": 12,
            "dict": {
              "dictType": "fraudFlagSource",
              "dictKey": [],
              "format": "value-label",
              "doRequest": {
                "url": "/views/risk/dictdata/fraudFlag.json",
                "method": "get"
              }
            },
            "rules": [
              {
                "required": true,
                "message": "该控件必填"
              }
            ]
          },
          "fraudType": {
            "label": "欺诈类型",
            "control": "select",
            "span": 12,
            "dict": {
              "dictType": "fraudTypeSource",
              "dictKey": [],
              "format": "value-label",
              "doRequest": {
                "url": "/views/risk/dictdata/fraudType.json",
                "method": "get"
              }
            },
            "rules": [
              {
                "required": true,
                "message": "该控件必填"
              }
            ]
          }
        },
        {
          "fraudAmount": {
            "label": "欺诈金额",
            "control": "text",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请正确输入"
              }
            ]
          },
          "fraudCaseFlag": {
            "label": "欺诈转案标识",
            "control": "select",
            "span": 12,
            "dict": {
              "dictType": "fraudCaseFlagSource",
              "dictKey": [],
              "format": "value-label",
              "doRequest": {
                "url": "/views/risk/dictdata/fraudCaseFlag.json",
                "method": "get"
              }
            },
            "rules": [
              {
                "required": true,
                "message": "该控件必填"
              }
            ]
          }
        },
        {
          "fraudCaseId": {
            "label": "转案案件ID",
            "control": "text",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "该控件必填",
                "max": 40
              }
            ]
          },
          "investResultFlag": {
            "label": "调查结果标识",
            "control": "select",
            "span": 12,
            "dict": {
              "dictType": "investResultFlagSource",
              "dictKey": [],
              "format": "value-label",
              "doRequest": {
                "url": "/views/risk/dictdata/investResultFlag.json",
                "method": "get"
              }
            },
            "rules": [
              {
                "required": true,
                "message": "该控件必填"
              }
            ]
          }
        },
        {
          "remark": {
            "label": "备注",
            "control": "text",
            "span": 12
          }
        }
      ],
      "initRequest": "${{params}}",
      "doRequest": {
        "url": "/API/risk-management-service/risk-alert-information/update",
        "method": "post",
        "params": {
          "updateUser": "$g{{user_info.userCode}}"
        }
      }
    }
  }
}