{
    "module": "v-search-btn-datagrid",
    "layout": {
        "form": {
            "controls": [
                {
                    "alertId": {
                        "label": "警报信息ID",
                        "control": "text",
                        "span": 12,
                        "rules": [
                            {
                                "message": "字段长度超限",
                                "max": 40
                            }
                        ]
                    }
                }
            ]
        },
        "toolbar": {
            "btns": [
                {
                    "type": "add",
                    "label": "新增",
                    "toRequest": {
                        "url": "/views/risk/risk_alert_information/risk_alert_information_add.json",
                        "method": "get",
                        "params": {}
                    },
                    "props": {
                        "width": "800px",
                        "title": "新增"
                    }
                }
            ]
        },
        "datagrid": {
            "isInitSearch": false,
            "doRequest": {
                "url": "/API/risk-management-service/risk-alert-information/page-select",
                "method": "post"
            },
            "thead": [
                {
                    "label": "警报信息ID",
                    "prop": "alertId"
                },
                {
                    "label": "警报日期",
                    "prop": "alertDate"
                },
                {
                    "label": "警报风险级别",
                    "prop": "alertRiskLevel"
                },
                {
                    "label": "警报交易流水号",
                    "prop": "alertTrxnSeq"
                },
                {
                    "label": "卡号",
                    "prop": "cardNumber"
                },
                {
                    "label": "账号",
                    "prop": "accountId"
                },
                {
                    "label": "客户号",
                    "prop": "customerId"
                },
                {
                    "label": "国家代码",
                    "prop": "countryCode"
                },
                {
                    "label": "商户号",
                    "prop": "merchantId"
                },
                {
                    "label": "商户名称",
                    "prop": "merchantName"
                },
                {
                    "label": "终端号",
                    "prop": "terminalId"
                },
                {
                    "label": "警报状态",
                    "prop": "alertStatus"
                },
                {
                    "label": "处理方式",
                    "prop": "processingFlag"
                },
                {
                    "label": "所属队列",
                    "prop": "alertQueueId"
                },
                {
                    "label": "分配人员",
                    "prop": "assignOperator"
                },
                {
                    "label": "分配时间",
                    "prop": "assignTime"
                },
                {
                    "label": "欺诈交易标识",
                    "prop": "fraudFlag"
                },
                {
                    "label": "欺诈类型",
                    "prop": "fraudType"
                },
                {
                    "label": "欺诈金额",
                    "prop": "fraudAmount"
                },
                {
                    "label": "欺诈转案标识",
                    "prop": "fraudCaseFlag"
                },
                {
                    "label": "转案案件ID",
                    "prop": "fraudCaseId"
                },
                {
                    "label": "调查结果标识",
                    "prop": "investResultFlag"
                },
                {
                    "label": "备注",
                    "prop": "remark"
                },
                {
                    "label": "创建日期",
                    "prop": "createTime",
                    "type": "dateTimePicker",
                    "format": "yyyy-MM-dd HH:mm:ss"
                },
                {
                    "label": "创建人",
                    "prop": "createUser"
                },
                {
                    "label": "最后修改日期",
                    "prop": "updateTime",
                    "type": "dateTimePicker",
                    "format": "yyyy-MM-dd HH:mm:ss"
                },
                {
                    "label": "最后修改人",
                    "prop": "updateUser"
                },
                {
                    "label": "操作",
                    "btns": [
                        {
                            "type": "edit",
                            "label": "编缉",
                            "toRequest": {
                                "url": "/views/risk/risk_alert_information/risk_alert_information_edit.json",
                                "method": "get"
                            },
                            "props": {}
                        },
                        {
                            "type": "look",
                            "label": "查看",
                            "toRequest": {
                                "url": "/views/risk/risk_alert_information/risk_alert_information_view.json",
                                "method": "get"
                            },
                            "props": {}
                        },
                        {
                            "type": "delete",
                            "label": "删除",
                            "doRequest": {
                                "url": "/API/risk-management-service/risk-alert-information/delete",
                                "method": "post"
                            }
                        }
                    ]
                }
            ],
            "pagination": {
                "show": true,
                "pageSizes": [
                    "10",
                    "50",
                    "100",
                    "200"
                ],
                "pageSize": 10
            }
        }
    }
}