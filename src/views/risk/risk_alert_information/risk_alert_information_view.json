{
    "module": "v-realonlyForm-btn",
    "layout": {
        "form": {
            "initRequest": {
                "url": "/API/risk-management-service/risk-alert-information/query",
                "method": "post",
                "params": {
                    "alertId": "${{params.alertId}}"
                }
            },
            "controls": [
                {
                    "alertId": {
                        "label": "警报信息ID",
                        "control": "string",
                        "span": 12
                    },
                    "alertDate": {
                        "label": "警报日期",
                        "control": "string",
                        "span": 12
                    }
                },
                {
                    "alertRiskLevel": {
                        "label": "警报风险级别",
                        "control": "string",
                        "span": 12,
                        "dict": {
                            "dictType": "alertRiskLevelSource",
                            "dictKey": [],
                            "format": "value-label",
                            "doRequest": {
                                "url": "/views/risk/dictdata/alertRiskLevel.json",
                                "method": "get"
                            }
                        }
                    },
                    "alertTrxnSeq": {
                        "label": "警报交易流水号",
                        "control": "string",
                        "span": 12
                    }
                },
                {
                    "cardNumber": {
                        "label": "卡号",
                        "control": "string",
                        "span": 12
                    },
                    "accountId": {
                        "label": "账号",
                        "control": "string",
                        "span": 12
                    }
                },
                {
                    "customerId": {
                        "label": "客户号",
                        "control": "string",
                        "span": 12
                    },
                    "countryCode": {
                        "label": "国家代码",
                        "control": "string",
                        "span": 12,
                        "dict": {
                            "dictType": "countryCodeSource",
                            "dictKey": [],
                            "format": "value-label",
                            "valueField": "numericCode",
                            "labelField": "chineseName",
                            "doRequest": {
                                "url": "/API/risk-management-service/risk-country-code/query",
                                "method": "post"
                            }
                        }
                    }
                },
                {
                    "merchantId": {
                        "label": "商户号",
                        "control": "string",
                        "span": 12
                    },
                    "merchantName": {
                        "label": "商户名称",
                        "control": "string",
                        "span": 12
                    }
                },
                {
                    "terminalId": {
                        "label": "终端号",
                        "control": "string",
                        "span": 12
                    },
                    "alertStatus": {
                        "label": "警报状态",
                        "control": "string",
                        "span": 12,
                        "dict": {
                            "dictType": "alertStatusSource",
                            "dictKey": [],
                            "format": "value-label",
                            "doRequest": {
                                "url": "/views/risk/dictdata/alertStatus.json",
                                "method": "get"
                            }
                        }
                    }
                },
                {
                    "processingFlag": {
                        "label": "处理方式",
                        "control": "string",
                        "span": 12,
                        "dict": {
                            "dictType": "processingFlagSource",
                            "dictKey": [],
                            "format": "value-label",
                            "doRequest": {
                                "url": "/views/risk/dictdata/processingFlag.json",
                                "method": "get"
                            }
                        }
                    },
                    "alertQueueId": {
                        "label": "所属队列",
                        "control": "string",
                        "span": 12,
                        "dict": {
                            "dictType": "alertQueueIdSource",
                            "dictKey": [],
                            "format": "value-label",
                            "valueField": "queueId",
                            "labelField": "queueName",
                            "doRequest": {
                                "url": "/API/risk-management-service/risk-alert-queue/queryAll",
                                "method": "post"
                            }
                        }
                    }
                },
                {
                    "assignOperator": {
                        "label": "分配人员",
                        "control": "string",
                        "span": 12
                    },
                    "assignTime": {
                        "label": "分配时间",
                        "control": "string",
                        "span": 12,
                        "type": "dateTimePicker",
                        "format": "yyyy-MM-dd HH:mm:ss"
                    }
                },
                {
                    "fraudFlag": {
                        "label": "欺诈交易标识",
                        "control": "string",
                        "span": 12,
                        "dict": {
                            "dictType": "fraudFlagSource",
                            "dictKey": [],
                            "format": "value-label",
                            "doRequest": {
                                "url": "/views/risk/dictdata/fraudFlag.json",
                                "method": "get"
                            }
                        }
                    },
                    "fraudType": {
                        "label": "欺诈类型",
                        "control": "string",
                        "span": 12,
                        "dict": {
                            "dictType": "fraudTypeSource",
                            "dictKey": [],
                            "format": "value-label",
                            "doRequest": {
                                "url": "/views/risk/dictdata/fraudType.json",
                                "method": "get"
                            }
                        }
                    }
                },
                {
                    "fraudAmount": {
                        "label": "欺诈金额",
                        "control": "string",
                        "span": 12
                    },
                    "fraudCaseFlag": {
                        "label": "欺诈转案标识",
                        "control": "string",
                        "span": 12,
                        "dict": {
                            "dictType": "fraudCaseFlagSource",
                            "dictKey": [],
                            "format": "value-label",
                            "doRequest": {
                                "url": "/views/risk/dictdata/fraudCaseFlag.json",
                                "method": "get"
                            }
                        }
                    }
                },
                {
                    "fraudCaseId": {
                        "label": "转案案件ID",
                        "control": "string",
                        "span": 12
                    },
                    "investResultFlag": {
                        "label": "调查结果标识",
                        "control": "string",
                        "span": 12,
                        "dict": {
                            "dictType": "investResultFlagSource",
                            "dictKey": [],
                            "format": "value-label",
                            "doRequest": {
                                "url": "/views/risk/dictdata/investResultFlag.json",
                                "method": "get"
                            }
                        }
                    }
                },
                {
                    "createTime": {
                        "label": "创建日期",
                        "control": "string",
                        "type": "dateTimePicker",
                        "format": "yyyy-MM-dd HH:mm:ss",
                        "span": 12
                    },
                    "createUser": {
                        "label": "创建人",
                        "control": "string",
                        "span": 12
                    }
                },
                {
                    "updateTime": {
                        "label": "最后修改日期",
                        "control": "string",
                        "type": "dateTimePicker",
                        "format": "yyyy-MM-dd HH:mm:ss",
                        "span": 12
                    },
                    "updateUser": {
                        "label": "最后更新人",
                        "control": "string",
                        "span": 12
                    }
                },
                {
                    "remark": {
                        "label": "备注",
                        "control": "string",
                        "span": 12
                    }
                }
            ]
        }
    }
}