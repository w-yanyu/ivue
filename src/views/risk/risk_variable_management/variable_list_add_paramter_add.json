{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                          "servicecode": "deci050050"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "variable_class_code": {
                        "control": "text",
                        "label": "参数代码",
                        "rules": [
                          {
                            "required": true,
                            "message": "请输入"
                          },
                          {
                            "max": 30,
                            "message": "最多可以输入 30 个字节"
                          }
                        ]
                    },
                    "super_class_code": {
                        "control": "text",
                        "label": "参数名称",
                        "rules": [
                          {
                            "required": true,
                            "message": "请输入"
                          },
                          {
                            "max": 30,
                            "message": "最多可以输入 30 个字节"
                          }
                        ]
                    }
                },
                {
                  "variable_class_name": {
                    "control": "text",
                    "label": "描述"
                  },
                  "data_type": {
                    "label": "数据类型",
                    "control": "select",
                    "dict": {
                      "format": "label",
                      "dictKey": [
                        "DECI_E_DATA_TYPE"
                      ],
                      "dictType": "DECI_E_DATA_TYPE"
                    }
                  }
                }
            ]
        }
    }
}
