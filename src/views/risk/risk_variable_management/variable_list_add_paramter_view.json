{
  "module": "v_realonlyForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "controls": [
        {
          "parm_code": {
            "control": "string",
            "label": "参数代码"
          },
          "parm_name": {
            "control": "string",
            "label": "参数名称"
          }
        },
        {
          "data_type": {
            "label": "数据类型",
            "control": "select",
            "dict": {
              "format": "label",
              "dictKey": ["DECI_E_DATA_TYPE"],
              "dictType": "DECI_E_DATA_TYPE"
            },
            "disabled": true
          },
          "description": {
            "control": "string",
            "label": "描述"
          }
        },
        {
          "data_version": {
            "control": "string",
            "label": "版本"
          },
          "data_create_user": {
            "control": "string",
            "label": "创建人"
          }
        },
        {
          "data_create_time": {
            "control": "string",
            "label": "创建时间"
          },
          "data_update_user": {
            "control": "string",
            "label": "最近更新人"
          }
        },
        {
          "data_update_time": {
            "control": "string",
            "label": "最近更新时间"
          }
        }
      ]
    }
  }
}
