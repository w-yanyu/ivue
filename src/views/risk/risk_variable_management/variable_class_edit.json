{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "deci050051"
            }
          }
        }
      ],
      "controls": [
        {
          "variable_class_code": {
            "control": "text",
            "disabled": true,
            "label": "变量分类代码",
            "width": "130px"
          },
          "super_class_code": {
            "control": "text",
            "label": "上级变量分类代码",
            "width": "130px",
            "disabled": true,
            "value": "${{params.variable_class_code}}"
          }
        },
        {
          "variable_class_name": {
            "control": "text",
            "label": "变量分类名称",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入"
              },
              {
                "max": 30,
                "message": "最多可以输入 30 个字节"
              }
            ]
          }
        }
      ]
    }
  }
}