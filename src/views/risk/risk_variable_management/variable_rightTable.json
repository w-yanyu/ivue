{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "scope": "formData",
      "initRequest": "${{params}}",
      "controls": [
        {
          "variable_code": {
            "control": "text",
            "label": "变量代码"
          },
          "variable_name": {
            "control": "text",
            "label": "变量名称"
          },
          "variable_status": {
            "label": "变量状态",
            "control": "select",
            "dict": {
              "format": "label",
              "dictKey": ["DECI_E_VARIABLE_STATUS"],
              "dictType": "DECI_E_VARIABLE_STATUS"
            }
          },
          "start_status": {
            "label": "启停状态",
            "control": "select",
            "dict": {
              "format": "label",
              "dictKey": ["DECI_E_START_STATUS"],
              "dictType": "DECI_E_START_STATUS"
            }
          },
          "regist_status": {
            "label": "注册状态",
            "control": "select",
            "dict": {
              "format": "label",
              "dictKey": ["DECI_E_REGIST_STATUS"],
              "dictType": "DECI_E_REGIST_STATUS"
            }
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "target": "dialog",
          "label": "新增",
          "toRequest": {
            "url": "@variableListAdd",
            "method": "GET",
            "params": {
              "props": "${{params}}",
              "variable_class_code": "${{params.variable_class_code}}",
              "pageType": "add"
            }
          },
          "props": {
            "title": "新增变量",
            "width": "90%",
            "height": "90%",
            "top": "4vh"
          }
        },
        {
          "type": "custom",
          "label": "注册",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "deci050064",
              "fieldList": "${{current.selectedData}}"
            }
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
            "servicecode": "deci050057",
            "variable_class_code": "${{params.variable_class_code}}"
        }
      },
      "thead": [
        {
          "type": "selection",
          "align": "center",
          "width": "55",
          "fixed": "left"
        },
        {
          "align": "center",
          "label": "变量代码",
          "prop": "variable_code"
        },
        {
          "align": "center",
          "label": "变量名称",
          "prop": "variable_name"
        },
        {
          "align": "center",
          "label": "变量分类",
          "prop": "variable_class_code"
        },
        {
          "align": "center",
          "label": "变量状态",
          "prop": "variable_status",
          "dict": {
            "dictType": "DECI_E_VARIABLE_STATUS",
            "dictKey": ["DECI_E_VARIABLE_STATUS"],
            "format": "label"
          }
        },
        {
          "align": "center",
          "label": "启停状态",
          "prop": "start_status",
          "dict": {
            "dictType": "DECI_E_START_STATUS",
            "dictKey": ["DECI_E_START_STATUS"],
            "format": "label"
          }
        },
        {
          "align": "center",
          "label": "注册状态",
          "prop": "regist_status",
          "dict": {
            "dictType": "DECI_E_REGIST_STATUS",
            "dictKey": ["DECI_E_REGIST_STATUS"],
            "format": "label"
          }
        },
        {
          "align": "center",
          "label": "变量描述",
          "prop": "description"
        },
        {
          "align": "center",
          "label": "版本",
          "prop": "data_version"
        },
        {
          "align": "center",
          "label": "创建人",
          "prop": "data_create_user"
        },
        {
          "align": "center",
          "label": "创建时间",
          "prop": "data_create_time"
        },
        {
          "align": "center",
          "label": "最近更新人",
          "prop": "data_update_user"
        },
        {
          "align": "center",
          "label": "最近更新时间",
          "prop": "data_update_time"
        },
        {
          "label": "操作",
          "align": "center",
          "fixed": "right",
          "width": "250px",
          "btns": [
            {
              "type": "edit",
              "authId": "constant_upd_btn",
              "label": "维护",
              "toRequest": {
                "url": "@variableListAdd",
                "method": "GET",
                "params": {
                    "props": "${{params}}",
                    "pageType": "edit"
                }
              },
              "props": {
                "width": "70%",
                "height": "70%"
              }
            },
            {
              "type": "look",
              "label": "查看",
              "toRequest": {
                "url": "@variableListAdd",
                "method": "GET",
                "params": {
                    "props": "${{params}}",
                    "pageType": "look"
                }
              },
              "props": {
                "width": "70%",
                "height": "70%"
              }
            },
            {
              "type": "delete",
              "label": "删除",
              "disabled": "'${{current.is_delete}}' === '0'",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "post",
                "params": {
                  "servicecode": "deci050056",
                  "variable_code": "${{current.variable_code}}"
                }
              },
              "confirm": {
                "title": "提示信息",
                "msg": "确认删除当前数据？",
                "confirmButtonText": "确定",
                "cancelButtonText": "取消"
              }
            },
            {
              "type": "dropdown",
              "label": "更多",
              "trigger": "hover",
              "options": [
                {
                  "type": "custom",
                  "label": "测试",
                  "toRequest": {
                    "target": "drawer",
                    "method": "get",
                    "url": "@variableTest",
                    "params": {
                      "variable_code": "${{current.variable_code}}"
                    }
                  },
                  "props": {
                    "width": "40%",
                    "title": "测试",
                    "close": "",
                    "openType": "drawer",
                    "that": "this",
                    "position": "right"
                  }
                },
                {
                  "type": "custom",
                  "label": "提交审核",
                  "doRequest": {
                    "url": "/SUMP/icmcall/icmRPCCall",
                    "method": "post",
                    "params": {
                      "servicecode": "deci050060",
                      "variable_code": "${{current.variable_code}}"
                    }
                  }
                },
                {
                  "type": "custom",
                  "label": "提交运行",
                  "doRequest": {
                    "url": "/SUMP/icmcall/icmRPCCall",
                    "method": "post",
                    "params": {
                      "servicecode": "deci050061",
                      "variable_code": "${{current.variable_code}}"
                    }
                  }
                },
                {
                  "type": "custom",
                  "label": "提交下线",
                  "doRequest": {
                    "url": "/SUMP/icmcall/icmRPCCall",
                    "method": "post",
                    "params": {
                      "servicecode": "deci050062",
                      "variable_code": "${{current.variable_code}}"
                    }
                  }
                },
                {
                  "type": "custom",
                  "label": "启用",
                  "doRequest": {
                    "url": "/SUMP/icmcall/icmRPCCall",
                    "method": "post",
                    "params": {
                      "servicecode": "deci050063",
                      "variable_code": "${{current.variable_code}}",
                      "start_status":""
                    }
                  },
                  "show": {
                    "expression": "'${{current.start_status}}' == '1'"
                  }
                },
                {
                  "type": "custom",
                  "label": "停用",
                  "doRequest": {
                    "url": "/SUMP/icmcall/icmRPCCall",
                    "method": "post",
                    "params": {
                      "servicecode": "deci050008",
                      "variable_code": "${{current.variable_code}}",
                      "start_status":""
                    }
                  },
                  "show": {
                    "expression": "'${{current.start_status}}' == '0'"
                  }
                },
                {
                  "type": "custom",
                  "label": "注册",
                  "doRequest": {
                    "url": "/SUMP/icmcall/icmRPCCall",
                    "method": "post",
                    "params": {
                      "servicecode": "deci050008",
                      "variable_code": "${{current.variable_code}}"
                    }
                  }
                }
              ]
            }
          ]
        }
      ]
    }
  }
}
