{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "scope": "formData",
      "initRequest": "${{params}}",
      "controls": [
        {
          "variable_code": {
            "control": "text",
            "label": "变量代码"
          },
          "variable_name": {
            "control": "text",
            "label": "变量名称"
          },
          "variable_status": {
            "label": "变量状态",
            "control": "select",
            "dict": {
              "format": "label",
              "dictKey": ["DECI_E_VARIABLE_STATUS"],
              "dictType": "DECI_E_VARIABLE_STATUS"
            }
          },
          "start_status": {
            "label": "启停状态",
            "control": "select",
            "dict": {
              "format": "label",
              "dictKey": ["DECI_E_START_STATUS"],
              "dictType": "DECI_E_START_STATUS"
            }
          },
          "regist_status": {
            "label": "注册状态",
            "control": "select",
            "dict": {
              "format": "label",
              "dictKey": ["DECI_E_REGIST_STATUS"],
              "dictType": "DECI_E_REGIST_STATUS"
            }
          }
        }
      ]
    },
   
    "datagrid": {
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
            "servicecode": "deci050057"
        }
      },
      "thead": [
        {
          "type": "selection",
          "align": "center",
          "width": "55",
          "fixed": "left"
        },
        {
          "align": "center",
          "label": "变量代码",
          "prop": "variable_code"
        },
        {
          "align": "center",
          "label": "变量名称",
          "prop": "variable_name"
        },
        {
          "align": "center",
          "label": "变量分类",
          "prop": "variable_class_code"
        },
        {
          "align": "center",
          "label": "变量状态",
          "prop": "variable_status",
          "dict": {
            "dictType": "DECI_E_VARIABLE_STATUS",
            "dictKey": ["DECI_E_VARIABLE_STATUS"],
            "format": "label"
          }
        },
        {
          "align": "center",
          "label": "启停状态",
          "prop": "start_status",
          "dict": {
            "dictType": "DECI_E_START_STATUS",
            "dictKey": ["DECI_E_START_STATUS"],
            "format": "label"
          }
        },
        {
          "align": "center",
          "label": "注册状态",
          "prop": "regist_status",
          "dict": {
            "dictType": "DECI_E_REGIST_STATUS",
            "dictKey": ["DECI_E_REGIST_STATUS"],
            "format": "label"
          }
        },
        {
          "align": "center",
          "label": "变量描述",
          "prop": "description"
        },
        {
          "align": "center",
          "label": "版本",
          "prop": "data_version"
        },
        {
          "align": "center",
          "label": "创建人",
          "prop": "data_create_user"
        },
        {
          "align": "center",
          "label": "创建时间",
          "prop": "data_create_time"
        },
        {
          "align": "center",
          "label": "最近更新人",
          "prop": "data_update_user"
        },
        {
          "align": "center",
          "label": "最近更新时间",
          "prop": "data_update_time"
        },
        {
          "label": "操作",
          "width": "150px",
          "btns": [
            {
              "type": "bringback",
              "label": "选择"
            }
          ]
        }
      ]
    }
  }
}
