{
  "module": "v_search_editableDataGrid",
  "layout": {
    "datagrid": {
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "deci050058",
          "variable_code": "${{params.variable_code}}"
        }
      },
      "name": "paramsList",
      "###initRequest": "${{params.paramsList}}",
      "pagination": {
        "show": false
      },
      "thead": [
        {
          "type": "selection",
          "align": "center",
          "width": "55",
          "fixed": "left",
          "disabled": "'${{params.pageType}}' === 'look'"
        },
        {
          "align": "center",
          "label": "参数代码",
          "prop": "parm_code",
          "type": "text",
          "disabled": true
        },
        {
          "align": "center",
          "label": "参数名称",
          "prop": "parm_name",
          "type": "text",
          "disabled": true
        },
        {
          "align": "center",
          "label": "参数值",
          "prop": "parm_value",
          "type": "text",
          "disabled": "'${{params.pageType}}' === 'look'"
        },
        {
          "label": "操作",
          "align": "center",
          "fixed": "right",
          "btns": [
            {
              "type": "delete",
              "label": "删除"
            }
          ]
        }
      ]
    }
  }
}
