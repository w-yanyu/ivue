{
  "module": "v-form-btn-dynamic",
  "layout": {
    "form": {
      "controls": [
        {
          "days": {
            "label": "加工天数",
            "control": "inputNumber",
            "span": 8,
            "rules": [{
              "required": true,
              "message": "请输入正整数",
              "type":"integer",
              "min": 0
            }]
          },
          "channelId": {
            "label": "渠道",
            "control": "select",
            "span": 8,
            "format": "value-label",
            "options":[
              {
                "label": "运通",
                "value": "AMX"
              },
              {
                "label": "银联",
                "value": "CUP"
              },
              {
                "label": "VSS",
                "value": "VSS"
              },
              {
                "label": "JCB",
                "value": "JCB"
              },
              {
                "label": "MCD",
                "value": "MCD"
              }
            ],
            "rules": [{
              "required": true,
              "message": "请输入渠道号"
            }]
          },
          "subChannelId": {
            "label": "子渠道",
            "control": "text",
            "span": 8
          }
        },
        {
          "areaCode": {
            "label": "省/市/区",
            "control": "cascader",
            "span": 8,
            "rules": [
              {
                "required": true,
                "message": "请输入省/市/区"
              }
            ],
            "doRequest": {
              "url": "/API/risk-management-service/combobox-select/area_code",
              "method": "post",
              "params": {}
            }
          },
          "billingAmt": {
            "label": "手机号",
            "control": "text",
            "span": 8,
            "rules": [
              {
                "type": "string",
                "required": true,
                "message": "请填写手机号",
                "pattern": "^1[0-9]{10}$"
              }
            ]
          },
          "tlCardNo": {
            "label": "卡号",
            "control": "text",
            "span": 8,
            "rules": [
              {
                "type": "number",
                "required": true,
                "message": "填写正确卡号"
              }
            ]
          }
        },
        {
          "customerNo": {
            "label": "客户号",
            "control": "text",
            "span": 8,
            "rules": [
              {
                "required": true,
                "message": "请输入客户号"
              }
            ]
          },
          "trxnCode": {
            "label": "交易码",
            "control": "text",
            "span": 8,
            "rules": [{
              "required": true,
              "message": "请输入交易码"
            }]
          }
        },
        {
          "mcc": {
            "label": "商户类别",
            "control": "text",
            "span": 8,
            "rules": [{
              "required": true,
              "message": "请输入商户类别"
            }]
          },
          "responseCode": {
            "label": "响应码",
            "control": "text",
            "span": 8,
            "rules": [{
              "required": true,
              "message": "请输入响应码"
            }]
          },
          "acctId": {
            "label": "账户号",
            "control": "text",
            "span": 8,
            "value": "4385380000228661156",
            "rules": [
              {
                "required": true,
                "message": "请输入账户号"
              }
            ]
          },
          "merchantId": {
            "label": "商户号",
            "control": "text",
            "span": 8,
            "rules": [{
              "required": true,
              "message": "请输入商户号"
            }]
          },
          "terminalId": {
            "label": "终端号",
            "control": "text",
            "span": 8,
            "rules": [{
              "required": true,
              "message": "请输入终端号"
            }]
          }
        },
        {
          "randomIpAddr": {
            "label": "开启随机IP地址",
            "control": "radio",
            "span": 8,
            "options":[
              {
                "label": "是",
                "value": true
              },
              {
                "label": "否",
                "value": false
              }
            ],
            "rules": [{
              "required": true,
              "message": "请选择是/否"
            }]
          },
          "randomMacAddr": {
            "label": "开启随机MAC地址",
            "control": "radio",
            "span": 8,
            "options":[
              {
                "label": "是",
                "value": true
              },
              {
                "label": "否",
                "value": false
              }
            ],
            "rules": [{
              "required": true,
              "message": "请选择是/否"
            }]
          },
          "randomGpsLocation": {
            "label": "开启随机位置",
            "control": "radio",
            "span": 8,
            "options":[
              {
                "label": "是",
                "value": true
              },
              {
                "label": "否",
                "value": false
              }
            ],
            "rules": [{
              "required": true,
              "message": "请选择是/否"
            }]
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "doRequest": {
            "url": "/simulate/risk-trade-service/sendBehavior",
            "method": "post"
          }
        },
        {
          "type": "reset",
          "btnStyle": "success",
          "label": "清空"
        }
      ]
    }
  }
}
