{
  "module": "v-form-btn-dynamic",
  "layout": {
    "form": {
      "controls": [
        {
          "days": {
            "label": "加工天数",
            "control": "inputNumber",
            "span": 8,
            "rules": [
              {
                "required": true,
                "message": "请输入正整数",
                "type": "integer",
                "min": 0
              }
            ]
          },
          "channelId": {
            "label": "渠道",
            "control": "select",
            "span": 8,
            "format": "value-label",
            "options": [
              {
                "label": "运通",
                "value": "AMX"
              },
              {
                "label": "银联",
                "value": "CUP"
              },
              {
                "label": "VSS",
                "value": "VSS"
              },
              {
                "label": "JCB",
                "value": "JCB"
              },
              {
                "label": "MCD",
                "value": "MCD"
              }
            ],
            "rules": [
              {
                "required": true,
                "message": "请输入渠道号"
              }
            ]
          },
          "subChannelId": {
            "label": "子渠道",
            "control": "text",
            "span": 8
          }
        },
        {
          "billingAmt": {
            "label": "交易金额",
            "control": "text",
            "span": 8,
            "rules": [
              {
                "required": true,
                "message": "填写交易金额"
              }
            ]
          },
          "randomBillingAmt": {
            "label": "是否开启随机金额",
            "control": "select",
            "span": 8,
            "options": [
              {
                "label": "是",
                "value": true
              },
              {
                "label": "否",
                "value": false
              }
            ],
            "rules": [
              {
                "required": true,
                "message": "请选择是/否"
              }
            ]
          },
          "tlCardNo": {
            "label": "卡号",
            "control": "text",
            "span": 8,
            "value": "4385380000228661",
            "rules": [
              {
                "type": "number",
                "required": true,
                "message": "填写正确卡号"
              }
            ]
          }
        },
        {
          "customerNo": {
            "label": "客户号",
            "control": "text",
            "span": 8,
            "value": "8190918135441143",
            "rules": [
              {
                "required": true,
                "message": "请输入客户号"
              }
            ]
          },
          "acctId": {
            "label": "账户号",
            "control": "text",
            "span": 8,
            "value": "4385380000228661156",
            "rules": [
              {
                "required": true,
                "message": "请输入账户号"
              }
            ]
          },
          "tranTime": {
            "label": "交易时间",
            "control": "text",
            "span": 8,
            "rules": [
              {
                "required": true,
                "message": "请输入交易时间"
              }
            ]
          },
          "countryCode": {
            "label": "国家代码",
            "control": "text",
            "span": 8,
            "value": "156",
            "rules": [
              {
                "required": true,
                "message": "请输入国家代码",
                "type": "string",
                "pattern": "^\\d{3}$"
              }
            ]
          },
          "mcc": {
            "label": "商户类别",
            "control": "text",
            "span": 8,
            "value": "5412",
            "rules": [
              {
                "required": true,
                "message": "请输入商户类别"
              }
            ]
          }
        },
        {
          "merchantId": {
            "label": "商户号",
            "control": "text",
            "span": 8,
            "value": "0000001",
            "rules": [
              {
                "required": true,
                "message": "请输入商户号"
              }
            ]
          },
          "terminalId": {
            "label": "终端号",
            "control": "text",
            "span": 8,
            "value": "0000001",
            "rules": [
              {
                "required": true,
                "message": "请输入终端号"
              }
            ]
          },
          "trxnCode": {
            "label": "交易码",
            "control": "text",
            "span": 8,
            "value": "AMX101",
            "rules": [
              {
                "required": true,
                "message": "请输入交易码"
              }
            ]
          },
          "transClass": {
            "label": "交易类别",
            "control": "text",
            "span": 8,
            "value": "20",
            "rules": [
              {
                "required": true,
                "message": "请输入交易类别"
              }
            ]
          },
          "responseCode": {
            "label": "响应码",
            "control": "text",
            "span": 8,
            "value": "200",
            "rules": [
              {
                "required": true,
                "message": "请输入响应码"
              }
            ]
          },
          "transDirection": {
            "label": "交易方向",
            "control": "select",
            "span": 8,
            "format": "value-label",
            "options": [
              {
                "label": "借方",
                "value": "D"
              },
              {
                "label": "贷方",
                "value": "C"
              },
              {
                "label": "非金融交易",
                "value": "N"
              }
            ],
            "rules": [
              {
                "required": true,
                "message": "请输入交易方向"
              }
            ]
          },
          "messageType": {
            "label": "消息类型",
            "control": "text",
            "span": 8,
            "rules": [
              {
                "required": true,
                "message": "请输入消息类型"
              }
            ]
          },
          "posEntryMode": {
            "label": "服务点输入方式",
            "control": "text",
            "span": 8,
            "rules": [
              {
                "required": true,
                "message": "请输入服务点输入方式"
              }
            ]
          },
          "tranCcy": {
            "label": "交易币种",
            "control": "text",
            "span": 8,
            "rules": [
              {
                "required": true,
                "message": "请输入交易币种"
              }
            ]
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "doRequest": {
            "url": "/simulate/risk-trade-service/mqProducer/send",
            "method": "post"
          }
        },
        {
          "type": "reset",
          "btnStyle": "success",
          "label": "清空"
        }
      ]
    }
  }
}
