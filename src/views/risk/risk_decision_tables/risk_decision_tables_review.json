{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "btns": [
        {
          "type": "close",
          "label": "取消"
        },
        {
          "type": "submit",
          "target": "addlog",
          "label": "确定",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "post",
            "params": {
              "servicecode": "deci020409"
            }
          }
        }
      ],
      "controls": [
        {
          "deci_tables_code": {
            "control": "text",
            "disabled": "true",
            "width":"auto",
            "value": "${{params.deci_tables_code}}",
            "label": "决策表代码",
            "rules": [
              {
                "required": true
              }
            ],
            "span": 24
          }
        },
        {
          "deci_tables_name": {
            "control": "text",
            "label": "决策表名称",
            "value": "${{params.deci_tables_name}}",
            "width":"auto",
            "disabled": "true",
            "rules": [
              {
                "required": true
              }
            ],
            "span": 24
          }
        },
        {
          "deci_tables_operation_type": {
            "label": "操作类型",
            "value": "${{params.deci_tables_operation_type}}",
            "width":"auto",
            "disabled": "true",
            "rules": [
              {
                "required": true
              }
            ],
            "control": "select",
            "dict": {
              "dictType": "DECI_E_RISK_OPERATION_TYPE",
              "dictKey": [
                "DECI_E_RISK_OPERATION_TYPE"
              ],
              "format": "label"
            },
            "span": 24
          }
        }
      ]
    }
  }
}