{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "controls": [
        {
          "deci_tables_code": {
            "label": "决策表代码",
            "control": "string",
            "width": "auto",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "该控件必填",
                "max": 64
              }
            ],
            "value": "客户评级决策表"
          },
          "deci_tables_name": {
            "label": "决策表名称",
            "width": "auto",
            "control": "text",
            "span": 12,
            "disabled": true,
            "rules": [
              {
                "required": true,
                "pattern": "^[a-z0-9A-Z\u4e00-\u9fa5]+$",
                "message": "仅可为字母、数字、文字及相关组合",
                "max": 125
              }
            ],
            "value": "客户评级决策表"
          }
        },
        {
          "busi_version": {
            "label": "版本号",
            "width": "auto",
            "control": "string",
            "span": 12,
            "disabled": true
          }
        },
        {
          "deci_tables_describe": {
            "label": "决策表描述",
            "width": "auto",
            "rows": 5,
            "control": "textarea",
            "showWordLimit": 200,
            "maxlength": 200,
            "span": 24,
            "autosize": {
              "minRows": 5
            }
          }
        },
        {
          "allData": {
            "label": "决策表配置",
            "width": "auto",
            "initShow": true,
            "span": 24,
            "control": "dynamic",
            "toRequest": {
              "url": "@dynamicEditTable",
              "params": {
                "deci_tables_code": "${{params.deci_tables_code}}",
                "showButton":true,
                "lookView":"tmp"
              }
            },
            "props": {}
          }
        }
      ],
      "initRequest": "${{params}}",
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "deci020406"
        }
      }
    }
  }
}