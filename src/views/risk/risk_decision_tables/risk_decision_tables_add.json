{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "##initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pd0214",
          "cust_class_list": "10|20|30|70|99",
          "prod_type": "CL",
          "sale_prod_cd": "0000001"
        }
      },
      "controls": [
        {
          "deci_tables_code": {
            "label": "决策表代码",
            "width":"auto",
            "control": "text",
            "span": 12,
            "disabled":false,
            "rules": [ {
              "required": true,
              "message": "请正确输入",
              "max":64
            },{
              "required": true,
              "message": "请正确输入",
              "max": 128
            }]
          },
          "deci_tables_name": {
            "label": "决策表名称",
            "width":"auto",
            "control": "text",
            "span": 12,
            "disabled":false,
            "rules": [{
              "required": true,
              "pattern": "^[a-z0-9A-Z\u4e00-\u9fa5]+$",
              "message": "仅可为字母、数字、文字及相关组合",
              "max": 128
            }]
          }
        },
        
        {
          "deci_tables_describe": {
            "label": "备注",
            "control": "textarea",
            "width":"auto",
            "showWordLimit": 200,
            "maxlength": 200,
            "rows":5,
            "span": 24,
            "rules": [{
              "message": "请正确输入",
              "max": 200
            }]
          }
        }
      ],
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "deci020403"
        }
      }
    }
  }
}
