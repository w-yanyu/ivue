{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest":"${{params}}" ,
      "btns": [
        {
          "type": "submit",
          "target": "addlog",
          "label": "复制",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "deci020453"
            }
          }
        } ,
        {
          "type": "close",
          "label": "取消",
          "icon": ""
      }
      ],
      "controls": [
        { 
          "deci_tables_code": {
            "control": "hidden",
            "label": "决策表代码",
            "value": "${{params.deci_tables_code}}"
          },
          "deci_tables_name": {
            "control": "hidden",
            "label": "决策表名称",
            "value": "${{params.deci_tables_name}}"
          },
          "busi_version": {
            "control": "hidden",
            "label": "当前版本",
            "value": "${{params.busi_version}}"
          },
          "new_tables_code": {
            "control": "text",
            "align": "center",
            "span": 16,
            "disabled":false,
            "inputWidth": "250px",
            "label": "决策表代码",
            "rules": [ 
              {
              "required": true,
              "message": "规则树代码不得为空"
             }]
          },
          "new_tables_name": {
            "control": "text",
            "label": "决策表名称",
            "align": "center",
            "span": 16,
            "inputWidth": "250px",
            "rules": [
              {
                "required": true,
                "pattern": "^[a-z0-9A-Z\u4e00-\u9fa5]+$",
                "message": "仅可为字母、数字、文字及相关组合"
            }
            ]
          } ,
            "new_tables_describe": {
              "label": "备注",
              "control": "textarea",
              "span": 17,
              "showWordLimit": 200,
              "maxlength": 200,
              "rows": 5,
              "rules": [
                {
                  "message": "请正确输入",
                  "max": 200
                }
              ]
            }
        }
      ]
    }
  }
}