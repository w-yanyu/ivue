{
  "module": "v-search-btn-datagrid",
  "layout": {
    "datagrid": {
      "doRequest": {
        "url": "/API/risk-management-service/combobox-select/aggregate_field_code",
        "method": "post",
        "params":{
          "aggregateSource":"${{parent.form.aggregateSource}}",
          "statisticDimension":"${{parent.form.indicatorDimension}}",
          "dataType":"${{parent.form.dataType}}"
        }
      },
      "pagination": {
        "pageSizes": [
          5,
          10,
          20,
          50,
          100
        ],
        "pageSize": 5
      },
      "thead": [
        {
          "label": "聚合字段代码",
          "prop": "value",
          "width": "200px",
          "align": "center"
        },
        {
          "label": "聚合字段名称",
          "prop": "label"
        },
        {
          "label": "操作",
          "width": "70px",
          "align": "center",
          "btns": [
            {
              "type": "bringback",
              "label": "选择"
            }
          ]
        }
      ]
    }
  }
}
