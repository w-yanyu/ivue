{
  "module": "v-realonlyForm-btn",
  "layout": {
    "form": {
      "controls": [
        {
          "aggregateIndicatorCode": {
            "label": "聚合指标代码",
            "control": "string",
            "span": 12
          },
          "aggregateIndicatorName": {
            "label": "聚合指标名称",
            "control": "string",
            "span": 12
          }
        },
        {
          "aggregateSource": {
            "label": "聚合指标数据来源",
            "control": "string",
            "span": 12,
            "dict": {
              "dictType": "aggregateSource",
              "dictKey": [],
              "format": "value-label",
              "doRequest": {
                "url": "/views/risk/dictdata/aggregateSource.json",
                "method": "get"
              }
            },
            "showItem": {
              "24HourTrxn":["cruleId","cruleName"],
              "24HourBehavior":["cruleId","cruleName"],
              "":[]
            }
          },
          "aggregateFunction": {
            "label": "聚合函数",
            "control": "string",
            "span": 12,
            "dict": {
              "dictType": "aggregateFunction",
              "dictKey": [],
              "format": "value-label",
              "doRequest": {
                "url": "/views/risk/dictdata/aggregateFunction.json",
                "method": "get"
              }
            }
          }
        },
        {
          "indicatorDimension": {
            "label": "指标维度",
            "control": "string",
            "span": 12,
            "dict": {
              "dictType": "indicatorDimension",
              "dictKey": [],
              "format": "value-label",
              "doRequest": {
                "url": "/views/risk/dictdata/indicatorDimension.json",
                "method": "get"
              }
            }
          },
          "dataType": {
            "label": "指标数据类型",
            "control": "string",
            "span": 12,
            "dict": {
              "dictType": "dataType",
              "dictKey": [],
              "format": "value-label",
              "doRequest": {
                "url": "/views/risk/dictdata/dataType.json",
                "method": "get"
              }
            }
          }
        },
        {
          "aggregateFieldCode": {
            "label": "聚合字段代码",
            "control": "string",
            "span": 12
          },
          "aggregateFieldName": {
            "label": "聚合字段名称",
            "control": "string",
            "span": 12
          }
        },
        {
          "cruleId": {
            "label": "过滤规则",
            "control": "string",
            "initShow": false,
            "span": 12
          },
          "cruleName": {
            "label": "过滤规则名称",
            "control": "string",
            "initShow": false,
            "span": 12
          }
        },
        {
          "groupDemensionList": {
            "label": "指标分组维度",
            "control": "string",
            "multiple": true,
            "span": 24,
            "dict": {
              "dictType": "statisticField",
              "dictKey": [],
              "format": "value-label",
              "doRequest": {
                "url": "/API/risk-management-service/combobox-select/fact/trans",
                "method": "post"
              }
            }
          }
        },
        {
          "containsCurrent": {
            "label": "是否包含当笔交易",
            "control": "string",
            "span": 12,
            "dict": {
              "dictType": "yesOrNo",
              "dictKey": [],
              "format": "value-label",
              "doRequest": {
                "url": "/views/risk/dictdata/yesOrNo.json",
                "method": "get"
              }
            }
          },
          "aggregateUnit": {
            "label": "聚合时间单位",
            "control": "string",
            "span": 12,
            "dict": {
              "dictType": "controlUnit",
              "dictKey": [],
              "format": "value-label",
              "doRequest": {
                "url": "/views/risk/dictdata/controlUnit.json",
                "method": "get"
              }
            }
          }
        },
        {
          "aggregateStart": {
            "label": "聚合起始时间",
            "control": "string",
            "span": 12
          },
          "aggregateNumber": {
            "label": "聚合截止时间",
            "control": "string",
            "span": 12
          }
        },
        {
          "updateUser": {
            "label": "最后更新人",
            "control": "string",
            "span": 12
          },
          "updateTime": {
            "label": "最后更新时间",
            "control": "string",
            "type": "dateTimePicker",
            "format": "yyyy-MM-dd HH:mm:ss",
            "span": 12
          }
        },
        {
          "description": {
            "label": "备注",
            "control": "string",
            "span": 12
          }
        }
      ],
      "initRequest": {
        "url": "/API/risk-management-service/risk-aggregate-indicator/query",
        "method": "post"
      }
    }
  }
}
