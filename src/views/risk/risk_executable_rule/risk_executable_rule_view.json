{
  "module": "v-realonlyForm-btn",
  "layout": {
    "form": {
      "controls": [
        {
          "eruleId": {
            "label": "规则id",
            "control": "string",
            "span": 12
          },
          "eruleName": {
            "label": "规则名称",
            "control": "string",
            "span": 12
          }
        },
        {
          "cruleId": {
            "label": "组合规则ID",
            "control": "string",
            "span": 12
          },
          "cruleCode": {
            "label": "组合规则代码",
            "control": "string",
            "span": 12
          }
        },
        {
          "cruleVersion": {
            "label": "规则版本",
            "control": "string",
            "span": 12
          },
          "resultHandlers": {
            "label": "规则匹配处置措施",
            "control": "string",
            "span": 12
          }
        },
        {
          "executableExpression": {
            "label": "规则可执行表达式",
            "control": "string",
            "span": 24
          }
        },
        {
          "effectDate": {
            "label": "生效日期",
            "control": "string",
            "type": "dateTimePicker",
            "format": "yyyy-MM-dd HH:mm:ss",
            "span": 12
          },
          "expiryDate": {
            "label": "失效日期",
            "control": "string",
            "type": "dateTimePicker",
            "format": "yyyy-MM-dd HH:mm:ss",
            "span": 12
          }
        },
        {
          "updateUser": {
            "label": "最后更新人",
            "control": "string",
            "span": 12
          },
          "updateTime": {
            "label": "最后修改日期",
            "control": "string",
            "format": "yyyy-MM-dd HH:mm:ss",
            "span": 12
          }
        },
        {
          "description": {
            "label": "备注",
            "control": "string",
            "span": 24
          }
        }
      ],
      "initRequest": "${{params}}"
    }
  }
}
