{
  "module": "v-search-btn-datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "cruleId": {
            "label": "规则id",
            "control": "text",
            "span": 12,
            "rules": [
              {
                "message": "字段长度超限",
                "max": 8
              }
            ]
          },
          "cruleName": {
            "label": "规则名称",
            "control": "text",
            "span": 12,
            "rules": [
              {
                "message": "字段长度超限",
                "max": 100
              }
            ]
          }
        },
        {
          "cruleCode": {
            "label": "规则代码",
            "control": "text",
            "span": 12,
            "rules": [
              {
                "message": "字段长度超限",
                "max": 40
              }
            ]
          },
          "cruleVersion": {
            "label": "规则版本",
            "control": "text",
            "span": 12
          }
        }
      ]
    },
    "datagrid": {
      "doRequest": {
        "url": "/API/risk-management-service/risk-combine-rule/page-select",
        "method": "post",
        "params": {
          "cruleType": "TR"
        }
      },
      "pagination": {
        "pageSizes": [
          5,
          10,
          20,
          50,
          100
        ],
        "pageSize": 5
      },
      "thead": [
        {
          "label": "规则id",
          "prop": "cruleId",
          "width": "100px",
          "align": "center"
        },
        {
          "label": "规则名称",
          "prop": "cruleName"

        },
        {
          "label": "规则代码",
          "prop": "cruleCode",
          "width": "200px",
          "align": "center"
        },
        {
          "label": "规则版本",
          "prop": "cruleVersion",
          "width": "80px",
          "align": "center"
        },
        {
          "label": "操作",
          "width": "70px",
          "align": "center",
          "btns": [
            {
              "type": "bringback",
              "label": "选择"
            }
          ]
        }
      ]
    }
  }
}
