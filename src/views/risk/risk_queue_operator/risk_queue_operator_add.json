{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "controls": [
        {
          "queueId": {
            "label": "队列ID",
            "control": "text",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请正确输入",
                "max": 20
              }
            ]
          },
          "operatorId": {
            "label": "操作员ID",
            "control": "text",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请正确输入",
                "max": 40
              }
            ]
          }
        },
        {
          "operatorName": {
            "label": "操作员名称",
            "control": "text",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请正确输入",
                "max": 100
              }
            ]
          },
          "remark": {
            "label": "备注",
            "control": "textarea",
            "span": 12,
            "rows": 1,
            "rules": [
              {
                "message": "请正确输入",
                "max": 500
              }
            ]
          }
        }
      ],
      "doRequest": {
        "url": "/API/risk-management-service/risk-queue-operator/add",
        "method": "POST",
        "params": {
          "createUser": "$g{{user_info.userCode}}",
          "updateUser": "$g{{user_info.userCode}}"
        }
      }
    }
  }
}