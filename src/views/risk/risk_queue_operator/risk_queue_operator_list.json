{
  "module": "v-search-btn-datagrid",
  "pageId": "risk_queue_operator",
  "layout": {
    "form": {
      "controls": [
        {
          "queueId": {
            "label": "队列ID",
            "control": "text",
            "span": 8,
            "rules": [
              {
                "message": "请正确输入",
                "max": 20
              }
            ]
          },
          "operatorId": {
            "label": "队列操作员",
            "control": "text",
            "span": 8,
            "rules": [
              {
                "message": "请正确输入",
                "max": 40
              }
            ]
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "label": "新增",
          "authId": "queue_operator_add_btn",
          "toRequest": {
            "url": "/views/risk/risk_queue_operator/risk_queue_operator_add.json",
            "method": "get",
            "params": {}
          },
          "props": {
            "width": "70%",
            "title": "新增"
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/API/risk-management-service/risk-queue-operator/page-select",
        "method": "post"
      },
      "thead": [
        {
          "label": "队列ID",
          "prop": "queueId",
          "align": "center"
        },
        {
          "label": "操作员ID",
          "prop": "operatorId",
          "align": "center"
        },
        {
          "label": "操作员名称",
          "prop": "operatorName",
          "align": "center"
        },
        {
          "label": "创建人",
          "prop": "createUser",
          "align": "center"
        },
        {
          "label": "创建日期",
          "prop": "createTime",
          "type": "dateTimePicker",
          "format": "yyyy-MM-dd HH:mm:ss",
          "align": "center"
        },
        {
          "label": "备注",
          "prop": "remark",
          "align": "center"
        },
        {
          "label": "最后更新人",
          "prop": "updateUser",
          "align": "center"
        },
        {
          "label": "最后修改日期",
          "prop": "updateTime",
          "type": "dateTimePicker",
          "format": "yyyy-MM-dd HH:mm:ss",
          "align": "center"
        },
        {
          "label": "操作",
          "width": "150px",
          "btns": [
            {
              "type": "edit",
              "label": "编缉",
              "authId": "queue_operator_upd_btn",
              "toRequest": {
                "url": "/views/risk/risk_queue_operator/risk_queue_operator_edit.json",
                "method": "get"
              },
              "props": {
               
              }
            },
            {
              "type": "look",
              "label": "查看",
              "toRequest": {
                "url": "/views/risk/risk_queue_operator/risk_queue_operator_view.json",
                "method": "get"
              },
              "props": {
               
              }
            },
            {
              "type": "delete",
              "authId": "queue_operator_del_btn",
              "label": "删除",
              "doRequest": {
                "url": "/API/risk-management-service/risk-queue-operator/delete",
                "method": "post"
              }
            }
          ]
        }
      ]
    }
  }
}
