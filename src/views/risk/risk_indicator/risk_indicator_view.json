{
  "module": "v-realonlyForm-btn",
  "layout": {
    "form": {
      "controls": [
        {
          "indicatorCode": {
            "label": "指标变量代码",
            "control": "string",
            "span": 12
          },
          "sourceType": {
            "label": "指标来源",
            "control": "string",
            "span": 12,
            "value":"Inner",
            "dict": {
              "dictType": "sourceType",
              "dictKey": [],
              "format": "value-label",
              "doRequest": {
                "url": "/views/risk/dictdata/sourceType.json",
                "method": "get"
              }
            }
          }
        },
        {
          "indicatorName": {
            "label": "指标变量名称",
            "control": "string",
            "span": 12
          },
          "indicatorType": {
            "label": "指标变量类型",
            "control": "string",
            "span": 12,
            "dict": {
              "dictType": "indicatorType",
              "dictKey": [],
              "format": "value-label",
              "doRequest": {
                "url": "/views/risk/dictdata/indicatorType.json",
                "method": "get"
              }
            }
          }
        },
        {
          "dataType": {
            "label": "指标数据类型",
            "control": "string",
            "span": 12,
            "dict": {
              "dictType": "dataType",
              "dictKey": [],
              "format": "value-label",
              "doRequest": {
                "url": "/views/risk/dictdata/dataType.json",
                "method": "get"
              }
            }
          },
          "sourceFile": {
            "label": "来源文件名",
            "control": "string",
            "span": 12
          }
        },
        {
          "updateUser": {
            "label": "最后更新人",
            "control": "string",
            "span": 12
          },
          "updateTime": {
            "label": "最后修改日期",
            "control": "string",
            "type": "dateTimePicker",
            "format": "yyyy-MM-dd HH:mm:ss",
            "span": 12
          }
        },
        {
          "description": {
            "label": "描述",
            "control": "string",
            "span": 24
          }
        }
      ],
      "initRequest": "${{params}}"
    }
  }
}
