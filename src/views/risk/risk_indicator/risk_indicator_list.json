{
  "module": "v-search-btn-datagrid",
  "pageId": "risk_indicator",
  "layout": {
    "form": {
      "controls": [
        {
          "indicatorName": {
            "label": "指标变量名称",
            "control": "text",
            "span": 8,
            "rules": [
              {
                "message": "字段长度超限",
                "max": 100
              }
            ]
          },
          "indicatorCode": {
            "label": "指标变量代码",
            "control": "text",
            "span": 8,
            "rules": [
              {
                "message": "字段长度超限",
                "max": 40
              }
            ]
          },
          "indicatorType": {
            "label": "指标变量类型",
            "control": "select",
            "span": 8,
            "dict": {
              "dictType": "indicatorType",
              "dictKey": [],
              "format": "value-label",
              "doRequest": {
                "url": "/views/risk/dictdata/indicatorType.json",
                "method": "get"
              }
            }
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "authId": "indicator_add_btn",
          "label": "新增",
          "toRequest": {
            "url": "/views/risk/risk_indicator/risk_indicator_add.json",
            "method": "get",
            "params": {}
          },
          "props": {
            "width": "70%",
            "title": "新增"
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/API/risk-management-service/risk-indicator/page-select",
        "method": "post"
      },
      "thead": [
        {
          "label": "指标变量名称",
          "prop": "indicatorName"
        },
        {
          "label": "指标变量代码",
          "prop": "indicatorCode"
        },
        {
          "label": "指标变量类型",
          "prop": "indicatorType",
          "dict": {
            "dictType": "indicatorType",
            "dictKey": [],
            "format": "label",
            "doRequest": {
              "url": "/views/risk/dictdata/indicatorType.json",
              "method": "get"
            }
          }
        },
        {
          "label": "指标数据类型",
          "prop": "dataType",
          "dict": {
            "dictType": "dataType",
            "dictKey": [],
            "doRequest": {
              "url": "/views/risk/dictdata/dataType.json",
              "method": "get"
            }
          }
        },
        {
          "label": "指标来源",
          "prop": "sourceType",
          "dict": {
            "dictType": "sourceType",
            "dictKey": [],
            "format": "label",
            "doRequest": {
              "url": "/views/risk/dictdata/sourceType.json",
              "method": "get"
            }
          }
        },
        {
          "label": "操作",
          "width": "150px",
          "btns": [
            {
              "type": "edit",
              "authId": "indicator_upd_btn",
              "label": "编缉",
              "toRequest": {
                "url": "/views/risk/risk_indicator/risk_indicator_edit.json",
                "method": "get"
              },
              "props": {
                "width": "70%"
              }
            },
            {
              "type": "look",
              "label": "查看",
              "toRequest": {
                "url": "/views/risk/risk_indicator/risk_indicator_view.json",
                "method": "get"
              },
              "props": {
                "width": "70%"
              }
            },
            {
              "type": "delete",
              "authId": "indicator_del_btn",
              "label": "删除",
              "doRequest": {
                "url": "/API/risk-management-service/risk-indicator/delete",
                "method": "post"
              }
            }
          ]
        }
      ]
    }
  }
}
