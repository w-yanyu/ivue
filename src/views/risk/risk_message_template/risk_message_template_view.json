{
  "module": "v_realonlyForm_btn",
  "layout": {
    "form": {
      "controls": [
        {
          "templateCode": {
            "label": "消息模板代码",
            "control": "string",
            "span": 12
          },
          "status": {
            "label": "状态",
            "control": "string",
            "span": 12,
            "dict": {
              "dictType": "status",
              "dictKey": [],
              "format": "value - label",
              "doRequest": {
                "url": "/views/risk/dictdata/enabledStatus.json",
                "method": "get"
              }
            }
          }
        },
        {
          "templateContent": {
            "label": "消息模板内容",
            "control": "textarea",
            "span": 24,
            "autosize": {
              "minRows": 4,
              "maxRows": 6
            },
            "disabled": true
          }
        },
        {
          "description": {
            "label": "描述",
            "control": "textarea",
            "span": 24,
            "autosize": {
              "minRows": 4,
              "maxRows": 6
            },
            "disabled": true
          }
        },
        {
          "createUser": {
            "label": "创建人",
            "control": "string",
            "span": 11
          },
          "createTime": {
            "label": "创建时间",
            "control": "string",
            "format": "yyyy-MM-dd HH:mm:ss",
            "span": 11
          }
        },
        {
          "updateUser": {
            "label": "最后修改人",
            "control": "string",
            "span": 11
          },
          "updateTime": {
            "label": "最后修改时间",
            "control": "string",
            "format": "yyyy-MM-dd HH:mm:ss",
            "span": 11
          }
        }
      ],
      "initRequest": {
        "url": "/API/risk-management-service/risk-message-template/query",
        "method": "post"
      }
    }
  }
}
