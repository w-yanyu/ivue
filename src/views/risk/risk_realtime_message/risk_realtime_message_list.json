{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "mobilePhoneNumber": {
            "label": "手机号",
            "control": "text",
            "span": 8,
            "rules": [
              {
                "max": 20,
                "message": "请正确输入"
              }
            ]
          },
          "customerId": {
            "label": "客户号",
            "control": "text",
            "span": 8,
            "rules": [
              {
                "max": 40,
                "message": "请正确输入"
              }
            ]
          },
          "cardNumber": {
            "label": "卡号",
            "control": "text",
            "span": 8,
            "rules": [
              {
                "max": 19,
                "message": "请正确输入"
              }
            ]
          }
        },
        {
          "accountId": {
            "label": "账号",
            "control": "text",
            "span": 8,
            "rules": [
              {
                "max": 40,
                "message": "请正确输入"
              }
            ]
          },
          "sendTimeStart": {
            "label": "发送开始日期",
            "control": "dateTimePicker",
            "format": "yyyy-MM-dd",
            "span": 8
          },
          "sendTimeEnd": {
            "label": "发送结束日期",
            "control": "dateTimePicker",
            "format": "yyyy-MM-dd",
            "span": 8
          }
        },
        {
          "status": {
            "label": "状态",
            "control": "select",
            "span": 8,
            "dict": {
              "dictType": "smsSendStatus",
              "dictKey": [],
              "format": "value-label",
              "doRequest": {
                "url": "/views/risk/dictdata/smsSendStatus.json",
                "method": "get"
              }
            }
          }
        }
      ]
    },
    "toolbar": {
      "btns": []
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/API/risk-management-service/risk-realtime-message/findByPageAndType",
        "method": "post"
      },
      "thead": [
        {
          "label": "消息ID",
          "prop": "messageId"
        },
        {
          "label": "消息模板代码",
          "prop": "templateCode",
          "dict": {
            "dictType": "risk-message-template",
            "dictKey": [],
            "format": "label",
            "doRequest": {
              "url": "/API/risk-management-service/combobox-select/risk-message-template",
              "params": {},
              "method": "POST"
            }
          }
        },
        {
          "label": "消息内容",
          "prop": "messageContent"
        },
        {
          "label": "手机号",
          "prop": "mobilePhoneNumber"
        },
        {
          "label": "客户号",
          "prop": "customerId"
        },
        {
          "label": "卡号",
          "prop": "cardNumber"
        },
        {
          "label": "账号",
          "prop": "accountId"
        },
        {
          "label": "发送时间",
          "prop": "sendTime",
          "type": "dateTimePicker",
          "format": "yyyy-MM-dd HH:mm:ss"
        },
        {
          "label": "状态",
          "prop": "status",
          "dict": {
            "dictType": "smsSendStatus",
            "dictKey": [],
            "format": "label",
            "doRequest": {
              "url": "/views/risk/dictdata/smsSendStatus.json",
              "method": "get"
            }
          }
        },
        {
          "label": "操作",
          "btns": [
            {
              "type": "look",
              "label": "查看",
              "toRequest": {
                "url": "/views/risk/risk_realtime_message/risk_realtime_message_view.json",
                "method": "get"
              },
              "props": {
               
              }
            }
          ]
        }
      ],
      "pagination": {
        "show": true,
        "pageSizes": [
          "10",
          "50",
          "100",
          "200"
        ],
        "pageSize": 10
      }
    }
  }
}
