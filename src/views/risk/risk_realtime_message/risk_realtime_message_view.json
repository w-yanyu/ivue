{
  "module": "v_realonlyForm_btn",
  "layout": {
    "form": {
      "controls": [
        {
          "messageId": {
            "label": "消息ID",
            "control": "string",
            "span": 12
          },
          "createDate": {
            "label": "创建日期",
            "control": "string",
            "span": 12
          }
        },
        {
          "templateCode": {
            "label": "消息模板代码",
            "control": "string",
            "span": 12,
            "dict": {
              "dictType": "risk-message-template",
              "dictKey": [],
              "format": "value-label",
              "doRequest": {
                "url": "/API/risk-management-service/combobox-select/risk-message-template",
                "params": {},
                "method": "POST"
              }
            }
          },
          "variablesValue": {
            "label": "消息变量值",
            "control": "string",
            "span": 12
          }
        },
        {
          "mobilePhoneNumber": {
            "label": "手机号",
            "control": "string",
            "span": 12
          },
          "customerId": {
            "label": "客户号",
            "control": "string",
            "span": 12
          }
        },
        {
          "cardNumber": {
            "label": "卡号",
            "control": "string",
            "span": 12
          },
          "accountId": {
            "label": "账号",
            "control": "string",
            "span": 12
          }
        },
        {
          "sendTime": {
            "label": "发送时间",
            "control": "string",
            "span": 12,
            "type": "dateTimePicker",
            "format": "yyyy-MM-dd HH:mm:ss"
          },
          "status": {
            "label": "状态",
            "control": "string",
            "span": 12,
            "dict": {
              "dictType": "smsSendStatus",
              "dictKey": [],
              "format": "value-label",
              "doRequest": {
                "url": "/views/risk/dictdata/smsSendStatus.json",
                "method": "get"
              }
            }
          }
        },
        {
          "messageContent": {
            "label": "消息内容",
            "control": "string",
            "span": 24
          }
        },
        {
          "description": {
            "label": "描述",
            "control": "string",
            "span": 24
          }
        },
        {
          "createTime": {
            "label": "创建日期",
            "control": "string",
            "span": 12,
            "type": "dateTimePicker",
            "format": "yyyy-MM-dd HH:mm:ss"
          },
          "createUser": {
            "label": "创建人",
            "control": "string",
            "span": 12
          }
        },
        {
          "updateTime": {
            "label": "最后修改日期",
            "control": "string",
            "span": 12,
            "type": "dateTimePicker",
            "format": "yyyy-MM-dd HH:mm:ss"
          },
          "updateUser": {
            "label": "最后修改人",
            "control": "string",
            "span": 12
          }
        }
      ],
      "initRequest": {
        "url": "/API/risk-management-service/risk-realtime-message/selectByPrimaryKey",
        "method": "post"
      }
    }
  }
}
