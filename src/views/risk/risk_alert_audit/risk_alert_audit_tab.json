{
  "module": "dynamicTabs",
  "layout": {
    "tabs": {
      "scope": "templateScope",
      "tabHeader": [
        {
          "id": "baseInfo1",
          "label": "审核处理",
          "closable": false,
          "disabled": false,
          "icon": "el-icon-date",
          "toRequest": {
            "url": "/views/risk/risk_alert_audit/risk_alert_audit_audit.json",
            "params": {
              "idField": {
                "reviewId": "${{params.reviewId}}",
                "alertId": "${{params.alertId}}"
              }
            }
          },
          "params": {},
          "props": {
           
          }
        },
        {
          "id": "resetPwd",
          "label": "警报详情",
          "icon": "el-icon-date",
          "closable": true,
          "disabled": false,
          "toRequest": {
            "url": "/views/risk/risk_alert_assign/risk_alert_assign_detail.json",
            "params": {
              "idField": {
                "alertId": "${{params.alertId}}"
              }
            }
          },
          "params": {},
          "props": {
           
          }
        },
        {
          "id": "resetPwd1",
          "label": "交易信息",
          "icon": "el-icon-date",
          "closable": true,
          "disabled": false,
          "toRequest": {
            "url": "/views/risk/risk_alert_assign/risk_alert_assign_trxn.json",
            "params": {
              "trxnSeq": "${{params.alertTrxnSeq}}"
            }
          },
          "params": {},
          "props": {
           
          }
        }
      ],
      "defaultActiveKey": "baseInfo1",
      "position": "top",
      "styleClass": "card"
    }
  }
}