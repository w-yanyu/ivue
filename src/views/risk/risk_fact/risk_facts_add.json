{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "createUser": "$g{{user_info.userCode}}",
          "updateUser": "$g{{user_info.userCode}}",
          "servicecode": "deci010102"
        }
      },
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "deci010109"
        }
      },
      "controls": [
        {
          "fact_code": {
            "label": "事实代码",
            "width":"auto",
            "control": "text",
            "span": 8,
            "disabled": true,
            "rules": [
              {
                "required": true,
                "pattern": "^[A-Za-z][A-Za-z\\d_]{0,59}$",
                "message": "请正确输入"
              }
            ]
          },
          "fact_scene": {
            "label": "事实场景",
            "control": "select",
            "span": 8,
            "rules": [
              {
                "required": true,
                "message": "请输入事实场景",
                "max": 100
              }
            ],
            "width": "auto",
            "dict": {
              "dictType": "RISK_E_FACTUAL_SCENARIO",
              "dictKey": [
                "RISK_E_FACTUAL_SCENARIO"
              ],
              "format": "label"
            }
          },
          "fact_name": {
            "label": "事实名称",
            "width":"auto",
            "control": "text",
            "span": 8,
            "rules": [
              {
                "required": true,
                "pattern": "^[a-z0-9A-Z\u4e00-\u9fa5]+$",
                "message": "仅可为字母、数字、文字及相关组合"
              }
            ]
          }
        },
        {
          "description": {
            "label": "备注",
            "width":"auto",
            "control": "textarea",
            "span": 24,
            "maxlength": 200,
            "showWordLimit": 200,
            "rows": 5,
            "rules": [
              {
                "message": "请正确输入",
                "max": 200
              }
            ]
          }
        }
      ]
    }
  }
}