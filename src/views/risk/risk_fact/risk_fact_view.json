{
  "module": "v-realonlyForm-btn",
  "layout": {
    "form": {
      "controls": [
        {
          "fact_code": {
            "label": "事实代码",
            "control": "select",
            "span": 12,
            "dict": {
              "dictType": "factCode",
              "dictKey": [],
              "format": "value-label",
              "doRequest": {
                "url": "/API/risk-management-service/combobox-select/fact-without-field",
                "method": "post"
              }
            },
            "disabled":true
          },
          "field_code": {
            "label": "事实属性代码",
            "control": "string",
            "span": 12
          }
        },
        {
          "field_name": {
            "label": "事实属性名称",
            "control": "string",
            "span": 12
          },
          "field_type": {
            "label": "事实属性类型",
            "control": "select",
            "span": 12,
            "dict": {
              "dictType": "dataType",
              "dictKey": [],
              "format": "value-label",
              "doRequest": {
                "url": "/views/risk/dictdata/dataType.json",
                "method": "get"
              }
            },
            "disabled":true
          }
        },
        {
          "referenceCollectionCode": {
            "label": "引用集合代码",
            "control": "select",
            "span": 12,
            "dict": {
              "dictType": "rule_constant",
              "dictKey": [],
              "format": "value-label",
              "doRequest": {
                "url": "/API/risk-management-service/combobox-select/rule_constant",
                "method": "post"
              }
            },
            "disabled":true
          },
          "description": {
            "label": "备注",
            "control": "string",
            "span": 12
          }
        },
        {
          "data_update_user": {
            "label": "最后修改人",
            "control": "string",
            "span": 12
          },
          "data_update_time": {
            "label": "最后修改时间",
            "control": "string",
            "type": "dateTimePicker",
            "format": "yyyy-MM-dd HH:mm:ss",
            "span": 12
          }
        }
      ],
      "initRequest": "${{params}}"
    }
  }
}
