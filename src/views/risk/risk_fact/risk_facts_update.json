{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "btns":[],
      "controlsGroup": [
        {
          "title": "事实信息",
          "tooltip": "事实信息",
          "titleIcon": "el-icon-info",
          "expand": true,
          "accordion": false,
          "controls": [
            {
              "info01": {
                "span": 24,
                "control": "dynamic",
                "toRequest": {
                  "url": "/views/risk/risk_fact/risk_facts_info.json",
                  "params":{
                    "fact_code":"${{params.fact_code}}",
                    "fact_name":"${{params.fact_name}}",
                    "fact_scene":"${{params.fact_scene}}",
                    "description":"${{params.description}}"
                  }
                },
                "props": {
                 
                }
              }
            }
          ]
        },
        {
          "title": "属性信息",
          "tooltip": "属性信息",
          "titleIcon": "el-icon-info",
          "expand": true,
          "accordion": false,
          "controls": [
            {
              "info02": {
                "span": 24,
                "control": "dynamic",
                "toRequest": {
                  "url": "/views/risk/risk_fact/attribute/risk_attribute_list.json",
                  "params":{
                    "fact_code": "${{params.fact_code}}",
                    "flag":"1"
                  }
                },
                "props": {
                 
                }
              }
            }
          ]
        }
      ]
    }
  }
}