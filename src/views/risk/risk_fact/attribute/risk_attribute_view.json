{
    "module": "v-inputDataForm-btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "btns":[],
            "controls": [
                {
                    "field_code": {
                        "label": "属性代码",
                        "control": "string",
                        "span": 12  ,
                        "rules": [
                            {
                                "required": true
                            }
                        ]
                    },
                    "field_name": {
                        "label": "属性名称",
                        "control": "string",
                        "span": 12 ,
                        "rules": [
                            {
                                "required": true
                            }
                        ]
                    }
                },
                {
                    "field_type": {
                        "label": "属性类型",
                        "control": "select",
                        "valueField":"dictId",
                        "labelField":"dictName",
                        "disabled":true,
                        "span": 12, 
                        "rules": [
                            {
                                "required": true
                            }
                        ],
                        "dict": {
                            "dictType": "dataType",
                            "dictKey": [],
                            "format": "value-label",
                            "doRequest": {
                                "url": "/views/risk/dictdata/dataType.json",
                                "method": "get"
                            }
                        }
                    }
                }
            ]
        }
    }
}