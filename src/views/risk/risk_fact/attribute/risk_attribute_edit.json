{
    "module": "v-inputDataForm-btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
           
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "post",
                "params": {
                    "servicecode": "deci010106",
                    "fact_code": "${{params.fact_code}}",
                    "updateUser": "$g{{user_info.userCode}}"
                }
            },
            "controls": [
                {
                    "field_code": {
                        "label": "属性代码",
                        "control": "string",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "pattern": "^[A-Za-z][A-Za-z\\d_]{0,59}$",
                                "message": "该控件必填"
                            }
                        ]
                    },
                    "field_name": {
                        "label": "属性名称",
                        "control": "text",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入属性名称"
                            }
                        ]
                    }
                },
                {
                    "field_type": {
                        "label": "属性类型",
                        "control": "select",
                        "span": 12,
                        "valueField":"dictId",
                        "labelField":"dictName",
                        "dict": {
                            "dictType": "dataType",
                            "dictKey": [],
                            "format": "value-label",
                            "doRequest": {
                                "url": "/views/risk/dictdata/dataType.json",
                                "method": "get"
                            }
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择属性类型",
                                "max": 100
                            }
                        ]
                    }
                }
            ]
        }
    }
}