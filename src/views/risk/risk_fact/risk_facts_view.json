{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "btns": [],
      "controls": [
        {
          "fact_code": {
            "label": "事实代码",
            "width":"auto",
            "control": "string",
            "span": 8,
            "rules": [
              {
                "required":true
              }
            ]
          },
          "fact_scene": {
            "label": "事实场景",
            "control": "select",
            "span": 8,
            "width": "auto",
            "disabled": true,
            "dict": {
              "dictType": "RISK_E_FACTUAL_SCENARIO",
              "dictKey": [
                "RISK_E_FACTUAL_SCENARIO"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required":true
              }
            ]
          },
          "fact_name": {
            "label": "事实名称",
            "width":"auto",
            "control": "string",
            "span": 8,
            "rules": [
              {
                "required":true
              }
            ]
          }
        },
        {
          "data_update_time": {
            "label": "最新维护时间",
            "width":"auto",
            "control": "text",
            "span": 8,
            "disabled": true,
            "rules": [
              {
                "required":true
              }
            ]
          }
        },
        {
          "description": {
            "label": "备注",
            "width":"auto",
            "control": "textarea",
            "disabled": true,
            "span": 24,
            "maxlength": 200,
            "showWordLimit": 200,
            "rows": 5,
            "rules": [
              {
                "message": "请正确输入",
                "max": 200
              }
            ]
          }
        },
        {
          "attributesInfo": {
            "label": "事实属性",
            "width":"auto",
            "initShow": true,
            "span": 24,
            "control": "dynamic",
            "toRequest": {
              "url": "/views/risk/risk_fact/attribute/risk_attribute_list.json",
              "method": "get",
              "params": {
                "fact_code": "${{params.fact_code}}",
                "flag": "0"
              }
            },
            "props": {
              
            }
          }
        }
      ]
    }
  }
}