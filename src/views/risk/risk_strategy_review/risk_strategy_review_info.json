{
  "module": "v-cardList4",
  "init": {
    "initRequest": "${{parentInitData}}"
  },
  "layout": {
    "card": {
      "span": {
        "xs": 12,
        "sm": 12,
        "md": 12,
        "lg": 12,
        "xl": 12
      },
      "list": [
        {
          "label": "策略配置(旧)",
          "toRequest": {
            "url": "/views/risk/risk_strategy_review/risk_strategy_review_old.json",
            "method": "get"
          },
          "props": {
            
          }
        },
        {
          "label": "策略配置(新)",
          "toRequest": {
            "url": "/views/risk/risk_strategy_review/risk_strategy_review_new.json",
            "method": "get",
            "params":{
              "isString":"${{params.isUpdate}}",
              "isShowGroup": "${{params.isShow}}",
              "isShowGroupList": "${{params.isShowList}}"
            }
          },
          "props": {
            
          }
        }
      ]
    }
  }
}
