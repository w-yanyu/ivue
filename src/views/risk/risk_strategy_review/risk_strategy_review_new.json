{
  "module": "v-realonlyForm-btn",
  "init": {
    "initRequest": "${{parentInitData}}"
  },
  "layout": {
    "form": {
      "controls": [
        {
          "strategyNameNew": {
            "label": "策略名称",
            "control": "text",
            "disabled": "${{params.isString}}",
            "span": 24
          }
        },
        {
          "transTypeNew": {
            "label": "交易种类",
            "control": "select",
            "disabled": "${{params.isString}}",
            "span": 24,
            "dict": {
              "dictType": "transType",
              "dictKey": [],
              "format": "value-label",
              "doRequest": {
                "url": "/views/risk/dictdata/transType.json",
                "method": "get"
              }
            }
          }
        },
        {
          "transClassNew": {
            "label": "交易类型",
            "disabled": "${{params.isString}}",
            "control": "select",
            "span": 24,
            "dict": {
              "dictType": "transClass",
              "dictKey": [],
              "format": "value-label",
              "doRequest": {
                "url": "/views/risk/dictdata/transClass.json",
                "method": "get"
              }
            }
          }
        },
        {
          "businessSceneNew": {
            "label": "业务场景",
            "disabled": "${{params.isString}}",
            "control": "select",
            "span": 24,
            "dict": {
              "dictType": "businessScene",
              "dictKey": [],
              "format": "value-label",
              "doRequest": {
                "url": "/views/risk/dictdata/businessScene.json",
                "method": "get"
              }
            }
          }
        },
        {
          "handlerModeNew": {
            "label": "判定模式",
            "disabled": "${{params.isString}}",
            "control": "select",
            "span": 24,
            "dict": {
              "dictType": "handlerMode",
              "dictKey": [],
              "format": "value-label",
              "doRequest": {
                "url": "/views/risk/dictdata/handlerMode.json",
                "method": "get"
              }
            }
          }
        },
        {
          "ruleGroupsNew": {
            "label": "规则组",
            "control": "string",
            "initShow": {
              "expression":"${{params.isShowGroup}}"
            },
            "disabled": "${{params.isString}}",
            "span": 24
          },
          "ruleGroupNewList": {
            "label": "规则组",
            "control": "lookup",
            "span": 24,
            "initShow": {
              "expression":"${{params.isShowGroupList}}"
            },
            "disabled": "${{params.isString}}",
            "multiple": true,
            "keyField": {
              "ruleGroupNewList":"groupId"
            },
            "toRequest":{
              "url": "/views/risk/risk_transaction_diversion/rule_group_query.json",
              "method": "get",
              "params": {
              }
            },
            "props": {
              "width": "55%",
              "title": "规则组查询",
              "close": "",
              "target": ""
            }
          }
        },
        {
          "effectStatusNew": {
            "label": "生效状态",
            "disabled": "${{params.isString}}",
            "control": "select",
            "span": 24,
            "dict": {
              "dictType": "effectStatus",
              "dictKey": [],
              "format": "value-label",
              "doRequest": {
                "url": "/views/risk/dictdata/effectStatus.json",
                "method": "get"
              }
            }
          }
        },
        {
          "descriptionNew": {
            "label": "描述",
            "control": "text",
            "disabled": "${{params.isString}}",
            "span": 24
          }
        }
      ]
    }
  }
}
