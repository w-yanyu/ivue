{
  "module": "v-realonlyForm-btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/API/risk-management-service/risk_strategy_review/query",
        "method": "post"
      },
      "controls": [
        {
          "reviewId": {
            "label": "审核ID",
            "control": "string",
            "span": 12
          },
          "strategyId": {
            "label": "策略ID",
            "control": "string",
            "span": 12
          }
        },
        {
          "reviewType": {
            "label": "审核类型",
            "control": "string",
            "span": 12,
            "dict": {
              "dictType": "reviewType",
              "dictKey": [],
              "format": "value-label",
              "doRequest": {
                "url": "/views/risk/dictdata/reviewType.json",
                "method": "get"
              }
            }
          },
          "reviewStatus": {
            "label": "审核状态",
            "control": "string",
            "span": 12,
            "dict": {
              "dictType": "reviewStatus",
              "dictKey": [],
              "format": "value-label",
              "doRequest": {
                "url": "/views/risk/dictdata/reviewStatus.json",
                "method": "get"
              }
            }
          }
        },
        {
          "reviewInfo": {
            "span": 24,
            "control": "dynamic",
            "toRequest": {
              "url": "/views/risk/risk_strategy_review/risk_strategy_review_info.json",
              "params":{
                "isUpdate": true,
                "isShow": true,
                "isShowList": false
              }
            },
            "props": {
              
            }
          }
        },
        {
          "reviewUser": {
            "label": "审核人",
            "control": "string",
            "span": 12
          },
          "reviewTime": {
            "label": "审核时间",
            "control": "string",
            "type": "dateTimePicker",
            "format": "yyyy-MM-dd HH:mm:ss",
            "span": 12
          }
        },
        {
          "reviewRemark": {
            "label": "审核意见",
            "control": "string",
            "span": 24
          }
        }
      ]
    }
  }
}
