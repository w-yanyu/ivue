{
  "module": "v-search-btn-datagrid",
  "layout": {
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "deci010632",
          "rule_code": "${{params.tree_code}}"
        }
      },
      "pagination": {
        "hideOnSinglePage": false,
        "show": true,
        "pageSizes": [10, 15, 20],
        "pageSize": 10
      },
      "thead": [

        {
          "label": "运行结果",
          "prop": "executeStatus"
        },
        {
          "label": "录入值",
          "prop": "inputValue"
        },
        {
          "label": "运行情况",
          "prop": "result"
        }
      ]
    }
  }
}
