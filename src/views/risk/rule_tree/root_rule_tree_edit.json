{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "controls": [
        {
          "treeCode": {
            "label": "规则树代码",
            "control": "string",
            "width": "auto",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "该控件必填",
                "max": 64
              }
            ]
          }
        },
        {
          "treeName": {
            "label": "规则树名称",
            "width": "auto",
            "control": "text",
            "span": 12,
            "rules": [
              {
                "required": true,
                "pattern": "^[a-z0-9A-Z\u4e00-\u9fa5]+$",
                "message": "仅可为字母、数字、文字及相关组合",
                "max": 125
              }
            ]
          },
          "busiVersion": {
            "label": "版本号",
            "width": "auto",
            "control": "string",
            "span": 12,
            "disabled": true
          }
        },
        {
          "treeDesc": {
            "label": "规则树描述",
            "width": "auto",
            "rows": 5,
            "control": "textarea",
            "showWordLimit": 200,
            "maxlength": 200,
            "span": 24,
            "autosize": {
              "minRows": 5
            }
          }
        }
      ],
      "initRequest": "${{params}}",
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "deci040207"
        }
      }
    }
  }
}