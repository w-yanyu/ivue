{
    "module": "dynamicTabs",
    "layout": {
      "showFormBtn": false,
      "tabs": {
        "defaultActiveKey": "hitList",
        "position": "top",
        "styleClass": "card",
        "tabHeader": [
          {
            "id": "hitList",
            "label": "命中列表",
            "closable": false,
            "disabled": false,
            "icon": "el-icon-date",
            "toRequest": {
              "url": "/views/risk/rule_tree/root_rule_tree_result_hit.json",
              "method": "GET",
              "params": {
                "children": "${{params.children}}"
              }
            },
            "props": {
              "width": "65%"
            }
          },
          {
            "id": "notHitList",
            "label": "未命中列表",
            "icon": "el-icon-date",
            "closable": false,
            "disabled": false,
            "toRequest": {
              "url": "/views/risk/rule_tree/root_rule_tree_result_nothit.json",
              "method":"GET",
              "params": {
                "children": "${{params.children}}"
              }
            },
            "props": {
              "width": "65%"
            }
          }
        ]
      }
    }
  }