{
  "module": "v-realonlyForm-btn",
  "layout": {
    "form": {
      "controls": [
        {
          "treeId": {
            "label": "规则树ID",
            "width":"auto",
            "control": "hidden",
            "span": 12
          },
          "treeCode": {
            "label": "规则树代码",
            "width":"auto",
            "control": "string",
            "span": 12
          },"treeName": {
            "label": "规则树名称",
            "width":"auto",
            "control": "string",
            "span": 12
          }
        },
        {
          "branchId": {
            "label": "机构名称",
            "width":"auto",
            "control": "string",
            "span": 12,
            "disabled": true,
            "dict": {
              "dictType": "DECI_E_BRANCH_MARK",
              "dictKey": [
                "DECI_E_BRANCH_MARK"
              ],
              "format": "label"
            }
          },
          "busiVersion": {
            "label": "版本号",
            "width":"auto",
            "control": "string",
            "span": 12,
            "disabled": true
          },
          "applyTime": {
            "label": "最新维护时间",
            "width":"auto",
            "control": "string",
            "type": "dateTimePicker",
            "span": 12,
            "#format": "yyyy-MM-dd HH:mm:ss"
          }
        },
        {
          "treeDesc": {
            "label": "备注",
            "width":"auto",
            "control": "textarea",
            "disabled":true,
            "rows": 5,
            "span": 24
          }
        }
      ],
      "initRequest": "${{params}}"

    }
  }
}
