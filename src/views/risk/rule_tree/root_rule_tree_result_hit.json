{
    "module": "v-search-btn-datagrid",
    "layout": {
      "form": {
        "btns":[],
        "controls": []

      },
      "datagrid": {
        "initRequest": "${{params.children}}",
        "pagination": {
          "hideOnSinglePage":false,
          "show": true,
          "pageSizes": [
            10,
            15,
            20,
            50,
            100
          ],
          "pageSize": 10
        },
        "thead": [
          {
            "label": "流水号",
            "prop": "trxnSeq"
          },
          {
            "label": "节点编号",
            "prop": "ruleTreeNodeId"
          },
          {
            "label": "节点类型",
            "prop": "node_type"
          },
          {
            "label": "是否命中",
            "prop": "hit_flag"
          },
          {
            "label": "交易时间",
            "prop": "createTime",
            "type": "dateTimePicker",
            "format": "yyyy-MM-dd HH:mm:ss"
          },
          {
            "label": "操作",
            "width": "150px",
            "align":"center",
            "btns": [
              {
                "type": "look",
                "label": "动作查看",
                "toRequest": {
                  "url": "/views/risk/risk_decision_rule/risk_decision_rule_action_result.json",
                  "method": "get"
                },
                "props": {
                  "width": "70%"
                }
              }
            ]
          }
        ]
      }
    }
  }
