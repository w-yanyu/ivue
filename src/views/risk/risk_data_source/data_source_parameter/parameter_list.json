{
  "module": "v-search-btn-datagrid",
  "pageId": "risk_rule_constant",
  "layout": {
    "form": {
      "controls": [{
        "parm_code": {
          "label": "参数代码",
          "control": "text",
          "span": 8,
          "rules": [{
            "message": "长度不能超过40个字符",
            "max": 40
          }]
        },
        "parm_name": {
          "label": "参数名称",
          "control": "text",
          "span": 8,
          "rules": [{
            "message": "长度不能超过100个字符",
            "max": 100
          }]
        },
        "data_type": {
          "label": "数据类型",
          "control": "select",
          "span": 8,
          "dict": {
            "format": "label",
            "dictKey": [
              "DECI_E_DATA_TYPE"
            ],
            "dictType": "DECI_E_DATA_TYPE"
          }
        }
      }]
    },
    "toolbar": {
      "btns": [{
        "type": "add",
        "label": "新增",
        "toRequest": {
          "url": "/views/risk/risk_data_source/data_source_parameter/parameter_add.json",
          "method": "get"
        },
        "props": {
          "width": "70%",
          "title": "新增"
        }
      }]
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
            "servicecode": "deci050009"
        }
      },
      "thead": [
        {
        "label": "序号",
        "type": "index"
        },
        {
          "label": "参数方代码",
          "prop": "parm_code"
        },
        {
          "label": "参数名称",
          "prop": "parm_name"
        },
        {
          "label": "数据类型",
          "prop": "data_type",
          "dict": {
            "format": "label",
            "dictKey": [
              "DECI_E_DATA_TYPE"
            ],
            "dictType": "DECI_E_DATA_TYPE"
          }
        },
        {
          "label": "描述",
          "prop": "description"
        },
        {
          "label": "法人代码",
          "prop": "org_id"
        },
        {
          "label": "数据创建时间",
          "prop": "data_create_time"
        },
        {
          "label": "数据更新时间",
          "prop": "data_update_time"
        },
        {
          "label": "数据创建柜员",
          "prop": "data_create_user"
        },
        {
          "label": "数据更新柜员",
          "prop": "data_update_user"
        },
        {
          "label": "数据版本号",
          "prop": "data_version"
        },
        {
          "label": "操作",
          "width": "250px",
          "btns": [{
              "type": "edit",
              "authId": "constant_upd_btn",
              "label": "维护",
              "toRequest": {
                "url": "/views/risk/risk_data_source/data_source_parameter/parameter_edit.json",
                "method": "get"
              },
              "props": {
                "width": "70%"
              }
            },
            {
              "type": "look",
              "label": "查看",
              "toRequest": {
                "url": "/views/risk/risk_data_source/data_source_parameter/parameter_view.json",
                "method": "get"
              },
              "props": {
                "width": "70%"
              }
            },
            {
              "type": "delete",
              "label": "删除",
              "disabled": "'${{current.is_delete}}' === '0'",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "post",
                "params": {
                  "servicecode": "deci050008",
                  "parm_code": "${{current.parm_code}}"
                }
              },
              "confirm": {
                  "title": "提示信息",
                  "msg": "确认删除当前数据？",
                  "confirmButtonText": "确定",
                  "cancelButtonText": "取消"
              }
            }
          ]
        }
      ]
    }
  }
}
