{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "controls": [
        {
          "parm_code": {
            "label": "参数代码",
            "control": "text",
            "span": 12,
            "rules": [
              {
                "message": "长度不能超过40个字符",
                "max": 40
              }
            ]
          },
          "parm_name": {
            "label": "参数名称",
            "control": "text",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "长度不能超过100个字符",
                "max": 100
              }
            ]
          }
        },
        {
          "data_type": {
            "label": "数据类型",
            "control": "select",
            "span": 12,
            "dict": {
              "format": "label",
              "dictKey": [
                "DECI_E_DATA_TYPE"
              ],
              "dictType": "DECI_E_DATA_TYPE"
            }
          },
          "description": {
            "label": "描述",
            "control": "textarea",
            "span": 12,
            "rows": 1,
            "rules": [
              {
                "message": "长度不能超过500个字符",
                "max": 500
              }
            ]
          }
        }
      ],
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "deci050007"
        }
      }
    }
  }
}