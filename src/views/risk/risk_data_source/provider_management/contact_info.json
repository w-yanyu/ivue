{
  "module": "v-form-editableDataGrid-btn",
  "init": {
    "initRequest": "${{params}}"
  },
  "layout": {
    "datagrid": {
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "provider_code": "${{params.provider_code}}",
          "servicecode": "deci050004"
        }
      },
      "pagination": {
        "show": false
      },
      "thead": [
        {
          "prop": "contact_person_no",
          "label": "编号",
          "type": "string"
        },
        {
          "prop": "contact_person_name",
          "label": "姓名",
          "type": "string"
        },
        {
          "prop": "contact_info",
          "label": "联系方式",
          "type": "string"
        },
        {
          "prop": "position",
          "label": "职位",
          "type": "string"
        },
        {
          "prop": "email",
          "label": "邮箱",
          "type": "string"
        },
        {
          "prop": "wechat_id",
          "label": "微信",
          "type": "string"
        },
        {
          "prop": "remark",
          "label": "备注",
          "type": "string"
        }
      ]
    }
  }
}
