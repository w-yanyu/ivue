{
  "module": "v-form-editableDataGrid-btn",
  "init": {
    "initRequest": "${{params}}"
  },
  "layout": {
    "toolbar": {
      "#btnPlace": "right",
      "btns": [
        {
          "type": "add",
          "label": "新增",
          "icon": "el-icon-plus"
        }
      ]
    },
    "datagrid": {
      "name": "contactList",
      "pagination": {
        "show": false
      },
      "initRequest": "${{params.contactList}}",
      "thead": [
        {
          "prop": "contact_person_no",
          "label": "编号",
          "type": "text",
          "disabled": true
        },
        {
          "prop": "contact_person_name",
          "label": "姓名",
          "type": "text",
          "rules": [
            {
              "required": true,
              "message": "请输入"
            }
          ]
        },
        {
          "prop": "contact_info",
          "label": "联系方式",
          "type": "text",
          "rules": [
            {
              "required": true,
              "message": "请输入"
            },
            {
              "pattern":"^1[3-9]\\d{9}$",
              "message": "请输入正确的号码"
            }
          ]
        },
        {
          "prop": "position",
          "label": "职位",
          "type": "text"
        },
        {
          "prop": "email",
          "label": "邮箱",
          "type": "text",
          "rules": [
            {
              "pattern":"^[a-z0-9]+([._\\-]*[a-z0-9])*@([a-z0-9]+[-a-z0-9]*[a-z0-9]+.){1,63}[a-z0-9]+$",
              "message": "请输入正确的邮箱"
            }
          ]
        },
        {
          "prop": "wechat_id",
          "label": "微信",
          "type": "text",
          "rules": [
            {
              "pattern":"^[A-Za-z0-9]+$",
              "message": "请输入正确的微信"
            }
          ]
        },
        {
          "label": "备注",
          "prop": "remark",
          "type": "text",
          "rules": [
            {
              "message": "请正确输入",
              "max": 100
            }
          ]
        },
        {
          "label": "操作",
          "width": "70px",
          "btns": [
            {
              "type": "delete",
              "label": "删除"
            }
          ],
          "align": "center"
        }
      ]
    }
  }
}
