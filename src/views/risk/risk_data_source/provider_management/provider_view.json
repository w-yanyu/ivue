{
  "module": "v_realonlyForm_btn",
  "layout": {
    "form": {
      "controls": [
        {
          "provider_code": {
            "label": "提供方代码",
            "control": "string",
            "span": 12
          },
          "provider_name": {
            "label": "提供方名称",
            "control": "string",
            "span": 12
          }
        },
        {
          "service_type": {
            "label": "服务类型",
            "control": "select",
            "span": 12,
            "dict": {
              "format": "label",
              "dictKey": [
                "DECI_E_SERVICE_TYPE"
              ],
              "dictType": "DECI_E_SERVICE_TYPE"
            },
            "disabled": true
          }
        },
        {
          "description": {
            "label": "备注",
            "control": "string",
            "span": 24
          }
        },
        {
          "contactList": {
            "label": "提供方联系人",
            "initShow": true,
            "span": 24,
            "control": "dynamic",
            "toRequest": {
              "url": "/views/risk/risk_data_source/provider_management/contact_info.json",
              "params": {
                "provider_code": "${{params.provider_code}}"
              }
            },
            "props": {
             
            }
          }
        }
      ],
      "initRequest": "${{params}}"
    }
  }
}
