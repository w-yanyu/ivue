{
  "module": "v-form-editableDataGrid-btn",
  "init": {
    "initRequest": "${{params}}"
  },
  "layout": {
    "toolbar": {
      "#btnPlace": "right",
      "btns": [
        {
          "type": "add",
          "disabled": "'${{params.pageType}}' === 'look'",
          "label": "新增",
          "icon": "el-icon-plus"
        }
      ]
    },
    "datagrid": {
      "name": "contactList",
      "pagination": {
        "show": false
      },
      "initRequest": "${{params.contactList}}",
      "thead": [
        {
          "prop": "contact_person_no",
          "label": "参数名称",
          "type": "text",
          "rules": [
            {
              "required": true,
              "message": "请输入"
            }
          ]
        },
        {
          "prop": "contact_person_name",
          "label": "参数值",
          "type": "text",
          "rules": [
            {
              "required": true,
              "message": "请输入"
            }
          ]
        },
        {
          "label": "操作",
          "width": "70px",
          "btns": [
            {
              "type": "delete",
              "label": "删除"
            }
          ],
          "align": "center"
        }
      ]
    }
  }
}
