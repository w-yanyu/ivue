{
  "module": "v-form-editableDataGrid-btn",
  "init": {
    "initRequest": "${{params}}"
  },
  "layout": {
    "toolbar": {
      "#btnPlace": "right",
      "btns": [
        {
          "type": "add",
          "label": "新增",
          "icon": "el-icon-plus",
          "disabled": "'${{params.pageType}}' === 'look'"
        }
      ]
    },
    "datagrid": {
      "name": "resultList",
      "pagination": {
        "show": false
      },
      "initRequest": "${{params.resultList}}",
      "thead": [
        {
          "prop": "field_code",
          "label": "字段代码",
          "type": "text",
          "rules": [
            {
              "required": true,
              "message": "请输入"
            }
          ]
        },
        {
          "prop": "field_name",
          "label": "字段名称",
          "type": "text",
          "rules": [
            {
              "required": true,
              "message": "请输入"
            }
          ]
        },
        {
          "prop": "value_path",
          "label": "取值路径",
          "type": "text",
          "rules": [
            {
              "required": true,
              "message": "请输入"
            }
          ]
        },
        {
          "prop": "data_type",
          "label": "字段类型",
          "type": "select",
          "dict": {
            "dictType": "DECI_E_DATA_TYPE",
            "dictKey": [
              "DECI_E_DATA_TYPE"
            ],
            "format": "label"
          },
          "rules": [
            {
              "required": true,
              "message": "请输入"
            }
          ]
        },
        {
          "prop": "value_precision",
          "label": "精度",
          "type": "text"
        },
        {
          "label": "描述",
          "prop": "remark",
          "type": "text",
          "rules": [
            {
              "message": "请正确输入",
              "max": 100
            }
          ]
        },
        {
          "label": "操作",
          "width": "70px",
          "btns": [
            {
              "type": "delete",
              "label": "删除"
            }
          ],
          "align": "center"
        }
      ]
    }
  }
}
