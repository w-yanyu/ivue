{
  "module": "v-form-editableDataGrid-btn",
  "init": {
    "initRequest": "${{params}}"
  },
  "layout": {
    "toolbar": {
      "#btnPlace": "right",
      "btns": [
        {
          "type": "add",
          "label": "新增",
          "disabled": "'${{params.pageType}}' == 'look'",
          "icon": "el-icon-plus"
        }
      ]
    },
    "datagrid": {
      "name": "contactList",
      "pagination": {
        "show": false
      },
      "initRequest": "${{params.contactList}}",
      "thead": [
        {
          "prop": "res_token_code_value_path",
          "label": "鉴权Token响应代码取值路径（errcode）",
          "type": "text",
          "rules": [
            {
              "required": true,
              "message": "请输入"
            }
          ]
        },
        {
          "prop": "res_code_function",
          "label": "响应代码函数",
          "type": "textarea",
          "rules": [
            {
              "required": true,
              "message": "请输入"
            }
          ]
        },
        {
          "prop": "res_token_msg_value_path",
          "label": "鉴权Token响应消息取值路径(errmsg)",
          "type": "text",
          "rules": [
            {
              "required": true,
              "message": "请输入"
            }
          ]
        },
        {
          "prop": "res_msg_function",
          "label": "响应消息函数",
          "type": "textarea",
          "rules": [
            {
              "required": true,
              "message": "请输入"
            }
          ]
        },
        {
          "prop": "token_value_path",
          "label": "Token值取值路径",
          "type": "text",
          "rules": [
            {
              "required": true,
              "message": "请输入"
            }
          ]
        },
        {
          "label": "操作",
          "width": "70px",
          "btns": [
            {
              "type": "delete",
              "label": "删除"
            }
          ],
          "align": "center"
        }
      ]
    }
  }
}
