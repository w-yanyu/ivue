{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "controls": [
        {
          "scene_type": {
            "label": "业务场景",
            "control": "select",
            "span": 12,
            "dict": {
              "format": "label",
              "dictKey": [
                "DECI_E_SCENE_TYPE"
              ],
              "dictType": "DECI_E_SCENE_TYPE"
            }
          },
          "prod_no": {
            "label": "产品",
            "control": "text",
            "span": 12
          }
        },
        {
          "valid_period": {
            "label": "有效期（天）",
            "control": "text",
            "span": 12
          }
        }
      ],
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "deci050029",
          "valid_period_id": "${{params.valid_period_id}}"
        }
      }
    }
  }
}