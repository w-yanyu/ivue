{
  "module": "v-search-btn-datagrid",
  "pageId": "risk_rule_constant",
  "layout": {
    "form": {
      "controls": [
        {
          "results_list_code": {
            "label": "结果清单代码",
            "control": "text",
            "span": 8,
            "rules": [
              {
                "message": "长度不能超过40个字符",
                "max": 40
              }
            ]
          },
          "results_list_name": {
            "label": "结果清单名称",
            "control": "text",
            "span": 8,
            "rules": [
              {
                "message": "长度不能超过100个字符",
                "max": 100
              }
            ]
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "label": "新增",
          "disabled": "'${{params.pageType}}' == 'look'",
          "toRequest": {
            "url": "@interfaceListAddOrEdit",
            "method": "get",
            "params":{
              "data_source_code": "${{params.data_source_code}}"
            }
          },
          "props": {
            "width": "70%",
            "height": "70%",
            "title": "新增"
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "deci050047",
          "data_source_code": "${{params.data_source_code}}"
        }
      },
      "thead": [
        {
          "label": "序号",
          "type": "index"
        },
        {
          "label": "结果清单代码",
          "prop": "results_list_code"
        },
        {
          "label": "结果清单代码名称",
          "prop": "results_list_name"
        },
        {
          "label": "描述",
          "prop": "description"
        },
        {
          "label": "操作",
          "width": "200px",
          "btns": [
            {
              "type": "edit",
              "authId": "constant_upd_btn",
              "label": "维护",
              "toRequest": {
                "url": "@interfaceListAddOrEdit",
                "method": "get",
                "params": { "pageType": "edit" }
              },
              "props": {
                "width": "70%"
              }
            },
            {
              "type": "look",
              "label": "查看",
              "toRequest": {
                "url": "@interfaceListAddOrEdit",
                "method": "get",
                "params": { "pageType": "look" }
              },
              "props": {
                "width": "70%"
              }
            },
            {
              "type": "delete",
              "label": "删除",
              "disabled": "'${{current.is_delete}}' === '0'",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "post",
                "params": {
                  "servicecode": "deci050018",
                  "parm_code": "${{current.results_list_code}}"
                }
              },
              "confirm": {
                "title": "提示信息",
                "msg": "确认删除当前数据？",
                "confirmButtonText": "确定",
                "cancelButtonText": "取消"
              }
            }
          ]
        }
      ]
    }
  }
}
