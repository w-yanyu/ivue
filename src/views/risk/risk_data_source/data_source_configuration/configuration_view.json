{
  "module": "dynamicTabs",
  "layout": {
    "tabs": {
      "tabHeader": [
        {
          "id": "01",
          "label": "数据源配置",
          "closable": false,
          "disabled": false,
          "icon": "el-icon-date",
          "toRequest": {
            "url": "@configurationAddTab1",
            "method": "post",
            "params": "${{params}}"
          }
        },
        {
          "id": "02",
          "label": "接入接口配置",
          "closable": false,
          "disabled": false,
          "icon": "el-icon-date",
          "toRequest": {
            "url": "@configurationAddTab2",
            "method": "post",
            "params": "${{params}}"
          }
        },
        {
          "id": "03",
          "label": "结算配置",
          "closable": false,
          "disabled": false,
          "icon": "el-icon-date",
          "toRequest": {
            "url": "/views/risk/risk_data_source/data_source_configuration/configuration_tab3_settlement_allocation.json",
            "method": "post",
            "params": "${{params}}"
          }
        },
        {
          "id": "04",
          "label": "数据源有效期配置",
          "closable": false,
          "disabled": false,
          "icon": "el-icon-date",
          "toRequest": {
            "url": "/views/risk/risk_data_source/data_source_configuration/configuration_tab4_data_source_validity.json",
            "#method": "post",
            "method": "get",
            "params": "${{params}}"
          }
        },{
          "id": "05",
          "label": "数据源超时配置",
          "closable": false,
          "disabled": false,
          "icon": "el-icon-date",
          "toRequest": {
              "url": "/views/risk/risk_data_source/data_source_configuration/configuration_tab5_data_source_timeout.json",
              "method": "get",
              "params": "${{params}}"
             
          }
      },{
        "id": "06",
        "label": "数据源返回结果示例",
        "closable": false,
        "disabled": false,
        "icon": "el-icon-date",
        "toRequest": {
            "url": "/views/risk/risk_data_source/data_source_configuration/configuration_tab6.json",
            "method": "get",
            "params": "${{params}}"
           
        }
    }
      ],
      "defaultActiveKey": "01",
      "position": "top",
      "styleClass": "card"
    }


  }
}
