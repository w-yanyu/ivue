{
  "module": "v-search-btn-datagrid",
  "pageId": "risk_rule_constant",
  "layout": {
    "form": {
      "controls": [{
        "data_source_code": {
          "label": "数据源代码",
          "control": "text",
          "span": 8,
          "rules": [{
            "message": "长度不能超过40个字符",
            "max": 40
          }]
        },
        "data_source_name": {
          "label": "数据源名称",
          "control": "text",
          "span": 8,
          "rules": [{
            "message": "长度不能超过100个字符",
            "max": 100
          }]
        },
        "data_source_type": {
          "label": "数据类型",
          "control": "select",
          "span": 8,
          "dict": {
            "format": "label",
            "dictKey": [
              "DECI_E_DATA_SOURCE_TYPE"
            ],
            "dictType": "DECI_E_DATA_SOURCE_TYPE"
          }
        },
        "data_source_status": {
          "label": "数据源状态",
          "control": "select",
          "span": 8,
          "dict": {
            "format": "label",
            "dictKey": [
              "DECI_E_DATA_SOURCE_STATUS"
            ],
            "dictType": "DECI_E_DATA_SOURCE_STATUS"
          }
        },
        "start_status": {
          "label": "启停状态",
          "control": "select",
          "span": 8,
          "dict": {
            "format": "label",
            "dictKey": [
              "DECI_E_START_STATUS"
            ],
            "dictType": "DECI_E_START_STATUS"
          }
        },
        "regist_status": {
          "label": "注册状态",
          "control": "select",
          "span": 8,
          "dict": {
            "format": "label",
            "dictKey": [
              "DECI_E_REGIST_STATUS"
            ],
            "dictType": "DECI_E_REGIST_STATUS"
          }
        }
      }]
    },

    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/call/RPCCall",
        "method": "POST",
        "params": {
          "servicecode": "deci050013"
        }
      },
      "thead": [
        {
        "label": "序号",
        "type": "index"
        },
        {
          "label": "数据源代码",
          "prop": "data_source_code"
        },
        {
          "label": "数据源名称",
          "prop": "data_source_name"
        },
        {
          "label": "提供方名称",
          "prop": "provider_name"
        },
        {
          "label": "数据源类型",
          "prop": "data_source_status",
          "type": "select",
          "dict": {
            "format": "label",
            "dictKey": [
              "DECI_E_DATA_SOURCE_TYPE"
            ],
            "dictType": "DECI_E_DATA_SOURCE_TYPE"
          }
        },
        {
          "label": "数据源状态",
          "prop": "data_source_type",
          "dict": {
            "format": "label",
            "dictKey": [
              "DECI_E_DATA_SOURCE_TYPE"
            ],
            "dictType": "DECI_E_DATA_SOURCE_TYPE"
          }
        },
        {
          "label": "启停状态",
          "prop": "start_status",
          "dict": {
            "format": "label",
            "dictKey": [
              "DECI_E_START_STATUS"
            ],
            "dictType": "DECI_E_START_STATUS"
          }
        },
        {
          "label": "注册状态",
          "prop": "regist_status",
          "dict": {
            "format": "label",
            "dictKey": [
              "DECI_E_REGIST_STATUS"
            ],
            "dictType": "DECI_E_REGIST_STATUS"
          }
        },
        {
          "label": "数据创建时间",
          "prop": "data_create_time"
        },
        {
          "label": "数据更新时间",
          "prop": "data_update_time"
        },
        {
          "label": "数据创建柜员",
          "prop": "data_create_user"
        },
        {
          "label": "数据更新柜员",
          "prop": "data_update_user"
        },
        {
          "label": "数据版本号",
          "prop": "data_version"
        },
        {
          "prop": "provider_code",
          "isShow": false
        },
        {
          "prop": "default_validity_day",
          "isShow": false
        },
        {
          "prop": "connection_timeout",
          "isShow": false
        },
        {
          "prop": "read_timeout",
          "isShow": false
        },
        {
          "prop": "url_type",
          "isShow": false
        },
        {
          "prop": "url_path",
          "isShow": false
        },
        {
          "prop": "value_expression",
          "isShow": false
        },
        {
          "prop": "example_results",
          "isShow": false
        },
        {
          "prop": "trxn_version",
          "isShow": false
        },
        {
          "label": "操作",
          "align": "center",
          "fixed": "right",
          "btns": [
            {
              "type": "bringback",
              "label": "选择"
            }
          ]
        }
      ]
    }
  }
}