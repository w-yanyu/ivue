{
  "module": "v-search-btn-datagrid",
  "pageId": "risk_rule_constant",
  "layout": {
    "form": {
      "controls": [{
        "data_source_code": {
          "label": "数据源代码",
          "control": "text",
          "span": 8,
          "rules": [{
            "message": "长度不能超过40个字符",
            "max": 40
          }]
        },
        "data_source_name": {
          "label": "数据源名称",
          "control": "text",
          "span": 8,
          "rules": [{
            "message": "长度不能超过100个字符",
            "max": 100
          }]
        },
        "data_source_type": {
          "label": "数据类型",
          "control": "select",
          "span": 8,
          "dict": {
            "format": "label",
            "dictKey": [
              "DECI_E_DATA_SOURCE_TYPE"
            ],
            "dictType": "DECI_E_DATA_SOURCE_TYPE"
          }
        },
        "data_source_status": {
          "label": "数据源状态",
          "control": "select",
          "span": 8,
          "dict": {
            "format": "label",
            "dictKey": [
              "DECI_E_DATA_SOURCE_STATUS"
            ],
            "dictType": "DECI_E_DATA_SOURCE_STATUS"
          }
        },
        "start_status": {
          "label": "启停状态",
          "control": "select",
          "span": 8,
          "dict": {
            "format": "label",
            "dictKey": [
              "DECI_E_START_STATUS"
            ],
            "dictType": "DECI_E_START_STATUS"
          }
        },
        "regist_status": {
          "label": "注册状态",
          "control": "select",
          "span": 8,
          "dict": {
            "format": "label",
            "dictKey": [
              "DECI_E_REGIST_STATUS"
            ],
            "dictType": "DECI_E_REGIST_STATUS"
          }
        }
      }]
    },
    "toolbar": {
      "btns": [{
        "type": "add",
        "label": "新增",
        "toRequest": {
          "url": "@configurationSteps",
          "method": "get",
          "params": {
            "pageType": "add"
          }
        },
        "props": {
          "width": "70%",
          "height": "70%",
          "title": "新增"
        }
      }]
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/call/RPCCall",
        "method": "POST",
        "params": {
          "servicecode": "deci050013"
        }
      },
      "thead": [
        {
        "label": "序号",
        "type": "index"
        },
        {
          "label": "数据源代码",
          "prop": "data_source_code"
        },
        {
          "label": "数据源名称",
          "prop": "data_source_name"
        },
        {
          "label": "提供方名称",
          "prop": "provider_name"
        },
        {
          "label": "数据源类型",
          "prop": "data_source_status",
          "type": "select",
          "dict": {
            "format": "label",
            "dictKey": [
              "DECI_E_DATA_SOURCE_TYPE"
            ],
            "dictType": "DECI_E_DATA_SOURCE_TYPE"
          }
        },
        {
          "label": "数据源状态",
          "prop": "data_source_type",
          "dict": {
            "format": "label",
            "dictKey": [
              "DECI_E_DATA_SOURCE_TYPE"
            ],
            "dictType": "DECI_E_DATA_SOURCE_TYPE"
          }
        },
        {
          "label": "启停状态",
          "prop": "start_status",
          "dict": {
            "format": "label",
            "dictKey": [
              "DECI_E_START_STATUS"
            ],
            "dictType": "DECI_E_START_STATUS"
          }
        },
        {
          "label": "注册状态",
          "prop": "regist_status",
          "dict": {
            "format": "label",
            "dictKey": [
              "DECI_E_REGIST_STATUS"
            ],
            "dictType": "DECI_E_REGIST_STATUS"
          }
        },
        {
          "label": "数据创建时间",
          "prop": "data_create_time"
        },
        {
          "label": "数据更新时间",
          "prop": "data_update_time"
        },
        {
          "label": "数据创建柜员",
          "prop": "data_create_user"
        },
        {
          "label": "数据更新柜员",
          "prop": "data_update_user"
        },
        {
          "label": "数据版本号",
          "prop": "data_version"
        },
        {
          "prop": "provider_code",
          "isShow": false
        },
        {
          "prop": "default_validity_day",
          "isShow": false
        },
        {
          "prop": "connection_timeout",
          "isShow": false
        },
        {
          "prop": "read_timeout",
          "isShow": false
        },
        {
          "prop": "url_type",
          "isShow": false
        },
        {
          "prop": "url_path",
          "isShow": false
        },
        {
          "prop": "value_expression",
          "isShow": false
        },
        {
          "prop": "example_results",
          "isShow": false
        },
        {
          "prop": "trxn_version",
          "isShow": false
        },
        {
          "label": "操作",
          "width": "250px",
          "btns": [{
              "type": "edit",
              "authId": "constant_upd_btn",
              "label": "维护",
              "toRequest": {
                "url": "@configurationSteps",
                "method": "get",
                "params": {
                  "pageType": "edit",
                  "rowData": "${{current}}"
                }
              },
              "props": {
                "width": "70%",
                "height": "70%"
              }
            },
            {
              "type": "look",
              "label": "查看",
              "toRequest": {
                "url": "@configurationTabs",
                "method": "get",
                "params": {
                  "pageType": "look",
                  "rowData": "${{current}}"
                }
              },
              "props": {
                "width": "70%",
                "height": "70%"
              }
            },
            {
              "type": "delete",
              "label": "删除",
              "disabled": "'${{current.is_delete}}' === '0'",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "post",
                "params": {
                  "servicecode": "deci050012"
                }
              },
              "confirm": {
                  "title": "提示信息",
                  "msg": "确认删除当前数据？",
                  "confirmButtonText": "确定",
                  "cancelButtonText": "取消"
              }
            },
            {
              "type": "dropdown",
              "label": "更多",
              "trigger": "hover",
              "options": [
                {
                  "type": "custom",
                  "label": "测试",
                  "toRequest": {
                    "target": "drawer",
                    "method": "get",
                    "url": "@testIndex"
                  },
                  "props": {
                    "width": "65%",
                    "title": "测试配置",
                    "close": "",
                    "openType": "drawer",
                    "that": "this",
                    "position": "right"
                  }
                },
                {
                  "type": "custom",
                  "label": "提交审核",
                  "doRequest": {
                    "url": "/SUMP/icmcall/icmRPCCall",
                    "method": "post",
                    "params": {
                      "servicecode": "deci050008",
                      "parm_code": "${{current.parm_code}}"
                    }
                  }
                },
                {
                  "type": "custom",
                  "label": "提交运行",
                  "doRequest": {
                    "url": "/SUMP/icmcall/icmRPCCall",
                    "method": "post",
                    "params": {
                      "servicecode": "deci050008",
                      "parm_code": "${{current.parm_code}}"
                    }
                  }
                },
                {
                  "type": "custom",
                  "label": "提交下线",
                  "doRequest": {
                    "url": "/SUMP/icmcall/icmRPCCall",
                    "method": "post",
                    "params": {
                      "servicecode": "deci050008",
                      "parm_code": "${{current.parm_code}}"
                    }
                  }
                },
                {
                  "type": "custom",
                  "label": "启用",
                  "doRequest": {
                    "url": "/SUMP/icmcall/icmRPCCall",
                    "method": "post",
                    "params": {
                      "servicecode": "deci050008",
                      "parm_code": "${{current.parm_code}}"
                    }
                  },
                  "show": {
                    "expression": "'${{current.prod_status}}' == '00'"
                  }
                },
                {
                  "type": "custom",
                  "label": "停用",
                  "doRequest": {
                    "url": "/SUMP/icmcall/icmRPCCall",
                    "method": "post",
                    "params": {
                      "servicecode": "deci050008",
                      "parm_code": "${{current.parm_code}}"
                    }
                  },
                  "show": {
                    "expression": "'${{current.prod_status}}' == '00'"
                  }
                },
                {
                  "type": "custom",
                  "label": "注册",
                  "doRequest": {
                    "url": "/SUMP/icmcall/icmRPCCall",
                    "method": "post",
                    "params": {
                      "servicecode": "deci050008",
                      "parm_code": "${{current.parm_code}}"
                    }
                  }
                }
              ]
            }
          ]
        }
      ]
    }
  }
}