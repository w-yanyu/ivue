{
  "module": "d-steps",
  "layout": {
    "steps": {
      "scope": "formObj",
      "direction": "horizontal",
      "active": 0,
      "alignCenter": true,
      "finishStatus": "success",
      "simpleType": false,
      "nextStepType": "validate",
      "skip": true,
      "list": [
        {
          "scope": "step1",
          "title": "数据源配置",
          "icon": "el-icon-edit",
          "skip": true,
          "toRequest": {
            "url": "@configurationAddTab1",
            "method": "get"
          }
        },
        {
          "scope": "step2",
          "title": "接入接口配置",
          "icon": "el-icon-edit",
          "forbidBack": false,
          "toRequest": {
            "url": "@configurationAddTab2",
            "method": "get"
          }
        },
        {
          "scope": "step3",
          "title": "结算配置",
          "icon": "el-icon-edit",
          "forbidBack": false,
          "toRequest": {
            "url": "/views/risk/risk_data_source/data_source_configuration/configuration_tab3_settlement_allocation.json",
            "method": "get"
          }
        },
        {
          "scope": "step4",
          "title": "数据源有效期配置",
          "icon": "el-icon-edit",
          "forbidBack": false,
          "toRequest": {
            "url": "/views/risk/risk_data_source/data_source_configuration/configuration_tab4_data_source_validity.json",
            "method": "get"
          }
        },
        {
          "scope": "step5",
          "title": "数据源超时配置",
          "icon": "el-icon-edit",
          "forbidBack": false,
          "toRequest": {
            "url": "/views/risk/risk_data_source/data_source_configuration/configuration_tab5_data_source_timeout.json",
            "method": "get"
          },
          "doRequest": {
            "url": "/pte/json/v_search_btn_datagrid/format_msg.json",
            "method": "post",
            "params": {
              "code": "${{id1.id12}}"
            }
          }
        },
        {
          "title": "数据源返回结果示例",
          "icon": "el-icon-edit",
          "forbidBack": false,
          "toRequest": {
            "url": "/views/risk/risk_data_source/data_source_configuration/configuration_tab6.json",
            "method": "get"
          },
          "doRequest": {
            "url": "/pte/json/v_search_btn_datagrid/format_msg.json",
            "method": "get",
            "params": {
              "testParam": "${{params}}"
            }
          }
        }
      ]
    }
  }
}
