{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "controls": [
        {
          "scoreCardId": {
            "label": "评分卡ID",
            "control": "string",
            "span": 12
          }
        },
        {
          "scoreCardCode": {
            "label": "评分卡代码",
            "control": "string",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "该控件必填"
              }
            ]
          }
        },
        {
          "scoreCardName": {
            "label": "评分卡名称",
            "control": "text",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "该控件必填"
              }
            ]
          }
        },
        {
          "scoreCardStatus": {
            "label": "评分卡状态",
            "control": "radio",
            "span": 12,
            "dict": {
              "dictType": "activeStatus",
              "dictKey": [],
              "doRequest": {
                "url": "/views/risk/dictdata/activeStatus.json",
                "method": "get"
              }
            }
          }
        },
        {
          "description": {
            "label": "备注",
            "control": "textarea",
            "span": 24
          }
        }
      ],
      "initRequest": "${{params}}",
      "btns": [
        {}
      ]
    }
  }
}
