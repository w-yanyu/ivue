{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "scoreCardCode": {
            "label": "评分卡代码",
            "control": "text",
            "span": 6
          },
          "scoreCardName": {
            "label": "评分卡名称",
            "control": "text",
            "span": 6
          },
          "version": {
            "label": "版本号",
            "control": "inputNumber",
            "decimal":"2",
            "min":"0",
            "step": "0.01",
            "span": 6
          }
        }
      ]
    },
    "datagrid": {
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          
          "scoreCardType": "${{params.scoreCardType}}",
          "servicecode": "deci030007"
        }
      },
      "pagination": {
        "hideOnSinglePage": false,
        "show": true,
        "pageSizes": [
          10,
          15,
          20,
          50,
          100
        ],
        "pageSize": 10
      },
      "thead": [
        {
          "label": "评分卡类型",
          "prop": "scoreCardType",
          "dict": {
            "dictType": "scoreCardType",
            "dictKey": [],
            "format": "label",
            "doRequest": {
              "url": "/views/risk/dictdata/scoreCardType.json",
              "method": "get"
            }
          }
        },
        {
          "label": "评分卡代码",
          "prop": "scoreCardCode"
        },
        {
          "label": "评分卡名称",
          "prop": "scoreCardName"
        },
        {
          "label": "版本号",
          "prop": "version"
        },
        {
          "label": "操作",
          "width": "150px",
          "btns": [
            {
              "type": "bringback",
              "label": "选择"
            }
          ]
        }
      ]
    }
  }
}