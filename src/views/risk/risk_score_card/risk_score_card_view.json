{
  "module": "v-realonlyForm-btn",
  "layout": {
    "form": {
      "controls": [
        {
          "scoreCardId": {
            "label": "评分卡ID",
            "control": "string",
            "span": 12
          },
          "scoreCardCode": {
            "label": "评分卡代码",
            "control": "string",
            "span": 12
          }
        },
        {
          "scoreCardName": {
            "label": "评分卡名称",
            "control": "string",
            "span": 12
          },
          "scoreCardStatus": {
            "label": "评分卡状态",
            "control": "radio",
            "disabled": true,
            "span": 12,
            "options":[
              {
                "label":"未启用",
                "value":"U"
              },
              {
                "label":"启用",
                "value":"Y"
              }
            ]
          }
        },
        {
          "updateUser": {
            "label": "最后更新人",
            "control": "string",
            "span": 12
          },
          "updateTime": {
            "label": "最后更新时间",
            "control": "string",
            "type": "dateTimePicker",
            "format": "yyyy-MM-dd HH:mm:ss",
            "span": 12
          }
        },
        {
          "version": {
            "label": "版本号",
            "control": "string",
            "span": 12
          }
        },
        {
          "description": {
            "label": "备注",
            "control": "string",
            "span": 24
          }
        }
      ],
      "initRequest": "${{params}}"
    }
  }
}
