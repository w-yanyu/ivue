{
  "module": "v-search-btn-datagrid",
  "pageId": "risk_score_card",
  "layout": {
    "form": {
      "controls": [
        {
          "scoreCardCode": {
            "label": "复杂评分卡代码",
            "width": "auto",
            "control": "text",
            "span": 8,
            "rules": [
              {
                "message": "字段长度超限",
                "max": 40
              }
            ]
          },
          "scoreCardName": {
            "label": "复杂评分卡名称",
            "width": "auto",
            "control": "text",
            "span": 8,
            "rules": [
              {
                "message": "字段长度超限",
                "max": 40
              }
            ]
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "label": "新增",
          "authId": "score_card_add_btn",
          "#icon": "el-icon-plus",
          "toRequest": {
            "url": "/views/risk/risk_score_card/complex_score_card_add.json",
            "method": "get",
            "params": {
              "scoreCardType": "1"
            }
          },
          "props": {
            "width": "70%",
            "title": "新增"
          }
        },
        {
          "##type": "export",
          "##label": "下载导入模板",
          "##authId": "score_card_inm_btn",
          "##icon": "iconfont icon-daochu",
          "##doRequest": {
            "##url": "/API/risk-management-service/risk-score-card/downloadImportTemplate",
            "##method": "post"
          }
        },
        {
          "##type": "import",
          "##label": "导入",
          "##authId": "score_card_in_btn",
          "##icon": "iconfont icon-daoru1",
          "##props": {
            "##width": "600px",
            "##title": "导入",
            "##doRequest": {
              "##url": "/API/risk-management-service/risk-score-card/import",
              "##method": "post"
            },
            "##multiple": false,
            "##name": "import",
            "##accept": ".xls,.xlsx",
            "##limit": 1,
            "##size": 1024,
            "##allSize": 1
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "scoreCardType": "1",
          "servicecode": "deci020201",
          "isList": true
        }
      },
      "pagination": {
        "hideOnSinglePage": false,
        "show": true,
        "pageSizes": [
          10,
          15,
          20
        ],
        "pageSize": 10
      },
      "thead": [
        {
          "label": "序号",
          "type": "index"
        },
        {
          "label": "复杂评分卡代码",
          "prop": "scoreCardCode"
        },
        {
          "label": "复杂评分卡名称",
          "prop": "scoreCardName"
        },
        {
          "label": "操作类型",
          "prop": "riskOperationType",
          "dict": {
            "dictType": "operationType2",
            "dictKey": [],
            "format": "label",
            "doRequest": {
              "url": "/views/risk/dictdata/operationType2.json",
              "method": "get"
            }
          }
        },
        {
          "label": "复杂评分卡状态",
          "prop": "scoreCardStatus",
          "dict": {
            "dictType": "riskStatus",
            "dictKey": [],
            "format": "label",
            "doRequest": {
              "url": "/views/risk/dictdata/riskStatus.json",
              "method": "get"
            }
          }
        },
        {
          "label": "备注",
          "prop": "description"
        },
        {
          "label": "最新维护时间",
          "prop": "updateTime"
        },
        {
          "label": "操作",
          "width": "200px",
          "align": "left",
          "btns": [
            {
              "type": "look",
              "label": "查看",
              "authId": "score_card_vie_btn",
              "#icon": "el-icon-view",
              "toRequest": {
                "###url": "@riskScoreCardModelViewNew",
                "url": "@simpleScoreCardTmpTableView",
                "method": "get"
              } ,"props": {
                "width": "90%",
                "top": "4vh",
                "message":""
              }
            },
            {
              "type": "edit",
              "label": "维护",
              "authId": "score_card_edt_btn",
              "toRequest": { 
                "url": "@simpleScoreCardTable",
                "method": "get"
              },
              "props": {
                "width": "90%",
                "message":"维护"
              },
              "show": {
                "expression": "'${{current.riskOperationType}}'!== 'S' && '${{current.riskOperationType}}' !== 'D'"
              }
            },
            {
              "type": "edit",
              "label": "维护",
              "authId": "score_card_edt_btn",
              "toRequest": {
                "url": "@complexScoreCardVersionBack",
                "method": "get"
              },
              "props": {
                "width": "90%"
              },
              "show": {
                "expression": "'${{current.riskOperationType}}'==='S'"
              }
            },
            {
              "type": "delete",
              "label": "删除",
              "authId": "score_card_del_btn",
              "#icon": "el-icon-delete",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "post",
                "params": {
                  "servicecode": "deci020204",
                  "scoreCardType": "1"
                }
              },
              "show": {
                "expression": "'${{current.scoreCardStatus}}'!= 'D' && '${{current.scoreCardStatus}}'!='C'"
              }
            },
            {
              "type": "custom",
              "label": "提交审核",
              "authId": "score_card_edt_btn",
              "#icon": "el-icon-edit",
              "toRequest": {
                "url": "/views/risk/risk_score_card/complex_score_card_review.json",
                "method": "get",
                "params": {
                  "scoreCardCode": "${{current.scoreCardCode}}",
                  "scoreCardName": "${{current.scoreCardName}}",
                  "#currVersion": "${{current.currVersion}}",
                  "approveStatus": "01",
                  "scoreCardType": "1",
                  "riskOperationType": "${{current.riskOperationType}}"
                }
              },
              "props": {
                "width": "40%"
              },
              "show": {
                "expression": "true"
              }
            },
            {
              "type": "custom",
              "label": "复制",
              "toRequest": {
                "url": "/views/risk/risk_score_card/risk_score_card_copy.json",
                "method": "get",
                "params": {
                  "scoreCardCode": "${{current.scoreCardCode}}",
                  "scoreCardName": "${{current.scoreCardName}}",
                  "currVersion": "${{current.currVersion}}"
                }
              },
              "props": {
                
              },
              "show": {
                "expression": "false"
              }
            },
            {
              "type": "custom",
              "label": "还原",
              "authId": "score_card_del_btn",
              "#icon": "el-icon-delete",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "post",
                "params": {
                  "scoreCardType": "1",
                  "servicecode": "deci020122"
                }
              },
              "show": {
                "expression": "false"
              },
              "confirm": {
                "title": "提示信息",
                "msg": "确定要还原至最新生效版本吗?",
                "confirmButtonText": "确定",
                "cancelButtonText": "取消"
              }
            },
            {
              "type": "edit",
              "label": "维护",
              "authId": "score_card_edt_btn",
              "#icon": "el-icon-edit",
              "toRequest": {
                "url": "@simpleScoreCardTable",
                "method": "get"
              },
              "props": {
                "width": "90%",
                "top": "4vh"
              },
              "show": {
                "expression": "false"
              }
            },
            {
              "type": "edit",
              "label": "版本切换",
              "authId": "score_card_del_btn",
              "#icon": "el-icon-delete",
              "toRequest": {
                "url": "@simpleScoreCardVersionBack",
                "method": "get",
                "params": {}
              },
              "props": {
               
              },
              "show": {
                "expression": "false"
              }
            }
          ]
        }
      ]
    }
  }
}