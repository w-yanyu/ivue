{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "btns": [
        {
          "type": "close",
          "label": "取消"
        },
        {
          "type": "submit",
          "target": "addlog",
          "label": "确定",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "post",
            "params": {
              "servicecode": "deci020113",
              "approveStatus": "01",
              "scoreCardType": "0",
              "branchId": "$g{{user_info.branch_id}}"
            }
          }
        }
      ],
      "controls": [
        {
          "scoreCardCode": {
            "control": "text",
            "disabled": "true",
            "width":"auto",
            "label": "简单评分卡代码",
            "value": "${{params.scoreCardCode}}",
            "rules": [
              {
                "required": true
              }
            ],
            "span": 24
          }
        },
        {
          "scoreCardName": {
            "control": "text",
            "label": "简单评分卡名称",
            "width":"auto",
            "disabled": "true",
            "value": "${{params.scoreCardName}}",
            "rules": [
              {
                "required": true
              }
            ],
            "span": 24
          }
        },
        {
          "riskOperationType": {
            "label": "操作类型",
            "width":"auto",
            "disabled": "true",
            "rules": [
              {
                "required": true
              }
            ],
            "control": "select",
            "value": "${{params.riskOperationType}}",
            "dict": {
              "dictType": "DECI_E_RISK_OPERATION_TYPE",
              "dictKey": [
                "DECI_E_RISK_OPERATION_TYPE"
              ],
              "format": "label"
            },
            "span": 24
          }
        }
      ]
    }
  }
}