{
  "module": "v-search-btn-datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "trxnSeq": {
            "label": "trxnSeq",
            "control": "text",
            "span": 8,
            "rules": [
              {
                "message": "字段长度超限",
                "max": 40
              }
            ]
          },
          "customerId": {
            "label": "customerId",
            "control": "text",
            "span": 8,
            "rules": [
              {
                "message": "字段长度超限",
                "max": 40
              }
            ]
          },
          "cardNumber": {
            "label": "cardNumber",
            "control": "text",
            "span": 8,
            "rules": [
              {
                "message": "字段长度超限",
                "max": 40
              }
            ]
          },
          "tranDate": {
            "label": "tranDate",
            "control": "text",
            "span": 8,
            "rules": [
              {
                "message": "字段长度超限",
                "max": 8
              }
            ]
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/API/risk-management-service/risk-log/risk-decision-page-select",
        "method": "post"
      },
      "thead": [
        {
          "label": "流水号",
          "prop": "trxnSeq"
        },
        {
          "label": "客户号",
          "prop": "customerId"
        },
        {
          "label": "交易日期",
          "prop": "tranDate",
          "type": "dateTimePicker",
          "format": "yyyy-MM-dd"
        },
        {
          "label": "交易时间",
          "prop": "tranTime",
          "type": "dateTimePicker",
          "format": "HH:mm:ss"
        },
        {
          "label": "交易金额",
          "prop": "tranAmount"
        },
        {
          "label": "等价人民币金额",
          "prop": "rmbAmount"
        },
        {
          "label": "命中规则",
          "prop": "alertRules"
        },
        {
          "label": "处置措施",
          "prop": "handlers"
        },
        {
          "label": "响应码",
          "prop": "responseCode"
        },
        {
          "label": "操作",
          "btns": [
            {
              "type": "look",
              "label": "查看",
              "toRequest": {
                "url": "/views/risk/risk_list/risk_list_view.json",
                "method": "get"
              },
              "props": {
                "width": "70%"
              }
            }
          ]
        }
      ]
    }
  }
}
