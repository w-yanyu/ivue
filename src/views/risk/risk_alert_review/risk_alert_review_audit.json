{
    "module": "v-realonlyForm-btn",
    "layout": {
        "form": {
            "controls": [
                {
                    "reviewId": {
                        "label": "审核信息ID",
                        "control": "string",
                        "span": 12
                    },
                    "alertId": {
                        "label": "警报信息ID",
                        "control": "string",
                        "span": 12
                    }
                },
                {
                    "alertRiskLevel": {
                        "label": "警报风险级别",
                        "control": "string",
                        "span": 12
                    },
                    "alertTrxnSeq": {
                        "label": "警报交易流水号",
                        "control": "string",
                        "span": 12
                    }
                },
                {
                    "cardNumber": {
                        "label": "卡号",
                        "control": "string",
                        "span": 12
                    },
                    "accountId": {
                        "label": "账号",
                        "control": "string",
                        "span": 12
                    }
                },
                {
                    "customerId": {
                        "label": "客户号",
                        "control": "string",
                        "span": 12
                    },
                    "processingFlag": {
                        "label": "处理方式",
                        "control": "string",
                        "span": 12
                    }
                },
                {
                    "applyOperator": {
                        "label": "申请人",
                        "control": "string",
                        "span": 12
                    },
                    "applyTime": {
                        "label": "申请时间",
                        "control": "string",
                        "span": 12
                    }
                },
                {
                    "status": {
                        "label": "审核状态",
                        "control": "string",
                        "span": 12,
                        "dict": {
                            "dictType": "alertReviewStatus",
                            "dictKey": [],
                            "format": "value-label",
                            "doRequest": {
                                "url": "/views/risk/dictdata/alertReviewStatus.json",
                                "method": "get"
                            }
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "该控件必填",
                                "max": 1
                            }
                        ]
                    },
                    "reviewOperator": {
                        "label": "审核人",
                        "control": "string",
                        "span": 12
                    }
                },
                {
                    "reviewTime": {
                        "label": "审核时间",
                        "control": "string",
                        "span": 12
                    },
                    "reviewRemark": {
                        "label": "审核意见",
                        "control": "string",
                        "span": 12
                    }
                },
                {
                    "createTime": {
                        "label": "创建日期",
                        "control": "string",
                        "type": "dateTimePicker",
                        "format": "yyyy-MM-dd HH:mm:ss",
                        "span": 12
                    },
                    "createUser": {
                        "label": "创建人",
                        "control": "string",
                        "span": 12
                    }
                },
                {
                    "updateTime": {
                        "label": "最后修改日期",
                        "control": "string",
                        "type": "dateTimePicker",
                        "format": "yyyy-MM-dd HH:mm:ss",
                        "span": 12
                    },
                    "updateUser": {
                        "label": "最后更新人",
                        "control": "string",
                        "span": 12
                    }
                }
            ],
            "initRequest": "${{params}}"
        }
    }
}
