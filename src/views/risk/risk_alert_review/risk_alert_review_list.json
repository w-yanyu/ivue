{
    "module": "v-search-btn-datagrid",
    "layout": {
        "form": {
            "controls": [
                {
                    "alertId": {
                        "label": "警报信息ID",
                        "control": "text",
                        "span": 12,
                        "rules": [
                            {
                                "message": "字段长度40,请正确输入",
                                "max": 40
                            }
                        ]
                    },
                    "alertTrxnSeq": {
                        "label": "交易流水号",
                        "control": "text",
                        "span": 12,
                        "rules": [
                            {
                                "message": "字段长度40,请正确输入",
                                "max": 40
                            }
                        ]
                    }
                },
                {
                    "accountId": {
                        "label": "账号",
                        "control": "text",
                        "span": 12,
                        "rules": [
                            {
                                "message": "字段长度40,请正确输入",
                                "max": 40
                            }
                        ]
                    },
                    "cardNumber": {
                        "label": "卡号",
                        "control": "text",
                        "span": 12,
                        "rules": [
                            {
                                "message": "字段长度19,请正确输入",
                                "max": 19
                            }
                        ]
                    }
                },
                {
                    "startReviewDate": {
                        "label": "申请开始日期",
                        "control": "dateTimePicker",
                        "valueFormat": "timestamp",
                        "format": "yyyy-MM-dd HH:mm:ss",
                        "span": 12
                    },
                    "endReviewDate": {
                        "label": "申请结束日期",
                        "control": "dateTimePicker",
                        "valueFormat": "timestamp",
                        "format": "yyyy-MM-dd HH:mm:ss",
                        "span": 12
                    }
                },
                {
                    "applyOperator": {
                        "label": "申请人",
                        "control": "text",
                        "span": 12,
                        "rules": [
                            {
                                "message": "字段长度40,请正确输入",
                                "max": 40
                            }
                        ]
                    }
                }
            ]
        },
        "toolbar": {
            "btns": [
                {
                    "type": "add",
                    "label": "新增",
                    "toRequest": {
                        "url": "/views/risk/risk_alert_review/risk_alert_review_add.json",
                        "method": "get",
                        "params": {}
                    },
                    "props": {
                        "width": "70%",
                        "title": "新增"
                    }
                }
            ]
        },
        "datagrid": {
            "isInitSearch": false,
            "doRequest": {
                "url": "/API/risk-management-service/risk-alert-review/page-select",
                "method": "post"
            },
            "thead": [
                {
                    "label": "审核信息ID",
                    "prop": "reviewId",
                    "align": "center"
                },
                {
                    "label": "警报信息ID",
                    "prop": "alertId",
                    "align": "center"
                },
                {
                    "label": "警报交易流水号",
                    "prop": "alertTrxnSeq",
                    "align": "center"
                },
                {
                    "label": "卡号",
                    "prop": "cardNumber",
                    "align": "center"
                },
                {
                    "label": "账号",
                    "prop": "accountId",
                    "align": "center"
                },
                {
                    "label": "客户号",
                    "prop": "customerId",
                    "align": "center"
                },
                {
                    "label": "处理方式",
                    "prop": "processingFlag",
                    "align": "center"
                },
                {
                    "label": "申请人",
                    "prop": "applyOperator",
                    "align": "center"
                },
                {
                    "label": "申请时间",
                    "prop": "applyTime",
                    "type": "dateTimePicker",
                    "format": "yyyy-MM-dd HH:mm:ss",
                    "align": "center"
                },
                {
                    "label": "操作",
                    "btns": [
                        {
                            "type": "look",
                            "label": "详情",
                            "toRequest": {
                                "url": "/views/risk/risk_alert_review/risk_alert_review_edit.json",
                                "method": "get"
                            },
                            "props": {}
                        },
                        {
                            "type": "edit",
                            "label": "审核",
                            "toRequest": {
                                "url": "/views/risk/risk_alert_review/risk_alert_review_audit.json",
                                "method": "get"
                            },
                            "props": {}
                        }
                    ]
                }
            ],
            "pagination": {
                "show": true,
                "pageSizes": [
                    "10",
                    "50",
                    "100",
                    "200"
                ],
                "pageSize": 10
            }
        }
    }
}
