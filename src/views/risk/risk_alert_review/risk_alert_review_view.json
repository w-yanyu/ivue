{
    "module": "v-search-btn-datagrid",
    "layout": {
        "datagrid": {
            "isInitSearch": true,
            "doRequest": {
                "url": "/API/risk-management-service/risk-alert-review/page-select",
                "method": "post",
                "params": {
                    "alertId": "${{params.alertId}}"
                }
            },
            "thead": [
                {
                    "label": "审核信息ID",
                    "prop": "reviewId",
                    "align": "center"
                },
                {
                    "label": "处理方式",
                    "prop": "processingFlag",
                    "align": "center",
                    "dict": {
                        "dictType": "processingFlagSource",
                        "dictKey": [],
                        "format": "label",
                        "doRequest": {
                            "url": "/views/risk/dictdata/processingFlag.json",
                            "method": "get"
                        }
                    }
                },
                {
                    "label": "申请人",
                    "prop": "applyOperator",
                    "align": "center"
                },
                {
                    "label": "申请时间",
                    "prop": "applyTime",
                    "type": "dateTimePicker",
                    "format": "yyyy-MM-dd HH:mm:ss",
                    "align": "center"
                },
                {
                    "label": "审核状态",
                    "prop": "status",
                    "align": "center"
                },
                {
                    "label": "审核人",
                    "prop": "reviewOperator",
                    "align": "center"
                },
                {
                    "label": "审核时间",
                    "prop": "reviewTime",
                    "type": "dateTimePicker",
                    "format": "yyyy-MM-dd HH:mm:ss",
                    "align": "center"
                },
                {
                    "label": "审核意见",
                    "prop": "reviewRemark",
                    "align": "center"
                }
            ]
        }
    }
}