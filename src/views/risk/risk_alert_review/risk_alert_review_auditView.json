{
    "module": "v-realonlyForm-btn",
    "layout": {
        "form": {
            "initRequest": {
                "url": "/API/risk-management-service/risk-alert-review/queryInfo",
                "method": "post",
                "params": {
                    "reviewId": "${{params.reviewId}}"
                }
            },
            "controls": [
                {
                    "reviewId": {
                        "label": "审核信息ID",
                        "control": "string",
                        "span": "auto"
                    },
                    "processingFlag": {
                        "label": "处理方式",
                        "control": "string",
                        "span": "auto",
                        "dict": {
                            "dictType": "processingFlagSource",
                            "dictKey": [],
                            "format": "value-label",
                            "doRequest": {
                                "url": "/views/risk/dictdata/processingFlag.json",
                                "method": "get"
                            }
                        }
                    }
                },
                {
                    "applyOperator": {
                        "label": "申请人",
                        "control": "string",
                        "span": "auto"
                    },
                    "applyTime": {
                        "label": "申请时间",
                        "control": "string",
                        "span": "auto",
                        "type": "dateTimePicker",
                        "format": "yyyy-MM-dd HH:mm:ss"
                    }
                },
                {
                    "status": {
                        "label": "审核状态",
                        "control": "string",
                        "span": "auto",
                        "dict": {
                            "dictType": "alertReviewStatusSource",
                            "dictKey": [],
                            "format": "value-label",
                            "doRequest": {
                                "url": "/views/risk/dictdata/alertReviewStatus.json",
                                "method": "get"
                            }
                        }
                    },
                    "reviewTime": {
                        "label": "审核时间",
                        "control": "string",
                        "span": "auto",
                        "type": "dateTimePicker",
                        "format": "yyyy-MM-dd HH:mm:ss"
                    }
                },
                {
                    "reviewRemark": {
                        "label": "审核意见",
                        "control": "string",
                        "span": "auto"
                    }
                }
            ]
        }
    }
}