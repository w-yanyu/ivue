{
  "module": "v-realonlyForm-btn",
  "layout": {
    "form": {
      "controls": [
        {
          "statisticCode": {
            "label": "统计代码",
            "control": "string",
            "span": 12
          },
          "statisticName": {
            "label": "统计名称",
            "control": "string",
            "span": 12
          }
        },
        {
          "statisticType": {
            "label": "统计类型",
            "control": "string",
            "span": 12,
            "dict": {
              "dictType": "statisticType",
              "dictKey": [],
              "format": "value-label",
              "doRequest": {
                "url": "/views/risk/dictdata/statisticType.json",
                "method": "get"
              }
            }
          },
          "statisticSource": {
            "label": "统计来源",
            "control": "string",
            "span": 12,
            "dict": {
              "dictType": "statisticSource",
              "dictKey": [],
              "format": "value-label",
              "doRequest": {
                "url": "/views/risk/dictdata/statisticSource.json",
                "method": "get"
              }
            }
          }
        },
        {
          "statisticDimension": {
            "label": "统计维度",
            "control": "string",
            "span": 12,
            "dict": {
              "dictType": "indicatorDimension",
              "dictKey": [],
              "format": "value-label",
              "doRequest": {
                "url": "/views/risk/dictdata/indicatorDimension.json",
                "method": "get"
              }
            }
          },
          "groupDimensionList": {
            "label": "统计分组维度",
            "control": "string",
            "multiple": true,
            "span": 12,
            "dict": {
              "dictType": "statisticField",
              "dictKey": [],
              "format": "value-label",
              "doRequest": {
                "url": "/API/risk-management-service/combobox-select/fact/trans",
                "method": "post"
              }
            }
          }
        },
        {
          "cruleId": {
            "label": "过滤组合规则ID",
            "control": "string",
            "span": 12,
            "dict": {
              "dictType": "rule_combine_list",
              "dictKey": [],
              "format": "value-label",
              "doRequest": {
                "url": "/API/risk-management-service/combobox-select/rule_combine_list",
                "method": "post",
                "params": {
                  "cruleType":"RR"
                }
              }
            }
          },
          "statisticFunction": {
            "label": "统计函数",
            "control": "string",
            "span": 12,
            "dict": {
              "dictType": "statisticFunction",
              "dictKey": [],
              "format": "value-label",
              "doRequest": {
                "url": "/views/risk/dictdata/statisticFunction.json",
                "method": "get"
              }
            }
          }
        },
        {
          "statisticUnit": {
            "label": "统计时间单位",
            "control": "string",
            "span": 12,
            "dict": {
              "dictType": "statisticUnit",
              "dictKey": [],
              "format": "value-label",
              "doRequest": {
                "url": "/views/risk/dictdata/statisticUnit.json",
                "method": "get"
              }
            }
          },
          "statisticCycle": {
            "label": "统计周期",
            "control": "string",
            "span": 12
          }
        },
        {
          "statisticField": {
            "label": "统计字段",
            "control": "string",
            "span": 12,
            "dict": {
              "dictType": "statisticField",
              "dictKey": [],

              "format": "value-label",
              "doRequest": {
                "url": "/API/risk-management-service/combobox-select/fact/trxn-info",
                "method": "post"
              }
            }
          },
          "dataType": {
            "label": "统计数据类型",
            "control": "string",
            "span": 12,
            "disabledOptions": ["String", "Boolean"],
            "dict": {
              "dictType": "dataType",
              "dictKey": [],
              "format": "value-label",
              "doRequest": {
                "url": "/views/risk/dictdata/dataType.json",
                "method": "post"
              }
            }
          }
        },
        {
          "updateTime": {
            "label": "最后修改时间",
            "control": "string",
            "type": "dateTimePicker",
            "format": "yyyy-MM-dd HH:mm:ss",
            "span": 12
          },
          "updateUser": {
            "label": "最后修改人",
            "control": "string",
            "span": 12
          }
        },
        {
          "description": {
            "label": "描述",
            "control": "string",
            "span": 24
          }
        }
      ],
      "initRequest": {
        "url": "/API/risk-management-service/risk-indicator-statistic/query",
        "method": "post"
      }
    }
  }
}
