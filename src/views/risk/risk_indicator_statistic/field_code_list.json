{
  "module": "v-search-btn-datagrid",
  "layout": {
    "toolbar": {
      "btns": [
        {
          "type": "bringback",
          "label": "选择"
        }
      ]
    },
    "datagrid": {
      "doRequest": {
        "url": "/API/risk-management-service/risk-fact/page-select",
        "method": "post",
        "params":{
          "factCode":"trans"
        }
      },
      "pagination": {
        "pageSizes": [
          5,
          10,
          20,
          50,
          100
        ],
        "pageSize": 5
      },
      "thead": [
        {
          "type": "selection",
          "width": "55"
        },
        {
          "label": "事实属性代码",
          "prop": "fieldCode",
          "width": "200px",
          "align": "center"
        },
        {
          "label": "事实属性名称",
          "prop": "fieldName"
        }
      ]
    }
  }
}
