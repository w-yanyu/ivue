{
  "module": "v-search-btn-datagrid",
  "layout": {
    "form": {
      "foldLineNumber": 3,
      "controls": [
        {
          "application_id": {
            "label": "申请编号",
            "control": "text",
            "width": "auto",
            "span": 8
          },
          "system_number_identification": {
            "label": "系统编号标识",
            "control": "text",
            "width": "auto",
            "span": 8
          },
          "system_name": {
            "label": "系统名称",
            "control": "text",
            "width": "auto",
            "span": 8
          },
          "table_code": {
            "label": "规则表代码",
            "control": "lookup",
            "span": 8,
            "width": "auto",
            "keyField": {
              "tree_code": "treeCode",
              "tree_name": "treeName"
            },
            "toRequest": {
              "url": "/views/risk/rule_tree/risk_rule_tree_lookup_one.json",
              "method": "get"
            },
            "props": {
              "title": "规则表选择",
              "width": "80%"
            }
          },
          "table_name": {
            "label": "规则表名称",
            "control": "text",
            "width": "auto",
            "span": 8
          },
          "status": {
            "label": "状态",
            "control": "select",
            "span": 8,
            "width": "auto",
            "dict": {
              "dictType": "DECI_E_ABNORMAL_CAUSE",
              "dictKey": [
                "DECI_E_ABNORMAL_CAUSE"
              ],
              "format": "label"
            }
          },
          "update_time": {
            "control": "dateTimePicker",
            "label": "交易日期",
            "type": "daterange",
            "span": 8,
            "width": "auto",
            "startPlaceholder": "开始时间",
            "endPlaceholder": "结束时间",
            "value": [
              "today-6",
              "today"
            ],
            "unlinkPanels": true,
            "rules": [
              {
                "required": true,
                "message": "请选择交易日期"
              }
            ]
          },
          "branch_id": {
            "label": "机构名称",
            "control": "remoteSelect",
            "labelField": "branch_name",
            "valueField": "branch_id",
            "searchField": "branch_name",
            "span": 8,
            "value": "$g{{user_info.branch_id}}",
            "width": "auto",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "post",
              "params": {
                "servicecode": "ct2191"
              }
            },
            "rules": [
              {
                "required": true,
                "message": "请输入机构名称",
                "max": 125
              }
            ],
            "format": "label"
          }
        }
      ]
    },
    "toolbar": {},
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "uccp5014",
          "limit_component_type": "0",
          "limit_status":"2"
        }
      },
      "pagination": {
        "hideOnSinglePage": false,
        "show": true,
        "pageSizes": [
          10,
          15,
          20
        ],
        "pageSize": 10
      },
      "thead": [
        {
          "label": "序号",
          "type": "index",
          "value":""
        },
        {
          "label": "申请编号",
          "prop": "application_id",
          "value":"202401289850000000371"
        },
        {
          "label": "系统编号标识",
          "prop": "system_number_identification",
          "value":"ICMS"
        },
        {
          "label": "系统名称",
          "prop": "system_name",
          "value":"进件中心"
        },
        {
          "label": "规则表代码",
          "prop": "tableCode",
          "value":"sum001"
        },
        {
          "label": "规则表名称",
          "prop": "tableName",
          "value":"risk_table_sum"
        },
        {
          "label": "交易时间",
          "prop": "transaction_start_time",
          "valueFormat": "yyyy-MM-dd HHmmss",
          "format": "yyyy-MM-dd HH:mm:ss",
          "value":"2024-01-28 10:23:41"
        },
        {
          "label": "结束时间",
          "prop": "transaction_end_time",
          "format": "yyyy-MM-dd HH:mm:ss",
          "value":"2024-01-28 10:23:44"
        },
        {
          "label": "状态",
          "prop": "status",
          "dict": {
            "dictType": "DECI_E_ABNORMAL_CAUSE",
            "dictKey": [
              "DECI_E_ABNORMAL_CAUSE"
            ],
            "format": "label"
          },
          "value":"1"
        },
        {
          "label": "异常原因",
          "prop": "abnormal_cause",
          "value":"没有异常"
        },
        {
          "label": "机构名称",
          "prop": "branch_id",
          "dict": {
            "dictType": "DECI_E_BRANCH_MARK",
            "dictKey": [
              "DECI_E_BRANCH_MARK"
            ],
            "format": "label"
          },
          "value":"985000"
        },
        {
          "label": "操作",
          "width": "150px",
          "align": "center",
          "btns": [
            {
              "type": "look",
              "label": "详情",
              "toRequest": {
                "url": "@decisionTreeResult",
                "method": "get"
              },
              "props": {
                "width": "80%",
                "height": "80%"
              }
            },
            {
              "type": "look",
              "label": "报文明细",
              "toRequest": {
                "#url": "/views/risk/rule_flow/tran_result/rule_flow_result_tabs.json",
                "url": "@RiskDecisionRuleDetail",
                "method": "get"
              },
              "props": {
                "width": "50%"
              }
            }
          ]
        }
      ]
    }
  }
}