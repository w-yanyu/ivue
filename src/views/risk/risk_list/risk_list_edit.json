{
	"module": "v-inputDataForm-btn",
	"layout": {
		"form": {
			"controls": [
				{
					"indicatorCode": {
						"label": "指标变量代码",
						"control": "string",
						"span": 12,
						"rules": [
							{
								"required": true,
								"message": "该控件必填"
							}
						],
            "dict": {
              "dictType": "list-indicator",
              "dictKey": [],
              "format": "value-label",
              "doRequest": {
                "url": "/API/risk-management-service/combobox-select/list-indicator",
                "method": "POST"
              }
            }
					},
					"ownerId": {
						"label": "属主ID",
						"control": "string",
						"span": 12,
						"rules": [
							{
								"required": true,
								"message": "请正确输入",
                "max": 40
							}
						]
					}
				},
        {
          "taskId": {
            "label": "批量任务id",
            "control": "text",
            "span": 12,
            "rules": [
              {
                "message": "请正确输入",
                "max": 20
              }
            ]
          },
          "dateTime":{
            "label": "有效期",
            "control": "dateTimePicker",
            "type": "datetimerange",
            "valueFormat": "timestamp",
            "format": "yyyy-MM-dd HH:mm:ss",
            "#disabledDate": {
              "startRange": "$g{{user_info.tradeDate}}",
              "endRange": "2099-01-01 00:00:00"
            },
            "startField":"effectDate",
            "endField":"expiryDate",
            "span": 12
          }
        },
				{
					"description": {
						"label": "备注",
						"control": "textarea",
						"span": 24,
            "rules": [
              {
                "message": "请正确输入",
                "max": 500
              }
            ]
					}
				}
			],
      "initRequest": "${{params}}",
			"doRequest": {
				"url": "/API/risk-management-service/risk-list/update",
				"method": "post",
        "params":{
          "updateUser": "$g{{user_info.userCode}}"
        }
			}
		}
	}
}
