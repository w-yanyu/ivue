{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "rule_code": {
            "label": "规则代码",
            "control": "text",
            "span": 8
          },
          "rule_name": {
            "label": "规则名称",
            "control": "text",
            "span": 8
          },
          "branch_id": {
            "label": "机构名称",
            "control": "remoteSelect",
            "labelField": "branch_name",
            "valueField": "branch_id",
            "searchField":"branch_name",
            "disabled": true,
            "span": 6,
            "value": "$g{{user_info.branch_id}}",
            "width": "auto",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "post",
              "params": {
                "servicecode": "ct2191"
              }
            },
            "rules": [
              {
                "required": true,
                "message": "请输入机构名称",
                "max": 125
              }
            ],
            "format": "label"
          },
          "busi_version": {
            "label": "版本号",
            "control": "inputNumber",
            "decimal": "1",
            "min": 0,
            "step": 0.1,
            "span": 8
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
            "servicecode": "deci010626"
        }
    },

      "pagination": {
        "hideOnSinglePage": false,
        "show": true,
        "pageSizes": [10, 15, 20],
        "pageSize": 10
      },
      "thead": [
        {
          "label": "规则代码",
          "prop": "ruleCode"
        },
        {
          "label": "规则名称",
          "prop": "ruleName"
        },
        {
          "label": "机构名称",
          "prop": "branch_id",
          "dict": {
            "dictType": "DECI_E_BRANCH_MARK",
            "dictKey": [
              "DECI_E_BRANCH_MARK"
            ],
            "format": "label"
          }
        },
        {
          "label": "版本号",
          "prop": "version"
        },
        {
          "label": "最新维护时间",
          "prop": "dataUpdateTime"
        },
        {
          "label": "操作",
          "width": "150px",
          "btns": [
            {
              "type": "bringback",
              "label": "选择"
            }
          ]
        }
      ]
    }
  }
}
