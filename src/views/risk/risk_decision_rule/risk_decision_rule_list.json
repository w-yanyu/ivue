{
  "module": "v-search-btn-datagrid",
  "pageId": "risk_general_rule",
  "layout": {
    "form": {
      "controls": [
        {"ruleCode": {
          "label": "规则代码",
          "width": "auto",
          "control": "text",
          "span": 8,
          "rules": [
            {
              "message": "字段长度超限",
              "max": 40
            }
          ]
        },
          "ruleName": {
            "label": "规则名称",
            "width": "auto",
            "control": "text",
            "span": 8,
            "rules": [
              {
                "message": "字段长度超限",
                "max": 100
              }
            ]
          },
          "branchId": {
            "label": "机构名称",
            "control": "remoteSelect",
            "labelField": "branch_name",
            "valueField": "branch_id",
            "searchField":"branch_name",
            "span": 6,
            "value": "$g{{user_info.branch_id}}",
            "width": "auto",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "post",
              "params": {
                "servicecode": "ct2191"
              }
            },
            "format": "label"
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "label": "新增",
          "authId": "general_rule_add_btn",
          "toRequest": {
            "##url": "@riskDecisonRule",
            "url": "@decisionRuleEdit",
            "method": "get",
            "params": {}
          },
          "props": {
            "width": "85%",
            "title": "新增"
          },
          "show":{
            "expression":"false"
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "deci010501",
          
          "isList": true
        }
      },
      "pagination": {
        "hideOnSinglePage": false,
        "show": true,
        "pageSizes": [10, 15, 20],
        "pageSize": 10
      },
      "thead": [
        {
          "label": "序号",
          "type": "index"
        },
        {
          "label": "规则代码",
          "width": "150px",
          "prop": "ruleCode"
        },
        {
          "label": "规则名称",
          "width": "120px",
          "prop": "ruleName"
        },
        {
          "label": "机构名称",
          "prop": "branchId",
          "dict": {
            "dictType": "DECI_E_BRANCH_MARK",
            "dictKey": [
              "DECI_E_BRANCH_MARK"
            ],
            "format": "label"
          }
        },
        {
          "label": "版本号",
          "prop": "version"
        }, 
        {
          "label": "服务对象",
          "prop": "serviceObject"
        },   {
          "label": "备注",
          "prop": "description"
        },
        {
          "label": "最新维护人",
          "prop": "applyUser",
          "dict": {
            "dictType":"deciOnl",
              "dictKey": [],
              "format": "label",
              "labelField": "user_name",
              "valueField": "user_id",
              "doRequest": {
                  "url": "/SUMP/icmcall/icmRPCCall",
                  "method": "POST",
                  "params": {
                    "length":1000,
                    "start":1,
                      "servicecode": "ct2503"
                  }
              }
          }
        },
       
        {
          "label": "最新维护时间",
          "prop": "applyTime"
        },
        {
          "label": "操作",
          "width": "200px",
          "align": "left",
          "btns": [
            {
              "type": "look",
              "label": "查看",
              "authId": "general_rule_vie_btn",
              "toRequest": {
                "url": "@riskDecisonRuleView",
                "method": "get"
              },
              "props": {
                "width": "85%"
              }
            },
            {
              "type": "confirmIncreaseBtn",
              "label": "变更",
              "width": "70px",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "deci010607",
                  "riskOperationType":"U",
                  "approveStatus":"00"

                }
              },"show": {
                "expression": "'${{current.branchId}}' == '$g{{user_info.branch_id}}'"
              },
              "confirm": {
                "title": "提示信息",
                "msgTemplate":"是否对“${{current.ruleCode}}”“${{current.ruleName}}”发起变更？",
                "msg": "",
                "confirmButtonText": "确定",
                "cancelButtonText": "取消"
              }
            },{
              "type": "custom",
              "label": "复制",
              "toRequest": {
                "url": "/views/risk/risk_decision_rule/risk_decision_rule_copy.json",
                "method": "get",
                "params": {
                  "scoreCardCode":"${{current.ruleCode}}",
                  "scoreCardName":"${{current.ruleName}}",
                  "currVersion":"${{current.currVersion}}"
                }
              },
              "props": {
               
              },
              "show": {
                "expression": "true"
              }
            },
            {
              "type": "confirmIncreaseBtn",
              "label": "版本切换",
              "width": "70px",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "deci010607",
                  "riskOperationType":"S",
                  "approveStatus":"00"
                }
              },
              "confirm": {
                "title": "提示信息",
                "msgTemplate":"是否对“${{current.ruleCode}}”“${{current.ruleName}}”发起版本切换？",
                "msg": "",
                "confirmButtonText": "确定",
                "cancelButtonText": "取消"
              },"show": {
                "expression": "'${{current.branchId}}' == '$g{{user_info.branch_id}}'"
              }
            },
            {
              "type": "delete",
              "label": "删除",
              "authId": "general_rule_del_btn",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "post",
                "params": {
                  "servicecode": "deci010607",
                  "riskOperationType":"D",
                  "approveStatus":"00"
                }
              },
              "show": {
                "expression": "'${{current.branchId}}' == '$g{{user_info.branch_id}}'"
              }
            },
            {
              "type": "custom",
              "label": "还原",
              "#authId": "general_rule_cancel_btn",
              "#icon": "el-icon-delete",
              "doRequest": {
                "url": "/SUMP/riskController/riskCall",
                "method": "post",
                "params": {
                  "scoreCardType": "1",
                  "servicecode": "risk-management-service/risk-decision-rule/cancel",
                  "appId": "risk-management-service"
                }
              },
              "show": {
                "expression": "false"
              },
              "confirm": {
                "title": "提示信息",
                "msg": "确定要还原至最新生效版本吗?",
                "confirmButtonText": "确定",
                "cancelButtonText": "取消"
              }
            },
            {
              "type": "custom",
              "label": "审核",
              "#authId": "general_rule_review_btn",
              "#icon": "el-icon-edit",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "post",
                "params": {
                  "servicecode": "deci010521"
                }
              },
              "show": {
                "expression": "false"
              },
              "confirm": {
                "title": "提示信息",
                "msg": "确定提交审核吗?",
                "confirmButtonText": "确定",
                "cancelButtonText": "取消"
              }
            }
          ]
        }
      ]
    }
  }
}
