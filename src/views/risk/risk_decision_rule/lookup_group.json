{
  "module": "v_search_btn_datagrid",
  "layout": {
   "toolbar": {
     "btns": [
       {
         "type": "bringback",
         "label": "确认选择",
         "theme": "primary"
       }
     ]
   },
    "datagrid": {
      "doRequest": {
        "url": "/API/risk-management-service/risk-decision-rule-group/page-select",
        "method": "post"
      },
      "thead": [{
          "type": "selection",
          "width": "55"
        },
       {
         "label": "规则组id",
         "prop": "groupId"
       },
       {
         "label": "规则组名称",
         "prop": "groupName"
       },
       {
         "label": "规则组代码",
         "prop": "groupCode"
       },
       {
         "label": "备注",
         "prop": "description"
       },
       {
         "label": "最后更新人",
         "prop": "updateUser"
       },
       {
         "label": "最后修改日期",
         "prop": "updateTime",
         "format": "yyyy-MM-dd HH:mm:ss"
       }
      ]
    }
  }
}
