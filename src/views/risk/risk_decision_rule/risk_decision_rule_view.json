{
  "module": "v-realonlyForm-btn",
  "layout": {
    "form": {
      "controls": [
        {
          "ruleCode": {
            "label": "规则代码",
            "width": "auto",
            "control": "string",
            "span": 8,
            "rules": [{
              "required": true
            }]
          },
          "ruleName": {
            "label": "规则名称",
            "width": "auto",
            "control": "string",
            "span": 8,
            "rules": [{
              "required": true
            }]
          },
          "version": {
            "label": "版本号",
            "width": "auto",
            "control": "string",
            "span": 8
          },
          "applyTime": {
            "label": "最新维护时间",
            "width": "auto",
            "control": "string",
            "type": "dateTimePicker",
            "#format": "yyyy-MM-dd HH:mm:ss",
            "span": 8,
            "rules": [{
              "required": true
            }]
          },
          "branchId": {
            "label": "机构名称",
            "width": "auto",
            "disabled": true,
            "control": "select",
            "dict": {
              "dictType": "DECI_E_BRANCH_MARK",
              "dictKey": [
                "DECI_E_BRANCH_MARK"
              ],
              "format": "label"
            },
            "span": 8,
            "rules": [{
              "required": true
            }]
          },
          "ruleId": {
            "label": "规则id",
            "control": "hidden",
            "hidden": "true",
            "span": 12
          }
        },
        {
          "description": {
            "label": "备注",
            "width": "auto",
            "control": "textarea",
            "disabled": true,
            "span": 24,
            "autosize": {
              "minRows": 4
            }
          }
        }
      ],
      "initRequest": "${{params}}"
    }
  }
}