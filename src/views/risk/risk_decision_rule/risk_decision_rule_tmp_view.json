{
  "module": "v-realonlyForm-btn",
  "layout": {
    "form": {
      "controls": [{
         
          "ruleCode": {
            "label": "规则代码",
            "control": "string",
            "width":"auto",
            "span": 8,
            "rules": [{
              "required": true
            }]
          },
          "ruleName": {
            "label": "规则名称",
            "control": "string",
            "width":"auto",
            "span": 8,
            "rules": [{
              "required": true
            }]
          },
          "version": {
            "label": "版本号",
            "control": "string",
            "width":"auto",
            "span": 8,
            "rules": [{
              "required": true
            }]
          },
          "updateTime": {
            "label": "最新维护时间",
            "control": "string",
            "type": "dateTimePicker",
            "width":"auto",
            "#format": "yyyy-MM-dd HH:mm:ss",
            "span": 8,
            "rules": [{
              "required": true
            }]
          }
        },
        {
          "riskOperationType": {
            "label": "操作类型",
            "control": "select",
            "span": 8,
            "rules": [{
              "required": true
            }],
            "disabled": true,
            "width":"auto",
            "options": [
              {
              "label": "新增",
              "value": "A"
              },{
              "label": "变更",
              "value": "U"
              },
              {
                "label": "删除",
                "value": "D"
                },
               {
                  "label": "复制",
                  "value": "C"
                },
                {
                  "label": "版本切换",
                  "value": "S"
                }
          ]
          }
          , "ruleId": {
            "label": "规则id",
            "control": "hidden",
            "hidden":"true",
            "width":"auto",
            "span": 12
          }
         
        },
        {
          "description": {
            "label": "备注",
            "control": "textarea",
            "disabled": true,
            "span": 24,
            "width":"auto",
            "autosize": {
              "minRows": 4
            }
          }
        }
      ],
      "initRequest": "${{params}}"
    }
  }
}
