{
  "module": "dynamicTabs",
  "layout": {
    "showFormBtn": false,
    "tabs": {
      "defaultActiveKey": "primaryInfo",
      "position": "top",
      "styleClass": "card",
      "tabHeader": [
        {
          "id": "primaryInfo",
          "label": "基本信息",
          "closable": false,
          "disabled": false,
          "icon": "el-icon-date",
          "toRequest": {
            "url": "/views/risk/risk_decision_rule/group/risk_decision_rule_group_view_look.json",
            "method": "GET",
            "params": {
              "roleCode": "${{params.roleCode}}"
            }
          },
          "props": {
            "width": "65%"
          }
        },
        {
          "id": "resource",
          "label": "授权资源",
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "toRequest": {
            "url": "/views/risk/risk_decision_rule/group/risk_decision_rule_group_rela_look.json"
          },
          "props": {
            "width": "65%"
          }
        }
      ]
    }
  }
}