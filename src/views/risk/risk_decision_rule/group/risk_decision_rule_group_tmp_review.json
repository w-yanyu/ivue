{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "btns": [
        {
          "type": "close",
          "label": "取消"
        },
        {
          "type": "submit",
          "target": "addlog",
          "label": "确定",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "post",
            "params": {
              "servicecode": "deci010620",
              "approveStatus": "01"
            }
          }
        }
      ],
      "controls": [
        {
          "groupId": {
            "control": "hidden",
            "disabled": "true",
            "width": "auto",
            "label": "规则组ID",
            "value": "${{params.groupId}}"
            
          },
          "groupCode": {
            "control": "text",
            "disabled": "true",
            "width": "auto",
            "label": "规则组代码",
            "value": "${{params.groupCode}}",
            "rules": [
              {
                "required": true
              }
            ],
            "span": 24
          }
        },
        {
          "groupName": {
            "control": "text",
            "label": "规则组名称",
            "width": "auto",
            "disabled": "true",
            "value": "${{params.groupName}}",
            "rules": [
              {
                "required": true
              }
            ],
            "span": 24
          }
        },
        {
          "riskOperationType": {
            "label": "操作类型",
            "width": "auto",
            "disabled": "true",
            "rules": [
              {
                "required": true
              }
            ],
            "control": "select",
            "value": "${{params.riskOperationType}}",
            "dict": {
              "dictType": "DECI_E_RISK_OPERATION_TYPE",
              "dictKey": [
                "DECI_E_RISK_OPERATION_TYPE"
              ],
              "format": "label"
            },
            "span": 24
          }
        }
      ]
    }
  }
}