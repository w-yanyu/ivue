{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "btns": [],
      "initRequest": "${{params}}",
      "controls": [
        {
          "groupId": {
            "label": "规则组ID",
            "control": "hidden",
            "span": 8
          },
          "groupCode": {
            "label": "规则组代码",
            "width": "auto",
            "control": "string",
            "span": 8
          },
          "groupName": {
            "label": "规则组名称",
            "width": "auto",
            "control": "string",
            "span": 8
          },
          "busiVersion": {
            "label": "版本号",
            "width": "auto",
            "control": "string",
            "span": 8
          },
          "updateTime": {
            "label": "最新维护时间",
            "width": "auto",
            "control": "string",
            "#type": "dateTimePicker",
            "span": 8,
            "#format": "yyyy-MM-dd HH:mm:ss"
          },
          "riskOperationType": {
            "label": "操作类型",
            "width": "auto",
            "control": "select",
            "span": 8,
            "disabled": true,
            "options": [
              {
                "label": "新增",
                "value": "A"
              },
              {
                "label": "变更",
                "value": "U"
              },
              {
                "label": "删除",
                "value": "D"
              },
              {
                "label": "复制",
                "value": "C"
              },
              {
                "label": "版本切换",
                "value": "S"
              }
            ]
          }
        },
        {
          "description": {
            "label": "备注",
            "width": "auto",
            "control": "string",
            "rows": 5,
            "span": 24
          }
        }
      ]
    }
  }
}