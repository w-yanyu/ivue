{
  "module": "lr-transferTable",
  "layout": {
    "transfer": {
      "height": "500px",
      "left": {
        "title": "未分配",
        "query": ["ruleName"],
        "btn": {
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "groupId": "${{params.groupId}}",
              "servicecode": "deci010512",
              "isList": true
            }
          }
        },
        "datagrid": {
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "post",
            "params": {
              "groupId": "${{params.groupId}}",
              "busiVersion":"${{params.busiVersion}}",
              "servicecode": "deci010619",
              "isList": true
            }
          },
          "pagination": {
            "hideOnSinglePage": false,
            "show": true,
            "pageSizes": [10, 15, 20],
            "pageSize": 10
          },
          "thead": [
            {
              "prop": "ruleCode",
              "label": "规则编码",
              "align": "left"
            },
            {
              "prop": "ruleName",
              "label": "规则名称",
              "align": "left"
            }
          ]
        }
      },
      "right": {
        "title": "已分配",
        "query": ["ruleName"],
        "btn": {
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "post",
            "params": {
              "group_id": "${{params.groupId}}",
              "roleCode": "${{params.roleCode}}",
              "servicecode": "deci010518",
              "isList": true
            }
          }
        },
        "datagrid": {
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "post",
            "params": {
              "groupId": "${{params.groupId}}",
              "busiVersion":"${{params.busiVersion}}",
              "servicecode": "deci010618",
              "isList": true
            }
          },
          "pagination": {
            "hideOnSinglePage": false,
            "show": true,
            "pageSizes": [10, 15, 20],
            "pageSize": 10
          },
          "thead": [
            {
              "prop": "ruleCode",
              "label": "规则编码",
              "align": "left"
            },
            {
              "prop": "ruleName",
              "label": "规则名称",
              "align": "left"
            }
          ]
        }
      }
    }
  }
}
