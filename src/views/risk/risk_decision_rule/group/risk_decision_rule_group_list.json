{
  "module": "v-search-btn-datagrid",
  "pageId": "risk_general_rule_group",
  "layout": {
    "form": {
      "controls": [
        {  
        "groupCode": {
          "label": "规则组代码",
          "control": "text",
          "span": 8,
          "width": "auto",
          "align": "left",
          "rules": [
            {
              "message": "字段长度超限",
              "max": 40
            }
          ]
        },
          "groupName": {
            "label": "规则组名称",
            "control": "text",
            "span": 8,
            "width": "auto",
            "rules": [
              {
                "message": "字段长度超限",
                "max": 100
              }
            ]
          },
          "branchId": {
            "label": "机构",
            "control": "remoteSelect",
            "labelField": "branch_name",
            "valueField": "branch_id",
            "searchField":"branch_name",
            "span": 6,
            "value": "$g{{user_info.branch_id}}",
            "width": "auto",
            "format": "label"
          }
        }
      ]
    },
    "toolbar": {
      "btns": []
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "deci010506",
          "isList": true
        }
      },
      "pagination": {
        "hideOnSinglePage": false,
        "show": true,
        "pageSizes": [
          10,
          15,
          20
        ],
        "pageSize": 10
      },
      "thead": [
        {
          "label": "序号",
          "type": "index"
        },
        {
          "label": "规则组代码",
          "width": "140px",
          "prop": "groupCode"
        },
        {
          "label": "规则组名称",
          "width": "130px",
          "prop": "groupName"
        },
        {
          "label": "机构名称",
          "prop": "branchId",
          "dict": {
            "dictType": "DECI_E_BRANCH_MARK",
            "dictKey": [
              "DECI_E_BRANCH_MARK"
            ],
            "format": "label"
          }
        },
        {
          "label": "版本号",
          "prop": "busiVersion"
        },
        {
          "label": "服务对象",
          "prop": "serviceObject"
        },
        {
          "label": "备注",
          "prop": "description"
        },
        {
          "label": "最新维护人",
          "prop": "applyUser",
          "dict": {
            "dictType":"deciOnl",
              "dictKey": [],
              "format": "label",
              "labelField": "user_name",
              "valueField": "user_id",
              "doRequest": {
                  "url": "/SUMP/icmcall/icmRPCCall",
                  "method": "POST",
                  "params": {
                    "length":1000,
                    "start":1,
                      "servicecode": "ct2503"
                  }
              }
          }
        },
        {
          "label": "最新维护时间",
          "prop": "applyTime"
        },
        {
          "label": "操作",
          "width": "220px",
          "btns": [
            {
              "type": "look",
              "label": "查看",
              "authId": "general_rule_group_vie_btn",
              "toRequest": {
                "url": "/views/risk/risk_decision_rule/group/risk_decision_rule_group_tabs_look.json",
                "method": "get"
              },
              "props": {
                "title": "查看",
                "width": "80%"
              }
            },
            {
              "type": "confirmIncreaseBtn",
              "label": "变更",
              "width": "70px",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "deci010615",
                  "riskOperationType": "U",
                  "approveStatus": "00"
                }
              },
              "confirm": {
                "title": "提示信息",
                "msgTemplate":"是否对“${{current.groupCode}}”“${{current.groupName}}”发起变更？",
                "msg": "",
                "confirmButtonText": "确定",
                "cancelButtonText": "取消"
              },
              "show": {
                "expression": "'${{current.branchId}}' == '$g{{user_info.branch_id}}'"
              }
            },
            {
              "type": "custom",
              "label": "复制",
              "toRequest": {
                "url": "/views/risk/risk_decision_rule/group/risk_decision_rule_group_copy.json",
                "method": "get",
                "params": {
                  "scoreCardCode": "${{current.groupCode}}",
                  "scoreCardName": "${{current.groupName}}",
                  "currVersion": "${{current.currVersion}}"
                }
              },
              "props": {
              },
              "show": {
                "expression": "'${{current.branchId}}' == '$g{{user_info.branch_id}}'"
              }
            },
            {
              "type": "confirmIncreaseBtn",
              "label": "版本切换",
              "width": "70px",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "deci010615",
                  "riskOperationType": "S",
                  "approveStatus": "00"
                }
              },
              "confirm": {
                "title": "提示信息",
                "msgTemplate":"是否对“${{current.groupCode}}”“${{current.groupName}}”发起版本切换？",
                "msg": "",
                "confirmButtonText": "确定",
                "cancelButtonText": "取消"
              },
              "show": {
                "expression": "'${{current.branchId}}' == '$g{{user_info.branch_id}}'"
              }
            },
            {
              "type": "delete",
              "label": "删除",
              "authId": "general_rule_group_del_btn",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "post",
                "params": {
                  "servicecode": "deci010615",
                  "riskOperationType": "D",
                  "approveStatus": "00"
                }
              },
              "show": {
                "expression": "'${{current.branchId}}' == '$g{{user_info.branch_id}}'"
              }
            }
          ]
        }
      ]
    }
  }
}