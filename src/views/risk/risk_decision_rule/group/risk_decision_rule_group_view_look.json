{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "btns":[],
      "initRequest": "${{params}}",
      "controls": [
        {
          "groupId": {
            "label": "规则组ID",
            "control": "hidden",
            "span": 8
          },
          "groupCode": {
            "label": "规则组代码",
            "width":"auto",
            "control": "string",
            "span": 8
          },
          "groupName": {
            "label": "规则组名称",
            "width":"auto",
            "control": "string",
            "span": 8
          },
          "busiVersion": {
            "label": "版本号",
            "width":"auto",
            "control": "string",
            "span": 8
          }
        },
        {
        
          "applyTime": {
            "label": "最新维护时间",
            "width":"auto",
            "control": "string",
            "#type": "dateTimePicker",
            "span": 8,
            "#format": "yyyy-MM-dd HH:mm:ss"
          },
          "branchId": {
            "label": "机构名称",
            "width": "auto",
            "disabled": true,
            "control": "select",
            "dict": {
              "dictType": "DECI_E_BRANCH_MARK",
              "dictKey": [
                "DECI_E_BRANCH_MARK"
              ],
              "format": "label"
            },
            "span": 8
          }
        },
        {
          "description": {
            "label": "备注",
            "width":"auto",
            "control": "string",
            "span": 24
          }
        }
      ]
    }
  }
}
