{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "controls": [
        {
          "groupCode": {
            "label": "规则组代码",
            "control": "string",
            "width": "auto",
            "span": 8,
            "rules": [
              {
                "required": true,
                "message": "请正确输入"
              }
            ]
          },
          "groupName": {
            "label": "规则组名称",
            "control": "text",
            "width": "auto",
            "span": 8,
            "rules": [
              {
                "required": true,
                "pattern": "^[a-z0-9A-Z\u4e00-\u9fa5]+$",
                "message": "仅可为字母、数字、文字及相关组合"
              }
            ]
          },
          "busiVersion": {
            "label": "版本号",
            "control": "string",
            "width": "auto",
            "span": 8,
            "rules": [
              {
                "required": true
              }
            ]
          },
          "groupId": {
            "label": "规则组id",
            "control": "hidden"
          }
        },
        {
          "riskOperationType": {
            "label": "操作类型",
            "control": "string",
            "span": 8,
            "width": "auto",
            "dict": {
              "dictType": "DECI_E_RISK_OPERATION_TYPE",
              "dictKey": [
                "DECI_E_RISK_OPERATION_TYPE"
              ],
              "format": "label"
            }
          }
        },
        {
          "description": {
            "label": "备注",
            "control": "textarea",
            "showWordLimit": 200,
            "maxlength": 200,
            "span": 24,
            "width": "auto",
            "autosize": {
              "minRows": 5
            }
          }
        }
      ],
      "initRequest": "${{params}}",
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "data_create_user": "$g{{user_info.userCode}}",
          "data_update_user": "$g{{user_info.userCode}}",
          "servicecode": "deci010613"
        }
      }
    }
  }
}