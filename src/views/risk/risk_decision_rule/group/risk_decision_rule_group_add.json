{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "controls": [
        {
          "group_code": {
            "label": "规则组代码",
            "control": "text",
            "span": 12,
            "rules": [
              {
                "required": true,
                "pattern": "^[A-Za-z][A-Za-z\\d_]{0,59}$",
                "message": "请正确输入"
              }
            ]
          },
          "group_name": {
            "label": "规则组名称",
            "control": "text",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请正确输入"
              }
            ]
          }
        },
        {
          "description": {
            "label": "备注",
            "control": "textarea",
            "span": 24,
            "rules": [
              {
                "message": "字段长度超限",
                "max": 500
              }
            ]
          }
        }
      ],
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",

        "params":{
          "data_create_user": "$g{{user_info.userCode}}",
          "data_update_user": "$g{{user_info.userCode}}",
          "servicecode": "deci010510"
        }
      }
    }
  }
}
