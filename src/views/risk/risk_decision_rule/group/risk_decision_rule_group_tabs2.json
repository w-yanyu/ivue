{
  "module": "dynamicTabs",
  "layout": {
    "showFormBtn": false,
    "tabs": {
      "defaultActiveKey": "primaryInfo",
      "position": "top",
      "styleClass": "card",
      "tabHeader": [
        {
          "id": "primaryInfo",
          "label": "基本信息",
          "closable": false,
          "disabled": false,
          "icon": "el-icon-date",
          "toRequest": {
            "url": "/views/risk/risk_decision_rule/group/risk_decision_rule_group_view.json",
            "method": "GET",
            "params": {
              "groupCode": "${{params.groupCode}}",
              "groupName": "${{params.groupName}}",
              "busiVersion": "${{params.busiVersion}}",
              "updateUser": "${{params.updateUser}}",
              "updateTime": "${{params.updateTime}}",
              "riskOperationType": "${{params.riskOperationType}}",
              "description": "${{params.description}}",
              "objectName": "${{params.serviceObject}}"
            }
          },
          "props": {
            "width": "65%"
          }
        },
        {
          "id": "resource",
          "label": "授权资源",
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "toRequest": {
            "url": "/views/risk/risk_decision_rule/group/risk_decision_rule_group_rela2.json",
            "method": "GET",
             "params": {
              "groupId": "${{params.groupId}}",
              "groupCode": "${{params.groupCode}}",
              "groupName": "${{params.groupName}}",
              "busiVersion": "${{params.busiVersion}}",
              "updateUser": "${{params.updateUser}}",
              "updateTime": "${{params.updateTime}}",
              "riskOperationType": "${{params.riskOperationType}}",
              "description": "${{params.description}}"
            }
          },
          "props": {
            "width": "65%"
          }
        }
      ]
    }
  }
}