{
  "module": "v-search-btn-datagrid",
  "layout": {
    "form": {
      "foldLineNumber": 3,
      "controls": [
        {
          "application_id": {
            "label": "申请编号",
            "control": "text",
            "width": "auto",
            "span": 8
          },
          "system_number_identification": {
            "label": "系统编号标识",
            "control": "text",
            "width": "auto",
            "span": 8
          },
          "system_name": {
            "label": "系统名称",
            "control": "text",
            "width": "auto",
            "span": 8
          },
          "rule_code": {
            "label": "规则代码",
            "control": "lookup",
            "span": 8,
            "width": "auto",
            "keyField": {
              "rule_code": "ruleCode",
              "rule_name": "ruleName"
            },
            "toRequest": {
              "url": "/views/risk/risk_decision_rule/lookup.json",
              "method": "get"
            },
            "props": {
              "title": "规则选择",
              "width": "80%"
            }
          },
          "rule_name": {
            "label": "规则名称",
            "control": "text",
            "width": "auto",
            "span": 8
          },
          "status": {
            "label": "状态",
            "control": "select",
            "width": "auto",
            "span": 8,
            "dict": {
              "dictType": "DECI_E_ABNORMAL_CAUSE",
              "dictKey": [
                "DECI_E_ABNORMAL_CAUSE"
              ],
              "format": "label"
            }
          },
          "update_time": {
            "control": "dateTimePicker",
            "label": "交易日期",
            "type": "daterange",
            "span": 8,
            "width": "auto",
            "startPlaceholder": "开始时间",
            "endPlaceholder": "结束时间",
            "value": [
              "today-6",
              "today"
            ],
            "unlinkPanels": true,
            "rules": [
              {
                "required": true,
                "message": "请选择交易日期"
              }
            ]
          },
          "branch_id": {
            "label": "机构名称",
            "control": "remoteSelect",
            "labelField": "branch_name",
            "valueField": "branch_id",
            "searchField": "branch_name",
            "span": 8,
            "value": "$g{{user_info.branch_id}}",
            "width": "auto",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "post",
              "params": {
                "servicecode": "ct2191"
              }
            },
            "rules": [
              {
                "required": true,
                "message": "请输入机构名称",
                "max": 125
              }
            ],
            "format": "label"
          }
        }
      ]
    },
    "toolbar": {},
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "deci060103",
          "isList": true
        }
      },
      "pagination": {
        "hideOnSinglePage": false,
        "show": true,
        "pageSizes": [
          10,
          15,
          20
        ],
        "pageSize": 10
      },
      "thead": [
        {
          "label": "序号",
          "type": "index"
        },
        {
          "label": "申请编号",
          "prop": "application_id"
        },
        {
          "label": "系统编号标识",
          "prop": "system_number_identification"
        },
        {
          "label": "系统名称",
          "prop": "system_name"
        },
        {
          "label": "规则代码",
          "prop": "rule_code"
        },
        {
          "label": "规则名称",
          "prop": "rule_name"
        },
        {
          "label": "是否命中",
          "prop": "hit_flag",
          "dict": {
            "dictType": "hitFlag",
            "dictKey": [],
            "doRequest": {
              "url": "/views/risk/dictdata/hitFlag.json",
              "method": "get"
            }
          }
        },
        {
          "label": "交易时间",
          "prop": "transaction_start_time",
          "valueFormat": "yyyy-MM-dd HHmmss",
          "format": "yyyy-MM-dd HH:mm:ss"
        },
        {
          "label": "结束时间",
          "prop": "transaction_end_time",
          "format": "yyyy-MM-dd HH:mm:ss"
        },
        {
          "label": "状态",
          "prop": "status",
          "dict": {
            "dictType": "DECI_E_ABNORMAL_CAUSE",
            "dictKey": [
              "DECI_E_ABNORMAL_CAUSE"
            ],
            "format": "label"
          }
        },
        {
          "label": "异常原因",
          "prop": "abnormal_cause"
        },
        {
          "label": "机构名称",
          "prop": "branch_id",
          "dict": {
            "dictType": "DECI_E_BRANCH_MARK",
            "dictKey": [
              "DECI_E_BRANCH_MARK"
            ],
            "format": "label"
          }
        },
        {
          "label": "操作",
          "width": "150px",
          "align": "center",
          "btns": [
            {
              "type": "look",
              "label": "详情",
              "toRequest": {
                "###url": "/views/risk/risk_decision_rule/risk_decision_rule_action_result.json",
                "url": "@normalRuleTestResultSearch",
                "method": "get"
              },
              "props": {
                "width": "80%"
              },
              "show": {
                "expression": " '${{current.hit_flag}}'== '是' "
              }
            },
            {
              "type": "look",
              "label": "报文明细",
              "toRequest": {
                "#url": "/views/risk/rule_flow/tran_result/rule_flow_result_tabs.json",
                "url": "@RiskDecisionRuleDetail",
                "method": "get"
              },
              "props": {
                "width": "50%"
              }
            }
          ]
        }
      ]
    }
  }
}