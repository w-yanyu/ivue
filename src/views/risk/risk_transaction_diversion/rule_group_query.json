{
  "module": "v-search-btn-datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "groupName": {
            "label": "规则组名称 ",
            "control": "text",
            "span": 12,
            "rules": [
              {
                "message": "字段长度超限",
                "max": 100
              }
            ]
          },
          "groupId": {
            "label": "规则组ID",
            "control": "text",
            "span": 12,
            "rules": [
              {
                "message": "字段长度超限",
                "max": 8
              }
            ]
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "bringback",
          "label": "选择"
        }
      ]
    },
    "datagrid": {
      "doRequest": {
        "url": "/API/risk-management-service/risk-rule-group/page-select",
        "method": "post"
      },
      "pagination": {
        "pageSizes": [
          5,
          10,
          20,
          50,
          100
        ],
        "pageSize": 5
      },
      "thead": [
        {
          "type": "selection",
          "width": "55"
        },
        {
          "label": "规则组ID",
          "prop": "groupId",
          "width": "200px",
          "align": "center"
        },
        {
          "label": "规则组名称",
          "prop": "groupName"
        }
      ]
    }
  }
}
