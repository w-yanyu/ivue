{
  "module": "v-realonlyForm-btn",
  "layout": {
    "form": {
      "controls": [
        {
          "id": {
            "label": "交易分流id",
            "control": "string",
            "span": 12
          },
          "trxnClass": {
            "label": "交易类型",
            "control": "string",
            "span": 12,
            "dict": {
              "dictType": "trxnClass",
              "dictKey": [],
              "format": "value-label",
              "doRequest": {
                "url": "/views/risk/dictdata/trxnClass.json",
                "method": "get"
              }
            }
          }
        },
        {
          "networkType": {
            "label": "网络类型",
            "control": "string",
            "span": 12,
            "dict": {
              "dictType": "networkType",
              "dictKey": [],
              "format": "value-label",
              "doRequest": {
                "url": "/views/risk/dictdata/networkType.json",
                "method": "get"
              }
            }
          },
          "posEntryMode": {
            "label": "输入方式",
            "control": "string",
            "span": 12,
            "dict": {
              "dictType": "posEntryMode",
              "dictKey": [],
              "format": "value-label",
              "doRequest": {
                "url": "/views/risk/dictdata/posEntryMode.json",
                "method": "get"
              }
            }
          }
        },
        {
          "countryCode": {
            "label": "国家代码",
            "control": "string",
            "span": 12
          },
          "policyGroups": {
            "label": "政策性规则组ID",
            "control": "string",
            "span": 12
          }
        },
        {
          "defaultGroups": {
            "label": "默认规则组ID",
            "control": "string",
            "span": 12
          },
          "businessGroups": {
            "label": "业务规则组ID",
            "control": "string",
            "span": 12
          }
        },
        {
          "description": {
            "label": "描述",
            "control": "string",
            "span": 24
          }
        },
        {
          "updateUser": {
            "label": "最后更新人",
            "control": "string",
            "span": 12
          },
          "updateTime": {
            "label": "最后更新时间",
            "control": "string",
            "type": "dateTimePicker",
            "format": "yyyy-MM-dd HH:mm:ss",
            "span": 12
          }
        }
      ],
      "initRequest": "${{params}}"
    }
  }
}
