{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "deci050109",
          "apply_no": "${{params.apply_no}}"
        }
      },
      "thead": [
        {
          "type": "index",
          "align": "center"
        },
        {
          "label": "字段分组",
          "prop": "field_group_code",
          "align": "center",
          "dict": {
            "dictKey": [
              "DECI_E_FIELD_GROUP"
            ],
            "format": "value-label",
            "dictType": "DECI_E_FIELD_GROUP"
          }
        },
        {
          "label": "字段名",
          "prop": "field_code",
          "align": "center"
        },
        {
          "label": "字段中文名称",
          "prop": "field_name",
          "align": "center"
        },
        {
          "label": "字段输入值",
          "align": "center",
          "prop": "input_value"
        },
        {
          "label": "操作",
          "fixed": "right",
          "width": 100,
          "align": "center",
          "btns": [
            {
              "type": "look",
              "label": "进件字段详情",
              "toRequest": {
                "url": "/views/risk/risk_icms_management/icms_trans/icms_trans_field_detail.json",
                "method": "POST"
              },
              "props": {
                "width": "40%",
                "title": "进件字段详情"
              }
            }
          ]
        }
      ]
    }
  }
}