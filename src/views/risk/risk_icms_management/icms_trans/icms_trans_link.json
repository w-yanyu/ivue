{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "daterange": {
            "control": "dateTimePicker",
            "label": "进件起止时间",
            "type": "daterange",
            "unlinkPanels": true,
            "valueFormat": "yyyyMMdd",
            "startPlaceholder": "请选择开始时间",
            "endPlaceholder": "请选择结束时间",
            "startField": "start_date",
            "endField": "end_date"
          },
          "case_id": {
            "control": "text",
            "label": "进件单号"
          },
          "case_status": {
            "control": "select",
            "label": "业务场景",
            "dict": {
              "dictType": "CS_E_STATUS_CD",
              "dictKey": [
                "CS_E_STATUS_CD"
              ],
              "format": "value-label"
            }
          },
          "cust_name": {
            "control": "text",
            "label": "客户名称"
          },
          "cert_type": {
            "label": "证件类型",
            "control": "select",
            "dict": {
              "dictType": "MS_E_CRET_TYPE",
              "dictKey": [
                "MS_E_CRET_TYPE"
              ],
              "labelField": "dictName",
              "valueField": "dictId"
            }
          },
          "cert_no": {
            "control": "text",
            "label": "证件号码"
          },
          "contact_no": {
            "control": "text",
            "label": "手机号码"
          },
          "status": {
            "label": "状态",
            "control": "select",
            "dict": {
              "dictType": "MS_E_CRET_TYPE",
              "dictKey": [
                "MS_E_CRET_TYPE"
              ],
              "labelField": "dictName",
              "valueField": "dictId"
            }
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "deci050108"
        }
      },
      "thead": [
        {
          "type": "index",
          "align": "center"
        },
        {
          "label": "进件单号",
          "prop": "branch_id",
          "align": "center"
        },
        {
          "label": "业务场景",
          "prop": "branch_name",
          "align": "center"
        },
        {
          "label": "客户名称",
          "prop": "branch_name",
          "align": "center"
        },
        {
          "label": "证件类型",
          "prop": "real_branch_ind",
          "align": "center",
          "dict": {
            "dictType": "MS_E_YESORNO",
            "dictKey": [
              "MS_E_YESORNO"
            ],
            "format": "label"
          }
        },
        {
          "label": "证件号码",
          "align": "center",
          "prop": "branch_address"
        },
        {
          "label": "手机号码",
          "align": "center",
          "prop": "branch_phone"
        },
        {
          "label": "产品编号",
          "align": "center",
          "prop": "branch_mail"
        },
        {
          "label": "产品名称",
          "align": "center",
          "prop": "postcode"
        },
        {
          "label": "渠道编号",
          "align": "center",
          "prop": "contacts_name"
        },
        {
          "label": "渠道名称",
          "align": "center",
          "prop": "contacts_phone"
        },
        {
          "label": "进件时间",
          "align": "center",
          "prop": "contacts_mail"
        },
        {
          "label": "结束时间",
          "align": "center",
          "prop": "contacts_mail"
        },
        {
          "label": "状态",
          "align": "center",
          "prop": "branch_function_class",
          "dict": {
            "dictType": "CT_E_BRCHTP",
            "dictKey": [
              "CT_E_BRCHTP"
            ],
            "format": "label"
          }
        },
        {
          "label": "异常原因",
          "align": "center",
          "prop": "swift_no"
        },
        {
          "label": "操作",
          "fixed": "right",
          "width": 200,
          "align": "center",
          "btns": [
            {
              "type": "look",
              "label": "进件详情",
              "toRequest": {
                "url": "/views/ismt/ct/branch/branch-info-detail.json",
                "method": "POST"
              },
              "props": {
                "title": "进件详情"
              }
            },
            {
              "type": "look",
              "label": "链路详情",
              "toRequest": {
                "url": "/views/ismt/ct/branch/branch-info-detail.json",
                "method": "POST"
              },
              "props": {
                "title": "链路详情"
              }
            }
          ]
        }
      ]
    }
  }
}