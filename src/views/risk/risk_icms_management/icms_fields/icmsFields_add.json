{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "deci050101"
            }
          }
        }
      ],
      "controls": [
        {
          "fact_scene": {
            "dict": {
              "dictKey": [
                "DECI_E_SCENE_TYPE"
              ],
              "format": "value-label",
              "dictType": "DECI_E_SCENE_TYPE"
            },
            "control": "select",
            "label": "进件场景",
            "rules": [
              {
                "message": "请选择进件场景",
                "required": true
              }
            ]
          },
          "field_group_code": {
            "dict": {
              "dictKey": [
                "DECI_E_FIELD_GROUP"
              ],
              "format": "value-label",
              "dictType": "DECI_E_FIELD_GROUP"
            },
            "control": "select",
            "label": "字段分组代码",
            "rules": [
              {
                "message": "请选择字段分组代码",
                "required": true
              }
            ]
          },
          "field_code": {
            "control": "text",
            "rules": [
              {
                "message": "请输入字段名",
                "required": true
              }
            ],
            "label": "字段名"
          },
          "field_name": {
            "control": "text",
            "rules": [
              {
                "message": "请输入字段中文名",
                "required": true
              }
            ],
            "label": "字段中文名"
          },
          "description": {
            "control": "text",
            "label": "字段描述"
          },
          "data_type": {
            "dict": {
              "dictKey": [
                "DECI_E_DATA_TYPE"
              ],
              "format": "value-label",
              "dictType": "DECI_E_DATA_TYPE"
            },
            "control": "select",
            "label": "字段类型",
            "rules": [
              {
                "message": "请选择字段类型",
                "required": true
              }
            ]
          },
          "value_precision": {
            "control": "text",
            "label": "精度"
          },
          "is_enumeration": {
            "dict": {
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "dictType": "MS_E_YESORNO"
            },
            "control": "select",
            "showItem": {
              "1": [
                "const_code"
              ]
            },
            "label": "是否存在枚举"
          },
          "const_code": {
            "control": "lookup",
            "label": "常量代码",
            "keyField": {
              "const_code": "const_code"
            },
            "toRequest": {
              "url": "/views/risk/risk_rule_constant/risk_rule_constant_lookup.json",
              "method": "post",
              "params": {
                "org_id": "${{params.org_id}}"
              }
            }
          }
        }
      ]
    }
  }
}