{
	"module": "v-search-btn-datagrid",
	"layout": {
		"datagrid": {
			"isInitSearch": true,
			"doRequest": {
				"url": "/API/risk-management-service/risk-memo-information/page-select",
				"method": "post",
				"params": {
					"memoOwnerId": "${{params.memoOwnerId}}"
				}
			},
			"thead": [
				{
					"label": "备注信息ID",
					"prop": "memoId",
					"align": "center"
				},
				{
					"label": "备注日期",
					"prop": "memoDate",
					"align": "center"
				},
				{
					"label": "备注内容",
					"prop": "memoRemark",
					"align": "center"
				},
				{
					"label": "创建时间",
					"prop": "createTime",
					"align": "center",
					"type": "dateTimePicker",
					"format": "yyyy-MM-dd HH:mm:ss"
				},
				{
					"label": "创建人",
					"prop": "createUser",
					"align": "center"
				}
			]
		}
	}
}