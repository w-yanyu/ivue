{
  "module": "v-search-btn-datagrid",
  "layout": {
    "datagrid": {
      "doRequest": {
        "url": "/API/risk-management-service/risk-list-batch/page-select",
        "method": "post"
      },
      "pagination": {
        "pageSizes": [
          5,
          10,
          20,
          50,
          100
        ],
        "pageSize": 5
      },
      "thead": [
        {
          "label": "批次id",
          "prop": "taskId",
          "width": "200px",
          "align": "center"
        },
        {
          "label": "来源文件",
          "prop": "sourceFile"
        },
        {
          "label": "批量导入日期",
          "prop": "importDate"
        },
        {
          "label": "操作",
          "width": "70px",
          "align": "center",
          "btns": [
            {
              "type": "bringback",
              "label": "选择"
            }
          ]
        }
      ]
    }
  }
}
