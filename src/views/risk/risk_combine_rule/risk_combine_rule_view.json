{
  "module": "v-realonlyForm-btn",
  "layout": {
    "form": {
      "controls": [
        {
          "cruleId": {
            "label": "规则id",
            "control": "string",
            "span": 12
          },
          "cruleName": {
            "label": "规则名称",
            "control": "string",
            "span": 12
          }
        },
        {
          "cruleCode": {
            "label": "规则代码",
            "control": "string",
            "span": 12
          },
          "cruleVersion": {
            "label": "规则版本",
            "control": "string",
            "span": 12
          }
        },
        {
          "cruleType": {
            "label": "规则类型",
            "control": "string",
            "span": 12
          },
          "natureExpression": {
            "label": "规则自然语言表达式",
            "control": "string",
            "span": 12
          }
        },
        {
          "updateUser": {
            "label": "最后更新人",
            "control": "string",
            "span": 12
          },
          "updateTime": {
            "label": "最后更新时间",
            "control": "string",
            "type": "dateTimePicker",
            "format": "yyyy-MM-dd HH:mm:ss",
            "span": 12
          }
        },
        {
          "executableExpression": {
            "label": "规则可执行表达式",
            "control": "string",
            "span": 24
          }
        },
        {
          "description": {
            "label": "备注",
            "control": "string",
            "span": 24
          }
        }
      ],
      "initRequest": "${{params}}"
    }
  }
}
