{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "controls": [
        {
          "cruleId": {
            "label": "规则id",
            "control": "string",
            "span": 12
          },
          "cruleName": {
            "label": "规则名称",
            "control": "text",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "该控件必填"
              }
            ]
          }
        },
        {
          "cruleCode": {
            "label": "规则代码",
            "control": "text",
            "span": 12,
            "disabled": true,
            "rules": [
              {
                "required": true,
                "message": "该控件必填"
              }
            ]
          },
          "cruleVersion": {
            "label": "规则版本",
            "control": "inputNumber",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请输入正整数",
                "type":"integer",
                "min": 0
              }
            ]
          }
        },
        {
          "cruleType": {
            "label": "规则类型",
            "control": "select",
            "span": 12,
            "disabled": true,
            "options": [{
                "label": "TR-实时决策规则",
                "value": "TR"
              },
              {
                "label": "RR-实时指标加工规则 ",
                "value": "RR"
              }
            ],
            "rules": [
              {
                "required": true,
                "message": "该控件必填"
              }
            ]
          },
          "natureExpression": {
            "label": "规则自然语言表达式",
            "control": "text",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "该控件必填"
              }
            ]
          }
        },
        {
          "description": {
            "label": "备注",
            "control": "textarea",
            "span": 24
          }
        }
      ],
      "initRequest": "${{params}}",
      "btns": [{}]
    }
  }
}
