{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "controls": [{
          "cruleName": {
            "label": "规则名称",
            "control": "text",
            "span": 12,
            "rules": [{
              "required": true,
              "message": "请正确输入"
            }]
          }
        },
        {
          "cruleCode": {
            "label": "规则代码",
            "control": "text",
            "span": 12,
            "rules": [{
              "required": true,
              "message": "请正确输入"
            }]
          }
        },
        {
          "cruleType": {
            "label": "规则类型",
            "control": "select",
            "span": 12,
            "value": "TR",
            "options": [{
                "label": "TR-实时决策规则",
                "value": "TR"
              },
              {
                "label": "RR-实时指标加工规则 ",
                "value": "RR"
              }
            ],
            "rules": [{
              "required": true,
              "message": "该控件必填"
            }]
          }
        },
        {
          "description": {
            "label": "备注",
            "control": "textarea",
            "span": 24
          }
        }
      ],
      "initRequest": "${{params}}",
      "btns": [{}]
    }
  }
}
