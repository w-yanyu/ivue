{
    "module": "v-form-btn-dynamic",
    "layout": {
        "form": {
            "ref": "search",
            "formSetTitle": "筛选条件",
            "formResultTitle": "筛选结果",
            "controls": [
                {
                    "operatorId": {
                        "label": "用户",
                        "control": "select",
                        "span": 8,
                        "dict": {
                            "dictType": "operatorIdSource",
                            "dictKey": [],
                            "format": "value-label",
                            "valueField": "operatorId",
                            "labelField": "operatorName",
                            "doRequest": {
                                "url": "/API/risk-management-service/risk-queue-operator/queryOperator",
                                "method": "post"
                            }
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "该控件必填"
                            }
                        ]
                    },
                    "startAlertDate": {
                        "control": "dateTimePicker",
                        "label": "处理开始日期",
                        "format": "yyyyMMdd",
                        "span": 8,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入开始日期"
                            }
                        ]
                    },
                    "endAlertDate": {
                        "control": "dateTimePicker",
                        "label": "处理结束日期",
                        "format": "yyyyMMdd",
                        "span": 8,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入结束日期"
                            }
                        ]
                    }
                }
            ]
        },
        "toolbar": {
            "btns": [
                {
                    "type": "submit",
                    "label": "查询",
                    "doRequest": {
                        "url": "/views/risk/risk_alert_deal/responce.json",
                        "method": "get"
                    },
                    "toRequest": {
                        "url": "/views/risk/chart_list/user_chart_list_view.json",
                        "method": "get"
                    },
                    "props": {}
                },
                {
                  "type": "export",
                  "label": "导出",
                  "doRequest": {
                    "url": "/API/risk-management-service/risk-queue-operator/downloadReport",
                    "method":"post"
                  }
                },
                {
                    "type": "reset",
                    "btnStyle": "success",
                    "label": "清空"
                }
            ]
        }
    }
}