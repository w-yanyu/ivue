{
    "module": "v-form-btn-dynamic",
    "layout": {
        "form": {
            "ref": "search",
            "formSetTitle": "筛选条件",
            "formResultTitle": "筛选结果",
            "controls": [
                {
                    "channelId": {
                        "label": "渠道",
                        "control": "select",
                        "span": 8,
                        "format": "value-label",
                        "options": [
                            {
                                "label": "运通",
                                "value": "AMX"
                            },
                            {
                                "label": "银联",
                                "value": "CUP"
                            },
                            {
                                "label": "VSS",
                                "value": "VSS"
                            },
                            {
                                "label": "JCB",
                                "value": "JCB"
                            },
                            {
                                "label": "MCD",
                                "value": "MCD"
                            }
                        ],
                        "rules": [
                            {
                                "required": true,
                                "message": "该控件必填"
                            }
                        ]
                    },
                    "startAlertDate": {
                        "control": "dateTimePicker",
                        "label": "处理结束日期",
                        "format": "yyyyMMdd",
                        "span": 8,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入开始时间"
                            }
                        ]
                    },
                    "endAlertDate": {
                        "control": "dateTimePicker",
                        "label": "处理结束日期",
                        "format": "yyyyMMdd",
                        "span": 8,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入结束时间"
                            }
                        ]
                    }
                }
            ]
        },
        "toolbar": {
            "btns": [
                {
                    "type": "submit",
                    "label": "查询",
                    "doRequest": {
                        "url": "/views/risk/risk_alert_deal/responce.json",
                        "method": "get"
                    },
                    "toRequest": {
                        "url": "/views/risk/chart_list/channel_chart_list_view.json",
                        "method": "get"
                    },
                    "props": {}
                },
                {
                    "type": "submit",
                    "btnStyle": "success",
                    "label": "生成表格",
                    "doRequest": {
                        "url": "/API/risk-management-service/risk-decision-message/createReport",
                        "method": "post",
                        "params": {
                            "channelId": "CUP",
                            "startAlertDate": "20210201",
                            "endAlertDate": "20210301"
                        }
                    }
                },
                {
                    "type": "export",
                    "label": "下载",
                    "doRequest": {
                        "url": "/API/risk-management-service/risk-decision-message/downloadReport",
                        "method": "post"
                    }
                },
                {
                    "type": "submit",
                    "label": "删除",
                    "doRequest": {
                        "url": "/API/risk-management-service/risk-decision-message/deleteChart",
                        "method": "post"
                    }
                },
                {
                    "type": "reset",
                    "btnStyle": "success",
                    "label": "清空"
                }
            ]
        }
    }
}