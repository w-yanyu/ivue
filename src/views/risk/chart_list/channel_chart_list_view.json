{
  "module": "v-charts",
  "layout": {
    "rows": [
      [
        {
          "span": 12,
          "type": "ve-histogram",
          "props": {
            "settings": {},
            "data": {
              "columns": [
                "tranDate",
                "empty",
                "counMccLimitAmountDA",
                "delay",
                "limitAmount00",
                "limitAmountD4",
                "limitAmountD5",
                "limitCount00",
                "limitCountD6",
                "merLimitAmountD9",
                "merWarn00",
                "refuse",
                "refuseTrxn",
                "refuse_pass",
                "temp",
                "usaMer",
                "warn"
              ],
              "rows": {
                "initRequest": {
                  "url": "/API/risk-management-service/risk-decision-message/decisionMessageChartByHistogram",
                  "method": "post",
                  "params": {
                    "channelId": "${{parent.form.channelId}}",
                    "startAlertDate": "${{parent.form.startAlertDate}}",
                    "endAlertDate": "${{parent.form.endAlertDate}}"
                  }
                }
              }
            }
          }
        },
        {
          "span": 12,
          "type": "ve-line",
          "props": {
            "settings": {
              "axisSite": { "left": ["empty","counMccLimitAmountDA","delay","limitAmount00",
                "limitAmountD4","limitAmountD5","limitCount00","limitCountD6","merLimitAmountD9","merWarn00",
                "refuse","refuseTrxn","refuse_pass","temp","usaMer","warn"] },
              "yAxisType": ["percent"],
              "yAxisName": ["rate"],
              "opacity": 0.5
          },
          "data": {
            "columns": [
              "tranDate",
              "empty",
              "counMccLimitAmountDA",
              "delay",
              "limitAmount00",
              "limitAmountD4",
              "limitAmountD5",
              "limitCount00",
              "limitCountD6",
              "merLimitAmountD9",
              "merWarn00",
              "refuse",
              "refuseTrxn",
              "refuse_pass",
              "temp",
              "usaMer",
              "warn"
            ],
              "rows": {
                "initRequest": {
                  "url": "/API/risk-management-service/risk-decision-message/decisionMessageChartByLine",
                  "method": "post",
                  "params": {
                    "channelId": "${{parent.form.channelId}}",
                    "startAlertDate": "${{parent.form.startAlertDate}}",
                    "endAlertDate": "${{parent.form.endAlertDate}}"
                  }
                }
              }
            }
          }
        }
      ]
    ]
  }
}
      
    
  
