{
    "module": "v_cardList4",
    "layout": {
        "card": {
            "span": {
                "xs": 24,
                "sm": 24,
                "md": 24,
                "lg": 24,
                "xl": 24
            },
            "list": [
                {
                    "label": "警报详细信息",
                    "toRequest": {
                        "url": "/views/risk/risk_alert_information/risk_alert_information_view.json",
                        "method": "get",
                        "params": {
                            "alertId": "${{params.alertId}}"
                        }
                    },
                    "params": {},
                    "props": {}
                },
                {
                    "label": "警报备注信息",
                    "toRequest": {
                        "url": "/views/risk/risk_memo_information/risk_memo_information_view.json",
                        "method": "get",
                        "params": {
                            "memoOwnerId": "${{params.alertId}}"
                        }
                    },
                    "props": {},
                    "params": {}
                },
                {
                    "label": "警报调查信息",
                    "toRequest": {
                        "url": "/views/risk/risk_invest_information/risk_invest_information_view.json",
                        "method": "get",
                        "params": {
                            "investOwnerId": "${{params.alertId}}"
                        }
                    },
                    "params": {},
                    "props": {}
                },
                {
                    "label": "警报审核信息",
                    "toRequest": {
                        "url": "/views/risk/risk_alert_review/risk_alert_review_view.json",
                        "method": "get",
                        "params": {
                            "alertId": "${{params.alertId}}"
                        }
                    },
                    "params": {},
                    "props": {}
                },
                {
                    "label": "警报处理",
                    "toRequest": {
                        "url": "/views/risk/risk_alert_deal/risk_alert_deal_deal.json",
                        "method": "get",
                        "params": {
                            "alertId": "${{params.alertId}}"
                        }
                    },
                    "params": {},
                    "props": {}
                }
            ]
        }
    }
}