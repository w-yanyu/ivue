{
  "module": "dynamicTabs",
  "layout": {
    "tabs": {
      "scope": "templateScope",
      "tabHeader": [
        {
          "id": "baseInfo",
          "label": "警报处理",
          "closable": false,
          "disabled": false,
          "icon": "el-icon-date",
          "toRequest": {
            "url": "/views/risk/risk_alert_deal/risk_alert_deal_detail.json",
            "params": {
              "idField": {
                "alertId": "${{params.alertId}}"
              }
            }
          },
          "params": {},
          "props": {}
        },
        {
          "id": "resetPwd",
          "label": "交易信息",
          "icon": "el-icon-date",
          "closable": true,
          "disabled": false,
          "toRequest": {
            "url": "/views/risk/risk_alert_assign/risk_alert_assign_trxn.json",
            "params": {
              "trxnSeq": "${{params.alertTrxnSeq}}"
            }
          },
          "params": {},
          "props": {}
        }
      ],
      "defaultActiveKey": "baseInfo",
      "position": "top",
      "styleClass": "card"
    }
  }
}