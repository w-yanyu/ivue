{
  "module": "v-search-btn-datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "eruleId": {
            "label": "规则id",
            "control": "text",
            "span": 12,
            "rules": [
              {
                "message": "字段长度超限",
                "max": 8
              }
            ]
          },
          "eruleName": {
            "label": "规则名称",
            "control": "text",
            "span": 12,
            "rules": [
              {
                "message": "字段长度超限",
                "max": 100
              }
            ]
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "bringback",
          "label": "选择"
        }
      ]
    },
    "datagrid": {
      "doRequest": {
        "url": "/API/risk-management-service/risk-executable-rule/page-select",
        "method": "post"
      },
      "pagination": {
        "pageSizes": [
          5,
          10,
          20,
          50,
          100
        ],
        "pageSize": 5
      },
      "thead": [
        {
          "type": "selection",
          "width": "55"
        },
        {
          "label": "规则id",
          "prop": "eruleId",
          "width": "100px",
          "align": "center"
        },
        {
          "label": "规则名称",
          "prop": "eruleName"

        },
        {
          "label": "操作",
          "width": "70px",
          "btns": [
            {
              "type": "bringback",
              "label": "选择"
            }
          ],
          "align": "center"
        }
      ]
    }
  }
}
