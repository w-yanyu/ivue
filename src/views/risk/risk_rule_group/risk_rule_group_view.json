{
  "module": "v-form-editableDataGrid-btn",
  "init":{
    "initRequest": {
      "url": "/API/risk-management-service/risk-rule-group/query",
      "method": "post"
    }
  },
  "layout": {
    "form": {
      "controls": [
        {
          "groupId": {
            "label": "规则组id",
            "control": "string",
            "span": 12
          },
          "groupName": {
            "label": "规则组名称",
            "control": "string",
            "span": 12
          }
        }
      ]
    },
    "datagrid": {
      "pagination": {
        "show": false
      },
      "name":"allData",
      "thead": [
        {
          "label": "可执行规则ID",
          "prop": "eruleId",
          "type": "string"
        },
        {
          "label": "可执行规则名称",
          "prop": "eruleName",
          "type": "string"
        }
      ]
    }
  }
}
