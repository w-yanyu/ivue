{
  "module": "v-form-editableDataGrid-btn",
  "init": {
    "initRequest": "${{params}}"
  },
  "layout": {
    "datagrid": {
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "const_code": "${{params.const_code}}",
          "servicecode": "deci010206",
          "isList": true
        }
      },
      "pagination": {
        "show": false
      },
      "thead": [
        {
          "prop": "const_value",
          "label": "常量值",
          "type": "string"
        },
        {
          "label": "描述",
          "prop": "description",
          "type": "string"
        }
      ]
    }
  }
}
