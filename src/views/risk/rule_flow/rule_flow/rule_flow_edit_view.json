{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "btns":{

      },
      "controls": [
        {
          "flowCode": {
            "label": "规则流代码",
            "control": "text",
            "span": 12,
            "disabled": true,
            "rules": [
              {
                "required": true,
                "message": "请正确输入",
                "max": 64
              },
              {
                "required": true,
                "pattern": "^[A-Za-z][A-Za-z\\d_]{0,65}$",
                "message": "以首字母为大小写或加数字组合"
              }
            ]
          },
          "flowName": {
            "label": "规则流名称",
            "control": "text",
            "span": 12,
            "disabled": true,
            "rules": [
              {
                "required": true,
                "message": "请正确输入",
                "max": 128
              }
            ]
          }
        },
        {
          "currVersion": {
            "label": "版本",
            "control": "text",
            "span": 12,
            "disabled": true,
            "rules": [
              {
                "required": true,
                "message": "请正确输入",
                "max": 128
              }
            ]
          },
          "flowDescription": {
            "label": "备注",
            "control": "textarea",
            "span": 24,
            "disabled": true,
            "rules": [
              {
                "required": false,
                "message": "请正确输入",
                "max": 2000
              }
            ]
          },
          "flowId": {
            "label": "规则流ID",
            "control": "hidden",
            "span": 12
          }
        }
      ],
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "deci010708",
          "is_add":"0"
        }
      }
    }
  }
}
