{
  "module": "v_search_btn_datagrid",
  "pageId": "rule_flow",
  "layout": {
    "toolbar": {
    },
    "datagrid": {
      "scope": "searchTable",
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "deci010712",
          "flowCode": "${{params.flowCode}}"
        }
      },
      "pagination": {
        "show": true,
        "pageSize": 10
      },
      "thead": [
        {
          "type": "radio",
          "align": "center",
          "label" : "选择"
        },
        {
          "label": "规则流代码",
          "prop": "flowCode"
        },
        {
          "label": "规则流名称",
          "prop": "flowName"
        },
        {
          "label": "版本号",
          "prop": "currVersion"
        },
        {
          "label": "规则流描述",
          "prop": "flowDescription"
        }
      ]
    }
  }
}