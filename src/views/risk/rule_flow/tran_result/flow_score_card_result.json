{
  "module": "v-search-btn-datagrid",
  "layout": {
    "form": {
      "btns": [],
      "controls": []
    },
    "datagrid": {
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/riskController/riskCall",
        "method": "POST",
        "params": {
          "trxnSeq": "${{params.trxnSeq}}",
          "servicecode": "risk-management-service/risk-score-card-result/page-select",
          "appId": "risk-management-service",
          "isList": true
        }
      },
      "pagination": {
        "hideOnSinglePage": false,
        "show": true,
        "pageSizes": [10, 15, 20, 50, 100],
        "pageSize": 10
      },
      "thead": [
        {
          "label": "流水号",
          "prop": "trxnSeq"
        },
        {
          "label": "评分卡代码",
          "prop": "scoreCardCode"
        },
        {
          "label": "评分卡名称",
          "prop": "scoreCardName"
        },
        {
          "label": "操作",
          "width": "150px",
          "align": "center",
          "btns": [
            {
              "type": "look",
              "label": "查看",
              "toRequest": {
                "url": "@riskScoreCardResulrView",
                "method": "get"
              },
              "props": {
                "width": "70%"
              }
            }
          ]
        }
      ]
    }
  }
}
