{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "controls": [
        {
          "flow_code": {
            "label": "规则流代码",
            "control": "text",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请正确输入",
                "max": 64
              },
              {
                "required": true,
                "pattern": "^[A-Za-z][A-Za-z\\d_]{0,65}$",
                "message": "以首字母为大小写或加数字组合"
              }
            ]
          },
          "flow_name": {
            "label": "规则流名称",
            "control": "text",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请正确输入",
                "max": 128
              }
            ]
          }
        },
        {
          "flow_description": {
            "label": "规则流描述",
            "control": "textarea",
            "span": 24,
            "rows": 5,
            "showWordLimit": 500,
            "maxlength": 500,
            "rules": [
              {
                "required": false,
                "message": "请正确输入",
                "max": 500
              }
            ]
          }
        }
      ],
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "deci010701"
        }
      }
    }
  }
}