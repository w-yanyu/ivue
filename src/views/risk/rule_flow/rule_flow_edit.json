{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "####Request": "${{params}}",
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "deci010704",
          "flow_code": "${{params.flowCode}}"
        }
      },
      "btns": [],
      "controls": [
        {
          "flowCode": {
            "label": "规则流代码",
            "control": "text",
            "width": "auto",
            "span": 12,
            "disabled": true,
            "rules": [
              {
                "required": true,
                "message": "请正确输入",
                "max": 64
              },
              {
                "required": true,
                "pattern": "^[A-Za-z][A-Za-z\\d_]{0,65}$",
                "message": "以首字母为大小写或加数字组合"
              }
            ]
          },
          "flowName": {
            "label": "规则流名称",
            "control": "text",
            "width": "auto",
            "span": 12,
            "disabled": true,
            "rules": [
              {
                "required": true,
                "message": "请正确输入"
              }
            ]
          }
        },
        {
          "currVersion": {
            "label": "版本号",
            "control": "text",
            "width": "auto",
            "span": 12,
            "disabled": true,
            "rules": [
              {
                "required": false,
                "message": "请正确输入",
                "max": 2000
              }
            ]
          },
          "branchId": {
            "label": "机构名称",
            "width": "auto",
            "disabled": true,
            "control": "select",
            "dict": {
              "dictType": "DECI_E_BRANCH_MARK",
              "dictKey": [
                "DECI_E_BRANCH_MARK"
              ],
              "format": "label"
            },
            "span": 12
          }
        },
        {
          "applyTime": {
            "disabled": true,
            "label": "最新维护时间",
            "control": "text",
            "width": "auto",
            "span": 12
          }
        },
        {
          "flow_description": {
            "label": "备注",
            "control": "textarea",
            "width": "auto",
            "span": 24,
            "disabled": true,
            "rules": [
              {
                "required": false,
                "message": "请正确输入",
                "max": 2000
              }
            ]
          }
        }
      ]
    }
  }
}
