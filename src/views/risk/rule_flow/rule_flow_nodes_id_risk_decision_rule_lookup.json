{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "doRequest": {
        "url": "/API/risk-management-service/risk-decision-rule/page-select",
        "method": "post"
      },
      "pagination": {
        "hideOnSinglePage":false,
        "show": true,
        "pageSizes": [
          10,
          15,
          20,
          50,
          100
        ],
        "pageSize": 10
      },
      "thead": [
        {
          "label": "普通规则节点ID",
          "prop": "ruleId"
        },
        {
         "label": "普通规则代码",
         "prop": "ruleCode"
        },
        {
         "label": "普通规则名称",
         "prop": "ruleName"
        },
       
       {
         "label": "操作",
         "width": "150px",
         "btns": [
           {
             "type": "bringback",
             "label": "选择"
           }
         ]
       }
      ]
    }
  }
}
