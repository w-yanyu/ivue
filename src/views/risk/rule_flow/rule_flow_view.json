{
  "module": "v-realonlyForm-btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "controls": [
        
        {
          "flowId": {
            "label": "规则流ID",
            "control": "string",
            "span": 8
          },
          "flowCode": {
            "label": "规则流代码",
            "control": "text",
            "span": 8,
            "disabled":true
          },
          "flowName": {
            "label": "规则流名称",
            "control": "text",
            "span": 8,
            "disabled":true
          }
        },
        {
          "createUser": {
            "label": "创建人",
            "control": "string",
            "span": 12,
            "disabled": true
          },
          "createTime": {
            "label": "创建时间",
            "control": "string",
            "type": "dateTimePicker",
            "span": 12,
            "format": "yyyy-MM-dd HH:mm:ss",
            "disabled": true
          }
        },
        {
          "updateUser": {
            "label": "最后修改人",
            "control": "string",
            "span": 12
          },
          "updateTime": {
            "label": "最后修改时间",
            "control": "string",
            "type": "dateTimePicker",
            "span": 12,
            "format": "yyyy-MM-dd HH:mm:ss"
          }
        },
        {
          "description": {
            "label": "规则流描述",
            "control": "textarea",
            "span": 24,
            "rows": 5,
            "maxlength": 500,
            "showWordLimit": 500,
            "disabled":true
          }
        }
      ]
    }
  }
}