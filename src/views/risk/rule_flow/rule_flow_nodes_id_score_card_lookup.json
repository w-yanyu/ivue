{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "doRequest": {
        "url": "/API/risk-management-service/risk-score-card/page-select",
        "method": "post",
        "params":{
          "scoreCardStatus":"Y"
        }
      },
      "pagination": {
        "hideOnSinglePage":false,
        "show": true,
        "pageSizes": [
          10,
          15,
          20,
          50,
          100
        ],
        "pageSize": 10
      },
      "thead": [
        {
          "label": "评分卡节点ID",
          "prop": "scoreCardId"
        },
        {
         "label": "评分卡代码",
         "prop": "scoreCardCode"
        },
        {
         "label": "评分卡名称",
         "prop": "scoreCardName"
        },
       {
         "label": "操作",
         "width": "150px",
         "btns": [
           {
             "type": "bringback",
             "label": "选择"
           }
         ]
       }
      ]
    }
  }
}
