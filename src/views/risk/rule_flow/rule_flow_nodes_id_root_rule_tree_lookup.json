{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "doRequest": {
        "url": "/API/risk-management-service/root-rule-tree/page-select",
        "method": "post"
      },
      "pagination": {
        "hideOnSinglePage":false,
        "show": true,
        "pageSizes": [
          10,
          15,
          20,
          50,
          100
        ],
        "pageSize": 10
      },
      "thead": [
        {
          "label": "规则树节点ID",
          "prop": "treeId"
        },
        {
         "label": "规则树代码",
         "prop": "treeCode"
        },
        {
         "label": "规则树名称",
         "prop": "treeName"
        },
      
       {
         "label": "操作",
         "width": "150px",
         "btns": [
           {
             "type": "bringback",
             "label": "选择"
           }
         ]
       }
      ]
    }
  }
}
