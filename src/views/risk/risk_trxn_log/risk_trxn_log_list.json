{
  "module": "v-search-btn-datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "trxnSeq": {
            "label": "交易流水",
            "control": "text",
            "span": 8
          },
          "customerId": {
            "label": "客户号",
            "control": "text",
            "span": 8,
            "rules": [
              {
                "required": true,
                "message": "请正确输入",
                "max": 40
              }
            ]
          },
          "cardNumber": {
            "label": "卡号",
            "control": "text",
            "span": 8
          }
        },
        {
          "tranDate": {
            "label": "交易日期",
            "control": "text",
            "span": 8
          },
          "trxnCode": {
            "label": "交易代码",
            "control": "text",
            "span": 8
          }
        }
      ]
    },
    "toolbar": {
      "btns": []
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/API/risk-management-service/risk-trxn-log/page-select",
        "method": "POST"
      },
      "thead": [
        {
          "label": "交易流水",
          "prop": "trxnSeq"
        },
        {
          "label": "交易代码",
          "prop": "trxnCode"
        },
        {
          "label": "交易名称",
          "prop": "trxnName"
        },
        {
          "label": "交易渠道",
          "prop": "channelId"
        },
        {
          "label": "交易日期",
          "prop": "tranDate"
        },
        {
          "label": "交易时间",
          "prop": "tranTime"
        },
        {
          "label": "卡号",
          "prop": "cardNumber"
        },
        {
          "label": "客户号",
          "prop": "customerId"
        },
        {
          "label": "交易金额",
          "prop": "tranAmount"
        },
        {
          "label": "等价人民币金额",
          "prop": "rmbAmount"
        },
        {
          "label": "操作",
          "btns": [
            {
              "type": "look",
              "label": "查看",
              "toRequest": {
                "url": "/views/risk/risk_trxn_log/risk_trxn_log_view.json",
                "method": "get"
              },
              "props": {
                "width": "70%"
              }
            }
          ]
        }
      ],
      "pagination": {
        "show": true,
        "pageSizes": [
          "10",
          "50",
          "100",
          "200"
        ],
        "pageSize": 10
      }
    }
  }
}
