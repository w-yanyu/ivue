{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "createUser": "$g{{user_info.userCode}}",
          "updateUser": "$g{{user_info.userCode}}",
          "servicecode": "deci090202"
        }
      }, 
      "initRequest": "${{params}}",
      "controls": [
        {
          "systemName": {
            "label": "系统名称",
            "width": "auto",
            "control": "text",
            "span": 24,
            "disabled":"true",
            "rules": [
              {
                "required": true,
                "message":"请输入系统名称"
              }
            ]
          }
        },
        {
          "systemId": {
            "label": "系统编号标识",
            "disabled":"true",
            "width": "auto",
            "control": "text",
            "span": 24,
            "rules": [
              {
                "required": true,
                "message":"请输入系统编号标识"
              }
            ]
          }
        },
        {
          "systemBranchId": {
            "label": "系统所属机构",
            "disabled":"true",
            "control": "select",
            "span": 24,
            "rules": [
              {
                "required": true,
                "message":"请选择系统所属机构"
              }
            ],
            "width": "auto",
            "dict": {
              "dictType": "DECI_E_BRANCH_MARK2",
              "dictKey": [
                "DECI_E_BRANCH_MARK2"
              ],
              "format": "label"
            }
          }
        },
        {
          "remark": {
            "label": "备注",
            "width": "auto",
            "control": "textarea",
            "span": 24,
            "maxlength": 200,
            "showWordLimit": 200,
            "rows": 5,
            "rules": [
              {
                "message": "请正确输入",
                "max": 200
              }
            ]
          }
        }
      ]
    }
  }
}