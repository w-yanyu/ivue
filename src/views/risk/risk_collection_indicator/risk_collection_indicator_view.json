{
  "module": "v-realonlyForm-btn",
  "layout": {
    "form": {
      "controls": [
        {
          "collectionIndicatorCode": {
            "label": "集合指标代码",
            "control": "string",
            "span": 12
          },
          "collectionIndicatorName": {
            "label": "集合指标名称",
            "control": "string",
            "span": 12
          }
        },
        {
          "factCode": {
            "label": "事实代码",
            "control": "string",
            "span": 12
          },
          "fieldCode": {
            "label": "事实属性代码",
            "control": "string",
            "span": 12
          }
        },
        {
          "relation": {
            "label": "关系",
            "control": "string",
            "span": 12,
            "dict": {
              "dictType": "relation",
              "dictKey": [],
              "format": "value-label",
              "doRequest": {
                "url": "/views/risk/dictdata/relation.json",
                "method": "get"
              }
            }
          },
          "type": {
            "label": "类型",
            "control": "string",
            "span": 12,
            "dict": {
              "dictType": "rciType",
              "dictKey": [],
              "format": "value-label",
              "doRequest": {
                "url": "/views/risk/dictdata/rciType.json",
                "method": "get"
              }
            }
          }
        },
        {
          "collectionSource": {
            "label": "集合指标数据来源",
            "control": "string",
            "span": 12,
            "dict": {
              "dictType": "collectionSource",
              "dictKey": [],
              "format": "value-label",
              "doRequest": {
                "url": "/views/risk/dictdata/collectionSource.json",
                "method": "get"
              }
            }
          },
          "collectionCode": {
            "label": "集合字段代码",
            "control": "string",
            "span": 12
          }
        },
        {
          "collectionName": {
            "label": "集合字段名称",
            "control": "string",
            "span": 12
          }
        },
        {
          "createTime": {
            "label": "创建时间",
            "control": "string",
            "type": "dateTimePicker",
            "format": "yyyy-MM-dd HH:mm:ss",
            "span": 12
          },
          "createUser": {
            "label": "创建人",
            "control": "string",
            "span": 12
          }
        },
        {
          "updateTime": {
            "label": "最后修改时间",
            "control": "string",
            "type": "dateTimePicker",
            "format": "yyyy-MM-dd HH:mm:ss",
            "span": 12
          },
          "updateUser": {
            "label": "最后修改人",
            "control": "string",
            "span": 12
          }
        }
      ],
      "initRequest": "${{params}}"
    }
  }
}
