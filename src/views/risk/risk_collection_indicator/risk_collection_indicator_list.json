{
  "module": "v-search-btn-datagrid",
  "pageId": "risk_collection_indicator",
  "layout": {
    "form": {
      "controls": [
        {
          "collectionIndicatorCode": {
            "label": "集合指标代码",
            "control": "text",
            "span": 8,
            "rules": [
              {
                "message": "字段长度超限",
                "max": 40
              }
            ]
          },
          "collectionIndicatorName": {
            "label": "集合指标名称",
            "control": "text",
            "span": 8,
            "rules": [
              {
                "message": "字段长度超限",
                "max": 100
              }
            ]
          },
          "collectionSource": {
            "label": "集合指标数据来源",
            "control": "select",
            "span": 8,
            "dict": {
              "dictType": "collectionSource",
              "dictKey": [],
              "format": "value-label",
              "doRequest": {
                "url": "/views/risk/dictdata/collectionSource.json",
                "method": "get"
              }
            },
            "rules": [
              {
                "message": "字段长度超限",
                "max": 10
              }
            ]
          }
        },
        {
          "collectionCode": {
            "label": "集合字段代码",
            "control": "text",
            "span": 8,
            "rules": [
              {
                "message": "字段长度超限",
                "max": 40
              }
            ]
          },
          "collectionName": {
            "label": "集合字段名称",
            "control": "text",
            "span": 8,
            "rules": [
              {
                "message": "字段长度超限",
                "max": 100
              }
            ]
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "authId": "collection_indicator_add_btn",
          "label": "新增",
          "toRequest": {
            "url": "/views/risk/risk_collection_indicator/risk_collection_indicator_add.json",
            "method": "get",
            "params": {}
          },
          "props": {
            "width": "70%",
            "title": "新增"
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/API/risk-management-service/risk-collection-indicator/page-select",
        "method": "post"
      },
      "thead": [
        {
          "label": "集合指标代码",
          "prop": "collectionIndicatorCode"
        },
        {
          "label": "集合指标名称",
          "prop": "collectionIndicatorName"
        },
        {
          "label": "事实代码",
          "prop": "factCode"
        },
        {
          "label": "事实属性代码",
          "prop": "fieldCode"
        },
        {
          "label": "集合指标数据来源",
          "prop": "collectionSource",
          "dict": {
            "dictType": "collectionSource",
            "dictKey": [],
            "doRequest": {
              "url": "/views/risk/dictdata/collectionSource.json",
              "method": "get"
            }
          }
        },
        {
          "label": "集合字段代码",
          "prop": "collectionCode"
        },
        {
          "label": "集合字段名称",
          "prop": "collectionName"
        },
        {
          "label": "操作",
          "width": "150px",
          "btns": [
            {
              "type": "edit",
              "authId": "collection_indicator_upd_btn",
              "label": "编缉",
              "toRequest": {
                "url": "/views/risk/risk_collection_indicator/risk_collection_indicator_edit.json",
                "method": "get"
              },
              "props": {
                "width": "70%"
              }
            },
            {
              "type": "look",
              "label": "查看",
              "toRequest": {
                "url": "/views/risk/risk_collection_indicator/risk_collection_indicator_view.json",
                "method": "get"
              },
              "props": {
                "width": "70%"
              }
            },
            {
              "type": "delete",
              "authId": "collection_indicator_del_btn",
              "label": "删除",
              "doRequest": {
                "url": "/API/risk-management-service/risk-collection-indicator/delete",
                "method": "post"
              }
            }
          ]
        }
      ]
    }
  }
}
