{
  "module": "v-search-btn-datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "factCode": {
            "label": "事实代码",
            "control": "select",
            "span": 12,
            "dict": {
              "dictType": "factCode",
              "dictKey": [],
              "format": "value-label",
              "doRequest": {
                "url": "/API/risk-management-service/combobox-select/fact-without-field",
                "method": "post"
              }
            },
            "rules": [
              {
                "required": true,
                "message": "该控件必填"
              }
            ]
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/API/risk-management-service/risk-fact/page-select",
        "method": "post",
        "params":{
        }
      },
      "pagination": {
        "pageSizes": [
          5,
          10,
          20,
          50,
          100
        ],
        "pageSize": 5
      },
      "thead": [
        {
          "label": "事实属性代码",
          "prop": "fieldCode",
          "width": "200px",
          "align": "center"
        },
        {
          "label": "事实属性名称",
          "prop": "fieldName"
        },
        {
          "label": "操作",
          "width": "70px",
          "align": "center",
          "btns": [
            {
              "type": "bringback",
              "label": "选择"
            }
          ]
        }
      ]
    }
  }
}
