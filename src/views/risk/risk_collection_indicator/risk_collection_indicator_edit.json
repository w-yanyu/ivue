{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "controls": [
        {
          "collectionIndicatorCode": {
            "label": "集合指标代码",
            "control": "string",
            "span": 12
          },
          "collectionIndicatorName": {
            "label": "集合指标名称",
            "control": "text",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请正确输入",
                "max": 100
              }
            ]
          }
        },
        {
          "factList": {
            "label": "事实",
            "control": "cascader",
            "span": 12,
            "doRequest": {
              "url": "/API/risk-management-service/combobox-select/fact_code_list",
              "method": "post",
              "params": {}
            },
            "rules": [
              {
                "required": true,
                "message": "该控件必填"
              }
            ]
          },
          "relation": {
            "label": "关系",
            "control": "select",
            "span": 12,
            "dict": {
              "dictType": "relation",
              "dictKey": [],
              "format": "value-label",
              "doRequest": {
                "url": "/views/risk/dictdata/relation.json",
                "method": "get"
              }
            },
            "rules": [
              {
                "required": true,
                "message": "该控件必填",
                "max": 10
              }
            ]
          }
        },
        {
          "collectionIndicatorList": {
            "label": "集合指标数据",
            "control": "cascader",
            "span": 12,
            "doRequest": {
              "url": "/API/risk-management-service/combobox-select/collection_source_list",
              "method": "post",
              "params": {}
            },
            "rules": [
              {
                "required": true,
                "message": "该控件必填"
              }
            ]
          },
          "type": {
            "label": "类型",
            "control": "select",
            "span": 12,
            "dict": {
              "dictType": "rciType",
              "dictKey": [],
              "format": "value-label",
              "doRequest": {
                "url": "/views/risk/dictdata/rciType.json",
                "method": "get"
              }
            },
            "rules": [
              {
                "required": true,
                "message": "该控件必填",
                "max": 10
              }
            ]
          }
        }
      ],
      "initRequest": {
        "url": "/API/risk-management-service/risk-collection-indicator/query",
        "method": "post"
      },
      "doRequest": {
        "url": "/API/risk-management-service/risk-collection-indicator/update",
        "method": "post",
        "params":{
          "updateUser": "$g{{user_info.userCode}}"
        }
      }
    }
  }
}
