{
  "module": "v-search-btn-datagrid",
  "layout": {
    "datagrid": {
      "doRequest": {
        "url": "/API/risk-management-service/combobox-select/collection-code",
        "method": "post",
        "params":{
          "collectionSource":"${{parent.form.collectionSource}}"
        }
      },
      "pagination": {
        "pageSizes": [
          5,
          10,
          20,
          50,
          100
        ],
        "pageSize": 5
      },
      "thead": [
        {
          "label": "代码",
          "prop": "value",
          "width": "250px",
          "align": "center"
        },
        {
          "label": "名称",
          "prop": "label"

        },
        {
          "label": "操作",
          "width": "70px",
          "align": "center",
          "btns": [
            {
              "type": "bringback",
              "label": "选择"
            }
          ]
        }
      ]
    }
  }
}
