{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/WEB/user/random-string",
        "method": "GET"
      },
      "doRequest": {
        "url": "/WEB/user/reset-pwd",
        "method": "PUT"
      },
      "controls": [
        {
          "userCode": {
            "control": "string",
            "label": "用户编码"
          },
          "userName": {
            "control": "string",
            "label": "用户名"
          }
        },
        {
          "userPassword": {
            "control": "text",
            "label": "重置密码",
            "required": true
          }
        }
      ]
    }
  }
}
