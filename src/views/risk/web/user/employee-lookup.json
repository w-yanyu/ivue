{
  "module": "v-search-btn-datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "employeeCode": {
            "control": "text",
            "label": "员工编码"
          },
          "employeeName": {
            "control": "text",
            "label": "员工名称"
          }
        }
      ]
    },
    "toolbar": {
      "btns": []
    },
    "datagrid": {
      "doRequest": {
        "url": "/WEB/employee/page",
        "method": "get"
      },
      "pagination": {
        "pageSizes": [
          10,
          15,
          20,
          50,
          100
        ],
        "pageSize": 10
      },
      "thead": [
        {
          "type": "index",
          "label": "序号"
        },
        {
          "label": "员工编码",
          "prop": "employeeCode"
        },
        {
          "label": "员工名称",
          "prop": "employeeName"
        },
        {
          "label": "员工编号",
          "prop": "employeeCode"
        },
        {
          "label": "状态",
          "prop": "status",
          "dict": {
            "dictType": "WEB",
            "dictKey": [
              "WEB",
              "employeeStatus"
            ],
            "labelField": "dictName",
            "valueField": "dictCode",
            "doRequest": {
              "url": "/WEB/dict/common",
              "method": "post"
            }
          }
        },
        {
          "label": "操作",
          "width": "160",
          "btns": [
            {
              "type": "bringback",
              "label": "选择"
            }
          ]
        }
      ]
    }
  }
}
