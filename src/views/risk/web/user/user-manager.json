{
  "module": "v-search-btn-datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "userCode": {
            "control": "text",
            "label": "用户编码"
          },
          "userName": {
            "control": "text",
            "label": "用户名称"
          }
        },
        {
          "status": {
            "control": "select",
            "label": "用户状态",
            "dict": {
              "dictType": "WEB",
              "dictKey": [
                "WEB",
                "user-status"
              ],
              "labelField": "dictName",
              "valueField": "dictCode",
              "doRequest": {
                "url": "/WEB/dict/common",
                "method": "post"
              }
            }
          },
          "enable": {
            "control": "select",
            "label": "是否有效",
            "value": "1",
            "dict": {
              "dictType": "WEB",
              "dictKey": [
                "WEB",
                "boolean"
              ],
              "labelField": "dictName",
              "valueField": "dictCode",
              "doRequest": {
                "url": "/WEB/dict/common",
                "method": "post"
              }
            }
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "target": "navTab",
          "label": "新增",
          "toRequest": {
            "url": "/views/risk/web/user/add.json",
            "method": "GET"
          },
          "props": {
            "width": "65%",
            "title": "新增用户"
          }
        },
        {
          "type": "delete",
          "label": "删除",
          "icon": "el-icon-close",
          "doRequest": {
            "url": "/WEB/user/batch",
            "method": "DELETE"
          }
        }
      ]
    },
    "datagrid": {
      "doRequest": {
        "url": "/WEB/user/page",
        "method": "get"
      },
      "pagination": {
        "pageSizes": [
          10,
          15,
          20,
          50,
          100
        ],
        "pageSize": 10
      },
      "thead": [
        {
          "width": "55",
          "type": "selection"
        },
        {
          "type": "index",
          "label": "序号"
        },
        {
          "label": "用户编码",
          "prop": "userCode"
        },
        {
          "label": "用户名称",
          "prop": "userName"
        },
        {
          "label": "员工编号",
          "prop": "employeeCode"
        },
        {
          "label": "用户状态",
          "prop": "status",
          "dict": {
            "dictType": "WEB",
            "dictKey": [
              "WEB",
              "user-status"
            ]
          }
        },
        {
          "label": "是否有效",
          "prop": "enable",
          "dict": {
            "dictType": "WEB",
            "dictKey": [
              "WEB",
              "boolean"
            ]
          }
        },
        {
          "label": "操作",
          "width": "200",
          "btns": [
            {
              "type": "edit",
              "label": "编辑",
              "toRequest": {
                "url": "/views/risk/web/user/update.json",
                "method": "GET"
              },
              "props": {
                "width": "65%",
                "title": "编辑"
              }
            },
            {
              "type": "edit",
              "label": "重置密码",
              "toRequest": {
                "url": "/views/risk/web/user/reset-pwd.json",
                "method": "GET"
              },
              "props": {
                "width": "65%",
                "title": "重置密码"
              }
            },
            {
              "type": "custom",
              "label": "锁定",
              "doRequest": {
                "url": "/WEB/user/lock",
                "method": "PUT"
              },
              "confirm": {
                "title": "锁定",
                "msg": "确定锁定用户吗？",
                "confirmButtonText": "确定",
                "cancelButtonText": "取消"
              },
              "show": {
                "prop": "status",
                "is": [
                  "A"
                ]
              }
            },
            {
              "type": "custom",
              "label": "激活",
              "doRequest": {
                "url": "/WEB/user/active",
                "method": "PUT"
              },
              "confirm": {
                "title": "激活",
                "msg": "确定激活用户吗？",
                "confirmButtonText": "确定",
                "cancelButtonText": "取消"
              },
              "show": {
                "prop": "status",
                "is": [
                  "E",
                  "F"
                ]
              }
            },
            {
              "type": "delete",
              "label": "删除",
              "doRequest": {
                "url": "/WEB/user",
                "method": "DELETE"
              }
            }
          ]
        }
      ]
    }
  }
}
