{
  "module": "v-search-btn-datagrid",
  "layout": {
    "form": {
      "scope": "employeeQueryForm",
      "controls": [
        {
          "employeeCode": {
            "control": "text",
            "label": "人员编码"
          },
          "employeeName": {
            "control": "text",
            "label": "人员名称"
          }
        },
        {
          "gender": {
            "control": "select",
            "label": "性别",
            "dict": {
              "dictType": "WEB",
              "dictKey": [
                "WEB",
                "gender"
              ],
              "labelField": "dictName",
              "valueField": "dictCode",
              "doRequest": {
                "url": "/WEB/dict/common",
                "method": "post"
              }
            }
          },
          "enable": {
            "control": "select",
            "label": "是否有效",
            "value": "1",
            "dict": {
              "dictType": "WEB",
              "dictKey": [
                "WEB",
                "boolean"
              ],
              "labelField": "dictName",
              "valueField": "dictCode",
              "doRequest": {
                "url": "/WEB/dict/common",
                "method": "post"
              }
            }
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "target": "navTab",
          "label": "新增",
          "toRequest": {
            "url": "/views/risk/web/employee/employee-add.json",
            "method": "GET"
          },
          "props": {
            "width": "65%",
            "title": "新增员工"
          }
        },
        {
          "type": "delete",
          "label": "删除",
          "icon": "el-icon-close",
          "doRequest": {
            "url": "/WEB/employee/batch",
            "method": "DELETE"
          }
        }
      ]
    },
    "datagrid": {
      "doRequest": {
        "url": "/WEB/employee/page",
        "method": "GET"
      },
      "pagination": {
        "pageSizes": [
          10,
          15,
          20,
          50,
          100
        ],
        "pageSize": 10
      },
      "thead": [
        {
          "width": "55",
          "type": "selection"
        },
        {
          "type": "index",
          "label": "序号"
        },
        {
          "label": "人员编码",
          "prop": "employeeCode"
        },
        {
          "label": "人员名称",
          "prop": "employeeName"
        },
        {
          "label": "性别",
          "prop": "gender",
          "dict": {
            "dictType": "WEB",
            "dictKey": [
              "WEB",
              "gender"
            ],
            "labelField": "dictName",
            "valueField": "dictCode",
            "doRequest": {
              "url": "/WEB/dict/common",
              "method": "post"
            }
          }
        },
        {
          "label": "是否有效",
          "prop": "enable",
          "dict": {
            "dictType": "WEB",
            "dictKey": [
              "WEB",
              "boolean"
            ],
            "labelField": "dictName",
            "valueField": "dictCode",
            "doRequest": {
              "url": "/WEB/dict/common",
              "method": "post"
            }
          }
        },
        {
          "label": "操作",
          "width": "100",
          "btns": [
            {
              "type": "edit",
              "label": "编辑",
              "toRequest": {
                "url": "/views/risk/web/employee/employee-update.json",
                "method": "GET"
              },
              "props": {
                "width": "65%",
                "title": "编辑应用"
              }
            },
            {
              "type": "delete",
              "label": "删除",
              "doRequest": {
                "url": "/WEB/employee",
                "method": "DELETE"
              }
            }
          ]
        }
      ]
    }
  }
}
