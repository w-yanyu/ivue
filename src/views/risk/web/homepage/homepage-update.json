{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/WEB/homepage",
        "method": "GET"
      },
      "doRequest": {
        "url": "/WEB/homepage",
        "method": "PUT"
      },
      "controls": [
        {
          "homePageCode": {
            "control": "string",
            "label": "首页编码",
            "required": true
          },
          "homePageName": {
            "control": "text",
            "label": "首页名称",
            "required": true
          }
        },
        {
          "application": {
            "control": "select",
            "label": "所属应用",
            "required": true,
            "dict": {
              "dictType": "WEB",
              "dictKey": [
                "homepage",
                "application"
              ],
              "labelField": "appName",
              "valueField": "appCode",
              "doRequest": {
                "url": "/WEB/application/list",
                "method": "GET"
              }
            }
          }
        },
        {
          "description": {
            "control": "textarea",
            "span": 24,
            "label": "备注"
          }
        }
      ]
    }
  }
}
