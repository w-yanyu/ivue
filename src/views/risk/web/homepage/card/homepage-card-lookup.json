{
  "module": "v-search-btn-datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "cardCode": {
            "control": "text",
            "label": "模块编码"
          },
          "cardName": {
            "control": "text",
            "label": "模块名称"
          }
        }
      ]
    },
    "datagrid": {
      "doRequest": {
        "url": "/WEB/homepage-card/page",
        "method": "GET"
      },
      "pagination": {
        "pageSizes": [
          10,
          15,
          20,
          50,
          100
        ],
        "pageSize": 10
      },
      "thead": [
        {
          "type": "index",
          "label": "序号"
        },
        {
          "label": "模块编码",
          "prop": "cardCode"
        },
        {
          "label": "模块名称",
          "prop": "cardName"
        },
        {
          "label": "模块地址",
          "prop": "cardUrl"
        },
        {
          "label": "操作",
          "width": "80px",
          "btns": [
            {
              "type": "bringback",
              "label": "选择"
            }
          ]
        }
      ]
    }
  }
}
