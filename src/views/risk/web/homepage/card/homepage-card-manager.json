{
  "module": "v-search-btn-datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "cardCode": {
            "control": "text",
            "label": "模块编码"
          },
          "cardName": {
            "control": "text",
            "label": "模块名称"
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "target": "navTab",
          "label": "新增",
          "toRequest": {
            "url": "/views/risk/web/homepage/card/homepage-card-add.json",
            "method": "GET"
          },
          "props": {
            "width": "65%",
            "title": "新增模块"
          }
        },
        {
          "type": "delete",
          "label": "删除",
          "icon": "el-icon-close",
          "doRequest": {
            "url": "/WEB/homepage-card/batch",
            "method": "DELETE"
          }
        }
      ]
    },
    "datagrid": {
      "doRequest": {
        "url": "/WEB/homepage-card/page",
        "method": "get"
      },
      "pagination": {
        "pageSizes": [
          10,
          15,
          20,
          50,
          100
        ],
        "pageSize": 10
      },
      "thead": [
        {
          "width": "55",
          "type": "selection"
        },
        {
          "type": "index",
          "label": "序号"
        },
        {
          "label": "模块编码",
          "prop": "cardCode"
        },
        {
          "label": "模块名称",
          "prop": "cardName"
        },
        {
          "label": "模块地址",
          "prop": "cardUrl"
        },
        {
          "label": "操作",
          "width": "220",
          "btns": [
            {
              "type": "edit",
              "label": "编辑",
              "toRequest": {
                "url": "/views/risk/web/homepage/card/homepage-card-update.json",
                "method": "GET"
              },
              "props": {
                "width": "65%",
                "title": "编辑"
              }
            },
            {
              "type": "delete",
              "label": "删除",
              "doRequest": {
                "url": "/WEB/homepage-card",
                "method": "DELETE"
              }
            }
          ]
        }
      ]
    }
  }
}
