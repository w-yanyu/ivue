{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "doRequest": {
        "url": "/WEB/homepage-card",
        "method": "POST"
      },
      "controls": [
        {
          "cardCode": {
            "control": "text",
            "label": "模块编码",
            "required": true
          },
          "cardName": {
            "control": "text",
            "label": "模块名称",
            "required": true
          }
        },
        {
          "cardUrl": {
            "control": "text",
            "label": "模块地址",
            "required": true
          }
        },
        {
          "description": {
            "control": "textarea",
            "span": 24,
            "label": "备注"
          }
        }
      ]
    }
  }
}
