{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/WEB/dict/type/${{params.id}}",
        "method": "GET"
      },
      "doRequest": {
        "url": "/WEB/dict/type",
        "method": "PUT"
      },
      "controls": [
        {
          "dictType": {
            "control": "string",
            "label": "字典类型",
            "placeholder": "字典类型",
            "required": true
          },
          "dictName": {
            "control": "text",
            "label": "字典名称",
            "required": true
          }
        },
        {
          "application": {
            "control": "string",
            "label": "应用编码",
            "required": true,
            "dict": {
              "dictType": "WEB",
              "dictKey": [
                "homepage",
                "application"
              ],
              "labelField": "appName",
              "valueField": "appCode",
              "doRequest": {
                "url": "/WEB/application/list",
                "method": "GET"
              }
            }
          },
          "sequence": {
            "control": "inputNumber",
            "label": "排序"
          }
        },
        {
          "i18nCode": {
            "control": "text",
            "label": "名称国际化编码"
          },
          "enable": {
            "control": "radio",
            "label": "是否有效",
            "value": "1",
            "required": true,
            "dict": {
              "dictType": "WEB",
              "dictKey": [
                "WEB",
                "boolean"
              ],
              "labelField": "dictName",
              "valueField": "dictCode",
              "doRequest": {
                "url": "/WEB/dict/common",
                "method": "post"
              }
            }
          }
        },
        {
          "description": {
            "control": "textarea",
            "span": 24,
            "label": "说明"
          }
        }
      ]
    }
  }
}
