{
  "module": "v-search-btn-datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "dictType": {
            "control": "text",
            "label": "字典类型",
            "placeholder": "字典类型"
          },
          "dictName": {
            "control": "text",
            "label": "字典名称",
            "placeholder": "字典名称"
          }
        },
        {
          "application": {
            "control": "select",
            "label": "所属应用",
            "dict": {
              "dictType": "WEB",
              "dictKey": [
                "homepage",
                "application"
              ],
              "labelField": "appName",
              "valueField": "appCode",
              "doRequest": {
                "url": "/WEB/application/list",
                "method": "GET"
              }
            }
          },
          "enable": {
            "control": "select",
            "label": "是否有效",
            "value": "1",
            "dict": {
              "dictType": "WEB",
              "dictKey": [
                "WEB",
                "boolean"
              ],
              "labelField": "dictName",
              "valueField": "dictCode",
              "doRequest": {
                "url": "/WEB/dict/common",
                "method": "post"
              }
            }
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "target": "dialog",
          "label": "新增",
          "toRequest": {
            "url": "/views/risk/web/dict/dict-add.json",
            "method": "GET"
          },
          "props": {
            "width": "65%",
            "title": "新增字典"
          }
        },
        {
          "type": "delete",
          "label": "删除",
          "icon": "el-icon-close",
          "doRequest": {
            "url": "/WEB/dict/type/batch",
            "method": "DELETE"
          }
        }
      ]
    },
    "datagrid": {
      "doRequest": {
        "url": "/WEB/dict/page",
        "method": "GET"
      },
      "pagination": {
        "pageSizes": [
          10,
          15,
          20,
          50,
          100
        ],
        "pageSize": 10
      },
      "thead": [
        {
          "type": "selection",
          "width": "55"
        },
        {
          "type": "index",
          "label": "序号"
        },
        {
          "label": "字典类型",
          "prop": "dictType"
        },
        {
          "label": "类型名称",
          "prop": "dictName"
        },
        {
          "label": "所属应用",
          "prop": "application",
          "dict": {
            "dictType": "WEB",
            "dictKey": [
              "homepage",
              "application"
            ],
            "labelField": "appName",
            "valueField": "appCode",
            "doRequest": {
              "url": "/WEB/application/list",
              "method": "GET"
            }
          }
        },
        {
          "label": "排序号",
          "prop": "sequence"
        },
        {
          "label": "是否有效",
          "prop": "enable",
          "dict": {
            "dictType": "WEB",
            "dictKey": [
              "WEB",
              "boolean"
            ],
            "labelField": "dictName",
            "valueField": "dictCode",
            "doRequest": {
              "url": "/WEB/dict/common",
              "method": "post"
            }
          }
        },
        {
          "label": "说明",
          "prop": "description"
        },
        {
          "label": "操作",
          "width": "120",
          "btns": [
            {
              "type": "edit",
              "label": "编辑",
              "toRequest": {
                "url": "/views/risk/web/dict/dict-update.json",
                "method": "GET"
              },
              "props": {
                "width": "65%",
                "title": "编辑字典"
              }
            },
            {
              "type": "look",
              "label": "字典项",
              "toRequest": {
                "url": "/views/risk/web/dict/dict-items-manager.json",
                "method": "GET"
              },
              "props": {
                "width": "65%",
                "title": "字典项"
              }
            }
          ],
          "type": "btns"
        }
      ]
    }
  }
}
