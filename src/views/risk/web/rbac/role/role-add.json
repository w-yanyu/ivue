{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "doRequest": {
        "url": "/WEB/role",
        "method": "POST"
      },
      "controls": [
        {
          "roleCode": {
            "control": "text",
            "label": "角色编码",
            "rules": [
              {
                "required": true,
                "message": "请输入角色编码"
              }
            ]
          },
          "roleName": {
            "control": "text",
            "label": "角色名称",
            "rules": [
              {
                "required": true,
                "message": "请输入角色名称"
              }
            ]
          }
        },
        {
          "application": {
            "control": "select",
            "label": "所属应用",
            "dict": {
              "dictType": "WEB",
              "dictKey": [
                "homepage",
                "application"
              ],
              "labelField": "appName",
              "valueField": "appCode",
              "doRequest": {
                "url": "/WEB/application/list",
                "method": "GET"
              }
            }
          },
          "enable": {
            "control": "radio",
            "label": "是否有效",
            "value": "1",
            "dict": {
              "dictType": "WEB",
              "dictKey": [
                "WEB",
                "boolean"
              ],
              "labelField": "dictName",
              "valueField": "dictCode",
              "doRequest": {
                "url": "/WEB/dict/common",
                "method": "post"
              }
            }
          }
        },
        {
          "description": {
            "span": 24,
            "control": "textarea",
            "label": "备注"
          }
        }
      ]
    }
  }
}
