{
  "module": "dynamicTabs",
  "layout": {
    "showFormBtn": false,
    "tabs": {
      "defaultActiveKey": "user",
      "position": "top",
      "styleClass": "card",
      "tabHeader": [
        {
          "id": "user",
          "label": "角色用户",
          "closable": false,
          "disabled": false,
          "icon": "el-icon-date",
          "toRequest": {
            "url": "/views/risk/web/rbac/role/set-user.json",
            "method": "GET",
            "params": {
              "roleCode": "${{params.roleCode}}"
            }
          },
          "props": {
          }
        },
        {
          "id": "resource",
          "label": "授权资源",
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "toRequest": {
            "url": "/views/risk/web/rbac/role/set-resource.json"
          },
          "props": {
          }
        }
      ]
    }
  }
}