{
  "module": "v-selectableTree",
  "layout": {
    "tree": {
      "idField": "resourceCode",
      "pIdField": "resourceParent",
      "labelField": "resourceName",
      "checkedField": "checked",
      "isLeafField": "isLeaf",
      "disabledField": "disabled",
      "iconField": "icon",
      "initRequest": {
        "url": "/WEB/role/resources",
        "method": "GET"
      },
      "doRequest": {
        "url": "/WEB/role/grant-resource",
        "method": "POST",
        "params": {
          "roleCode": "${{params.roleCode}}"
        }
      }
    }
  }
}
