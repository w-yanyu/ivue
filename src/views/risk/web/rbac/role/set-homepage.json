{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/WEB/homepage/role/relation",
        "method": "GET"
      },
      "doRequest": {
        "url": "/WEB/homepage/role/relation",
        "method": "POST"
      },
      "controls": [
        {
          "roleCode": {
            "control": "string",
            "label": "角色编码"
          },
          "roleName": {
            "control": "string",
            "label": "角色名称",
            "value": "${{params.roleName}}"
          }
        },
        {
          "homePageCode": {
            "control": "select",
            "label": "选择首页",
            "dict": {
              "dictType": "WEB",
              "dictKey": [
                "role",
                "homepage"
              ],
              "labelField": "homePageName",
              "valueField": "homePageCode",
              "doRequest": {
                "url": "/WEB/homepage/list",
                "method": "GET"
              }
            }
          }
        }
      ]
    }
  }
}
