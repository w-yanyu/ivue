{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/WEB/resource",
        "method": "GET",
        "params": {
          "resourceCode": "${{params.resourceCode}}"
        }
      },
      "doRequest": {
        "url": "/WEB/resource",
        "method": "PUT"
      },
      "controls": [
        {
          "resourceCode": {
            "control": "string",
            "label": "资源编码"
          },
          "resourceName": {
            "control": "text",
            "label": "资源名称"
          }
        },
        {
          "application": {
            "control": "select",
            "label": "所属应用",
            "dict": {
              "dictType": "WEB",
              "dictKey": [
                "homepage",
                "application"
              ],
              "labelField": "appName",
              "valueField": "appCode",
              "doRequest": {
                "url": "/WEB/application/list",
                "method": "GET"
              }
            }
          },
          "sequence": {
            "control": "text",
            "label": "顺序号"
          }
        },
        {
          "resourceType": {
            "control": "select",
            "label": "资源类型",
            "disabled": true,
            "dict": {
              "dictType": "WEB",
              "dictKey": [
                "WEB",
                "resourceType"
              ]
            }
          },
          "resourceDisplay": {
            "control": "select",
            "label": "显示状态",
            "dict": {
              "dictType": "WEB",
              "dictKey": [
                "WEB",
                "display"
              ]
            }
          }
        },
        {
          "enable": {
            "control": "radio",
            "label": "是否有效",
            "dict": {
              "dictType": "WEB",
              "dictKey": [
                "WEB",
                "boolean"
              ]
            }
          }
        },
        {
          "description": {
            "span": 24,
            "control": "textarea",
            "label": "备注"
          }
        }
      ]
    }
  }
}
