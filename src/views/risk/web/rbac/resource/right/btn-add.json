{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "doRequest": {
        "url": "/WEB/resource",
        "method": "POST",
        "params": {
          "resourceParent": "${{params.resourceCode}}"
        }
      },
      "controls": [
        {
          "resourceCode": {
            "control": "text",
            "label": "资源编码",
            "rules": [
              {
                "required": true,
                "message": "请输入资源编码"
              }
            ]
          },
          "resourceName": {
            "control": "text",
            "label": "资源名称",
            "rules": [
              {
                "required": true,
                "message": "请输入资源名称"
              }
            ]
          }
        },
        {
          "application": {
            "control": "select",
            "label": "所属应用",
            "dict": {
              "dictType": "WEB",
              "dictKey": [
                "homepage",
                "application"
              ],
              "labelField": "appName",
              "valueField": "appCode",
              "doRequest": {
                "url": "/WEB/application/list",
                "method": "GET"
              }
            }
          },
          "sequence": {
            "control": "text",
            "label": "顺序号"
          }
        },
        {
          "resourceType": {
            "control": "select",
            "label": "资源类型",
            "value": "btn",
            "disabled": true,
            "dict": {
              "dictType": "WEB",
              "dictKey": [
                "WEB",
                "resourceType"
              ],
              "labelField": "dictName",
              "valueField": "dictCode",
              "doRequest": {
                "url": "/WEB/dict/common",
                "method": "post"
              }
            }
          },
          "resourceIcon": {
            "control": "text",
            "label": "资源图标"
          }
        },
        {
          "resourceMethod": {
            "control": "select",
            "label": "方法",
            "dict": {
              "dictType": "WEB",
              "dictKey": [
                "WEB",
                "method"
              ],
              "labelField": "dictName",
              "valueField": "dictCode",
              "doRequest": {
                "url": "/WEB/dict/common",
                "method": "post"
              }
            }
          },
          "resourceUrl": {
            "control": "text",
            "label": "资源地址"
          }
        },
        {
          "enable": {
            "control": "radio",
            "label": "是否有效",
            "value": "1",
            "dict": {
              "dictType": "WEB",
              "dictKey": [
                "WEB",
                "boolean"
              ],
              "labelField": "dictName",
              "valueField": "dictCode",
              "doRequest": {
                "url": "/WEB/dict/common",
                "method": "post"
              }
            }
          },
          "resourceDisplay": {
            "control": "select",
            "label": "显示状态",
            "value": "true",
            "dict": {
              "dictType": "WEB",
              "dictKey": [
                "WEB",
                "display"
              ],
              "labelField": "dictName",
              "valueField": "dictCode",
              "doRequest": {
                "url": "/WEB/dict/common",
                "method": "post"
              }
            }
          }
        },
        {
          "description": {
            "span": 24,
            "control": "textarea",
            "label": "备注"
          }
        }
      ]
    }
  }
}
