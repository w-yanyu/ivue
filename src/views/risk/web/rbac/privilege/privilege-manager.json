{
  "module": "v-search-btn-datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "privCode": {
            "control": "text",
            "label": "编码"
          },
          "privName": {
            "control": "text",
            "label": "名称"
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "target": "navTab",
          "label": "新增",
          "toRequest": {
            "url": "/views/risk/web/rbac/privilege/privilege-add.json",
            "method": "GET"
          },
          "props": {
            "width": "65%",
            "title": "新增"
          }
        },
        {
          "type": "delete",
          "label": "删除",
          "icon": "el-icon-close",
          "doRequest": {
            "url": "/WEB/priv/batch",
            "method": "DELETE"
          }
        }
      ]
    },
    "datagrid": {
      "doRequest": {
        "url": "/WEB/priv/page",
        "method": "get"
      },
      "pagination": {
        "pageSizes": [
          10,
          15,
          20,
          50,
          100
        ],
        "pageSize": 10
      },
      "thead": [
        {
          "width": "55",
          "type": "selection"
        },
        {
          "type": "index",
          "label": "序号"
        },
        {
          "label": "编码",
          "prop": "privCode"
        },
        {
          "label": "名称",
          "prop": "privName"
        },
        {
          "label": "所属系统",
          "prop": "application",
          "dict": {
            "dictType": "WEB",
            "dictKey": [
              "homepage",
              "application"
            ],
            "labelField": "appName",
            "valueField": "appCode",
            "doRequest": {
              "url": "/WEB/application/list",
              "method": "GET"
            }
          }
        },
        {
          "label": "说明",
          "prop": "description"
        },
        {
          "label": "操作",
          "width": "180",
          "btns": [
            {
              "type": "edit",
              "label": "编辑",
              "toRequest": {
                "url": "/views/risk/web/rbac/privilege/privilege-update.json",
                "method": "GET"
              },
              "props": {
                "width": "65%",
                "title": "编辑"
              }
            },
            {
              "type": "delete",
              "label": "删除",
              "doRequest": {
                "url": "/WEB/priv",
                "method": "DELETE"
              }
            }
          ]
        }
      ]
    }
  }
}
