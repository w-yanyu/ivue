{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/WEB/priv",
        "method": "GET"
      },
      "doRequest": {
        "url": "/WEB/priv",
        "method": "PUT"
      },
      "controls": [
        {
          "privCode": {
            "control": "string",
            "label": "编码",
            "rules": [
              {
                "required": true,
                "message": "请输入编码"
              }
            ]
          },
          "privName": {
            "control": "text",
            "label": "名称",
            "rules": [
              {
                "required": true,
                "message": "请输入名称"
              }
            ]
          }
        },
        {
          "application": {
            "control": "select",
            "label": "所属应用",
            "dict": {
              "dictType": "WEB",
              "dictKey": [
                "homepage",
                "application"
              ],
              "labelField": "appName",
              "valueField": "appCode",
              "doRequest": {
                "url": "/WEB/application/list",
                "method": "GET"
              }
            }
          }
        },
        {
          "description": {
            "span": 24,
            "control": "textarea",
            "label": "说明"
          }
        }
      ]
    }
  }
}
