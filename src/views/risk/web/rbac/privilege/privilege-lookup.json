{
  "module": "v-search-btn-datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "privCode": {
            "control": "text",
            "label": "编码"
          },
          "privName": {
            "control": "text",
            "label": "名称"
          }
        }
      ]
    },
    "datagrid": {
      "doRequest": {
        "url": "/WEB/priv/page",
        "method": "GET"
      },
      "pagination": {
        "pageSizes": [
          10,
          15,
          20,
          50,
          100
        ],
        "pageSize": 10
      },
      "thead": [
        {
          "type": "index",
          "label": "序号"
        },
        {
          "label": "编码",
          "prop": "privCode"
        },
        {
          "label": "名称",
          "prop": "privName"
        },
        {
          "label": "所属系统",
          "prop": "application",
          "dict": {
            "dictType": "WEB",
            "dictKey": [
              "homepage",
              "application"
            ],
            "labelField": "appName",
            "valueField": "appCode",
            "doRequest": {
              "url": "/WEB/application/list",
              "method": "GET"
            }
          }
        },
        {
          "label": "操作",
          "width": "80px",
          "btns": [
            {
              "type": "bringback",
              "label": "选择"
            }
          ]
        }
      ]
    }
  }
}
