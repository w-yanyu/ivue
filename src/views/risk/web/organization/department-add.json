{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "doRequest": {
        "url": "/WEB/organization",
        "method": "POST",
        "params": {
          "orgParent": "${{params.orgCode}}"
        }
      },
      "controls": [
        {
          "orgCode": {
            "control": "text",
            "label": "部门编码",
            "rules": [
              {
                "required": true,
                "message": "请输入部门编码"
              }
            ]
          },
          "orgName": {
            "control": "text",
            "label": "部门名称",
            "rules": [
              {
                "required": true,
                "message": "请输入部门名称"
              }
            ]
          }
        },
        {
          "orgType": {
            "control": "select",
            "label": "类型",
            "value": "department",
            "disabled": true,
            "dict": {
              "dictType": "WEB",
              "dictKey": [
                "WEB",
                "organization-type"
              ]
            },
            "required": true
          },
          "sequence": {
            "control": "inputNumber",
            "label": "排序号"
          }
        },
        {
          "enable": {
            "control": "radio",
            "label": "是否有效",
            "value": "1",
            "dict": {
              "dictType": "WEB",
              "dictKey": [
                "WEB",
                "boolean"
              ]
            },
            "required": true
          }
        },
        {
          "description": {
            "span": 24,
            "control": "textarea",
            "label": "备注"
          }
        }
      ]
    }
  }
}
