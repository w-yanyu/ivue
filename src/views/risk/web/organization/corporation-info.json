{
  "module": "v-realonlyForm-btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/WEB/organization",
        "method": "GET",
        "params": {
          "orgCode": "${{params.ID}}"
        }
      },
      "controls": [
        {
          "orgCode": {
            "control": "string",
            "label": "法人编码"
          }
        },
        {
          "orgName": {
            "control": "string",
            "label": "法人名称"
          }
        },
        {
          "orgSequence": {
            "control": "string",
            "label": "排序号"
          }
        },
        {
          "description": {
            "span": 24,
            "control": "string",
            "label": "备注"
          }
        }
      ]
    }
  }
}
