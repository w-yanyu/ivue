{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/WEB/application",
        "method": "GET"
      },
      "doRequest": {
        "url": "/WEB/application",
        "method": "PUT"
      },
      "controls": [
        {
          "appCode": {
            "control": "string",
            "label": "应用编码",
            "placeholder": "应用编码",
            "required": true
          },
          "appName": {
            "control": "text",
            "label": "应用名称",
            "placeholder": "应用名称",
            "required": true
          }
        },
        {
          "appFullName": {
            "control": "text",
            "label": "应用全称",
            "placeholder": "应用全称"
          },
          "i18nCode": {
            "control": "text",
            "label": "名称国际化编码",
            "placeholder": "名称国际化编码"
          }
        },
        {
          "appIcon": {
            "control": "text",
            "label": "应用图标",
            "placeholder": "应用图标"
          },
          "sortNo": {
            "control": "text",
            "label": "排序",
            "placeholder": "排序"
          }
        },
        {
          "isPublic": {
            "control": "radio",
            "label": "是否公共应用",
            "required": true,
            "dict": {
              "dictType": "WEB",
              "dictKey": [
                "WEB",
                "boolean"
              ],
              "labelField": "dictName",
              "valueField": "dictCode",
              "doRequest": {
                "url": "/WEB/dict/common",
                "method": "post"
              }
            }
          },
          "isExternal": {
            "control": "radio",
            "label": "是否外部应用",
            "required": true,
            "dict": {
              "dictType": "WEB",
              "dictKey": [
                "WEB",
                "boolean"
              ],
              "labelField": "dictName",
              "valueField": "dictCode",
              "doRequest": {
                "url": "/WEB/dict/common",
                "method": "post"
              }
            },
            "showItem": {
              "1": [
                "externalUrl"
              ]
            }
          }
        },
        {
          "externalUrl": {
            "initShow": false,
            "control": "text",
            "label": "外部应用地址",
            "placeholder": "外部应用地址"
          }
        },
        {
          "description": {
            "span": 24,
            "control": "textarea",
            "label": "说明"
          }
        }
      ]
    }
  }
}
