{
  "module": "v-form-editableDataGrid-btn",
  "init":{
    "initRequest": {
      "url": "/API/risk-management-service/risk-collection/query",
      "method": "post"
    }
  },
  "layout": {
    "form": {
      "controls": [
        {
          "collectionCode": {
            "label": "集合代码",
            "control": "string",
            "span": 12
          },
          "collectionName": {
            "label": "集合中文名称",
            "control": "text",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请正确输入",
                "max": 100
              }
            ]
          },
          "dataType": {
            "label": "元素数据类型",
            "control": "select",
            "disabledOptions": ["Boolean"],
            "span": 12,
            "dict": {
              "dictType": "dataType",
              "dictKey": [],
              "format": "value-label",
              "doRequest": {
                "url": "/views/risk/dictdata/dataType.json",
                "method": "get"
              }
            },
            "rules": [
              {
                "required": true,
                "message": "该控件必填",
                "max": 8
              }
            ]
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "label": "新增",
          "icon": "el-icon-plus"
        },
        {
          "type": "batchDelete",
          "label": "批量删除",
          "icon": "el-icon-close"
        },
        {
          "type": "submit",
          "label": "保存",
          "icon": "el-icon-tickets",
          "doRequest": {
            "url": "/API/risk-management-service/risk-collection/update",
            "method": "post",
            "params":{
              "updateUser": "$g{{user_info.userCode}}"
            }
          }
        }
      ]
    },
    "datagrid": {
      "pagination": {
        "show": false
      },
      "allData": true,
      "name":"elementList",
      "thead": [
        {
          "type": "selection",
          "width": "50",
          "prop": {
            "matchKey": "string",
            "selectVal": "2a"
          }
        },
        {
          "prop": "elementValue",
          "label": "元素值",
          "type": "text",
          "rules": [
            {
              "required": true,
              "message": "请正确输入",
              "max": 40
            }
          ]
        },
        {
          "label": "元素名称",
          "prop": "elementName",
          "type": "text",
          "rules": [
            {
              "required": true,
              "message": "请正确输入",
              "max": 100
            }
          ]
        },
        {
          "label": "备注",
          "prop": "description",
          "type": "text",
          "rules": [
            {
              "message": "请正确输入",
              "max": 500
            }
          ]
        }
      ]
    }
  }
}
