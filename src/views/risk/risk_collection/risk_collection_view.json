{
  "module": "v-form-editableDataGrid-btn",
  "init":{
    "initRequest": {
      "url": "/API/risk-management-service/risk-collection/query",
      "method": "post"
    }
  },
  "layout": {
    "form": {
      "controls": [
        {
          "collectionCode": {
            "label": "集合代码",
            "control": "string",
            "span": 12
          },
          "collectionName": {
            "label": "集合中文名称",
            "control": "string",
            "span": 12
          }
        },
        {
          "dataType": {
            "label": "元素数据类型",
            "control": "string",
            "span": 12,
            "dict": {
              "dictType": "dataType",
              "dictKey": [],
              "format": "value-label",
              "doRequest": {
                "url": "/views/risk/dictdata/dataType.json",
                "method": "get"
              }
            }
          }
        }
      ],
      "#initRequest": {
        "url": "/API/risk-management-service/risk-collection/query",
        "method": "post"
      }
    },
    "datagrid": {
      "pagination": {
        "show": false
      },
      "name":"elementList",
      "thead": [
        {
          "prop": "elementValue",
          "label": "元素值",
          "type": "string"
        },
        {
          "label": "元素名称",
          "prop": "elementName",
          "type": "string"
        },
        {
          "label": "备注",
          "prop": "description",
          "type": "string"
        }
      ]
    }
  }
}
