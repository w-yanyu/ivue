{
  "module": "v-search-btn-datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "statisticKey": {
            "label": "统计键值",
            "control": "text",
            "span": 8,
            "rules": [
              {
                "required": true,
                "message": "请正确输入",
                "max": 40
              }
            ]
          },
          "statisticCode": {
            "label": "统计代码",
            "control": "text",
            "span": 8,
            "rules": [
              {
                "message": "字段长度超限",
                "max": 40
              }
            ]
          },
          "statisticMonth": {
            "label": "统计月份",
            "control": "text",
            "span": 8,
            "rules": [
              {
                "message": "字段长度超限",
                "max": 6
              }
            ]
          }
        }
      ]
    },
    "toolbar": {
      "btns": []
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/API/risk-management-service/risk-noncust-statistic/page-select",
        "method": "post"
      },
      "thead": [
        {
          "label": "统计键值",
          "prop": "statisticKey"
        },
        {
          "label": "统计代码",
          "prop": "statisticCode"
        },
        {
          "label": "统计月份",
          "prop": "statisticMonth"
        },
        {
          "label": "操作",
          "btns": [
            {
              "type": "look",
              "label": "查看",
              "toRequest": {
                "url": "/views/risk/risk_other_statistic/risk_other_statistic_view.json",
                "method": "get"
              },
              "props": {
                "width": "70%"
              }
            }
          ]
        }
      ]
    }
  }
}
