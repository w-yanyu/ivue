{
  "module": "v-search-btn-datagrid",
  "pageId": "risk_alert_queue",
  "layout": {
    "form": {
      "controls": [
        {
          "queueId": {
            "label": "队列ID",
            "control": "text",
            "span": 8,
            "rules": [
              {
                "message": "请正确输入",
                "max": 20
              }
            ]
          },
          "queueName": {
            "label": "队列名称",
            "control": "text",
            "span": 8,
            "rules": [
              {
                "message": "请正确输入",
                "max": 60
              }
            ]
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "authId": "alert_queue_add_btn",
          "label": "新增",
          "toRequest": {
            "url": "/views/risk/risk_alert_queue/risk_alert_queue_add.json",
            "method": "get",
            "params": {}
          },
          "props": {
            "width": "800px",
            "title": "新增"
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/API/risk-management-service/risk-alert-queue/page-select",
        "method": "post"
      },
      "thead": [
        {
          "label": "队列ID",
          "prop": "queueId"
        },
        {
          "label": "队列名称",
          "prop": "queueName"
        },
        {
          "label": "优先级",
          "prop": "priority"
        },
        {
          "label": "状态",
          "prop": "status"
        },
        {
          "label": "创建人",
          "prop": "createUser"
        },
        {
          "label": "创建日期",
          "prop": "createTime",
          "type": "dateTimePicker",
          "format": "yyyy-MM-dd HH:mm:ss"
        },
        {
          "label": "备注",
          "prop": "remark"
        },
        {
          "label": "最后更新人",
          "prop": "updateUser"
        },
        {
          "label": "最后修改日期",
          "prop": "updateTime",
          "type": "dateTimePicker",
          "format": "yyyy-MM-dd HH:mm:ss"
        },
        {
          "label": "操作",
          "width": "150px",
          "btns": [
            {
              "type": "edit",
              "label": "编缉",
              "authId": "alert_queue_upd_btn",
              "toRequest": {
                "url": "/views/risk/risk_alert_queue/risk_alert_queue_edit.json",
                "method": "get"
              },
              "props": {}
            },
            {
              "type": "look",
              "label": "查看",
              "toRequest": {
                "url": "/views/risk/risk_alert_queue/risk_alert_queue_view.json",
                "method": "get"
              },
              "props": {}
            },
            {
              "type": "delete",
              "label": "删除",
              "authId": "alert_queue_del_btn",
              "doRequest": {
                "url": "/API/risk-management-service/risk-alert-queue/delete",
                "method": "post"
              }
            }
          ]
        }
      ],
      "pagination": {
        "show": true,
        "pageSizes": [
          "10",
          "50",
          "100",
          "200"
        ],
        "pageSize": 10
      }
    }
  }
}
