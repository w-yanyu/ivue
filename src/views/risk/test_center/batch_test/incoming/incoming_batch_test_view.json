{
  "module": "v-realonlyForm-btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "controls": [
        {
          "task_code": {
            "control": "text",
            "disabled":true,
            "label": "任务代码",
            "width": "auto"
          },
          "task_name": {
            "control": "text",
            "disabled":true,
            "label": "任务名称",
            "width": "auto"
          },
          "task_status": {
            "control": "select",
            "label": "任务状态",
            "disabled":true,
            "width": "auto",
            "options":[
              {
                "label": "待验证",
                "value": "W"
              },
              {
                "label": "验证中",
                "value": "V"
              },
              {
                "label": "验证完成",
                "value": "D"
              }
            ]
          },
          "service_name": {
            "control": "select",
            "disabled":true,
            "label": "决策服务",
            "width": "auto",
            "options":[
              {
                "label": "决策服务一",
                "value": "W"
              },
              {
                "label": "决策服务二",
                "value": "V"
              },
              {
                "label": "决策服务三",
                "value": "D"
              }
            ],
            "rules": [
              {
                "required": true,
                "message": "请选择决策服务"
              }
            ]
          },
          "sandbox_name": {
            "control": "select",
            "disabled":true,
            "label": "沙箱",
            "width": "auto",
            "options":[
              {
                "label": "沙箱一",
                "value": "W"
              },
              {
                "label": "沙箱二",
                "value": "V"
              },
              {
                "label": "沙箱三",
                "value": "D"
              }
            ],
            "rules": [
              {
                "required": true,
                "message": "请选择沙箱"
              }
            ]
          },
          "report_name": {
            "control": "select",
            "disabled":true,
            "label": "报告模板",
            "width": "auto",
            "options":[
              {
                "label": "报告模板一",
                "value": "W"
              },
              {
                "label": "报告模板二",
                "value": "V"
              },
              {
                "label": "报告模板三",
                "value": "D"
              }
            ],
            "rules": [
              {
                "required": true,
                "message": "请选择报告模板"
              }
            ]},
          "verify_progress": {
            "control": "text",
            "disabled":true,
            "label": "验证进度(%)",
            "width": "auto"
          },
          "test_version": {
            "control": "text",
            "disabled":true,
            "label": "版本",
            "width": "auto"
          },
          "data_create_user": {
            "control": "text",
            "disabled":true,
            "label": "创建人",
            "width": "auto"
          },
          "data_create_time": {
            "control": "text",
            "disabled":true,
            "label": "创建时间",
            "width": "auto"
          },
          "data_update_user": {
            "control": "text",
            "disabled":true,
            "label": "最近更新人",
            "width": "auto"
          },
          "data_update_time": {
            "control": "text",
            "disabled":true,
            "label": "最近更新时间",
            "width": "auto"
          },
          "task_desc": {
            "control": "textarea",
            "disabled":true,
            "label": "任务描述",
            "width": "auto",
            "span": 24,
            "autosize": {
              "minRows": 3,
              "maxRows": 5
            }
          }
      }
      ]
  }
}
}
