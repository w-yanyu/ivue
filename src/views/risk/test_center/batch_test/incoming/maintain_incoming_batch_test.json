{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "scope":"formData",
      "initRequest": "${{params}}",
      "doRequest": {
        "url": "/SUMP/call/RPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ce0032",
          "batch_execute_id": "${{params.batch_execute_id}}"
        }
      },
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/call/RPCCall",
            "method": "POST",
            "params": {
              "servicecode": "deci050077",
              "batch_test_type": "0",
              "batch_execute_id": "${{params.batch_execute_id}}"
            }
          }
        }
      ],
      "controls": [
        {
          "task_code": {
            "control": "text",
            "disabled":true,
            "label": "任务代码",
            "width": "auto",
            "rules": [
              {
                "required": true,
                "message": "请输入任务代码"
              },
              {
                "max": 32,
                "message": "长度不能超过32个字符"
              }
            ]
          },
          "task_name": {
            "control": "text",
            "label": "任务名称",
            "width": "auto",
            "rules": [
              {
                "required": true,
                "message": "请输入任务名称"
              },
              {
                "max": 100,
                "message": "长度不能超过100个字符"
              }
            ]
          }
        },
        {
          "service_name": {
            "control": "select",
            "label": "决策服务",
            "width": "auto",
            "options":[
              {
                "label": "决策服务一",
                "value": "决策服务一"
              },
              {
                "label": "决策服务二",
                "value": "决策服务一"
              },
              {
                "label": "决策服务三",
                "value": "决策服务一"
              }
            ],
            "rules": [
              {
                "required": true,
                "message": "请选择决策服务"
              }
            ]
          },
          "sandbox_name": {
            "control": "select",
            "label": "沙箱",
            "width": "auto",
            "options":[
              {
                "label": "沙箱一",
                "value": "沙箱一"
              },
              {
                "label": "沙箱二",
                "value": "沙箱二"
              },
              {
                "label": "沙箱三",
                "value": "沙箱三"
              }
            ],
            "rules": [
              {
                "required": true,
                "message": "请选择沙箱"
              }
            ]
          }
        },{
          "report_name": {
            "control": "select",
            "label": "报告模板",
            "width": "auto",
            "options":[
              {
                "label": "报告模板一",
                "value": "报告模板一"
              },
              {
                "label": "报告模板二",
                "value": "报告模板二"
              },
              {
                "label": "报告模板三",
                "value": "报告模板三"
              }
            ],
            "rules": [
              {
                "required": true,
                "message": "请选择部件对象"
              }
            ]
          },
          "task_desc": {
            "control": "textarea",
            "label": "任务描述",
            "width": "auto",
            "span": 24,
            "autosize": {
              "minRows": 3,
              "maxRows": 5
            },
            "maxlength": 500,
            "showWordLimit": true,
            "rules": [
              {
                "required": true,
                "message": "请输入任务描述"
              },
              {
                "max": 500,
                "message": "长度不能超过500个字符"
              }
            ]
          }
        }
      ]
    }
  }
}
