{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "controls": [
        {
          "parm_code": {
            "label": "配置项编号",
            "width": "120px",
            "control": "text",
            "span": 8,
            "rules": [
              {
                "message": "长度不能超过40个字符",
                "max": 40
              }
            ]
          },
          "parm_name": {
            "label": "配置项名称",
            "width": "120px",
            "control": "text",
            "span": 8,
            "rules": [
              {
                "required": true,
                "message": "长度不能超过100个字符",
                "max": 100
              }
            ]
          },
          "parm_code1": {
            "label": "是否生效",
            "width": "120px",
            "control": "select",
            "span": 8,
            "dict": {
              "format": "label",
              "dictKey": [
                "MS_YES_OR_NO"
              ],
              "dictType": "MS_YES_OR_NO"
            }
          }
        },
        {
          "parm_name2": {
            "label": "信息详情页面",
            "width": "120px",
            "control": "text",
            "span": 24,
            "rules": [
              {
                "message": "长度不能超过100个字符",
                "max": 100
              }
            ]
          }
        },
        {
          "parm_name3": {
            "label": "模板打印页面",
            "control": "text",
            "width": "120px",
            "span": 24,
            "rules": [
              {
                "message": "长度不能超过100个字符",
                "max": 100
              }
            ]
          }
        }
      ],
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": ""
        }
      }
    }
  }
}