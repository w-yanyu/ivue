{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "controls": [
        {
          "parm_code": {
            "label": "模板编号",
            "control": "text",
            "span": 12,
            "rules": [
              {
                "message": "长度不能超过40个字符",
                "max": 40
              }
            ]
          },
          "parm_name": {
            "label": "模板名称",
            "control": "text",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "长度不能超过100个字符",
                "max": 100
              }
            ]
          }
        }
      ],
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": ""
        }
      }
    }
  }
}