{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "controls": [
        {
          "task_code": {
            "label": "任务代码",
            "control": "text",
            "span": 12,
            "rules": [
              {
                "message": "长度不能超过40个字符",
                "max": 40
              }
            ]
          },
          "task_name": {
            "label": "任务名称",
            "control": "text",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "长度不能超过100个字符",
                "max": 100
              }
            ]
          }
        },
        {
          "task_decs": {
            "label": "任务描述",
            "control": "text",
            "span": 12,
            "rules": [
              {
                "message": "长度不能超过40个字符",
                "max": 40
              }
            ]
          },
          "task_status": {
            "label": "任务状态",
            "control": "select",
            "span": 12,
            "dict": {
              "format": "label",
              "dictKey": [
                "DECI_E_DATA_TYPE"
              ],
              "dictType": "DECI_E_DATA_TYPE"
            }
          }
        },
        {
          "task_progress": {
            "label": "验证进度(%)",
            "control": "text",
            "span": 12
          },
          "task_created_user": {
            "label": "创建人",
            "control": "text",
            "span": 12
          }
        },
        {
          "task_created_time": {
            "label": "创建时间",
            "control": "text",
            "span": 12
          },
          "task_updated_user": {
            "label": "最近更新人",
            "control": "text",
            "span": 12
          }
        },
        {
          "task_updated_time": {
            "label": "最近更新时间",
            "control": "text",
            "span": 12
          },
          "task_juece": {
            "label": "决策",
            "control": "select",
            "span": 12,
            "dict": {
              "format": "label",
              "dictKey": [
                "DECI_E_DATA_TYPE"
              ],
              "dictType": "DECI_E_DATA_TYPE"
            }
          }
        },
        {
          "data_type": {
            "label": "沙箱",
            "control": "select",
            "span": 12,
            "dict": {
              "format": "label",
              "dictKey": [
                "DECI_E_DATA_TYPE"
              ],
              "dictType": "DECI_E_DATA_TYPE"
            }
          },
          "description": {
            "label": "报告模板",
            "control": "select",
            "span": 12,
            "dict": {
              "format": "label",
              "dictKey": [
                "DECI_E_DATA_TYPE"
              ],
              "dictType": "DECI_E_DATA_TYPE"
            }
          }
        }
      ],
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": ""
        }
      }
    }
  }
}