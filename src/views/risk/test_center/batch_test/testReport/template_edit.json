{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "controls": [
        {
          "parm_code": {
            "label": "模板代码",
            "control": "text",
            "span": 12,
            "rules": [
              {
                "message": "长度不能超过40个字符",
                "max": 40
              }
            ]
          },
          "parm_name": {
            "label": "模板名称",
            "control": "text",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "长度不能超过100个字符",
                "max": 100
              }
            ]
          }
        },
        {
          "data_type": {
            "label": "模板信息",
            "control": "checkbox",
            "span": 12,
            "options": [
              {
                "label": "基本信息",
                "value": "1"
              },
              {
                "label": "执行日志",
                "value": "2"
              },
              {
                "label": "数据源调用情况",
                "value": "3"
              },
              {
                "label": "变量调用情况",
                "value": "4"
              },
              {
                "label": "测试结果明细",
                "value": "5"
              },
              {
                "label": "结果分布统计",
                "value": "6"
              }
            ]
          },
          "description": {
            "label": "描述",
            "control": "textarea",
            "span": 12,
            "rows": 1,
            "rules": [
              {
                "message": "长度不能超过500个字符",
                "max": 500
              }
            ]
          }
        }
      ],
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "deci050007"
        }
      }
    }
  }
}