{
  "module": "v-search-btn-datagrid",
  "pageId": "risk_rule_constant",
  "layout": {
    "form": {
      "controls": [{
        "parm_code": {
          "label": "执行开始时间",
          "control": "dateTimePicker",
          "span": 8
        },
        "parm_name": {
          "label": "执行结束时间",
          "control": "dateTimePicker",
          "span": 8
        }
      }]
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
            "servicecode": "deci050042"
        }
      },
      "thead": [
        {
        "label": "序号",
        "type": "index"
        },
        {
          "label": "执行开始时间",
          "prop": "parm_code"
        },
        {
          "label": "执行结束时间",
          "prop": "parm_name"
        },
        {
          "label": "执行内容",
          "prop": "description"
        },
        {
          "label": "执行总数量",
          "prop": "org_id"
        },
        {
          "label": "执行人",
          "prop": "data_create_time"
        },
        {
          "label": "平均耗时",
          "prop": "data_update_time"
        }
      ]
    }
  }
}
