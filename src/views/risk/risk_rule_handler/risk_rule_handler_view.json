{
  "module": "v-realonlyForm-btn",
  "layout": {
    "form": {
      "controls": [
        {
          "handlerCode": {
            "label": "处置措施代码",
            "control": "string",
            "span": 12
          },
          "handlerName": {
            "label": "处置措施名称",
            "control": "string",
            "span": 12
          }
        },
        {
          "handlerType": {
            "label": "处置措施类型",
            "control": "string",
            "span": 12,
            "dict": {
              "dictType": "handlerType",
              "dictKey": [],
              "format": "value-label",
              "doRequest": {
                "url": "/views/risk/dictdata/handlerType.json",
                "method": "get"
              }
            }
          },
          "indicatorCode": {
            "label": "管控指标代码",
            "control": "string",
            "span": 12
          }
        },
        {
          "indicatorName": {
            "label": "管控指标名称",
            "control": "string",
            "span": 12
          },
          "controlDimension": {
            "label": "管控维度",
            "control": "string",
            "span": 12,
            "dict": {
              "dictType": "indicatorDimension",
              "dictKey": [],
              "format": "value-label",
              "doRequest": {
                "url": "/views/risk/dictdata/indicatorDimension.json",
                "method": "get"
              }
            }
          }
        },
        {
          "controlUnit": {
            "label": "管控时间单位",
            "control": "string",
            "span": 12,
            "dict": {
              "dictType": "controlUnit",
              "dictKey": [],
              "format": "value-label",
              "doRequest": {
                "url": "/views/risk/dictdata/controlUnit.json",
                "method": "get"
              }
            }
          },
          "controlNumber": {
            "label": "管控时间数值",
            "control": "string",
            "span": 12
          }
        },
        {
          "alertMessageFlag": {
            "label": "是否生成警报",
            "control": "string",
            "span": 12,
            "dict": {
              "dictType": "yesOrNo",
              "dictKey": [],
              "format": "value-label",
              "doRequest": {
                "url": "/views/risk/dictdata/yesOrNo.json",
                "method": "get"
              }
            }
          },
          "alertRiskLevel": {
            "label": "警报风险级别",
            "control": "string",
            "span": 12,
            "dict": {
              "dictType": "",
              "dictKey": [],
              "format": "value-label",
              "doRequest": {
                "url": "/views/risk/dictdata/alertRiskLevel.json",
                "method": "get"
              }
            }
          }
        },
        {
          "priority": {
            "label": "优先级",
            "control": "string",
            "span": 12
          },
          "returnCode": {
            "label": "返回码",
            "control": "string",
            "span": 12
          }
        },
        {
          "returnMessage": {
            "label": "返回信息",
            "control": "string",
            "span": 12
          },
          "smsTemplateCode": {
            "label": "短信模板代码",
            "control": "string",
            "span": 12
          }
        },
        {
          "updateUser": {
            "label": "最后更新人",
            "control": "string",
            "span": 12
          },
          "updateTime": {
            "label": "最后更新时间",
            "control": "string",
            "type": "dateTimePicker",
            "format": "yyyy-MM-dd HH:mm:ss",
            "span": 12
          }
        },
        {
          "alertQueueId": {
            "label": "警报队列",
            "control": "string",
            "span": 12,
            "dict": {
              "dictType": "risk-alert-queue",
              "dictKey": [],
              "format": "value-label",
              "doRequest": {
                "url": "/API/risk-management-service/combobox-select/risk-alert-queue",
                "params": {},
                "method": "POST"
              }
            }
          }
        },
        {
          "description": {
            "label": "备注",
            "control": "string",
            "span": 24
          }
        }
      ],
      "initRequest": "${{params}}"
    }
  }
}
