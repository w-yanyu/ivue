{
  "module": "v-search-btn-datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "collectionIndicatorCode": {
            "label": "统计代码",
            "control": "text",
            "span": 12,
            "rules": [
              {
                "message": "字段长度超限",
                "max": 40
              }
            ]
          },
          "collectionIndicatorName": {
            "label": "统计名称",
            "control": "text",
            "span": 12,
            "rules": [
              {
                "message": "字段长度超限",
                "max": 100
              }
            ]
          }
        }
      ]
    },
    "datagrid": {
      "doRequest": {
        "url": "/API/risk-management-service/risk-collection-indicator/page-select",
        "method": "post",
        "params":{
          "collectionSource": "List"
        }
      },
      "pagination": {
        "pageSizes": [
          10,
          15,
          20,
          50,
          100
        ],
        "pageSize": 10
      },
      "thead": [
        {
          "label": "统计代码",
          "prop": "collectionIndicatorCode",
          "width": "200px",
          "align": "center"
        },
        {
          "label": "统计名称",
          "prop": "collectionIndicatorName"
        },
        {
          "label": "操作",
          "width": "70px",
          "align": "center",
          "btns": [
            {
              "type": "bringback",
              "label": "选择"
            }
          ]
        }
      ]
    }
  }
}
