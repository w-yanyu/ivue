{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "controls": [
        {
          "bpParamTableId": {
            "label": "参数表ID",
            "control": "string",
            "span": 12
          },
          "bpParamMainKey": {
            "label": "参数主键值",
            "control": "string",
            "span": 12
          }
        },
        {
          "bpParamSubKey": {
            "label": "参数次键值",
            "control": "string",
            "span": 12
          },
          "bpParamValue": {
            "label": "参数字符值",
            "control": "text",
            "span": 12,
            "rules": [
              {
                "message": "请正确输入",
                "max": 100
              }
            ]
          }
        },
        {
          "bpParamValueDec": {
            "label": "参数数字值",
            "control": "inputNumber",
            "span": 12,
            "rules": [
              {
                "type": "number",
                "message": "请正确输入",
                "max": 18
              }
            ]
          },
          "bpParamDesc": {
            "label": "参数描述",
            "control": "text",
            "span": 12,
            "rules": [
              {
                "message": "请正确输入",
                "max": 100
              }
            ]
          }
        },
        {
          "bpParamCheckRule": {
            "label": "参数校验规则",
            "control": "text",
            "span": 12,
            "rules": [
              {
                "message": "请正确输入",
                "max": 5
              }
            ]
          }
        }
      ],
      "initRequest": "${{params}}",
      "doRequest": {
        "url": "/API/risk-management-service/risk-param/update",
        "method": "post",
        "params":{
          "updateUser": "$g{{user_info.userCode}}"
        }
      }
    }
  }
}
