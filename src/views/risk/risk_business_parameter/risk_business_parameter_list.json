{
  "module": "v-search-btn-datagrid",
  "pageId": "risk_business_parameter",
  "layout": {
    "form": {
      "controls": [
        {
          "bpParamMainKey": {
            "label": "参数主键值",
            "control": "text",
            "span": 8,
            "rules": [
              {
                "message": "字段长度超限",
                "max": 30
              }
            ]
          },
          "bpParamSubKey": {
            "label": "参数次键值",
            "control": "text",
            "span": 8,
            "rules": [
              {
                "message": "字段长度超限",
                "max": 30
              }
            ]
          },
          "bpParamDesc": {
            "label": "参数描述",
            "control": "text",
            "span": 8,
            "rules": [
              {
                "message": "字段长度超限",
                "max": 100
              }
            ]
          }
        }
      ]
    },
    "toolbar": {
      "btns": []
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/API/risk-management-service/risk-param/page-select",
        "method": "post"
      },
      "thead": [
        {
          "label": "参数主键值",
          "prop": "bpParamMainKey"
        },
        {
          "label": "参数次键值",
          "prop": "bpParamSubKey"
        },
        {
          "label": "参数字符值",
          "prop": "bpParamValue"
        },
        {
          "label": "参数数字值",
          "prop": "bpParamValueDec"
        },
        {
          "label": "参数描述",
          "prop": "bpParamDesc"
        },
        {
          "label": "操作",
          "btns": [
            {
              "type": "edit",
              "authId": "business_param_upd_btn",
              "label": "编缉",
              "toRequest": {
                "url": "/views/risk/risk_business_parameter/risk_business_parameter_edit.json",
                "method": "get"
              },
              "props": {
                "width": "70%"
              }
            },
            {
              "type": "look",
              "label": "查看",
              "toRequest": {
                "url": "/views/risk/risk_business_parameter/risk_business_parameter_view.json",
                "method": "get"
              },
              "props": {
                "width": "70%"
              }
            }
          ]
        }
      ]
    }
  }
}
