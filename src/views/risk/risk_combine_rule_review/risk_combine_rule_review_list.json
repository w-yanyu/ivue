{
  "module": "v-search-btn-datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "cruleId": {
            "label": "规则id",
            "control": "text",
            "span": 8,
            "rules": [
              {
                "message": "字段长度超限",
                "max": 8
              }
            ]
          },
          "cruleNameAfter": {
            "label": "规则名称（新）",
            "control": "text",
            "span": 8,
            "rules": [
              {
                "message": "字段长度超限",
                "max": 40
              }
            ]
          },
          "operType": {
            "label": "操作类型",
            "control": "text",
            "span": 8
          }
        },
        {
          "reviewStatus": {
            "label": "审核状态",
            "control": "text",
            "span": 8
          },
          "applyUser": {
            "label": "申请人",
            "control": "text",
            "span": 8,
            "rules": [
              {
                "message": "字段长度超限",
                "max": 40
              }
            ]
          },
          "applyTime": {
            "label": "申请时间",
            "control": "text",
            "span": 8
          }
        }
      ]
    },
    "toolbar": {
      "btns": []
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/API/risk-management-service/risk-combine-rule-review/page-select",
        "method": "post"
      },
      "thead": [
        {
          "label": "审核编号",
          "prop": "reviewId"
        },
        {
          "label": "规则id",
          "prop": "cruleId"
        },
        {
          "label": "规则名称(旧)",
          "prop": "cruleNameBefore"
        },
        {
          "label": "规则名称（新）",
          "prop": "cruleNameAfter"
        },
        {
          "label": "操作类型",
          "prop": "operType"
        },
        {
          "label": "审核状态",
          "prop": "reviewStatus"
        },
        {
          "label": "申请人",
          "prop": "applyUser"
        },
        {
          "label": "申请时间",
          "prop": "applyTime",
          "type": "dateTimePicker",
          "format": "yyyy-MM-dd HH:mm:ss"
        },
        {
          "label": "审核时间",
          "prop": "reviewTime",
          "type": "dateTimePicker",
          "format": "yyyy-MM-dd HH:mm:ss"
        },
        {
          "label": "操作",
          "btns": [
            {
              "type": "look",
              "label": "查看",
              "toRequest": {
                "url": "/views/risk/risk_combine_rule_review/risk_combine_rule_review_view.json",
                "method": "get"
              },
              "props": {
                "width": "800px"
              }
            }
          ]
        }
      ]
    }
  }
}
