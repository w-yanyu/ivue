{
  "module": "v-realonlyForm-btn",
  "layout": {
    "form": {
      "controls": [
        {
          "reviewId": {
            "label": "审核编号",
            "control": "string",
            "span": 12
          }
        },
        {
          "operType": {
            "label": "操作类型",
            "control": "string",
            "span": 12
          },
          "cruleCode": {
            "label": "规则代码",
            "control": "string",
            "span": 12
          }
        },
        {
          "cruleId": {
            "label": "规则id",
            "control": "string",
            "span": 12
          },
          "cruleVersion": {
            "label": "规则版本",
            "control": "string",
            "span": 12
          }
        },
        {
          "reviewStatus": {
            "label": "审核状态",
            "control": "string",
            "span": 12
          },
          "reviewDescription": {
            "label": "审核意见",
            "control": "string",
            "span": 12
          }
        },
        {
          "reviewUser": {
            "label": "审核人",
            "control": "string",
            "span": 12
          },
          "reviewTime": {
            "label": "审核时间",
            "control": "string",
            "type": "dateTimePicker",
            "format": "yyyy-MM-dd HH:mm:ss",
            "span": 12
          }
        },
        {
          "applyUser": {
            "label": "申请人",
            "control": "string",
            "span": 12
          },
          "applyTime": {
            "label": "申请时间",
            "control": "string",
            "type": "dateTimePicker",
            "format": "yyyy-MM-dd HH:mm:ss",
            "span": 12
          }
        },
        {
          "cruleNameBefore": {
            "label": "规则名称(旧)",
            "control": "string",
            "span": 12
          },
          "cruleNameAfter": {
            "label": "规则名称（新）",
            "control": "string",
            "span": 12
          }
        },
        {
          "natureExpressionBefore": {
            "label": "规则自然语言表达式(旧)",
            "control": "string",
            "span": 12
          },
          "natureExpressionAfter": {
            "label": "规则自然语言表达式(新)",
            "control": "string",
            "span": 12
          }
        },
        {
          "executableExpressionBefore": {
            "label": "规则可执行表达式（旧）",
            "control": "string",
            "span": 12
          },
          "executableExpressionAfter": {
            "label": "规则可执行表达式（新）",
            "control": "string",
            "span": 12
          }
        },
        {
          "descriptionBefore": {
            "label": "备注(旧)",
            "control": "string",
            "span": 12
          },
          "descriptionAfter": {
            "label": "备注(新)",
            "control": "string",
            "span": 12
          }
        }
      ],
      "initRequest": "${{params}}"
    }
  }
}
