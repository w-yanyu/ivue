{
  "module": "v_search_btn_datagrid",
  "pageId": "risk_habit_model",
  "layout": {
    "form": {
      "controls": [
        {
          "habitModelId": {
            "label": "行为模型ID",
            "control": "text",
            "span": 8,
            "rules": [
              {
                "max": 20,
                "message": "请正确输入"
              }
            ]
          },
          "habitModelName": {
            "label": "行为模型名称",
            "control": "text",
            "span": 8,
            "rules": [
              {
                "max": 100,
                "message": "请正确输入"
              }
            ]
          },
          "referenceStatisticCode": {
            "label": "关联统计代码",
            "control": "select",
            "span": 8,
            "dict": {
              "dictType": "risk-indicator-statistic",
              "dictKey": [],
              "format": "value-label",
              "doRequest": {
                "url": "/API/risk-management-service/combobox-select/risk-indicator-statistic/Batch",
                "method": "POST"
              }
            }
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "authId": "habit_model_add_btn",
          "label": "新增",
          "toRequest": {
            "url": "/views/risk/risk_habit_model/risk_habit_model_add.json",
            "method": "get",
            "params": {}
          },
          "props": {
            "width": "70%",
            "title": "新增"
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/API/risk-management-service/risk-habit-model/findByPageAndType",
        "method": "post"
      },
      "thead": [
        {
          "label": "行为模型ID",
          "prop": "habitModelId"
        },
        {
          "label": "行为模型名称",
          "prop": "habitModelName"
        },
        {
          "label": "关联统计代码",
          "prop": "referenceStatisticCode",
          "dict": {
            "dictType": "risk-indicator-statistic",
            "dictKey": [],
            "format": "label",
            "doRequest": {
              "url": "/API/risk-management-service/combobox-select/risk-indicator-statistic/Batch",
              "method": "POST"
            }
          }
        },
        {
          "label": "习惯阀值类型",
          "prop": "thresholdType",
          "dict": {
            "dictType": "thresholdType",
            "dictKey": [],
            "format": "label",
            "doRequest": {
              "url": "/views/risk/dictdata/thresholdType.json",
              "method": "get"
            }
          }
        },
        {
          "label": "习惯阀值",
          "prop": "threshold"
        },
        {
          "label": "操作",
          "width": "150px",
          "btns": [
            {
              "type": "edit",
              "authId": "habit_model_upd_btn",
              "label": "编缉",
              "toRequest": {
                "url": "/views/risk/risk_habit_model/risk_habit_model_edit.json",
                "method": "get"
              },
              "props": {
              
              }
            },
            {
              "type": "look",
              "label": "查看",
              "toRequest": {
                "url": "/views/risk/risk_habit_model/risk_habit_model_view.json",
                "method": "get"
              },
              "props": {
                
              }
            },
            {
              "type": "delete",
              "authId": "habit_model_del_btn",
              "label": "删除",
              "doRequest": {
                "url": "/API/risk-management-service/risk-habit-model/deleteByPrimaryKey",
                "method": "post"
              }
            }
          ]
        }
      ],
      "pagination": {
        "show": true,
        "pageSizes": [
          "10",
          "50",
          "100",
          "200"
        ],
        "pageSize": 10
      }
    }
  }
}
