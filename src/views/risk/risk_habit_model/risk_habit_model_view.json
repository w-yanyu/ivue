{
  "module": "v_realonlyForm_btn",
  "layout": {
    "form": {
      "controls": [
        {
          "habitModelId": {
            "label": "行为模型ID",
            "control": "string",
            "span": 12
          },
          "habitModelName": {
            "label": "行为模型名称",
            "control": "string",
            "span": 12
          }
        },
        {
          "referenceStatisticCode": {
            "label": "关联统计代码",
            "control": "string",
            "span": 12,
            "dict": {
              "dictType": "risk-indicator-statistic",
              "dictKey": [],
              "format": "value-label",
              "doRequest": {
                "url": "/API/risk-management-service/combobox-select/risk-indicator-statistic/Batch",
                "method": "POST"
              }
            }
          },
          "thresholdType": {
            "label": "习惯阀值类型",
            "control": "string",
            "span": 12,
            "dict": {
              "dictType": "thresholdType",
              "dictKey": [],
              "format": "value-label",
              "doRequest": {
                "url": "/views/risk/dictdata/thresholdType.json",
                "method": "get"
              }
            }
          }
        },
        {
          "threshold": {
            "label": "习惯阀值",
            "control": "string",
            "span": 12
          },
          "createUser": {
            "label": "创建人",
            "control": "string",
            "span": 12
          }
        },
        {
          "createTime": {
            "label": "创建时间",
            "control": "string",
            "span": 12,
            "type": "dateTimePicker",
            "format": "yyyy-MM-dd HH:mm:ss"
          },
          "updateUser": {
            "label": "最后修改人",
            "control": "string",
            "span": 12
          }
        },
        {
          "updateTime": {
            "label": "最后修改时间",
            "control": "string",
            "span": 12,
            "type": "dateTimePicker",
            "format": "yyyy-MM-dd HH:mm:ss"
          }
        }
      ],
      "initRequest": {
        "url": "/API/risk-management-service/risk-habit-model/selectByPrimaryKey",
        "method": "post"
      }
    }
  }
}
