{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "doRequest": {
        "url":"/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "adcopr"
        }
      },
      "btns": [
        {
          "type": "submit",
          "label": "提交"
        }
      ],
      "controls": [
        {
          "opcred": {
            "control": "select",
            "label": "合作方证件类型：",
            "span": "18",
            "dict": {
              "dictType": "E_IDTFTP",
              "dictKey": [
                "E_IDTFTP"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "合作方证件类型不能为空"
              }
            ]
          }
        },
        {
          "cmcrno": {
            "control": "text",
            "label": "合作方证件号码：",
            "span": "18",
            "rules": [
              {
                "min": 8,
                "max": 20,
                "message": "证件号码长度应该为8到20位"
              },
              {
                "required": true,
                "message": "请输入证件号码"
              }
            ]
          }
        },
        {
          "ofpana": {
            "control": "text",
            "label": "合作方名称",
            "span": "18",
            "rules": [
              {
                "required": true,
                "message": "合作方名称不能为空"
              }
            ]
          }
        },
        {
          "provsc": {
            "control": "currency",
            "label": "省级规模金额(元)",
            "span": "18",
            "rules": [
              {
                "required": true,
                "message": "省级规模金额不能为空"
              }
            ]
          }
        },
        {
          "cosina": {
            "control": "text",
            "label": "合作方简称",
            "span": "18",
            "rules": [
              {
                "required": true,
                "message": "合作方简称不能为空"
              }
            ]
          }
        }
      ]
    }
  }
}
