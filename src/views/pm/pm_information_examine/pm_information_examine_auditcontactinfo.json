{
  "module": "v-realonlyForm-btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "qrconn"
        }
      },
      "controls": [
        {
          "connna": {
            "control": "string",
            "label": "合作方联系人名称",
            "width": "170px",
            "rules": [
              {
                "max": 80,
                "message": "长度不能超过80个字符"
              },
              {
                "required": true,
                "message": "请输入合作方联系人名称"
              }
            ]
          },
          "cncrtp": {
            "control": "select",
            "label": "合作方联系人证件类型",
            "disabled": true,
            "required": false,
            "width": "170px",
            "rules": [
              {
                "max": 3,
                "message": "长度不能超过3个字符"
              }
            ],
            "dict": {
              "dictType": "E_IDTFTP",
              "dictKey": [
                "E_IDTFTP"
              ],
              "format": "label"
            }
          }
        },
        {
          "cncrno": {
            "control": "string",
            "label": "合作方联系人证件号码",
            "required": false,
            "width": "170px",
            "rules": [
              {
                "max": 20,
                "message": "长度不能超过20个字符"
              }
            ]
          },
          "connph": {
            "control": "string",
            "label": "合作方联系人手机号",
            "width": "170px",
            "rules": [
              {
                "max": 20,
                "message": "长度不能超过20个字符"
              },
              {
                "required": true,
                "message": "请输入合作方联系人手机号"
              }
            ]
          }
        },
        {
          "contel": {
            "control": "string",
            "label": "合作方联系人固定电话",
            "width": "170px",
            "rules": [
              {
                "max": 20,
                "message": "长度不能超过20个字符"
              }
            ]
          },
          "confax": {
            "control": "string",
            "label": "合作方联系人传真号码",
            "width": "170px",
            "rules": [
              {
                "max": 20,
                "message": "长度不能超过20个字符"
              }
            ]
          }
        },
        {
          "coneml": {
            "control": "string",
            "label": "合作方联系人电子邮箱",
            "width": "170px",
            "rules": [
              {
                "max": 40,
                "message": "长度不能超过40个字符"
              }
            ]
          },
          "conjob": {
            "control": "select",
            "label": "合作方联系人职务",
            "disabled": true,
            "width": "170px",
            "rules": [
              {
                "max": 1,
                "message": "长度不能超过1个字符"
              }
            ],
            "dict": {
              "dictType": "E_CONJOB",
              "dictKey": [
                "E_CONJOB"
              ],
              "format": "label"
            }
          }
        }
      ]
    }
  }
}
