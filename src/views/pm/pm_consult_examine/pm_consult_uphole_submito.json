{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "chagin",
          "ofpacd": "${{params.ofpacd}}",
          "seqnum": "${{params.seqnum}}",
          "statusxy":"03"
        }
      },
      "btns":[
        {
          "type":"submit",
          "label":"提交"
        }
      ],
      "controls": [
        {
          "subtyp": {
            "control": "select",
            "label": "审核",
            "filterable": false,
            "options": [
              {
                "label": "审核通过",
                "value": "1"
              },
              {
                "label": "审核退回",
                "value": "2"
              }
            ]
          }
        },
        {
          "reason": {
            "control": "textarea",
            "label": "审核建议",
            "placeholder":"请输入"
          }
        }
      ]
    }
  }
}
