{
  "module": "v-realonlyForm-btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "qraddf"
        }
      },
      "controls": [
        {
          "regadd": {
            "control": "string",
            "label": "注册地址",
            "width": "160px",
            "rules": [
              {
                "max": 80,
                "message": "长度不能超过80个字符"
              },
              {
                "required": false,
                "message": "请输入注册地址"
              }
            ]
          },
          "pstcod": {
            "control": "string",
            "label": "注册地址邮编",
            "width": "160px",
            "rules": [
              {
                "max": 6,
                "message": "长度不能超过6个字符"
              },
              {
                "required": false,
                "message": "请输入注册地址邮编"
              }
            ]
          }
        },
        {
          "busadd": {
            "control": "string",
            "label": "经营地址",
            "width": "160px",
            "rules": [
              {
                "max": 20,
                "message": "长度不能超过20个字符"
              },
              {
                "required": false,
                "message": "请输入经营地址"
              }
            ]
          },
          "buscod": {
            "control": "string",
            "label": "经营地址邮编",
            "width": "160px",
            "rules": [
              {
                "max": 20,
                "message": "长度不能超过20个字符"
              },
              {
                "required": false,
                "message": "请输入经营地址邮编"
              }
            ]
          }
        }
      ]
    }
  }
}
