{
  "module": "lr_tree_dynamic",
  "layout": {
    "initReqeust": "${{params}}",
    "tree": {
      "scope": "productParam",
      "title": "伙伴体系",
      "lazy": false,
      "idField": "nodeid",
      "pIdField": "prndid",
      "labelField": "nodena",
      "isLeafField": "isleaf",
      "nodeTypeField": "nodelv",
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "qrstrt",
          "qrtype":"1"
        }
      },
      "#root": {
        "rootNode": {
          "name": "伙伴体系",
          "value": "-1",
          "icon": "xitongguanli",
          "disabled": true
        }
      },
      "nodeTypes": {
        "1": {
          "icon": "el-icon-news",
          "children": [
            "nodeid",
            "nodena"
          ],
          "toRequest": {
            "method": "get",
            "url": "/views/pm/pm_information_uphold/pm_information_uphold_partstrsee.json",
            "params": {
              "operater_ind": "M",
              "nodeid": "${{productParam.nodeid}}",
              "prndid": "${{productParam.prndid}}",
              "qrtype":"1"
            }
          },
          "props": {
            "title": "合作伙伴体系详情"
          },
          "#contextMenu": [
            {
              "type": "add",
              "label": "下级合作伙伴新增",
              "childNodeType": "2",
              "icon": "el-icon-circle-plus-outline",
              "toRequest": {
                "url": "/views/pm/pm_information_uphold/pm_information_uphold_partstradd1.json",
                "target": "dialog",
                "props": {
                  "title": "下级合作伙伴体系新增"
                }
              }
            }
          ]
        },
        "2": {
          "icon": "el-icon-news",
          "children": [
            "nodeid",
            "nodena"
          ],
          "toRequest": {
            "method": "get",
            "url": "/views/pm/pm_information_uphold/pm_information_uphold_partstrsee.json",
            "params": {
              "operater_ind": "M",
              "nodeid": "${{productParam.nodeid}}",
              "prndid": "${{productParam.prndid}}"
            }
          },
          "props": {
            "title": "合作伙伴体系详情"
          },
          "#contextMenu": [
            {
              "type": "delete",
              "label": "删除当前节点",
              "childNodeType": "2",
              "icon": "el-icon-remove-outline",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "delstr"
                }
              },
              "confirm": {
                "title": "提示信息",
                "msg": "确定删除该目录吗?",
                "confirmButtonText": "确定",
                "cancelButtonText": "取消"
              }
            },
            {
              "type": "add",
              "label": "下级合作伙伴新增",
              "childNodeType": "2",
              "icon": "el-icon-circle-plus-outline",
              "toRequest": {
                "url": "/views/pm/pm_information_uphold/pm_information_uphold_partstradd1.json",
                "target": "dialog",
                "props": {
                  "title": "下级合作伙伴体系新增"
                }
              }
            }
          ]
        },
        "3": {
          "icon": "el-icon-news",
          "children": [
            "nodeid",
            "nodena"
          ],
          "toRequest": {
            "method": "get",
            "url": "/views/pm/pm_information_uphold/pm_information_uphold_partstrsee.json",
            "params": {
              "operater_ind": "M",
              "nodeid": "${{productParam.nodeid}}",
              "prndid": "${{productParam.prndid}}"
            }
          },
          "props": {
            "title": "合作伙伴体系详情"
          },
          "#contextMenu": [
            {
              "type": "delete",
              "label": "删除当前节点",
              "childNodeType": "3",
              "icon": "el-icon-remove-outline",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "delstr"
                }
              },
              "confirm": {
                "title": "提示信息",
                "msg": "确定删除该目录吗?",
                "confirmButtonText": "确定",
                "cancelButtonText": "取消"
              }
            },
            {
              "type": "add",
              "label": "下级合作伙伴新增",
              "childNodeType": "3",
              "icon": "el-icon-circle-plus-outline",
              "toRequest": {
                "url": "/views/pm/pm_information_uphold/pm_information_uphold_partstradd1.json",
                "target": "dialog",
                "props": {
                  "title": "下级合作伙伴体系新增"
                }
              }
            }
          ]
        }
      }
    }
  }
}