{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initDataOnce":true,
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "AS_IPMS_PT_QREXPO",
          "cortno": "${{params.cortno}}"
        }
      },
      "btns": [
        {
          "type": "submit",
          "label": "保存",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "AS_IPMS_PT_UPEXPO"
            }
          }
        }
      ],
      "controls": [
        {
          "cortno": {
            "control": "text",
            "label": "协议编号",
            "width": "120px",
            "span": 12,
            "disabled": "true",
            "value": "${{params.cortno}}"
          },
          "ofpacd": {
            "control": "text",
            "label": "合作商编号",
            "width": "120px",
            "span": 12,
            "disabled": "true",
            "value": "${{params.ofpacd}}"
          },
          "crtdcd": {
            "control": "text",
            "label": "可售产品编号",
            "width": "120px",
            "span": 12,
            "disabled": "true",
            "value": "${{params.crtdcd}}"
          }
        },
        {
          "profit": {
            "control": "select",
            "label": "是否分润",
            "width": "120px",
            "span": 12,
            "dict": {
              "format": "label",
              "dictKey": [
                  "E_BOOLEAN"
              ],
              "dictType": "E_BOOLEAN"
            },
            "value": "${{params.profit}}"
          },
          "commis": {
            "control": "select",
            "label": "是否分佣",
            "width": "120px",
            "span": 12,
