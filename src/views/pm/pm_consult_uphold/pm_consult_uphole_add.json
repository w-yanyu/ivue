{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "ofpana": {
            "control": "text",
            "label": "合作方名称",
            "rules": [
              {
                "max": 20,
                "message": "长度不能超过10个字符"
              }
            ]
          },
          "cmcrno": {
            "control": "text",
            "label": "合作方证件号码",
            "rules": [
              {
                "max": 100,
                "message": "长度不能超过100个字符"
              }
            ]
          }

        }

      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "qrfoal",

          "page_id": "/views/pm/pm_consult_uphold/pm_consult_add.json"
        }
      },
      "thead": [
        {
          "type": "index"
        },
        {
          "label": "合作方名称",
          "prop": "ofpana",
          "align":"center"
        },
        {
          "label": "合作方证件类型",
          "prop": "opcred",
          "align":"center",
          "dict": {
            "dictType": "E_IDTFTP",
            "dictKey": [
              "E_IDTFTP"
            ],
            "format": "label"
          }
        },
        {
          "label": "合作方证件号码",
          "prop": "cmcrno",
          "align":"center"
        },
        {
          "label": "所属机构",
          "prop": "orgnam",
          "align":"center"
        },
        {
          "label": "操作",
          "fixed": "right",
          "width": 200,
          "align":"center",
          "btns": [


            {
              "type": "look",
              "label": "选择",
              "toRequest": {
                "url": "/views/pm/pm_consult_uphold/pm_consult_uphole_chouse.json",
                "method": "POST"
              },
              "props": {
                "width": "75%",
                "title": "新增协议信息"
              }
            }

          ]
        }
      ]
    }
  }
}
