{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "adcoag",
              "blbrch": "${{params.belorg}}",
              "cmcrno": "${{params.cmcrno}}",
              "opcred": "${{params.opcred}}",
              "crdtcd": ""

            }
          }
        }
      ],

      "controls": [
        {
          "cortno": {
            "control": "text",
            "label": "合作方编号",
            "width": "100px",
            "disabled": true,
            "value": "${{params.ofpacd}}",
            "rules": [
              {
                "required": true,
                "message": "合作方编号不得为空"
              },
              {
                "max": 100,
                "message": "长度不能超过100个字符"
              }
            ]
          }

        },
        {
          "pronam": {
            "control": "text",
            "label": "协议名称",
            "width": "100px",
            "value": "${{params.pronam}}",
            "rules": [
              {
                "required": true,
                "message": "协议名称不得为空"
              },
              {
                "max": 50,
                "message": "长度不能超过50个字符"
              }
            ]
          },
          "procor": {
            "control": "select",
            "label": "法人",
            "width": "100px",
            "value": "${{params.procor}}",
            "rules": [
              {
                "required": true,
                "message": "法人名称不得为空"
              }

            ],
            "dict": {
              "dictType": "E_LEGALER",
              "dictKey": [
                "E_LEGALER"
              ],
              "format": "label"
            }
          }
        },
        {
          "effedt": {
            "control": "dateTimePicker",
            "label": "生效日期",
            "width": "100px",
            "value": "${{effedt}}",
            "rules": [
              {
                "required": true,
                "message": "生效日期不得为空"
              }
            ]
          },
          "exprdt": {
            "control": "dateTimePicker",
            "label": "失效日期",
            "width": "100px",
            "value": "${{exprdt}}",
            "rules": [
              {
                "required": true,
                "message": "失效日期不得为空"
              }

            ]
          }
        }
      ]
    }
  }
}
