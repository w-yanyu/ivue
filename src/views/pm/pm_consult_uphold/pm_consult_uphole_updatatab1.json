{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {

      "initDataOnce":true,
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "qrcoag",
          "seqnum": "${{params.seqnum}}"
        }
      },
      "btns": [
        {
          "type": "submit",
          "label": "保存",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "upcoag",
              "seqnum": "${{params.seqnum}}"
            }
          }
        }
      ],
      "controls": [
        {
          "cortno1": {
            "control": "text",
            "label": "合作方编号",
            "width": "50%",
            "disabled": true,
            "value": "${{params.ofpacd}}",
            "rules": [
              {
                "required": true,
                "message": "合作方编号"
              },
              {
                "max": 100,
                "message": "长度不能超过10个字符"
              }
            ]
          },
          "ofpana": {
            "control": "text",
            "label": "合作方名称",
            "width": "50%",
            "disabled": true,
            "value": "${{params.ofpana}}",
            "rules": [
              {
                "required": true,
                "message": "合作方名称"
              },
              {
                "max": 100,
                "message": "长度不能超过100个字符"
              }
            ]
          }

        },
        {
          "pronam": {
            "control": "text",
            "label": "协议名称",
            "width": "50%",
            "value": "${{params.pronam}}",
            "rules": [
              {
                "required": true,
                "message": "协议名称不得为空"
              },
              {
                "max": 30,
                "message": "长度不能超过30个字符"
              }
            ]
          },
          "procor": {
            "control": "select",
            "label": "法人",
            "width": "50%",
            "value": "${{params.procor}}",
            "rules": [
              {
                "required": true,
                "message": "请输入法人名称"
              }

            ],
            "dict": {
              "dictType": "E_LEGALER",
              "dictKey": [
                "E_LEGALER"
              ],
              "format": "label"
            }
          }
        },
        {
          "cortno": {
            "disabled": true,
            "control": "text",
            "label": "合同编号",
            "width": "50%",
            "value": "${{params.cortno}}",
            "rules": [
              {
                "required": true,
                "message": "合同编号不得为空"
              }
            ]
          },
          "cmcrno": {
            "control": "text",
            "disabled": true,
            "label": "合作方证件号码",
            "width": "50%",
            "value": "${{params.cmcrno}}",
            "rules": [
              {
                "required": true,
                "message": "合作方证件号码不得为空"
              }

            ]
          }
        },
        {
          "opcred": {
            "control": "select",
            "disabled": true,
            "label": "合作方证件类型",
            "width": "50%",
            "value": "${{params.opcred}}",
            "rules": [
              {
                "required": true,
                "message": "合作方证件类型不得为空"
              }
            ],
            "dict": {
              "dictType": "E_IDTFTP",
              "dictKey": [
                "E_IDTFTP"
              ],
              "format": "label"
            }

          },
          "gmt_create": {
            "control": "dateTimePicker",
            "label": "创建日期",
            "width": "50%",
            "rules": [
              {
                "required": false,
                "message": "生效日期不得为空"
              }
            ]
          }
        },
        {
          "effedt": {
            "control": "dateTimePicker",
            "label": "生效日期",
            "width": "50%",
            "value": "${{params.effedt}}",
            "rules": [
              {
                "required": true,
                "message": "生效日期不得为空"
              }
            ]
          },
          "exprdt": {
            "control": "dateTimePicker",
            "label": "失效日期",
            "width": "50%",
            "value": "${{params.exprdt}}",
            "rules": [
              {
                "required": true,
                "message": "请输入失效日期"
              }

            ]
          }
        },
        {

          "offsta": {
            "control": "select",
            "disabled": true,
            "label": "状态",
            "width": "50%",
            "value": "${{params.offsta}}",
            "rules": [
              {
                "required": true,
                "message": "状态不得为空"
              }
            ],
            "dict": {
              "dictType": "E_OFFSTA",
              "dictKey": [
                "E_OFFSTA"
              ],
              "format": "label"
            }

          },

          "blbrch": {
            "control": "select",
            "label": "所属机构",
            "width": "50%",
            "value": "${{params.blbrch}}",
            "rules": [
              {
                "required": true,
                "message": "请输入所属机构"
              }
            ],
            "dict": {
              "dictType": "E_BLBRCH",
              "dictKey": [
                "E_BLBRCH"
              ],
              "format": "label"
            }
          }
        }
      ]
    }
  }
}
