{
  "module": "dynamicTabs",
  "layout": {
    "tabs": {
      "tabHeader": [
        {
          "id": "tab1",
          "label": "基本信息展示",
          "closable": false,
          "disabled": false,
          "icon": "el-icon-date",
          "toRequest": {
            "url": "/views/pm/pm_consult_see/pm_consult_see_tabs_baseInfo_update.json",
            "method": "post"
          }
        },
        {
          "id": "tab2",
          "label": "规模管理信息",
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "toRequest": {
            "url": "/views/pm/pm_consult_see/pm_consult_see_tabs_scale_update.json",
            "method": "post"
          }
        },
        {
          "id": "tab3",
          "label": "提交审核",
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "toRequest": {
            "url":"/views/pm/pm_consult_see/pm_consult_commit_audit.json",
            "method": "post",
            "params": {
              "ofpacd": "${{params.ofpacd}}"
            }
          }
        }
      ],
      "defaultActiveKey": "tab1",
      "position": "top",
      "styleClass": "card"
    }
  }
}

