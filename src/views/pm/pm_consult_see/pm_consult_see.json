{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "ofpana": {
            "control": "text",
            "label": "合作方名称",
            "rules": [
              {
                "max": 80,
                "message": "长度不能超过80个字符"
              }
            ]
          },
          "cmcrno": {
            "control": "text",
            "label": "合作方证件号码",
            "rules": [
              {
                "max": 50,
                "message": "长度不能超过50个字符"
              }
            ]
          },
          "ofpacd": {
            "control": "text",
            "label": "合作方编号",
            "rules": [
              {
                "max": 30,
                "message": "长度不能超过30个字符"
              }
            ]
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "qralcf",
          "page_id": "/views/pm/pm_consult_see/pm_consult_see.json"
        }
      },
      "thead": [
        {
          "label": "序号",
          "type" :  "index"
        },
        {
          "label": "合作方名称",
          "prop": "ofpana"
        },
        {
          "label": "合作方协议名称",
          "prop": "pronam"
        },
        {
          "label": "合作方编号",
          "prop": "ofpacd"
        },
        {
          "label": "合作方证件类型",
          "prop": "opcred",
          "dict": {
            "dictType": "E_IDTFTP",
            "dictKey": [
              "E_IDTFTP"
            ],
            "format": "label"
          }
        },
        {
          "label": "合作方证件号码",
          "prop": "cmcrno"
        },
        {
          "label": "合同编号",
          "prop": "cortno"
        },
        {
          "label": "所属机构",
          "prop": "blbrch"
        },
        {
          "label": "生效日期",
          "prop": "effedt"
        },
        {
          "label": "失效日期",
          "prop": "exprdt"
        },
        {
          "label": "状态",
          "prop": "offsta",
          "dict": {
            "dictType": "E_OFFSTA",
            "dictKey": [
              "E_OFFSTA"
            ],
            "format": "label"
          }
        },
        {
          "label": "创建操作员",
          "prop": "tranus"
        },
        {
          "label": "创建日期",
          "prop": "gmt_create"
        },
        {
          "label": "操作",
          "fixed": "right",
          "width": 200,
          "align":"center",
          "btns": [
            {
              "type": "look",
              "label": "详情",
              "toRequest": {
                "url": "/views/pm/pm_consult_see/pm_consult_see_tabs.json",
                "method": "POST"
              },
              "props": {
                "width": "80%",
                "title": "协议信息详细"
              }
            },
            {
              "type": "look",
              "label": "审核记录",
              "toRequest": {
                "url": "/views/pm/pm_consult_uphold/pm_consult_uphole_see.json",
                "method": "POST"
              },
              "props": {
                "width": "65%",
                "title": "审核记录"
              }
            }
          ]
        }
      ]
    }
  }
}
