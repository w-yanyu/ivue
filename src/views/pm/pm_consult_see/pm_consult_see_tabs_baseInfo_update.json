{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "scope": "templateScope",
      "initDataOnce":true,
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "qrcoag",
          "seqnum": "${{params.seqnum}}",
          "type": 1
        }
      },
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "page_id": "/views/pm/pm_consult_see/pm_consult_see_tabs_baseInfo_update.json",
          "servicecode": "upcoag",
          "seqnum": "${{params.seqnum}}"
        }
      },
      "btns":[
        {
          "type": "submit",
          "label": "保存"
        }
      ],
      "controls": [
        {
          "ofpacd": {
            "control": "text",
            "label": "合作方编号:",
            "disabled": true
          },
          "ofpana": {
            "control": "text",
            "label": "合作方名称:",
            "disabled": true
          }
        },
        {
          "pronam": {
            "control": "text",
            "label": "协议名称:",
            "rules": [
              {
                "required": true,
                "message": "请输入姓名"
              }
            ]
          },
          "procor": {
            "control": "text",
            "label": "法人",
            "value": "985",
            "multiple": false,
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          }
        },
        {
          "cortno": {
            "control": "text",
            "label": "合同编号:",
            "disabled": true
          },
          "cmcrno": {
            "control": "text",
            "label": "合作方证件号码:",
            "disabled": true
          }
        },
        {
          "opcred": {
            "control": "select",
            "label": "合作方证件类型:",
            "disabled": true,
            "dict": {
              "dictType": "E_IDTFTP",
              "dictKey": [
                "E_IDTFTP"
              ],
              "format": "label"
            }
          },
          "gmt_create": {
            "control": "dateTimePicker",
            "label": "创建日期:",
            "format": "yyyyMMdd"
          }
        },
        {
          "effedt": {
            "control": "dateTimePicker",
            "label": "生效日期:",
            "format": "yyyyMMdd"
          },
          "exprdt": {
            "control": "dateTimePicker",
            "label": "失效日期:",
            "format": "yyyyMMdd"
          }
        },
        {
          "offsta": {
            "control": "select",
            "label": "状态:",
            "value": "${{params.offsta}}",
            "disabled": true,
            "dict": {
              "dictType": "E_OFFSTA",
              "dictKey": [
                "E_OFFSTA"
              ],
              "format": "label"
            }
          },
          "blbrch": {
            "control": "text",
            "label": "所属机构",
            "value": "${{params.blbrch}}",
            "disabled": true
          }
        }
      ]
    }
  }
}
