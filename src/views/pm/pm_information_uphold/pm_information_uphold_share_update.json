                "E_YES_"
              ],
              "format": "label"
            }
          },
          "commission_max": {
            "control": "text",
            "label": "分佣最大值",
            "span":6,
            "rules": [
              {
                "max": 17,
                "message": "长度不能超过17个字符"
              },
              {
                "required": false,
                "message": "请输入分润最大值"
              }
            ]
          } ,
          "commission_min_flag": {
            "control": "select",
            "label": "是否分佣是否限定分佣最小值",
            "span": 6,
            "value":"N",
            "rules": [
              {
                "required": true,
                "message": "请选择是否限定分佣最小值"
              }
            ],
            "dict": {
              "dictType": "E_YES_",
              "dictKey": [
                "E_YES_"
              ],
              "format": "label"
            }
          },
          "commission_min": {
            "control": "text",
            "label": "分佣最小值",
            "span":6,
            "rules": [
              {
                "max": 17,
                "message": "长度不能超过17个字符"
              },
              {
                "required": false,
                "message": "请输入分佣最大值"
              }
            ]
          } 
        },
        {
          "profit_max_flag": {
            "control": "select",
            "label": "是否限定分润最大值",
            "span": 6,
            "value":"N",
            "rules": [
              {
                "required": true,
                "message": "请选择是否限定分润最大值"
              }
            ],
            "dict": {
              "dictType": "E_YES_",
              "dictKey": [
                "E_YES_"
              ],
              "format": "label"
            }
          },
          "profit_max": {
            "control": "text",
            "label": "分润最大值",
            "span":6,
            "rules": [
              {
                "max": 17,
                "message": "长度不能超过17个字符"
              },
              {
                "required": false,
                "message": "请输入分润最大值"
              }
            ]
          } ,
          "profit_min_flag": {
            "control": "select",
            "label": "是否限定分润最小值",
            "span": 6,
            "value":"N",
            "rules": [
              {
                "required": true,
                "message": "请选择是否限定分润最小值"
              }
            ],
            "dict": {
              "dictType": "E_YES_",
              "dictKey": [
                "E_YES_"
              ],
              "format": "label"
            }
          },
          "profit_min": {
            "control": "text",
            "label": "分润最小值",
            "span":6,
            "rules": [
              {
                "max": 17,
