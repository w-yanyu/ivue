{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "scope":"strForm",
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "qrSharePpsl"
        }
      },
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "saveSharePpsl",
          "share":{
            "ofpacd":"${{strForm.ofpacd}}",
            "ofpana":"${{strForm.ofpana}}",
            "commission_flag":"${{strForm.commission_flag}}",
            "commission_rule":"${{strForm.commission_rule}}",
            "commission_value":"${{strForm.commission_value}}",
            "commission_max_flag":"${{strForm.commission_max_flag}}",
            "commission_max":"${{strForm.commission_max}}",
            "commission_min_flag":"${{strForm.commission_min_flag}}",
            "commission_min":"${{strForm.commission_min}}",
            "profit_flag":"${{strForm.profit_flag}}",
            "profit_rule":"${{strForm.profit_rule}}",
            "profit_value":"${{strForm.profit_value}}",
            "profit_max_flag":"${{strForm.profit_max_flag}}",
            "profit_max":"${{strForm.profit_max}}",
            "profit_min_flag":"${{strForm.profit_min_flag}}",
            "profit_min":"${{strForm.profit_min}}"
          }
        }
      },
      "controls": [
        {
          "ofpacd": {
            "control": "string",
            "label": "合作商编号",
            "span":8,
            "rules": [
              {
                "max": 30,
                "message": "长度不能超过30个字符"
              },
              {
                "required": true,
                "message": "请输入合作商编号"
              }
            ]
          },
          "ofpana": {
            "control": "string",
            "label": "合作方名称",
            "span":16,
            "rules": [
              {
                "max": 100,
                "message": "长度不能超过100个字符"
              },
              {
                "required": true,
                "message": "请输入节点层级"
              }
            ]
          }
        },
        {
          "commission_flag": {
            "control": "select",
            "label": "是否分佣",
            "span": 8,
            "value":"N",
            "rules": [
              {
                "required": true,
                "message": "请选择是否分佣"
              }
            ],
            "dict": {
              "dictType": "E_YES_",
              "dictKey": [
                "E_YES_"
              ],
              "format": "label"
            }
          },
          "commission_rule": {
            "control": "select",
            "label": "分佣规则",
            "span": 8,
            "rules": [
              {
                "required": false,
                "message": "请选择是否分佣"
              }
            ],
            "dict": {
              "dictType": "E_SHARE_RULE",
              "dictKey": [
                "E_SHARE_RULE"
              ],
              "format": "label"
            }
          },
          "commission_value": {
            "control": "text",
            "label": "分佣设定值",
            "span":8,
            "rules": [
              {
                "max": 17,
                "message": "长度不能超过17个字符"
              },
              {
                "required": false,
                "message": "请输入分佣设定值"
              }
            ]
          } 
        },
        {
          "profit_flag": {
            "control": "select",
            "label": "是否分润",
            "span": 8,
            "value":"N",
            "rules": [
              {
                "required": true,
                "message": "请选择是否分润"
              }
            ],
            "dict": {
              "dictType": "E_YES_",
              "dictKey": [
                "E_YES_"
              ],
              "format": "label"
            }
          },
          "profit_rule": {
            "control": "select",
            "label": "分润规则",
            "span": 8,
            "rules": [
              {
                "required": false,
                "message": "请选择是否分佣"
              }
            ],
            "dict": {
              "dictType": "E_SHARE_RULE",
              "dictKey": [
                "E_SHARE_RULE"
              ],
              "format": "label"
            }
          },
          "profit_value": {
            "control": "text",
            "label": "分润设定值",
            "span":8,
            "rules": [
              {
                "max": 17,
                "message": "长度不能超过17个字符"
              },
              {
                "required": false,
                "message": "请输入分佣设定值"
              }
            ]
          } 
        },
        {
          "commission_max_flag": {
            "control": "select",
            "label": "是否限定分佣最大值",
