{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "scope":"strForm",
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "qrnode"
        }
      },
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "upstrinfo"
        }
      },
      "controls": [
        {
          "nodeid": {
            "control": "string",
            "label": "节点编号",
            "span":8,
            "rules": [
              {
                "max": 30,
                "message": "长度不能超过30个字符"
              },
              {
                "required": true,
                "message": "请输入节点编号"
              }
            ]
          },
          "nodena": {
            "control": "string",
            "label": "节点名称",
            "span":8,
            "rules": [
              {
                "max": 80,
                "message": "长度不能超过80个字符"
              },
              {
                "required": true,
                "message": "请输入节点名称"
              }
            ]
          },
          "ofpacd": {
            "control": "string",
            "label": "合作商编号",
            "span":8,
            "rules": [
              {
                "max": 30,
                "message": "长度不能超过30个字符"
              },
              {
                "required": true,
                "message": "请输入合作商编号"
              }
            ]
          }
        },
        {
          "nodelv": {
            "control": "string",
            "label": "节点层级",
            "span":8,
            "rules": [
              {
                "max": 2,
                "message": "长度不能超过2个字符"
              },
              {
                "required": true,
                "message": "请输入节点层级"
              }
            ]
          },
          "prndid": {
            "control": "string",
            "label": "父级节点",
            "span":8,
            "rules": [
              {
                "max": 30,
                "message": "长度不能超过30个字符"
              },
              {
                "required": true,
                "message": "请输入父级节点"
              }
            ]
          },
          "isleaf": {
            "control": "string",
            "label": "是否叶子节点",
            "span":8,
            "rules": [
              {
                "max": 10,
                "message": "长度不能超过10个字符"
              },
              {
                "required": true,
                "message": "请选择是否叶子节点"
              }
            ]
          }
        },
        {         
          "principal_part_name": {
            "control": "text",
            "label": "合作主体名称",
            "disabled":false,
            "span":8,
            "rules": [
              {
                "max": 66,
                "message": "长度不能超过66个字符"
              },
              {
                "required": false,
                "message": "请输入合作主体名称"
              }
            ]
          },
          "cert_no": {
            "control": "text",
            "label": "合作方证件号码",
            "disabled":false,
            "span":8,
            "rules": [
              {
                "max": 32,
                "message": "长度不能超过20个字符"
              },
              {
                "required": false,
                "message": "请输入合作方证件号码"
              }
            ]
          },
          "issuing_authority": {
            "control": "text",
            "label": "发证机关",
            "disabled":false,
            "span":8,
            "rules": [
              {
                "max": 100,
                "message": "长度不能超过100个字符"
              },
              {
                "required": false,
                "message": "请输入发证机关"
              }
            ]
          }
        },
        {         
          "issuing_start_date": {
            "control": "text",
            "label": "证件签发日期",
            "disabled":false,
            "span":8,
            "rules": [
              {
                "max": 8,
                "message": "长度不能超过8个字符"
              },
              {
                "required": false,
                "message": "请输入证件签发日期"
              }
            ]
          },
          "issuing_end_date": {
            "control": "text",
            "label": "证件到期日期",
            "disabled":false,
            "span":8,
            "rules": [
              {
                "max": 8,
                "message": "长度不能超过8个字符"
              },
              {
                "required": false,
                "message": "请输入证件到期日期"
              }
            ]
          },
          "busi_license_num": {
            "control": "text",
            "label": "营业执照号码",
            "disabled":false,
            "span":8,
            "rules": [
              {
                "max": 20,
                "message": "长度不能超过20个字符"
              },
              {
                "required": false,
                "message": "请输入营业执照号码"
              }
            ]
          }
        },
        {         
          "tax_reg_num": {
            "control": "text",
            "label": "税务登记证号码",
            "disabled":false,
            "span":8,
            "rules": [
              {
                "max": 20,
                "message": "长度不能超过20个字符"
              },
              {
                "required": false,
                "message": "请输入税务登记证号码"
              }
            ]
          },
          "director_name": {
            "control": "text",
            "label": "负责人姓名",
            "disabled":false,
            "span":8,
            "rules": [
              {
                "max": 60,
                "message": "长度不能超过60个字符"
              },
              {
                "required": false,
                "message": "请输入负责人姓名"
              }
            ]
          },
          "merchant_phone": {
            "control": "text",
            "label": "商户云预留手机号",
            "disabled":false,
            "span":8,
            "rules": [
              {
                "max": 20,
                "message": "长度不能超过20个字符"
              },
              {
                "required": false,
                "message": "请输入商户云预留手机号"
              }
            ]
          }
        },
        { 
          "belong_org": {
            "control": "text",
            "label": "商户所属机构",
            "disabled":false,
            "span":8,
            "rules": [
              {
                "max": 200,
                "message": "长度不能超过200个字符"
              },
              {
                "required": false,
                "message": "请输入商户所属机构"
              }
            ]
          },  
          "acct_no": {
            "control": "text",
            "label": "清算账号",
            "disabled":false,
            "span":8,
            "rules": [
              {
                "max": 40,
                "message": "长度不能超过40个字符"
              },
              {
                "required": false,
                "message": "请输入清算账号"
              }
            ]
          },   
          "acct_name": {
            "control": "text",
            "label": "清算账号名称",
            "disabled":false,
            "span":8,
            "rules": [
              {
                "max": 130,
                "message": "长度不能超过130个字符"
              },
              {
                "required": false,
                "message": "请输入清算账号名称"
              }
            ]
          }
        },
        {
          "bank_id": {
            "control": "text",
            "label": "联行号",
            "disabled":false,
            "span":8,
            "rules": [
              {
                "max": 30,
                "message": "长度不能超过30个字符"
              },
              {
                "required": false,
                "message": "请输入清算账号名称"
              }
            ]
          },
          "open_bank_name": {
            "control": "text",
            "label": "开户行名称",
            "disabled":false,
            "span":8,
            "rules": [
              {
                "max": 130,
                "message": "长度不能超过130个字符"
              },
              {
                "required": false,
                "message": "请输入开户行名称"
              }
            ]
          },
          "open_bank_id": {
            "control": "text",
            "label": "开户行行号",
            "disabled":false,
            "span":8,
            "rules": [
              {
                "max": 30,
                "message": "长度不能超过30个字符"
              },
              {
                "required": false,
                "message": "请输入开户行行号"
              }
            ]
          }
        },
        {
          "open_bank_prov": {
            "control": "text",
            "label": "开户行省份",
            "disabled":false,
            "span":8,
            "rules": [
              {
                "max": 100,
                "message": "长度不能超过100个字符"
              },
              {
                "required": false,
                "message": "请输入开户行省份"
              }
            ]
          },
          "open_bank_city": {
            "control": "text",
            "label": "开户行城市",
            "disabled":false,
            "span":8,
            "rules": [
              {
                "max": 100,
                "message": "长度不能超过100个字符"
              },
              {
                "required": false,
                "message": "请输入开户行城市"
              }
            ]
          }
        },
        {                  
          "area_name": {
            "control": "lookup",
            "label": "绩效归属区域中心",
            "edit": true,
            "span":8,
            "keyField": {
              "area_code": "branchNo",
              "area_name": "brachName"

            },
            "toRequest": {
              "url": "/views/pm/pm_information_uphold/pm_area_choose_single.json",
              "method": "post"
            },
            "multiple": false,
            "props": {
              "title": "请选择"
            },
            "rules": [
              {
                "max": 26,
                "message": "长度不能超过26个字符"
              },
              {
                "required": false,
                "message": "请输入绩效归属区域中心名称"
              }
            ]
          },
          "area_code": {
            "control": "hidden",
            "label": "绩效归属区域中心",
            "disabled":false,
            "span":8,
            "rules": [
              {
                "max": 20,
                "message": "长度不能超过20个字符"
              },
              {
                "required": false,
                "message": "请输入绩效归属区域中心"
              }
            ]
          },
          "offipart_area": {
            "control": "hidden",
            "label": "合作区域中心",
            "disabled":false,
            "span":8,
            "rules": [
              
              {
                "required": false,
                "message": "请输入绩效归属区域中心"
              }
            ]
          },
          "offipart_area_name": {
            "control": "lookup",
            "label": "合作区域中心名称",
            "edit": true,
            "span":16,
            "keyField": {
              "offipart_area": "branchNo",
              "offipart_area_name": "brachName"

            },
            "toRequest": {
              "url": "/views/pm/pm_information_uphold/pm_area_choose_mult.json",
              "method": "post"
            },
            "multiple": true,
            "props": {
              "title": "请选择"
            },
            "rules": [
              
              {
                "required": false,
                "message": "请输入绩效归属区域中心名称"
              }
            ]
          }
        },
        {           
          "province_code": {          
            "control": "remoteSelect",
            "label": "省",
            "placeholder":"请选择省",
            "disabled": false,
            "valueField":"regionNo",
            "labelField":"reginName",
            "span":8,
            "doRequest":{
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "qrRegion",
                "curOrgin":"0"
              }
            },
            "rules": [
              {
                "max": 20,
                "message": "长度不能超过200个字符"
              },
              {
                "required": false,
                "message": "请输入省籍代码"
              }
            ],
            "events":{
              "emit":["province_name","city_code","city_name","district_code","district_name"],
              "condition":{
                "$default":{
                  "control":{
                    "province_name":{
                      "format":"${{current.reginName}}"
                    }
                  }
                }
              }
            }
          },
          "province_name": {
            "name":"province_name",
            "control": "hidden",
            "label": "省籍名称",
            "disabled":false,
            "rules": [
              {
                "max": 26,
                "message": "长度不能超过26个字符"
              },
              {
                "required": false,
                "message": "请输入省籍名称"
              }
            ]
          },   
          "city_code": {
            "name":"city_code",
            "control": "remoteSelect",
            "label": "市",
            "placeholder":"请选择市",
            "disabled": false,
            "valueField":"regionNo",
            "labelField":"reginName",
            "span":8,
            "lazy":true,
            "doRequest":{
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "qrRegion",
                "curOrgin":"${{strForm.province_code}}"
              }
            },
            "rules": [
              {
                "max": 20,
                "message": "长度不能超过20个字符"
              },
              {
                "required": false,
                "message": "请输入市"
              }
            ],
            "events":{
              "emit":["city_name","district_code","district_name"],
              "condition":{
                "$default":{
                  "control":{
                    "city_name":{
                      "format":"${{current.reginName}}"
                    }
                  }
                }
              }
            }
          },
          "city_name": {
            "control": "hidden",
            "label": "市籍名称",
            "disabled":false,
            "rules": [
              {
                "max": 26,
                "message": "长度不能超过26个字符"
              },
              {
                "required": false,
                "message": "请输入市籍名称"
              }
            ]
          },
          "district_code": {
            "name":"district_code",
            "control": "remoteSelect",
            "label": "区",
            "placeholder":"请选择区",
            "disabled": false,
            "valueField":"regionNo",
            "labelField":"reginName",
            "span":8,
            "lazy":true,
            "doRequest":{
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "qrRegion",
                "curOrgin":"${{strForm.city_code}}"
              }
            },
            "rules": [
              {
                "max": 20,
                "message": "长度不能超过20个字符"
              },
              {
                "required": false,
                "message": "请输入区"
              }
            ],
            "events":{
              "emit":["district_name"],
              "condition":{
                "$default":{
                  "control":{
                    "district_name":{
                      "format":"${{current.reginName}}"
                    }
                  }
                }
              }
            }
          },
          "district_name": {
            "control": "hidden",
            "label": "区名称",
            "disabled":false,
            "rules": [
              {
                "max": 26,
                "message": "长度不能超过26个字符"
              },
              {
                "required": false,
                "message": "请输入区名称"
              }
            ]
          }
        },
        {
          "address": {
            "control": "text",
            "label": "详细地址",
            "disabled":false,
            "span":24,
            "rules": [
              {
                "max": 100,
                "message": "长度不能超过100个字符"
              },
              {
                "required": false,
                "message": "请输入网点详细地址"
              }
            ]
          }  
        }
      ]
    }
  }
}
