{
  "module": "v_inputDataForm_btn",
  "layout": {
    "initReqeust": "${{params}}",
    "form": {
      "btns": [
        {
          "type": "submit",
          "label": "确定"
        }
      ],
      "controls": [
        {
          "seqnum": {
          "control": "hidden",
          "value": "${{params.seqnum}}",
          "width": "170px"
        },
          "subtyp": {
            "control": "select",
            "label": "提交类型",
            "width": "160px",
            "rules": [
              {
                "required": true,
                "message": "请输入提交类型"
              }
            ],
            "options": [
              {
                "label": "同意审核",
                "value": "0"
              }
            ]
          }
        },
        {
          "busscp": {
            "control": "textarea",
            "label": "处理意见",
            "width": "160px",
            "rules": [
              {
                "max": 200,
                "message": "长度不能超过200个字符"
              }
            ]
          }
        },
        {
          "belorg": {
            "control": "text",
            "label": "商户所属机构",
            "value": "${{params.belorg}}",
            "disabled": true,
            "width": "160px",
            "rules": [
              {
                "max": 12,
                "message": "和基本信息中的填写一致，长度不能超过12个字符"
              },
              {
                "required": true,
                "message": "请输入商户所属机构"
              }
            ]
          }
        },
        {
          "ofpacd": {
            "control": "text",
            "label": "合作方编号",
            "value":"${{params.ofpacd}}",
            "disabled": true,
            "width": "160px",
            "rules": [
              {
                "max": 30,
                "message": "长度不能超过30个字符"
              },
              {
                "required": true,
                "message": "请输入合作方编号"
              }
            ]
          }
        }
      ]
    }
  }
}
