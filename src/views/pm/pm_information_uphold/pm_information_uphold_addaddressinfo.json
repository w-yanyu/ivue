{
  "module": "v_inputDataForm_btn",
  "layout": {
    "initReqeust": "${{params}}",
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "qraddr",
          "belorg": "${{params.belorg}}",
          "ofpacd": "${{params.ofpacd}}",
          "seqnum": "${{params.seqnum}}"
        }
      },
      "controls": [
        {
          "regadd": {
            "control": "text",
            "label": "注册地址",
            "width": "160px",
            "rules": [
              {
                "required": true,
                "message": "注册地址不得为空"
              },
              {
                "max": 300,
                "message": "长度不能超过300个字符"
              }
            ]
          },
          "pstcod": {
            "control": "text",
            "label": "注册地址邮编",
            "width": "160px",
            "rules": [
              {
                "required": true,
                "message": "注册地址邮编不得为空"
              },
              {
                "max": 6,
                "message": "长度不能超过6个字符"
              }
            ]
          }
        },
        {
          "busadd": {
            "control": "text",
            "label": "经营地址",
            "width": "160px",
            "rules": [
              {
                "required": true,
                "message": "经营地址不得为空"
              },
              {
                "max": 300,
                "message": "长度不能超过300个字符"
              }
            ]
          },
          "buscod": {
            "control": "text",
            "label": "经营地址邮编",
            "width": "160px",
            "rules": [
              {
                "required": true,
                "message": "经营地址邮编不得为空"
              },
              {
                "max": 6,
                "message": "长度不能超过6个字符"
              }
            ]
          }
        },
        {
          "belorg": {
            "control": "text",
            "label": "商户所属机构",
            "disabled": true,
            "width": "160px",
            "rules": [
              {
                "max": 12,
                "message": "长度不能超过12个字符"
              },
              {
                "required": true,
                "message": "请输入商户所属机构"
              }
            ]
          },
          "ofpacd": {
            "control": "text",
            "label": "合作方编号",
            "disabled": true,
            "width": "160px",
            "rules": [
              {
                "max": 30,
                "message": "长度不能超过30个字符"
              },
              {
                "required": true,
                "message": "请输入合作方编号"
              }
            ]
          },
          "seqnum": {
            "control": "hidden",
            "label": "序号"
          }
        }
      ]
    }
  }
}