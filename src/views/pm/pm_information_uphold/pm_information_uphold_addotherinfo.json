{
  "module": "v_inputDataForm_btn",
  "layout": {
    "initReqeust": "${{params}}",
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "#servicecode": "qrexit",
          "servicecode": "qrothe",
          "belorg": "${{params.belorg}}",
          "ofpacd": "${{params.ofpacd}}",
          "seqnum": "${{params.seqnum}}"
        }
      },
      
      "scope": "controlsdata",
      "controls": [
        {
          "seqnum": {
          "control": "hidden",
          "value": "${{params.seqnum}}",
          "width": "170px"
        },
          "gmt_modified": {
            "control": "dateTimePicker",
            "label": "更新日期",
            "width": "160px",
            "rules": [
              {
                "max": 26,
                "message": "长度不能超过26个字符"
              }
            ]
          },
          "updusr": {
            "control": "text",
            "label": "更新操作员",
            "width": "160px",
            "rules": [
              {
                "max": 15,
                "message": "长度不能超过15个字符"
              }
            ]
          }
        },
        {
          "gmt_create": {
            "control": "dateTimePicker",
            "label": "创建日期",
            "width": "160px",
            "rules": [
              {
                "max": 26,
                "message": "长度不能超过26个字符"
              }
            ]
          },
          "crtusr": {
            "control": "text",
            "label": "创建操作员",
            "width": "160px",
            "rules": [
              {
                "max": 15,
                "message": "长度不能超过15个字符"
              }
            ]
          }
        },
        {
          "belorg": {
            "control": "text",
            "label": "商户所属机构",
            "required": true,
            "disabled":true,
            "width": "160px",
            "rules": [
              {
                "max": 12,
                "message": "和基本信息中的填写一致，长度不能超过12个字符"
              }
            ]
          },
          "ofpacd": {
            "control": "text",
            "label": "合作方编号",
            "required": true,
            "width": "160px",
            "rules": [
              {
                "max": 30,
                "message": "长度不能超过30个字符"
              }
            ]
          }
        }
      ]
    }
  }
}
