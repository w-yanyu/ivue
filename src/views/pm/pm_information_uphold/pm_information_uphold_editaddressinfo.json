{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "btns": [
        {
            "type": "submit",
            "label": "保存",
            "size": "",
            "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "POST",
                "params": {
                  "belorg":"${{params.belorg}}",
                    "ofpacd":"${{params.ofpacd}}",
                    "seqnum":"${{params.seqnum}}",
                    "servicecode": "svaddr",
                 
                    "page_id":"/views/pm/pm_information_uphold/pm_information_uphold_addbasicinfo.json"
                }
            }
        }
    ],
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "qraddr"
        }
      },
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "belorg":"${{params.belorg}}",
          "ofpacd":"${{params.ofpacd}}",
          "seqnum":"${{params.seqnum}}",
          "servicecode": "svaddr"
        }
      },
      "controls": [
        {
          "regadd": {
            "control": "text",
            "label": "注册地址",
            "width": "160px",
            "rules": [
              {
                "required": true,
                "message": "注册地址不得为空"
              },
              {
                "max": 80,
                "message": "长度不能超过80个字符"
              }
            ]
          },
          "pstcod": {
            "control": "text",
            "label": "注册地址邮编",
            "width": "160px",
            "rules": [
              {
                "required": true,
                "message": "注册地址邮编不得为空"
              },
              {
                "max": 6,
                "message": "长度不能超过6个字符"
              }
            ]
          }
        },
        {
          "busadd": {
            "control": "text",
            "label": "经营地址",
            "width": "160px",
            "rules": [
              {
                "required": true,
                "message": "经营地址不得为空"
              },
              {
                "max": 80,
                "message": "长度不能超过80个字符"
              }
            ]
          },
          "buscod": {
            "control": "text",
            "label": "经营地址邮编",
            "width": "160px",
            "rules": [
              {
                "required": true,
                "message": "经营地址邮编不得为空"
              },
              {
                "max": 6,
                "message": "长度不能超过6个字符"
              }
            ]
          }
        }
      ]
    }
  }
}
