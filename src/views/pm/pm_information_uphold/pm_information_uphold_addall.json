{
  "module": "dynamicTabs",
  "layout": {
      "initReqeust": "${{params}}",
      "form": {
        "tabs": {
          "scope": "templateScope",
          "tabHeader": [
            {
              "id": "baseInfo",
              "label": "基本信息",
              "closable": false,
              "disabled": false,
              "icon": "el-icon-date",
              "toRequest": {
                "url": "/views/pm/pm_information_uphold/pm_information_uphold_addbasicinfo.json",
                "method": "get"
              }
            },
            {
              "id": "contactInfo",
              "label": "联系信息",
              "icon": "el-icon-date",
              "closable": false,
              "disabled": false,
              "toRequest": {
                "url": "/views/pm/pm_information_uphold/pm_information_uphold_addcontactinfo.json",
                "method": "get",
                "params":{
                  "belorg": "${{templateScope.baseInfo.belorg}}",
                  "ofpacd": "${{templateScope.baseInfo.ofpacd}}",
                  "seqnum": "${{templateScope.baseInfo.seqnum}}"
                }
              }
            },
            {
              "id": "addressInfo",
              "label": "地址信息",
              "icon": "el-icon-date",
              "closable": false,
              "disabled": false,
              "toRequest": {
                "url": "/views/pm/pm_information_uphold/pm_information_uphold_addaddressinfo.json",
                "method": "get"
              }
            },
            {
              "id": "extensionInfo",
              "label": "扩展信息",
              "icon": "el-icon-date",
              "closable": false,
              "disabled": false,
              "toRequest": {
                "url": "/views/pm/pm_information_uphold/pm_information_uphold_addextensioninfo.json",
                "method": "get"
              }
            },
            {
              "id": "otherInfo",
              "label": "其它信息",
              "icon": "el-icon-date",
              "closable": false,
              "disabled": false,
              "toRequest": {
                "url": "/views/pm/pm_information_uphold/pm_information_uphold_addotherinfo.json",
                "method": "get"
              }
            },
            {
              "id": "subauditInfo",
              "label": "提交审核",
              "icon": "el-icon-date",
              "closable": false,
              "disabled": false,
              "toRequest": {
                "url": "/views/pm/pm_information_uphold/pm_information_uphold_addsubauditinfo.json",
                "method": "get"
              }
            }
          ],
          "defaultActiveKey": "baseInfo",
          "position": "top",
          "styleClass": "card"
        }
      }
  }
}
