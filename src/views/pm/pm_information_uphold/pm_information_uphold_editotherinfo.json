{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "qrothe"
        }
      },
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "svothe"
        }
      },
      "controls": [
        {
          "gmt_modified": {
            "control": "dateTimePicker",
            "disabled": true,
            "label": "更新日期",
            "width": "160px",
            "rules": [
              {
                "max": 26,
                "message": "长度不能超过26个字符"
              }
            ]
          },
          "updusr": {
            "control": "text",
            "label": "更新操作员",
            "disabled": true,
            "width": "160px",
            "rules": [
              {
                "max": 15,
                "message": "长度不能超过15个字符"
              }
            ]
          }
        },
        {
          "gmt_create": {
            "control": "dateTimePicker",
            "disabled": true,
            "label": "创建日期",
            "width": "160px",
            "rules": [
              {
                "max": 8,
                "message": "长度不能超过8个字符"
              }
            ]
          },
          "crtusr": {
            "control": "text",
            "disabled": true,
            "label": "创建操作员",
            "width": "160px",
            "rules": [
              {
                "max": 15,
                "message": "长度不能超过15个字符"
              }
            ]
          }
        }
      ]
    }
  }
}
