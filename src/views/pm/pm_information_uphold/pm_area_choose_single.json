{
    "module": "v_search_btn_datagrid",
    "layout": {
        
        "form": {
            "doRequest": {
                "url": "",
                "method": "",
                "params": {
                   
                }
            },
            "controls": [
                {
                    "branchNo": {
                        "control": "text",
                        "label": "机构代码",
                        "rules": [
                            {
                                "max": 30,
                                "message": "长度不能超过30个字符"
                            }
                        ]
                    },
                    "branchLvl": {
                        "control":"text",
                        "label":"机构级别",
                        "rules": [
                            {
                                "max":5,
                                "message": "长度不能超过5个字符"
                            }
                        ] 

                    },
                    "branchType": {
                        "control":"select",
                        "label":"机构类别",
                        "multiple":false,
                        "value":"6",
                        "dict":{
                            "dictType":"E_BRANCHTYPE",
                            "dictKey":[
                                "E_BRANCHTYPE"
                            ],
                            "format":"label"
                        }
                    },
                    "city": {
                        "control":"text",
                        "label":"市",
                        "rules": [
                            {
                                "max":30,
                                "message": "长度不能超过30个字符"
                            }
                        ] 

                    }
                }
                
            ]
        },
        "datagrid": {
            "isInitSearch": false,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST", 
                "params": {
                    "servicecode": "ISMTCTST2157"
                    
                }
            },
            "thead": [
                {
                    "type": "string",
                    "label": "机构代码",
                    "prop": "branchNo",
                    "align": "center"
                },
                {
                    "type": "string",
                    "label": "机构名称",
                    "prop": "brachName",
                    "align": "center"
                },   
                {
                    "type": "string",
                    "label": "上级机构",
                    "prop": "upbranch",
                    "align": "center"
                },
                {
                    "type": "string",
                    "label": "状态",
                    "prop": "status",
                    "align": "center"
                },
                {
                    "label": "操作",
                    "btns": [
                        {
                            "type": "bringback", 
                            "label": "选择"
                        }
                    ]
                }
            ]
        }
    }
}