{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "scope": "controldata",
      "controls": [
        {
          "opcred": {
            "control": "select",
            "label": "合作方证件类型",
            "span": "18",
            "rules": [
              {
                "required": true,
                "message": "请输入合作方证件类型"
              }
            ],
            "options": [
              {
                "label": "组织机构代码证",
                "value": "203"
              }
            ]
          }
        },
        {
          "cmcrno": {
            "control": "text",
            "label": "合作方证件号码",
            "span": "18",
            "rules": [
              {
                "min": 8,
                "max": 20,
                "message": "证件号码长度应该为8到20位"
              },
              {
                "required": true,
                "message": "请输入证件号码"
              }
            ]
          }
        },
        {
          "channel": {
            "control": "select",
            "label": "条线渠道",
            "span": "18",
            "rules": [
              {
                "required": true,
                "message": "请选择条线渠道"
              }
            ],
            "dict": {
              "dictType": "E_TXCHANNEL",
              "dictKey": [
                "E_TXCHANNEL"
              ],
              "format": "label"
            }
          }
        }
      ]
    }
  }
}
