{
  "module": "v_inputDataForm_btn",
  "layout": {
    "initReqeust": "${{params}}",
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "#servicecode": "qrexit",
          "servicecode": "qrtend",
          "belorg": "${{params.belorg}}",
          "ofpacd": "${{params.ofpacd}}",
          "seqnum": "${{params.seqnum}}"
        }
      },
      "scope": "controlsdata",
      "controls": [
        {
          "regamt": {
            "control": "text",
            "label": "注册资金(元)",
            "width": "160px",
            "rules": [
              {
                "max": 17,
                "message": "长度不能超过17个字符"
              }
            ]
          },
          "regidt": {
            "control": "dateTimePicker",
            "label": "注册日期",
            "width": "160px",
            "rules": [
              {
                "max": 8,
                "message": "长度不能超过8个字符"
              }
            ]
          }
        },
        {
          "estadt": {
            "control": "dateTimePicker",
            "label": "成立日期",
            "width": "160px",
            "rules": [
              {
                "max": 8,
                "message": "长度不能超过8个字符"
              }
            ]
          },
          "stfnum": {
            "control": "text",
            "label": "员工人数",
            "width": "160px",
            "rules": [
              {
                "max": 8,
                "message": "长度不能超过8个字符"
              }
            ]
          }
        },
        {
          "cooptp": {
            "control": "select",
            "label": "合作类型",
            "width": "160px",
            "rules": [
              {
                "required": true,
                "message": "合作类型不得为空"
              },
              {
                "max": 2,
                "message": "长度不能超过2个字符"
              }
            ],
            "dict": {
              "dictType": "E_COOPTP",
              "dictKey": [
                "E_COOPTP"
              ],
              "format": "label"
            }
          },
          "busscp": {
            "control": "textarea",
            "label": "经营范围",
            "width": "160px",
            "rules": [
              {
                "max": 200,
                "message": "长度不能超过200个字符"
              }
            ]
          }
        },
        {
          "belorg": {
            "control": "text",
            "label": "商户所属机构",
            "disabled": true,
            "width": "160px",
            "rules": [
              {
                "max": 12,
                "message": "和基本信息中的填写一致，长度不能超过12个字符"
              },
              {
                "required": true,
                "message": "请输入商户所属机构"
              }
            ]
          }
        },
        {
          "ofpacd": {
            "control": "text",
            "label": "合作方编号",
            "width": "160px",
            "disabled": true,
            "rules": [
              {
                "max": 30,
                "message": "长度不能超过30个字符"
              },
              {
                "required": true,
                "message": "请输入合作方编号"
              }
            ]
          },
          "seqnum": {
            "control": "hidden",
            "label": "序号"
          }
        }
      ]
    }
  }
}