{
    "module": "v_inputDataForm_btn",
    "layout": {
      "initReqeust": "${{params}}",
      "form": {
        "initRequest": {
          "url": "/SUMP/icmcall/icmRPCCall",
          "method": "POST",
          "params": {
            "servicecode": "accsave",
            "ofpacd": "${{params.ofpacd}}",
            "acct_type": "${{params.acct_type}}"
          }
        },
        "scope": "controlsdata",
        "controls": [
            {
              "ofpacd": {
                "value":"${{controlsdata.ofpacd}}",
                "control": "text",
                "label": "合作商编号",
                "width": "160px",
                "rules": [
                  {
                    "required": true,
                    "message": "合作商编号不得为空"
                  },
                  {
                    "max": 80,
                    "message": "长度不能超过80个字符"
                  }
                ]
              },
              "acct_type": {
                "value":"${{controlsdata.acct_type}}",
                "control": "select",
                "label": "账户类型",
                "width": "190px",
                "rules": [
                  {
                    "required": true,
                    "message": "账户类型不得为空"
                  },
                  {
                    "max": 80,
                    "message": "长度不能超过80个字符"
                  }
                ],
                "dict": {
                  "dictType": "E_ACCTYP",
                  "dictKey": [
                    "E_ACCTYP"
                  ],
                  "format": "label"
                }
              }
            },
            {
              "acct_no": {
                "control": "text",
                "label": "账号",
                "width": "160px",
                "rules": [
                  {
                    "required": true,
                    "message": "账号不得为空"
                  },
                  {
                    "max": 40,
                    "message": "长度不能超过40个字符"
                  }
                ]
              },
              "acct_name": {
                "control": "text",
                "label": "账号名",
                "width": "160px",
                "rules": [
                  {
                    "required": false,
                    "message": "账号名不得为空"
                  },
                  {
                    "max": 400,
                    "message": "长度不能超过400个字符"
                  }
                ]
              }
            },
            {
              "bank_id": {
                "control": "text",
                "label": "联行号",
                "width": "160px",
                "rules": [
                  {
                    "required": false,
                    "message": "联行号不得为空"
                  },
                  {
                    "max": 30,
                    "message": "长度不能超过30个字符"
                  }
                ]
              },
              "open_bank_id": {
                "control": "text",
                "label": "开户行行号",
                "width": "160px",
                "rules": [
                  {
                    "required": false,
                    "message": "开户行行号不得为空"
                  },
                  {
                    "max": 30,
                    "message": "长度不能超过30个字符"
                  }
                ]
              }
            },
            {
              "open_bank_name": {
                "control": "text",
                "label": "开户行名称",
                "width": "160px",
                "rules": [
                  {
                    "required": true,
                    "message": "开户行名称不得为空"
                  },
                  {
                    "max": 400,
                    "message": "长度不能超过400个字符"
                  }
                ]
              },
              "open_bank_prov": {
                "control": "text",
                "label": "开户行省份",
                "width": "160px",
                "rules": [
                  {
                    "required": true,
                    "message": "开户行省份不得为空"
                  },
                  {
                    "max": 300,
                    "message": "长度不能超过300个字符"
                  }
                ]
              }
            },
            {
              "open_bank_city": {
                "control": "text",
                "label": "开户行城市",
                "width": "160px",
                "rules": [
                  {
                    "required": true,
                    "message": "开户行城市不得为空"
                  },
                  {
                    "max": 300,
                    "message": "长度不能超过300个字符"
                  }
                ]
              }
            }
          ]
        }
    }
  }
  