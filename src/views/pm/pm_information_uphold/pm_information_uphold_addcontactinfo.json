{
  "module": "v_inputDataForm_btn",
  "layout": {
    "initReqeust": "${{params}}",
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "qrconn",
          "#servicecode": "qrexit",
          "belorg": "${{params.belorg}}",
          "ofpacd": "${{params.ofpacd}}",
          "seqnum": "${{params.seqnum}}"
        }
      },
      "scope": "controlsdata",
      "controls": [
        {
          "seqnum": {
            "control": "hidden",
            "value": "${{params.seqnum}}",
            "width": "170px"
          },
          "connna": {
            "control": "text",
            "label": "合作方联系人名称",
            "width": "170px",
            "rules": [
              {
                "required": true,
                "message": "合作方联系人不得为空"
              },
              {
                "max": 80,
                "message": "长度不能超过80个字符"
              }
            ]
          },
          "cncrtp": {
            "control": "select",
            "label": "合作方联系人证件类型",
            "required": false,
            "width": "170px",
            "rules": [
              {
                "max": 3,
                "message": "长度不能超过3个字符"
              }
            ],
            "options": [
              {
                "label": "身份证",
                "value": "101"
              },
              {
                "label": "户口簿",
                "value": "102"
              },
              {
                "label": "护照",
                "value": "103"
              },
              {
                "label": "军官证",
                "value": "104"
              },
              {
                "label": "港澳居民来往内地通行证",
                "value": "111"
              },
              {
                "label": "台湾同胞来往内地通行证",
                "value": "112"
              },
              {
                "label": "营业执照",
                "value": "202"
              },
              {
                "label": "组织机构代码",
                "value": "203"
              },
              {
                "label": "机构信用代码证",
                "value": "204"
              }
            ]
          }
        },
        {
          "cncrno": {
            "control": "text",
            "label": "合作方联系人证件号码",
            "required": false,
            "width": "170px",
            "rules": [
              {
                "max": 20,
                "message": "长度不能超过20个字符"
              }
            ]
          },
          "connph": {
            "control": "text",
            "label": "合作方联系人手机号",
            "width": "170px",
            "rules": [
              {
                "required": true,
                "message": "合作方联系人手机号不得为空"
              },
              {
                "max": 20,
                "message": "长度不能超过20个字符"
              }
            ]
          }
        },
        {
          "contel": {
            "control": "text",
            "label": "合作方联系人固定电话",
            "width": "170px",
            "rules": [
              {
                "max": 20,
                "message": "长度不能超过20个字符"
              }
            ]
          },
          "confax": {
            "control": "text",
            "label": "合作方联系人传真号码",
            "width": "170px",
            "rules": [
              {
                "max": 20,
                "message": "长度不能超过20个字符"
              }
            ]
          }
        },
        {
          "coneml": {
            "control": "text",
            "label": "合作方联系人电子邮箱",
            "width": "170px",
            "rules": [
              {
                "max": 40,
                "message": "长度不能超过40个字符"
              }
            ]
          },
          "conjob": {
            "control": "select",
            "label": "合作方联系人职务",
            "width": "170px",
            "rules": [
              {
                "max": 1,
                "message": "长度不能超过1个字符"
              }
            ],
            "dict": {
              "dictType": "E_CONJOB",
              "dictKey": [
                "E_CONJOB"
              ],
              "format": "label"
            }
          }
        },
        {
          "belorg": {
            "control": "text",
            "label": "商户所属机构",
            "disabled":true,
            "width": "170px",
            "rules": [
              {
                "max": 12,
                "message": "和基本信息中的填写一致，长度不能超过12个字符"
              },
              {
                "required": true,
                "message": "请输入商户所属机构"
              }
            ]
          },
          "ofpacd": {
            "control": "text",
            "label": "合作方编号",
            "width": "170px",
            "disabled":true,
            "rules": [
              {
                "max": 30,
                "message": "长度不能超过30个字符"
              },
              {
                "required": true,
                "message": "请输入合作方编号"
              }
            ]
          },
          "seqnum": {
            "control": "hidden",
            "value": "${{controlsdata.seqnum}}"
        }
        }
      ]
    }
  }
}
