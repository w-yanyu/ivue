{
  "module": "v_inputDataForm_btn",
  "layout": {
    "initReqeust": "${{params}}",
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "qrexit"
        }
      },
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "apbase",
          "belorg":"${{params.belorg}}",
          "ofpacd":"${{params.ofpacd}}",
          "seqnum":"${{params.seqnum}}"
        }
      },
      "btns": [
        {
          "type": "submit",
          "label": "确定",
          "icon": ""
        }
      ],
      "scope": "controlsdata",
      "controls": [
        {
          "subtyp": {
            "control": "select",
            "label": "提交类型",
            "width": "160px",
            "rules": [
              {
                "required": true,
                "message": "提交类型不得为空"
              }
             
            ],
            "options": [
              {
                "label": "同意审核",
                "value": "0"
              }
            ]
          }
        },
        {
          "remart": {
            "control": "textarea",
            "label": "处理意见",
            "width": "160px",
            "rules": [
              {
                "max": 200,
                "message": "长度不能超过200个字符"
              }
            ]
          }
        }
      ]
    }
  }
}
