{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "accupt",
          "ofpacd": "${{params.ofpacd}}"   
        }
      },
      "thead": [
        {
          "type": "index",
          "label": "序号"
        },
        {
          "label": "合作方编号",
          "prop": "ofpacd"
        },
        {
          "label": "账户类型",
          "prop": "acct_type",
          "dict": {
            "dictType": "E_ACCTYP",
            "dictKey": [
              "E_ACCTYP"
            ],
            "format": "label"
          }
        },
        {
          "label": "账号",
          "prop": "acct_no"
        },
        {
          "label": "账号名",
          "prop": "acct_name"
        },
        {
          "label": "联行号",
          "prop": "bank_id"
        },
        {
          "label": "开户行行号",
          "prop": "open_bank_id"
        },
        {
          "label": "开户行名称",
          "prop": "open_bank_name"
        },
        {
          "label": "开户行省份",
          "prop": "open_bank_prov"
        },
        {
          "label": "开户行城市",
          "prop": "open_bank_city"
        }
      ]
    }
  }
}