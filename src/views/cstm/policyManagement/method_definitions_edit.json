{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "scope": "templateScope",
            "initRequest":"${{params}}",
            "initDataOnce":true,

            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "post",
              "params":{
                "servicecode": "AS_RLMS_CP_UPMETH",
                    "htxxid": "${{params.methcd}}"
              }
            },
            "btns":[
				{
					"type":"submit",
					"label":"提交"
				}
			],
            "controls": [
                {
                    "methcd": {
                        "label": "方法编号",
                        "control": "text",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入方法编号"
                            }
                        ]
                    },
                    "methna": {
                        "label": "方法名称",
                        "control": "text",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入方法名称"
                            }
                        ]
                    },
                    "quljmc": {
                        "label": "全路径类名",
                        "control": "text",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入全路径类名"
                            }
                        ]
                    },
                    "isflag": {
                        "control": "select",
                        "label": "有效标志",
                        "dict": {
                            "dictType": "BPM_E_ISFLAG",
                            "dictKey": [
                                "BPM_E_ISFLAG"
                            ],
                            "format": "label"
                        },
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择有效标志"
                            }
                        ]
                    }
                }
            ]
        }
    }
}
