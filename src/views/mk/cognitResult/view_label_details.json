{
    "module": "v_search_btn_datagrid",
    "layout": {
      "form": {
        "controls": [
          {
            "label_id": {
              "control": "text",
              "label": "标签编号"
            },
            "label_name": {
              "control": "text",
              "label": "标签名称"
            }
          }
        ]
      },

      "toolbar": {
        "btns": [
        ]
      },

      "datagrid": {
        "scope": "searchTable",
        "isInitSearch": false,
        "doRequest": {
          "url": "/SUMP/icmcall/icmRPCCall",
          "method": "POST",
          "params": {
            "servicecode": "mk0012",
            "#appr_status":"01"
          }
        },
        "thead": [
          {
            "type":"string",
            "label": "标签编号",
            "prop": "label_id",
            "align": "center"
          },
          {
            "type":"string",
            "label": "标签姓名",
            "prop": "label_name",
            "align": "center"
          },
          {
            "type":"string",
            "label": "客户编号",
            "prop": "cust_no",
            "align": "center"
          },
          {
            "type":"string",
            "label": "客户姓名",
            "prop": "cust_name",
            "align": "center"
          },
          {
            "type":"string",
            "label": "证件号码",
            "prop": "cert_no",
            "align": "center"
          },

          {
            "type":"string",
            "label": "法人编号",
            "prop": "org_id",
            "align": "center"
          },
          {
            "type":"string",
            "label": "最后修改时间",
            "prop": "data_update_time",
            "align": "center"
          }
        ]
      }
    }
  }
  