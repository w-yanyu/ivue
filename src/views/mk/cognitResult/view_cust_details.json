{
    "module": "v_search_btn_datagrid",
    "layout": {
      "form": {
        "controls": [
          {
            "cust_name": {
              "control": "text",
              "label": "客户姓名"
            },
            "cert_no": {
              "control": "text",
              "label": "证件号码"
            }
          }
        ]
      },

      "toolbar": {
        "btns": [
        ]
      },

      "datagrid": {
        "scope": "searchTable",
        "isInitSearch": false,
        "doRequest": {
          "url": "/SUMP/icmcall/icmRPCCall",
          "method": "POST",
          "params": {
            "servicecode": "mk0004",
            "#appr_status":"01"
          }
        },
        "thead": [
          {
            "type":"string",
            "label": "客户编号",
            "prop": "cust_no",
            "align": "center"
          },
          {
            "type":"string",
            "label": "客户姓名",
            "prop": "cust_name",
            "align": "center"
          },
          {
            "type":"string",
            "label": "证件号码",
            "prop": "cert_no",
            "align": "center"
          },
  
          {
            "type":"string",
            "label": "标签数量",
            "prop": "label_count",
            "align": "center"
          },
  
          {
            "type":"string",
            "label": "标签名称",
            "prop": "label_set",
            "align": "center"
          },

          {
            "label": "操作",
            "fixed": "right",
            "width":"100px",
            "align": "center",
            "btns": [
              {
                "type": "edit",
                "label": "维护",
                "toRequest": {
                    "url": "/views/mk/cognitResult/view_cust_details_edit.json",
                    "method": "POST"
                },
                "props": {
                    "width": "60%",
                    "title": "修改客户标签"
                }
              }
            ]
          }
        ]
      }
    }
  }
  