{
    "module": "lr_transferTable",
    "layout": {
      "transfer": {
        "height":"500px",
        "scope": "templateScope",
        "initRequest":"${{params}}",
        "initDataOnce":true,
        "left": {
          "filterable":false,
          "title": "未认定标签",
          "btn":{
            "label": "选择关联",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "mk0007",
                "cust_no": "${{params.cust_no}}",
                "cust_name": "${{params.cust_name}}",
                "cert_no": "${{params.cert_no}}",
                "cert_type": "${{params.cert_type}}"
              }
            },
            "eventAfter": {
              "callback": "restCallBack"
            }
          },
          "datagrid": {
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "mk0006",
                "cust_no": "${{params.cust_no}}"
              }
            },
            "pagination": {
              "show":false
            },
            "checked":"checked",
            "thead": [
              {
                "label": "序号",
                "type": "index",
                "align": "center"
              },
              {
                "label": "标签编号",
                "prop": "label_id",
                "align": "center"
              },
              {
                "label": "标签名称",
                "prop": "label_name",
                "align": "center"
              }
            ]
          }
        },
        "right": {
          "filterable":false,
          "title":"已认定标签",
          "btn":{
            "label":"取消关联",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "mk0008",
                "cust_no": "${{params.cust_no}}"
              }
            },
            "eventAfter": {
              "callback": "restCallBack"
            }
          },
          "datagrid": {
            "scope": "left",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "mk0005",
                "cust_no": "${{params.cust_no}}"
              }
            },
            "pagination": {
              "show":false
            },
            "thead": [
              {
                "label": "序号",
                "type": "index",
                "align": "center"
              },
              {
                "label": "标签编号",
                "prop": "label_id",
                "align": "center"
              },
              {
                "label": "标签名称",
                "prop": "label_name",
                "align": "center"
              }
            ]
          }
        }
      }
    }
  }
  