{
    "module": "v_search_btn_datagrid",
    "layout": {
      "form": {
        "controls": [
          {
            "active_id": {
              "control": "text",
              "label": "活动编号"
            },
            "active_name": {
              "control": "text",
              "label": "活动名称"
            }
          },
          {
            "start_date": {
                "control": "dateTimePicker",
                "label": "开始日期",
                "span": 12,
                "type": "date",
                "format": "yyyyMMdd",
                "disabledDate": {
                    "startRange": "19000101",
                    "endRange": "30000101"
                }
            },
            "end_date": {
                "control": "dateTimePicker",
                "label": "结束日期",
                "type": "date",
                "span": 12,
                "format": "yyyyMMdd",
                "disabledDate": {
                    "startRange": "19000101",
                    "endRange": "30000101"
                }
            }
          }
        ]
      },

      "toolbar": {
        "btns": [
        ]
      },

      "datagrid": {
        "scope": "searchTable",
        "isInitSearch": false,
        "doRequest": {
          "url": "/SUMP/icmcall/icmRPCCall",
          "method": "POST",
          "params": {
            "servicecode": "mk0026"
          }
        },
        "thead": [
          {
            "type":"string",
            "label": "活动编号",
            "prop": "active_id",
            "align": "center"
          },
          {
            "type":"string",
            "label": "活动名称",
            "prop": "active_name",
            "align": "center"
          },
          {
            "type":"string",
            "label": "开始日期",
            "prop": "start_date",
            "align": "center"
          },
          {
            "type":"string",
            "label": "结束日期",
            "prop": "end_date",
            "align": "center"
          },

          {
            "type":"string",
            "label": "负责部门",
            "prop": "active_sects",
            "align": "center",
            "dict":{
              "dictKey": [
                "active_sects"
              ],
			        "#format": "value-label",
              "labelField": "branch_name",
              "valueField": "branch_id",
              "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "POST",
                "params": {
                "servicecode": "ct2155",
                "page_id": "/views/ismt/ct/branch/brannch-info-manager.json"
                }
              }
            }
          },

          {
            "type":"string",
            "label": "工具编号",
            "prop": "tool_id",
            "align": "center"
          },
          {
            "type":"string",
            "label": "活动数量",
            "prop": "active_count",
            "align": "center"
          },
          {
            "type":"string",
            "label": "活动金额",
            "prop": "active_amt",
            "align": "center"
          }
        ]
      }
    }
  }
  