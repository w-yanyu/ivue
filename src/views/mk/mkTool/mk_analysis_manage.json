{
    "module": "v_search_btn_datagrid",
    "layout": {
      "form": {
        "controls": [
          {
            "customer_oriented": {
              "control": "text",
              "label": "面向客群"
            },
            "tool_name": {
              "control": "text",
              "label": "营销工具"
            }
          },
          {
            "start_date": {
              "label": "起始日期",
              "control": "dateTimePicker",
              "span": 12,
              "format": "yyyyMMdd"
            },
            "end_date": {
              "label": "结束日期",
              "control": "dateTimePicker",
              "span": 12,
              "format": "yyyyMMdd"
            }
          },{
            "sale_prod_name": {
              "control": "text",
              "label": "产品名称"
            }
          }
        ]
      },
      "toolbar": {
      },
      "datagrid": {
        "scope": "searchTable",
        "isInitSearch": false,
        "doRequest": {
          "url": "/SUMP/icmcall/icmRPCCall",
          "method": "POST",
          "params": {
            "servicecode": "mk0033"
          }
        },
        "thead": [
          {
            "type":"string",
            "label": "交易日期",
            "prop": "effect_date",
            "align": "center"
          },
          {
            "type":"string",
            "label": "营销活动",
            "prop": "tool_name",
            "align": "center"
          },
          {
            "type":"string",
            "label": "面向客群",
            "prop": "customer_oriented",
            "align": "center"
          },
          {
            "type":"string",
            "label": "产品名称",
            "prop": "sale_prod_name",
            "align": "center"
          },
          {
            "type":"string",
            "label": "投入成本(元)",
            "prop": "input_cost",
            "format": {
              "thousand": ","
            },
            "align": "center"
          },{
            "type":"string",
            "label": "曝光量(次)",
            "prop": "amount_of_exposure",
            "align": "center"
          },{
            "type":"string",
            "label": "点击量(次)",
            "prop": "click_volume",
            "align": "center"
          },{
            "type":"string",
            "label": "成交量(笔)",
            "prop": "trading_volume",
            "align": "center"
          },{
            "type":"string",
            "label": "转化率(%)",
            "prop": "conversion_rate",
            "align": "center"
          },{
            "type":"string",
            "label": "销售额(元)",
            "prop": "amount_of_sale",
            "format": {
              "thousand": ","
            },
            "align": "center"
          } 
        ]
      }
    }
  }
  