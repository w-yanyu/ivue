{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "scope": "formData",
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "mk0010"
            }
          }
        }
      ],
      "controls": [
        {
          "tool_name": {
            "control": "text",
            "label": "工具名称",
            "width": "25px",
            "rules": [
              {
                "required": true,
                "message": "请输入工具名称"
              },
              {
                "max": 500,
                "message": "长度不能超过500个字符"
              }
            ]
          },
          "tool_flg": {
            "control": "text",
            "label": "工具图标",
            "width": "25px",
            "rules": [
              {
                "required": false,
                "message": "请选择工具图标"
              },
              {
                "max": 80,
                "message": "长度不能超过100个字符"
              }
            ]
          }
        },
        {
          "tool_comment": {
            "control": "textarea",
            "label": "工具简介",
            "width": "25px",
            "rules": [
              {
                "required": false,
                "message": "请输入工具简介"
              },
              {
                "max": 200,
                "message": "长度不能超过200个字符"
              }
            ],
            "rows": 2,
            "#span": 20
          },
          "tool_state": {
            "control": "select",
            "label": "工具状态",
            "width": "25px",
            "dict": {
              "dictType": "E_TOOLSTATE",
              "dictKey": [
                "E_TOOLSTATE"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择"
              }
            ]
          }
        },
        {
          "tool_type": {
            "control": "select",
            "label": "工具类型",
            "width": "25px",
            "dict": {
              "dictType": "E_TOOLTYPE",
              "dictKey": [
                "E_TOOLTYPE"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择"
              }
            ],
            "events": {
              "showItem": {
                "0": [
                  "coupon_perc"
                ],
                "1": [
                  "inst_type",
                  "inst_perc",
                  "inst_term",
                  "inst_amt"
                ],
                "2": [
                  "repay_amt"
                ],
                "#$default": [
                  "coupon_perc",
                  "inst_type",
                  "inst_perc",
                  "inst_term",
                  "inst_amt",
                  "repay_amt"
                ]
              }
            }
          },
          "coupon_perc": {
            "#control": "inputNumber",
            "control": "currency",
            "label": "优惠比例(%)",
            "width": "25px",
            "min": 0,
            "rules": [
              {
                "required": true,
                "message": "请输入"
              },
              {
                "expression": "${{formData.coupon_perc}} <= 100",
                "message": "只能输入小于等于 100 的整数"
              },
              {
                "expression": " ${{form.inst_term}} >=  0 ",
                "message": "优惠比例初值不能小于零"
              },
              {
                "pattern": "^[1-9]+[0-9]*$",
                "message": "请输入正整数"
              },
              {
                "#pattern": "^(100.00|[1-9]?\\d(\\.\\d\\d\\d?)?)$",
                "pattern": "^(([1-9]?\\d(\\.\\d{1,3})?)|100|100.00)$",
                "#message": "请输入(0-100)之间的比例",
                "message": "请输入小于等于100的比例"
              }
            ]
          },
          "repay_amt": {
            "label": "还款金金额(元)",
            "#span": "7",
            "control": "currency",
            "suffix": "元",
            "width": "25px",
            "thousand": ",",
            "rules": [
              {
                "required": true,
                "message": "请输入"
              },
              {
                "expression": " ${{form.repay_amt}} >=  0 ",
                "message": "还款金金额末值不能小于零"
              },
              {
                "pattern": "^\\d+(\\.{0,1}[\\d]{1,2}){0,1}$",
                "message": "请输入大于或等于零的数值"
              }
            ]
          },
          "inst_type": {
            "control": "select",
            "label": "贴息类型",
            "width": "25px",
            "dict": {
              "dictType": "E_INSTTYPE",
              "dictKey": [
                "E_INSTTYPE"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择"
              }
            ],
            "events": {
              "showItem": {
                "0": [
                  "inst_amt"
                ],
                "1": [
                  "inst_perc"
                ],
                "2": [
                  "inst_term"
                ],
                "#$default": [
                  "inst_perc",
                  "inst_term",
                  "inst_amt"
                ]
              }
            }
          }
        },
        {
          "inst_perc": {
            "control": "currency",
            "label": "贴息比例(%)",
            "width": "25px",
            "min": 0,
            "rules": [
              {
                "required": true,
                "message": "请输入"
              },
              {
                "expression": "${{formData.inst_perc}} <= 100",
                "message": "只能输入小于等于 100 的整数"
              },
              {
                "pattern": "^[1-9]+[0-9]*$",
                "message": "请输入正整数"
              },
              {
                "#pattern": "^(100.00|[1-9]?\\d(\\.\\d\\d\\d?)?)$",
                "pattern": "^(([1-9]?\\d(\\.\\d{1,3})?)|100|100.00)$",
                "message": "请输入小于等于100的比例"
              }
            ]
          },
          "inst_term": {
            "control": "text",
            "label": "贴息期数",
            "width": "25px",
            "rules": [
              {
                "required": true,
                "message": "请输入"
              },
              {
                "expression": " ${{form.inst_term}} >=  0 ",
                "message": "贴息期数初值不能小于零"
              },
              {
                "pattern": "^[1-9]+[0-9]*$",
                "message": "请输入整数"
              }
            ]
          },
          "inst_amt": {
            "label": "贴息金额(元)",
            "#span": "7",
            "control": "currency",
            "suffix": "元",
            "width": "25px",
            "thousand": ",",
            "rules": [
              {
                "required": true,
                "message": "请输入"
              },
              {
                "expression": " ${{form.inst_amt}} >=  0 ",
                "message": "贴息金额末值不能小于零"
              }
            ]
          }
        }
      ]
    }
  }
}
