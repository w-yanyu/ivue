{
    "module": "v-realonlyForm-btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "controls": [
              {
                "tool_id": {
                    "control": "text",
                    "disabled":true,
                    "label": "工具编号",
                    "width": "25px"
                    
                },
                "tool_name": {
                    "control": "text",
                    "disabled":true,
                    "label": "工具名称",
                    "width": "25px"
                }
              },
              {
                "tool_flg": {
                  "control": "text",
                  "disabled":true,
                  "label": "工具图标",
                  "width": "25px"
                },
                "tool_state": {
                  "control": "select",
                  "disabled":true,
                 "label": "工具状态",
                  "width": "25px",
                  "dict": {
                 "dictType": "E_TOOLSTATE",
                  "dictKey": [
                    "E_TOOLSTATE"
                  ],
                  "#format": "value-label"
                 }
                }
              },
              {
                "tool_comment": {
                  "control": "textarea",
                  "disabled":true,
                  "label": "工具简介",
                  "width": "25px",
                  "rows": 2,
                  "#span": 20
                },
                "tool_type": {
                  "control": "select",
                  "disabled":true,
                  "label": "工具类型",
                  "width": "25px",
                  "dict": {
                    "dictType": "E_TOOLTYPE",
                    "dictKey": [
                      "E_TOOLTYPE"
                    ],
                    "#format": "value-label"
                  },
                  "events": {
                    "showItem": {
                        "0": [
                          "coupon_perc"
                        ],
                        "1": [
                          "inst_type", "inst_perc","inst_term","inst_amt"
                        ],
                        "2": [
                            "repay_amt"
                          ]
                      }
                    }
                }
              },
              {
                "coupon_perc": {
                  "control": "text",
                  "disabled":true,
                  "label": "优惠比例(%)",
                  "width": "25px"
                },
                "repay_amt": {
                  "control": "text",
                  "disabled":true,
                  "label": "还款金金额",
                  "width": "25px"
                },
                "inst_type": {
                  "control": "select",
                  "disabled":true,
                  "label": "贴息类型",
                  "width": "25px",
                  "dict": {
                    "dictType": "E_INSTTYPE",
                    "dictKey": [
                      "E_INSTTYPE"
                    ],
                    "#format": "value-label"
                  },
                  "events": {
                    "showItem": {
                        "0": [
                          "inst_amt"
                        ],
                        "1": [
                          "inst_perc"
                        ],
                        "2": [
                            "inst_term"
                          ]
                      }
                    }
                },
                "inst_perc": {
                  "control": "text",
                  "disabled":true,
                  "label": "贴息比例(%)",
                  "width": "25px"
                },
                "inst_term": {
                  "control": "text",
                  "disabled":true,
                  "label": "贴息期数",
                  "width": "25px"
                },
                "inst_amt": {
                  "control": "text",
                  "disabled":true,
                  "label": "贴息金额",
                  "width": "25px"
                }
              },
              {
                "data_create_time": {
                  "control": "text",
                  "disabled":true,
                  "label": "数据创建时间",
                  "width": "25px"
                  
                },
                "data_create_user": {
                  "control": "text",
                  "disabled":true,
                  "label": "数据创建用户",
                  "width": "25px"
                }
                
              },
              {
                "data_update_time": {
                  "control": "text",
                  "disabled":true,
                  "label": "数据更新时间",
                  "width": "25px"
                  
                },
                "data_update_user": {
                  "control": "text",
                  "disabled":true,
                  "label": "数据更新用户",
                  "width": "25px"
                }
              },
              {
                "org_id": {
                  "control": "text",
                  "disabled":true,
                  "label": "法人代码",
                  "width": "25px"
                }
              }
          ]
        }
    }
}
