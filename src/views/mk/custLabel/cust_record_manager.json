{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "cust_no": {
            "control": "text",
            "label": "客户编号"
          },
          "cust_name": {
            "control": "text",
            "label": "客户姓名"
          }
        },
        {
            "tool_id": {
              "control": "text",
              "label": "卡券编号"
            },
          "card_use_state": {
            "label": "卡券使用状态",
            "#width": "130px",
            "control": "select",
            "dict": {
              "dictType": "E_USECARDSTATE",
              "dictKey": [
                "E_USECARDSTATE"
              ],
              "format": "label"
            }
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "mk0034"
        }
      },
      "thead": [
        {
          "type": "string",
          "label": "客户编号",
          "prop": "cust_no",
          "align": "center"
        },
        {
          "type": "string",
          "label": "客户姓名",
          "prop": "cust_name",
          "align": "center"
        },
        {
          "type": "string",
          "label": "卡券编号",
          "prop": "tool_id",
          "align": "center"
        },
        {
          "type": "string",
          "label": "卡券名称",
          "prop": "tool_name",
          "align": "center"
        },
        {
          "type": "string",
          "label": "卡券类型",
          "prop": "tool_type",
          "align": "center",
          "dict": {
            "dictType": "E_TOOLTYPE",
            "dictKey": [
              "E_TOOLTYPE"
            ],
            "format": "label"
          }
        },
        {
          "type": "string",
          "label": "卡券使用状态",
          "prop": "card_use_state",
          "dict": {
            "dictType": "E_USECARDSTATE",
            "dictKey": [
              "E_USECARDSTATE"
            ],
            "format": "label"
          },
          "align": "center"
        },
        {
          "type": "string",
          "label": "申请号",
          "prop": "apply_no",
          "align": "center"
        }
      ]
    }
  }
}
