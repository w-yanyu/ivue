{
    "module": "dynamicTabs",
    "layout": {
      "tabs": {
        "tabHeader": [
          {
            "id": "01",
            "label": "活动基本信息",
            "closable": false,
            "disabled": false,
            "icon": "el-icon-date",
            "toRequest": {
              "url": "/views/mk/mkActivity/mk_activity_look_baseInfo.json",
              "method": "post"
            }
          },
          {
            "id": "02",
            "label": "面向客群",
            "closable": false,
            "disabled": false,
            "icon": "el-icon-date",
            "toRequest": {
              "url": "/views/mk/mkActivity/mk_activity_look_custGroup.json",
              "method": "post"
            }
          },
          {
            "id": "03",
            "label": "产品信息",
            "closable": false,
            "disabled": false,
            "icon": "el-icon-date",
            "toRequest": {
              "url": "/views/mk/mkActivity/mk_activity_look_prod.json",
              "method": "post"
            }
          },
          {
            "id": "04",
            "label": "营销工具",
            "closable": false,
            "disabled": false,
            "icon": "el-icon-date",
            "toRequest": {
              "url": "/views/mk/mkActivity/mk_activity_look_tool.json",
              "#method": "post",
              "method": "get",
              "params": {
                  "active_id": "${{params.active_id}}",
                  "active_name": "${{params.active_name}}",
                  "tool_id": "${{params.tool_id}}",
                  "tool_name": "${{params.tool_name}}",
                  "active_count": "${{params.active_count}}",
                  "active_amt": "${{params.active_amt}}"
                }
            }
          },{
            "id": "05",
            "label": "营销页面",
            "closable": false,
            "disabled": false,
            "icon": "el-icon-date",
            "toRequest": {
                "url": "/views/mk/mkActivity/mk_activity_edit_baseInfoImg_read.json",
                "method": "get",
                "params": {
                    "active_id": "${{params.active_id}}",
                    "active_name": "${{params.active_name}}",
                    "start_date": "${{params.start_date}}",
                    "end_date": "${{params.end_date}}",
                    "active_topic": "${{params.active_topic}}",
                    "active_connect": "${{params.active_connect}}",
                    "active_sects": "${{params.active_sects}}",
                    "active_img": "${{params.active_img}}"
                  }
               
            }
        }
        ],
        "defaultActiveKey": "01",
        "position": "top",
        "styleClass": "card"
      }
  
  
    }
  }
  