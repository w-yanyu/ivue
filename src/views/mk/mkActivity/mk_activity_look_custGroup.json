{
    "module": "v_search_btn_datagrid",
    "layout": {
        "datagrid": {
            "isInitSearch": true,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode":"mk0014",
                    "label_id": "${{params.label_id}}",
                    "label_name": "${{params.label_name}}",
                    "active_id": "${{params.active_id}}"
                }
            },
            "pagination": {
                "show": false
            },
            "thead": [
                {
                    "label": "标签编号",
                    "prop": "label_id",
                    "align": "center"
                },
                {
                    "label": "标签名称",
                    "prop": "label_name",
                    "align": "center"
                }
            ]
        }
    }
}