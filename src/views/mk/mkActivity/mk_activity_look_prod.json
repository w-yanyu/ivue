{
    "module": "v_search_btn_datagrid",
    "layout": {
        "datagrid": {
            "isInitSearch": true,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode":"mk0015",
                    "sale_prod_cd": "${{params.sale_prod_cd}}",
                    "sale_prod_name": "${{params.sale_prod_name}}",
                    "active_id": "${{params.active_id}}"
                }
            },
            "pagination": {
                "show": false
            },
            "thead": [
                {
                    "label": "可售产品编号",
                    "prop": "sale_prod_cd",
                    "align": "center"
                },
                {
                    "label": "可售产品名称",
                    "prop": "sale_prod_name",
                    "align": "center"
                }
            ]
        }
    }
}