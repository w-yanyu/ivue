{
    "module": "v_search_btn_datagrid",
    "layout": {
      "form": {
        
        "title":"查询条件",
        "controls": [
          {
            "prod_code": {
              "control": "text",
              "value":"${{params.prod_code}}",
              "label": "产品代码",
            "props": {
                "width": "65%",
                "title": "标题"
            }
            },
            "cmpt_id": {
                "control": "text",
                "label": "部件ID"
              }

            
          }
        ]
      },
      
      "datagrid": {
        "scope": "searchTable",
        "isInitSearch": false,
        "doRequest": {
          "url": "/SUMP/icmcall/icmRPCCall",
          "method": "POST",
          "params": {
            "servicecode": "ln3250",
            "outlistid":"lstKlnf_cmpt"
          }
        },
        "thead": [
          {
            "label": "部件ID",
            "prop": "cmpt_id",
            "align": "center"
          },
          {
            "label": "部件名称",
            "prop": "cmpt_nm",
            "align": "center"
          },
          {
            "label": "部件描述",
            "prop": "cmpt_dsc",
            "align": "center"
          },
          {
            "label": "部件对象",
            "prop": "cmpt_obj",
            "align": "center"
          }
        ]
      }
    }
  }
  