{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "cmpt_id": {
            "control": "text",
            "label": "部件ID",
            "rules": [
              {
                "max": 18,
                "message": "长度不能超过18个字符"
              }
            ]
          },
          "cmpt_nm": {
            "control": "text",
            "label": "部件名称",
            "rules": [
              {
                "max": 80,
                "message": "长度不能超过80个字符"
              }
            ]
          }

        },
        {
          "core_busi_dman": {
            "control": "select",
            "label": "业务领域",
            "dict": {
              "dictType": "LN_E_BUSI_DMAN",
              "dictKey": [
                "LN_E_BUSI_DMAN"
              ],
              "format": "label"
            }
          },
          "loan_cmpt_clasf": {
            "control": "select",
            "label": "部件分类",
            "value": "01",
            "dict": {
              "dictType": "LN_E_LOAN_CMPT_CLASF",
              "dictKey": [
                "LN_E_LOAN_CMPT_CLASF"
              ],
              "format": "label"
            }
          }
        },
        {
          "loan_cmpt_ste": {
            "control": "select",
            "label": "部件状态",
            "dict": {
              "dictType": "LN_E_LOAN_CMPT_STE",
              "dictKey": [
                "LN_E_LOAN_CMPT_STE"
              ],
              "format": "label"
            }
          }
        }
      ]
    },
    "toolbar": {
      "btns": [{
        "type": "bringback",
        "label": "提交"
      }]
    },
    "datagrid": {
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "",
        "params": {
          "servicecode": "ln3250",
          "outlistid": "lstKlnf_cmpt",
          "length": 999
        }
      },
      "pagination": {
        "show":false
      },
      "thead": [
        {
          "type": "selection",
          "width": "55"
        },
        {
          "type": "index",
          "label": "序号"
        },
        {
          "label": "部件ID",
          "prop": "cmpt_id",
          "align": "center"
        },
        {
          "label": "部件名称",
          "prop": "cmpt_nm",
          "align": "center"
        },
        {
          "label": "部件描述",
          "prop": "cmpt_dsc",
          "align": "center"
        },
        {
          "label": "部件对象",
          "prop": "cmpt_obj",
          "align": "center",
          "dict": {
            "dictType": "LN_E_CMPT_OBJ",
            "dictKey": [
              "LN_E_CMPT_OBJ"
            ],
            "format": "label"
          }
        },
        {
          "label": "部件分类",
          "prop": "loan_cmpt_clasf",
          "align": "center",
          "dict": {
            "dictType": "LN_E_LOAN_CMPT_CLASF",
            "dictKey": [
              "LN_E_LOAN_CMPT_CLASF"
            ],
            "format": "label"
          }
        },
        {
          "label": "依赖部件ID",
          "prop": "depc_cmpt_id",
          "align": "center"
        },
        {
          "label": "排斥部件ID",
          "prop": "excl_cmpt_id",
          "align": "center"
        },
        {
          "label": "贷款币种规则",
          "prop": "loan_ccy_rule",
          "align": "center",
          "dict": {
            "dictType": "LN_E_CCY_RULE",
            "dictKey": [
              "LN_E_CCY_RULE"
            ],
            "format": "label"
          }
        },
        {
          "label": "指定币种",
          "prop": "asgn_ccy",
          "align": "center",
          "dict": {
            "dictType": "MS_CURRENCY",
            "dictKey": [
              "MS_CURRENCY"
            ],
            "format": "label"
          }
        },
        {
          "label": "部件状态",
          "prop": "loan_cmpt_ste",
          "align": "center",
          "dict": {
            "dictType": "LN_E_LOAN_CMPT_STE",
            "dictKey": [
              "LN_E_LOAN_CMPT_STE"
            ],
            "format": "label"
          }
        }
      ]
    }
  }
}
