{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "scope":"strForm",
      "initRequest": "${{params}}",
      "doRequest": {
        "url": "/SUMP/call/RPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ln3253",
          "operater_ind": "M"
        }
      },
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/call/RPCCall",
            "method": "POST",
            "params": {
              "servicecode": "ln3253",
              "operater_ind": "M"
            }
          }
        }
      ],
      "controls": [
        {
          "cmpt_id": {
            "control": "text",
            "label": "部件ID",
            "width": "100px",
            "disabled": "true"
          },
          "cmpt_nm": {
            "control": "text",
            "label": "部件名称",
            "width": "100px",
            "rules": [
              {
                "required": true,
                "message": "请输入部件名称"
              },
              {
                "max": 100,
                "message": "长度不能超过100个字符"
              }
            ]
          }
        },{
          "cmpt_dsc": {
            "control": "text",
            "label": "部件描述",
            "width": "100px",
            "rules": [
              {
                "max": 100,
                "message": "长度不能超过10个字符"
              }
            ]
          },
          "cmpt_obj": {
            "control": "select",
            "label": "部件对象",
            "width": "100px",
            "dict": {
              "dictType": "LN_E_CMPT_OBJ",
              "dictKey": [
                "LN_E_CMPT_OBJ"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择部件对象"
              }
            ]
          }
        },
        {
          "loan_cmpt_clasf": {
            "control": "select",
            "label": "部件分类",
            "width": "100px",
            "value": "01",
            "dict": {
              "dictType": "LN_E_LOAN_CMPT_CLASF",
              "dictKey": [
                "LN_E_LOAN_CMPT_CLASF"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择部件分类"
              }
            ]
          },
          "depc_cmpt_id": {
            "control": "lookup",
            "edit": true,
            "label": "依赖部件ID",
            "width": "100px",
            "multiple": true,
            "keyField": {
              "depc_cmpt_id": "cmpt_id"
            },
            "toRequest": {
              "url": "/views/product/productModel/productPartManage/pro_part_choose.json",
              "method": "post"
            },
            "props": {
              "title": "查询部件信息"
            },
            "format": "label"
          }
        },
        {
          "excl_cmpt_id": {
            "control": "lookup",
            "label": "排斥部件ID",
            "width": "100px",
            "multiple": true,
            "keyField": {
              "excl_cmpt_id": "cmpt_id"
            },
            "toRequest": {
              "url": "/views/product/productModel/productPartManage/pro_part_choose.json",
              "method": "post"
            },
            "props": {
              "title": "查询部件信息"
            },
            "format": "label"
          },
          "loan_ccy_rule": {
            "control": "select",
            "label": "货币币种规则",
            "width": "100px",
            "value": "1",
            "dict": {
              "dictType": "LN_E_CCY_RULE",
              "dictKey": [
                "LN_E_CCY_RULE"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择货币币种规则"
              }
            ]
          }
        },
        {
          "asgn_ccy": {
            "control": "select",
            "label": "指定币种",
            "width": "100px",
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": [
                "MS_CURRENCY"
              ],
              "format": "label"
            }
          },
          "core_busi_dman": {
            "control": "hidden",
            "width": "100px",
            "label": "业务领域",
            "value": "01",
            "rules": [
              {
                "required": true,
                "message": "请选择货币币种规则"
              }
            ],
            "dict": {
              "dictType": "LN_E_BUSI_DMAN",
              "dictKey": [
                "LN_E_BUSI_DMAN"
              ],
              "format": "label"
            }
          }
        }
      ]
    }
  }
}
