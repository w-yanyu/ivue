{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "cmpt_id": {
            "control": "text",
            "label": "部件ID",
            "rules": [
              {
                "max": 18,
                "message": "长度不能超过18个字符"
              }
            ]
          },
          "cmpt_nm": {
            "control": "text",
            "label": "部件名称",
            "rules": [
              {
                "max": 80,
                "message": "长度不能超过80个字符"
              }
            ]
          }

        },
        {
          "core_busi_dman": {
            "control": "hidden",
            "label": "业务领域",
            "value": "01",
            "dict": {
              "dictType": "LN_E_BUSI_DMAN",
              "dictKey": [
                "LN_E_BUSI_DMAN"
              ],
              "format": "label"
            }
          },
          "loan_cmpt_clasf": {
            "control": "select",
            "label": "部件分类",
            "value": "01",
            "dict": {
              "dictType": "LN_E_LOAN_CMPT_CLASF",
              "dictKey": [
                "LN_E_LOAN_CMPT_CLASF"
              ],
              "format": "label"
            }
          }
        },
        {
          "loan_cmpt_ste": {
            "control": "select",
            "label": "部件状态",
            "dict": {
              "dictType": "LN_E_LOAN_CMPT_STE",
              "dictKey": [
                "LN_E_LOAN_CMPT_STE"
              ],
              "format": "label"
            }
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "target": "dialog",
          "label": "新增",
          "toRequest": {
            "method": "GET",
            "url": "/views/product/productModel/productPartManage/pro_part_add.json"
          },
          "props": {
            "title": "产品部件新增"
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ln3250",
          "outlistid": "lstKlnf_cmpt"
        }
      },
      "thead": [
        {
          "label": "部件ID",
          "prop": "cmpt_id",
          "align": "center"
        },
        {
          "label": "部件名称",
          "prop": "cmpt_nm",
          "align": "center"
        },
        {
          "label": "部件描述",
          "prop": "cmpt_dsc",
          "align": "center"
        },
        {
          "label": "部件对象",
          "prop": "cmpt_obj",
          "align": "center",
          "dict": {
            "dictType": "LN_E_CMPT_OBJ",
            "dictKey": [
              "LN_E_CMPT_OBJ"
            ],
            "format": "label"
          }
        },
        {
          "label": "部件分类",
          "prop": "loan_cmpt_clasf",
          "align": "center",
          "dict": {
            "dictType": "LN_E_LOAN_CMPT_CLASF",
            "dictKey": [
              "LN_E_LOAN_CMPT_CLASF"
            ],
            "format": "label"
          }
        },
        {
          "label": "依赖部件ID",
          "prop": "depc_cmpt_id",
          "align": "center"
        },
        {
          "label": "排斥部件ID",
          "prop": "excl_cmpt_id",
          "align": "center"
        },
        {
          "label": "贷款币种规则",
          "prop": "loan_ccy_rule",
          "align": "center",
          "dict": {
            "dictType": "LN_E_CCY_RULE",
            "dictKey": [
              "LN_E_CCY_RULE"
            ],
            "format": "label"
          }
        },
        {
          "label": "指定币种",
          "prop": "asgn_ccy",
          "align": "center",
          "dict": {
            "dictType": "MS_CURRENCY",
            "dictKey": [
              "MS_CURRENCY"
            ],
            "format": "label"
          }
        },
        {
          "label": "部件状态",
          "prop": "loan_cmpt_ste",
          "align": "center",
          "dict": {
            "dictType": "LN_E_LOAN_CMPT_STE",
            "dictKey": [
              "LN_E_LOAN_CMPT_STE"
            ],
            "format": "label"
          }
        },
        {
          "label": "操作",
          "fixed": "right",
          "width": 300,
          "align": "center",
          "btns": [
            {
              "type": "look",
              "label": "详情",
              "toRequest": {
                "url": "/views/product/productModel/productPartManage/pro_part_info.json",
                "method": "POST"
              },
              "props": {
                "width": "50%",
                "title": "基础产品信息详情"
              }
            },
            {
              "type": "edit",
              "label": "维护",
              "toRequest": {
                "url": "/views/product/productModel/productPartManage/pro_part_update.json",
                "method": "GET"
              },
              "props": {
                "width": "50%",
                "title": "基础产品信息维护"
              }
            },
            {
              "type": "custom",
              "label": "逻辑删除",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "ln3254",
                  "del_class": "2"
                }
              },
              "confirm": {
                "title": "确定删除",
                "confirmButtonText": "确定",
                "cancelButtonText": "取消"
              }
            },
            {
              "type": "custom",
              "label": "物理删除",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "ln3254",
                  "del_class": "1"
                }
              },
              "confirm": {
                "title": "确定删除",
                "confirmButtonText": "确定",
                "cancelButtonText": "取消"
              }
            }
          ]
        }
      ]
    }
  }
}
