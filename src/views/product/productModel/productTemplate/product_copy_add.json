{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "scope": "formData",
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "ln3265"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "orig_tplt_code": {
                        "control": "text",
                        "label": "原模板代码",
                        "width": "130px",
                        "value": "${{params.prod_tplt_code}}",
                        "disabled": true
                    },
                    "orig_tplt_nm": {
                        "control": "text",
                        "label": "原模版名称",
                        "width": "130px",
                        "value": "${{params.tplt_nm}}",
                        "disabled": true
                    }
                },
                {
                    "old_lgl_pern_code": {
                        "control": "hidden",
                        "label": "原法人代码",
                        "width": "130px",
                        "value": "$g{{user_info.org_id}}",
                        "disabled": true
                    },
                    "new_lgl_pern_code": {
                        "control": "hidden",
                        "label": "新法人代码",
                        "width": "130px",
                        "value": "$g{{user_info.org_id}}"
                    }
                },
                {
                    "new_tplt_code": {
                        "control": "text",
                        "label": "新模板代码",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入新模板代码"
                            },
                            {
                                "max": 10,
                                "message": "长度不能超过10个字符"
                            }
                        ]
                    },
                    "new_tplt_nm": {
                        "control": "text",
                        "label": "新模版名称",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入新模版名称"
                            },
                            {
                                "max": 750,
                                "message": "长度不能超过750个字符"
                            }
                        ]
                    }
                }
            ]
        }
    }
}