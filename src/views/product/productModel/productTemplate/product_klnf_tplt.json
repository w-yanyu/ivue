{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "controls": [
                {
                    "prod_tplt_code": {
                        "control": "text",
                        "label": "产品模板代码",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入产品模板代码"
                            },
                            {
                                "max": 10,
                                "message": "长度不能超过10个字符"
                            }
                        ]
                    },
                    "tplt_nm": {
                        "control": "text",
                        "label": "模版名称",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入模版名称"
                            },
                            {
                                "max": 750,
                                "message": "长度不能超过750个字符"
                            }
                        ]
                    }
                },
                {
                    "core_busi_dman": {
                        "control": "hidden",
                        "label": "业务领域",
                        "width": "130px",
                        "value": "01",
                        "dict": {
                            "dictType": "LN_E_BUSI_DMAN",
                            "dictKey": [
                                "LN_E_BUSI_DMAN"
                            ],
                            "format": "label"
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择业务领域"
                            }
                        ]
                    },
                    "tplt_protp_code": {
                        "control": "lookup",
                        "width": "130px",
                        "edit": true,
                        "label": "模版原型代码",
                        "keyField": {
                            "tplt_protp_code": "tplt_protp_code"
                        },
                        "toRequest": {
                            "url": "/views/product/productModel/productTemplate/pro_temprototype_mannage.json",
                            "method": "post",
                            "params": "${{form.tplt_protp_code}}"
                        },
                        "props": {
                            "width": "80%",
                            "title": "模版原型代码查询"
                        },
                        "format": "label"
                    },
                    "tplt_dsc": {
                        "label": "模板描述",
                        "control": "textarea",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入模板描述"
                            }
                        ],
                        "span": 12,
                        "autosize": {
                            "minRows": 3,
                            "maxRows": 5
                        },
                        "maxlength": 300,
                        "showWordLimit": true
                    }
                }
            ]
        }
    }
}