{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "prod_tplt_code": {
            "control": "text",
            "label": "产品模板代码",
            "rules": [
              {
                "max": 10,
                "message": "长度不能超过10个字符"
              }
            ]
          },
          "tplt_nm": {
            "control": "text",
            "label": "模板名称",
            "rules": [
              {
                "max": 750,
                "message": "长度不能超过750个字符"
              }
            ]
          }
        },
        {
          "core_busi_dman": {
            "control": "hidden",
            "value": "01",
            "label": "业务领域",
            "dict": {
              "dictType": "LN_E_BUSI_DMAN",
              "dictKey": [
                "LN_E_BUSI_DMAN"
              ],
              "format": "label"
            }
          },
          "loan_tplt_ste": {
            "control": "select",
            "label": "贷款模板状态",
            "dict": {
              "dictType": "LN_E_LOAN_CMPT_STE",
              "dictKey": [
                "LN_E_LOAN_CMPT_STE"
              ],
              "format": "label"
            }
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "target": "dialog",
          "label": "新增",
          "toRequest": {
            "method": "GET",
            "url": "/views/product/productModel/productTemplate/product_add.json"
          },
          "props": {
            "title": "产品模板新增"
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ln3260",
          "outlistid": "lstTpltInfo"
        }
      },
      "thead": [
        {
          "label": "产品模版代码",
          "prop": "prod_tplt_code",
          "align": "center"
        },
        {
          "label": "模版名称",
          "prop": "tplt_nm",
          "align": "center"
        },
        {
          "label": "模版描述",
          "prop": "tplt_dsc",
          "align": "center"
        },
        {
          "label": "业务领域",
          "prop": "core_busi_dman",
          "align": "center",
          "dict": {
            "dictType": "LN_E_BUSI_DMAN",
            "dictKey": [
              "LN_E_BUSI_DMAN"
            ],
            "format": "label"
          }
        },
        {
          "label": "模板原型代码",
          "prop": "tplt_protp_code",
          "align": "center"
        },
        {
          "label": "贷款模板状态",
          "prop": "loan_tplt_ste",
          "align": "center",
          "dict": {
            "dictType": "LN_E_LOAN_CMPT_STE",
            "dictKey": [
              "LN_E_LOAN_CMPT_STE"
            ],
            "format": "label"
          }
        },
        {
          "label": "操作",
          "fixed": "right",
          "width": 200,
          "align": "center",
          "btns": [
            {
              "type": "look",
              "label": "详情",
              "toRequest": {
                "url": "/views/product/productModel/productTemplate/product_info.json",
                "method": "POST"
              },
              "props": {
                "width": "70%",
                "title": "产品模板信息详情"
              }
            },
            {
              "type": "edit",
              "label": "维护",
              "toRequest": {
                "url": "/views/product/productModel/productTemplate/product_info_update.json",
                "method": "GET"
              },
              "props": {
                "width": "70%",
                "title": "产品模板信息维护"
              }
            },
            {
              "type": "add",
              "label": "拷贝",
              "toRequest": {
                "url": "/views/product/productModel/productTemplate/product_copy_add.json",
                "method": "POST"
              },
              "props": {
                "width": "70%",
                "title": "产品模板拷贝"
              }
            },
            {
              "type": "edit",
              "label": "默认值维护",
              "toRequest": {
                "url": "@baseProduct_valueEdit",
                "method": "GET",
                "target": "navTab",
                "params": {
                  "prod_code": "${{current.prod_tplt_code}}"
                }
              },
              "props": {
                "title": "默认值维护"
              }
            },
            {
              "type": "custom",
              "label": "物理删除",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "ln3264",
                  "del_class": "1"
                }
              },
              "confirm": {
                "title": "确定删除",
                "confirmButtonText": "确定",
                "cancelButtonText": "取消"
              }
            },
            {
              "type": "custom",
              "label": "逻辑删除",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "ln3264",
                  "del_class": "2"
                }
              },
              "confirm": {
                "title": "确定删除",
                "confirmButtonText": "确定",
                "cancelButtonText": "取消"
              }
            }
          ]
        }
      ]
    }
  }
}