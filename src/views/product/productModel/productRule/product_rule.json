{
    "module": "v_search_btn_datagrid",
    "layout": {
      "form": {
        "foldLineNumber": 2,
        "controls": [
          {
            "rule_id": {
              "control": "text",
              "label": "规则ID"
            },
            "prod_code": {
                "control": "text",
                "label": "产品代码"
            }
          },{
            "main_cond_cmpt_id": {
              "control": "text",
              "label": "主条件部件ID"
          },
          "main_cond_attr": {
              "control": "text",
              "label": "主条件属性"
          }
          },{
            "astat_cond_cmpt_id": {
              "control": "text",
              "label": "副条件部件ID"
          },
          "astat_cond_attr": {
              "control": "text",
              "label": "副条件属性"
          }
          },{
            "main_cnstrn_cmpt_id": {
              "control": "text",
              "label": "主约束部件ID"
          },
          "main_cnstrn_attr": {
              "control": "text",
              "label": "主约束属性"
          }
          },{
            "astat_cnstrn_cmpt_id": {
              "control": "text",
              "label": "副约束部件ID"
          },
          "astat_cnstrn_attr": {
              "control": "text",
              "label": "副约束属性"
          }
          }
            
        ]
      },
      "toolbar": {
        "btns": [
          {
            "type": "add",
            "target": "dialog",
            "label": "新增",
            "toRequest": {
              "method": "GET",
              "url": "/views/product/productModel/productRule/product_rule_add.json"
            },
            "props": {
              "title": "产品规则新增"
            }
          }
        ]
      },
      "datagrid": {
        "scope": "searchTable",
        "isInitSearch": false,
        "doRequest": {
          "url": "/SUMP/icmcall/icmRPCCall",
          "method": "POST",
          "params": {
            "servicecode": "ln3282",
            "outlistid":"lstKlnf_tplt_rule"
          }
        },
        "thead": [
          {
            "label": "规则ID",
            "prop": "rule_id",
            "align": "center",
            "width":150
          },
          {
            "label": "产品代码",
            "prop": "prod_code",
            "align": "center"
          },
          {
            "label": "主条件部件ID",
            "prop": "main_cond_cmpt_id",
            "align": "center"
          },
          {
            "label": "主条件属性",
            "prop": "main_cond_attr",
            "align": "center"
          },{
            "label": "副条件部件ID",
            "prop": "astat_cond_cmpt_id",
            "align": "center"
          },
          {
            "label": "副条件属性",
            "prop": "astat_cond_attr",
            "align": "center"
          },
          {
            "label": "主约束部件ID",
            "prop": "main_cnstrn_cmpt_id",
            "align": "center"
          },
          {
            "label": "主约束属性",
            "prop": "main_cnstrn_attr",
            "align": "center"
          },{
            "label": "副约束部件ID",
            "prop": "astat_cnstrn_cmpt_id",
            "align": "center"
          },
          {
            "label": "副约束属性",
            "prop": "astat_cnstrn_attr",
            "align": "center"
          },        {
            "label": "操作",
            "fixed": "right",
            "width": 200,
            "align": "center",
            "btns": [
              {
                "type": "look",
                "label": "详情",
                "toRequest": {
                  "url": "/views/product/productModel/productRule/product_rule_detail.json",
                  "method": "POST"
                },
                "props": {
                  "width": "70%",
                  "title": "产品规则信息详情"
                }
              },
              {
                "type": "edit",
                "label": "维护",
                "toRequest": {
                  "url": "/views/product/productModel/productRule/product_rule_edit.json",
                  "method": "POST"
                },
                "props": {
                  "width": "70%",
                  "title": "产品规则信息维护"
                }
              },
              {
                "type": "custom",
                "label": "删除",
                "doRequest": {
                  "url": "/SUMP/icmcall/icmRPCCall",
                  "method": "POST",
                  "params": {
                    "servicecode": "ln3286"
                  }
                },
                "confirm": {
                  "title": "确定删除",
                  "confirmButtonText": "确定",
                  "cancelButtonText": "取消"
                }
              }
            ]
          }
        ]
      }
    }
  }
  