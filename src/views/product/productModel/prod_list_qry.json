{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "title": "查询条件",
      "controls": [
        {
          "prod_code": {
            "control": "text",
            "label": "产品代码",
            "props": {
              "width": "65%",
              "title": "标题"
            }
          },
          "prod_nm": {
            "control": "text",
            "label": "产品名称"
          },
          "loan_prod_ste": {
            "control": "select",
            "label": "贷款产品状态",
            "dict": {
              "dictType": "LN_E_LOAN_PROD_STE",
              "dictKey": [
                "LN_E_LOAN_PROD_STE"
              ],
              "format": "label"
            }
          },
          "loan_obj": {
            "control": "select",
            "label": "贷款对象",
            "dict": {
              "dictType": "LN_E_LOAN_OBJ",
              "dictKey": [
                "LN_E_LOAN_OBJ"
              ],
              "format": "label"
            }
          },
          "isLxyk": {
            "control": "hidden",
            "label": "是否类信用卡产品",
            "value": "0"
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "syln3270",
          "outlistid": "lstProd"
        }
      },
      "thead": [
        {
          "label": "操作",
          "align": "center",
          "btns": [
            {
              "type": "bringback",
              "label": "选择"
            }
          ]
        },
        {
          "label": "产品代码",
          "prop": "prod_code",
          "align": "center"
        },
        {
          "label": "产品名称",
          "prop": "prod_nm",
          "align": "center"
        },
        {
          "label": "产品描述",
          "prop": "prod_dsc",
          "align": "center"
        },
        {
          "label": "贷款对象",
          "prop": "loan_obj",
          "align": "center",
          "dict": {
            "dictType": "LN_E_LOAN_OBJ",
            "dictKey": [
              "LN_E_LOAN_OBJ"
            ],
            "format": "label"
          }
        },
        {
          "label": "生效日期",
          "prop": "eff_dt",
          "align": "center"
        },
        {
          "label": "失效日期",
          "prop": "invld_dt",
          "align": "center"
        },
        {
          "label": "贷款产品状态",
          "prop": "loan_prod_ste",
          "align": "center",
          "dict": {
            "dictType": "LN_E_LOAN_PROD_STE",
            "dictKey": [
              "LN_E_LOAN_PROD_STE"
            ],
            "format": "label"
          }
        },
        {
          "label": "产品组代码",
          "prop": "prod_grp_code",
          "align": "center"
        },
        {
          "label": "产品组名称",
          "prop": "prod_grp_nm",
          "align": "center"
        },
        {
          "label": "产品模板代码",
          "prop": "prod_tplt_code",
          "align": "center"
        },
        {
          "label": "产品创建日期",
          "prop": "prod_crea_dt",
          "align": "center"
        },
        {
          "label": "产品创建用户",
          "prop": "prod_crea_tlr",
          "align": "center"
        }
      ]
    }
  }
}