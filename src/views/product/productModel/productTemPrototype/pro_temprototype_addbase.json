{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "controls": [
        {
          "tplt_protp_code": {
            "control": "text",
            "label": "模板原型代码",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入模板原型代码"
              },
              {
                "max": 10,
                "message": "长度不能超过10个字符"
              }
            ]
          },
          "tplt_protp_nm": {
            "control": "text",
            "label": "模板原型名称",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入模板原型名称"
              },
              {
                "max": 750,
                "message": "长度不能超过750个字符"
              }
            ]
          }
        },
        {
          "tplt_protp_dsc": {
            "label": "模板描述",
            "control": "textarea",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入模板描述"
              }
            ],
            "span": 12,
            "autosize": {
              "minRows": 3,
              "maxRows": 5
            },
            "maxlength": 300,
            "showWordLimit": true
          },
          "core_busi_dman": {
            "control": "select",
            "label": "业务领域",
            "width": "130px",
            "value": "01",
            "initShow": false
          }
        }
      ]
    }
  }
}
