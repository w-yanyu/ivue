{
    "module": "v-inputDataForm-btn",
    "layout": {
        "form": {
            "scope": "formData",
          "initRequest": {
            "url": "/SUMP/call/RPCCall",
            "method": "POST",
            "params": {
              "asset_list_id": "${{params.prod_code}}",
              "servicecode": "ln3280"
            }
          },
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": "",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "syln3281",
                            "alert_cmpt_id":"08",
                            "prod_code": "${{params.prod_code}}"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "dsct_catg": {
                        "control": "select",
                        "label": "折扣种类",
                        "dict": {
                            "dictType": "LN_E_DSCT_CATG",
                            "dictKey": [
                                "LN_E_DSCT_CATG"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    },
                    "dsct_cdcn_tp": {
                        "control": "select",
                        "label": "折扣券类型",
                        "dict": {
                            "dictType": "LN_E_DSCT_CDCN_TP",
                            "dictKey": [
                                "LN_E_DSCT_CDCN_TP"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    }
                },
                {
                    "pref_cnt": {
                        "control": "text",
                        "label": "折扣数",
                        "width": "130px"
                    },
                    "pref_dsct": {
                        "control": "text",
                        "label": "优惠折扣",
                        "width": "130px"
                    }
                }

            ]
        }
    }
}
