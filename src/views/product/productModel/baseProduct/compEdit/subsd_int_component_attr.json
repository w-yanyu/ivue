{
    "module": "v-inputDataForm-btn",
    "layout": {
        "form": {
            "scope": "formData",
          "initRequest": {
            "url": "/SUMP/call/RPCCall",
            "method": "POST",
            "params": {
              "asset_list_id": "${{params.prod_code}}",
              "servicecode": "ln3280"
            }
          },
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": "",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "syln3281",
                            "alert_cmpt_id":"07",
                            "prod_code": "${{params.prod_code}}"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "subsd_loan": {
                        "control": "select",
                        "label": "补贴贷款",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    },
                    "subsd_aplbl_cust_tp": {
                        "control": "select",
                        "label": "补贴适用客户类型",
                        "dict": {
                            "dictType": "LN_E_SUBSD_APLBL_CUST_TP",
                            "dictKey": [
                                "LN_E_SUBSD_APLBL_CUST_TP"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    }
                },
                {
                    "subsd_suplr": {
                        "control": "select",
                        "label": "补贴提供方",
                        "dict": {
                            "dictType": "LN_E_SUBSD_SUPLR",
                            "dictKey": [
                                "LN_E_SUBSD_SUPLR"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    },
                    "subsd_loan_cptl_purp": {
                        "control": "select",
                        "label": "补贴贷款资金用途",
                        "dict": {
                            "dictType": "LN_E_SUBSD_LOAN_CPTL_PURP",
                            "dictKey": [
                                "LN_E_SUBSD_LOAN_CPTL_PURP"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    }
                },
                {
                    "subsd_amt_caln_way": {
                        "control": "select",
                        "label": "补贴金额计算方式",
                        "dict": {
                            "dictType": "LN_E_SUBSD_AMT_CALN_WAY",
                            "dictKey": [
                                "LN_E_SUBSD_AMT_CALN_WAY"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    },
                    "subsd_amt_pcnt": {
                        "control": "text",
                        "label": "补贴金额或比例",
                        "width": "130px"
                    }
                },
                {
                    "subsd_aply_rcev_tm_pnt": {
                        "control": "select",
                        "label": "补贴申领时点",
                        "dict": {
                            "dictType": "LN_E_SUBSD_APLY_RCEV_TM_PNT",
                            "dictKey": [
                                "LN_E_SUBSD_APLY_RCEV_TM_PNT"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    }
                }

            ]
        }
    }
}
