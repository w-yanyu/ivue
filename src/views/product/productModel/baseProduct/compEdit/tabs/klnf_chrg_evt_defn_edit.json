{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "scope": "formData",
            "initRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "ln3280",
                    "outlistid": "lstklnf_chrg_evt_defn",
                    "prod_code": "${{params.prod_code}}"
                }
            },
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": "",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "syln3281",
                            "alert_cmpt_id":"13",
                            "comp_list":"chrg"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "prod_code": {
                        "control": "string",
                        "label": "产品代码",
                        "width": "130px"
                    }
                },
                {
                    "chrg_evt": {
                        "control": "text",
                        "label": "收费事件",
                        "width": "130px"
                    },
                    "chrg_evt_nm": {
                        "control": "text",
                        "label": "收费事件名称",
                        "width": "130px"
                    }
                },
                {
                    "chrg_catg": {
                        "control": "select",
                        "label": "收费种类",
                        "dict": {
                            "dictType": "LN_E_CHRG_CATG",
                            "dictKey": [
                                "LN_E_CHRG_CATG"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    },
                    "chrg_code": {
                        "control": "text",
                        "label": "收费代码",
                        "width": "130px"
                    }
                },
                {
                    "chrg_code_nm": {
                        "control": "text",
                        "label": "收费代码名称",
                        "width": "130px"
                    },
                    "chrg_amt": {
                        "control": "text",
                        "label": "收费金额",
                        "width": "130px"
                    }
                }
                
            ]
        }
    }
}