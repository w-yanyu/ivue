{
    "module": "v-inputDataForm-btn",
    "layout": {
        "form": {
            "scope": "formData",
          "initRequest": {
            "url": "/SUMP/call/RPCCall",
            "method": "POST",
            "params": {
              "asset_list_id": "${{params.prod_code}}",
              "servicecode": "ln3280"
            }
          },
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": "",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "syln3281",
                            "alert_cmpt_id":"10",
                            "prod_code": "${{params.prod_code}}"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "sydctd_loan_flg": {
                        "control": "select",
                        "label": "银团贷款",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    },
                    "sydctd_loan_way": {
                        "control": "select",
                        "label": "银团贷款方式",
                        "dict": {
                            "dictType": "LN_E_SYDCTD_LOAN_WAY",
                            "dictKey": [
                                "LN_E_SYDCTD_LOAN_WAY"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    }
                },
                {
                    "sydctd_loan_deal_mode": {
                        "control": "select",
                        "label": "银团贷款处理模式",
                        "dict": {
                            "dictType": "LN_E_SYDCTD_LOAN_DEAL_MODE",
                            "dictKey": [
                                "LN_E_SYDCTD_LOAN_DEAL_MODE"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    },
                    "syndct_class": {
                        "control": "select",
                        "label": "银团类别",
                        "dict": {
                            "dictType": "LN_E_SYNDCT_CLASS",
                            "dictKey": [
                                "LN_E_SYNDCT_CLASS"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    }
                },
                {
                    "intrl_syndct_mber_tp": {
                        "control": "select",
                        "label": "内部银团成员行类型",
                        "dict": {
                            "dictType": "LN_E_INTRL_SYNDCT_MBER_BANK_TP",
                            "dictKey": [
                                "LN_E_INTRL_SYNDCT_MBER_BANK_TP"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    },
                    "exte_syndct_mber_tp": {
                        "control": "select",
                        "label": "外部银团成员行类型",
                        "dict": {
                            "dictType": "LN_E_EXTE_SYNDCT_MBER_BANK_TP",
                            "dictKey": [
                                "LN_E_EXTE_SYNDCT_MBER_BANK_TP"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    }
                }

            ]
        }
    }
}
