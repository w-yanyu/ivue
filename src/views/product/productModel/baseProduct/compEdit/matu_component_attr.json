{
    "module": "v-inputDataForm-btn",
    "layout": {
        "form": {
            "scope": "formData",
          "initRequest": {
            "url": "/SUMP/call/RPCCall",
            "method": "POST",
            "params": {
              "asset_list_id": "${{params.prod_code}}",
              "servicecode": "ln3280"
            }
          },
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": "",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "syln3281",
                            "alert_cmpt_id":"05",
                            "prod_code": "${{params.prod_code}}"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "loan_matu_hldy_rule": {
                        "control": "select",
                        "label": "整笔到期遇节假日顺延规则",
                        "dict": {
                            "dictType": "LN_E_ECNTER_HLDY_DEAL_WAY",
                            "dictKey": [
                                "LN_E_ECNTER_HLDY_DEAL_WAY"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    },
                    "alw_loan_extd": {
                        "control": "select",
                        "label": "允许贷款展期",
                        "dict": {
                            "dictType": "LN_E_ISEPYM",
                            "dictKey": [
                                "LN_E_ISEPYM"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    }
                },
                {
                    "extd_rule_num": {
                        "control": "text",
                        "label": "展期规则编号",
                        "width": "130px"
                    },
                    "extd_way": {
                        "control": "select",
                        "label": "展期方式",
                        "dict": {
                            "dictType": "LN_E_EXTD_WAY",
                            "dictKey": [
                                "LN_E_EXTD_WAY"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    }
                },
                {
                    "extd_lgst_matu": {
                        "control": "text",
                        "label": "展期最长期限",
                        "width": "130px"
                    },
                    "extd_max_cnt": {
                        "control": "text",
                        "label": "展期最大次数",
                        "width": "130px"
                    }
                },
                {
                    "extd_need_sfc_amt_ddc_mny": {
                        "control": "select",
                        "label": "展期需足额扣款",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    }
                }

            ]
        }
    }
}
