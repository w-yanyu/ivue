{
    "module": "v-inputDataForm-btn",
    "layout": {
        "form": {
            "scope": "formData",
          "initRequest": {
            "url": "/SUMP/call/RPCCall",
            "method": "POST",
            "params": {
              "asset_list_id": "${{params.prod_code}}",
              "servicecode": "ln3280"
            }
          },
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": "",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "syln3281",
                            "alert_cmpt_id":"11",
                            "prod_code": "${{params.prod_code}}"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "asmpn_mode": {
                        "control": "select",
                        "label": "代偿模式",
                        "dict": {
                            "dictType": "LN_E_ASMPN_MODE",
                            "dictKey": [
                                "LN_E_ASMPN_MODE"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    },
                    "acct_cncl_case_exstd_mode": {
                        "control": "select",
                        "label": "账销案存模式",
                        "dict": {
                            "dictType": "LN_E_ACCT_CNCL_CASE_EXSTD_MODE",
                            "dictKey": [
                                "LN_E_ACCT_CNCL_CASE_EXSTD_MODE"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    }
                },
                {
                    "asmpn_acct_num": {
                        "control": "text",
                        "label": "代偿账号",
                        "width": "130px"
                    },
                    "asmpn_sub_acct_serl_num": {
                        "control": "text",
                        "label": "代偿账号子序号",
                        "width": "130px"
                    }
                },
                {
                    "asmpn_int_caln_flg": {
                        "control": "select",
                        "label": "代偿计息标志",
                        "dict": {
                            "dictType": "LN_E_INT_CALN_FLG",
                            "dictKey": [
                                "LN_E_INT_CALN_FLG"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    },
                    "asmpn_wrng_prds": {
                        "control": "text",
                        "label": "代偿预警期数",
                        "width": "130px"
                    }
                },
                {
                    "asmpn_refund_acct_num": {
                        "control": "text",
                        "label": "代偿回款账号",
                        "width": "130px"
                    },
                    "asmpn_refund_sub_acct_serl_num": {
                        "control": "text",
                        "label": "代偿回款账号子序号",
                        "width": "130px"
                    }
                }

            ]
        }
    }
}
