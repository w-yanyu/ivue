{
    "module": "v-inputDataForm-btn",
    "layout": {
        "form": {
            "scope": "formData",
          "initRequest": {
            "url": "/SUMP/call/RPCCall",
            "method": "POST",
            "params": {
              "asset_list_id": "${{params.prod_code}}",
              "servicecode": "ln3280"
            }
          },
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": "",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "syln3281",
                            "alert_cmpt_id":"17",
                            "prod_code": "${{params.prod_code}}"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "nrl_adv_ntc": {
                        "control": "select",
                        "label": "正常提前通知",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    },
                    "ntc_adv_dys": {
                        "control": "text",
                        "label": "通知提前天数",
                        "width": "130px"
                    }
                },
                {
                    "ovdue_urge_collt_ntc": {
                        "control": "select",
                        "label": "逾期催收通知",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    },
                    "ntc_intrv_dys": {
                        "control": "text",
                        "label": "通知间隔天数",
                        "width": "130px"
                    }
                },
                {
                    "int_rate_change_ntc": {
                        "control": "select",
                        "label": "利率变更通知",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    },
                    "bal_change_ntc": {
                        "control": "select",
                        "label": "余额变更通知",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    }
                },
                {
                    "cust_recpt_prt": {
                        "control": "select",
                        "label": "客户回单打印",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    }
                }

            ]
        }
    }
}
