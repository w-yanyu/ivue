{
    "module": "v-inputDataForm-btn",
    "layout": {
        "form": {
            "scope": "formData",
          "initRequest": {
            "url": "/SUMP/call/RPCCall",
            "method": "POST",
            "params": {
              "asset_list_id": "${{params.prod_code}}",
              "servicecode": "ln3280"
            }
          },
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": "",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "syln3281",
                            "alert_cmpt_id":"12",
                            "prod_code": "${{params.prod_code}}"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "adv_rpymt_fine_num": {
                        "control": "text",
                        "label": "提前还款罚金编号",
                        "width": "130px"
                    },
                    "adv_rpymt_fine_nm": {
                        "control": "text",
                        "label": "提前还款罚金名称",
                        "width": "130px"
                    }
                },
                {
                    "adv_rpymt_adtnl_fine_amt": {
                        "control": "text",
                        "label": "提前还款附加罚金金额",
                        "width": "130px"
                    },
                    "ovdue_rpymt_fine_num": {
                        "control": "text",
                        "label": "逾期还款罚金编号",
                        "width": "130px"
                    }
                },
                {
                    "ovdue_rpymt_fine_nm": {
                        "control": "text",
                        "label": "逾期还款罚金名称",
                        "width": "130px"
                    },
                    "ovdue_rpymt_adtnl_fine_amt": {
                        "control": "text",
                        "label": "逾期还款附加罚金金额",
                        "width": "130px"
                    }
                }

            ]
        }
    }
}
