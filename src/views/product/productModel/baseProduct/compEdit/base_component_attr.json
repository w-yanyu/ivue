{
    "module": "v-inputDataForm-btn",
    "layout": {
        "form": {
          "scope": "formData",
          "initRequest": {
            "url": "/SUMP/call/RPCCall",
            "method": "POST",
            "params": {
              "asset_list_id": "${{params.prod_code}}",
              "servicecode": "ln3280"
            }
          },
            "autoReset": true,
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": "",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "syln3281",
                            "alert_cmpt_id": "00",
                            "prod_code": "${{params.prod_code}}"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "prod_code": {
                        "control": "string",
                        "label": "产品代码",
                        "width": "130px"
                    },
                    "prod_nm": {
                        "control": "string",
                        "label": "产品名称",
                        "width": "130px"
                    }
                },
                {
                    "loan_obj": {
                        "control": "select",
                        "label": "贷款对象",
                        "dict": {
                            "dictType": "LN_E_LOAN_OBJ",
                            "dictKey": [
                                "LN_E_LOAN_OBJ"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    },
                    "loan_obj_sbsct": {
                        "control": "select",
                        "label": "贷款对象细分",
                        "dict": {
                            "dictType": "LN_E_LOAN_OBJ_SBSCT",
                            "dictKey": [
                                "LN_E_LOAN_OBJ_SBSCT"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    }
                },
                {
                    "off_bal_sheet_prod": {
                        "control": "select",
                        "label": "表外产品",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    },
                    "loan_busi_clasf": {
                        "control": "select",
                        "label": "贷款业务分类",
                        "dict": {
                            "dictType": "LN_E_BUSI_CLASF",
                            "dictKey": [
                                "LN_E_BUSI_CLASF"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    }
                },
                {
                    "use_loan_cmtmt_flg": {
                        "control": "select",
                        "label": "使用贷款承诺标志",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    },
                    "cmtmt_loan": {
                        "control": "select",
                        "label": "承诺贷款",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    }
                },
                {
                    "rvlg_loan_flg": {
                        "control": "select",
                        "label": "循环贷款标志",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    },
                    "deriv_loan_flg": {
                        "control": "select",
                        "label": "衍生贷款标志",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    }
                },
                {
                    "deriv_busi_tp": {
                        "control": "select",
                        "label": "衍生业务类型",
                        "dict": {
                            "dictType": "LN_E_DERIV_BUSI_TP",
                            "dictKey": [
                                "LN_E_DERIV_BUSI_TP"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    },
                    "loan_prod_mgmt_dprtmt": {
                        "control": "text",
                        "label": "贷款管理部门",
                        "width": "130px"
                    }
                },
              {
                "fee_flg": {
                  "control": "select",
                  "label": "收费标志",
                  "dict": {
                    "dictType": "MS_E_YESORNO",
                    "dictKey": [
                      "MS_E_YESORNO"
                    ],
                    "format": "label"
                  },
                  "width": "130px"
                }
              }
            ]
        }
    }
}
