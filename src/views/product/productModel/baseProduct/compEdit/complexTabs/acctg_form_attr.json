{
    "module": "v-inputDataForm-btn",
    "layout": {
        "form": {
            "scope": "formData",
            "initRequest": "${{params}}",
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": "",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "syln3281",
                            "alert_cmpt_id":"06",
                            "prod_code": "${{params.prod_code}}"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "acrdg_acd_non_acd_busi_acctg": {
                        "control": "select",
                        "label": "按应计非应计核算",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    },
                    "acrdg_old_acctg": {
                        "control": "select",
                        "label": "按一逾两呆核算",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    }
                },
                {
                    "form_dstgs_sbj_busi_acctg_flg": {
                        "control": "select",
                        "label": "按贷款形态分科目核算标志",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    },
                    "auto_form_trfr": {
                        "control": "select",
                        "label": "自动形态转移",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    }
                },
                {
                    "int_trfr_out_rule": {
                        "control": "select",
                        "label": "利息转出规则",
                        "dict": {
                            "dictType": "LN_E_INT_TRFR_OUT_RULE",
                            "dictKey": [
                                "LN_E_INT_TRFR_OUT_RULE"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    },
                    "int_trfr_back_rule": {
                        "control": "select",
                        "label": "利息转回规则",
                        "dict": {
                            "dictType": "LN_E_INT_TRFR_BACK_RULE",
                            "dictKey": [
                                "LN_E_INT_TRFR_BACK_RULE"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    }
                },
                {
                    "loan_busi_acctg_way": {
                        "control": "select",
                        "label": "贷款核算方式",
                        "dict": {
                            "dictType": "LN_E_BUSI_ACCTG_WAY",
                            "dictKey": [
                                "LN_E_BUSI_ACCTG_WAY"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    },
                    "loan_imprmt_mesr_rule": {
                        "control": "select",
                        "label": "贷款减值计量规则",
                        "dict": {
                            "dictType": "LN_E_LOAN_IMPRMT_MESR_RULE",
                            "dictKey": [
                                "LN_E_LOAN_IMPRMT_MESR_RULE"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    }
                },
                {
                    "off_bal_sheet_busi_acctg_code": {
                        "control": "text",
                        "label": "表外核算码",
                        "width": "130px"
                    },
                    "loan_trfr_dys": {
                        "control": "text",
                        "label": "贷款转列天数",
                        "width": "130px"
                    }
                },
                {
                  "prod_code": {
                    "control": "text",
                    "initShow":false,
                    "label": "产品代码",
                    "width": "130px"
                }
                }
                
            ]
        }
        
    }
}