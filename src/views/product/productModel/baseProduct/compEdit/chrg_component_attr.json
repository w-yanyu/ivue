{
  "module": "v_realonlyForm_btn",
  "layout": {
    "form": {
      "controlsGroup": [
        {
          "title": "贷款产品收费属性",
          "tooltip": "贷款产品收费属性",
          "titleIcon": "el-icon-info",
          "expand": true,
          "accordion": false,
          "controls": [
            {
              "info02": {
                "span": 24,
                "control": "dynamic",
                "toRequest": {
                  "url": "/views/product/productModel/baseProduct/compEdit/complexTabs/chrg_form_attr.json",
                  "params": {
                    "loan_chrg_tp": "${{params.loan_chrg_tp}}",
                    "freq_chrg_tp": "${{params.freq_chrg_tp}}",
                    "chrg_prd": "${{params.chrg_prd}}",
                    "chrg_catg": "${{params.chrg_catg}}",
                    "chrg_code": "${{params.chrg_code}}",
                    "chrg_code_nm": "${{params.chrg_code_nm}}",
                    "chrg_amt": "${{params.chrg_amt}}",
                    "prod_code": "${{params.prod_code}}"
                  }
                }
              }
            }
          ]
        },
        {
          "title": "收费事件定义表",
          "tooltip": "收费事件定义表",
          "titleIcon": "el-icon-info",
          "expand": true,
          "accordion": false,
          "controls": [
            {
              "info03": {
                "span": 24,
                "control": "dynamic",
                "toRequest": {
                  "url": "/views/product/productModel/baseProduct/compEdit/complexTabs/chrg_table_attr.json",
                  "params": {
                    "prod_code": "${{params.prod_code}}"
                  }
                }
              }
            }
          ]
        }
      ]
    }
  }
}
