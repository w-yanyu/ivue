{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "scope": "formData",
          "initRequest": {
            "url": "/SUMP/call/RPCCall",
            "method": "POST",
            "params": {
              "asset_list_id": "${{params.prod_code}}",
              "servicecode": "ln3280"
            }
          },
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": "",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "syln3281",
                            "alert_cmpt_id":"09",
                            "prod_code": "${{params.prod_code}}"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "agnc_busi_acctg_way": {
                        "control": "select",
                        "label": "代理核算方式",
                        "dict": {
                            "dictType": "LN_E_AGNC_BUSI_ACCTG_WAY",
                            "dictKey": [
                                "LN_E_AGNC_BUSI_ACCTG_WAY"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    },
                    "agnc_info_asgn_rule": {
                        "control": "select",
                        "label": "代理信息指定规则",
                        "dict": {
                            "dictType": "LN_E_AGNC_INFO_ASGN_RULE",
                            "dictKey": [
                                "LN_E_AGNC_INFO_ASGN_RULE"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    }
                },
                {
                    "agnc_info_val_rule": {
                        "control": "select",
                        "label": "代理信息取值规则",
                        "dict": {
                            "dictType": "LN_E_AGNC_INFO_VAL_RULE",
                            "dictKey": [
                                "LN_E_AGNC_INFO_VAL_RULE"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    },
                    "entstd_dept_busi_code": {
                        "control": "text",
                        "label": "委托存款账户业务编码",
                        "width": "130px"
                    }
                }

            ]
        }
    }
}
