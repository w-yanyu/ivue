{
    "module": "v-inputDataForm-btn",
    "layout": {
        "form": {
            "scope": "formData",
          "initRequest": {
            "url": "/SUMP/call/RPCCall",
            "method": "POST",
            "params": {
              "asset_list_id": "${{params.prod_code}}",
              "servicecode": "ln3280"
            }
          },
            "autoReset": true,
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": "",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "syln3281",
                            "alert_cmpt_id":"01",
                            "prod_code": "${{params.prod_code}}"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "lendg_tp": {
                        "control": "select",
                        "label": "放款类型",
                        "dict": {
                            "dictType": "LN_E_LENDMD",
                            "dictKey": [
                                "LN_E_LENDMD"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    },
                    "auto_lendg_flg": {
                        "control": "select",
                        "label": "自动放款标志",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    }
                },
                {
                    "prdcly_lendg_flg": {
                        "control": "select",
                        "label": "周期性放款标志",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    },
                    "lendg_prd": {
                        "control": "text",
                        "label": "放款周期",
                        "width": "130px"
                    }
                },
                {
                    "lendg_way": {
                        "control": "select",
                        "label": "放款方式",
                        "dict": {
                            "dictType": "LN_E_LENDTP",
                            "dictKey": [
                                "LN_E_LENDTP"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    },
                    "evry_tm_lendg_amt_pcnt": {
                        "control": "text",
                        "label": "每次放款金额或比例",
                        "width": "130px"
                    }
                },
                {
                    "cptl_src_tp": {
                        "control": "select",
                        "label": "放款资金来源类型",
                        "dict": {
                            "dictType": "LN_E_CPTL_SRC_TP",
                            "dictKey": [
                                "LN_E_CPTL_SRC_TP"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    },
                    "lendg_cptl_deal_way": {
                        "control": "select",
                        "label": "放款资金处理方式",
                        "dict": {
                            "dictType": "LN_E_FUNDMD",
                            "dictKey": [
                                "LN_E_FUNDMD"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    }
                },
                {
                    "lgst_loan_matu": {
                        "control": "text",
                        "label": "最长贷款期限",
                        "width": "130px"
                    },
                    "shrtst_loan_matu": {
                        "control": "text",
                        "label": "最短贷款期限",
                        "width": "130px"
                    }
                },
                {
                    "evry_tm_max_amt": {
                        "control": "text",
                        "label": "每次最大金额",
                        "width": "130px"
                    },
                    "evry_tm_min_amt": {
                        "control": "text",
                        "label": "每次最小金额",
                        "width": "130px"
                    }
                },
                {
                    "max_lendg_amt": {
                        "control": "text",
                        "label": "最大放款金额",
                        "width": "130px"
                    },
                    "min_lendg_amt": {
                        "control": "text",
                        "label": "最小放款金额",
                        "width": "130px"
                    }
                },
                {
                    "lendg_max_intrv": {
                        "control": "text",
                        "label": "放款最大间隔",
                        "width": "130px"
                    },
                    "lendg_min_intrv": {
                        "control": "text",
                        "label": "放款最小间隔",
                        "width": "130px"
                    }
                },
                {
                    "max_lendg_cnt": {
                        "control": "text",
                        "label": "最大放款次数",
                        "width": "130px"
                    },
                    "lendg_vld_matu": {
                        "control": "text",
                        "label": "放款有效期限（天）",
                        "width": "130px"
                    }
                },
                {
                    "alw_intra_hmym_acct_lendg": {
                        "control": "select",
                        "label": "允许对行内同名账户放款",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    },
                    "alw_intra_non_hmym_acct_lendg": {
                        "control": "select",
                        "label": "允许对行内非同名账户放款",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    }
                },
                {
                    "alw_lendg_to_out_bank_acct": {
                        "control": "select",
                        "label": "允许对行外账户放款",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    },
                    "alw_lendg_to_intrl_acct": {
                        "control": "select",
                        "label": "允许对内部账户放款",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    }
                },
                {
                    "alw_spec_lendg_flg": {
                        "control": "select",
                        "label": "允许特殊放款标志",
                        "dict": {
                            "dictType": "LN_E_ISEPYM",
                            "dictKey": [
                                "LN_E_ISEPYM"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    },
                    "fducy_pymt_busi_code": {
                        "control": "text",
                        "label": "受托支付核算码",
                        "width": "130px"
                    }
                },
                {
                    "lendg_acctg_way": {
                        "control": "select",
                        "label": "放款记账方式",
                        "dict": {
                            "dictType": "LN_E_LENDG_ACCTG_WAY",
                            "dictKey": [
                                "LN_E_LENDG_ACCTG_WAY"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    },
                    "refinc_flg": {
                        "control": "select",
                        "label": "借新还旧标志",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    }
                },
                {
                    "lendg_due_bill_mgmt_mode": {
                        "control": "select",
                        "label": "自动放款借据管理模式",
                        "dict": {
                            "dictType": "LN_E_AUTOTG",
                            "dictKey": [
                                "LN_E_AUTOTG"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    }
                },
                {
                    "refinc_ctrl": {
                        "control": "select",
                        "label": "借新还旧控制",
                        "dict": {
                            "dictType": "LN_E_ROCNDN",
                            "dictKey": [
                                "LN_E_ROCNDN"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    },
                    "refinc_rpymt_ctrl": {
                        "control": "select",
                        "label": "借新还旧还款控制",
                        "dict": {
                            "dictType": "LN_E_RORPFG",
                            "dictKey": [
                                "LN_E_RORPFG"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    }
                }

            ]
        }
    }
}
