{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "doRequest": {
            "url": "/SUMP/call/RPCCall",
            "method": "POST",
            "params": {
              "servicecode": "syln3273",
              "prod_code": "${{params.prod_code}}",
              "prod_nm": "${{params.prod_nm}}",
              "eff_dt": "${{params.eff_dt}}",
              "invld_dt": "${{params.invld_dt}}",
              "prod_grp_code": "${{params.prod_grp_code}}",
              "prod_grp_nm": "${{params.prod_grp_nm}}",
              "prod_tplt_code": "${{params.prod_tplt_code}}",
              "operation": 0
            }
          }
        }
      ],
      "controls": [
        {
          "prod_aplbl_tp": {
            "control": "select",
            "label": "产品适用类型",
            "width": "100px",
            "dict": {
              "dictType": "LN_E_APLBL_TP",
              "dictKey": [
                "LN_E_APLBL_TP"
              ],
              "format": "label-value"
            },
            "rules": [
              {
                "required": true,
                "message": "请输入产品适用类型"
              }
            ]
          },
          "org_aplbl_flg": {
            "control": "select",
            "label": "机构适用标志",
            "width": "100px",
            "dict": {
              "dictType": "LN_E_ORG_APLBL_FLG",
              "dictKey": [
                "LN_E_ORG_APLBL_FLG"
              ],
              "format": "label-value"
            },
            "rules": [
              {
                "required": true,
                "message": "请输入机构适用标志"
              }
            ]
          }
        },
        {
          "aplbl_scope": {
            "control": "text",
            "label": "适用范围",
            "width": "100px",
            "rules": [
              {
                "required": true,
                "message": "请输入适用范围"
              },
              {
                "max": 300,
                "message": "长度不能超过300个字符"
              }
            ]
          }
        }
      ]
    }
  }
}