{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/call/RPCCall",
            "method": "POST",
            "params": {
              "servicecode": "ln3273",
              "prod_code": "${{params.prod_code}}"
            }
          }
        }
      ],
      "controls": [
        {
          "prod_code": {
            "control": "text",
            "label": "产品代码",
            "width": "130px",
            "disabled": true
          },
          "prod_nm": {
            "control": "text",
            "label": "产品名称",
            "width": "130px",
            "disabled": true
          }
        },
        {
          "prod_dsc": {
            "label": "产品描述",
            "control": "textarea",
            "width": "130px",
            "span": 12,
            "autosize": {
              "minRows": 3,
              "maxRows": 5
            },
            "maxlength": 1500,
            "showWordLimit": true
          },
          "prod_tplt_code": {
            "control": "lookup",
            "width": "130px",
            "edit": true,
            "rules": [
              {
                "required": true,
                "message": "请输入产品模版代码"
              }
            ],
            "label": "产品模版代码",
            "keyField": {
              "prod_tplt_code": "prod_tplt_code"
            },
            "toRequest": {
              "url": "/views/product/productModel/baseProduct/product_template.json",
              "method": "post",
              "params": "${{form.prod_tplt_code}}"
            },
            "props": {
              "width": "80%",
              "title": "产品模版代码查询"
            },
            "format": "label"
          }
        },
        {
          "eff_dt": {
            "control": "dateTimePicker",
            "rules": [
              {
                "required": "true",
                "message": "请输入生效日期"
              }
            ],
            "label": "生效日期",
            "width": "130px",
            "type": "date",
            "endPlaceholder": "选择生效日期",
            "unlinkPanels": true,
            "format": "yyyyMMdd",
            "disabledDate": {
              "startRange": "19000101",
              "endRange": "99991231"
            }
          },
          "invld_dt": {
            "control": "dateTimePicker",
            "rules": [
              {
                "required": "true",
                "message": "请输入失效日期"
              }
            ],
            "label": "失效日期",
            "type": "date",
            "width": "130px",
            "endPlaceholder": "选择失效日期",
            "unlinkPanels": true,
            "format": "yyyyMMdd",
            "disabledDate": {
              "startRange": "19000101",
              "endRange": "99991231"
            }
          }
        },
        {
          "prod_on_shlvs_dt": {
            "control": "dateTimePicker",
            "label": "产品上架日期",
            "type": "date",
            "width": "130px",
            "endPlaceholder": "选择产品上架日期",
            "unlinkPanels": true,
            "format": "yyyyMMdd",
            "disabledDate": {
              "startRange": "19000101",
              "endRange": "99991231"
            }
          },
          "prod_off_shlvs_dt": {
            "control": "dateTimePicker",
            "label": "产品下架日期",
            "type": "date",
            "width": "130px",
            "endPlaceholder": "产品下架日期",
            "unlinkPanels": true,
            "format": "yyyyMMdd",
            "disabledDate": {
              "startRange": "19000101",
              "endRange": "99991231"
            }
          }
        },
        {
          "prod_grp_code": {
            "control": "lookup",
            "width": "130px",
            "edit": true,
            "rules": [
              {
                "required": true,
                "message": "请输入产品组代码"
              }
            ],
            "label": "产品组代码",
            "keyField": {
              "prod_grp_code": "parm_val",
              "prod_grp_nm": "parm_nm"
            },
            "toRequest": {
              "url": "/views/product/productModel/baseProduct/product_prod_grp_code.json",
              "method": "post",
              "params": "${{form.prod_grp_code}}"
            },
            "props": {
              "width": "80%",
              "title": "产品组代码代码查询"
            },
            "format": "label"
          },
          "prod_grp_nm": {
            "control": "text",
            "label": "产品组名称",
            "width": "130px",
            "disabled": true
          }
        }
      ]
    }
  }
}