{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "doRequest": {
            "url": "/SUMP/call/RPCCall",
            "method": "POST",
            "params": {
              "servicecode": "syln3273",
              "prod_code": "${{params.prod_code}}",
              "prod_nm": "${{params.prod_nm}}",
              "eff_dt": "${{params.eff_dt}}",
              "invld_dt": "${{params.invld_dt}}",
              "prod_grp_code": "${{params.prod_grp_code}}",
              "prod_grp_nm": "${{params.prod_grp_nm}}",
              "prod_tplt_code": "${{params.prod_tplt_code}}",
              "operation": 0
            }
          }
        }
      ],
      "controls": [
        {
          "prod_lv_tab_nm": {
            "control": "text",
            "label": "产品级表名",
            "width": "100px",
            "rules": [
              {
                "required": true,
                "message": "请输入产品级表名"
              },
              {
                "max": 32,
                "message": "长度不能超过32个字符"
              }
            ]
          },
          "prod_lv_fld": {
            "control": "text",
            "label": "产品级字段",
            "width": "100px",
            "rules": [
              {
                "required": true,
                "message": "请输入产品级字段"
              },
              {
                "max": 30,
                "message": "长度不能超过30个字符"
              }
            ]
          },
          "prod_lv_fld_dsc": {
            "control": "text",
            "label": "产品级字段描述",
            "width": "100px",
            "rules": [
              {
                "required": true,
                "message": "请输入产品级字段描述"
              },
              {
                "max": 80,
                "message": "长度不能超过80个字符"
              }
            ]
          },
          "acct_lv_tab_nm": {
            "control": "text",
            "label": "账户级表名",
            "width": "100px",
            "rules": [
              {
                "required": true,
                "message": "请输入账户级表名"
              },
              {
                "max": 32,
                "message": "长度不能超过32个字符"
              }
            ]
          },
          "acct_lv_fld": {
            "control": "text",
            "label": "账户级字段",
            "width": "100px",
            "rules": [
              {
                "required": true,
                "message": "请输入账户级字段"
              },
              {
                "max": 30,
                "message": "长度不能超过30个字符"
              }
            ]
          },
          "acct_lv_fld_dsc": {
            "control": "text",
            "label": "账户级字段描述",
            "width": "100px",
            "rules": [
              {
                "required": true,
                "message": "请输入账户级字段描述"
              },
              {
                "max": 80,
                "message": "长度不能超过80个字符"
              }
            ]
          },
          "acct_mod_rule": {
            "control": "select",
            "label": "账户修改规则",
            "width": "100px",
            "dict": {
              "dictType": "LN_E_ACCT_MOD_RULE",
              "dictKey": [
                "LN_E_ACCT_MOD_RULE"
              ],
              "format": "label-value"
            },
            "rules": [
              {
                "required": true,
                "message": "请输入账户修改规则"
              }
            ]
          }
        }
      ]
    }
  }
}