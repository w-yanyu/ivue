{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "scope": "formData",
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "ln3276"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "prod_code": {
                        "control": "text",
                        "label": "产品代码",
                        "width": "100px",
                        "disabled": true
                    },
                    "prod_nm": {
                        "control": "text",
                        "label": "产品名称",
                        "width": "100px",
                        "disabled": true
                    }
                },
                {
                    "prod_on_shlvs_dt": {
                        "control": "dateTimePicker",
                        "label": "产品上架日期",
                        "type": "date",
                        "width": "100px",
                        "endPlaceholder": "选择产品上架日期",
                        "unlinkPanels": true,
                        "format": "yyyyMMdd",
                        "disabledDate": {
                            "startRange": "19000101",
                            "endRange": "99991231"
                        }
                    },
                    "prod_off_shlvs_dt": {
                        "control": "dateTimePicker",
                        "label": "产品下架日期",
                        "type": "date",
                        "width": "100px",
                        "endPlaceholder": "产品下架日期",
                        "unlinkPanels": true,
                        "format": "yyyyMMdd",
                        "disabledDate": {
                            "startRange": "19000101",
                            "endRange": "99991231"
                        }
                    }
                },
                {
                    "on_off_shlvs_ste": {
                        "control": "select",
                        "label": "上下架状态",
                        "width": "100px",
                        "dict": {
                            "dictType": "LN_E_ON_OFF_SHLVS_STE",
                            "dictKey": [
                                "LN_E_ON_OFF_SHLVS_STE"
                            ],
                            "format": "label"
                        }
                    }
                }
            ]
        }
    }
}