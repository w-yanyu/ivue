{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "scope": "formData",
            "initRequest": "${{params}}",
            "btns": [
               
            ],
            "controls": [
                {
                    "loan_chrg_tp": {
                        "control": "string",
                        "label": "贷款收费类型",
                        "dict": {
                            "dictType": "LN_E_LOAN_CHRG_TP",
                            "dictKey": [
                                "LN_E_LOAN_CHRG_TP"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    },
                    "freq_chrg_tp": {
                        "control": "string",
                        "label": "频率收费类型",
                        "dict": {
                            "dictType": "LN_E_FREQ_CHRG_TP",
                            "dictKey": [
                                "LN_E_FREQ_CHRG_TP"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    }
                },
                {
                    "chrg_prd": {
                        "control": "string",
                        "label": "收费周期",
                        "width": "130px"
                    },
                    "chrg_catg": {
                        "control": "string",
                        "label": "收费种类",
                        "dict": {
                            "dictType": "LN_E_CHRG_CATG",
                            "dictKey": [
                                "LN_E_CHRG_CATG"
                            ],
                            "format": "label"
                        },
                        "width": "130px"
                    }
                },
                {
                    "chrg_code": {
                        "control": "string",
                        "label": "收费代码",
                        "width": "130px"
                    },
                    "chrg_code_nm": {
                        "control": "string",
                        "label": "收费代码名称",
                        "width": "130px"
                    }
                },
                {
                    "chrg_amt": {
                        "control": "string",
                        "label": "收费金额",
                        "width": "130px"
                    }
                }
                
            ]
        },
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": true,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "ln3280",
                  "prod_code":"${{params.prod_code}}",
                  "outlistid": "lstklnf_chrg_evt_defn"
                }
            },
            "thead": [
              {
                "label": "收费事件",
                "prop": "chrg_evt",
                "align": "center"
              },
              {
                "label": "收费事件名称",
                "prop": "chrg_evt_nm",
                "align": "center"
              },
              {
                "label": "收费种类",
                "prop": "chrg_catg",
                "align": "center"
              },
              {
                "label": "收费代码",
                "prop": "chrg_code",
                "align": "center"
              },
              {
                "label": "收费代码名称",
                "prop": "chrg_code_nm",
                "align": "center"
              },
              {
                "label": "收费金额",
                "prop": "chrg_amt",
                "align": "center"
              }
            ]
          }
    }
}