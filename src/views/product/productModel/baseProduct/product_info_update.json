  {
    "module": "dynamicTabs",
    "layout": {
      "tabs": {
        "tabHeader": [
          {
            "id": "01",
            "label": "产品信息",
            "closable": false,
            "disabled": false,
            "icon": "el-icon-date",
            "toRequest": {
              "url": "/views/product/productModel/baseProduct/update/product_info.json",
              "method": "post"
            }
          },
          {
            "id": "02",
            "label": "部件信息",
            "closable": false,
            "disabled": false,
            "icon": "el-icon-date",
            "toRequest": {
              "url": "/views/product/productModel/productTemplate/product_assembly_unit.json",
              "method": "post",
              "params": {
                "prod_tplt_code": "${{params.prod_code}}",
                "rgstn_tp": 2
              }
            }
          },
          {
            "id": "03",
            "label": "产品控制信息",
            "closable": false,
            "disabled": false,
            "icon": "el-icon-date",
            "toRequest": {
              "url": "/views/product/productModel/baseProduct/update/product_cpkz.json",
              "method": "post"
            }
          },
          {
            "id": "04",
            "label": "产品适用范围",
            "closable": false,
            "disabled": false,
            "icon": "el-icon-date",
            "toRequest": {
              "url": "/views/product/productModel/baseProduct/update/product_cpsyfw.json",
              "method": "post"
            }
          }
        ],
        "defaultActiveKey": "01",
        "position": "top",
        "styleClass": "card"
      }
    }
  }