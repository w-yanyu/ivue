

{
    "module": "v_search_btn_datagrid",
    "layout": {
        "datagrid": {
            "isInitSearch": true,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "ln3271",
                    "prod_code": "${{params.prod_code}}",
                    "outlistid": "lstklnf_aplbl_scope_ctrl"
                }
            },
            "thead": [
                {
                    "label": "产品适用类型",
                    "prop": "prod_aplbl_tp",
                    "align": "center",
                    "disabled": true,
                    "dict": {
                        "dictType": "LN_E_APLBL_TP",
                        "dictKey": [
                            "LN_E_APLBL_TP"
                        ],
                        "format": "label-value"
                    }
                },
                {
                    "label": "机构适用标志",
                    "prop": "org_aplbl_flg",
                    "align": "center",
                    "disabled": true,
                    "dict": {
                        "dictType": "LN_E_ORG_APLBL_FLG",
                        "dictKey": [
                            "LN_E_ORG_APLBL_FLG"
                        ],
                        "format": "label-value"
                    }
                },
                {
                    "label": "适用范围",
                    "prop": "aplbl_scope",
                    "align": "center"
                }
            ]
        }
    }
}