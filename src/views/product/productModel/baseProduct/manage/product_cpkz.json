{
    "module": "v_search_btn_datagrid",
    "layout": {
        "datagrid": {
            "isInitSearch": true,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "ln3271",
                    "prod_code": "${{params.prod_code}}",
                    "outlistid": "lstklnf_dflt_ctrl"
                }
            },
            "thead": [
                {
                    "label": "产品级表名",
                    "prop": "prod_lv_tab_nm",
                    "align": "center",
                    "disabled": true
                },
                {
                    "label": "产品级字段",
                    "prop": "prod_lv_fld",
                    "align": "center",
                    "disabled": true
                },
                {
                    "label": "产品级字段描述",
                    "prop": "prod_lv_fld_dsc",
                    "align": "center",
                    "disabled": true
                },
                {
                    "label": "账户级表名",
                    "prop": "acct_lv_tab_nm",
                    "align": "center",
                    "disabled": true
                },
                {
                    "label": "账户级字段",
                    "prop": "acct_lv_fld",
                    "align": "center",
                    "disabled": true
                },
                {
                    "label": "账户级字段描述",
                    "prop": "acct_lv_fld_dsc",
                    "align": "center",
                    "disabled": true
                },
                {
                    "label": "账户修改规则",
                    "prop": "acct_mod_rule",
                    "align": "center",
                    "disabled": true,
                    "dict": {
                        "dictType": "LN_E_ACCT_MOD_RULE",
                        "dictKey": [
                            "LN_E_ACCT_MOD_RULE"
                        ],
                        "format": "label-value"
                    }
                }
            ]
        }
    }
}
