{
  "module": "v_realonlyForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "controls": [
        {
          "prod_code": {
            "control": "text",
            "label": "产品代码",
            "width": "100px",
            "disabled": true
          },
          "prod_nm": {
            "control": "text",
            "label": "产品名称",
            "width": "100px",
            "disabled": true
          }
        },
        {
          "prod_dsc": {
            "control": "text",
            "label": "产品描述",
            "width": "100px",
            "disabled": true
          },
          "prod_tplt_code": {
            "control": "text",
            "label": "产品模板代码",
            "width": "100px",
            "disabled": true
          }
        },
        {
          "eff_dt": {
            "control": "text",
            "label": "生效日期",
            "width": "100px",
            "disabled": true
          },
          "invld_dt": {
            "control": "text",
            "label": "失效日期",
            "width": "100px",
            "disabled": true
          }
        },
        {
          "prod_on_shlvs_dt": {
            "control": "text",
            "label": "产品上架日期",
            "width": "100px",
            "disabled": true
          },
          "prod_off_shlvs_dt": {
            "control": "text",
            "label": "产品下架日期",
            "width": "100px",
            "disabled": true
          }
        },
        {
          "prod_grp_code": {
            "control": "text",
            "label": "产品组代码",
            "width": "100px",
            "disabled": true
          },
          "prod_grp_nm": {
            "control": "text",
            "label": "产品组名称",
            "width": "100px",
            "disabled": true
          }
        },
        {
          "loan_prod_ste": {
            "control": "select",
            "label": "贷款产品状态",
            "width": "100px",
            "disabled": true,
            "dict": {
              "dictType": "LN_E_LOAN_PROD_STE",
              "dictKey": [
                "LN_E_LOAN_PROD_STE"
              ],
              "format": "label"
            }
          },
          "on_off_shlvs_ste": {
            "control": "select",
            "label": "上下架状态",
            "width": "100px",
            "disabled": true,
            "dict": {
              "dictType": "LN_E_ON_OFF_SHLVS_STE",
              "dictKey": [
                "LN_E_ON_OFF_SHLVS_STE"
              ],
              "format": "label"
            }
          }
        },
        {
          "prod_crea_dt": {
            "control": "text",
            "label": "产品创建日期",
            "width": "100px",
            "disabled": true
          },
          "prod_crea_tlr": {
            "control": "text",
            "label": "产品创建用户",
            "width": "100px",
            "disabled": true
          }
        }
      ]
    }
