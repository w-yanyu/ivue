{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "btns": [],
      "controls": [
        {
          "prodcd": {
            "control": "text",
            "label": "产品代码",
            "width": "160px",
            "disabled": true
          },
          "crcycd": {
            "control": "select",
            "label": "货币代号",
            "width": "160px",
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": [
                "MS_CURRENCY"
              ],
              "format": "label"
            },
            "disabled": true
          }
        },
        {
          "mxtlam": {
            "control": "currency",
            "label": "单笔放款最大金额",
            "width": "160px",
            "disabled": true
          },
          "mntlam": {
            "control": "currency",
            "label": "单笔放款最小金额",
            "width": "160px",
            "disabled": true
          }
        },
        {
          "mxterm": {
            "control": "text",
            "label": "单笔放款最长贷款期限",
            "width": "160px",
            "disabled": true
          },
          "mnterm": {
            "control": "text",
            "label": "单笔放款最短贷款期限",
            "width": "160px",
            "disabled": true
          }
        },
        {
          "lendmd": {
            "control": "select",
            "label": "放款类型",
            "width": "160px",
            "dict": {
              "dictType": "E_LENDMD",
              "dictKey": [
                "E_LENDMD"
              ],
              "format": "label"
            },
            "disabled": true
          },
          "fundsr": {
            "control": "select",
            "label": "放款资金来源类型",
            "width": "160px",
            "dict": {
              "dictType": "E_FUNDSR",
              "dictKey": [
                "E_FUNDSR"
              ],
              "format": "label"
            },
            "disabled": true
          }
        },
        {
          "fundmd": {
            "control": "select",
            "label": "支付方式",
            "width": "160px",
            "dict": {
              "dictType": "E_FUNDMD",
              "dictKey": [
                "E_FUNDMD"
              ],
              "format": "label"
            },
            "disabled": true
          },
          "bchlty": {
            "control": "select",
            "label": "自主支付卡类型",
            "width": "160px",
            "dict": {
              "dictType": "E_BCHLTY",
              "dictKey": [
                "E_BCHLTY"
              ],
              "format": "label"
            },
            "disabled": true
          }
        },
        {
          "mxtmes": {
            "control": "text",
            "label": "最大放款次数",
            "width": "160px",
            "rules": [
              {
                "max": "19",
                "message": "长度不能超过19位字符"
              }
            ],
            "disabled": true
          },
          "contsm": {
            "control": "text",
            "label": "每日最大放款次数",
            "width": "160px",
            "disabled": true
          }
        },
        {
          "vadays": {
            "control": "text",
            "label": "首次动用有效期限(天)",
            "width": "160px",
            "disabled": true
          },
          "isauld": {
            "control": "select",
            "label": "自动放款标志",
            "width": "160px",
            "dict": {
              "dictType": "E_ISAULD",
              "dictKey": [
                "E_ISAULD"
              ],
              "format": "label"
            },
            "disabled": true
          }
        }
      ]
    }
  }
}