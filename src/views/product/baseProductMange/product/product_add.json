{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "scope": "formData",
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": "",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "CORELNTS9077"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "proddp": {
                        "control": "select",
                        "label": "产品系",
                        "width": "130px",
                        "dict": {
                            "dictType": "E_PRODDP",
                            "dictKey": [
                                "E_PRODDP"
                            ],
                            "format": "label"
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择产品系"
                            }
                        ]
                    },
                    "prodcd": {
                        "control": "text",
                        "label": "产品代码",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入产品代码"
                            },
                            {
                                "max": 18,
                                "message": "长度不能超过18个字符"
                            }
                        ]
                    }
                },
                {
                    "prodna": {
                        "control": "text",
                        "label": "产品名称",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入产品名称"
                            },
                            {
                                "max": 80,
                                "message": "长度不能超过80个字符"
                            }
                        ]
                    },
                    "prodtx": {
                        "control": "text",
                        "label": "产品描述",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入产品描述"
                            },
                            {
                                "max": 200,
                                "message": "长度不能超过200个字符"
                            }
                        ]
                    }
                },
                {
                    "prodst": {
                        "control": "select",
                        "label": "产品状态",
                        "width": "130px",
                        "dict": {
                            "dictType": "E_PRODST",
                            "dictKey": [
                                "E_PRODST"
                            ],
                            "format": "label"
                        },
                        "value": "0",
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择产品状态"
                            }
                        ]
                    },
                    "acmode": {
                        "control": "select",
                        "label": "核算模式",
                        "width": "130px",
                        "dict": {
                            "dictType": "E_ACMODE",
                            "dictKey": [
                                "E_ACMODE"
                            ],
                            "format": "label"
                        }
                    }
                }
            ]
        }
    }
}