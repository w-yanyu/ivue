{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "CORELNTS9051",
          "prodcd": "${{params.prodcd}}"
        }
      },
      "thead": [
        {
          "label": "产品代码",
          "prop": "prodcd"
        },
        {
          "label": "业务编码",
          "prop": "busino"
        },
        {
          "label": "交易类型",
          "prop": "trantp",
          "dict": {
            "dictType": "E_CLANTP",
            "dictKey": [
              "E_CLANTP"
            ],
            "format": "label"
          }
        },
        {
          "label": "清算类型",
          "prop": "clertp",
          "dict": {
            "dictType": "E_CLERTP",
            "dictKey": [
              "E_CLERTP"
            ],
            "format": "label"
          }
        },
        {
          "label": "渠道",
          "prop": "pychnl"
        },
        {
          "label": "操作",
          "fixed": "right",
          "width": 200,
          "align": "center",
          "btns": [
            {
              "type": "edit",
              "label": "详情",
              "toRequest": {
                "url": "/views/product/baseProductMange/product/lnfCler/lnf_cler_sub.json",
                "method": "GET"
              },
              "props": {
                "width": "70%",
                "title": "产品清算属性详情"
              }
            }
          ]
        }
      ]
    }
  }
}