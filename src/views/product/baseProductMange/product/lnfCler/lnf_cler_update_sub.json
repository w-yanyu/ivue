{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "btns": [
                {
                    "type": "submit",
                    "label": "确定",
                    "icon": "",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "CORELNTS9050"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "prodcd": {
                        "control": "text",
                        "label": "产品代码",
                        "width": "130px",
                        "disabled": true,
                        "value": "${{params.prodcd}}"
                    },
                    "busino": {
                        "control": "text",
                        "label": "业务编码",
                        "width": "130px",
                        "rules": [
                            {
                                "required": false,
                                "message": "请输入产品名称"
                            },
                            {
                                "max": 100,
                                "message": "长度不能超过100个字符"
                            }
                        ]
                    }
                },
                {
                    "trantp": {
                        "control": "string",
                        "label": "交易类型",
                        "width": "130px",
                        "dict": {
                            "dictType": "E_CLANTP",
                            "dictKey": [
                                "E_CLANTP"
                            ],
                            "format": "label",
                            "value": 0
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入交易类型"
                            }
                        ]
                    },
                    "clertp": {
                        "control": "select",
                        "label": "清算类型",
                        "width": "130px",
                        "dict": {
                            "dictType": "E_CLERTP",
                            "dictKey": [
                                "E_CLERTP"
                            ],
                            "format": "label",
                            "value": 0
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入清算类型"
                            }
                        ]
                    }
                },
                {
                    "pychnl": {
                        "control": "string",
                        "label": "渠道",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入渠道类型"
                            }
                        ]
                    }
                }
            ]
        }
    }
}