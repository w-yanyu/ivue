{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "CORELNTS9058",
          "prodcd": "${{params.prodcd}}"
        }
      },
      "thead": [
        {
          "label": "产品代码",
          "prop": "prodcd"
        },
        {
          "label": "期限",
          "prop": "termfm"
        },
        {
          "label": "币种",
          "prop": "crcycd",
          "dict": {
            "dictType": "MS_CURRENCY",
            "dictKey": [
              "MS_CURRENCY"
            ],
            "format": "label"
          }
        },
        {
          "label": "核算代码",
          "prop": "dtitcd"
        },
        {
          "label": "担保方式",
          "prop": "guarty",
          "dict": {
            "dictType": "E_GUARTY",
            "dictKey": [
              "E_GUARTY"
            ],
            "format": "label"
          }
        },
        {
          "label": "操作",
          "fixed": "right",
          "width": 200,
          "align": "center",
          "btns": [
            {
              "type": "edit",
              "label": "详情",
              "toRequest": {
                "url": "/views/product/baseProductMange/product/lnfDtrf/lnf_dtrf_sub.json",
                "method": "GET"
              },
              "props": {
                "width": "70%",
                "title": "产品核算代码映射详情"
              }
            }
          ]
        }
      ]
    }
  }
}