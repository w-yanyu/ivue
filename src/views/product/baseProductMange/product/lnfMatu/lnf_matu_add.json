{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "btns": [
        {
          "type": "submit",
          "label": "确定",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "CORELNTS9014"
            }
          }
        }
      ],
      "controls": [
        {
          "prodcd": {
            "control": "text",
            "label": "产品代码",
            "width": "160px",
            "disabled": true
          },
          "crcycd": {
            "disabled": true,
            "value":"CNY",
            "control": "select",
            "label": "货币代号",
            "width": "160px",
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": [
                "MS_CURRENCY"
              ],
              "format": "label"
            }
          }
        },
        {
          "instrp": {
            "control": "select",
            "label": "到期扣息规则",
            "width": "160px",
            "disabled": true,
            "dict": {
              "dictType": "E_INSTRP",
              "dictKey": [
                "E_INSTRP"
              ],
              "format": "label"
            },
            "#value": "#固化1-扣息",
            "value": "1"
          },
          "isexpd": {
            "control": "select",
            "label": "是否允许贷款展期",
            "width": "160px",
            "disabled": true,
            "dict": {
              "dictType": "E_ISEXPD",
              "dictKey": [
                "E_ISEXPD"
              ],
              "format": "label"
            },
            "#value": "#固化0-否",
            "value":"0",
            "showItem": {
              "1": [
                "expdtp",
                "expdts",
                "expdtm",
                "expdrp"
              ],
              "0": []
            }
          }
        },
        {
          "expdtp": {
            "initShow": false,
            "control": "select",
            "label": "展期方式",
            "width": "160px",
            "dict": {
              "dictType": "E_EXPDTP",
              "dictKey": [
                "E_EXPDTP"
              ],
              "format": "label"
            }
          },
          "expdts": {
            "initShow": false,
            "control": "text",
            "label": "展期最大次数",
            "width": "160px"
          }
        },
        {
          "expdtm": {
            "initShow": false,
            "control": "text",
            "label": "展期最长期限",
            "width": "160px"
          },
          "expdrp": {
            "initShow": false,
            "control": "select",
            "label": "展期需足额扣款",
            "width": "160px",
            "dict": {
              "dictType": "E_EXPDRP",
              "dictKey": [
                "E_EXPDRP"
              ],
              "format": "label"
            }
          }
        }
      ]
    }
  }
}