{
  "module": "v_search_btn_datagrid",
  "layout": {

    "toolbar": {
      "btns": [
        {
          "type": "add",
          "target": "dialog",
          "label": "新增",
          "toRequest": {
            "method": "GET",
            "url": "/views/product/baseProductMange/product/lnfChrg/lnf_chrg_add.json",
            "params": {
              "prodcd": "${{params.prodcd}}"
            }
          },
          "props": {
            "width": "70%",
            "title": "新增产品收费事件属性"
          }
        }
      ]
    },
    "datagrid": {
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "CORELNTS9031",
          "prodcd": "${{params.prodcd}}"
        }
      },
      "thead": [
        {
          "label": "产品代码",
          "prop": "prodcd"
        },
        {
          "label": "货币代号",
          "prop": "crcycd",
          "dict": {
            "dictType": "MS_CURRENCY",
            "dictKey": [
              "MS_CURRENCY"
            ],
            "format": "label"
          }
        },
        {
          "label": "收费类型",
          "prop": "chrtyp",
          "dict": {
            "dictType": "E_CHRTYP",
            "dictKey": [
              "E_CHRTYP"
            ],
            "format": "label"
          }
        },
        {
          "label": "提前还款罚金编号",
          "prop": "epcgcd"
        },
        {
          "label": "提前还款罚金名称",
          "prop": "epcgna"
        },
        {
          "label": "提前还款附加罚金金额",
          "prop": "epcgam"
        },
        {
          "label": "逾期还款罚金编号",
          "prop": "opcgcd"
        },
        {
          "label": "逾期还款罚金名称",
          "prop": "opcgna"
        },
        {
          "label": "逾期还款附加罚金金额",
          "prop": "opcgam"
        },
        {
          "label": "操作",
          "fixed": "right",
          "width": 200,
          "align": "center",
          "btns": [
            {
              "type": "edit",
              "label": "修改",
              "toRequest": {
                "url": "/views/product/baseProductMange/product/lnfChrg/lnf_chrg_update_sub.json",
                "method": "GET"
              },
              "props": {
                "width": "70%",
                "title": "修改产品收费基础属性"
              }
            },
            {
              "type": "custom",
              "label": "删除",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "CORELNTS9033"
                }
              },
              "confirm": {
                "title": "确定删除",
                "confirmButtonText": "确定",
                "cancelButtonText": "取消"
              }
            }
          ]
        }
      ]
    }
  }
}
