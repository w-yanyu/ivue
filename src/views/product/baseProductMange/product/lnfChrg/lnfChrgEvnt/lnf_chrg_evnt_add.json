{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "btns": [
                {
                    "type": "submit",
                    "label": "确定",
                    "icon": "",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "CORELNTS9029"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "prodcd": {
                      "control": "text",
                      "label": "产品代码",
                      "width": "160px",
                      "disabled": true
                    },
                    "crcycd": {
                        "disabled": true,
                        "value":"CNY",
                        "control": "select",
                        "label": "货币代号",
                        "width": "160px",
                        "dict": {
                            "dictType": "MS_CURRENCY",
                            "dictKey": [
                                "MS_CURRENCY"
                            ],
                            "format": "label"
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "货币代号不能为空"
                            }
                        ]
                    }
                },
                {
                    "envtcd": {
                        "control": "text",
                        "label": "收费事件",
                        "width": "160px",
                        "rules": [
                            {
                                "required": true,
                                "message": "收费事件不能为空"
                            }
                        ]
                    },
                    "envtna": {
                        "control": "text",
                        "label": "收费事件名称",
                        "width": "160px"
                    }
                },
                {
                    "chrgfg": {
                        "control": "select",
                        "label": "收费种类",
                        "width": "160px",
                        "dict": {
                            "dictType": "E_CHRGFG",
                            "dictKey": [
                                "E_CHRGFG"
                            ],
                            "format": "label"
                        }
                    },
                    "recdst": {
                        "control": "select",
                        "label": "记录状态",
                        "width": "160px",
                        "dict": {
                            "dictType": "E_RECDST",
                            "dictKey": [
                                "E_RECDST"
                            ],
                            "format": "label"
                        }
                    }
                },
                {
                    "chrgcd": {
                        "control": "text",
                        "label": "收费代码",
                        "width": "160px"
                    },
                    "chrgna": {
                        "control": "text",
                        "label": "收费代码名称",
                        "width": "160px"
                    }
                },
                {
                    "chrgam": {
                        "control": "text",
                        "label": "收费金额",
                        "width": "160px"
                    }
                }
            ]
        }
    }
}