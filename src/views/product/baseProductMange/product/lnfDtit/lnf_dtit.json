{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "CORELNTS9035",
          "prodcd": "${{params.prodcd}}"
        }
      },
      "thead": [
        {
          "label": "产品代码",
          "prop": "prodcd"
        },
        {
          "label": "货币代号",
          "prop": "crcycd",
          "dict": {
            "dictType": "MS_CURRENCY",
            "dictKey": [
              "MS_CURRENCY"
            ],
            "format": "label"
          }
        },
        {
          "label": "按一逾两呆核算",
          "prop": "clssfg",
          "dict": {
            "dictType": "E_CLSSFG",
            "dictKey": [
              "E_CLSSFG"
            ],
            "format": "label"
          }
        },
        {
          "label": "自动形态转移",
          "prop": "isauto",
          "dict": {
            "dictType": "E_ISAUTO",
            "dictKey": [
              "E_ISAUTO"
            ],
            "format": "label"
          }
        },
        {
          "label": "应计非应计核算",
          "prop": "prlnfg",
          "dict": {
            "dictType": "E_PRLNFG",
            "dictKey": [
              "E_PRLNFG"
            ],
            "format": "label"
          }
        },
        {
          "label": "贷款形态分科目核算",
          "prop": "dtitfg",
          "dict": {
            "dictType": "E_DTITFG",
            "dictKey": [
              "E_DTITFG"
            ],
            "format": "label"
          }
        },
        {
          "label": "利息转出规则",
          "prop": "inottp",
          "dict": {
            "dictType": "E_INOTTP",
            "dictKey": [
              "E_INOTTP"
            ],
            "format": "label"
          }
        },
        {
          "label": "利息转回规则",
          "prop": "inintp",
          "dict": {
            "dictType": "E_ININTP",
            "dictKey": [
              "E_ININTP"
            ],
            "format": "label"
          }
        },  
        {
          "label": "转表外是否强制缩期",
          "prop": "isqzsq",
          "align": "center",
          "dict": {    
            "dictType": "E_YES___",
            "dictKey": [
              "E_YES___"
            ],
            "format": "label"
          }
        },
        {
          "#label": "核算方式",
          "#prop": "dtittp",
          "#dict": {
            "#dictType": "E_DTITTP",
            "#dictKey": [
              "E_DTITTP"
            ],
            "#format": "label"
          }
        },
        {
          "label": "操作",
          "fixed": "right",
          "width": 200,
          "align": "center",
          "btns": [
            {
              "type": "edit",
              "label": "详情",
              "toRequest": {
                "url": "/views/product/baseProductMange/product/lnfDtit/lnf_dtit_sub.json",
                "method": "GET"
              },
              "props": {
                "width": "70%",
                "title": "产品核算属性详情"
              }
            }
          ]
        }
      ]
    }
  }
}