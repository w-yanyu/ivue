{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "scope":"formData",
      "initRequest": "${{params}}",
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "CORELNTS9043"
            }
          }
        }
      ],
      "controls": [
        {
          "prodcd": {
            "control": "text",
            "label": "产品代码",
            "width": "160px",
            "disabled": true
          },
          "isrefd": {
            "control": "select",
            "label": "是否支持退货",
            "width": "160px",
            "dict": {
              "dictType": "E_ISREFD",
              "dictKey": [
                "E_ISREFD"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择是否支持退货"
              }
            ]
          }
        },
        {
          "isptrd": {
            "control": "select",
            "label": "是否支持部分退货",
            "width": "160px",
            "dict": {
              "dictType": "E_ISPTRD",
              "dictKey": [
                "E_ISPTRD"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择是否支持部分退货"
              }
            ]
          },
          "isrdin": {
            "control": "select",
            "label": "退货是否收息",
            "width": "160px",
            "dict": {
              "dictType": "E_ISRDIN",
              "dictKey": [
                "E_ISRDIN"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择退货是否收息"
              }
            ]
          }       
        },
        {
          "isrdfe": {
            "control": "select",
            "label": "是否收取退货手续费",
            "width": "160px",
            "dict": {
              "dictType": "E_ISRDFE",
              "dictKey": [
                "E_ISRDFE"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择是否收取退货手续费"
              }
            ]
          },
          "rdmxdy": {
            "control": "text",
            "label": "允许退货最大天数(天)",
            "width": "160px",
            "rules": [
              {
                "max": "19",
                "message": "长度不能超过19位字符"
              },
              {
                "required": true,
                "message": "请输入允许退货最大天数（天）"
              }
            ]
          }
        }          
      ]
    }
  }
}