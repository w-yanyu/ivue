{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": "",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "CORELNTS9034"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "prodcd": {
                        "control": "text",
                        "label": "产品代码",
                        "width": "160px",
                        "disabled":true
                    },
                    "proddp": {
                        "control": "select",
                        "label": "产品系",
                        "width": "160px",
                        "dict": {
                            "dictType": "E_PRODDP",
                            "dictKey": [
                                "E_PRODDP"
                            ],
                            "format": "label"
                        }
                    }
                },
                {
                    "prodna": {
                        "control": "text",
                        "label": "产品名称",
                        "width": "160px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入产品名称"
                            },
                            {
                                "max": 80,
                                "message": "长度不能超过80个字符"
                            }
                        ]
                    },
                    "prodtx": {
                        "control": "text",
                        "label": "产品描述",
                        "width": "160px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入产品描述"
                            },
                            {
                                "max": 200,
                                "message": "长度不能超过200个字符"
                            }
                        ]
                    }
                },
                {
                    "coorpr": {
                        "control": "select",
                        "label": "贷款对象",
                        "width": "160px",
                        "disabled": "true",
                        "dict": {
                            "dictType": "E_COORPR",
                            "dictKey": [
                                "E_COORPR"
                            ],
                            "format": "label"
                        },
                        "value": "1"
                    },
                    "lncutp": {
                        "control": "select",
                        "label": "贷款对象细分",
                        "width": "160px",
                        "disabled": "true",
                        "dict": {
                            "dictType": "E_LNCUTP",
                            "dictKey": [
                                "E_LNCUTP"
                            ],
                            "format": "label"
                        },
                        "value": "11"
                    }
                },
                {
                    "lnbztp": {
                        "control": "select",
                        "label": "业务分类",
                        "width": "160px",
                        "dict": {
                            "dictType": "E_LNBZTP",
                            "dictKey": [
                                "E_LNBZTP"
                            ],
                            "format": "label"
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入业务分类"
                            }
                        ]
                    },
                    "iscycl": {
                        "control": "select",
                        "label": "是否循环贷款",
                        "width": "160px",
                        "dict": {
                            "dictType": "E_ISCYCL",
                            "dictKey": [
                                "E_ISCYCL"
                            ],
                            "format": "label"
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入是否循环贷款"
                            }
                        ]
                    }
                },
                {
                    "istxln": {
                        "control": "select",
                        "label": "是否补贴贷款",
                        "width": "160px",
                        "dict": {
                            "dictType": "E_ISTXLN",
                            "dictKey": [
                                "E_ISTXLN"
                            ],
                            "format": "label"
                        }
                    },
                    "issynd": {
                        "control": "select",
                        "label": "是否联合贷款",
                        "width": "160px",
                        "dict": {
                            "dictType": "E_ISSYND",
                            "dictKey": [
                                "E_ISSYND"
                            ],
                            "format": "label"
                        },
                        "value": "0"
                    },
                    "iscred": {
                        "control": "select",
                        "label": "是否授信",
                        "width": "160px",
                        "dict": {
                            "dictType": "E_ISCRED",
                            "dictKey": [
                                "E_ISCRED"
                            ],
                            "format": "label"
                        }
                    }
                },
                {
                    "acmode": {
                        "control": "select",
                        "label": "核算模式",
                        "width": "160px",
                        "dict": {
                            "dictType": "E_ACMODE",
                            "dictKey": [
                                "E_ACMODE"
                            ],
                            "format": "label"
                        }
                    },
                    "efctdt": {
                        "label": "生效日期",
                        "width": "160px",
                        "control": "dateTimePicker",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入生效日期"
                            }
                        ]
                    },
                    "inefdt": {
                        "label": "失效日期",
                        "width": "160px",
                        "control": "dateTimePicker",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入失效日期"
                            }
                        ]
                    }
                },
                {
                    "prodst": {
                        "control": "hidden",
                        "label": "产品状态",
                        "width": "160px",
                        "dict": {
                            "dictType": "E_PRODST",
                            "dictKey": [
                                "E_PRODST"
                            ],
                            "format": "label"
                        }
                    }
                }
            ]
        }
    }
}