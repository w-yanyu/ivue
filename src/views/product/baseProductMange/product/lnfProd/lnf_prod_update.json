{
    "module": "v_search_btn_datagrid",
    "layout": {    
      "datagrid": {
        "doRequest": {
          "url": "/SUMP/icmcall/icmRPCCall",
          "method": "POST",
          "params": {
            "servicecode": "CORELNTS2101",
            "prodcd": "${{params.prodcd}}"
          }
        },
        "thead": [
          {
            "label": "产品代码",
            "prop": "prodcd"
          },
          {
            "label": "产品名称",
            "prop": "prodna"
          },
          {
            "label": "产品系",
            "prop": "proddp",
            "dict": {
              "dictType": "E_PRODDP",
              "dictKey": [
                "E_PRODDP"
              ],
              "format": "label"
            }
          },
          {
            "label": "产品描述",
            "prop": "prodtx"
          },
          {
            "label": "贷款对象",
            "prop": "coorpr",
            "dict": {
              "dictType": "E_COORPR",
              "dictKey": [
                "E_COORPR"
              ],
              "format": "label"
            }
          },
          {
            "label": "贷款对象细分",
            "prop": "lncutp",
            "dict": {
              "dictType": "E_LNCUTP",
              "dictKey": [
                "E_LNCUTP"
              ],
              "format": "label"
            }
          },
          {
            "label": "业务分类",
            "prop": "lnbztp",
            "dict": {
              "dictType": "E_LNBZTP",
              "dictKey": [
                "E_LNBZTP"
              ],
              "format": "label"
            }
          },
          {
            "label": "是否循环贷款",
            "prop": "iscycl",
            "dict": {
              "dictType": "E_ISCYCL",
              "dictKey": [
                "E_ISCYCL"
              ],
              "format": "label"
            }
          },
          {
            "label": "是否补贴贷款",
            "prop": "istxln",
            "dict": {
              "dictType": "E_ISTXLN",
              "dictKey": [
                "E_ISTXLN"
              ],
              "format": "label"
            }
          },
          {
            "label": "是否联合贷款",
            "prop": "issynd",
            "dict": {
              "dictType": "E_ISSYND",
              "dictKey": [
                "E_ISSYND"
              ],
              "format": "label"
            }
          },
          {
            "label": "是否授信",
            "prop": "iscred",
            "dict": {
              "dictType": "E_ISCRED",
              "dictKey": [
                "E_ISCRED"
              ],
              "format": "label"
            }
          },
          {
            "label": "核算模式",
            "prop": "acmode",
            "dict": {
                "dictType": "E_ACMODE",
                "dictKey": [
                    "E_ACMODE"
                ],
                "format": "label"
            }
          },
          {
            "label": "生效日期",
            "prop": "efctdt"
          },
          {
            "label": "失效日期",
            "prop": "inefdt"
          },
          {
            "label": "产品建立日期",
            "prop": "creadt"
          },
          {
            "label": "产品状态",
            "prop": "prodst",
            "dict": {
                "dictType": "E_PRODST",
                "dictKey": [
                    "E_PRODST"
                ],
                "format": "label"
            }
          },
          {
            "label": "操作",
            "fixed": "right",
            "width": 200,
            "align": "center",
            "btns": [
              {
                "type": "edit",
                "label": "修改",
                "toRequest": {
                  "url": "/views/product/baseProductMange/product/lnfProd/lnf_prod_update_sub.json",
                  "method": "GET"
                },
                "props": {
                  "width": "70%",
                  "title": "产品基础属性修改"
                }
              }
            ]
          }
        ]
      }
    }
  }