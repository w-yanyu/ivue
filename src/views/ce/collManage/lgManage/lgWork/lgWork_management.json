{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "foldLineNumber": 3,
            "scope": "formData",
            "controls": [
            {
              "id_no": {
                "control": "text",
                "label": "证件号码"
              },
              "cust_id": {
                "control": "text",
                "label": "客户编号"
              }
            },
            {
              "duebill_no": {
                "control": "text",
                "label": "借据号"
              },
              "is_call_end": {
                "control": "select",
                "label": "是否完成",
                "dict": {
                  "format": "label",
                  "dictKey": [
                    "MS_E_YESORNO"
                  ],
                  "dictType": "MS_E_YESORNO"
                }
              }
            },
            {
              "loan_legal_process": {
                "control": "remoteSelect",
                "label": "进度状态",
                "labelField": "drop_list_desc",
                "valueField": "drop_list_value",
                "doRequest": {
                  "url": "/SUMP/icmcall/icmRPCCall",
                  "method": "POST",
                  "params": {
                    "servicecode": "ap1071",
                    "drop_list_type": "loan_legal_process"
                  }
                },
                "format": "label"
              }
            },
            {
              "queue_cd": {
                "control": "remoteSelect",
                "label": "队列名称",
                "labelField": "queue_name",
                "valueField": "queue_cd",
                "doRequest": {
                  "url": "/SUMP/icmcall/icmRPCCall",
                  "method": "POST",
                  "params": {
                    "servicecode": "pl0402"
                  }
                },
                "format": "label"
              },
              "location_cd": {
                "control": "remoteSelect",
                "label": "案件类型",
                "labelField": "location_name",
                "valueField": "location_cd",
                "doRequest": {
                  "url": "/SUMP/icmcall/icmRPCCall",
                  "method": "POST",
                  "params": {
                    "servicecode": "pl0410"
                  }
                },
                "format": "label"
              }
            },
            {
              "status_cd": {
                "control": "remoteSelect",
                "label": "案件状态",
                "labelField": "status_name",
                "valueField": "status_cd",
                "doRequest": {
                  "url": "/SUMP/icmcall/icmRPCCall",
                  "method": "POST",
                  "params": {
                    "servicecode": "pl0411"
                  }
                },
                "format": "label"
              },
              "function_cd": {
                "control": "remoteSelect",
                "label": "逾期阶段",
                "labelField": "function_name",
                "valueField": "function_cd",
                "doRequest": {
                  "url": "/SUMP/icmcall/icmRPCCall",
                  "method": "POST",
                  "params": {
                    "servicecode": "pl0412"
                  }
                },
                "format": "label"
              }
            },
            {
              "start_day": {
                "control": "inputNumber",
                "label": "逾期天数开始值"
              },
              "end_day": {
                "control": "inputNumber",
                "label": "逾期天数结束值"
              }
            },
            {
              "ovd_amt_total_1": {
                "control": "currency",
                "label": "欠款金额区间开始值"
              },
              "ovd_amt_total_2": {
                "control": "currency",
                "label": "欠款金额区间结束值"
              }
            },
            {
              "collect_date_1": {
                "control": "inputNumber",
                "label": "入催天数区间开始值"

              },

              "collect_date_2": {
                "control": "inputNumber",
                "label": "入催天数区间结束值"
              }
            }
          ]
        },
        "toolbar": {
            "btns": [
                {
                  "type": "custom",
                  "icon": "el-icon-date",
                  "target": "dialog",
                  "checkCountMin": 1,
                  "label": "展期",
                  "toRequest": {
                    "method": "GET",
                    "url": "/views/pl/collManage/lgManage/lgWork/lgwork_extension.json"
                  }
                },
                {
                  "type": "custom",
                  "icon": "el-icon-back",
                  "target": "dialog",
                  "checkCountMin": 1,
                  "label": "退件",
                  "confirm": {
                    "title": "提示信息",
                    "msg": "确定退件吗？",
                    "confirmButtonText": "确定",
                    "cancelButtonText": "取消"
                  },
                  "doRequest": {
                    "method": "POST",
                    "url": "/SUMP/icmcall/icmRPCCall",
                    "params": {
                      "servicecode":"pl4686"
                    }
                  }
                },
                {
                  "type": "custom",
                  "icon": "el-icon-picture",
                  "target": "dialog",
                  "checkCountMin": 1,
                  "checkCountMax": 1,
                  "label": "影像资料",
                  "toRequest": {
                    "method": "GET",
                    "url": "/views/pl/collManage/lgManage/lgWork/lgwork_plVideo.json"
                  }
                },
                {
                  "type": "custom",
                  "icon": "el-icon-view",
                  "target": "dialog",
                  "checkCountMin": 1,
                  "label": "证据预览",
                  "toRequest": {
                    "method": "GET",
                    "url": "/views/pl/collManage/lgManage/lgWork/lgwork_plVideo.json"
                  }
                },
                {
                  "type": "custom",
                  "icon": "el-icon-circle-check",
                  "target": "dialog",
                  "checkCountMin": 1,
                  "label": "完成",
                  "confirm": {
                    "title": "提示信息",
                    "msg": "确定完成吗？",
                    "confirmButtonText": "确定",
                    "cancelButtonText": "取消"
                  },
                  "doRequest": {
                    "method": "POST",
                    "url": "/SUMP/icmcall/icmRPCCall",
                    "params": {
                      "servicecode":"pl4674"
                    }
                  }
                }
            ]
        },
        "datagrid": {
            "isInitSearch": false,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "pl4664"
                }
            },
            "thead": [
                {
                    "type": "selection",
                    "align":"center",
                    "width": "55",
                    "fixed": "left"
                },
                {
                    "label": "操作",
                    "align":"center",
                    "fixed": "right",
                    "width": "200px",
                    "btns": [
                        {
                          "type": "look",
                          "label": "案件详情",
                          "toRequest": {
                            "url": "/views/pl/assistManage/caseDetail/caseDetail_2/case_detail_main.json",
                            "method": "POST",
                            "params": {
                              "case_id": "${{current.case_id}}"
                            }
                          },
                          "props": {
                            "width": "80%",
                            "title": "案件详情"
                          }
                        }
                    ]
                },
                {
                    "type":"string",
                    "align":"center",
                    "label": "客户编号",
                    "prop": "case_id"

                },
                {
                    "type":"string",
                    "align":"center",
                    "label": "客户姓名",
                    "prop": "cust_name"
                },
                {
                  "type":"string",
                  "align":"center",
                  "label": "证件类型",
                  "width": "150",
                  "prop": "id_type",
                  "dict":{
                    "dictKey": [
                      "id_type"
                    ],
                    "format": "label",
                    "labelField": "drop_list_desc",
                    "valueField": "drop_list_value",
                    "doRequest": {
                      "url": "/SUMP/icmcall/icmRPCCall",
                      "method": "POST",
                      "params": {
                        "servicecode": "ap1071",
                        "drop_list_type": "CertType"
                      }
                    }
                  }
                },
                {
                    "type":"string",
                    "align":"center",
                    "label": "证件号码",
                    "prop": "id_no"
                },
                {
                    "type":"string",
                    "align":"center",
                    "label": "逾期天数",
                    "prop": "over_due_day"
                },
                {
                    "type":"string",
                    "align":"center",
                    "label": "诉讼公司",
                    "width": "180",
                    "prop": "legal_org_id",
                    "dict": {
                      "dictKey": [
                        "legal_org_id"
                      ],
                      "format": "label",
                      "labelField": "coll_org_name",
                      "valueField": "coll_org_id",
                      "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                          "servicecode":"pl0416",
                          "coll_org_type":"LG01",
                          "is_valid":"Y"
                        }
                      }
                    }
                },
                {
                  "type":"string",
                  "align":"center",
                  "width":"120",
                  "label": "诉讼状态",
                  "prop": "legal_status",
                  "dict": {
                    "dictKey": [
                      "legal_status"
                    ],
                    "format": "label",
                    "labelField": "drop_list_desc",
                    "valueField": "drop_list_value",
                    "doRequest": {
                      "url": "/SUMP/icmcall/icmRPCCall",
                      "method": "POST",
                      "params": {
                        "servicecode": "ap1071",
                        "drop_list_type": "lg_status"
                      }
                    }
                  }
                },
                {
                    "type":"string",
                    "align":"center",
                    "label": "诉讼金额",
                    "prop": "oa_amt"
                },
                {
                  "type":"string",
                  "align":"center",
                  "label": "法诉开始日期",
                  "prop": "legal_begin_date"
                },
                {
                    "type":"string",
                    "align":"center",
                    "label": "法诉结束日期",
                    "prop": "legal_end_date"
                },
                {
                    "type":"string",
                    "align":"center",
                    "width":"120",
                    "label": "进度状态",
                    "prop": "loan_legal_process",
                    "dict": {
                      "dictKey": [
                        "loan_legal_process"
                      ],
                      "format": "label",
                      "labelField": "drop_list_desc",
                      "valueField": "drop_list_value",
                      "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                          "servicecode": "ap1071",
                          "drop_list_type": "loan_legal_process"
                        }
                      }
                    }
                },
                {
                  "align":"center",
                  "label": "是否完成",
                  "prop": "is_call_end",
                  "type": "select",
                  "dict": {
                    "format": "label",
                    "dictKey": [
                      "MS_E_YESORNO"
                    ],
                    "dictType": "MS_E_YESORNO"
                  }
                }
            ]
        }
    }
}
