{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "foldLineNumber": 3,
            "scope": "formData",
            "controls": [
                {
                    "id_no": {
                        "control": "text",
                        "label": "证件号码"
                    },
                    "cust_name": {
                        "control": "text",
                        "label": "客户姓名"
                    }
                },
                {
                    "daterange": {
                        "control": "dateTimePicker",
                        "label": "诉讼日期",
                        "type": "daterange",
                        "unlinkPanels": true,
                        "valueFormat": "yyyyMMdd",
                        "startPlaceholder": "选择开始时间",
                        "endPlaceholder": "选择结束时间",
                        "startField":"legal_begin_date",
                        "endField":"legal_end_date",
                        "disabledDate":"dateAfter"
                    },
                    "oa_org_id": {
                        "control": "remoteSelect",
                        "label": "诉讼公司",
                        "labelField": "coll_org_name",
                        "valueField": "coll_org_id",
                        "doRequest":{
                            "url": "/SUMP/icmcall/icmRPCCall",
                            "method": "POST",
                            "params":{
                                "servicecode":"pl0416",
                                "coll_org_type":"LG01",
                                "is_valid":"Y"
                            }
                        },
                        "format":"label"
                    }
                }
            ]
        },
        "toolbar": {
          "btns": [
            {
              "type": "export",
              "icon": "iconfont icon-daochu",
              "target": "dialog",
              "label": "导出",
              "doRequest": {
                "method": "POST",
                "url": "/SUMP/icmcall/export",
                "params": {
                  "servicecode":"pl4689"
                }
              },
              "confirm":{
                "title":"提示信息",
                "msg":"确认导出吗？",
                "confirmButtonText":"确定",
                "cancelButtonText":"取消"
              }
            }
          ]
      },
        "datagrid": {
            "isInitSearch": false,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "pl4688"
                }
            },
            "thead": [
              {
                "type":"string",
                "align":"center",
                "width": "150",
                "label": "机构",
                "prop": "branch_id",
                "dict":{
                  "dictKey": [
                    "branch_id"
                  ],
                  "format": "label",
                  "labelField": "branch_name",
                  "valueField": "branch_id",
                  "doRequest": {
                    "url": "/SUMP/icmcall/icmRPCCall",
                    "method": "POST",
                    "params": {
                      "servicecode": "pl0400"
                    }
                  }
                }

              },
              {
                "type":"string",
                "align":"center",
                "width": "150",
                "label": "交易流水",
                "prop": "jioyls"
              },
              {
                "type":"string",
                "align":"center",
                "width": "150",
                "label": "案件编号",
                "prop": "case_id"
              },
              {
                "type":"string",
                "align":"center",
                "width": "150",
                "label": "客户姓名",
                "prop": "cust_name"
              },
              {
                "type":"string",
                "align":"center",
                "width": "150",
                "label": "借据号",
                "prop": "duebill_no"
              },
              {
                "type":"string",
                "align":"center",
                "width": "150",
                "label": "证件类型",
                "prop": "id_type"
              },
              {
                "type":"string",
                "align":"center",
                "width": "150",
                "label": "证件号码",
                "prop": "id_no"
              },
              {
                "type":"string",
                "align":"center",
                "width": "150",
                "label": "法诉公司",
                "prop": "legal_org_id"
              },
              {
                "type":"string",
                "align":"center",
                "width": "150",
                "label": "法诉开始日期",
                "prop": "legal_begin_date"
              },
              {
                "type":"string",
                "align":"center",
                "width": "150",
                "label": "法诉结束日期",
                "prop": "legal_end_date"
              },
              {
                "type":"string",
                "align":"center",
                "width": "150",
                "label": "实际结束日期",
                "prop": "real_end_date"
              },
              {
                "type":"string",
                "align":"center",
                "width": "150",
                "label": "法诉金额",
                "prop": "oa_amt"
              },
              {
                "type":"string",
                "align":"center",
                "width": "150",
                "label": "交易日期",
                "prop": "jioyrq"
              },
              {
                "type":"string",
                "align":"center",
                "width": "150",
                "label": "有效法诉回收金额",
                "prop": "pmt_amt_without_exce"
              },
              {
                "type":"string",
                "align":"center",
                "width": "150",
                "label": "逾期天数",
                "prop": "over_due_day"
              },
              {
                "type":"string",
                "align":"center",
                "width": "150",
                "label": "费率(%)",
                "prop": "principal_rate"
              },
              {
                "type":"string",
                "align":"center",
                "width": "150",
                "label": "佣金",
                "prop": "real_comm"
              }
            ]
        }
    }
}
