{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
          "foldLineNumber": 3,
            "scope": "formData",
            "controls": [
                {
                    "branch_org": {
                      "control": "remoteSelect",
                      "label": "机构",
                      "labelField": "branch_name",
                      "valueField": "branch_id",
                      "doRequest": {
                        "url": "/SUMP/call/RPCCall",
                        "method": "POST",
                        "params": {
                          "servicecode": "ct2173"
                        }
                      },
                      "format": "label"
                    },
                    "cust_name": {
                        "control": "text",
                        "label": "客户姓名"
                    }
                },
                {
                    "cert_no": {
                        "control": "text",
                        "label": "证件号码"
                    },
                    "cust_no": {
                        "control": "text",
                        "label": "客户编号"
                    }
                },
                {
                    "loan_no": {
                        "control": "text",
                        "label": "借据号"
                    },
                    "overdue_stage_code": {
                      "control": "remoteSelect",
                      "label": "逾期阶段",
                      "labelField": "ovd_period_name",
                      "valueField": "overdue_stage_code",
                      "doRequest": {
                        "url": "/SUMP/call/RPCCall",
                        "method": "POST",
                        "params": {
                          "servicecode": "ce0412"
                        }
                      },
                      "format": "value-label"
                    }
                },
                {
                    "start_day": {
                        "control": "inputNumber",
                        "label": "逾期天数开始值"
                    },
                    "end_day": {
                        "control": "inputNumber",
                        "label": "逾期天数结束值"
                    }
                },
                {
                    "queue_code": {
                      "control": "remoteSelect",
                      "label": "队列名称",
                      "labelField": "queue_name",
                      "valueField": "queue_code",
                      "doRequest": {
                        "url": "/SUMP/call/RPCCall",
                        "method": "POST",
                        "params": {
                          "servicecode": "ce0402"
                        }
                      },
                      "format": "value-label"
                    },
                    "casestate_code": {
                      "control": "remoteSelect",
                      "label": "案件状态",
                      "labelField": "status_name",
                      "valueField": "casestate_code",
                      "doRequest": {
                        "url": "/SUMP/call/RPCCall",
                        "method": "POST",
                        "params": {
                          "servicecode": "ce0411"
                        }
                      },
                      "format": "value-label"
                    }
                },
                {
                    "oa_hands": {
                        "control": "inputNumber",
                        "label": "手別"
                    },
                    "case_cls_code": {
                      "control": "remoteSelect",
                      "label": "案件类型",
                      "labelField": "location_name",
                      "valueField": "case_cls_code",
                      "doRequest": {
                        "url": "/SUMP/call/RPCCall",
                        "method": "POST",
                        "params": {
                          "servicecode": "ce0410"
                        }
                      },
                      "format": "value-label"
                    }
                },
                {
                    "ovd_amt_total_1": {
                        "control": "inputNumber",
                        "label": "欠款金额区间开始值"
                    },
                    "ovd_amt_total_2": {
                        "control": "inputNumber",
                        "label": "欠款金额区间结束值"
                    }
                },
                {
                    "collect_date_1": {
                        "control": "inputNumber",
                        "label": "入催天数区间开始值"
                    },
                    "collect_date_2": {
                        "control": "inputNumber",
                        "label": "入催天数区间结束值"
                    }
                }
            ]
        },
        "toolbar": {
            "btns": [
                {
                    "type": "custom",
                    "icon": "el-icon-edit",
                    "target": "dialog",
                    "checkCountMin":1,
                    "label": "人工分配",
                    "toRequest": {
                        "method": "GET",
                        "url": "/views/ce/collManage/oaManage/oaAssign/oaAssign_artificial.json"
                    }
                },
                {
                    "type": "custom",
                    "icon": "el-icon-bell",
                    "target": "dialog",
                    "checkCountMin":1,
                    "label": "自动分配",
                    "toRequest": {
                        "method": "GET",
                        "url": "/views/ce/collManage/oaManage/oaAssign/oaAssign_auto.json"
                    }
                },
                {
                    "type": "delete",
                    "icon": "el-icon-back",
                    "checkCountMin":1,
                    "label": "委外退案",
                    "doRequest": {
                        "url": "/SUMP/call/RPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "ce4561",
                            "case_no": ""
                        }
                    },
                    "confirm": {
                        "title": "委外退案提醒",
                        "msg": "确定将案件退案，退出委外队列？",
                        "confirmButtonText": "确定",
                        "cancelButtonText": "取消"
                    }
                }
            ]
        },

        "datagrid": {
            "isInitSearch": false,
            "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "ce4560"
                }
            },
            "thead": [
                {
                    "type": "selection",
                    "align":"center",
                    "width": "55",
                    "fixed": "left"
                },
                {
                  "type": "string",
                  "align": "center",
                  "width": "100",
                  "label": "机构",
                  "prop": "branch_org",
                  "dict": {
                    "dictKey": [
                      "branch_org"
                    ],
                    "format": "label",
                    "labelField": "branch_name",
                    "valueField": "branch_id",
                    "doRequest": {
                      "url": "/SUMP/call/RPCCall",
                      "method": "POST",
                      "params": {
                        "servicecode": "ct2173"
                      }
                    }
                  }
                },
                {
                    "type":"string",
                    "align":"center",
                     "width":"180px",
                    "label": "客户编号",
                    "prop": "cust_no"

                },
                {
                    "type":"string",
                    "align":"center",
                     "width":"150px",
                    "label": "客户姓名",
                    "prop": "cust_name"
                },
                {
                  "type": "string",
                  "align": "center",
                  "label": "证件类型",
                  "width": "150px",
                  "prop": "cert_type",
                  "dict": {
                    "dictType": "MS_E_CRET_TYPE",
                    "dictKey": [
                      "MS_E_CRET_TYPE"
                    ],
                    "format": "value-label"
                  }
                },
                {
                    "type":"string",
                    "align":"center",
                     "width":"180px",
                    "label": "证件号码",
                    "prop": "cert_no"
                },
                {
                    "type":"string",
                    "align":"center",
                     "width":"100px",
                    "label": "逾期阶段",
                    "prop": "former_ovd_period",
                    "dict":{
                        "dictKey": [
                            "overdue_stage_code"
                        ],
                        "format": "value-label",
                        "labelField": "ovd_period_name",
                        "valueField": "overdue_stage_code",
                        "doRequest": {
                            "url": "/SUMP/call/RPCCall",
                            "method": "POST",
                            "params": {
                                "servicecode":"ce0412"
                            }
                        }
                    }
                },
                {
                    "type":"string",
                    "align":"center",
                     "width":"100px",
                    "label": "逾期金额",
                    "prop": "overdue_amt"
                },
                {
                    "type":"string",
                    "align":"center",
                     "width":"100px",
                    "label": "逾期天数",
                    "prop": "over_due_day"
                },
                {
                    "type":"string",
                    "align":"center",
                     "width":"50px",
                    "label": "手别",
                    "prop": "oa_hands"
                },
                {
                    "type":"string",
                    "label": "案件分类",
                    "width":"100px",
                    "align":"center",
                    "prop": "case_cls_code",
                    "dict":{
                      "dictKey": [
                        "case_cls_code"
                      ],
                      "format": "value-label",
                      "labelField": "location_name",
                      "valueField": "case_cls_code",
                      "doRequest": {
                        "url": "/SUMP/call/RPCCall",
                        "method": "POST",
                        "params": {
                          "servicecode":"ce0410"
                        }
                      }
                    }
                },
                {
                    "type":"string",
                    "label": "案件状态",
                    "align":"center",
                    "width":"100px",
                    "prop": "casestate_code",
                    "dict":{
                      "dictKey": [
                        "casestate_code"
                      ],
                      "format": "value-label",
                      "labelField": "status_name",
                      "valueField": "casestate_code",
                      "doRequest": {
                        "url": "/SUMP/call/RPCCall",
                        "method": "POST",
                        "params": {
                          "servicecode":"ce0411"
                        }
                      }
                    }
                },
                {
                    "type":"string",
                    "label": "队列名称",
                    "align":"center",
                    "width":"100px",
                    "prop": "queue_code",
                    "dict":{
                      "dictKey": [
                        "queue_code"
                      ],
                      "format": "value-label",
                      "labelField": "queue_name",
                      "valueField": "queue_code",
                      "doRequest": {
                        "url": "/SUMP/call/RPCCall",
                        "method": "POST",
                        "params": {
                          "servicecode":"ce0402"
                        }
                      }
                    }
                },
                {
                  "type": "string",
                  "align": "center",
                  "width": "120",
                  "label": "入催日期",
                  "prop": "reminder_date"
                }
            ]
        }
    }
}
