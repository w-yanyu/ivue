{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "controls": [
                {
                    "showinfo": {
                        "control": "link",
                        "value": "",
                        "labelWidth": "",
                        "label": "确认将案件退案，退出委外队列？"
                    }
                }
            ],
            "btns": [
                {
                    "type": "submit",
                    "label": "确认",
                    "doRequest": {
                        "method": "POST",
                        "url": "/SUMP/call/RPCCall",
                        "params": {
                            "servicecode": "ce4561",
                            "case_no": "${{params.selectedData}}"
                        }
                    }
                },
                {
                    "type": "close",
                    "label": "取消"
                }
            ]
        }
    }
}