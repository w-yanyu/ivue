{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "foldLineNumber": 3,
      "scope": "formData",
      "controls": [
        {
          "branch_org": {
            "control": "remoteSelect",
            "label": "机构",
            "labelField": "branch_name",
            "valueField": "branch_id",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2173"
              }
            },
            "format": "label"
          },
          "cust_name": {
            "control": "text",
            "label": "客户姓名"
          }
        },
        {
          "cert_no": {
            "control": "text",
            "label": "证件号码"
          },
          "oa_org_code": {
            "control": "remoteSelect",
            "label": "委外公司",
            "labelField": "coll_org_name",
            "valueField": "coll_org_no",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ce0416",
                "coll_org_type": "OA01",
                "coll_kind":"02",
                "is_valid": "1"
              }
            },
            "format": "label"
          }
        },
        {
          "daterange": {
            "control": "dateTimePicker",
            "label": "委外日期",
            "type": "daterange",
            "unlinkPanels": true,
            "valueFormat": "yyyyMMdd",
            "startPlaceholder": "选择开始时间",
            "endPlaceholder": "选择结束时间",
            "startField":"outsourcing_start_date",
            "endField":"outsourcing_end_date"
          },
          "oa_hands": {
            "control": "inputNumber",
            "label": "手别"
          }
        },
        {
          "start_day": {
            "control": "inputNumber",
            "label": "逾期天数开始值"
          },
          "end_day": {
            "control": "inputNumber",
            "label": "逾期天数结束值"
          }
        },
        {
          "overdue_stage_code": {
            "control": "remoteSelect",
            "label": "逾期阶段",
            "labelField": "ovd_period_name",
            "valueField": "overdue_stage_code",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ce0412"
              }
            },
            "format": "label"
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "export",
          "icon": "iconfont icon-daochu",
          "target": "dialog",
          "label": "导出",
          "doRequest": {
            "method": "POST",
            "url": "/SUMP/icmcall/export",
            "params": {
              "servicecode":"pl4594"
            }
          },
          "confirm":{
            "title":"提示信息",
            "msg":"确认导出吗？",
            "confirmButtonText":"确定",
            "cancelButtonText":"取消"
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/call/RPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ce4593"
        }
      },
      "thead": [
        {
          "type":"string",
          "align":"center",
          "width": "150",
          "label": "机构",
          "prop": "branch_org",
          "dict":{
            "dictKey": [
              "branch_org"
            ],
            "format": "label",
            "labelField": "branch_name",
            "valueField": "branch_id",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2173"
              }
            }
          }

        },
        {
          "type":"string",
          "align":"center",
          "width": "150",
          "label": "交易流水",
          "prop": "trxn_seq"
        },
        {
          "type":"string",
          "align":"center",
          "width": "150",
          "label": "案件编号",
          "prop": "case_no"
        },
        {
          "type":"string",
          "align":"center",
          "width": "150",
          "label": "客户姓名",
          "prop": "cust_name"
        },
        {
          "type":"string",
          "align":"center",
          "width": "150",
          "label": "借据号",
          "prop": "loan_no"
        },
        {
          "type": "string",
          "align": "center",
          "width": "180px",
          "label": "证件类型",
          "prop": "cert_type",
          "dict": {
            "dictType": "MS_E_CRET_TYPE",
            "dictKey": [
              "MS_E_CRET_TYPE"
            ],
            "format": "label"
          }
        },
        {
          "type":"string",
          "align":"center",
          "width": "150",
          "label": "证件号码",
          "prop": "cert_no"
        },
        {
          "type": "string",
          "align": "center",
          "width": "150",
          "label": "委外公司",
          "prop": "oa_org_code",
          "dict": {
            "dictKey": [
              "oa_org_code"
            ],
            "format": "label",
            "labelField": "coll_org_name",
            "valueField": "coll_org_no",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ce0416"
              }
            }
          }
        },
        {
          "type":"string",
          "align":"center",
          "width": "150",
          "label": "委外开始日期",
          "prop": "outsourcing_start_date"
        },
        {
          "type":"string",
          "align":"center",
          "width": "150",
          "label": "委外结束日期",
          "prop": "outsourcing_end_date"
        },
        {
          "type":"string",
          "align":"center",
          "width": "150",
          "label": "实际结束日期",
          "prop": "actual_end_date"
        },
        {
          "type":"string",
          "align":"center",
          "width": "150",
          "label": "委外金额",
          "prop": "outsourcing_amt"
        },
        {
          "type":"string",
          "align":"center",
          "width": "150",
          "label": "交易日期",
          "prop": "tran_date"
        },
        {
          "type":"string",
          "align":"center",
          "width": "150",
          "label": "有效委外回收金额",
          "prop": "pmt_amt_without_exce"
        },
        {
          "type":"string",
          "align":"center",
          "width": "150",
          "label": "主要担保类型",
          "prop": "main_guar_way"
        },
        {
          "type":"string",
          "align":"center",
          "width": "150",
          "label": "逾期天数",
          "prop": "over_due_day"
        },
        {
          "type":"string",
          "align":"center",
          "width": "150",
          "label": "费率(%)",
          "prop": "principal_rate"
        },
        {
          "type":"string",
          "align":"center",
          "width": "150",
          "label": "佣金",
          "prop": "due_commis"
        }
      ]
    }
  }
}
