{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "foldLineNumber": 3,
      "scope": "formData",
      "controls": [
        {
          "branch_org": {
            "control": "remoteSelect",
            "label": "机构",
            "labelField": "branch_name",
            "valueField": "branch_id",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2173"
              }
            },
            "format": "label"
          },
          "cust_name": {
            "control": "text",
            "label": "客户姓名"
          }
        },
        {
          "cert_no": {
            "control": "text",
            "label": "证件号码"
          },
          "outsourcing_status": {
            "control": "select",
            "label": "委外状态",
            "dict": {
              "dictType": "CE_E_OA_STATUS",
              "dictKey": [
                "CE_E_OA_STATUS"
              ],
              "format": "label"
            }
          }
        },
        {
          "outsourcing_start_date": {
            "control": "dateTimePicker",
            "label": "派案日期",
            "unlinkPanels": false,
            "valueFormat": "yyyyMMdd"
          },
          "outsourcing_end_date": {
            "control": "dateTimePicker",
            "label": "回收日期",
            "unlinkPanels": false,
            "valueFormat": "yyyyMMdd"
          }
        },
        {
          "oa_hands": {
            "control": "inputNumber",
            "label": "手别"
          },
          "overdue_stage_code": {
            "control": "remoteSelect",
            "label": "逾期阶段",
            "labelField": "ovd_period_name",
            "valueField": "overdue_stage_code",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ce0412"
              }
            },
            "format": "label"
          }
        },
        {
          "oa_org_code": {
            "control": "remoteSelect",
            "label": "委外公司",
            "labelField": "coll_org_name",
            "valueField": "coll_org_no",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ce0416",
                "coll_org_type": "OA01",
                "coll_kind":"02",
                "is_valid": "1"
              }
            },
            "format": "label"
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/call/RPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ce4715"
        }
      },
      "thead": [
        {
          "type": "string",
          "align": "center",
          "width": "150",
          "label": "机构",
          "prop": "branch_org",
          "dict": {
            "dictKey": [
              "branch_org"
            ],
            "format": "label",
            "labelField": "branch_name",
            "valueField": "branch_id",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2173"
              }
            }
          }
        },
        {
          "type": "string",
          "align": "center",
          "width": "150",
          "label": "案件编号",
          "prop": "case_no"
        },
        {
          "type": "string",
          "align": "center",
          "width": "150",
          "label": "客户姓名",
          "prop": "cust_name"
        },
        {
          "type": "string",
          "align": "center",
          "width": "150",
          "label": "证件号码",
          "prop": "cert_no"
        },
        {
          "type": "string",
          "align": "center",
          "width": "150",
          "label": "批次",
          "prop": "batch_no"
        },
        {
          "type": "string",
          "align": "center",
          "width": "150",
          "label": "手别",
          "prop": "hands"
        },
        {
          "type": "string",
          "align": "center",
          "width": "150",
          "label": "委外公司",
          "prop": "oa_org_code",
          "dict": {
            "dictKey": [
              "oa_org_code"
            ],
            "format": "label",
            "labelField": "coll_org_name",
            "valueField": "coll_org_no",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ce0416"
              }
            }
          }
        },
        {
          "type": "string",
          "align": "center",
          "width": "150",
          "label": "委外金额",
          "prop": "outsourcing_amt"
        },
        {
          "type": "string",
          "align": "center",
          "width": "150",
          "label": "派案日期",
          "prop": "outsourcing_start_date"
        },
        {
          "type": "string",
          "align": "center",
          "width": "150",
          "label": "回收日期",
          "prop": "outsourcing_end_date"
        },
        {
          "type": "string",
          "align": "center",
          "width": "150",
          "label": "实际结束时间",
          "prop": "actual_end_date"
        },
        {
          "type":"string",
          "label": "委外状态",
          "align":"center",
          "width":"180px",
          "prop": "outsourcing_status",
          "dict": {
            "dictType": "CE_E_OA_STATUS",
            "dictKey": [
              "CE_E_OA_STATUS"
            ],
            "format": "label"
          }
        },
        {
          "type": "string",
          "align": "center",
          "width": "180px",
          "label": "案件状态",
          "prop": "casestate_code",
          "dict": {
            "dictKey": [
              "casestate_code"
            ],
            "format": "label",
            "labelField": "status_name",
            "valueField": "casestate_code",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ce0411"
              }
            }
          }
        },
        {
          "label": "操作",
          "align": "center",
          "fixed": "right",
          "width": "130px",
          "btns": [
            {
              "type": "edit",
              "label": "案件详情",
              "toRequest": {
                "url": "/views/ce/assistManage/caseDetail/caseDetail_2/case_detail_main.json",
                "method": "POST",
                "params": {
                  "case_no": "${{current.case_no}}"
                }
              },
              "props": {
                "width": "80%",
                "title": "案件详情"
              }
            }
          ]
        }
      ]
    }
  }
}
