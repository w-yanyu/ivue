{
    "module": "v_search_btn_datagrid",
    "layout": {
      "form": {
        "foldLineNumber": 3,
        "scope": "formData",
        "controls": [
          {
            "regional_center": {
              "control": "remoteSelect",
              "label": "区域中心",
              "selectFirstValue":true,
              "labelField": "brachName",
              "valueField": "branchNo",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "RLMSPLTS1068",
                  "isCurrent":"1"
                }
              },
              "format": "label"
            },
            "prodcd": {
              "control": "remoteSelect",
              "label": "产品名称",
              "labelField": "prodna",
              "valueField": "crdtcd",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "AS_ICMS_PD_QRPDLS",
                  "crdtcd": "",
                  "length": 100
                }
              },
              "format": "label"
            }
          },
          {
            "function_cd": {
              "control": "select",
              "multiple":true,
              "label": "账龄",
              "dict": {
                "dictType": "E_FUNCTION",
                "dictKey": [
                  "E_FUNCTION"
                ],
                "format": "label",
                "labelField": "dictName",
                "valueField": "dictId"
              }
            },
            "lncf_no": {
              "control": "text",
              "label": "借据号"
            }
          },
          {
            "cust_nm": {
              "control": "text",
              "label": "客户姓名"
            },
            "cert_id": {
              "control": "text",
              "label": "证件号码"
            }
          },
          {
            "oa_org_name": {
              "control": "remoteSelect",
              "label": "委案公司",
              "labelField": "oa_org_name",
              "valueField": "oa_org_id",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "RLMSPLTS1073"
                }
              },
              "format": "label"
            }
          
          }
          
        ]
      },
      "toolbar": {
        "btns": [
          {
            "type": "export",
            "label": "导出",
            "target": "dialog",
            "icon": "iconfont icon-daochu",
            "toRequest": {
              "method": "post",
              "url": "@exportOaFile",
              "params": {
                "servicecode": "RLMSPLTS1071",
                "regional_center":"${{formData.regional_center}}",
                "prodcd":"${{formData.prodcd}}",
                "function_cd":"${{formData.function_cd}}",
                "lncf_no":"${{formData.lncf_no}}",
                "cust_nm":"${{formData.cust_nm}}",
                "cert_id":"${{formData.cert_id}}",
                "oa_org_id":"${{formData.oa_org_name}}"
              }
            },
            "props": {
              "width": "30%",
              "title": "导出"
            }
        },
          {
            "type": "custom",
            "label": "取消分案导入",
            "toRequest": {
              "method": "GET",
              "url": "/views/pl/collManage/oaManage/pendDivision/quxfadru.json"
            },
            "props": {
              "title": "取消分案导入"
            }
          },
          {
            "type": "export",
            "label": "取消下载分案模板",
            "doRequest": {
              "method": "post",
              "url": "/SUMP/icmcall/plms/exportModelDaiweiai"
            }
          },
          {
            "type": "custom",
            "target": "dialog",
            "icon": "el-icon-upload",
            "label": "确认委案",
            "toRequest": {
              "method": "GET",
              "url": "@trustCaseConfirm"
            },
            "props": {
              "width": "75%",
              "title": "委案比例确认"
            }
          }
        ]
      },
      "datagrid": {
        "isInitSearch": false,
        "doRequest": {
          "url": "/SUMP/icmcall/icmRPCCall",
          "method": "POST",
          "params": {
            "servicecode": "RLMSPLTS1052"
          }
        },
        "thead": [
          {
            "type": "string",
            "align": "center",
            "label": "区域中心",
            "prop": "region_name"
          },
          {
            "type": "string",
            "align": "center",
            "label": "借据号",
            "prop": "lncf_no"
          },
          {
            "type": "string",
            "align": "center",
            "label": "客户姓名",
            "prop": "cust_nm"
          },
          {
            "type": "string",
            "align": "center",
            "label": "产品名称",
            "prop": "prod_name"
          },
          {
            "type": "string",
            "align": "center",
            "label": "逾期天数",
            "prop": "over_due_day"
          },
          {
            "type": "string",
            "align": "center",
            "label": "解除逾期金额",
            "prop": "overdueTotalAmt"
          },
          {
            "type": "string",
            "align": "center",
            "label": "账龄",
            "prop": "function_cd"
          },
          {
            "type": "string",
            "align": "center",
            "label": "现贷余额",
            "prop": "now_credit_balance"
          },
          {
            "type": "string",
            "align": "center",
            "label": "委案公司",
            "prop": "oa_org_id"
          },
          {
            "type": "string",
            "align": "center",
            "label": "上一月委案公司",
            "prop": "pro_oa_id"
          },
          {
            "type": "string",
            "align": "center",
            "label": "证件号码",
            "prop": "cert_id"
          },
          {
            "type": "string",
            "align": "center",
            "label": "是否高危",
            "prop": "is_high_risk"
          },
          {
            "type": "string",
            "align": "center",
            "label": "是否法诉",
            "prop": "whether_the_litigation"
          }
        ]
      }
    }
  }

