{
    "module": "v_search_btn_datagrid",
    "layout": {
      "form": {
        "foldLineNumber": 3,
        "scope": "formData",
        "controls": [
          {
            "regional_center": {
              "control": "remoteSelect",
              "label": "区域中心",
              "labelField": "brachName",
              "valueField": "branchNo",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "RLMSPLTS1068"
                }
              },
              "format": "label"
            },
            "prod_name": {
              "control": "remoteSelect",
              "label": "产品名称",
              "labelField": "prodna",
              "valueField": "crdtcd",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "AS_ICMS_PD_QRPDLS",
                  "crdtcd": "",
                  "length": 100
                }
              },
              "format": "label"
            }
          },
          {
            "lncfno": {
              "control": "text",
              "label": "借据号"
            },
            
              "oa_org_name": {
                "control": "remoteSelect",
                "label": "委案公司",
                "labelField": "oa_org_name",
                "valueField": "oa_org_id",
                "doRequest": {
                  "url": "/SUMP/icmcall/icmRPCCall",
                  "method": "POST",
                  "params": {
                    "servicecode": "RLMSPLTS1073"
                  }
                },
                "format": "label"
              }
            
          },
          {
            "cust_nm": {
              "control": "text",
              "label": "客户姓名"
            },
            "cert_id": {
              "control": "text",
              "label": "证件号码"
            }
          }
        ]
      },
      "datagrid": {
        "isInitSearch": false,
        "doRequest": {
          "url": "/SUMP/icmcall/icmRPCCall",
          "method": "POST",
          "params": {
            "servicecode": "RLMSPLTS1067"
          }
        },
        "thead": [
          {
            "type": "string",
            "align": "center",
            "label": "区域中心",
            "prop": "region_name"
          },
          {
            "type": "string",
            "align": "center",
            "label": "借据号",
            "prop": "lncfno"
          },
          {
            "type": "string",
            "align": "center",
            "label": "客户姓名",
            "prop": "cust_nm"
          },
          {
            "type": "string",
            "align": "center",
            "label": "产品名称",
            "prop": "prod_name"
          },
          {
            "type": "string",
            "align": "center",
            "label": "现贷余额",
            "prop": "now_credit_balance"
          },

          {
            "type": "string",
            "align": "center",
            "label": "账龄",
            "prop": "account_age"
          },
          {
            "type": "string",
            "align": "center",
            "label": "委案公司",
            "prop": "oa_org_id"
          },
          {
            "type": "string",
            "align": "center",
            "label": "留案原因",
            "prop": "lc_reson"
          },
          {
            "type": "string",
            "align": "center",
            "label": "是否高危",
            "prop": "is_high_risk"
          },
          {
            "type": "string",
            "align": "center",
            "label": "是否法诉",
            "prop": "whether_the_litigation"
          },
          {
            "type": "string",
            "align": "center",
            "label": "证件号码",
            "prop": "cert_id"
          }
        ]
      }
    }
  }

