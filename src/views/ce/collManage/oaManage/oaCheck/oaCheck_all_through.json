{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "scope":"formData",
            "initRequest": "${{params}}",
            "btns": [
                {
                    "type": "submit",
                    "label": "确认",
                    "doRequest": {
                        "method": "POST",
                        "url": "/SUMP/call/RPCCall",
                        "params": {
                            "servicecode":"ce4713",
                            "trxn_branch":"${{params.trxn_branch}}",
                            "cust_name":"${{params.cust_name}}",
                            "cert_no":"${{cert_no}}",
                            "cust_no":"${{params.cust_no}}",
                            "loan_no":"${{params.loan_no}}",
                            "appr_state":"${{params.appr_state}}",
                            "daterange":"${{params.daterange}}",
                            "outsourcing_status":"${{params.outsourcing_status}}",
                            "queue_code":"${{params.queue_code}}",
                            "case_cls_code":"${{params.case_cls_code}}",
                            "casestate_code":"${{params.casestate_code}}",
                            "overdue_stage_code":"${{params.overdue_stage_code}}",
                            "ovd_amt_total_1":"${{params.ovd_amt_total_1}}",
                            "ovd_amt_total_2":"${{params.ovd_amt_total_2}}",
                            "collect_date_1":"${{params.collect_date_1}}",
                            "collect_date_2":"${{params.collect_date_2}}"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "appr_comment": {
                        "control": "textarea",
                        "label": "审核说明",
                        "width": "180px",
                        "align": "center",
                        "span": "20",
                        "rows": 6,
                        "placeholder":"请输入",
                        "rules": [
                            {
                                "required": true,
                                "message":"请输入"
                            }
                        ]
                    }
                }
            ]
        }
    }
}
