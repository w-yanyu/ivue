{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "foldLineNumber": 3,
            "controls": [
                {
                    "branch_org": {
                      "control": "remoteSelect",
                      "label": "机构",
                      "labelField": "branch_name",
                      "valueField": "branch_id",
                      "doRequest": {
                        "url": "/SUMP/call/RPCCall",
                        "method": "POST",
                        "params": {
                          "servicecode": "ct2173"
                        }
                      },
                      "format": "label"
                    },
                    "cust_name": {
                        "control": "text",
                        "label": "客户姓名"
                    }
                },
                {
                    "cert_no": {
                        "control": "text",
                        "label": "证件号码"
                    },
                    "cust_no": {
                        "control": "text",
                        "label": "客户编号"
                    }
                },
                {
                    "loan_no": {
                        "control": "text",
                        "label": "借据号"
                    },
                    "appr_state": {
                        "control": "select",
                        "label": "审核状态",
                        "default": "NC",
                        "dict": {
                          "dictType": "CE_APPR_STATE",
                          "dictKey": [
                            "CE_APPR_STATE"
                          ],
                          "format": "value-label"
                        }
                    }
                },
                {
                    "daterange": {
                        "control": "dateTimePicker",
                        "label": "申请时间",
                        "type": "daterange",
                        "unlinkPanels": true,
                        "valueFormat": "yyyyMMdd",
                        "startPlaceholder": "选择开始时间",
                        "endPlaceholder": "选择结束时间",
                        "startField": "begin_date",
                        "endField": "end_date"
                    },
                    "outsourcing_status": {
                        "control": "select",
                        "label": "委外状态",
                        "dict": {
                          "dictType": "CE_E_OA_STATUS",
                          "dictKey": [
                            "CE_E_OA_STATUS"
                          ],
                          "format": "value-label"
                        }
                    }
                },
                {
                    "queue_code": {
                      "control": "remoteSelect",
                      "label": "队列名称",
                      "labelField": "queue_name",
                      "valueField": "queue_code",
                      "doRequest": {
                        "url": "/SUMP/call/RPCCall",
                        "method": "POST",
                        "params": {
                          "servicecode": "ce0402"
                        }
                      },
                      "format": "value-label"
                    },
                    "case_cls_code": {
                      "control": "remoteSelect",
                      "label": "案件类型",
                      "labelField": "location_name",
                      "valueField": "case_cls_code",
                      "doRequest": {
                        "url": "/SUMP/call/RPCCall",
                        "method": "POST",
                        "params": {
                          "servicecode": "ce0410"
                        }
                      },
                      "format": "value-label"
                    }
                },
                {
                    "casestate_code": {
                      "control": "remoteSelect",
                      "label": "案件状态",
                      "labelField": "status_name",
                      "valueField": "casestate_code",
                      "doRequest": {
                        "url": "/SUMP/call/RPCCall",
                        "method": "POST",
                        "params": {
                          "servicecode": "ce0411"
                        }
                      },
                      "format": "value-label"
                    },
                    "overdue_stage_code": {
                      "control": "remoteSelect",
                      "label": "逾期阶段",
                      "labelField": "ovd_period_name",
                      "valueField": "overdue_stage_code",
                      "doRequest": {
                        "url": "/SUMP/call/RPCCall",
                        "method": "POST",
                        "params": {
                          "servicecode": "ce0412"
                        }
                      },
                      "format": "value-label"
                    }
                },
                {
                    "ovd_amt_total_1": {
                        "control": "currency",
                        "label": "欠款金额区间开始值"
                    },
                    "ovd_amt_total_2": {
                        "control": "currency",
                        "label": "欠款金额区间结束值"
                    }
                },
                {
                    "collect_date_1": {
                        "control": "inputNumber",
                        "label": "入催天数区间开始值"
                    },
                    "collect_date_2": {
                        "control": "inputNumber",
                        "label": "入催天数区间结束值"
                    }
                }
            ]
        },
        "toolbar": {
            "btns": [
                {
                    "type": "custom",
                    "icon": "el-icon-edit",
                    "target": "dialog",
                    "checkCountMin": 1,
                    "label": "批量审核",
                    "toRequest": {
                        "method": "GET",
                        "url": "/views/ce/collManage/oaManage/oaCheck/oaCheck_auditing.json"
                    }
                },
                {
                    "type": "custom",
                    "icon": "el-icon-circle-check",
                    "target": "dialog",
                    "label": "全部核准",
                    "disabled":"'${{form.appr_state}}' === 'P' || '${{form.appr_state}}' === 'R' || '${{form.appr_state}}' === ''",
                    "toRequest": {
                        "method": "GET",
                        "url": "/views/ce/collManage/oaManage/oaCheck/oaCheck_all_through.json",
                        "params": {
                          "trxn_branch":"${{form.trxn_branch}}",
                          "cust_name":"${{form.cust_name}}",
                          "cert_no":"${{form.cert_no}}",
                          "cust_no":"${{form.cust_no}}",
                          "loan_no":"${{form.loan_no}}",
                          "appr_state":"NC",
                          "daterange":"${{form.daterange}}",
                          "outsourcing_status":"${{form.outsourcing_status}}",
                          "queue_code":"${{form.queue_code}}",
                          "case_cls_code":"${{form.case_cls_code}}",
                          "casestate_code":"${{form.casestate_code}}",
                          "overdue_stage_code":"${{form.overdue_stage_code}}",
                          "ovd_amt_total_1":"${{form.ovd_amt_total_1}}",
                          "ovd_amt_total_2":"${{form.ovd_amt_total_2}}",
                          "collect_date_1":"${{form.collect_date_1}}",
                          "collect_date_2":"${{form.collect_date_2}}"
                        }
                    }
                },
                {
                    "type": "custom",
                    "icon": "el-icon-circle-close",
                    "target": "dialog",
                    "label": "全部拒绝",
                    "disabled":"'${{form.appr_state}}' === 'P' || '${{form.appr_state}}' === 'R' || '${{form.appr_state}}' === ''",
                    "toRequest": {
                        "method": "GET",
                        "url": "/views/ce/collManage/oaManage/oaCheck/oaCheck_all_rejected.json",
                        "params": {
                          "trxn_branch":"${{form.trxn_branch}}",
                          "cust_name":"${{form.cust_name}}",
                          "cert_no":"${{form.cert_no}}",
                          "cust_no":"${{form.cust_no}}",
                          "loan_no":"${{form.loan_no}}",
                          "appr_state":"NC",
                          "daterange":"${{form.daterange}}",
                          "outsourcing_status":"${{form.outsourcing_status}}",
                          "queue_code":"${{form.queue_code}}",
                          "case_cls_code":"${{form.case_cls_code}}",
                          "casestate_code":"${{form.casestate_code}}",
                          "overdue_stage_code":"${{form.overdue_stage_code}}",
                          "ovd_amt_total_1":"${{form.ovd_amt_total_1}}",
                          "ovd_amt_total_2":"${{form.ovd_amt_total_2}}",
                          "collect_date_1":"${{form.collect_date_1}}",
                          "collect_date_2":"${{form.collect_date_2}}"
                        }
                    }
                }
            ]
        },
        "datagrid": {
            "isInitSearch": false,
            "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "ce4555"
                }
            },
            "thead": [
                {
                    "type": "selection",
                    "align": "center",
                    "width": "55",
                    "fixed": "left",
                    "show":{
                        "expression":"'${{appr_state}}' === 'NC'"
                    }
                },
                {
                  "type": "string",
                  "align": "center",
                  "width": "100",
                  "label": "机构",
                  "prop": "branch_org",
                  "dict": {
                    "dictKey": [
                      "branch_org"
                    ],
                    "format": "value-label",
                    "labelField": "branch_name",
                    "valueField": "branch_id",
                    "doRequest": {
                      "url": "/SUMP/call/RPCCall",
                      "method": "POST",
                      "params": {
                        "servicecode": "ct2173"
                      }
                    }
                  }
                },
                {
                    "type": "string",
                    "align": "center",
                    "width":"180px",
                    "label": "案件编号",
                    "prop": "case_no"
                },
                {
                    "type": "string",
                    "align": "center",
                    "width":"180px",
                    "label": "客户编号",
                    "prop": "cust_no"
                },
                {
                    "type": "string",
                    "align": "center",
                    "width":"100px",
                    "label": "客户姓名",
                    "prop": "cust_name"
                },
                {
                  "type": "string",
                  "align": "center",
                  "label": "证件类型",
                  "width": "150px",
                  "prop": "cert_type",
                  "dict": {
                    "dictType": "MS_E_CRET_TYPE",
                    "dictKey": [
                      "MS_E_CRET_TYPE"
                    ],
                    "format": "value-label"
                  }
                },
                {
                    "type": "string",
                    "align": "center",
                    "width":"200px",
                    "label": "证件号码",
                    "prop": "cert_no"
                },
                {
                  "type": "string",
                  "align": "center",
                  "label": "委外状态",
                  "width": "100px",
                  "prop": "outsourcing_status",
                  "dict": {
                    "dictType": "CE_E_OA_STATUS",
                    "dictKey": [
                      "CE_E_OA_STATUS"
                    ],
                    "format": "value-label"
                  }
                },
                {
                    "type": "string",
                    "align": "center",
                    "width":"100px",
                    "label": "手别",
                    "prop": "oa_hands"
                },
                {
                    "type": "string",
                    "align": "center",
                    "width":"100px",
                    "label": "展期次数",
                    "prop": "allow_extend_count"
                },
                {
                    "type": "string",
                    "align": "center",
                    "width": "180px",
                    "label": "申请人",
                    "prop": "appt",
                    "dict": {
                        "dictKey": [
                            "appt"
                        ],
                        "format": "label",
                        "labelField": "user_name",
                        "valueField": "user_id",
                        "doRequest": {
                            "url": "/SUMP/call/RPCCall",
                            "method": "POST",
                            "params": {
                                "servicecode": "ce0403"
                            }
                        }
                    }
                },
                {
                    "type": "string",
                    "align": "center",
                    "width":"180px",
                    "label": "申请日期",
                    "prop": "apply_date"
                },
                {
                    "type": "string",
                    "align": "center",
                    "width":"100px",
                    "label": "申请说明",
                    "prop": "apply_reason"
                },
                {
                    "type": "string",
                    "align": "center",
                    "width": "180px",
                    "label": "审核状态",
                    "prop": "appr_state",
                    "dict": {
                      "dictType": "CE_APPR_STATE",
                      "dictKey": [
                        "CE_APPR_STATE"
                      ],
                      "format": "value-label"
                    }
                },
                {
                    "type":"string",
                    "align":"center",
                    "width":"180px",
                    "label": "审核人",
                    "prop": "approver",
                    "dict": {
                        "dictKey": [
                          "approver"
                        ],
                        "format": "label",
                        "labelField": "user_name",
                        "valueField": "user_id",
                        "doRequest": {
                          "url": "/SUMP/call/RPCCall",
                          "method": "POST",
                          "params": {
                            "servicecode": "ce0403"
                          }
                        }
                      }
                },
                {
                    "type": "string",
                    "align": "center",
                    "width":"180px",
                    "label": "审核日期",
                    "prop": "appr_date"
                },
                {
                    "type": "string",
                    "align": "center",
                    "width":"100px",
                    "label": "审核说明",
                    "prop": "appr_comment"
                },
                {
                    "label": "操作",
                    "align": "center",
                    "fixed": "right",
                    "width": "130px",
                    "btns": [
                        {
                            "type": "custom",
                            "label": "案件详情",
                            "toRequest": {
                              "url": "/views/ce/assistManage/caseDetail/caseDetail_1/case_detail_main.json",
                              "method": "POST",
                                "params": {
                                    "case_no": "${{current.case_no}}"
                                }
                            },
                            "props": {
                                "width": "80%",
                                "title": "案件详情"
                            }
                        }
                    ]
                }
            ]
        }
    }
}
