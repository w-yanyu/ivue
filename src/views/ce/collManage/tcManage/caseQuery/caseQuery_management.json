{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "foldLineNumber": 3,
      "scope": "formData",
      "controls": [
        {
          "branch_org": {
            "control": "remoteSelect",
            "label": "机构",
            "labelField": "branch_name",
            "valueField": "branch_id",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2173"
              }
            },
            "format": "label"
          },
          "cust_name": {
            "control": "text",
            "label": "客户姓名"
          }
        },
        {
          "coll_group": {
            "control": "inputTree",
            "label": "催收组",
            "idField": "coll_group",
            "nodeTypeField": "type",
            "pIdField": "parent_coll_team",
            "labelField": "coll_team_name",
            "events": {
              "emit": [
                "collector"
              ]
            },
            "root": {
              "rootNode": {
                "name": "BYB-BRANCH",
                "value": "9999",
                "disabled": true
              }
            },
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ce0180"
              }
            },
            "format": "label"
          },
          "collector": {
            "control": "remoteSelect",
            "label": "催收员",
            "labelField": "collector_name",
            "valueField": "collector",
            "lazy": true,
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ce0184",
                "coll_team": "${{form.coll_group}}"
              }
            },
            "format": "label"
          }
        },
        {
          "cert_no": {
            "control": "text",
            "label": "证件号码"
          },
          "contact_info": {
            "control": "text",
            "label": "手机号码"
          }
        },
        {
          "cust_no": {
            "control": "text",
            "label": "客户号"
          },
          "loan_no": {
            "control": "text",
            "label": "借据号"
          }
        },
        {
          "deal_state": {
            "control": "select",
            "label": "处理状态",
            "dict": {
              "dictType": "CE_E_DEALSTATUS",
              "dictKey": [
                "CE_E_DEALSTATUS"
              ],
              "format": "label"
            }
          },
          "case_cls_code": {
            "control": "remoteSelect",
            "label": "案件类型",
            "labelField": "location_name",
            "valueField": "case_cls_code",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ce0410"
              }
            },
            "format": "label"
          }
        },
        {
          "casestate_code": {
            "control": "remoteSelect",
            "label": "案件状态",
            "labelField": "status_name",
            "valueField": "casestate_code",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ce0411"
              }
            },
            "format": "label"
          },
          "last_coll_result": {
            "control": "remoteSelect",
            "label": "最近催收结果",
            "labelField": "cnct_rslt_name",
            "valueField": "cnct_rslt_code",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ce0420"
              }
            },
            "format": "label"
          }
        },
        {
          "max_over_due_day": {
            "control": "inputNumber",
            "label": "最大逾期天数"
          },
          "executiondue_date": {
            "control": "dateTimePicker",
            "label": "到期日"
          }
        },
        {
          "queue_code": {
            "control": "remoteSelect",
            "label": "队列名称",
            "labelField": "queue_name",
            "valueField": "queue_code",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ce0402"
              }
            },
            "format": "label"
          },
          "overdue_stage_code": {
            "control": "remoteSelect",
            "label": "逾期阶段",
            "labelField": "ovd_period_name",
            "valueField": "overdue_stage_code",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ce0412"
              }
            },
            "format": "label"
          }
        },
        {
          "collect_date_1": {
            "control": "inputNumber",
            "label": "入催天数区间开始值"
          },
          "collect_date_2": {
            "control": "inputNumber",
            "label": "入催天数区间结束值"
          }
        },
        {
          "start_amt": {
            "control": "currency",
            "label": "欠款金额区间开始值"
          },
          "end_amt": {
            "control": "currency",
            "label": "欠款金额区间结束值"
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "custom",
          "icon": "el-icon-download",
          "target": "dialog",
          "label": "手动入催",
          "toRequest": {
            "method": "GET",
            "url": "/views/ce/collManage/tcManage/caseQuery/caseQuery_Import.json"
          }
        },
        {
          "type": "custom",
          "icon": "el-icon-check",
          "target": "dialog",
          "checkCountMin": 1,
          "label": "打标签",
          "toRequest": {
            "method": "GET",
            "url": "/views/ce/collManage/tcManage/caseQuery/mark_cd.json",
            "params": {
              "case_no": "${{current.case_no}}"
            }
          }
        },
        {
          "type": "export",
          "icon": "iconfont icon-daochu",
          "label": "导出",
          "doRequest": {
            "method": "POST",
            "url": "/SUMP/rest/file/export",
            "params": {
              "servicecode": "pl3507"
            }
          },
          "confirm":{
            "title":"提示信息",
            "msg":"确认导出吗？",
            "confirmButtonText":"确定",
            "cancelButtonText":"取消"
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/call/RPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ce3502"
        }
      },
      "thead": [
        {
          "type": "selection",
          "align": "center",
          "width": "55",
          "fixed": "left"
        },
        {
          "label": "操作",
          "align": "center",
          "width": "180px",
          "fixed": "right",
          "btns": [
            {
              "type": "custom",
              "label": "案件详情",
              "toRequest": {
                "url": "/views/ce/assistManage/caseDetail/caseDetail_2/case_detail_main.json",
                "method": "POST",
                "params": {
                  "case_no": "${{current.case_no}}"
                }
              },
              "props": {
                "width": "80%",
                "title": "案件详情"
              }
            },
            {
              "type": "custom",
              "label": "承诺信息",
              "toRequest": {
                "url": "/views/ce/collManage/tcManage/caseQuery/queryPromise.json",
                "method": "POST",
                "params": {
                  "case_id": "${{current.case_no}}"
                }
              },
              "props": {
                "width": "80%",
                "title": "承诺信息"
              }
            }
          ]
        },
        {
          "type": "string",
          "align": "center",
          "width": "180px",
          "label": "机构",
          "prop": "branch_org",
          "dict": {
            "dictKey": [
              "branch_org"
            ],
            "format": "label",
            "labelField": "branch_name",
            "valueField": "branch_id",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2173"
              }
            }
          }
        },
        {
          "type": "string",
          "align": "center",
          "width": "180px",
          "label": "客户号",
          "prop": "cust_no"
        },
        {
          "type": "string",
          "align": "center",
          "width": "180px",
          "label": "客户姓名",
          "prop": "cust_name"
        },
        {
          "type":"string",
          "label": "证件类型",
          "align":"center",
          "width":"180px",
          "prop": "cert_type",
          "dict": {
            "dictType": "MS_E_CRET_TYPE",
            "dictKey": [
              "MS_E_CRET_TYPE"
            ],
            "format": "label"
          }
        },
        {
          "type": "string",
          "align": "center",
          "width": "180px",
          "label": "证件号码",
          "prop": "cert_no"
        },
        {
          "type": "string",
          "align": "center",
          "width": "180px",
          "label": "手机号码",
          "prop": "contact_info"
        },
        {
          "type": "string",
          "align": "center",
          "width": "180px",
          "label": "借据号",
          "prop": "loan_no"
        },
        {
          "type":"string",
          "label": "处理状态",
          "align":"center",
          "width":"180px",
          "prop": "deal_state",
          "dict": {
            "dictType": "CE_E_DEALSTATUS",
            "dictKey": [
              "CE_E_DEALSTATUS"
            ],
            "format": "label"
          }
        },
        {
          "type": "string",
          "align": "center",
          "width":"180px",
          "label": "案件类型",
          "prop": "case_cls_code",
          "dict": {
            "dictKey": [
              "case_cls_code"
            ],
            "format": "label",
            "labelField": "location_name",
            "valueField": "case_cls_code",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ce0410"
              }
            }
          }
        },
        {
          "type": "string",
          "align": "center",
          "width": "180px",
          "label": "案件状态",
          "prop": "casestate_code",
          "dict": {
            "dictKey": [
              "casestate_code"
            ],
            "format": "label",
            "labelField": "status_name",
            "valueField": "casestate_code",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ce0411"
              }
            }
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "催收组",
          "width": "180px",
          "prop": "coll_group",
          "dict": {
            "dictKey": [
              "coll_group"
            ],
            "format": "label",
            "labelField": "coll_team_name",
            "valueField": "coll_group",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ce0180"
              }
            }
          }
        },
        {
          "type": "string",
          "align": "center",
          "width":"180px",
          "label": "催收员",
          "prop": "collector",
          "dict": {
            "dictKey": [
              "collector"
            ],
            "format": "label",
            "labelField": "user_name",
            "valueField": "user_id",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ce0403"
              }
            }
          }
        },
        {
          "type": "currency",
          "align": "center",
          "width": "180px",
          "label": "当前余额",
          "prop": "current_bal",
          "format": {
            "decimal": "2"
          }
        },
        {
          "type": "currency",
          "align": "center",
          "width": "180px",
          "label": "逾期金额",
          "prop": "overdue_amt",
          "format": {
            "decimal": "2"
          }
        },
        {
          "type": "string",
          "align": "center",
          "width": "180px",
          "label": "逾期天数",
          "prop": "overdue_days"
        },
        {
          "type": "string",
          "align": "center",
          "width": "180px",
          "label": "到期日",
          "prop": "executiondue_date"
        },
        {
          "type": "string",
          "align": "center",
          "width": "180px",
          "label": "跟催日期",
          "prop": "hold_date"
        },
        {
          "type": "string",
          "align": "center",
          "width": "180px",
          "label": "入催日期",
          "prop": "reminder_date"
        },
        {
          "type":"string",
          "label": "本期五级分类",
          "align":"center",
          "width":"180px",
          "prop": "classify_result",
          "dict": {
            "dictType": "MS_E_LEVEL5_TYPE",
            "dictKey": [
              "MS_E_LEVEL5_TYPE"
            ],
            "format": "label"
          }
        },
        {
          "type": "string",
          "align": "center",
          "width":"180px",
          "label": "最近催收结果",
          "prop": "last_coll_result",
          "dict": {
            "dictKey": [
              "last_coll_result"
            ],
            "format": "label",
            "labelField": "cnct_rslt_name",
            "valueField": "cnct_rslt_code",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ce0420"
              }
            }
          }
        },
        {
          "type":"string",
          "label": "案件标签",
          "align":"center",
          "width":"180px",
          "prop": "case_label",
          "dict": {
            "dictType": "CE_E_CASE_LABEL",
            "dictKey": [
              "CE_E_CASE_LABEL"
            ],
            "format": "label"
          }
        }
      ]
    }
  }
}
