{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "foldLineNumber": 3,
      "scope": "formData",
      "controls": [
        {
          "branch_org": {
            "control": "remoteSelect",
            "label": "机构",
            "labelField": "branch_name",
            "valueField": "branch_id",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2173"
              }
            },
            "format": "label"
          },
          "cust_name": {
            "control": "text",
            "label": "客户姓名"
          }
        },
        {
          "cert_no": {
            "control": "text",
            "label": "证件号码"
          },
          "cust_no": {
            "control": "text",
            "label": "客户号"
          }
        },
        {
          "loan_no": {
            "control": "text",
            "label": "借据号"
          },
          "case_cls_code": {
            "control": "remoteSelect",
            "label": "案件类型",
            "labelField": "location_name",
            "valueField": "case_cls_code",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ce0410"
              }
            },
            "format": "label"
          }
        },
        {
          "casestate_code": {
            "control": "remoteSelect",
            "label": "案件状态",
            "labelField": "status_name",
            "valueField": "casestate_code",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ce0411"
              }
            },
            "format": "label"
          },
          "max_over_due_day": {
            "control": "text",
            "label": "最大逾期天数"
          }
        },
        {
          "busi_type_name": {
            "control": "text",
            "label": "业务品种"
          },
          "classify_result": {
            "control": "select",
            "label": "本期五级分类",
            "dict": {
              "dictType": "MS_E_LEVEL5_TYPE",
              "dictKey": [
                "MS_E_LEVEL5_TYPE"
              ],
              "format": "label"
            }
          }
        },
        {
          "queue_code": {
            "control": "remoteSelect",
            "label": "队列名称",
            "labelField": "queue_name",
            "valueField": "queue_code",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ce0402"
              }
            },
            "format": "label"
          },
          "overdue_stage_code": {
            "control": "remoteSelect",
            "label": "逾期阶段",
            "labelField": "ovd_period_name",
            "valueField": "overdue_stage_code",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ce0412"
              }
            },
            "format": "label"
          }
        },
        {
          "collect_date_1": {
            "control": "inputNumber",
            "label": "入催天数区间开始值"
          },
          "collect_date_2": {
            "control": "inputNumber",
            "label": "入催天数区间结束值"
          }
        },
        {
          "arreag_start_amt": {
            "control": "currency",
            "label": "欠款金额区间开始值"
          },
          "arreag_end_amt": {
            "control": "currency",
            "label": "欠款金额区间结束值"
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "custom",
          "icon": "el-icon-edit",
          "target": "dialog",
          "checkCountMin": 1,
          "label": "代理申请",
          "toRequest": {
            "method": "GET",
            "url": "/views/ce/collManage/tcManage/proxyApply/proxyApply_oper.json"
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/call/RPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ce2534"
        }
      },
      "thead": [
        {
          "type": "selection",
          "align": "center",
          "width": "55",
          "fixed": "left"
        },
        {
          "type": "string",
          "align": "center",
          "width": "180px",
          "label": "机构",
          "prop": "branch_org",
          "dict": {
            "dictKey": [
              "branch_org"
            ],
            "format": "label",
            "labelField": "branch_name",
            "valueField": "branch_id",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2173"
              }
            }
          }
        },
        {
          "type": "string",
          "align": "center",
          "width": "180px",
          "label": "案件编号",
          "prop": "case_no"
        },
        {
          "type": "string",
          "align": "center",
          "width": "180px",
          "label": "客户号",
          "prop": "cust_no"
        },
        {
          "type": "string",
          "align": "center",
          "width": "180px",
          "label": "客户姓名",
          "prop": "cust_name"
        },
        {
          "type":"string",
          "label": "证件类型",
          "align":"center",
          "width":"180px",
          "prop": "cert_type",
          "dict": {
            "dictType": "MS_E_CRET_TYPE",
            "dictKey": [
              "MS_E_CRET_TYPE"
            ],
            "format": "label"
          }
        },
        {
          "type": "string",
          "align": "center",
          "width": "180px",
          "label": "证件号码",
          "prop": "cert_no"
        },
        {
          "type": "string",
          "align": "center",
          "label": "催收组",
          "width": "180px",
          "prop": "coll_group",
          "dict": {
            "dictKey": [
              "coll_group"
            ],
            "format": "label",
            "labelField": "coll_team_name",
            "valueField": "coll_group",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ce0180"
              }
            }
          }
        },
        {
          "type": "string",
          "align": "center",
          "width":"180px",
          "label": "催收员",
          "prop": "collector",
          "dict": {
            "dictKey": [
              "collector"
            ],
            "format": "label",
            "labelField": "user_name",
            "valueField": "user_id",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ce0403"
              }
            }
          }
        },
        {
          "type": "string",
          "align": "center",
          "width":"180px",
          "label": "案件类型",
          "prop": "case_cls_code",
          "dict": {
            "dictKey": [
              "case_cls_code"
            ],
            "format": "label",
            "labelField": "location_name",
            "valueField": "case_cls_code",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ce0410"
              }
            }
          }
        },
        {
          "type": "string",
          "align": "center",
          "width": "180px",
          "label": "案件状态",
          "prop": "casestate_code",
          "dict": {
            "dictKey": [
              "casestate_code"
            ],
            "format": "label",
            "labelField": "status_name",
            "valueField": "casestate_code",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ce0411"
              }
            }
          }
        },
        {
          "type": "string",
          "align": "center",
          "width": "180px",
          "label": "逾期天数",
          "prop": "over_due_day"
        },
        {
          "type": "currency",
          "align": "center",
          "width": "180px",
          "label": "逾期金额",
          "prop": "overdue_amt",
          "format": {
            "decimal": "2"
          }
        },
        {
          "type": "string",
          "align": "center",
          "width": "180px",
          "label": "跟催日期",
          "prop": "hold_date"
        },
        {
          "type": "string",
          "align": "center",
          "width": "180px",
          "label": "到期日",
          "prop": "executiondue_date"
        },
        {
          "type": "string",
          "align": "center",
          "width": "180px",
          "label": "业务品种",
          "prop": "busi_type_name"
        },
        {
          "type":"string",
          "label": "本期五级分类",
          "align":"center",
          "width":"180px",
          "prop": "classify_result",
          "dict": {
            "dictType": "MS_E_LEVEL5_TYPE",
            "dictKey": [
              "MS_E_LEVEL5_TYPE"
            ],
            "format": "label"
          }
        },
        {
          "type": "string",
          "align": "center",
          "width": "180px",
          "label": "借据号",
          "prop": "loan_no"
        },
        {
          "type": "currency",
          "align": "center",
          "width": "180px",
          "label": "贷款余额",
          "prop": "current_bal",
          "format": {
            "decimal": "2"
          }
        },
        {
          "label": "操作",
          "align": "center",
          "fixed": "right",
          "width": "130px",
          "btns": [
            {
              "type": "custom",
              "label": "代理信息",
              "toRequest": {
                "url": "/views/ce/collManage/tcManage/proxyApply/proxyApply_info.json",
                "method": "POST",
                "params": {
                  "case_id": "${{current.case_no}}"
                }
              },
              "props": {
                "width": "80%",
                "title": "案件代理信息"
              }
            }
          ]
        }
      ]
    }
  }
}
