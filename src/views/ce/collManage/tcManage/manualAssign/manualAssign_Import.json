{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "scope": "formData",
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pl2544"
        }
      },
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "el-icon-tickets"
        }
      ],
      "controls": [
        {
          "file_name": {
            "control": "dynamic",
            "isShowChildForm": true,
            "span": 12,
            "toRequest": {
              "url": "@SingleUpload",
              "method": "get",
              "params": {
                "path": "collRec",
                "uploadType": "sftp",
                "formKey": "file_name",
                "formLabel": "分配文件",
                "accept": ".xlsx,.xls",
                "required": "true",
                "tipText": "只能上传xls/xlsx，且不超过10MB"
              }
            }
          },
          "xxx": {
            "control": "button",
            "text": "没有模板 ? 点击下载",
            "type": "custom",
            "toRequest": {
              "url": "/views/pl/collManage/oaManage/oawork/oawork_downloads.json",
              "method": "get"
            },
            "props": {
              "width": "80%",
              "title": "模板列表"
            }
          }
        }
      ]
    }
  }
}
