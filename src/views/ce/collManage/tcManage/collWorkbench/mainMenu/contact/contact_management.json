{
    "module": "v_search_btn_datagrid",
    "layout": {
        "datagrid": {
            "isInitSearch": false,
            "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "ce3607",
                    "case_no": "${{params.case_no}}"
                }
            },
            "pagination": {
                "show": true,
                "pageSizes": [
                    5,
                    10,
                    15,
                    20
                ],
                "pageSize": 5
            },
            "thead": [
                {
                    "type": "string",
                    "label": "联系人姓名",
                    "align": "center",
                    "prop": "linkman"
                },
                {
                    "type": "string",
                    "label": "联系人关系",
                    "align": "center",
                    "prop": "relationship",
                    "dict": {
                      "dictType": "CE_E_RELATIONSHIP",
                      "dictKey": [
                        "CE_E_RELATIONSHIP"
                      ],
                      "format": "label"
                    }
                },
                {
                    "type": "string",
                    "label": "电话类型",
                    "align": "center",
                    "prop": "tele_type",
                    "dict": {
                      "dictType": "CE_E_TELE_TYPE",
                      "dictKey": [
                        "CE_E_TELE_TYPE"
                      ],
                      "format": "label"
                    }
                },
                {
                    "type": "string",
                    "label": "电话号码",
                    "align": "center",
                    "prop": "tel_no"
                },
                {
                    "type": "string",
                    "label": "是否有效",
                    "align": "center",
                    "prop": "is_valid",
                    "dict": {
                        "dictType": "MS_E_YESORNO",
                        "dictKey": [
                            "MS_E_YESORNO"
                        ],
                        "format": "label"
                    }
                }
            ]
        }
    }
}
