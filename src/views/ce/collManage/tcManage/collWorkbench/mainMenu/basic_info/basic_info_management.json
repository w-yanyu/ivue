{
    "module": "dynamicTabs",
    "layout": {
        "tabs": {
            "scope": "formDate",
            "tabHeader": [
                {
                    "id": "information",
                    "label": "基本信息",
                    "closable": false,
                    "disabled": false,
                    "icon": "el-icon-date",
                    "toRequest": {
                        "url": "/views/ce/collManage/tcManage/collWorkbench/mainMenu/basic_info/baseInfo.json",
                        "method": "post",
                        "params": {
                            "case_no": "${{params.case_no}}"
                        }
                    }
                },
                {
                    "id": "individual",
                    "label": "个人信息",
                    "icon": "el-icon-date",
                    "closable": false,
                    "disabled": false,
                    "toRequest": {
                        "url": "/views/ce/collManage/tcManage/collWorkbench/mainMenu/basic_info/indivInfo.json",
                        "method": "post",
                        "params": {
                            "case_no": "${{params.case_no}}"
                        }
                    }
                },
                {
                    "id": "dueBill",
                    "label": "借据信息",
                    "icon": "el-icon-date",
                    "closable": false,
                    "disabled": false,
                    "toRequest": {
                        "url": "/views/ce/collManage/tcManage/collWorkbench/mainMenu/basic_info/dueBill.json",
                        "method": "post",
                        "params": {
                            "case_no": "${{params.case_no}}"
                        }
                    }
                },

                {
                    "id": "arrearMsg",
                    "label": "催记信息",
                    "icon": "el-icon-date",
                    "closable": false,
                    "disabled": false,
                    "toRequest": {
                        "url": "/views/ce/collManage/tcManage/collWorkbench/mainMenu/basic_info/collrec.json",
                        "method": "post",
                        "params": {
                            "case_no": "${{params.case_no}}"
                        }
                    }
                }
            ],
            "defaultActiveKey": "information",
            "position": "top",
            "styleClass": "card"
        }
    }
}
