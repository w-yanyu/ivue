{
    "module": "v_search_btn_datagrid",
    "layout": {
        "datagrid": {
            "isInitSearch": false,
            "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "ce3605",
                    "case_no": "${{params.case_no}}"
                }
            },
            "thead": [
                {
                    "label": "操作",
                    "fixed": "left",
                    "align":"center",
                    "width":"100px",
                    "btns": [
                        {
                            "type": "look",
                            "label": "查看还款计划",
                            "toRequest": {
                                "url": "/views/ce/collManage/tcManage/collWorkbench/mainMenu/basic_info/repayment_schedule.json",
                                "#url": "/views/ce/collManage/tcManage/collWorkbench/mainMenu/basic_info/repayPlan.json",
                                "method": "POST",
                                "params":{
                                    "loan_no": "${{current.loan_no}}"
                                }
                            },
                            "props": {
                                "width":"80%",
                                "title": "还款计划"
                            }
                        }
                    ]
                },
                {
                    "type":"string",
                    "label": "借据号",
                    "align":"center",
                    "width":"180px",
                    "prop": "loan_no"
                },
                {
                    "type":"string",
                    "label": "客户编号",
                    "align":"center",
                    "width":"180px",
                    "prop": "cust_no"
                },
                {
                    "type":"string",
                    "label": "客户姓名",
                    "align":"center",
                    "width":"180px",
                    "prop": "cust_name"
                },
                {
                    "type":"string",
                    "label": "发放日",
                    "align":"center",
                    "width":"180px",
                    "prop": "release_date"
                },
                {
                    "type":"currency",
                    "label":"贷款金额",
                    "align":"center",
                    "width":"180px",
                    "prop": "lou_amt",
                    "format": {
                        "decimal": "2"
                    }
                },
                {
                    "type":"currency",
                    "label": "当前余额",
                    "align":"center",
                    "width":"180px",
                    "prop": "current_bal",
                    "format": {
                        "decimal": "2"
                    }
                },
                {
                    "type":"currency",
                    "label": "正常余额",
                    "align":"center",
                    "width":"180px",
                    "prop": "normal_bal",
                    "format": {
                        "decimal": "2"
                    }
                },
                {
                    "type":"currency",
                    "label": "逾期本金",
                    "align":"center",
                    "width":"180px",
                    "prop": "overdue_princi",
                    "format": {
                        "decimal": "2"
                    }
                },
                {
                    "type":"currency",
                    "label": "逾期利息",
                    "align":"center",
                    "width":"180px",
                    "prop": "overdue_inst",
                    "format": {
                        "decimal": "2"
                    }
                },
                {
                    "type":"currency",
                    "label": "逾期余额",
                    "align":"center",
                    "width":"180px",
                    "prop": "overdue_bal",
                    "format": {
                        "decimal": "2"
                    }
                },
                {
                    "type":"currency",
                    "label": "本金罚息",
                    "align":"center",
                    "width":"180px",
                    "prop": "princi_fine_inst",
                    "format": {
                        "decimal": "2"
                    }
                },
                {
                    "type":"currency",
                    "label": "利息罚息",
                    "align":"center",
                    "width":"180px",
                    "prop": "inst_fine_inst",
                    "format": {
                        "decimal": "2"
                    }
                },
                {
                    "type":"currency",
                    "label": "逾期金额",
                    "align":"center",
                    "width":"180px",
                    "prop": "overdue_amt_total",
                    "format": {
                        "decimal": "2"
                    }
                },
                {
                    "type":"string",
                    "label": "本金逾期天数",
                    "align":"center",
                    "width":"180px",
                    "prop": "overdue_days"
                },
                {
                    "type":"string",
                    "label": "利息逾期天数",
                    "align":"center",
                    "width":"180px",
                    "prop": "oweinterest_days"
                },
                {
                    "type":"string",
                    "label": "业务品种",
                    "align":"center",
                    "width":"200px",
                    "prop": "busi_type_name"
                },
                {
                    "type": "string",
                    "align": "center",
                    "width": "180px",
                    "label": "本期五级形态",
                    "prop": "classify_result",
                    "dict": {
                      "dictType": "MS_E_LEVEL5_TYPE",
                      "dictKey": [
                        "MS_E_LEVEL5_TYPE"
                      ],
                      "format": "label"
                    }
                },

                {
                    "type":"string",
                    "label": "还款帐号",
                    "align":"center",
                    "width":"180px",
                    "prop": "repay_acct"
                },
                {
                    "type":"string",
                    "label": "贷款用途",
                    "align":"center",
                    "width":"180px",
                    "prop": "purpose",
                    "dict": {
                      "dictType": "CE_E_PURPOSE",
                      "dictKey": [
                        "CE_E_PURPOSE"
                      ],
                      "format": "label"
                    }
                },
                {
                    "type":"string",
                    "label": "渠道",
                    "align":"center",
                    "width":"180px",
                    "prop": "initiate_tran_channel",
                    "dict": {
                      "dictType": "CE_E_CHANNEL_NUM",
                      "dictKey": [
                        "CE_E_CHANNEL_NUM"
                      ],
                      "format": "label"
                    }
                }
            ]
        }
    }
}
