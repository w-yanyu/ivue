          "format": {
            "decimal": "2"
          }
        },
        {
          "type": "currency",
          "label": "应计利息",
          "align": "center",
          "width": "180px",
          "prop": "accrued_inst",
          "format": {
            "decimal": "2"
          }
        },
        {
          "type": "currency",
          "label": "应计罚息",
          "align": "center",
          "width": "180px",
          "prop": "accrued_penalt_inst",
          "format": {
            "decimal": "2"
          }
        },
        {
          "type": "string",
