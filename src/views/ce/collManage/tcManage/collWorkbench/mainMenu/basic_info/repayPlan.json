{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form":{
      "btns": []

    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/call/RPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ce3609",
          "iou_no": "${{params.loan_no}}"
        }
      },

      "pagination": {
        "pageSizes": [5, 10, 15, 20],
        "pageSize": 5,
        "show": true
      },
      "thead": [
        {
          "label": "序号",
          "align": "center",
          "type": "index"
        },
        {
          "type": "string",
          "label": "借据编号",
          "align": "center",
          "width": "180px",
          "prop": "iou_no"
        },
        {
          "type": "string",
          "label": "期数",
          "align": "center",
          "width": "180px",
          "prop": "period_num"
        },
        {
          "type": "string",
          "label": "到期日",
          "align": "center",
          "width": "180px",
          "prop": "matu_date"
        },
        {
          "type": "currency",
          "label": "宽限期供到期日期",
          "align": "center",
          "width": "180px",
          "prop": "grace_due_date"
        },
        {
          "type": "currency",
          "label": "计息本金",
          "align": "center",
          "width": "180px",
          "prop": "inst_princi",
          "format": {
            "decimal": "2"
          }
