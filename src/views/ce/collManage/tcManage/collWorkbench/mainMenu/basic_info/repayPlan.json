          }
        },
        {
          "type": "string",
          "label": "是否结清",
          "align": "center",
          "width": "180px",
          "prop": "is_clear",
          "dict": {
            "dictType": "MS_E_YESORNO",
            "dictKey": [
              "MS_E_YESORNO"
            ],
            "format": "label"
          }
        }
      ]
    }
  }
