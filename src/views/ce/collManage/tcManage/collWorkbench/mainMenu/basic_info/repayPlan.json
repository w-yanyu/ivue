          "width": "180px",
          "prop": "term_prin",
          "format": {
            "decimal": "2"
          }
        },
        {
          "type": "currency",
          "label": "期供利息",
          "align": "center",
          "width": "180px",
          "prop": "period_inst",
          "format": {
            "decimal": "2"
          }
        },
        {
          "type": "currency",
          "label": "部分提前还本金",
          "align": "center",
          "width": "180px",
          "prop": "early_rpym_principal",
          "format": {
            "decimal": "2"
          }
