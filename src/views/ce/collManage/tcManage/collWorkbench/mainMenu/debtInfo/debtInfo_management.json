{
  "module": "v_realonlyForm_btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/call/RPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ce3601",
          "case_no": "${{params.case_no}}"
        }
      },
      "controls": [
        {
          "overdue_amt": {
            "control": "currency",
            "disabled": true,
            "label": "逾期金额",
            "inputWidth": "100px",
            "span": "10"
          },
          "overdue_princi": {
            "control": "currency",
            "disabled": true,
            "label": "逾期本金",
            "inputWidth": "100px",
            "span": "10"
          },
          "overdue_inst": {
            "control": "currency",
            "disabled": true,
            "label": "逾期利息",
            "inputWidth": "100px",
            "span": "10"
          },
          "overdue_days": {
            "control": "text",
            "disabled": true,
            "label": "逾期天数",
            "inputWidth": "100px",
            "span": "10"
          },
          "accumarr_times": {
            "control": "text",
            "disabled": true,
            "label": "逾期期数",
            "inputWidth": "100px",
            "span": "10"
          }
        }
      ]
    }
  }
}
