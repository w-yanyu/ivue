{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "scope": "formData",
            "initRequest": "${{params}}",
            "####doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "POST",
                "params": {
                    "servicecode": ""
                }
            },
            "btns": [],

            "controls": [
                {
                    "tel": {
                        "control": "text",
                        "label": "",
                        "placeholder": "请输入电话号码",
                        "width": "130px",
                        "rules": [
                            {
                                "#pattern":"\\d{3}-\\d{8}|\\d{4}-\\d{7}|^1(3[0-9]|4[57]|5[0-35-9]|7[0678]|8[0-9])\\d{8}$",
                                "pattern":"^(1\\d{10})+$|^0\\d{2,3}-?\\d{7,8}$",
                                "message":"请输入正确的电话号码！"
                            }
                        ]
                    }
                },
                {
                    "but1": {
                        "control": "button",
                        "icon": "el-icon-service",
                        "label": "拨打",
                        "type": "custom",
                        "span":"4",
                        "#doRequest":{}
                    },
                    "but2": {
                        "control": "button",
                        "icon": "el-icon-phone",
                        "label": "挂断",
                        "type": "custom",
                        "span":"4",
                        "#doRequest":{}
                    },
                    "but3": {
                        "control": "button",
                        "icon": "el-icon-phone-outline",
                        "label": "保持",
                        "type": "custom",
                        "span":"4",
                        "#doRequest":{}
                    },
                    "but4": {
                      "control": "button",
                      "icon": "el-icon-check",
                      "label": "恢复",
                      "type": "custom",
                      "span":"4",
                      "#doRequest":{}
                    }
                },
                {
                    "but5": {
                        "control": "button",
                        "icon": "el-icon-sort-down",
                        "label": "签入",
                        "type": "custom",
                        "span":"4",
                        "#doRequest":{}
                    },
                    "but6": {
                        "control": "button",
                        "icon": "el-icon-sort-up",
                        "label": "签出",
                        "type": "custom",
                        "span":"4",
                        "#doRequest":{}
                    },
                    "but7": {
                        "control": "button",
                        "icon": "el-icon-time",
                        "label": "示闲",
                        "type": "custom",
                        "span":"4",
                        "#doRequest":{}
                    },
                    "but8": {
                        "control": "button",
                        "icon": "el-icon-loading",
                        "label": "离席",
                        "type": "custom",
                        "span":"4",
                        "#doRequest":{}
                    },
                    "but9": {
                        "control": "button",
                        "icon": "el-icon-circle-check",
                        "label": "完成",
                        "type": "custom",
                        "span":"4",
                        "#doRequest":{}
                    }
                }
            ]
        }
    }
}
