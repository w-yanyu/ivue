{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/call/RPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ce3601",
          "case_no": "${{params.case_no}}"
        }
      },
      "btns": [
        {
          "type": "submit",
          "icon": "el-icon-document",
          "label": "保存",
          "doRequest": {
            "url": "/SUMP/call/RPCCall",
            "method": "POST",
            "params": {
              "servicecode": "ce3619",
              "case_no": "${{params.case_no}}"
            }
          }
        }
      ],
      "controls": [
        {
          "case_id": {
            "span": "24",
            "control": "checkbox",
            "labelWidth": "0px",
            "label": "请选择",
            "options": [
              {
                "label": "难联",
                "value": "1",
                "width": "100px"
              },
              {
                "label": "失联",
                "value": "2",
                "width": "100px"
              },
              {
                "label": "死亡",
                "value": "3",
                "width": "100px"
              },
              {
                "label": "投诉",
                "value": "4",
                "width": "100px"
              },
              {
                "label": "筹资",
                "value": "5",
                "width": "100px"
              },
              {
                "label": "重大疾病",
                "value": "6",
                "width": "100px"
              },
              {
                "label": "刑事拘役",
                "value": "7",
                "width": "100px"
              },
              {
                "label": "疑似欺诈",
                "value": "8",
                "width": "100px"
              }
            ]
          }
        }
      ]
    }
  }
}
