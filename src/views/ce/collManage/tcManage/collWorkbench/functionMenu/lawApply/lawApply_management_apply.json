{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "btns": [
        {
          "type": "custom",
          "target": "dialog",
          "label": "申请",
          "doRequest": {
            "url": "/SUMP/call/RPCCall",
            "method": "POST",
            "params": {
              "servicecode": "ce3528",
              "case_no":"${{params.case_no}}",
              "apply_reason": "${{form.apply_reason}}",
              "legal_type": "${{form.legal_type}}",
              "queue_code": "${{form.queue_code}}"
            }
          }
        },
        {
          "type": "close",
          "label": "返回"
        }
      ],
      "controls": [
        {
          "legal_type": {
            "control": "select",
            "span": 8,
            "labelWidth": "0",
            "inputWidth": "250px",
            "disabled": true,
            "default": "outer",
            "label": "法诉类型",
              "dict": {
                "dictType": "CE_E_LEGAL_TYPE",
                "dictKey": [
                  "CE_E_LEGAL_TYPE"
                ],
                "format": "label"
              },
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          },
          "queue_code": {
            "control": "remoteSelect",
            "span": 8,
            "labelWidth": "0",
            "inputWidth": "250px",
            "label": "法诉队列",
            "labelField": "queue_name",
            "valueField": "queue_code",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ce0054",
                "coll_kind": "02",
                "collection_type": "LG01",
                "is_valid": "1"
              }
            },
            "format": "label",
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          },
          "apply_reason": {
            "control": "textarea",
            "span": 22,
            "labelWidth": "0",
            "rows":"6",
            "label": "申请原因",
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          }
        }
      ]
    }
  }
}
