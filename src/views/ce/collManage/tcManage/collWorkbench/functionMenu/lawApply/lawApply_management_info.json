{
  "module": "v_search_btn_datagrid",
  "layout": {

    "toolbar": {
      "btns": []
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/call/RPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ce3554",
          "case_no": "${{params.case_no}}"
        }
      },
      "thead": [
        {
          "type": "string",
          "label": "申请人",
          "align": "center",
          "prop": "appt",
          "dict": {
            "dictKey": [
              "appt"
            ],
            "format": "label",
            "labelField": "user_name",
            "valueField": "user_id",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ce0403"
              }
            }
          }
        },
        {
          "type": "string",
          "label": "申请时间",
          "align": "center",
          "prop": "apply_date"
        },
        {
          "type": "string",
          "label": "申请原因",
          "align": "center",
          "prop": "apply_reason"
        },
        {
          "type": "string",
          "label": "法诉类型",
          "align": "center",
          "prop": "legal_type",
          "dict": {
            "dictType": "CE_E_LEGAL_TYPE",
            "dictKey": [
              "CE_E_LEGAL_TYPE"
            ],
            "format": "label"
          }
        },
        {
          "type": "string",
          "label": "审核状态",
          "align": "center",
          "prop": "appr_state",
          "dict": {
            "dictType": "CE_APPR_STATE",
            "dictKey": [
              "CE_APPR_STATE"
            ],
            "format": "label"
          }
        },
        {
          "type": "string",
          "label": "审核人",
          "align": "center",
          "prop": "approver",
          "dict": {
            "dictKey": [
              "approver"
            ],
            "format": "label",
            "labelField": "user_name",
            "valueField": "user_id",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ce0403"
              }
            }
          }
        },
        {
          "type": "string",
          "label": "审核日期",
          "align": "center",
          "prop": "appr_date"
        },
        {
          "type": "string",
          "label": "审核说明",
          "align": "center",
          "prop": "appr_comment"
        }
      ]
    }
  }
}
