{
  "module": "v-inputDataForm-btn",
  "layout": {
    "scope": "templateScope",
    "form": {
      "btns":[],
      "controls": [
        {
          "top": {
            "control":"dynamic",
            "span":24,
            "toRequest":{
              "url":"/views/ce/collManage/tcManage/collWorkbench/functionMenu/lawApply/lawApply_management_apply.json",
              "method": "post",
              "params": {
                "case_no":"${{params.case_no}}"
              }
            }
          },
          "bottom": {
            "control":"dynamic",
            "span":24,
            "toRequest":{
              "url":"/views/ce/collManage/tcManage/collWorkbench/functionMenu/lawApply/lawApply_management_info.json",
              "method": "post",
              "params": {
                "case_no":"${{params.case_no}}"
              }
            }
          }
        }
      ]
    }
  }
}
