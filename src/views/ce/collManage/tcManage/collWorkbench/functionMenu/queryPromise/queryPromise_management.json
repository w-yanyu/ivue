{
  "module": "v_search_btn_datagrid",
  "layout": {

    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/call/RPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ce3512",
          "case_no":"${{params.case_no}}"
        }
      },
      "thead": [
        {
          "type":"string",
          "label": "催收组",
          "align":"center",
          "width":"180px",
          "prop": "coll_group",
          "dict":{
            "dictKey": [
              "coll_group"
            ],
            "format": "label",
            "labelField": "branch_name",
            "valueField": "junior_brch_id",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "servicecode": "pl0400"
              }
            }
          }

        },
        {
          "type": "string",
          "align": "center",
          "width":"180px",
          "label": "催收员",
          "prop": "collector",
          "dict": {
            "dictKey": [
              "collector"
            ],
            "format": "label",
            "labelField": "user_name",
            "valueField": "user_id",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ce0403"
              }
            }
          }
        },
        {
          "type":"string",
          "label": "逾期金额",
          "align":"center",
          "width":"180px",
          "prop": "overdue_amt"
        },
        {
          "type":"string",
          "label": "承诺金额",
          "align":"center",
          "width":"180px",
          "prop": "proms_amt"
        },
        {
          "type":"string",
          "label": "承诺剩余金额",
          "align":"center",
          "width":"180px",
          "prop": "prom_rest_amt"
        },
        {
          "type":"string",
          "label": "承诺日期",
          "align":"center",
          "width":"180px",
          "prop": "commit_date"
        },
        {
          "type":"string",
          "label": "累计还款金额",
          "align":"center",
          "width":"180px",
          "prop": "repay_amt"
        },
        {
          "type":"string",
          "label": "承诺状态",
          "align":"center",
          "width":"180px",
          "prop": "promise_state",
          "dict": {
            "dictType": "CE_E_PTPSTATUS",
            "dictKey": [
              "CE_E_PTPSTATUS"
            ],
            "format": "label"
          }
        },
        {
          "type":"string",
          "label": "创建人",
          "align":"center",
          "width":"180px",
          "prop": "data_create_user",
          "dict":{
            "dictKey": [
              "data_create_user"
            ],
            "format": "label",
            "labelField": "user_name",
            "valueField": "user_id",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ce0403"
              }
            }
          }
        },
        {
          "type":"string",
          "label": "创建时间",
          "align":"center",
          "width":"180px",
          "prop": "data_create_time"
        },
        {
          "type":"string",
          "label": "是否有效",
          "align":"center",
          "width":"180px",
          "prop": "is_valid",
          "dict": {
            "dictType": "MS_E_YESORNO",
            "dictKey": [
              "MS_E_YESORNO"
            ],
            "format": "label"
          }
        }
      ]
    }
  }
}
