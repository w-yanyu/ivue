{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "btns": [
        {
          "type": "closeNavTab",
          "target": "dialog",
          "label": "申请",
          "doRequest": {
            "url": "/SUMP/call/RPCCall",
            "method": "POST",
            "params": {
              "servicecode": "ce4710",
              "case_no":"${{params.case_no}}",
              "apply_reason": "${{form.apply_reason}}",
              "writeoff_type": "${{form.writeoff_type}}"
            }
          }
        },
        {
          "type": "close",
          "label": "返回"
        }
      ],
      "controls": [
        {
          "writeoff_type": {
            "control": "select",
            "span": "6",
            "inputWidth": "200px",
            "label": "核销类型",
            "dict": {
              "dictType": "CE_E_WRITEOFF_TYPE",
              "dictKey": [
                "CE_E_WRITEOFF_TYPE"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          },
          "apply_reason": {
            "control": "textarea",
            "span": 22,
            "labelWidth": "0",
            "rows":"6",
            "label": "申请原因",
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          }
        }
      ]
    }
  }
}
