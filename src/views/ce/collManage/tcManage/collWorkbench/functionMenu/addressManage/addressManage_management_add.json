{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "el-icon-tickets",
          "doRequest": {
            "url": "/SUMP/call/RPCCall",
            "method": "POST",
            "params": {
              "servicecode": "ce3542",
              "case_no": "${{params.case_no}}"
            }
          }
        },
        {
          "type": "close",
          "label": "取消"
        }
      ],
      "controls": [
        {
          "assoc": {
            "control": "select",
            "span": "6",
            "inputWidth": "200px",
            "label": "客户关系",
            "dict": {
              "dictType": "CE_E_RELATIONSHIP",
              "dictKey": [
                "CE_E_RELATIONSHIP"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ],
            "events": {
              "condition": {
                "9999": {
                  "doRequest": {
                    "url": "/SUMP/call/RPCCall",
                    "method": "POST",
                    "params": {
                      "servicecode": "ce3602",
                      "case_no": "${{params.case_no}}"
                    },
                    "resultSet": {
                      "linkman": "name"
                    }
                  },
                  "control": {
                    "linkman": {
                      "disabled": true
                    }
                  }
                },
                "$default": {
                  "control": {
                    "linkman": {
                      "value": "",
                      "disabled": false
                    }
                  }
                }
              }
            }
          },
          "linkman": {
            "control": "text",
            "span": "8",
            "inputWidth": "200px",
            "label": "联系人姓名",
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          },
          "is_valid": {
            "control": "select",
            "span": "8",
            "inputWidth": "200px",
            "label": "是否有效",
            "default": "1",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          }
        },
        {
          "addr_type": {
            "control": "select",
            "span": "6",
            "inputWidth": "200px",
            "default": "",
            "label": "地址类型",
            "dict": {
              "dictType": "CE_E_ADDR_TYPE",
              "dictKey": [
                "CE_E_ADDR_TYPE"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ],
            "showItem": {
              "B": [
                "city-picker3"
              ],
              "F": [
                "city-picker3"
              ],
              "H": [
                "city-picker3"
              ],
              "W": [
                "city-picker3"
              ]
            }
          },
          "post_cd": {
            "control": "text",
            "span": "8",
            "inputWidth": "200px",
            "label": "邮编",
            "rules": [
              {
                "pattern": "^[1-9]\\d{5}$",
                "message": "请输入正确邮编！"
              }
            ]
          }
        },
        {
          "city-picker3": {
            "####":"省市区和详细地址存一个字段",
            "control": "hidden",
            "#control": "cascader",
            "span": "6",
            "inputWidth": "1000px",
            "label": "省/市/区",
            "doRequest": {
              "url": "/views/ce/collManage/tcManage/collWorkbench/functionMenu/addressManage/cascader-format.json",
              "method": "get",
              "params": {}
            },
            "###rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          }
        },
        {
          "address": {
            "control": "text",
            "span": "6",
            "inputWidth": "1000px",
            "label": "地址",
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          }
        },
        {
          "remark": {
            "control": "textarea",
            "span": "6",
            "inputWidth": "1000px",
            "rows": "12",
            "label": "备注"
          }
        }
      ]
    }
  }
}
