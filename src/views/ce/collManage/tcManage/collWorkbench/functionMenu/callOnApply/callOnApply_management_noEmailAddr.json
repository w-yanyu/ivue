{
  "module": "v_search_btn_datagrid",
  "layout": {

    "toolbar": {
      "btns": [
      ]
    },
    "datagrid": {
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/call/RPCCall",
        "method": "post",
        "params": {
          "servicecode": "ce3551",
          "case_no":"${{params.case_no}}"
        }
      },
      "pagination": {
        "pageSizes": [
          10,
          15,
          20,
          50,
          100
        ],
        "pageSize": 10
      },
      "thead": [
        {
          "label": "联系人姓名",
          "prop": "linkman",
          "align": "center"
        },
        {
          "label": "客户关系",
          "prop": "assoc",
          "align": "center",
          "dict": {
            "dictType": "CE_E_RELATIONSHIP",
            "dictKey": [
              "CE_E_RELATIONSHIP"
            ],
            "format": "label"
          }
        },
        {
          "label": "地址类型",
          "prop": "addr_type",
          "align": "center",
          "dict": {
            "dictType": "CE_E_ADDR_TYPE",
            "dictKey": [
              "CE_E_ADDR_TYPE"
            ],
            "format": "label"
          }
        },
        {
          "label":"地址",
          "prop": "address",
          "align": "center"
        },
        {
          "label":  "申请类型",
          "prop": "reg_type",
          "align": "center",
          "dict": {
            "dictType": "CE_E_REG_TYPE",
            "dictKey": [
              "CE_E_REG_TYPE"
            ],
            "format": "label"
          }
        },
        {
          "label": "是否有效",
          "prop": "is_valid",
          "align": "center",
          "dict": {
            "dictType": "MS_E_YESORNO",
            "dictKey": [
              "MS_E_YESORNO"
            ],
            "format": "label"
          }
        },
        {
          "label":  "备注",
          "prop": "remark",
          "align": "center"
        },
        {
          "label": "操作",
          "align": "center",
          "width": "100px",
          "btns": [
            {
              "type": "custom",
              "label": "申请",
              "toRequest": {
                "url": "/views/ce/collManage/tcManage/collWorkbench/functionMenu/callOnApply/callOnApply_management_apply.json",
                "method": "POST",
                "params": {
                  "case_no":"${{params.case_no}}",
                  "address": "${{current.address}}",
                  "relationship": "${{current.assoc}}"
                }
              }
            },
            {
              "props": {
                "width": "80%",
                "title": "label"
              }
            }
          ]
        }
      ]
    }
  }
}
