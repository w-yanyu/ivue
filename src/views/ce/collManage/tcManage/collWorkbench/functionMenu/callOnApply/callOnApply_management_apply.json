{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "btns": [
        {
          "type": "submit",
          "target": "dialog",
          "label": "申请",
          "doRequest": {
            "url": "/SUMP/call/RPCCall",
            "method": "POST",
            "params": {
              "servicecode": "ce3526",
              "case_no":"${{params.case_no}}",
              "apply_reason": "${{form.apply_reason}}",
              "relationship": "${{current.relationship}}",
              "address": "${{current.address}}",
              "callon_name":"${{current.linkman}}"
            }
          }
        }
      ],
      "controls": [
        {
          "linkman": {
            "control": "string",
            "span": 11,
            "labelWidth": "0",
            "inputWidth": "250px",
            "disabled": false,
            "label": "被访人姓名"
          },
          "relationship": {
            "control": "string",
            "width": "300px",
            "span": 10,
            "inputWidth": "245px",
            "disable": false,
            "label": "与持卡人关系",
            "dict": {
              "dictType": "CE_E_RELATIONSHIP",
              "dictKey": [
                "CE_E_RELATIONSHIP"
              ],
              "format": "label"
            }
          },
          "address": {
            "control": "string",
            "span": 24,
            "labelWidth": "0",
            "disable": false,
            "label": "被访人地址"
          },
          "apply_reason": {
            "control": "textarea",
            "span": 24,
            "labelWidth": "0",
            "rows": "6",
            "label": "申请原因",
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          }
        }
      ]
    }
  }
}
