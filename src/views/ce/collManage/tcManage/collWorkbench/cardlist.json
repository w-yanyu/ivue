{
  "module": "v-cardList4",
  "layout": {
    "card": {
      "span": {
        "xs": 12,
        "sm": 12,
        "md": 12,
        "lg": 12,
        "xl": 12
      },

      "list": [
        {
          "label": "逾期欠款（实时）",
          "height":"250px",
          "toRequest": {
            "url": "/views/ce/collManage/tcManage/collWorkbench/mainMenu/debtInfo/debtInfo_management.json",
            "method": "POST",
            "params": {
              "case_no": "${{params.case_no}}"
            }
          },
          "params": {}
        },
        {
          "label": "催收标识",
          "height":"250px",
          "toRequest": {
            "url": "/views/ce/collManage/tcManage/collWorkbench/mainMenu/collMark/collMark_management.json",
            "method": "POST",
            "params": {
              "case_no": "${{params.case_no}}"
            }
          },
          "params": {}
        },
        {
          "label": "话术指导",
          "height":"420px",
          "toRequest": {
            "url": "/views/ce/collManage/tcManage/collWorkbench/mainMenu/skillGuide/skillGuide_management.json",
            "method": "POST",
            "params": {
              "case_no": "${{params.case_no}}"
            }
          },
          "params": {}
        },
        {
          "label": "提交保存",
          "height":"420px",
          "toRequest": {
            "url": "/views/ce/collManage/tcManage/collWorkbench/mainMenu/control/control_management.json",
            "method": "POST",
            "params": {
              "case_no": "${{params.case_no}}",
              "queue_code":"${{params.queue_code}}"
            }
          },
          "params": {}
        },
        {
          "label": "联系信息",
          "##height":"800px",
          "toRequest": {
            "url": "/views/ce/collManage/tcManage/collWorkbench/mainMenu/contact/contact_management.json",
            "method": "POST",
            "params": {
              "case_no": "${{params.case_no}}"
            }
          },
          "###params": {}
        },
        {
          "label": "电话",
          "height":"230px",
          "toRequest": {
            "url": "/views/ce/collManage/tcManage/collWorkbench/mainMenu/telephone/telephone_management.json",
            "method": "POST",
            "params": {}
          },
          "params": {}
        }
      ]
    }
  }
}
