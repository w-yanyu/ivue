{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "record_id": {
            "span":"6",
            "control": "text",
            "label": "导入批次号"
          },
          "coll_date": {
            "span":"6",
            "control": "dateTimePicker",
            "label": "导入日期",
            "format": "yyyyMMdd"
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "RLMSPLTS1304_pl1304"
        }
      },
      "thead": [
        {
          "label": "导入批次号",
          "prop": "record_id"
        },

        {
          "label": "区域中心编码",
          "prop": "regional_center"
        },
        {
          "label": "催记月份",
          "prop": "coll_month"
        },
        {
          "label": "导入总记录数",
          "prop": "coll_all_count"
        },
        {
          "label": "导入成功数",
          "prop": "coll_succ_count"
        },
        {
          "label": "导入失败数",
          "prop": "coll_fail_count"
        },
        {
          "label": "导入操作员",
          "prop": "tranus"
        },
        {
          "label": "导入时间",
          "prop": "gmt_create"
        },
        {
          "label": "操作",
          "fixed": "right",
          "width": 180,
          "align":"center",
          "btns": [
            {
              "show": {
                "expression":"'${{current.coll_succ_count}}' > 0"
              },
              "type": "edit",
              "label": "成功明细",
              "toRequest": {
                "url": "/views/pl/collManage/collRecord/pls_coll_record_info.json",
                "method": "GET",
                "params": {
                 "record_id":"${{current.record_id}}"
                }
              },
              "props": {
                "width": "90%"
              }
            },
            {
              "type": "edit",
              "label": "失败明细",
              "show": {
                "expression":"'${{current.coll_fail_count}}' > 0"
              },
              "toRequest": {
                "url": "/views/pl/collManage/collRecord/pls_coll_record_upload_error.json",
                "method": "GET",
                "params": {
                  "record_id":"${{current.record_id}}"
                }
              },
              "props": {
                "width": "90%"
              }
            }
          ]
        }
      ]
    }
  }
}
