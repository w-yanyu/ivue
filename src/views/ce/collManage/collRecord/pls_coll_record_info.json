{
  "module": "v_search_btn_datagrid",
  "layout": {
    "initReqeust": "${{params}}",
    "form": {
      "controls": [
        {
          "record_id": {
            "span":"12",
            "control": "text",
            "label": "导入批次号",
            "initShow": false,
            "value": "${{params.record_id}}"
          },
          "oa_org_no": {
            "span":"8",
            "label": "委外公司",
            "control": "remoteSelect",
            "labelField": "dictName",
            "valueField": "dictId",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "RLMSPLTS1072"
              }
            },
            "format": "label"
          },
          "acct_no": {
            "span":"8",
            "control": "text",
            "label": "借据号"
          },
          "custna": {
            "span":"8",
            "control": "text",
            "label": "客户名称"
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "RLMSPLTS1305_pl1305"
        }
      },
      "thead": [
        {
          "label": "导入批次号",
          "prop": "record_id"
        },

        {
          "label": "借据号",
          "prop": "acct_no"
        },
        {
          "label": "客户名称",
          "prop": "custna"
        },
        {
          "label": "委外公司编号",
          "prop": "oa_org_name"
        },
        {
          "label": "委外公司名称",
          "prop": "oa_org_name"
        },
        {
          "label": "催收时间",
          "prop": "coll_time"
        },
        {
          "label": "催收方式",
          "prop": "collection_way"
        },
        {
          "label": "接洽人",
          "prop": "contacts"
        },
        {
          "label": "联系状态",
          "prop": "cont_status"
        },
        {
          "label": "还款意愿",
          "prop": "repay_will0"
        },
        {
          "label": "还款能力",
          "prop": "repay_ability"
        },
        {
          "label": "客群特征",
          "prop": "cust_feat"
        },
        {
          "label": "催收记录",
          "prop": "remake"
        },
        {
          "label": "催记月份",
          "prop": "coll_month"
        }
      ]
    }
  }
}
