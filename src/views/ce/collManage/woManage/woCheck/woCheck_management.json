{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "scope": "formData",
      "foldLineNumber": 3,
      "controls": [
        {
          "branch_org": {
            "control": "remoteSelect",
            "label": "机构",
            "labelField": "branch_name",
            "valueField": "branch_id",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2173"
              }
            },
            "format": "label"
          },
          "cust_name": {
            "control": "text",
            "label": "客户姓名"
          }
        },
        {
          "id_no": {
            "control": "text",
            "label": "证件号码"
          },
          "cust_id": {
            "control": "text",
            "label": "客户编号"
          }
        },
        {
          "duebill_no": {
            "control": "text",
            "label": "借据号"
          },
          "check_status": {
            "control": "remoteSelect",
            "label": "审核状态",
            "default": "NC",
            "labelField": "drop_list_desc",
            "valueField": "drop_list_value",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ap1071",
                "drop_list_type": "check_status"
              }
            },
            "format": "label"
          }
        },
        {
          "daterange": {
            "control": "dateTimePicker",
            "label": "申请时间",
            "type": "daterange",
            "unlinkPanels": true,
            "valueFormat": "yyyyMMdd",
            "startPlaceholder": "选择开始时间",
            "endPlaceholder": "选择结束时间",
            "startField": "start_date",
            "endField": "end_date"
          }
        },
        {
          "queue_cd": {
            "control": "remoteSelect",
            "label": "队列名称",
            "labelField": "queue_name",
            "valueField": "queue_cd",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "pl0402"
              }
            },
            "format": "label"
          },
          "location_cd": {
            "control": "remoteSelect",
            "label": "案件类型",
            "labelField": "location_name",
            "valueField": "location_cd",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "pl0410"
              }
            },
            "format": "label"
          }
        },
        {
          "status_cd": {
            "control": "remoteSelect",
            "label": "案件状态",
            "labelField": "status_name",
            "valueField": "status_cd",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "pl0411"
              }
            },
            "format": "label"
          },
          "function_cd": {
            "control": "remoteSelect",
            "label": "逾期阶段",
            "labelField": "function_name",
            "valueField": "function_cd",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "pl0412"
              }
            },
            "format": "label"
          }
        },
        {
          "ovd_amt_total_1": {
            "control": "currency",
            "label": "欠款金额区间开始值"
          },
          "ovd_amt_total_2": {
            "control": "currency",
            "label": "欠款金额区间结束值"
          }
        },
        {
          "collect_date_1": {
            "control": "inputNumber",
            "label": "入催天数区间开始值"
          },
          "collect_date_2": {
            "control": "inputNumber",
            "label": "入催天数区间结束值"
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "custom",
          "icon": "el-icon-edit",
          "target": "dialog",
          "checkCountMin": 1,
          "label": "批量审核",
          "toRequest": {
            "method": "GET",
            "url": "/views/pl/collManage/woManage/woCheck/woCheck_reason.json"
          }
        },
        {
          "type": "custom",
          "icon": "el-icon-circle-check",
          "label": "全部核准",
          "disabled":"'${{current.check_status}}' === 'P' || '${{current.check_status}}' === 'R' || '${{current.check_status}}' === ''",
          "doRequest": {
            "method": "POST",
            "url": "/SUMP/icmcall/icmRPCCall",
            "params": {
              "servicecode":"pl4706",
              "submit_type": "P",
              "check_desc":"批量审核-同意",
              "check_status": "NC",
              "branch_id": "${{formData.branch_id}}",
              "cust_name": "${{formData.cust_name}}",
              "id_no": "${{formData.id_no}}",
              "cust_id": "${{formData.cust_id}}",
              "duebill_no": "${{formData.duebill_no}}",
              "daterange": "${{formData.daterange}}",
              "queue_cd": "${{formData.queue_cd}}",
              "status_cd": "${{formData.status_cd}}",
              "function_cd": "${{formData.function_cd}}",
              "ovd_amt_total_1": "${{formData.ovd_amt_total_1}}",
              "ovd_amt_total_2": "${{formData.ovd_amt_total_2}}",
              "collect_date_1": "${{formData.collect_date_1}}",
              "collect_date_2": "${{formData.collect_date_2}}"
            }
          }
        },
        {
          "type": "custom",
          "icon": "el-icon-circle-close",
          "label": "全部拒绝",
          "disabled":"'${{current.check_status}}' === 'P' || '${{current.check_status}}' === 'R' || '${{current.check_status}}' === ''",
          "doRequest": {
            "method": "POST",
            "url": "/SUMP/icmcall/icmRPCCall",
            "params": {
              "servicecode":"pl4706",
              "submit_type": "R",
              "check_desc":"批量审核-拒绝",
              "check_status": "NC",
              "branch_id": "${{formData.branch_id}}",
              "cust_name": "${{formData.cust_name}}",
              "id_no": "${{formData.id_no}}",
              "cust_id": "${{formData.cust_id}}",
              "duebill_no": "${{formData.duebill_no}}",
              "daterange": "${{formData.daterange}}",
              "queue_cd": "${{formData.queue_cd}}",
              "status_cd": "${{formData.status_cd}}",
              "function_cd": "${{formData.function_cd}}",
              "ovd_amt_total_1": "${{formData.ovd_amt_total_1}}",
              "ovd_amt_total_2": "${{formData.ovd_amt_total_2}}",
              "collect_date_1": "${{formData.collect_date_1}}",
              "collect_date_2": "${{formData.collect_date_2}}"
            }
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pl4701"
        }
      },
      "thead": [
        {
          "type": "selection",
          "align": "center",
          "width": "55",
          "fixed": "left"
        },
        {
          "type": "string",
          "align": "center",
          "width": "100",
          "label": "机构",
          "prop": "branch_org",
          "dict": {
            "dictKey": [
              "branch_org"
            ],
            "format": "label",
            "labelField": "branch_name",
            "valueField": "branch_id",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2173"
              }
            }
          }
        },
        {
          "type": "string",
          "align": "center",
          "width":"180px",
          "label": "案件编号",
          "prop": "case_id"
        },
        {
          "type": "string",
          "align": "center",
          "width":"180px",
          "label": "客户编号",
          "prop": "cust_id"
        },
        {
          "type": "string",
          "align": "center",
          "width":"180px",
          "label": "客户姓名",
          "prop": "cust_name"
        },
        {
          "type": "string",
          "align": "center",
          "width":"180px",
          "label": "证件类型",
          "prop": "id_type",
          "dict": {
            "dictKey": [
              "id_type"
            ],
            "format": "label",
            "labelField": "drop_list_desc",
            "valueField": "drop_list_value",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ap1071",
                "drop_list_type": "CertType"
              }
            }
          }
        },
        {
          "type": "string",
          "align": "center",
          "width":"180px",
          "label": "证件号码",
          "prop": "id_no"
        },
        {
          "type": "string",
          "align": "center",
          "width":"180px",
          "label": "申请人",
          "prop": "apply_user",
          "dict": {
            "dictKey": [
              "apply_user"
            ],
            "format": "label",
            "labelField": "user_name",
            "valueField": "user_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0084"
              }
            }
          }
        },
        {
          "type": "string",
          "align": "center",
          "width":"180px",
          "label": "申请日期",
          "prop": "apply_time"
        },
        {
          "type": "string",
          "align": "center",
          "width":"180px",
          "label": "申请说明",
          "prop": "apply_reason"
        },
        {
          "type": "string",
          "align": "center",
          "width":"180px",
          "label": "审核状态",
          "prop": "check_status",
          "dict": {
            "dictKey": [
              "check_status"
            ],
            "format": "label",
            "labelField": "drop_list_desc",
            "valueField": "drop_list_value",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ap1071",
                "drop_list_type": "check_status"
              }
            }
          }
        },
        {
          "type": "string",
          "align": "center",
          "width":"180px",
          "label": "核销类型",
          "prop": "wo_type",
          "dict": {
            "dictKey": [
              "wo_type"
            ],
            "format": "label",
            "labelField": "drop_list_desc",
            "valueField": "drop_list_value",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ap1071",
                "drop_list_type": "wo_type"
              }
            }
          }
        },
        {
          "type": "string",
          "align": "center",
          "width":"180px",
          "label": "审核人",
          "prop": "check_user",
          "dict": {
            "dictKey": [
              "check_user"
            ],
            "format": "label",
            "labelField": "user_name",
            "valueField": "user_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0084"
              }
            }
          }
        },
        {
          "type": "string",
          "align": "center",
          "width":"180px",
          "label": "审核日期",
          "prop": "check_time"
        },
        {
          "type": "string",
          "align": "center",
          "width":"180px",
          "label": "审核说明",
          "prop": "check_desc"
        },
        {
          "label": "操作",
          "align": "center",
          "fixed": "right",
          "width": "130px",
          "btns": [
            {
              "type": "look",
              "label": "案件详情",
              "toRequest": {
                "url": "/views/pl/assistManage/caseDetail/caseDetail_2/case_detail_main.json",
                "method": "POST",
                "params": {
                  "case_id": "${{current.case_id}}"
                }
              },
              "props": {
                "width": "80%",
                "title": "案件详情"
              }
            }
          ]
        }
      ]
    }
  }
}
