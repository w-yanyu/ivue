{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": "",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "ap1317",
                            "page_id": "/views/pl/batchManage/systemSched/schedule_management.json"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "tran_flow_id": {
                        "control": "text",
                        "label": "流程ID",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            }   
                        ]
                    },
                    "schedule_name": {
                        "control": "text",
                        "label": "批量流程名称",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            },
                            {
                                "max": 30,
                                "message": "最多可以输入 30 个字节"
                            }
                        ]
                    }
                },
                {
                    "scheduling_status": {
                        "control": "remoteSelect",
                        "label": "状态",
                        "width": "130px",
                        "labelField":"drop_list_desc",
                        "valueField":"drop_list_value",
                        "doRequest":{
                            "url": "/SUMP/icmcall/icmRPCCall",
                            "method": "POST",
                            "params":{
                                "servicecode":"ap1071",
                                "drop_list_type": "diaoduzt"
                            }
                        },
                        "format":"label",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            }
                        ]
                    },
                    "schedule_year": {
                        "control": "text",
                        "label": "调度年",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            }
                        ]
                    }
                },
                {
                    "schedule_week": {
                        "control": "text",
                        "label": "调度星期",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            }
                        ]
                    },
                    "schedule_month": {
                        "control": "text",
                        "label": "调度月",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            }
                        ]
                    }
                },
                {
                    "day_of_month": {
                        "control": "text",
                        "label": "每月第几天",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            }
                        ]
                    },
                    "schedule_hour": {
                        "control": "text",
                        "label": "小时",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            }
                        ]
                    }
                },
                {
                    "schedule_minutes": {
                        "control": "text",
                        "label": "分钟",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            }
                        ]
                    },
                    "schedule_seconds": {
                        "control": "text",
                        "label": "秒",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            }
                        ]
                    }
                }
            ]
        }
    }
}