{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "btns": [
                {
                    "type": "submit",
                    "label": "登记",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "ap1321"
                        }
                    }
                },
                {
                    "type": "close",
                    "label": "关闭"
                }
            ],
            "controls": [
                {
                    "batch_flow_id": {
                        "control": "remoteSelect",
                        "label": "流程ID",
                        "width": "180px",
                        "align": "center",
                        "span": "16",
                        "labelField":"chinese_name",
                        "valueField":"tran_flow_id",
                        "doRequest":{
                            "url": "/SUMP/icmcall/icmRPCCall",
                            "method": "POST",
                            "params":{
                                "servicecode":"ap1300"
                            }
                        },
                        "format":"label",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            }
                        ]
                    }
                },
                {
                    "batch_date": {
                        "control": "dateTimePicker",
                        "label": "批量日期",
                        "span": "16",
                        "width": "180px",
                        "align": "center",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            }
                        ]
                    }
                }
            ]
        }
    }
}
