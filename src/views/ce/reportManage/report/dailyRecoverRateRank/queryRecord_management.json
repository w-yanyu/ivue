{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "scope": "formData",
      "controls": [
        {
          "report_date": {
            "control": "dateTimePicker",
            "label": "统计时间",
            "value": "today"
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "export",
          "label": "导出",
          "icon": "iconfont icon-daochu",
          "doRequest": {
            "method": "POST",
            "url": "/SUMP/icmcall/export",
            "params": {
              "servicecode": "ce5602"
            }
          },
          "confirm":{
            "title":"提示信息",
            "msg":"确认导出吗？",
            "confirmButtonText":"确定",
            "cancelButtonText":"取消"
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/call/RPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ce5502"
        }
      },
      "thead": [
        {
          "type": "selection",
          "align": "center",
          "width": "55",
          "fixed": "left",
          "show": {
            "prop": "changkuan",
            "is": [
              "0"
            ]
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "统计日期",
          "prop": "report_date"
        },
        {
          "type": "string",
          "align": "center",
          "label": "机构名称",
          "prop": "branch_name"
        },
        {
          "type": "string",
          "align": "center",
          "label": "催收员",
          "prop": "user_name"
        },
        {
          "type": "string",
          "align": "center",
          "label": "入催时贷款余额",
          "prop": "opening_amt"
        },
        {
          "type": "string",
          "align": "center",
          "label": "当日回收金额",
          "prop": "today_recover_amt"
        },
        {
          "type": "string",
          "align": "center",
          "label": "累计还款金额",
          "prop": "pmt_total_amt"
        },
        {
          "type": "string",
          "align": "center",
          "label": "回收率",
          "prop": "recy_rate"
        },
        {
          "type": "string",
          "align": "center",
          "label": "排名",
          "prop": "ranking"
        },
        {
          "type": "string",
          "align": "center",
          "label": "当日回收利息",
          "prop": "recovered_inst"
        },
        {
          "type": "string",
          "align": "center",
          "label": "当日回收本金(逾期)",
          "prop": "day_pmt_ovd_amt"
        },
        {
          "type": "string",
          "align": "center",
          "label": "当日回收本金(正常)",
          "prop": "day_pmt_nrl_amt"
        },
        {
          "type": "string",
          "align": "center",
          "label": "当日回收表内欠息",
          "prop": "day_pmt_in_amt"
        },
        {
          "type": "string",
          "align": "center",
          "label": "当日回收表外欠息",
          "prop": "day_pmt_out_amt"
        }
      ]
    }
  }
}
