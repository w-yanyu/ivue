{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "scope": "formData",
      "controls": [
        {
          "report_date": {
            "control": "dateTimePicker",
            "label": "统计时间",
            "value": "today"
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "export",
          "label": "导出",
          "icon": "iconfont icon-daochu",
          "doRequest": {
            "method": "POST",
            "url": "/SUMP/icmcall/export",
            "params": {
              "servicecode": "ce5604"
            }
          },
          "confirm":{
            "title":"提示信息",
            "msg":"确认导出吗？",
            "confirmButtonText":"确定",
            "cancelButtonText":"取消"
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/call/RPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ce5504"
        }
      },
      "thead": [
        {
          "type": "string",
          "align": "center",
          "label": "统计日期",
          "prop": "report_date"
        },
        {
          "type": "string",
          "align": "center",
          "label": "机构名称",
          "prop": "branch_name"
        },
        {
          "type": "string",
          "align": "center",
          "label": "当天案件处理数",
          "prop": "dealcase_num"
        },
        {
          "type": "string",
          "align": "center",
          "label": "当天完成案件金额",
          "prop": "finish_amt"
        },
        {
          "type": "string",
          "align": "center",
          "label": "案件总金额",
          "prop": "that_day_total_amt"
        },
        {
          "type": "string",
          "align": "center",
          "label": "案件处理总数",
          "prop": "case_num"
        },
        {
          "type": "string",
          "align": "center",
          "label": "Ptp次数",
          "prop": "ptp_cnt"
        },
        {
          "type": "string",
          "align": "center",
          "label": "Ptp百分比",
          "prop": "ptp_pct"
        },
        {
          "type": "string",
          "align": "center",
          "label": "迁徙率",
          "prop": "migrat_rate"
        }
      ]
    }
  }
}
