{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "lncfno": {
            "span": 8,
            "control": "text",
            "label": "借据号"
          },
          "source": {
            "span": 8,
            "control": "select",
            "label": "停催来源",
            "dict": {
              "format": "label",
              "dictType": "E_STOP_SOURCETYPE",
              "dictKey": [
                "E_STOP_SOURCETYPE"
              ]
            }
          },
          "custna": {
            "span": 8,
            "control": "text",
            "label": "客户姓名"
          }
        },
        {
          "idtfno": {
            "span": 8,
            "control": "text",
            "label": "证件号码"
          },
          "stoptype": {
            "span": 8,
            "control": "select",
            "label": "停催类型",
            "dict": {
              "format": "label",
              "dictType": "E_STOP_TYPE",
              "dictKey": [
                "E_STOP_TYPE"
              ]
            }
          },
          "stopreasion": {
            "span": 8,
            "control": "select",
            "label": "停催原因",
            "dict": {
              "format": "label",
              "dictType": "E_STOP_REASION",
              "dictKey": [
                "E_STOP_REASION"
              ]
            }
          },
          "prodna": {
            "span": 8,
            "control": "text",
            "label": "产品名称"
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": true,
      "scope": "searchTable",
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "RLMSPLTS1039"
        }
      },
      "thead": [
        {
          "label": "借据号",
          "align":"center",
          "prop": "lncfno"
        },
        {
          "label": "客户名称",
          "align":"center",
          "prop": "custna"
        },
        {
          "label": "证件号码",
          "align":"center",
          "prop": "idtfno"
        },
        {
          "label": "产品名称",
          "align":"center",
          "prop": "prodna"
        },
        {
          "label": "停催来源",
          "align":"center",
          "prop": "source",
          "dict": {
            "dictType": "E_STOP_SOURCETYPE",
            "dictKey": [
              "E_STOP_SOURCETYPE"
            ],
            "format": "label"
          }
        },
        {
          "label": "停催类型",
          "align":"center",
          "prop": "stoptype",
          "dict": {
            "dictType": "E_STOP_TYPE",
            "dictKey": [
              "E_STOP_TYPE"
            ],
            "format": "label"
          }
        },
        {
          "label": "停催开始时间",
          "align":"center",
          "prop": "stopstartdate"
        },
        {
          "label": "停催到期日期",
          "align":"center",
          "prop": "stopenddate"
        },
        {
          "label": "停催原因",
          "align":"center",
          "prop": "stopreasion",
          "dict": {
            "dictType": "E_STOP_REASION",
            "dictKey": [
              "E_STOP_REASION"
            ],
            "format": "label"
          }
        },
        {
          "label": "备注",
          "align":"center",
          "prop": "remark"
        }
      ]
    }
  }
}
