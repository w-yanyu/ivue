{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "applid": {
            "span": 8,
            "control": "text",
            "label": "申请编号"
          },
          "lncfno": {
            "span": 8,
            "control": "text",
            "label": "借据号"
          },
          "type": {
            "span": 8,
            "control": "select",
            "label": "业务类型",
            "dict": {
              "format": "label",
              "dictType": "E_BATCH_APPLY_TYPE",
              "dictKey": [
                "E_BATCH_APPLY_TYPE"
              ]
            }
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": true,
      "scope": "searchTable",
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "RLMSPLTS1040"
        }
      },
      "thead": [
        {
          "label": "申请号",
          "align":"center",
          "prop": "applid"
        },
        {
          "label": "借据号",
          "align":"center",
          "prop": "lncfno"
        },
        {
          "label": "停催类型",
          "align":"center",
          "prop": "type",
          "dict": {
            "dictType": "E_BATCH_APPLY_TYPE",
            "dictKey": [
              "E_BATCH_APPLY_TYPE"
            ],
            "format": "label"
          }
        },
        {
          "label": "校验结果",
          "align":"center",
          "prop": "result"
        }
      ]
    }
  }
}
