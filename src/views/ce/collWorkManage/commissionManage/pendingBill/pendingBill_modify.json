{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "doRequest": {
        "url": "/demo/submitData",
        "method": ""
      },
      "controls": [
        {
          "id": {
            "control": "text",
            "label": "结算月份",
            "rules": [
              {
                "required": true,
                "message": "请输入编号"
              }
            ]
          },
          "name": {
            "control": "select",
            "label": "区域中心",
            "rules": [
              {
                "required": true,
                "message": "请输入姓名"
              }
            ]
          }
        },
        {
          "age": {
            "control": "file",
            "label": "修改后账单",
            "type": "image/*",
            "btnLable": "请选择文件",
            "showFileList": true,
            "isDrag": false,
            "multiple": false
          }
        },
        {

          "area": {
            "control": "textarea",
            "label": "备注",
            "width": "100%"
          }
        }
      ]
    }
  }
}
