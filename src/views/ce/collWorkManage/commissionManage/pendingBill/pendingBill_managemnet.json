{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "foldLineNumber": 3,
      "scope": "formData",
      "controls": [
        {
          "B": {
            "control": "timePicker",
            "label": "结束月份",
            "span": 8
          },
          "A": {
            "control": "select",
            "label": "区域中心",
            "span": 8
          },
          "C": {
            "control": "select",
            "label": "催收公司",
            "span": 8
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "import",
          "icon": "el-icon-top-right",
          "target": "dialog",
          "label": "导出",
          "toRequest": {
            "method": "GET",
            "url": "/views/pl/collManage/oaManage/oaAssign/oaAssign_artificial.json"
          }
        },
        {
          "type": "custom",
          "icon": "el-icon-bell",
          "target": "dialog",
          "label": "账单确认",
          "confirm": {
            "title": "账单确认",
            "msg": "是否确定账单无误，提交审批流程？",
            "confirmButtonText": "确定",
            "cancelButtonText": "取消"
          }
        },
        {
          "type": "custom",
          "icon": "el-icon-back",
          "label": "账单修改",
          "toRequest": {
            "url": "/views/pl/collWorkManage/commissionManage/pendingBill/pendingBill_modify.json",
            "method": "POST",
            "params": {}
          },
          "props": {
            "width": "40%"
          }
        }
      ]
    },

    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pl4560"
        }
      },
      "thead": [
        {
          "type":"string",
          "align":"center",
          "width":"100px",
          "label": "区域中心",
          "prop": "branch_id"
        },
        {
          "type":"string",
          "align":"center",
          "label": "受理城市",
          "prop": "case_id"

        },
        {
          "type":"string",
          "align":"center",
          "width":"100px",
          "label": "消费金融账户",
          "prop": "cust_name"
        },
        {
          "type":"string",
          "align":"center",
          "label": "产品额度",
          "prop": "id_no"
        },
        {
          "type":"string",
          "align":"center",
          "width":"100px",
          "label": "逾期阶段",
          "prop": "pre_function"
        },
        {
          "type":"string",
          "align":"center",
          "width":"100px",
          "label": "客户状态",
          "prop": "over_due_day"
        },
        {
          "type": "string",
          "align": "center",
          "width": "100",
          "label": "是否高危",
          "prop": "collect_date"
        },
        {
          "type": "string",
          "align": "center",
          "width": "100",
          "label": "是否结清",
          "prop": "collect_date"
        },
        {
          "type": "string",
          "align": "center",
          "width": "100",
          "label": "是否法诉",
          "prop": "collect_date"
        },
        {
          "type":"string",
          "align":"center",
          "width":"100px",
          "label": "委案公司",
          "prop": "oa_hands"
        },
        {
          "type":"string",
          "align":"center",
          "width":"100px",
          "label": "委案时间",
          "prop": "location_cd"
        },
        {
          "type":"string",
          "align":"center",
          "width":"100px",
          "label": "还款日期",
          "prop": "status_cd"
        },
        {
          "type":"string",
          "align":"center",
          "label": "交易金额",
          "prop": "queue_cd"
        },
        {
          "type":"string",
          "align":"center",
          "width":"100px",
          "label": "交易类型",
          "prop": "location_cd"
        },
        {
          "type":"string",
          "align":"center",
          "width":"100px",
          "label": "结算费率",
          "prop": "status_cd"
        },
        {
          "type":"string",
          "align":"center",
          "label": "结算金额",
          "prop": "queue_cd"
        }
      ]
