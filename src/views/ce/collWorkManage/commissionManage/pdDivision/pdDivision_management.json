{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "foldLineNumber": 3,
      "scope": "formData",
      "controls": [
        {
          "A": {
            "control": "select",
            "label": "区域中心",
            "span": 8
          },
          "B": {
            "control": "select",
            "label": "产品大类",
            "span": 8
          },
          "C": {
            "control": "select",
            "label": "账龄",
            "span": 8
          }
        },
        {
          "D": {
            "control": "text",
            "label": "借据号",
            "span": 8
          },
          "E": {
            "control": "text",
            "label": "客户名称",
            "span": 8
          },
          "F": {
            "control": "text",
            "label": "证件号码",
            "span": 8
          }
        },
        {
          "warnLabel": {
            "label": "说明：账龄可以多选",
            "labelWidth": "800px",
            "span": 8,
            "control": "link"
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "export",
          "icon": "el-icon-top-right",
          "target": "dialog",
          "checkCountMin":1,
          "label": "导出",
          "toRequest": {
            "method": "GET",
            "url": "/views/pl/collManage/oaManage/oaAssign/oaAssign_artificial.json"
          }
        },
        {
          "type": "import",
          "icon": "el-icon-bottom-right",
          "target": "dialog",
          "checkCountMin":1,
          "label": "手工分案导入",
          "toRequest": {
            "method": "GET",
            "url": "/views/pl/collManage/oaManage/oaAssign/oaAssign_auto.json"
          }
        },
        {
          "type": "custom",
          "icon": "el-icon-download",
          "checkCountMin":1,
          "label": "下载分案模板",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "pl4561",
              "case_id": ""
            }
          },
          "confirm": {
            "title": "委外退案提醒",
            "msg": "确定将案件退案，退出委外队列？",
            "confirmButtonText": "确定",
            "cancelButtonText": "取消"
          }
        }
      ]
    },

    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pl4560"
        }
      },
      "thead": [
        {
          "type":"string",
          "align":"center",
          "width":"100px",
          "label": "区域中心",
          "prop": "branch_id"
        },
        {
          "type":"string",
          "align":"center",
          "label": "借据号",
          "prop": "case_id"

        },
        {
          "type":"string",
          "align":"center",
          "width":"100px",
          "label": "客户姓名",
          "prop": "cust_name"
        },
        {
          "type":"string",
          "align":"center",
          "label": "产品大类",
          "prop": "id_no"
        },
        {
          "type":"string",
          "align":"center",
          "width":"100px",
          "label": "首逾标志",
          "prop": "pre_function"
        },
        {
          "type":"string",
          "align":"center",
          "width":"100px",
          "label": "逾期天数",
          "prop": "over_due_day"
        },
        {
          "type":"string",
          "align":"center",
          "width":"100px",
          "label": "账龄",
          "prop": "oa_hands"
        },
        {
          "type":"string",
          "align":"center",
          "width":"100px",
          "label": "现贷余额",
          "prop": "location_cd"
        },
        {
          "type":"string",
          "align":"center",
          "width":"100px",
          "label": "解除逾期金额",
          "prop": "status_cd"
        },
        {
          "type":"string",
          "align":"center",
          "label": "上一委案公司",
          "prop": "queue_cd"
        },
        {
          "type": "string",
          "align": "center",
          "width": "100",
          "label": "是否高危",
          "prop": "collect_date"
        },
        {
          "type": "string",
          "align": "center",
          "width": "100",
          "label": "是否法诉",
          "prop": "collect_date"
        }
      ]
    }
  }
}
