{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "scope": "formData",
      "controls": [
        {
          "branch_org": {
            "control": "remoteSelect",
            "label": "机构",
            "labelField": "branch_name",
            "valueField": "branch_id",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2173"
              }
            },
            "format": "label"
          },
          "cust_name": {
            "control": "text",
            "label": "客户姓名"
          }
        },
        {
          "id_no": {
            "control": "text",
            "label": "证件号码"
          },
          "cust_id": {
            "control": "text",
            "label": "客户号"
          }
        },
        {
          "tel_no": {
            "control": "text",
            "label": "手机号码"
          },
          "daterange": {
            "control": "dateTimePicker",
            "label": "催收时间",
            "type": "daterange",
            "valueFormat": "yyyyMMdd",
            "startPlaceholder": "选择开始时间",
            "endPlaceholder": "选择结束时间",
            "unlinkPanels": true,
            "startField": "start_date",
            "endField": "end_date"
          }
        },
        {
          "action_code": {
            "control": "remoteSelect",
            "label": "是否接通",
            "labelField": "drop_list_desc",
            "valueField": "drop_list_value",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ap1071",
                "drop_list_type": "loan_action"
              }
            },
            "format": "label"
          },
          "sys_type": {
            "control": "remoteSelect",
            "label": "登记类型",
            "labelField": "drop_list_desc",
            "valueField": "drop_list_value",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ap1071",
                "drop_list_type": "sys_type"
              }
            },
            "format": "label"
          }
        },
        {
          "queue_cd": {
            "control": "remoteSelect",
            "label": "队列名称",
            "labelField": "queue_name",
            "valueField": "queue_cd",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "pl0402"
              }
            },
            "format": "label"
          },
          "status_cd": {
            "control": "remoteSelect",
            "label": "案件状态",
            "labelField": "status_name",
            "valueField": "status_cd",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "pl0411"
              }
            },
            "format": "label"
          }
        },
        {
          "function_cd": {
            "control": "remoteSelect",
            "label": "逾期阶段",
            "labelField": "function_name",
            "valueField": "function_cd",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "pl0412"
              }
            },
            "format": "label"
          }
        },
        {
          "ovd_amt_total_1": {
            "control": "inputNumber",
            "label": "欠款金额区间开始值"
          },
          "ovd_amt_total_2": {
            "control": "inputNumber",
            "label": "欠款金额区间结束值"
          }
        },
        {
          "collect_date_1": {
            "control": "inputNumber",
            "label": "入催天数区间开始值"
          },
          "collect_date_2": {
            "control": "inputNumber",
            "label": "入催天数区间结束值"
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "custom",
          "target": "dialog",
          "icon": "el-icon-upload",
          "label": "导入催记",
          "toRequest": {
            "method": "GET",
            "url": "/views/pl/collWorkManage/outCollManage/collRec/collRec_import.json"
          },
          "props": {
            "width": "50%",
            "title": "导入催记"
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pl6627"
        }
      },
      "thead": [
        {
          "type": "selection",
          "align": "center",
          "width": "55",
          "fixed": "left",
          "show": {
            "prop": "changkuan",
            "is": [
              "0"
            ]
          }
        },
        {
          "type": "string",
          "align": "center",
          "width": "100",
          "label": "机构",
          "prop": "branch_org",
          "dict": {
            "dictKey": [
              "branch_org"
            ],
            "format": "label",
            "labelField": "branch_name",
            "valueField": "branch_id",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2173"
              }
            }
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "案件编号",
          "prop": "case_id"
        },
        {
          "type": "string",
          "align": "center",
          "label": "客户姓名",
          "prop": "cust_name"
        },
        {
          "type": "string",
          "align": "center",
          "label": "证件类型",
          "prop": "id_type",
          "dict": {
            "dictKey": [
              "queryCollRec_id_type"
            ],
            "format": "label",
            "labelField": "drop_list_desc",
            "valueField": "drop_list_value",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ap1071",
                "drop_list_type": "CertType"
              }
            }
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "证件号码",
          "prop": "id_no"
        },
        {
          "type": "string",
          "align": "center",
          "label": "催收时间",
          "prop": "coll_time"
        },
        {
          "type": "string",
          "align": "center",
          "width": "120",
          "label": "催收员",
          "prop": "collector",
          "dict": {
            "dictKey": [
              "rec_collector"
            ],
            "format": "label",
            "labelField": "user_name",
            "valueField": "user_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0084"
              }
            }
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "客户关系",
          "prop": "relationship",
          "dict": {
            "dictKey": [
              "collRec_aaa"
            ],
            "format": "label",
            "labelField": "drop_list_desc",
            "valueField": "drop_list_value",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ap1071",
                "drop_list_type": "relationship"
              }
            }
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "联系姓名",
          "prop": "cont_name"
        },
        {
          "type": "string",
          "align": "center",
          "label": "电话号码",
          "prop": "tel_no"
        },
        {
          "type": "string",
          "align": "center",
          "label": "是否接通",
          "prop": "action_code",
          "dict": {
            "dictKey": [
              "queryCollRec_bbb"
            ],
            "format": "label",
            "labelField": "drop_list_desc",
            "valueField": "drop_list_value",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ap1071",
                "drop_list_type": "loan_action"
              }
            }
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "催收结果",
          "prop": "coll_result",
          "dict": {
            "dictKey": [
              "queryCollRec_ccc"
            ],
            "format": "label",
            "labelField": "contact_name",
            "valueField": "contact_cd",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "pl0420"
              }
            }
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "是否有效结果",
          "prop": "valid_result",
          "dict": {
            "dictType": "MS_E_YESORNO",
            "dictKey": [
              "MS_E_YESORNO"
            ],
            "format": "label"
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "登记类型",
          "prop": "sys_type",
          "dict":{
            "dictKey": [
              "queryCollRec_ddd"
            ],
            "format": "label",
            "labelField": "drop_list_desc",
            "valueField": "drop_list_value",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode":"ap1071",
                "drop_list_type": "sys_type"
              }
            }
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "承诺金额",
          "prop": "prom_amt"
        },
        {
          "type": "string",
          "align": "center",
          "label": "承诺日期",
          "prop": "prom_date"
        },
        {
          "type": "string",
          "align": "center",
          "label": "备注",
          "prop": "remark"
        }
      ]
    }
  }
}
