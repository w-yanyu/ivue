{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "scope": "formData",
            "controls": [
                {
                    "fee_cd": {
                        "control": "text",
                        "label": "费率代码"
                    },
                    "fee_type": {
                        "control": "remoteSelect",
                        "label": "费率类型",
                        "labelField": "drop_list_desc",
                        "valueField": "drop_list_value",
                        "doRequest": {
                            "url": "/SUMP/icmcall/icmRPCCall",
                            "method": "POST",
                            "params": {
                                "servicecode": "ap1071",
                                "drop_list_type": "fee_type_pl"
                            }
                        },
                        "format": "label"
                    },
                    "handstr": {
                      "control": "inputNumber",
                      "label": "手别",
                      "min": 0,
                      "max": 10
                    }
                }
            ]
        },
        "toolbar": {
            "btns": [
                {
                    "type": "add",
                    "icon": "el-icon-circle-plus",
                    "target": "dialog",
                    "label": "新增",
                    "toRequest": {
                        "method": "GET",
                        "url":"/views/pl/collParamManage/oaPayRuleManage/head/fee_add.json"
                    },
                    "props": {
                        "title": "新增佣金参数"
                    }
                }
            ]
        },
        "datagrid": {
            "isInitSearch": false,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "pl0084"
                }
            },
            "thead": [
                {
                  "type":"string",
                  "align":"center",
                  "width":"180px",
                  "label": "业绩归属",
                  "prop": "fee_cd2"
                },
                {
                    "type":"string",
                    "align":"center",
                    "width":"180px",
                    "label": "产品大类",
                    "prop": "fee_cd"
                },
                {
                    "type":"string",
                    "align":"center",
                    "width":"180px",
                    "label": "账龄",
                    "prop": "fee_type",
                    "dict":{
                        "dictKey": [
                            "fee_aaa"
                        ],
                        "format": "label",
                        "labelField": "drop_list_desc",
                        "valueField": "drop_list_value",
                        "doRequest": {
                            "url": "/SUMP/icmcall/icmRPCCall",
                            "method": "POST",
                            "params": {
                                "servicecode":"ap1071",
                                "drop_list_type": "fee_type_pl"
                            }
                        }
                    }
                },
                {
                    "type":"string",
                    "align":"center",
                    "width":"180px",
                    "label": "最小回收率(%)",
                    "prop": "principal_rate"
                },
                {
                    "type":"string",
                    "align":"center",
                    "width":"180px",
                    "label": "最大回收率(%)",
                    "prop": "lev_rate"
                },
                {
                    "type":"string",
                    "align":"center",
                    "width":"180px",
                    "label": "佣金上限",
                    "prop": "lev_rate"
                },
                {
                    "label": "操作",
                    "align":"center",
                    "fixed": "right",
                    "width": "150%",
                    "btns": [
                        {
                            "type":"look",
                            "label":"详情",
                            "toRequest":{
                                "url":"/views/pl/collParamManage/oaPayRuleManage/head/fee_info.json",
                                "method":"POST"
                            },
                            "props":{
                                "title":"佣金参数详情"
                            }
                        },
                        {
                            "type": "edit",
                            "label": "维护",
                            "toRequest": {
                                "url":"/views/pl/collParamManage/oaPayRuleManage/head/fee_modify.json",
                                "method": "POST"
                            },
                            "props": {
                                "title": "佣金参数维护"
                            }
                        },
                        {
                            "type": "delete",
                            "label": "删除",
                            "doRequest": {
                                "url": "/SUMP/icmcall/icmRPCCall",
                                "method": "POST",
                                "params": {
                                    "servicecode": "pl0081"
                                }
                            }
                        }
                    ]
                }
            ]
        }
    }
}
