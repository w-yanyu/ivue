{
    "module": "v-realonlyForm-btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "controls": [
                {
                    "fee_type": {
                      "control": "remoteSelect",
                      "label": "费率类型",
                      "disabled":true,
                      "default":"OA",
                      "width": "130px",
                      "labelField": "drop_list_desc",
                      "valueField": "drop_list_value",
                      "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                          "servicecode": "ap1071",
                          "drop_list_type": "fee_type_pl"
                        }
                      },
                      "format": "label"
                    },
                    "fee_cd": {
                        "control": "string",
                        "disabled":true,
                        "label": "费率代码",
                        "width": "130px"
                    }
                },
                {
                    "rate_type": {
                      "control": "remoteSelect",
                      "label": "费率类别",
                      "disabled":true,
                      "default":"A",
                      "width": "130px",
                      "labelField": "drop_list_desc",
                      "valueField": "drop_list_value",
                      "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                          "servicecode": "ap1071",
                          "drop_list_type": "RATE_TYPE_LOAN"
                        }
                      },
                      "format": "label"
                    },
                    "stage": {
                      "control": "remoteSelect",
                      "label": "阶段释义",
                      "disabled":true,
                      "default":"A3",
                      "width": "130px",
                      "labelField": "drop_list_desc",
                      "valueField": "drop_list_value",
                      "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                          "servicecode": "ap1071",
                          "drop_list_type": "STAGE_LOAN"
                        }
                      },
                      "format": "label"
                    }
                },
                {
                    "principal_rate": {
                        "control": "string",
                        "label": "本金费率(%)",
                        "width": "130px"
                    },
                    "lev_rate": {
                        "control": "string",
                        "label": "利费费率(%)",
                        "width": "130px"
                    }
                },
                {
                "hands": {
                  "width": "130px",
                  "control": "string",
                  "label": "手别"
                }
              }
            ]
        }
    }
}
