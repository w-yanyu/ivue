{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "applid": "${{params.applid}}",
          "servicecode": "RLMSPLTS1210_pl1210"
        }
      },
      "controls": [
        {
          "old_ratio_min": {
            "label": "调整前分配下限(%)",
            "control": "inputNumber",
            "controlsPosition": "right",
            "disabled": true
          },
          "old_ratio_max": {
            "label": "调整前分配上限(%)",
            "control": "inputNumber",
            "controlsPosition": "right",
            "disabled": true
          }
        },
        {
          "new_ratio_min": {
            "label": "最新分配下限(%)",
            "control": "inputNumber",
            "max": 0,
            "max": 100,
            "controlsPosition": "right",
            "rules": [
              {
                "required": true,
                "message": "请输入下限比率"
              }
            ]
          },
          "new_ratio_max": {
            "label": "最新分配上限(%)",
            "control": "inputNumber",
            "min": 100,
            "controlsPosition": "right",
            "rules": [
              {
                "required": true,
                "message": "请输入上限比率"
              }
            ]
          }
        }
      ],
      "btns": [
        {
          "type": "submit",
          "label": "保存",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "applid": "${{params.applid}}",
              "servicecode": "RLMSPLTS1161_pl1161"
            }
          }
        }
      ]
    }
  }
}
