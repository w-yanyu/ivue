{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "applid": "${{params.applid}}",
          "servicecode": "RLMSPLTS1209_pl1209"
        }
      },
      "pagination":{
        "show":false
      },
      "thead": [
        {
          "label": "业绩归属",
          "prop": "performance_attr",
          "dict": {
            "dictType": "E_STRATEGY_TYPE",
            "dictKey": [
              "E_STRATEGY_TYPE"
            ],
            "format": "label"
          }
        },
        {
          "label": "产品大类",
          "prop": "prod_type",
          "dict": {
            "dictType": "E_PRODTYPE",
            "dictKey": [
              "E_PRODTYPE"
            ],
            "format": "label"
          }
        },
        {
          "label": "账龄",
          "align": "center",
          "prop": "acct_duration"
        },

        {
          "label": "最小回收率%(不包含)",
          "align": "center",
          "prop": "min_recovery_rate"
        },
        {
          "label": "最大回收率%(包含)",
          "align": "center",
          "prop": "max_recovery_rate"
        }
      ,
        {
          "label": "佣金上限%",
          "align": "center",
          "prop": "comm_rate"
        }
      ]
    }
  }
}
