{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "#initRequest": {
        "url": "/views/pl/collParamManage/outsourcerManger/outsourcerTable/outsourcerConfirm/outsourcerInfo_data.json",
        "method": "get"
      },
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "#doRequest": {
            "url": "/pte/json/v_cardList4/add.json",
            "method": "get",
            "params": {}
          }
        },
        {
          "type": "close",
          "label": "取消",
          "icon": "",
          "#doRequest": {
            "url": "/pte/json/v_cardList4/add.json",
            "method": "get",
            "params": {}
          }
        }
      ],
      "controls": [
        {
          "cgpjb": {
            "control": "dynamic",
            "isShowChildForm": true,
            "span": 10,
            "toRequest": {
              "url": "@SingleUpload",
              "method": "get",
              "params": {
                "path":"collRec",
                "uploadType":"sftp",
                "formKey": "file_path",
                "formName": "file_name",
                "formLabel":"采购评审项目后评价表",
                "required": "true"
              }
            }
          },
          "wbpjb": {
            "control": "dynamic",
            "isShowChildForm": true,
            "span": 8,
            "width": "180px",
            "toRequest": {
              "url": "@SingleUpload",
              "method": "get",
              "params": {
                "path":"collRec",
                "uploadType":"sftp",
                "formKey": "file_path",
                "formName": "file_name",
                "formLabel":"外包项目后评价表",
                "required": "true"
              }
            }
          }
        },
        {
          "pjnf": {
            "control": "select",
            "label": "评价年份",
            "default": "0",
            "span": 7,
            "required": true,
            "options": [
              {
                "label": "2020",
                "value": "0"
              },
              {
                "label": "2021",
                "value": "1"
              },
              {
                "label": "2022",
                "value": "2"
              },
              {
                "label": "2023",
                "value": "3"
              }
            ]

          }
        },

        {
          "description": {
            "control": "textarea",
            "label": "备注",
            "span": 16,
            "width": "130px",
            "#placeholder": "",
            "rows": 3
          }
        }
      ]
    }
  }
}
