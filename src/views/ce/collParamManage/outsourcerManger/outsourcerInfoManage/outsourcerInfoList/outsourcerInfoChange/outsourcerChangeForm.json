{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "#initRequest": {
        "url": "/views/pl/collParamManage/outsourcerManger/outsourcerTable/outsourcerConfirm/outsourcerInfo_data.json",
        "method": "get"
      },
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "#doRequest": {
            "url": "/pte/json/v_cardList4/add.json",
            "method": "get",
            "params": {}
          }
        }
      ],
      "controls": [
        {
          "gsname": {
            "control": "string",
            "label": "公司名称",
            "default": "北京云翳信息科技",
            "span": 8,
            "width": "180px"
          },
          "bggsname": {
            "control": "text",
            "span": 12,
            "label": "变更后公司名称",
            "width": "180px"
          }
        },
        {
          "shxydm": {
            "control": "string",
            "label": "社会统一信用代码",
            "default": "300348",
            "span": 8,
            "width": "180px"
          },
          "bggsname": {
            "control": "text",
            "span": 12,
            "label": "变更后公司名称",
            "width": "180px"
          }
        },
        {
          "zczb": {
            "control": "string",
            "span": 8,
            "label": "注册资本",
            "default": "500W",
            "width": "180px"
          },
          "bgzczb": {
            "control": "text",
            "span": 12,
            "label": "变更后注册资本",
            "width": "180px"
          }
        },
        {
          "bgdz": {
            "control": "string",
            "span": 8,
            "label": "办公地址",
            "default": "上海市浦新区长泰广场",
            "width": "180px"
          },
          "bgbgdz": {
            "control": "text",
            "span": 12,
            "label": "变更后办公地址",
            "width": "180px"
          }
        },
        {
          "jyzt": {
            "control": "string",
            "span": 8,
            "label": "经营状态",
            "default": "营业",
            "width": "180px"
          },
          "bgjyzt": {
            "control": "text",
            "span": 12,
            "label": "变更后经营状态",
            "width": "180px"
          }
        },
        {
          "jyfw": {
            "control": "string",
            "span": 8,
            "label": "经营范围",
            "default": "软电话、信息科技",
            "width": "180px"
          },
          "bgjjyfw": {
            "control": "text",
            "span": 12,
            "label": "变更后经营范围",
            "width": "180px"
          }
        },
        {
          "frdb": {
            "control": "string",
            "span": 8,
            "label": "法人代表",
            "default": "张三",
            "width": "180px"
          },
          "bgfrdb": {
            "control": "text",
            "span": 12,
            "label": "变更后法人代表",
            "width": "180px"
          }
        },
        {
          "gdxx": {
            "control": "string",
            "span": 8,
            "label": "股东信息",
            "default": "张三大股东",
            "width": "180px"
          },
          "bggdxx": {
            "control": "text",
            "span": 12,
            "label": "变更后股东信息",
            "width": "180px"
          }
        },
        {
          "bghyyzz": {
            "control": "dynamic",
            "span": 8,
            "width": "100px",
            "isShowChildForm": true,
            "toRequest": {
              "url": "@SingleUpload",
              "method": "get",
              "params": {
                "path":"collRec",
                "uploadType":"sftp",
                "formKey": "file_path",
                "formName": "file_name",
                "formLabel":"变更后营业执照",
                "required": true
              }
            }
          },
          "bgtzs": {
            "control": "dynamic",
            "isShowChildForm": true,
            "span": 12,
            "width": "100px",
            "toRequest": {
              "url": "@SingleUpload",
              "method": "get",
              "params": {
                "path":"collRec",
                "uploadType":"sftp",
                "formKey": "file_path",
                "formName": "file_name",
                "formLabel":"变更通知书",
                "required": true
              }
            }
          }
        },
        {
          "description": {
            "control": "textarea",
            "label": "备注",
            "span": 16,
            "width": "150px",
            "#placeholder": "",
            "rows": 3
          }
        }
      ]
    }
  }
}
