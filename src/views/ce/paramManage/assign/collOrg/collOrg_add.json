{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "doRequest": {
            "url": "/SUMP/call/RPCCall",
            "method": "POST",
            "params": {
              "servicecode": "ce0040",
              "page_id": "/views/ce/paramManage/assign/collOrg/collOrg_add.json",
              "coll_kind":"02"
            }
          }
        }
      ],
      "controls": [
        {
          "coll_org_no": {
            "control": "text",
            "label": "催收机构编号",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入"
              },
              {
                "max": 20,
                "message": "最多可以输入 20 个字节"
              }
            ]
          },
          "coll_org_name": {
            "control": "text",
            "label": "催收机构名称",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入"
              },
              {
                "max": 30,
                "message": "最多可以输入 30 个字节"
              }
            ]
          }
        },
        {
          "coll_org_type": {
            "control": "select",
            "label": "催收机构类型",
            "width": "130px",
            "dict": {
              "dictType": "CE_COLL_ORG_TYPE",
              "dictKey": [
                "CE_COLL_ORG_TYPE"
              ],
              "format": "value-label"
            },
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          },

          "post_cd": {
            "control": "text",
            "label": "邮编",
            "width": "130px",
            "rules": [
              {
                "pattern": "^[1-9]\\d{5}$",
                "message":"邮编格式错误"
              }
            ]
          }
        },
        {
          "address": {
            "control": "text",
            "label": "地址",
            "width": "130px"
          },
          "tel_no": {
            "control": "text",
            "label": "电话号码",
            "width": "130px",
            "rules": [
              {
                "max": 30,
                "message": "最多可以输入 30 个字节"
              },
              {
                "pattern": "^([1]\\d{10}|([\\(（]?0[0-9]{2,3}[）\\)]?[-]?)?([2-9][0-9]{6,7})+(\\-[0-9]{1,4})?)$",
                "message": "电话号码格式错误"
              }
            ]
          }
        },
        {
          "fax": {
            "control": "text",
            "label": "传真",
            "width": "130px",
            "rules": [
              {
                "max": 20,
                "message": "最多可以输入 20 个字节"
              },
              {
                "pattern": "^[+]{0,1}(\\d){1,3}[ ]?([-]?((\\d)|[ ]){1,12})+$",
                "message": "传真格式错误"
              }
            ]
          },
          "linkman": {
            "control": "text",
            "label": "联系人",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          }
        },
        {
          "cntctpers_tel": {
            "control": "text",
            "label": "联系人电话",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入"
              },
              {
                "max": 11,
                "message": "最多可以输入 11 个字节"
              },
              {
                "pattern": "^1([358][0-9]|4[579]|66|7[0135678]|9[89])[0-9]{8}$",
                "message": "联系人电话格式错误"
              }
            ]
          },
          "is_valid": {
            "control": "select",
            "label": "是否启用",
            "default":"1",
            "width": "130px",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "value-label"
            }
          }
        }
      ]
    }
  }
}
