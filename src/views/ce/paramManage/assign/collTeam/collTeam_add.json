{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "doRequest": {
                        "url": "/SUMP/call/RPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "ce0181"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "coll_group": {
                        "control": "text",
                        "label": "催收组编码",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            },
                            {
                                "max": 20,
                                "message": "最多可以输入 20 个字节"
                            }
                        ]
                    },
                    "coll_team_name": {
                        "control": "text",
                        "label": "催收组名称",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            },
                            {
                                "max": 30,
                                "message": "最多可以输入 30 个字节"
                            }
                        ]
                    }
                },
                {
                    "parent_coll_team": {
                        "control": "text",
                        "label": "上级催收组",
                        "width": "130px",
                        "disabled": true,
                        "value": "${{params.props.coll_group}}"
                    },
                    "sort": {
                        "control": "text",
                        "label": "催收组序号",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            },
                            {
                                "max": 30,
                                "message": "最多可以输入 30 个字节"
                            }
                        ]
                    }
                }
            ]
        }
    }
}
