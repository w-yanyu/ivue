{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "scope":"formData",
            "initRequest": "${{params}}",
            "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "ce0187"
                }
            },
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": "el-icon-tickets"
                }
            ],
            "controls": [
                {
                    "collector": {
                        "control": "text",
                        "label": "催收员编码",
                        "disabled": true,
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            },
                            {
                                "max": 20,
                                "message": "最多可以输入 20 个字节"
                            }
                        ]
                    },
                    "collector_name": {
                        "control": "text",
                        "label": "催收员姓名",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            },
                            {
                                "max": 30,
                                "message": "最多可以输入 30 个字节"
                            }
                        ]
                    }
                },
                {
                    "coll_group": {
                        "control": "text",
                        "label": "所属催收组",
                        "width": "130px",
                        "disabled": true,
                        "value": "${{params.props.coll_group}}"
                    },
                    "coll_post": {
                        "control": "select",
                        "default":"1",
                        "label": "催收岗位",
                        "width": "130px",
                        "dict": {
                            "dictType": "CE_COLL_POST",
                            "dictKey": [
                                "CE_COLL_POST"
                            ],
                            "format": "value-label"
                        },
                        "rules": [
                            {
                                "required":true,
                                "message": "请输入"
                            }
                        ]
                    }
                },
                {
                    "work_no": {
                        "control": "text",
                        "label": "坐席工号",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            }
                        ]
                    },
                    "work_pass": {
                        "control": "text",
                        "label": "坐席密码",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            },
                            {
                                "max": 11,
                                "message": "最多可以输入 11 个字节"
                            }
                        ]
                    }
                },
                {
                    "extens_num": {
                        "control": "text",
                        "label": "分机号",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            },
                            {
                              "pattern": "^1[3-9]\\d{9}|0\\d{2,3}-\\d{7,8}(-\\d{1,6})?$",
                              "message": "分机号格式错误"
                            }
                        ]
                    }
                }
            ]
        }
    }
}
