{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "doRequest": {
            "url": "/SUMP/call/RPCCall",
            "method": "POST",
            "params": {
              "servicecode": "ce0060",
              "page_id": "/views/ce/paramManage/assign/scheme/scheme_add.json",
              "coll_kind":"02"
            }
          }
        }
      ],
      "controls": [
        {
          "allocate_proposal_no": {
            "control": "text",
            "label": "分配方案编号",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入"
              },
              {
                "max": 10,
                "message": "最多可以输入 10 个字节"
              }
            ]
          },
          "scheme_name": {
            "control": "text",
            "label": "分配方案名称",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入"
              },
              {
                "max": 30,
                "message": "最多可以输入 30 个字节"
              }
            ]
          }
        },
        {
          "scheme_type": {
            "control": "select",
            "label": "分配方案类型",
            "default": "TC01",
            "width": "130px",
            "dict": {
              "dictType": "CE_COLL_TYPE",
              "dictKey": [
                "CE_COLL_TYPE"
              ],
              "format": "value-label"
            },
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          },
          "scheme_pattern": {
            "control": "select",
            "label": "分配方式",
            "default": "A",
            "width": "130px",
            "dict": {
              "dictType": "CE_SCHEME_PATTERN",
              "dictKey": [
                "CE_SCHEME_PATTERN"
              ],
              "format": "value-label"
            },
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          }
        },
        {
          "priority": {
            "control": "inputNumber",
            "label": "优先级",
            "width": "130px",
            "min": 1,
            "rules": [
              {
                "required": true,
                "message": "请输入"
              },
              {
                "expression": "'${{form.priority}}'.length < 20",
                "message": "最多可以输入 19 个字节"
              }
            ]
          },
          "is_valid": {
            "control": "select",
            "label": "是否启用",
            "default": "1",
            "width": "130px",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "value-label"
            },
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          }
        }
      ]
    }
  }
}
