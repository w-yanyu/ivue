{
    "module": "v-realonlyForm-btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "controls": [
                {
                    "template_type": {
                        "control": "string",
                        "disabled":true,
                        "label": "模板类型",
                        "default":"EML",
                        "inputWidth": "200px",
                        "dict": {
                          "dictType": "CE_TEMPLATE_TYPE",
                          "dictKey": [
                            "CE_TEMPLATE_TYPE"
                          ],
                          "format": "value-label"
                        }
                    },
                    "template_code": {
                        "control": "string",
                        "disabled":true,
                        "label": "模板编号",
                        "inputWidth": "200px"
                    }
                },
                {
                    "template_name": {
                        "control": "string",
                        "label": "模板名称",
                        "inputWidth": "200px"
                    },
                    "template_desc": {
                        "control": "string",
                        "label": "模板描述",
                        "inputWidth": "200px"
                    }
                },
                {
                    "send_type": {
                        "control": "string",
                        "label": "发送方式",
                        "inputWidth": "200px",
                        "default":"TIME",
                        "dict": {
                          "dictType": "CE_SEND_TYPE",
                          "dictKey": [
                            "CE_SEND_TYPE"
                          ],
                          "format": "value-label"
                        }
                    }

                },
                {
                    "template_content": {
                        "control": "mce",
                        "disabled":true,
                        "label": "模板内容",
                        "labelWidth": "8",
                        "span":"20",
                        "autosize":"{ minRows: 2, maxRows: 4}",
                        "rules": [
                            {
                                "required": true
                            }
                        ]
                    }
                }
            ]
        }
    }
}
