{
    "module": "v-realonlyForm-btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "controls": [
                {
                    "template_type": {
                        "control": "string",
                        "disabled":true,
                        "label": "模板类型",
                        "default":"SMS",
                        "width": "130px",
                        "dict": {
                          "dictType": "CE_TEMPLATE_TYPE",
                          "dictKey": [
                            "CE_TEMPLATE_TYPE"
                          ],
                          "format": "value-label"
                        }
                      },
                      "template_code": {
                          "control": "string",
                          "disabled":true,
                          "label": "模板编号",
                          "width": "130px"
                      }
                },
                {
                    "template_name": {
                        "control": "string",
                        "label": "模板名称",
                        "width": "130px"
                    },
                    "template_desc": {
                        "control": "string",
                        "label": "模板描述",
                        "width": "130px"
                    }
                },
                {
                    "send_type": {
                        "control": "string",
                        "label": "发送方式",
                        "width": "130px",
                        "default":"TIME",
                        "dict": {
                          "dictType": "CE_SEND_TYPE",
                          "dictKey": [
                            "CE_SEND_TYPE"
                          ],
                          "format": "value-label"
                        }
                    }
                },
                {
                    "template_content": {
                        "control": "string",
                        "label": "模板内容",
                        "width": "130px",
                        "span":"24",
                        "autosize":"{ minRows: 2, maxRows: 4}"
                    }
                }
            ]
        }
    }
}
