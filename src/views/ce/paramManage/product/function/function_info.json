{
    "module": "v-realonlyForm-btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "controls": [
                {
                    "overdue_stage_code": {
                        "control": "string",
                        "label": "逾期阶段代码",
                        "width": "130px"

                    },
                    "ovd_period_name": {
                        "control": "string",
                        "label": "逾期阶段名称",
                        "width": "130px"
                    }
                },
                {
                    "overdue_period_type": {
                        "control": "string",
                        "label": "逾期阶段类型",
                        "width": "130px",
                        "dict": {
                          "dictType": "CE_OVER_DUE",
                          "dictKey": [
                            "CE_OVER_DUE"
                          ],
                          "format": "value-label"
                        }
                    },
                    "overdue_period_desc": {
                        "control": "string",
                        "label": "逾期阶段描述",
                        "width": "130px"
                    }
                },
                {
                    "pmt_percent": {
                        "control": "string",
                        "label": "承诺付款比例(%)",
                        "width": "130px"
                    },
                    "max_prom_days": {
                        "control": "string",
                        "label": "最大复核天数",
                        "width": "130px"
                    }
                },
                {
                    "max_hold_days": {
                        "control": "string",
                        "label": "最大滞留天数",
                        "width": "130px"
                    },
                    "is_default": {
                        "control": "string",
                        "label": "是否默认",
                        "width": "130px",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "value-label"
                        }
                    }
                }
            ]
        }
    }
}
