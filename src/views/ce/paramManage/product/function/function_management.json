{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "scope": "formData",
            "controls": [
                {
                    "overdue_stage_code": {
                        "control": "text",
                        "label": "逾期阶段代码"
                    },
                    "ovd_period_name": {
                        "control": "text",
                        "label": "逾期阶段名称"
                    }
                },
                {
                    "overdue_period_type": {
                        "control": "select",
                        "label": "逾期阶段类型",
                        "dict": {
                          "dictType": "CE_OVER_DUE",
                          "dictKey": [
                            "CE_OVER_DUE"
                          ],
                          "format": "value-label"
                        }
                    }
                }
            ]
        },
        "toolbar": {
            "btns": [
                {
                    "type": "add",
                    "icon": "el-icon-circle-plus",
                    "target": "dialog",
                    "label": "新增",
                    "toRequest": {
                        "method": "GET",
                        "url": "/views/ce/paramManage/product/function/function_add.json"
                    },
                    "props": {
                        "title": "新增逾期阶段定义"
                    }
                }
            ]
        },
        "datagrid": {
            "isInitSearch": false,
            "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "ce0014"
                }
            },
            "thead": [
                {
                    "type":"string",
                    "align":"center",
                    "label": "逾期阶段代码",
                    "prop": "overdue_stage_code"

                },
                {
                    "type":"string",
                    "align":"center",
                    "label": "逾期阶段名称",
                    "prop": "ovd_period_name"
                },
                {
                    "type":"string",
                    "align":"center",
                    "label": "逾期阶段类型",
                    "prop": "overdue_period_type",
                    "dict": {
                      "dictType": "CE_OVER_DUE",
                      "dictKey": [
                        "CE_OVER_DUE"
                      ],
                      "format": "value-label"
                    }
                },
                {
                    "type":"string",
                    "align":"center",
                    "label": "逾期阶段描述",
                    "prop": "overdue_period_desc"
                },
                {
                    "type":"string",
                    "align":"center",
                    "label": "承诺付款比例(%)",
                    "prop": "pmt_percent"
                },
                {
                    "type":"string",
                    "align":"center",
                    "label": "最大复核天数",
                    "prop": "max_prom_days"
                },

                {
                    "type":"string",
                    "align":"center",
                    "label": "最大滞留天数",
                    "prop": "max_hold_days"
                },
                {
                    "type":"string",
                    "label": "是否默认",
                    "align":"center",
                    "prop": "is_default",
                    "dict": {
                        "dictType": "MS_E_YESORNO",
                        "dictKey": [
                            "MS_E_YESORNO"
                        ],
                        "format": "value-label"
                    }
                },
                {
                    "label": "操作",
                    "align":"center",
                    "fixed": "right",
                    "width": "150%",
                    "btns": [
                        {
                            "type": "look",
                            "label": "详情",
                            "toRequest": {
                                "url": "/views/ce/paramManage/product/function/function_info.json",
                                "method": "POST"
                            },
                            "props": {
                                "title": "逾期阶段定义详情"
                            }
                        },
                        {
                            "type": "edit",
                            "label": "维护",
                            "toRequest": {
                                "url": "/views/ce/paramManage/product/function/function_modify.json",
                                "method": "POST"
                            },
                            "props": {
                                "title": "逾期阶段定义维护"
                            }
                        },
                        {
                            "type": "delete",
                            "label": "删除",
                            "doRequest": {
                                "url": "/SUMP/call/RPCCall",
                                "method": "POST",
                                "params": {
                                    "servicecode": "ce0011",
                                    "coll_kind":"02"
                                }
                            }
                        }
                    ]
                }
            ]
        }
    }
}
