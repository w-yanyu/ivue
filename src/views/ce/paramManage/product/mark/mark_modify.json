{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "scope":"formData",
            "initRequest": "${{params}}",
            "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "ce0173"
                }
            },
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": "el-icon-tickets"
                }
            ],
            "controls": [
                {
                    "case_tag_code": {
                        "control": "text",
                        "disabled":true,
                        "label": "案件标签代码",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            },
                            {
                                "min": 1,
                                "max": 10,
                                "message": "请输入长度在 1 到 10 之间的字符串"
                            }
                        ]
                    },
                    "mark_name": {
                        "control": "text",
                        "label": "案件标签名称",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            },
                            {
                                "max": 30,
                                "message": "最多可以输入 30 个字节"
                            }
                        ]
                    }
                },
                {
                    "mark_desc": {
                        "control": "text",
                        "label": "案件标签描述",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            },
                            {
                                "max": 100,
                                "message": "最多可以输入 100 个字节"
                            }
                        ]
                    }
                }
            ]
        }
    }
}
