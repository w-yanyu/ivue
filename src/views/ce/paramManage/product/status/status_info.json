{
    "module": "v-realonlyForm-btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "controls": [
                {
                    "casestate_code": {
                        "control": "string",
                        "disabled":true,
                        "label": "案件状态代码",
                        "width": "130px"

                    },
                    "status_name": {
                        "control": "string",
                        "label": "案件状态名称",
                        "width": "130px"

                    }
                },
                {
                    "case_status_desc": {
                        "control": "string",
                        "label": "案件状态描述",
                        "width": "130px"
                    },
                    "case_status_type": {
                        "control": "string",
                        "label": "案件状态类型",
                        "width": "130px",
                        "dict": {
                          "dictType": "CE_CASE_STATUS_TYPE",
                          "dictKey": [
                            "CE_CASE_STATUS_TYPE"
                          ],
                          "format": "value-label"
                        }
                    }
                },
                {
                    "is_default": {
                        "control": "string",
                        "label": "是否默认",
                        "width": "130px",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "value-label"
                        }
                    }
                }
            ]
        }
    }
}
