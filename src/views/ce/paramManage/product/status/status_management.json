{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "scope": "formData",
            "controls": [
                {
                    "casestate_code": {
                        "control": "text",
                        "label": "案件状态代码"
                    },
                    "status_name": {
                        "control": "text",
                        "label": "案件状态名称"
                    }
                },
                {
                    "case_status_desc": {
                        "control": "text",
                        "label": "案件状态描述"
                    },
                    "case_status_type": {
                        "control": "select",
                        "label": "案件状态类型",
                        "dict": {
                          "dictType": "CE_CASE_STATUS_TYPE",
                          "dictKey": [
                            "CE_CASE_STATUS_TYPE"
                          ],
                          "format": "value-label"
                        }
                    }
                }
            ]
        },
        "toolbar": {
            "btns": [
                {
                    "type": "add",
                    "icon": "el-icon-circle-plus",
                    "target": "dialog",
                    "label": "新增",
                    "toRequest": {
                        "method": "GET",
                        "url": "/views/ce/paramManage/product/status/status_add.json"
                    },
                    "props": {
                        "title": "新增案件状态定义"
                    }
                }
            ]
        },
        "datagrid": {
            "isInitSearch": false,
            "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "ce0034"
                }
            },
            "thead": [
                {
                    "type":"string",
                    "align":"center",
                    "label": "案件状态代码",
                    "prop": "casestate_code"

                },
                {
                    "type":"string",
                    "align":"center",
                    "label": "案件状态名称",
                    "prop": "status_name"
                },
                {
                    "type":"string",
                    "align":"center",
                    "label": "案件状态描述",
                    "prop": "case_status_desc"
                },
                {
                    "type":"string",
                    "align":"center",
                    "label": "案件状态类型",
                    "prop": "case_status_type",
                    "dict": {
                      "dictType": "CE_CASE_STATUS_TYPE",
                      "dictKey": [
                        "CE_CASE_STATUS_TYPE"
                      ],
                      "format": "value-label"
                    }
                },
                {
                    "label": "操作",
                    "align":"center",
                    "fixed": "right",
                    "btns": [
                        {
                            "type":"look",
                            "label":"详情",
                            "toRequest":{
                                "url":"/views/ce/paramManage/product/status/status_info.json",
                                "method":"POST"
                            },
                            "props":{
                                "title":"案件状态定义详情"
                            }
                        },
                        {
                            "type": "edit",
                            "label": "维护",
                            "toRequest": {
                                "url": "/views/ce/paramManage/product/status/status_modify.json",
                                "method": "POST"
                            },
                            "props": {
                                "title": "案件状态定义维护"
                            }
                        },
                        {
                            "type": "delete",
                            "label": "删除",
                            "doRequest": {
                                "url": "/SUMP/call/RPCCall",
                                "method": "POST",
                                "params": {
                                    "servicecode": "ce0031",
                                    "coll_kind":"02"
                                }
                            }
                        }
                    ]
                }
            ]
        }
    }
}
