{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "doRequest": {
                        "url": "/SUMP/call/RPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "ce0030",
                            "page_id": "/views/ce/paramManage/product/status/status_add.json",
                            "coll_kind":"02"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "casestate_code": {
                        "control": "text",
                        "label": "案件状态代码",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            },
                            {
                                "max": 10,
                                "message": "最多可以输入 10 个字节"
                            }
                        ]
                    },
                    "status_name": {
                        "control": "text",
                        "label": "案件状态名称",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            },
                            {
                                "max": 30,
                                "message": "最多可以输入 30 个字节"
                            }
                        ]
                    }
                },
                {
                    "case_status_desc": {
                        "control": "text",
                        "label": "案件状态描述",
                        "width": "130px",
                        "rules": [
                            {
                                "required":true,
                                "message":"请输入"
                            },
                            {
                                "max": 100,
                                "message": "最多可以输入 100 个字节"
                            }
                        ]
                    },
                    "case_status_type": {
                        "control": "select",
                        "label": "案件状态类型",
                        "default":"NW",
                        "width": "130px",
                        "dict": {
                          "dictType": "CE_CASE_STATUS_TYPE",
                          "dictKey": [
                            "CE_CASE_STATUS_TYPE"
                          ],
                          "format": "value-label"
                        }
                    }
                },
                {
                    "is_default": {
                        "control": "select",
                        "label": "是否默认",
                        "default":"0",
                        "width": "130px",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "value-label"
                        }
                    }
                }
            ]
        }
    }
}
