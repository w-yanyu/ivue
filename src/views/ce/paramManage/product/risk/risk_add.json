{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "doRequest": {
                        "url": "/SUMP/call/RPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "ce0020",
                            "page_id": "/views/ce/paramManage/product/risk/risk_add.json",
                            "coll_kind":"02"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "risklevel_code": {
                        "control": "text",
                        "label": "风险等级代码",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            },
                            {
                                "max": 10,
                                "message": "最多可以输入 10 个字节"
                            }
                        ]
                    },
                    "risklevel_name": {
                        "control": "text",
                        "label": "风险等级名称",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            },
                            {
                                "max": 30,
                                "message": "最多可以输入 30 个字节"
                            }
                        ]
                    }
                },
                {
                    "risk_grade_desc": {
                        "control": "text",
                        "label": "风险等级描述",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            }
                        ]
                    },
                    "is_default": {
                        "control": "select",
                        "selectFirstValue":"true",
                        "label": "是否默认",
                        "default":"0",
                        "width": "130px",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "value-label"
                        }
                    }
                }
            ]
        }
    }
}
