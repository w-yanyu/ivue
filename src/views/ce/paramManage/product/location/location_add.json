{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "doRequest": {
                        "url": "/SUMP/call/RPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "ce0000",
                            "page_id": "/views/ce/paramManage/product/location/location_add.json",
                            "coll_kind":"02"
                        }
                    }
                }
            ],

            "controls": [
                {
                    "case_cls_code": {
                        "control": "text",
                        "label": "案件分类代码",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            },
                            {
                                "min": 1,
                                "max": 10,
                                "message": "请输入长度在 1 到 10 之间的字符串"
                            }
                        ]
                    },
                    "location_name": {
                        "control": "text",
                        "label": "案件分类名称",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            },
                            {
                                "min": 1,
                                "max": 15,
                                "message": "最多可以输入 30 个字节"
                            }
                        ]
                    }
                },
                {
                    "case_type_desc": {
                        "control": "text",
                        "label": "案件分类描述",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            }
                        ]
                    },
                    "is_default": {
                        "control": "select",
                        "label": "是否默认",
                        "default":"0",
                        "width": "130px",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "value-label"
                        }
                    }
                }
            ]
        }
    }
}