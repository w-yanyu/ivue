{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "doRequest": {
                        "url": "/SUMP/call/RPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "ce0080",
                            "page_id": "/views/ce/paramManage/business/fee/fee_add.json",
                            "coll_kind":"02"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "fee_type": {
                        "control": "select",
                        "label": "费率类型",
                        "default":"OA",
                        "width": "130px",
                        "dict": {
                          "dictType": "CE_FEE_TYPE",
                          "dictKey": [
                            "CE_FEE_TYPE"
                          ],
                          "format": "value-label"
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            }
                        ]
                    },
                    "fee_code": {
                        "control": "text",
                        "label": "费率代码",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            },
                            {
                                "max": 30,
                                "message": "最多可以输入 30 个字节"
                            }
                        ]
                    }
                },
                {
                    "feerate_catg": {
                        "control": "select",
                        "label": "费率类别",
                        "default":"A",
                        "width": "130px",
                        "dict": {
                          "dictType": "CE_FEERATE_CATG",
                          "dictKey": [
                            "CE_FEERATE_CATG"
                          ],
                          "format": "value-label"
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            }
                        ]
                    },
                    "stage_interpretation": {
                        "control": "select",
                        "label": "阶段释义",
                        "default":"A3",
                        "width": "130px",
                        "dict": {
                          "dictType": "CE_STAGE_INTERPRETATION",
                          "dictKey": [
                            "CE_STAGE_INTERPRETATION"
                          ],
                          "format": "value-label"
                        },
                        "filterable":true,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            }
                        ]
                    }
                },
                {
                    "principal_rate": {
                        "control": "inputNumber",
                        "label": "本金费率(%)",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            },
                            {
                                "expression":"'${{form.principal_rate}}'.length <= 21",
                                "message":"最多可以输入 21 个字节"
                            }
                        ]
                    },
                    "lev_rate": {
                        "control": "inputNumber",
                        "label": "利费费率(%)",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            },
                            {
                                "expression":"'${{form.lev_rate}}'.length <= 21",
                                "message":"最多可以输入 21 个字节"
                            }
                        ]
                    }
                },
                {
                  "hands": {
                    "control": "inputNumber",
                    "width": "130px",
                    "label": "手别",
                    "min": 0,
                    "step": 1,
                    "max": 10,
                    "rules": [
                      {
                        "required": true,
                        "message": "请输入"
                      }
                    ]
                  }
                }
            ]
        }
    }
}
