{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "scope":"formData",
            "initRequest": "${{params}}",
            "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "ce0072",
                    "coll_kind":"02"
                }
            },
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": "el-icon-tickets"
                }
            ],
            "controls": [
                {
                    "cnct_rslt_code": {
                        "control": "text",
                        "disabled":true,
                        "label": "催收结果代码",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            },
                            {
                                "max": 20,
                                "message": "最多可以输入 20 个字节"
                            }
                        ]
                    },
                    "cnct_rslt_name": {
                        "control": "text",
                        "label": "催收结果名称",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            }
                        ]
                    }
                },
                {
                    "cnct_rslt_desc": {
                        "control": "text",
                        "label": "催收结果描述",
                        "width": "130px",
                        "rules": [
                            {
                                "max": 100,
                                "message": "最多可以输入 100 个字节"
                            }
                        ]
                    },
                    "priority": {
                        "control": "inputNumber",
                        "label": "优先级",
                        "width": "130px",
                        "min": 1,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            },
                            {
                                "expression":"'${{formData.priority}}'.length < 20",
                                "message":"最多可以输入 19 个字节"
                            }
                        ]
                    }
                },
                {
                    "cnct_rslt_type": {
                        "control": "select",
                        "disabled":true,
                        "label": "是否可联",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            }
                        ],
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "value-label"
                        }
                    },
                    "is_validresult": {
                        "control": "select",
                        "label": "是否有效结果",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            }
                        ],
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "value-label"
                        }
                    }
                },
                {
                    "is_valid": {
                        "control": "select",
                        "label": "是否启用",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            }
                        ],
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "value-label"
                        }
                    }
                }
            ]
        }
    }
}
