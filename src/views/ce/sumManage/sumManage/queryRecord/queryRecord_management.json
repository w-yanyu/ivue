{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "scope": "formData",
      "controls": [
        {
          "cust_name": {
            "control": "text",
            "label": "客户姓名"
          },
          "duebill_no": {
            "control": "text",
            "label": "借据号"
          }
        },
        {
          "scallee": {
            "control": "text",
            "label": "被叫号码"
          },
          "queue_code": {
            "control": "remoteSelect",
            "label": "队列名称",
            "labelField": "queue_name",
            "valueField": "queue_code",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ce0402"
              }
            },
            "format": "value-label"
          }
        },
        {
          "casestate_code": {
            "control": "remoteSelect",
            "label": "案件状态",
            "labelField": "status_name",
            "valueField": "casestate_code",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ce0411"
              }
            },
            "format": "value-label"
          },
          "overdue_stage_code": {
            "control": "remoteSelect",
            "label": "逾期阶段",
            "labelField": "ovd_period_name",
            "valueField": "overdue_stage_code",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ce0412"
              }
            },
            "format": "value-label"
          }
        },
        {
          "ovd_amt_total_1": {
            "control": "inputNumber",
            "label": "欠款金额区间开始值"
          },
          "ovd_amt_total_2": {
            "control": "inputNumber",
            "label": "欠款金额区间结束值"
          }
        },
        {
          "collect_date_1": {
            "control": "inputNumber",
            "label": "入催天数区间开始值"
          },
          "collect_date_2": {
            "control": "inputNumber",
            "label": "入催天数区间结束值"
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "export",
          "label": "导出",
          "icon": "iconfont icon-daochu",
          "doRequest": {
            "method": "POST",
            "url": "/SUMP/icmcall/export",
            "params": {
              "servicecode": "pl6644"
            }
          },
          "confirm":{
            "title":"提示信息",
            "msg":"确认导出吗？",
            "confirmButtonText":"确定",
            "cancelButtonText":"取消"
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/call/RPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ce6644"
        }
      },
      "thead": [
        {
          "type": "selection",
          "align": "center",
          "width": "55",
          "fixed": "left",
          "show": {
            "prop": "changkuan",
            "is": [
              "0"
            ]
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "被叫号码",
          "prop": "called_num"
        },
        {
          "type": "string",
          "align": "center",
          "label": "客户姓名",
          "prop": "cust_name"
        },
        {
          "type": "string",
          "align": "center",
          "label": "证件类型",
          "width": "150px",
          "prop": "cert_type",
          "dict": {
            "dictType": "MS_E_CRET_TYPE",
            "dictKey": [
              "MS_E_CRET_TYPE"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "证件号码",
          "prop": "cert_no"
        },
        {
          "type": "string",
          "align": "center",
          "label": "借据号",
          "prop": "loan_no"
        }
      ]
    }
  }
}
