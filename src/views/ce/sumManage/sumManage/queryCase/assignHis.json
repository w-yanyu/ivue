{
    "module": "v_search_btn_datagrid",
    "layout": {
        "datagrid": {
            "isInitSearch": false,
            "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "ce6523",
                    "case_no":"${{params.case_no}}",
                    "coll_kind":"${{params.coll_kind}}",
                    "org_id":"${{params.org_id}}"
                }
            },
            "thead": [
                {
                    "type":"string",
                    "label": "转出组",
                    "align":"center",
                    "prop": "out_coll_team"
                },
                {
                    "type":"string",
                    "label": "转出对象",
                    "align":"center",
                    "prop": "out_collector"
                },
                {
                    "type":"string",
                    "label": "转入组",
                    "align":"center",
                    "prop": "in_team_cd"
                },
                {
                    "type":"string",
                    "label": "转入对象",
                    "align":"center",
                    "prop": "in_collector"
                },
                {
                    "type":"string",
                    "label": "是否手动分案",
                    "align":"center",
                    "prop": "is_manul_change"
                },
                {
                    "type":"string",
                    "label": "创建人",
                    "align":"center",
                    "prop": "data_create_user"
                },
                {
                    "type":"string",
                    "label": "创建时间",
                    "align":"center",
                    "prop": "data_create_time"
                }
            ]
        }
    }
}
