{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "scope": "formData",
            "controls": [
                {
                    "branch_id": {
                        "control": "select",
                        "label": "机构",
                        "options": [
                            {
                                "label": "9999-BYB",
                                "value": "9999"
                            }
                        ]
                    },
                    "cust_name": {
                        "control": "text",
                        "label": "客户姓名"
                    }
                },
                {
                    "id_no": {
                        "control": "text",
                        "label": "证件号码"
                    },
                    "cust_id": {
                        "control": "text",
                        "label": "客户号"
                    }
                },
                {
                    "duebill_no": {
                        "control": "text",
                        "label": "借据号"
                    },
                    "function_cd": {
                        "control": "select",
                        "label": "逾期阶段",
                        "options": [
                            {
                                "label": "M6-M6阶段",
                                "value": "M6"
                            },
                            {
                                "label": "M6+-M7及以上阶段",
                                "value": "M6+"
                            },
                            {
                                "label": "M5-M5阶段",
                                "value": "M5"
                            },
                            {
                                "label": "M3-M3阶段",
                                "value": "M3"
                            },
                            {
                                "label": "M4-M4阶段",
                                "value": "M4"
                            },
                            {
                                "label": "M2-M2阶段",
                                "value": "M2"
                            },
                            {
                                "label": "M1-M1阶段",
                                "value": "M1"
                            },
                            {
                                "label": "M0-正常阶段",
                                "value": "M0"
                            }
                        ]
                    }
                },
                {
                    "start_day": {
                        "control": "inputNumber",
                        "label": "逾期天数开始值"
                    },
                    "end_day": {
                        "control": "inputNumber",
                        "label": "逾期天数结束值"
                    }
                },
                {
                    "queue_cd": {
                        "control": "select",
                        "label": "队列名称",
                        "options": [
                            {
                                "label": "DC_KXQ_TC-宽限期内停催队列",
                                "value": "DC_KXQ_TC"
                            },
                            {
                                "label": "DC_M1_C-电催M1队列",
                                "value": "DC_M1_C"
                            },
                            {
                                "label": "DC_M2_C-电催M2队列",
                                "value": "DC_M2_C"
                            },
                            {
                                "label": "DC_M3_C-电催M3队列",
                                "value": "DC_M3_C"
                            },
                            {
                                "label": "DC_M4_C-电催M4队列",
                                "value": "DC_M4_C"
                            },
                            {
                                "label": "DC_M5_C-电催M5队列",
                                "value": "DC_M5_C"
                            },
                            {
                                "label": "DC_M6+_C-电催M6+队列",
                                "value": "DC_M6+_C"
                            },
                            {
                                "label": "DC_M6_C-电催M6队列",
                                "value": "DC_M6_C"
                            },
                            {
                                "label": "HX_DL-核销队列",
                                "value": "HX_DL"
                            },
                            {
                                "label": "SS_DL-诉讼队列",
                                "value": "SS_DL"
                            },
                            {
                                "label": "TS_KE_DL-特殊客户队列",
                                "value": "TS_KE_DL"
                            },
                            {
                                "label": "WW_DL-委外队列",
                                "value": "WW_DL"
                            }
                        ]
                    },
                    "status_cd": {
                        "control": "select",
                        "label": "案件状态",
                        "options": [
                            {
                                "label": "CC-出催",
                                "value": "CC"
                            },
                            {
                                "label": "ZC-在催",
                                "value": "ZC"
                            },
                            {
                                "label": "TC-停催",
                                "value": "TC"
                            }
                        ]
                    }
                },
                {
                    "oa_hands": {
                        "control": "inputNumber",
                        "label": "手別"
                    },
                    "location_cd": {
                        "control": "select",
                        "label": "案件类型",
                        "options": [
                            {
                                "label": "ZY-自营",
                                "value": "ZY"
                            },
                            {
                                "label": "LZY-类自营",
                                "value": "LZY"
                            },
                            {
                                "label": "ZD-助贷",
                                "value": "ZD"
                            }
                        ]
                    }
                },
                {
                    "ovd_amt_total_1": {
                        "control": "inputNumber",
                        "label": "欠款金额区间开始值"
                    },
                    "ovd_amt_total_2": {
                        "control": "inputNumber",
                        "label": "欠款金额区间结束值"
                    }
                },
                {
                    "collect_date_1": {
                        "control": "inputNumber",
                        "label": "入催天数区间开始值"
                    },
                    "collect_date_2": {
                        "control": "inputNumber",
                        "label": "入催天数区间结束值"
                    }
                }
            ]
        },
        "toolbar": {
            "btns": [
                {
                    "type": "delete",
                    "label": "人工分配",
                    "toRequest": {
                        "url": "/views/pl/collManage/oaManage/oaAssign/oaAssign_management.json",
                        "method": "GET",
                        "params": {
                            "servicecode": "settleaccount",
                            "operateUser":"qingsuan",
                            "dealperson":"$g{{user_info.userCode}}",
                            "changkuan1" : "1"
                        }
                    }
                },
                {
                    "type": "delete",
                    "label": "自动分配",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "post",
                        "params": {
                            "servicecode": "settleaccount",
                            "operateUser":"qingsuan",
                            "dealperson":"$g{{user_info.userCode}}",
                            "changkuan1" : "1"
                        }
                    }
                },
                {
                    "type": "delete",
                    "label": "委外退案",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "post",
                        "params": {
                            "servicecode": "settleaccount",
                            "operateUser":"qingsuan",
                            "dealperson":"$g{{user_info.userCode}}",
                            "changkuan1" : "1"
                        }
                    }
                },
                {
                    "type": "custom",
                    "label": "批量人工分配",
                    "doRequest": {
                        "url": "/pss/call/RPCCall",
                        "method": "post",
                        "params": {
                            "servicecode": "settleaccount",
                            "operateUser":"qingsuan",
                            "dealperson":"$g{{user_info.userCode}}",
                            "changkuan1" : "1",
                            "dealFlag" : "1",
                            "begndt": "${{unionPayConfirm.begndt}}",
                            "termdt": "${{unionPayConfirm.termdt}}",
                            "chckdtbg": "${{unionPayConfirm.chckdtbg}}",
                            "chckdted": "${{unionPayConfirm.chckdted}}",
                            "refeno": "${{unionPayConfirm.refeno}}",
                            "sbrand": "${{unionPayConfirm.sbrand}}",
                            "finsty": "${{unionPayConfirm.finsty}}",
                            "changkuan": "${{unionPayConfirm.changkuan}}",
                            "shortLabel": "${{unionPayConfirm.shortLabel}}",
                            "inmeid": "${{unionPayConfirm.inmeid}}",
                            "busitp": "${{unionPayConfirm.busitp}}"

                        }
                    },
                    "confirm": {
                        "title": "提示信息",
                        "msg": "直接全部长款确认吗？",
                        "confirmButtonText": "确定",
                        "cancelButtonText": "取消"
                    }
                },
                {
                    "type": "custom",
                    "label": "批量自动分配",
                    "doRequest": {
                        "url": "/pss/call/RPCCall",
                        "method": "post",
                        "params": {
                            "servicecode": "settleaccount",
                            "operateUser":"qingsuan",
                            "dealperson":"$g{{user_info.userCode}}",
                            "changkuan1" : "1",
                            "dealFlag" : "1",
                            "begndt": "${{unionPayConfirm.begndt}}",
                            "termdt": "${{unionPayConfirm.termdt}}",
                            "chckdtbg": "${{unionPayConfirm.chckdtbg}}",
                            "chckdted": "${{unionPayConfirm.chckdted}}",
                            "refeno": "${{unionPayConfirm.refeno}}",
                            "sbrand": "${{unionPayConfirm.sbrand}}",
                            "finsty": "${{unionPayConfirm.finsty}}",
                            "changkuan": "${{unionPayConfirm.changkuan}}",
                            "shortLabel": "${{unionPayConfirm.shortLabel}}",
                            "inmeid": "${{unionPayConfirm.inmeid}}",
                            "busitp": "${{unionPayConfirm.busitp}}"

                        }
                    },
                    "confirm": {
                        "title": "提示信息",
                        "msg": "直接全部长款确认吗？",
                        "confirmButtonText": "确定",
                        "cancelButtonText": "取消"
                    }
                },
                {
                    "type": "custom",
                    "label": "批量委外退案",
                    "doRequest": {
                        "url": "/pss/call/RPCCall",
                        "method": "post",
                        "params": {
                            "servicecode": "settleaccount",
                            "operateUser":"qingsuan",
                            "dealperson":"$g{{user_info.userCode}}",
                            "changkuan1" : "1",
                            "dealFlag" : "1",
                            "begndt": "${{unionPayConfirm.begndt}}",
                            "termdt": "${{unionPayConfirm.termdt}}",
                            "chckdtbg": "${{unionPayConfirm.chckdtbg}}",
                            "chckdted": "${{unionPayConfirm.chckdted}}",
                            "refeno": "${{unionPayConfirm.refeno}}",
                            "sbrand": "${{unionPayConfirm.sbrand}}",
                            "finsty": "${{unionPayConfirm.finsty}}",
                            "changkuan": "${{unionPayConfirm.changkuan}}",
                            "shortLabel": "${{unionPayConfirm.shortLabel}}",
                            "inmeid": "${{unionPayConfirm.inmeid}}",
                            "busitp": "${{unionPayConfirm.busitp}}"

                        }
                    },
                    "confirm": {
                        "title": "提示信息",
                        "msg": "直接全部长款确认吗？",
                        "confirmButtonText": "确定",
                        "cancelButtonText": "取消"
                    }
                }
            ]
        },

        "datagrid": {
            "isInitSearch": false,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "pl4560"
                }
            },
            "thead": [
                {
                    "type": "selection",
                    "align":"center",
                    "width": "55",
                    "fixed": "left",
                    "show": {
                        "prop": "changkuan",
                        "is": [
                            "0"
                        ]
                    }
                },
                {
                    "type":"string",
                    "align":"center",
                    "label": "机构",
                    "prop": "branch_id"

                },
                {
                    "type":"string",
                    "align":"center",
                    "label": "客户编号",
                    "prop": "case_id"

                },
                {
                    "type":"string",
                    "align":"center",
                    "label": "客户姓名",
                    "prop": "cust_name"
                },
                {
                    "type":"string",
                    "align":"center",
                    "label": "证件类型",
                    "prop": "id_type"
                },
                {
                    "type":"string",
                    "align":"center",
                    "label": "证件号码",
                    "prop": "id_no"
                },
                {
                    "type":"string",
                    "align":"center",
                    "label": "逾期阶段",
                    "prop": "pre_function"
                },
                {
                    "type":"string",
                    "align":"center",
                    "label": "逾期金额",
                    "prop": "over_due_amt"
                },
                {
                    "type":"string",
                    "align":"center",
                    "label": "逾期天数",
                    "prop": "over_due_day"
                },
                {
                    "type":"string",
                    "align":"center",
                    "label": "手别",
                    "prop": "hands"
                },
                {
                    "type":"string",
                    "align":"center",
                    "label": "案件分类",
                    "prop": "location_cd"
                },
                {
                    "type":"string",
                    "align":"center",
                    "label": "案件状态",
                    "prop": "status_cd"
                },
                {
                    "type":"string",
                    "align":"center",
                    "label": "队列名称",
                    "prop": "queue_cd"
                }
            ]
        }
    }
}
