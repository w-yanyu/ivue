{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "foldLineNumber": 3,
      "controls": [
        {
          "cust_name": {
            "control": "text",
            "label": "客户姓名"
          },
          "cust_no": {
            "control": "text",
            "label": "客户号"
          }
        },
        {
          "daterange": {
            "control": "dateTimePicker",
            "label": "申请时间",
            "type": "daterange",
            "unlinkPanels": true,
            "valueFormat": "yyyyMMdd",
            "startPlaceholder": "选择开始时间",
            "endPlaceholder": "选择结束时间",
            "startField": "start_time",
            "endField": "expir_time",
            "disabledDate": "dateAfter"
          },
          "appr_state": {
            "control": "select",
            "label": "审核状态",
            "default": "NC",
            "dict": {
              "dictType": "CE_APPR_STATE",
              "dictKey": [
                "CE_APPR_STATE"
              ],
              "format": "value-label"
            }
          }
        },
        {
          "template_code": {
            "control": "remoteSelect",
            "label": "邮件模板",
            "labelField": "template_name",
            "valueField": "template_code",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ce0414",
                "coll_kind": "02",
                "template_type": "EML"
              }
            },
            "format": "label"
          },
          "email_addr": {
            "control": "text",
            "label": "邮件地址"
          }
        },
        {
          "classify_result": {
            "control": "select",
            "label": "本期五级分类",
            "dict": {
              "dictType": "MS_E_LEVEL5_TYPE",
              "dictKey": [
                "MS_E_LEVEL5_TYPE"
              ],
              "format": "value-label"
            }
          },
          "loan_no": {
            "control": "text",
            "label": "借据号"
          }
        },
        {
          "cert_no": {
            "control": "text",
            "label": "证件号码"
          },
          "busi_type_name": {
            "control": "text",
            "label": "业务品种"
          }
        },
        {
          "case_cust_type": {
            "control": "select",
            "label": "客户类型",
            "dict": {
              "dictType": "CE_CASE_CUST_TYPE",
              "dictKey": [
                "CE_CASE_CUST_TYPE"
              ],
              "format": "value-label"
            }
          },
          "is_valid_current": {
            "control": "select",
            "label": "是否欠款",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "value-label"
            }
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "custom",
          "target": "dialog",
          "checkCountMin": 1,
          "label": "批量操作",
          "toRequest": {
            "method": "GET",
            "url": "/views/ce/assistManage/eml/emlCheck/emlCheck_auditing.json"
          }
        },

        {
          "type": "custom",
          "target": "dialog",
          "label": "全部核准",
          "disabled":"'${{form.appr_state}}' === 'P' || '${{form.appr_state}}' === 'R' || '${{form.appr_state}}' === ''",
          "toRequest": {
            "method": "GET",
            "url": "/views/ce/assistManage/eml/emlCheck/emlCheck_all_through.json",
            "params": {
              "branch_org": "${{form.branch_org}}",
              "cust_name": "${{form.cust_name}}",
              "start_time": "${{form.start_time}}",
              "expir_time": "${{form.expir_time}}",
              "cert_no": "${{form.cert_no}}",
              "template_code": "${{form.template_code}}",
              "email_addr": "${{form.email_addr}}",
              "cust_no": "${{form.cust_no}}",
              "loan_no": "${{form.loan_no}}",
              "busi_type_name": "${{form.busi_type_name}}",
              "case_cust_type": "${{form.case_cust_type}}",
              "is_valid_current": "${{form.is_valid_current}}",
              "classify_result": "${{form.classify_result}}"
            }
          }
        },

        {
          "type": "custom",
          "target": "dialog",
          "label": "全部拒绝",
          "disabled":"'${{form.appr_state}}' === 'P' || '${{form.appr_state}}' === 'R' || '${{form.appr_state}}' === ''",
          "toRequest": {
            "method": "GET",
            "url": "/views/ce/assistManage/eml/emlCheck/emlCheck_all_rejected.json",
            "params": {
              "branch_org": "${{form.branch_org}}",
              "cust_name": "${{form.cust_name}}",
              "start_time": "${{form.start_time}}",
              "expir_time": "${{form.expir_time}}",
              "cert_no": "${{form.cert_no}}",
              "template_code": "${{form.template_code}}",
              "email_addr": "${{form.email_addr}}",
              "cust_no": "${{form.cust_no}}",
              "loan_no": "${{form.loan_no}}",
              "busi_type_name": "${{form.busi_type_name}}",
              "case_cust_type": "${{form.case_cust_type}}",
              "is_valid_current": "${{form.is_valid_current}}",
              "classify_result": "${{form.classify_result}}"
            }
          }
        },
        {
          "type": "export",
          "icon": "iconfont icon-daochu",
          "label": "导出",
          "doRequest": {
            "method": "POST",
            "url": "/SUMP/icmcall/export",
            "params": {
              "servicecode": "ce1526"
            }
          },
          "confirm":{
            "title":"提示信息",
            "msg":"确认导出吗？",
            "confirmButtonText":"确定",
            "cancelButtonText":"取消"
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/call/RPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ce1520"
        }
      },
      "thead": [
        {
          "type": "selection",
          "align": "center",
          "width": "55",
          "fixed": "left",
          "show":{
            "expression":"'${{appr_state}}' === 'NC'"
          }
        },
		{
		  "type": "string",
		  "align": "center",
		  "width": "100",
		  "label": "机构",
		  "prop": "branch_org",
		  "dict": {
		    "dictKey": [
		      "branch_org"
		    ],
		    "format": "label",
		    "labelField": "branch_name",
		    "valueField": "branch_id",
		    "doRequest": {
		      "url": "/SUMP/call/RPCCall",
		      "method": "POST",
		      "params": {
		        "servicecode": "ct2173"
		      }
		    }
		  }
		},
        {
          "type": "string",
          "align": "center",
          "width":"180px",
          "label": "客户姓名",
          "prop": "cust_name"
        },
        {
          "type": "string",
          "align": "center",
          "width":"180px",
          "label": "申请人",
          "prop": "appt",
          "dict": {
            "dictKey": [
              "appt"
            ],
            "format": "label",
            "labelField": "user_name",
            "valueField": "user_id",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ce0403"
              }
            }
          }
        },
        {
          "type": "string",
          "align": "center",
          "width":"180px",
          "label": "申请时间",
          "prop": "apply_date"
        },
        {
          "type": "string",
          "align": "center",
          "width":"180px",
          "label": "审核状态",
          "prop": "appr_state",
          "dict": {
            "dictType": "CE_APPR_STATE",
            "dictKey": [
              "CE_APPR_STATE"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "align": "center",
          "width": "180px",
          "label": "证件类型",
          "prop": "cert_type",
          "dict": {
            "dictType": "MS_E_CRET_TYPE",
            "dictKey": [
              "MS_E_CRET_TYPE"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "align": "center",
          "width":"180px",
          "label": "证件号码",
          "prop": "cert_no"
        },
        {
          "type": "string",
          "align": "center",
          "width":"180px",
          "label": "邮件地址",
          "prop": "email_addr"
        },
        {
          "type": "string",
          "align": "center",
          "width":"180px",
          "label": "邮件模板",
          "prop": "template_code",
          "dict": {
            "dictKey": [
              "template_code"
            ],
            "format": "label",
            "labelField": "template_name",
            "valueField": "template_code",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ce0414",
                "coll_kind": "02",
                "template_type": "EML"
              }
            }
          }
        },
        {
          "type": "string",
          "align": "center",
          "width":"180px",
          "label": "邮件内容",
          "prop": "email_content"
        },
        {
          "type": "string",
          "align": "center",
          "width":"180px",
          "label": "业务品种名称",
          "prop": "busi_type_name"
        },
        {
          "type": "string",
          "align": "center",
          "width":"180px",
          "label": "本期五级分类",
          "prop": "classify_result",
          "dict": {
            "dictType": "MS_E_LEVEL5_TYPE",
            "dictKey": [
              "MS_E_LEVEL5_TYPE"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "align": "center",
          "width":"180px",
          "label": "借据号",
          "prop": "loan_no"
        },
        {
          "type": "string",
          "align": "center",
          "width":"180px",
          "label": "客户号",
          "prop": "cust_no"
        },
        {
          "type": "string",
          "align": "center",
          "width":"180px",
          "label": "客户类型",
          "prop": "cust_type",
          "dict": {
            "dictType": "CE_CASE_CUST_TYPE",
            "dictKey": [
              "CE_CASE_CUST_TYPE"
            ],
            "format": "value-label"
          }
        },
        {
          "label": "操作",
          "align": "center",
          "fixed": "right",
          "width": "130px",
          "btns": [
            {
              "type": "custom",
              "label": "案件详情",
              "toRequest": {
                "url": "/views/ce/assistManage/caseDetail/caseDetail_2/case_detail_main.json",
                "method": "POST",
                "params": {
                  "case_no": "${{current.case_no}}"
                }
              },
              "props": {
                "width": "80%",
                "title": "案件详情"
              }
            }
          ]
        }
      ]
    }
  }
}
