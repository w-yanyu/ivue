{
    "module": "dynamicTabs",
    "layout": {
        "tabs": {
          "scope": "templateScope",
          "tabHeader": [
            {
              "id": "contactInfo",
              "label": "联系人信息",
              "closable": false,
              "disabled": false,
              "icon": "el-icon-date",
              "toRequest":{
                "url":"/views/ce/assistManage/caseDetail/contact_info.json",
                "method": "post",
                "params": {  
                  "case_no": "${{params.case_no}}"
                }
              }
            },
            {
              "id": "pushdownInfo",
              "label": "催记信息",
              "icon": "el-icon-date",
              "closable": false,
              "disabled": false,
              "toRequest":{
                "url":"/views/ce/assistManage/caseDetail/pushdown_info.json",
                "method": "post",
                "params": {  
                  "case_no": "${{params.case_no}}"
                }
              }
            }
          ],
          "defaultActiveKey": "contactInfo",
          "position": "top",
          "styleClass": "card"
        }
        
    }
  }
