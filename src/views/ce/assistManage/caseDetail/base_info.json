{
    "module": "v_realonlyForm_btn",
    "layout": {
        "form": {
            "initRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "ce3601",
                  "case_no": "${{params.case_no}}"
                }
              },
            "controls": [
                {
                    "cust_no": {
                        "control": "string",
                        "label": "客户编号",
                        "span":"6"
                    },
                    "cust_name": {
                        "control": "string",
                        "label": "客户姓名",
                        "span":"8"
                    },
                    "cert_no": {
                        "control": "string",
                        "label": "证件号码",
                        "span":"8"
                    },
                    "acct_amt": {
                      "control": "string",
                      "label": "贷款金额",
                      "span":"6",
                      "format": {
                        "decimal": "2"
                      }
                    },
                    "current_bal": {
                      "control": "string",
                      "label": "当前余额",
                      "span":"8",
                      "format": {
                        "decimal": "2"
                      }
                    },
                    "normal_bal": {
                      "control": "string",
                      "label": "正常余额",
                      "span":"8",
                      "format": {
                        "decimal": "2"
                      }
                    },
                    "overdue_princi": {
                      "control": "string",
                      "label": "逾期本金",
                      "span":"6",
                      "format": {
                        "decimal": "2"
                      }
                    },
                    "overdue_bal": {
                      "control": "string",
                      "label": "逾期余额",
                      "span":"8",
                      "format": {
                        "decimal": "2"
                      }
                    },
                    "overdue_inst": {
                      "control": "string",
                      "label": "逾期利息",
                      "span":"8",
                      "format": {
                        "decimal": "2"
                      }
                    },
                    "princi_fine_inst": {
                      "control": "string",
                      "label": "本金罚息",
                      "span":"6",
                      "format": {
                        "decimal": "2"
                      }
                    },
                    "inst_fine_inst": {
                      "control": "string",
                      "label": "利息罚息",
                      "span":"8",
                      "format": {
                        "decimal": "2"
                      }
                    },
                    "overdue_amt": {
                      "control": "string",
                      "label": "逾期金额",
                      "span":"8",
                      "format": {
                        "decimal": "2"
                      }
                    },
                    "first_loan_date": {
                        "control": "string",
                        "label": "贷款发放日",
                        "span":"6"
                    },
                    "overdue_days": {
                        "control": "string",
                        "label": "本金逾期天数",
                        "span":"8"
                    },
                    "oweinterest_days": {
                        "control": "string",
                        "label": "利息逾期天数",
                        "span":"8"
                    },
                    "accumarr_times": {
                        "control": "string",
                        "label": "当前欠供期数",
                        "span":"6"
                    },
                    "loan_no": {
                        "control": "string",
                        "label": "借据号",
                        "span":"8"
                    }
                }
            ]
        }
    }
}
