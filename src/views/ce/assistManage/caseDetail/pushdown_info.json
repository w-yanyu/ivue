{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/call/RPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ce3608",
          "case_no": "${{params.case_no}}",
          "is_default":""
        }
      },
      "thead": [
        {
          "type":"string",
          "label": "催收时间",
          "align":"center",
          "width":"180px",
          "prop": "coll_time"
        },
        {
          "type":"string",
          "label": "催收员",
          "align":"center",
          "width":"180px",
          "prop": "collector",
          "dict":{
            "dictKey": [
              "collector"
            ],
            "format": "label",
            "labelField": "user_name",
            "valueField": "user_id",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ce0403"
              }
            }
          }
        },
        {
          "type":"string",
          "label": "客户关系",
          "align":"center",
          "width":"180px",
          "prop": "relationship",
          "dict": {
            "dictType": "CE_E_RELATIONSHIP",
            "dictKey": [
              "CE_E_RELATIONSHIP"
            ],
            "format": "label"
          }
        },
        {
          "type":"string",
          "label": "联系姓名",
          "align":"center",
          "width":"180px",
          "prop": "linkman"
        },
        {
          "type":"string",
          "label": "电话号码",
          "align":"center",
          "width":"180px",
          "prop": "tel_no"
        },
        {
          "type":"string",
          "label": "是否接通",
          "align":"center",
          "width":"180px",
          "prop": "collection_action",
          "dict": {
            "dictType": "CE_E_COLLECTION_ACTION",
            "dictKey": [
              "CE_E_COLLECTION_ACTION"
            ],
            "format": "label"
          }
        },
        {
          "type":"string",
          "label": "内容类型",
          "align":"center",
          "width":"180px",
          "prop": "message_type",
          "dict": {
            "dictType": "CS_E_MESSAGE_TYPE",
            "dictKey": [
              "CS_E_MESSAGE_TYPE"
            ],
            "format": "label"
          }
        },
        {
          "type":"string",
          "label": "催收结果",
          "align":"center",
          "width":"180px",
          "prop": "coll_result",
          "dict":{
            "dictKey": [
              "cnct_rslt_code"
            ],
            "format": "label",
            "labelField": "cnct_rslt_name",
            "valueField": "cnct_rslt_code",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "servicecode":"ce0420",
                "is_valid": "1"
              }
            }
          }
        },
        {
          "type":"string",
          "label": "备注",
          "align":"center",
          "width":"180px",
          "prop": "remark"
        },
        {
          "type":"string",
          "label": "是否有效结果",
          "align":"center",
          "width":"180px",
          "prop": "is_validresult",
          "dict": {
            "dictType": "MS_E_YESORNO",
            "dictKey": [
              "MS_E_YESORNO"
            ],
            "format": "label"
          }
        }
      ]
    }
  }
}
