{
  "module": "v-inputDataForm-btn",
  "layout": {
    "scope": "templateScope",
    "form": {
      "btns":[],
      "controls": [
          {
            "top": {
              "control":"dynamic",
              "span":24,
              "toRequest":{
                "url":"/views/ce/assistManage/caseDetail/caseDetail_2/case_detail_top.json",
                "method": "post",
                "params": {
                  "case_no":"${{params.case_no}}"
                }
              }
            },
            "bottom": {
              "control":"dynamic",
              "span":24,
              "toRequest":{
                "url":"/views/ce/assistManage/caseDetail/case_detail_bottom.json",
                "method": "post",
                "params": {
                  "case_no":"${{params.case_no}}"
                }
              }
            }
          } 
      ]
    }
  }
}
