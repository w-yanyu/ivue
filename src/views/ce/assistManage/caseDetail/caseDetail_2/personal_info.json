{
  "module": "v_realonlyForm_btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/call/RPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ce3602",
          "case_no": "${{params.case_no}}"
        }
      },
      "controls": [
        {
          "name": {
            "control": "string",
            "label": "客户姓名",
            "span":"6"
          },
          "sex_tp": {
            "control": "string",
            "label": "性别",
            "span":"8",
            "dict": {
              "dictType": "MS_E_GENDER",
              "dictKey": [
                "MS_E_GENDER"
              ],
              "format": "label"
            }
          },
          "birth_date": {
            "control": "string",
            "label": "出生日",
            "span":"8"
          },
          "marr_state": {
            "control": "string",
            "label": "婚姻状况",
            "span":"6",
            "dict": {
              "dictType": "MS_E_MARITA",
              "dictKey": [
                "MS_E_MARITA"
              ],
              "format": "label"
            }
          },
          "cert_type": {
            "control": "string",
            "label": "证件类型",
            "span":"8",
            "dict": {
              "dictType": "MS_E_CRET_TYPE",
              "dictKey": [
                "MS_E_CRET_TYPE"
              ],
              "format": "label"
            }
          },
          "cert_no": {
            "control": "string",
            "label": "证件号码",
            "span":"8"
          },
          "mobile_no": {
            "control": "string",
            "label": "手机号码",
            "span":"6"
          },
          "email": {
            "control": "string",
            "label": "电子邮箱",
            "span":"8"
          },
          "job_corp": {
            "control": "string",
            "label": "工作单位",
            "span":"8"
          },
          "unit_charic": {
            "control": "string",
            "label": "单位类别",
            "span":"6"
          },
          "corp_tel": {
            "control": "string",
            "label": "单位电话",
            "span":"8"
          },
          "work_tp": {
            "control": "string",
            "label": "职业",
            "span":"8",
            "dict": {
              "dictType": "MS_E_WORKTP",
              "dictKey": [
                "MS_E_WORKTP"
              ],
              "format": "label"
            }
          },
          "unit_catg": {
            "control": "string",
            "label": "单位性质",
            "span":"6"
          },
          "personal_month_incm": {
            "control": "string",
            "label": "月收入",
            "span":"8",
            "format": {
              "decimal": "2"
            }
          },
          "max_educat_backg": {
            "control": "string",
            "label": "教育程度",
            "span":"8",
            "dict": {
              "dictType": "MS_E_MAXEDU",
              "dictKey": [
                "MS_E_MAXEDU"
              ],
              "format": "label"
            }
          },
          "nowworking_years": {
            "control": "string",
            "label": "现工龄",
            "span":"6"
          },
          "postal_addr": {
            "control": "string",
            "label": "通讯地址",
            "span":"8"
          },
          "work_addr": {
            "control": "string",
            "label": "单位地址",
            "span":"8",
            "##width":"130px"
          }
        }
      ]
    }
  }
}
