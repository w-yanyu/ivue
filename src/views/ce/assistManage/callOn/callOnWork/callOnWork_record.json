{
  "module": "v_search_btn_datagrid",
  "layout": {

    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/call/RPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ce1556",
          "work_seqno": "${{params.case_no}}"
        }
      },
      "thead": [
        {
          "type": "string",
          "align": "center",
          "width":"180px",
          "label": "被访人姓名",
          "prop": "callon_name"
        },
        {
          "type": "string",
          "align": "center",
          "width":"180px",
          "label": "外访日期",
          "prop": "callon_date"
        },
        {
          "type": "string",
          "align": "center",
          "width":"180px",
          "label": "外访结果",
          "prop": "callon_result"
        },
        {
          "type": "string",
          "align": "center",
          "width":"180px",
          "label": "备注",
          "prop": "remark"
        }
      ]
    }
  }
}
