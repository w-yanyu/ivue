{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "foldLineNumber": 3,
            "controls": [
                {
                    "cust_name": {
                        "control": "text",
                        "label": "客户姓名"
                    },
                    "cert_no": {
                        "control": "text",
                        "label": "证件号码"
                    }
                },
                {
                    "cust_no": {
                        "control": "text",
                        "label": "客户号"
                    }
                }
            ]
        },
        "toolbar": {},
        "datagrid": {
            "isInitSearch": false,
            "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "ce1560"
                }
            },
            "thead": [
                {
                    "type": "string",
                    "align": "center",
                    "width": "180px",
                    "label": "客户姓名",
                    "prop": "cust_name"
                },
                {
                    "type": "string",
                    "align": "center",
                    "width": "180px",
                    "label": "客户号",
                    "prop": "cust_no"
                },
                {
                    "type": "string",
                    "align": "center",
                    "width": "180px",
                    "label": "证件号码",
                    "prop": "cert_no"
                },
                {
                    "type": "string",
                    "align": "center",
                    "width": "180px",
                    "label": "被访人姓名",
                    "prop": "callon_name"
                },
                {
                    "type": "string",
                    "align": "center",
                    "width": "180px",
                    "label": "客户关系",
                    "prop": "relationship"
                },
                {
                    "type": "string",
                    "align": "center",
                    "width": "180px",
                    "label": "外访日期",
                    "prop": "callon_date"
                },
                {
                    "type": "string",
                    "align": "center",
                    "width": "180px",
                    "label": "外访结果",
                    "prop": "callon_result"
                },
                {
                    "type": "string",
                    "align": "center",
                    "width": "180px",
                    "label": "备注",
                    "prop": "remark"
                },
                {
                    "label": "操作",
                    "align": "center",
                    "fixed": "right",
                    "width": "130px",
                    "btns": [
                        {
                            "type": "custom",
                            "label": "案件详情",
                            "toRequest": {
                                "url": "/views/ce/assistManage/caseDetail/caseDetail_2/case_detail_main.json",
                                "method": "POST",
                                "params": {
                                    "case_no": "${{current.case_no}}"
                                }
                            },
                            "props": {
                                "width": "80%",
                                "title": "案件详情"
                            }
                        }
                    ]
                }
            ]
        }
    }
}
