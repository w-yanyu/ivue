{
  "module": "v_search_btn_datagrid",
  "layout": {

    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ce1591",
          "case_no": "${{params.case_no}}",
          "contract_no":"${{params.contact_no}}"
        }
      },
      "thead": [
        {
          "type":"string",
          "align":"center",
          "label": "案件编号",
          "prop": "case_no"
        },
        {
          "type":"string",
          "align":"center",
          "label": "客户姓名",
          "prop": "cust_name"
        },
        {
          "type":"string",
          "align":"center",
          "label": "短信发送数量",
          "prop": "msgSendCount"
        },
        {
          "type":"string",
          "align":"center",
          "label": "语音发送数量",
          "prop": "voiceSendCount"
        },
        {
          "type":"string",
          "align":"center",
          "label": "联系人姓名",
          "prop": "collectionName"
        },
        {
          "type":"string",
          "align":"center",
          "label": "联系人电话",
          "prop": "telphone"
        },
        {
          "type":"string",
          "align":"center",
          "label": "联系人关系",
          "prop": "relationShip"
        },
        {
          "type":"string",
          "align":"center",
          "label": "交互轮次",
          "prop": "replyTimes"
        },
        {
          "type":"string",
          "align":"center",
          "label": "电话状态",
          "prop": "telphoneStatus"
        },
        {
          "type":"string",
          "align":"center",
          "label": "联系状态",
          "prop": "contatcStatus"
        }
      ]
    }
  }
}
