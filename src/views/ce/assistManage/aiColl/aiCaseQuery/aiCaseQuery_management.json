{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "contract_no": {
            "control": "text",
            "label": "委托批次"
          },
          "cust_name": {
            "control": "text",
            "label": "客户姓名"
          },
          "cert_no": {
            "control": "text",
            "label": "证件号码"
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "export",
          "label": "导出",
          "doRequest": {
            "method": "POST",
            "url": "/SUMP/icmcall/export",
            "params": {
              "servicecode": "ce1580",
              "casestate_code": "AI"
            }
          },
          "confirm":{
            "title":"提示信息",
            "msg":"确认导出吗？",
            "confirmButtonText":"确定",
            "cancelButtonText":"取消"
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ce1590",
          "casestate_code": "AI"
        }
      },
      "thead": [
        {
          "type": "string",
          "width": "250px",
          "align": "center",
          "label": "委托批次",
          "prop": "contract_no"
        },
        {
          "type": "string",
          "width": "250px",
          "align": "center",
          "label": "案件编号",
          "prop": "case_no"
        },
        {
          "type": "string",
          "width": "150px",
          "align": "center",
          "label": "客户姓名",
          "prop": "cust_name"
        },
        {
          "type": "string",
          "width": "250px",
          "align": "center",
          "label": "证件号码",
          "prop": "cert_no"
        },
        {
          "type": "currency",
          "width": "150px",
          "align": "center",
          "label": "逾期金额",
          "prop": "overdue_amt",
          "format": {
            "decimal": "2"
          }
        },
        {
          "type": "string",
          "width": "150px",
          "align": "center",
          "label": "逾期天数",
          "prop": "overdue_days"
        },
        {
          "type": "string",
          "width": "150px",
          "align": "center",
          "label": "委托日期",
          "prop": "executiondue_date"
        },
        {
          "type": "string",
          "width": "150px",
          "align": "center",
          "label": "委托天数",
          "prop": "over_due_day"
        },
        {
          "label": "操作",
          "align": "center",
          "fixed": "right",
          "width": "150",
          "btns": [
            {
              "type": "look",
              "label": "查看催记",
              "toRequest": {
                "url": "/views/ce/assistManage/aiColl/aiCaseQuery/collrec_info.json",
                "method": "POST",
                "params": {
                  "case_no": "${{current.case_no}}",
                  "contract_no": "${{current.contact_no}}"
                },
                "props": {
                  "title": "催记信息",
                  "width": "80%"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
