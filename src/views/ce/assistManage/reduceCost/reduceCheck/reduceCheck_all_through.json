{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "btns": [
                {
                    "type": "submit",
                    "label": "确认",
                    "doRequest": {
                        "method": "POST",
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "params": {
                            "servicecode":"pl1567",
                            "submit_type":"P",
                            "check_status": "NC",
                            "cust_name": "${{params.cust_name}}",
                            "start_date": "${{params.start_date}}",
                            "end_date": "${{params.end_date}}",
                            "id_no": "${{params.id_no}}",
                            "entry_type": "${{params.entry_type}}"
                        }
                    }
                } 
            ],
            "controls": [
                {
                    "check_desc": {
                        "control": "textarea",
                        "label": "审核说明",
                        "width": "180px",
                        "align": "center",
                        "span": "20",
                        "rows": 6,
                        "placeholder":"请输入",
                        "rules": [
                            {
                                "required": true,
                                "message":"请输入"
                            }
                        ]
                    }
                }
            ]
        }
    }
}
