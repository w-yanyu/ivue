{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "foldLineNumber": 3,
            "controls": [
                {
                    "cust_name": {
                        "control": "text",
                        "label": "客户姓名"
                    },
                    "daterange": {
                        "control": "dateTimePicker",
                        "label": "申请时间",
                        "type": "daterange",
                        "unlinkPanels": true,
                        "valueFormat": "yyyyMMdd",
                        "startPlaceholder": "选择开始时间",
                        "endPlaceholder": "选择结束时间",
                        "startField": "start_date",
                        "endField": "end_date"
                    }
                },
                {
                    "id_no": {
                        "control": "text",
                        "label": "证件号码"
                    },
                    "entry_type": {
                        "control": "remoteSelect",
                        "label": "申请类型",
                        "labelField": "drop_list_desc",
                        "valueField": "drop_list_value",
                        "doRequest": {
                            "url": "/SUMP/icmcall/icmRPCCall",
                            "method": "POST",
                            "params": {
                                "servicecode": "ap1071",
                                "drop_list_type": "entry_type"
                            }
                        },
                        "format": "label"
                    }
                },
                {
                    "check_status": {
                        "control": "remoteSelect",
                        "label": "审核状态",
                        "default":"NC",
                        "labelField": "drop_list_desc",
                        "valueField": "drop_list_value",
                        "doRequest": {
                            "url": "/SUMP/icmcall/icmRPCCall",
                            "method": "POST",
                            "params": {
                                "servicecode": "ap1071",
                                "drop_list_type": "check_status"
                            }
                        },
                        "format": "label"
                    }
                }
            ]
        },
        "toolbar": {
            "btns": [
                {
                    "type": "custom",
                    "icon": "el-icon-edit",
                    "target": "dialog",
                    "checkCountMin": 1,
                    "label": "批量审核",
                    "toRequest": {
                        "method": "GET",
                        "url": "/views/ce/assistManage/reduceCost/reduceCheck/reduceCheck_auditing.json"
                    }
                },
                {
                    "type": "custom",
                    "icon": "el-icon-circle-check",
                    "target": "dialog",
                    "label": "全部核准",
                    "#disabled": "'${{current.check_status}}' === 'P' || '${{current.check_status}}' === 'R' || '${{current.check_status}}' === ''",
                    "toRequest": {
                        "method": "GET",
                        "url": "/views/ce/assistManage/reduceCost/reduceCheck/reduceCheck_all_through.json",
                        "params": {
                            "cust_name": "${{form.cust_name}}",
                            "start_date": "${{form.start_date}}",
                            "end_date": "${{form.end_date}}",
                            "id_no": "${{form.id_no}}",
                            "entry_type": "${{form.entry_type}}"
                        }
                    }
                },
                {
                    "type": "custom",
                    "icon": "el-icon-circle-close",
                    "target": "dialog",
                    "label": "全部拒绝",
                    "#disabled": "'${{current.check_status}}' === 'P' || '${{current.check_status}}' === 'R' || '${{current.check_status}}' === ''",
                    "toRequest": {
                        "method": "GET",
                        "url": "/views/ce/assistManage/reduceCost/reduceCheck/reduceCheck_all_rejected.json",
                        "params": {
                            "cust_name": "${{form.cust_name}}",
                            "start_date": "${{form.start_date}}",
                            "end_date": "${{form.end_date}}",
                            "id_no": "${{form.id_no}}",
                            "entry_type": "${{form.entry_type}}"
                        }
                    }
                }
            ]
        },
        "datagrid": {
            "isInitSearch": false,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "ce1565"
                }
            },
            "thead": [
                {
                    "type": "selection",
                    "align": "center",
                    "width": "55",
                    "fixed": "left"
                },
                {
                    "type": "string",
                    "align": "center",
                    "width": "180px",
                    "label": "客户姓名",
                    "prop": "cust_name"
                },
                {
                    "type": "string",
                    "align": "center",
                    "width": "180px",
                    "label": "证件号码",
                    "prop": "id_no"
                },
                {
                    "type": "string",
                    "align": "center",
                    "width": "180px",
                    "label": "手机号码",
                    "prop": "contact_no"
                },
                {
                    "type": "string",
                    "align": "center",
                    "width": "180px",
                    "label": "借据号",
                    "prop": "duebill_no"
                },
                {
                    "type": "string",
                    "align": "center",
                    "width": "180px",
                    "label": "逾期天数",
                    "prop": "over_due_day"
                },
                {
                    "type": "currency",
                    "align": "center",
                    "width": "180px",
                    "label": "逾期金额",
                    "prop": "over_due_amt",
                    "format": {
                        "decimal": "2"
                    }
                },
                {
                    "type": "currency",
                    "align": "center",
                    "width": "180px",
                    "label": "贷款金额",
                    "prop": "loan_amt",
                    "format": {
                        "decimal": "2"
                    }
                },
                {
                    "type": "string",
                    "align": "center",
                    "width": "180px",
                    "label": "减免类型",
                    "prop": "reduce_type"
                },
                {
                    "type": "currency",
                    "align": "center",
                    "width": "180px",
                    "label": "申请减免金额",
                    "prop": "reduce_amt",
                    "format": {
                        "decimal": "2"
                    }
                },
                {
                    "type": "string",
                    "align": "center",
                    "width": "180px",
                    "label": "减免金额占比",
                    "prop": "amt_ratio"
                },
                {
                    "type": "string",
                    "align": "center",
                    "width": "180px",
                    "label": "减免本金占比",
                    "prop": "prin_ratio"
                },
                {
                    "type": "string",
                    "align": "center",
                    "width":"100px",
                    "label": "审核状态",
                    "prop": "check_status",
                    "dict": {
                        "dictKey": [
                            "check_status"
                        ],
                        "format": "label",
                        "labelField": "drop_list_desc",
                        "valueField": "drop_list_value",
                        "doRequest": {
                            "url": "/SUMP/icmcall/icmRPCCall",
                            "method": "POST",
                            "params": {
                                "servicecode": "ap1071",
                                "drop_list_type": "check_status"
                            }
                        }
                    }
                },
                {
                    "type": "string",
                    "align": "center",
                    "width": "180px",
                    "label": "申请人",
                    "prop": "apply_user",
                    "dict": {
                        "dictKey": [
                            "apply_user"
                        ],
                        "format": "label",
                        "labelField": "user_name",
                        "valueField": "user_id",
                        "doRequest": {
                            "url": "/SUMP/icmcall/icmRPCCall",
                            "method": "POST",
                            "params": {
                                "servicecode": "ce0403"
                            }
                        }
                    }
                },
                {
                    "type": "string",
                    "align": "center",
                    "width": "180px",
                    "label": "申请时间",
                    "prop": "apply_time"
                },
                {
                    "type": "string",
                    "align": "center",
                    "width": "180px",
                    "label": "申请说明",
                    "prop": "apply_reason"
                },
                {
                    "type": "string",
                    "align": "center",
                    "width": "180px",
                    "label": "申请类型",
                    "prop": "entry_type",
                    "dict": {
                        "dictKey": [
                            "entry_type"
                        ],
                        "format": "label",
                        "labelField": "drop_list_desc",
                        "valueField": "drop_list_value",
                        "doRequest": {
                            "url": "/SUMP/icmcall/icmRPCCall",
                            "method": "POST",
                            "params": {
                                "servicecode": "ap1071",
                                "drop_list_type": "entry_type"
                            }
                        }
                    }
                },
                {
                    "label": "操作",
                    "align": "center",
                    "fixed": "right",
                    "width": "130px",
                    "btns": [
                        {
                            "type": "custom",
                            "label": "案件详情",
                            "toRequest": {
                                "url": "/views/ce/assistManage/caseDetail/caseDetail_2/case_detail_main.json",
                                "method": "POST",
                                "params": {
                                    "case_no": "${{current.case_no}}"
                                }
                            },
                            "props": {
                                "width": "80%",
                                "title": "案件详情"
                            }
                        }
                    ]
                }
            ]
        }
    }
}