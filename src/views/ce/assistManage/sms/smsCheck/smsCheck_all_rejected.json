{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "btns": [
                {
                    "type": "submit",
                    "label": "确认",
                    "doRequest": {
                        "method": "POST",
                        "url": "/SUMP/call/RPCCall",
                        "params": {
                            "servicecode":"ce1507",
                            "submit_type":"R",
                            "appr_state": "NC",
                            "branch_org": "${{params.branch_org}}",
                            "cust_name": "${{params.cust_name}}",
                            "start_time": "${{params.start_time}}",
                            "expir_time": "${{params.expir_time}}",
                            "cert_no": "${{params.cert_no}}",
                            "tel_no": "${{params.tel_no}}",
                            "cust_no": "${{params.cust_no}}",
                            "loan_no": "${{params.loan_no}}",
                            "busi_type_name": "${{params.busi_type_name}}",
                            "template_code": "${{params.template_code}}",
                            "case_cust_type": "${{params.case_cust_type}}",
                            "is_valid_current": "${{params.is_valid_current}}",
                            "classify_result": "${{params.classify_result}}"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "appr_comment": {
                        "control": "textarea",
                        "label": "审核说明",
                        "width": "180px",
                        "align": "center",
                        "span": "20",
                        "rows": 6,
                        "placeholder":"请输入",
                        "rules": [
                            {
                                "required": true,
                                "message":"请输入"
                            }
                        ]
                    }
                }
            ]
        }
    }
}
