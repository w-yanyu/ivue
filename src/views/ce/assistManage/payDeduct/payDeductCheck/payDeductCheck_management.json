{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "foldLineNumber": 3,
      "controls": [
        {
          "cust_name": {
            "control": "text",
            "label": "客户姓名"
          },
          "cert_no": {
            "control": "text",
            "label": "证件号码"
          }
        },
        {
          "appr_state": {
            "control": "select",
            "label": "审核状态",
            "default": "NC",
            "dict": {
              "dictType": "CE_APPR_STATE",
              "dictKey": [
                "CE_APPR_STATE"
              ],
              "format": "value-label"
            }
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "custom",
          "icon": "el-icon-edit",
          "target": "dialog",
          "checkCountMin": 1,
          "label": "批量审核",
          "toRequest": {
            "method": "GET",
            "url": "/views/ce/assistManage/payDeduct/payDeductCheck/payDeductCheck_auditing.json"
          }
        },
        {
          "type": "custom",
          "icon": "el-icon-circle-check",
          "target": "dialog",
          "label": "全部核准",
          "disabled": "'${{current.appr_state}}' === 'P' || '${{current.appr_state}}' === 'R' || '${{current.appr_state}}' === ''",
          "doRequest": {
            "method": "POST",
            "url": "/SUMP/call/RPCCall",
            "params": {
              "servicecode":"ce1578",
              "submit_type": "P",
              "appr_comment":"批量审核-同意",
              "appr_state": "NC",
              "cust_name": "${{selection.cust_name}}",
              "cert_no": "${{form.cert_no}}",
              "apply_journal_no": "${{form.apply_journal_no}}"
            }
          },
          "confirm": {
            "title": "提醒",
            "msg": "确定将案件全部核准？",
            "confirmButtonText": "确定",
            "cancelButtonText": "取消"
          }
        },
        {
          "type": "custom",
          "icon": "el-icon-circle-close",
          "target": "dialog",
          "label": "全部拒绝",
          "disabled": "'${{current.appr_state}}' === 'P' || '${{current.appr_state}}' === 'R' || '${{current.appr_state}}' === ''",
          "doRequest": {
            "method": "POST",
            "url": "/SUMP/call/RPCCall",
            "params": {
              "servicecode":"ce1578",
              "submit_type": "R",
              "appr_comment":"批量审核-拒绝",
              "cust_name": "${{selection.cust_name}}",
              "appr_state": "NC",
              "cert_no": "${{form.cert_no}}",
              "apply_journal_no": "${{form.apply_journal_no}}"
            }
          },
          "confirm": {
            "title": "提醒",
            "msg": "确定将案件全部拒绝？",
            "confirmButtonText": "确定",
            "cancelButtonText": "取消"
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/call/RPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ce1574"
        }
      },
      "thead": [
        {
          "type": "selection",
          "align": "center",
          "width": "55",
          "fixed": "left",
          "show":{
            "expression":"'${{appr_state}}' === 'NC'"
          }
        },
        {
          "type": "string",
          "align": "center",
          "width": "180px",
          "label": "客户姓名",
          "prop": "cust_name"
        },
        {
          "type": "string",
          "align": "center",
          "width": "180px",
          "label": "证件号码",
          "prop": "cert_no"
        },
        {
          "type": "string",
          "align": "center",
          "width": "180px",
          "label": "可划扣账户",
          "prop": "acct_no"
        },
        {
          "type": "string",
          "align": "center",
          "width": "180px",
          "label": "账户余额",
          "prop": "acct_bal"
        },
        {
          "type": "string",
          "align": "center",
          "width": "180px",
          "label": "逾期金额",
          "prop": "overdue_amt"
        },
        {
          "type": "string",
          "align": "center",
          "width": "180px",
          "label": "申请人",
          "prop": "appt",
          "dict": {
            "dictKey": [
              "appt"
            ],
            "format": "value-label",
            "labelField": "user_name",
            "valueField": "user_id",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ce0403"
              }
            }
          }
        },
        {
          "type": "currency",
          "align": "center",
          "width": "180px",
          "label": "申请时间",
          "prop": "apply_date"
        },
        {
          "type": "string",
          "align": "center",
          "width": "180px",
          "label": "申请说明",
          "prop": "apply_reason"
        },
        {
          "label": "操作",
          "align": "center",
          "fixed": "right",
          "width": "130px",
          "btns": [
            {
              "type": "custom",
              "label": "案件详情",
              "toRequest": {
                "url": "/views/ce/assistManage/caseDetail/caseDetail_2/case_detail_main.json",
                "method": "POST",
                "params": {
                  "case_no": "${{current.case_no}}"
                }
              },
              "props": {
                "width": "80%",
                "title": "案件详情"
              }
            }
          ]
        }
      ]
    }
  }
}
