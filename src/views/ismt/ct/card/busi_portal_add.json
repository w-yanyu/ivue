{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "ct3017",
              "appId":"deciOnl"
            }
          }
        }
      ],
      "controls": [
          {
            "card_name": {
              "control": "text",
              "label": "卡片名称",
              "width": "240px",
              "rules": [
                {
                  "required": true,
                  "message": "请输入卡片名称"
                }
              ]
            },
            "url": {
              "control": "text",
              "label": "卡片界面地址",
              "width": "240px",
              "rules": [
                {
                  "required": true,
                  "message": "请输入卡片界面地址"
                }
              ]
            },
            "busi_tp": {
              "control": "select",
              "label": "所属业务",
              "width": "240px",
              "dict": {
                "dictType": "CT_E_BUSI_TP",
                "dictKey": [
                  "CT_E_BUSI_TP"
                ],
                "format": "label"
              },
              "rules": [
                {
                  "required": true,
                  "message": "请选择所属业务"
                }
              ]
            },
            "card_icon": {
              "control": "text",
              "label": "图标类名称"
            }
          }
      ]
    },
    "alert":{
      "span": 8,
      "listStyle": "disc",
      "description":[
          "<p style='color:red; text-align: left'>卡片界面地址配置说明：</p>",
          "<p style='text-align: left'>    配置值为菜单全路径，如：/K1/K03/K0301</p>"
      ]
  }
  }
}
