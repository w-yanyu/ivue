{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "doRequest": {
                        "url": "/SUMP/call/RPCCall",
                        "method": "post",
                        "params": {
                            "servicecode": "ct2048",
                            "operater_ind": "D"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "route_id": {
                        "control": "string",
                        "label": "工作路线ID"
                    },
                    "route_desc": {
                        "control": "string",
                        "label": "工作路线描述"
                    }
                },
                {
                    "workjob_id": {
                        "control": "string",
                        "label": "工作任务ID"
                    },
                    "route_type": {
                        "control": "string",
                        "label": "工作路线种类",
                        "dict": {
                            "format": "label",
                            "dictType": "E_LINETYPE",
                            "dictKey": [
                                "E_LINETYPE"
                            ]
                        }
                    }
                },
                {
                    "next_job_id": {
                        "control": "string",
                        "label": "下一工作任务ID"
                    }
                }
            ]
        }
    }
}