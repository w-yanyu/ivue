{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": "",
                    "doRequest": {
                        "url": "/SUMP/call/RPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "ct2047",
                            "operater_ind": "A"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "route_id": {
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入工作路线ID"
                            },
                            {
                                "max": 32,
                                "message": "长度不能超过32个字符"
                            }
                        ],
                        "control": "text",
                        "label": "工作路线ID",
                        "width": "170px"
                    },
                    "route_desc": {
                        "control": "text",
                        "label": "工作路线描述",
                        "width": "170px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入工作路线描述"
                            },
                            {
                                "max": 100,
                                "message": "长度不能超过100个字符"
                            }
                        ]
                    }
                },
                {
                    "workjob_id": {
                        "control": "lookup",
                        "edit": false,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入工作任务ID"
                            },
                            {
                                "max": 32,
                                "message": "长度不能超过32个字符"
                            }
                        ],
                        "label": "工作任务ID",
                        "width": "170px",
                        "keyField": {
                            "workjob_id": "workjob_id"
                        },
                        "toRequest": {
                            "url": "/views/ismt/ct/ParameterManagement/WorkTasksInformation/WorkTasksManager_lookup.json",
                            "method": "post",
                            "params": "${{form.workjob_id}}"
                        },
                        "props": {
                            "title": "工作任务ID查询"
                        }
                    },
                    "route_type": {
                        "control": "select",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入工作路线种类"
                            }
                        ],
                        "label": "工作路线种类",
                        "width": "170px",
                        "dict": {
                            "dictType": "E_LINETYPE",
                            "dictKey": [
                                "E_LINETYPE"
                            ],
                            "format": "label"
                        },
                        "showItem": {
                            "C": [
                                "next_job_id"
                            ]
                        },
                        "events": {
                            "condition": {
                                "C": {
                                    "control": {
                                        "next_job_id": {
                                            "rules": [
                                                {
                                                    "required": true,
                                                    "message": "请选择下一工作任务"
                                                }
                                            ]
                                        }
                                    }
                                }
                            }
                        }
                    }
                },
                {
                    "next_job_id": {
                        "control": "lookup",
                        "edit": false,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入工作任务ID"
                            },
                            {
                                "max": 32,
                                "message": "长度不能超过32个字符"
                            }
                        ],
                        "label": "下一工作任务ID",
                        "width": "170px",
                        "keyField": {
                            "next_job_id": "workjob_id"
                        },
                        "toRequest": {
                            "url": "/views/ismt/ct/ParameterManagement/WorkTasksInformation/WorkTasksManager_lookup.json",
                            "method": "post",
                            "params": "${{form.workjob_id}}"
                        },
                        "props": {
                            "title": "下一工作任务ID查询"
                        }
                    }
                }
            ]
        }
    }
}