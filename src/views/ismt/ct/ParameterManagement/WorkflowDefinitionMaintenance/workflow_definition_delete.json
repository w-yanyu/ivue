{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "doRequest": {
                        "url": "/SUMP/call/RPCCall",
                        "method": "post",
                        "params": {
                            "servicecode": "ct2095",
                            "operater_ind": "D"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "workflow_id": {
                        "control": "string",
                        "label": "工作流ID"
                    },
                    "workflow_desc": {
                        "control": "string",
                        "label": "工作流描述"
                    }
                },
                {
                    "amount_exchange_ind": {
                        "control": "string",
                        "label": "自动折算标志",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        },
                        "disabled": true
                    },
                    "ccy_code": {
                        "control": "string",
                        "label": "货币代码",
                        "dict": {
                            "dictType": "MS_CURRENCY",
                            "dictKey": [
                                "MS_CURRENCY"
                            ],
                            "format": "label"
                        },
                        "disabled": true
                    }
                }
            ]
        }
    }
}