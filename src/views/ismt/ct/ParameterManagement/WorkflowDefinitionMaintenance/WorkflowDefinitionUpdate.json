{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": "",
                    "doRequest": {
                        "url": "/SUMP/call/RPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "ct2095",
                            "operater_ind": "M"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "workflow_id": {
                        "control": "string",
                        "label": "工作流ID",
                        "width": "220px",
                        "rules": [
                            {
                                "max": 32,
                                "message": "长度不能超过32个字符"
                            }
                        ]
                    },
                    "workflow_desc": {
                        "control": "text",
                        "label": "工作流描述",
                        "width": "220px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入工作流描述"
                            },
                            {
                                "max": 100,
                                "message": "长度不能超过100个字符"
                            }
                        ]
                    }
                },
                {
                    "amount_exchange_ind": {
                        "control": "select",
                        "label": "自动折算标志",
                        "width": "220px",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        }
                    },
                    "ccy_code": {
                        "control": "select",
                        "label": "货币代码",
                        "width": "220px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入货币代码"
                            }
                        ],
                        "dict": {
                            "dictType": "MS_CURRENCY",
                            "dictKey": [
                                "MS_CURRENCY"
                            ],
                            "format": "label"
                        }
                    }
                }
            ]
        }
    }
}
