{
    "module": "v-realonlyForm-btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "controls": [
                {
                    "workjob_id": {
                        "control": "string",
                        "label": "工作任务ID",
                        "width": "150px"
                    },
                    "role_id": {
                        "control": "string",
                        "label": "角色ID",
                        "width": "150px",
                        "disabled": true,
                        "dict": {
                            "dictType": "D_role_id",
                            "dictKey": [
                                "role_id"
                            ],
                            "format": "label",
                            "labelField": "drop_list_desc",
                            "valueField": "drop_list_value"
                        }
                    }
                },
                {
                    "serial_no": {
                        "control": "string",
                        "label": "序号",
                        "width": "150px"
                    },
                    "scope_of_role": {
                        "control": "string",
                        "label": "角色所在机构",
                        "width": "150px",
                        "dict": {
                            "dictType": "E_BRCONFINE",
                            "dictKey": [
                                "E_BRCONFINE"
                            ],
                            "format": "label"
                        }
                    }
                },
                {
                    "trxn_ccy": {
                        "control": "string",
                        "label": "交易币种",
                        "width": "150px",
                        "dict": {
                            "dictType": "MS_CURRENCY",
                            "dictKey": [
                                "MS_CURRENCY"
                            ],
                            "format": "label"
                        }
                    },
                    "min_limit_value": {
                        "control": "string",
                        "label": "审批金额下限",
                        "width": "150px"
                    }
                },
                {
                    "max_limit_value": {
                        "control": "string",
                        "label": "审批金额上限",
                        "width": "150px"
                    }
                }
            ]
        }
    }
}