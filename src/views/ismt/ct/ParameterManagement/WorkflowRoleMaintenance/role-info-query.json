{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "doRequest": {
                "url": "",
                "method": "POST",
                "params": {}
            },
            "controls": [
                {
                    "role_id": {
                        "control": "text",
                        "label": "角色ID",
                        "rules": [
                            {
                                "max": 64,
                                "message": "长度不能超过64个字符"
                            }
                        ]
                    },
                    "role_name": {
                        "control": "text",
                        "label": "角色名称",
                        "rules": [
                            {
                                "max": 100,
                                "message": "长度不能超过100个字符"
                            }
                        ]
                    }
                }
            ]
        },
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": false,
            "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "ct2050"
                }
            },
            "pagination": {
                "pageSizes": [
                    10,
                    15,
                    20
                ],
                "show": true,
                "pageSize": 10
            },
            "thead": [
                {
                    "type": "index"
                },
                {
                    "label": "角色ID",
                    "prop": "role_id"
                },
                {
                    "label": "角色名称",
                    "prop": "role_name"
                },
                {
                    "label": "操作",
                    "fixed": "left",
                    "btns": [
                        {
                            "type": "bringback",
                            "label": "选择"
                        }
                    ]
                }
            ]
        }
    }
}