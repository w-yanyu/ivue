{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "doRequest": {
                        "url": "/SUMP/call/RPCCall",
                        "method": "post",
                        "params": {
                            "servicecode": "ct2092",
                            "operater_ind": "D"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "workjob_id": {
                        "control": "string",
                        "label": "工作任务ID"
                    },
                    "role_id": {
                        "control": "string",
                        "label": "角色ID",
                        "dict": {
                            "dictType": "D_role_id",
                            "dictKey": [
                                "role_id"
                            ],
                            "format": "label"
                        }
                    }
                },
                {
                    "serial_no": {
                        "control": "string",
                        "label": "序号"
                    },
                    "scope_of_role": {
                        "control": "string",
                        "label": "角色所在机构",
                        "dict": {
                            "dictType": "E_BRCONFINE",
                            "dictKey": [
                                "E_BRCONFINE"
                            ],
                            "format": "label"
                        }
                    }
                },
                {
                    "trxn_ccy": {
                        "control": "string",
                        "label": "交易币种",
                        "dict": {
                            "dictType": "MS_CURRENCY",
                            "dictKey": [
                                "MS_CURRENCY"
                            ],
                            "format": "label"
                        }
                    },
                    "min_limit_value": {
                        "control": "string",
                        "label": "审批金额下限"
                    }
                },
                {
                    "max_limit_value": {
                        "control": "string",
                        "label": "审批金额上限"
                    }
                }
            ]
        }
    }
}