{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "controls": [
                {
                    "workjob_id": {
                        "control": "text",
                        "label": "工作任务ID",
                        "rules": [
                            {
                                "max": 32,
                                "message": "长度不能超过32个字符"
                            }
                        ],
                        "keyField": {
                            "workjob_id": "workjob_id"
                        },
                        "toRequest": {
                            "url": "/views/ismt/ct/ParameterManagement/WorkTasksInformation/WorkTasksManager_lookup.json",
                            "method": "post",
                            "params": "${{form.workjob_id}}"
                        },
                        "props": {
                            "title": "工作任务ID查询"
                        },
                        "format": "label"
                    },
                    "workflow_id": {
                        "control": "text",
                        "label": "工作流ID",
                        "rules": [
                            {
                                "max": 32,
                                "message": "长度不能超过32个字符"
                            }
                        ],
                        "keyField": {
                            "workflow_id": "workflow_id"
                        },
                        "toRequest": {
                            "url":"/views/ismt/ct/ParameterManagement/WorkflowDefinitionMaintenance/WorkflowDefinitionManager_lookup.json",
                            "method": "post",
                            "params": "${{form.workflow_id}}"
                        },
                        "props":{
                            "title":"工作流ID查询"
                        },
                        "format": "label"
                    }
                },
                {
                    "begin_ind": {
                        "control": "select",
                        "label": "起始节点标志",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        }
                    },
                    "end_ind": {
                        "control": "select",
                        "label": "结束节点标志",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        }
                    }
                }
            ]
        },
        "toolbar": {
            "btns": [
                {
                    "type": "add",
                    "target": "dialog",
                    "label": "新增",
                    "toRequest": {
                        "method": "GET",
                        "url": "/views/ismt/ct/ParameterManagement/WorkTasksInformation/WorkTasksAdd.json"
                    },
                    "props": {
                        "title": "工作任务信息新增"
                    }
                }
            ]
        },
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": false,
            "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "ct2027"
                }
            },
            "thead": [
                {
                    "type": "index"
                },
                {
                    "label": "工作任务ID",
                    "prop": "workjob_id"
                },
                {
                    "label": "工作任务描述",
                    "prop": "workjob_desc"
                },
                {
                    "label": "工作流ID",
                    "prop": "workflow_id"
                },
                {
                    "label": "起始节点标志",
                    "prop": "begin_ind",
                    "dict": {
                        "dictType": "MS_E_YESORNO",
                        "dictKey": [
                            "MS_E_YESORNO"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "结束节点标志",
                    "prop": "end_ind",
                    "dict": {
                        "dictType": "MS_E_YESORNO",
                        "dictKey": [
                            "MS_E_YESORNO"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "任务签收方式",
                    "prop": "job_rcv_method",
                    "dict": {
                        "dictType": "E_RECSTYLE",
                        "dictKey": [
                            "E_RECSTYLE"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "操作",
                    "fixed": "right",
                    "width": 200,
                    "btns": [
                        {
                            "type": "look",
                            "label": "详情",
                            "toRequest": {
                                "url": "/views/ismt/ct/ParameterManagement/WorkTasksInformation/work_task_detail.json",
                                "method": "POST"
                            },
                            "props": {
                                "title": "工作流任务信息详情"
                            }
                        },
                        {
                            "type": "edit",
                            "label": "维护",
                            "toRequest": {
                                "url": "/views/ismt/ct/ParameterManagement/WorkTasksInformation/WorkTasksUpdate.json",
                                "method": "GET"
                            },
                            "props": {
                                "title": "工作任务信息维护"
                            }
                        },
                        {
                            "type": "look",
                            "label": "删除",
                            "toRequest": {
                                "url": "/views/ismt/ct/ParameterManagement/WorkTasksInformation/workflow_info_delete.json",
                                "method": "post"
                            },
                            "props": {
                                "title": "工作任务信息删除"
                            }
                        }
                    ]
                }
            ]
        }
    }
}