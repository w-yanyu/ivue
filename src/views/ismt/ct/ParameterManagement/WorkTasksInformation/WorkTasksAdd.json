{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": "",
                    "doRequest": {
                        "url": "/SUMP/call/RPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "ct2029",
                            "operater_ind": "A"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "workjob_id": {
                        "control": "text",
                        "label": "工作任务ID",
                        "width": "200px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入工作任务ID"
                            },
                            {
                                "max": 32,
                                "message": "长度不能超过32个字符"
                            }
                        ]
                    },
                    "workjob_desc": {
                        "control": "text",
                        "label": "工作任务描述",
                        "width": "200px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入工作任务描述"
                            },
                            {
                                "max": 100,
                                "message": "长度不能超过100个字符"
                            }
                        ]
                    }
                },
                {
                    "workflow_id": {
                        "control": "lookup",
                        "edit": false,
                        "label": "工作流ID",
                        "width": "200px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入工作流ID"
                            },
                            {
                                "max": 32,
                                "message": "长度不能超过32个字符"
                            }
                        ],
                        "keyField": {
                            "workflow_id": "workflow_id"
                        },
                        "toRequest": {
                            "url":"/views/ismt/ct/ParameterManagement/WorkflowDefinitionMaintenance/WorkflowDefinitionManager_lookup.json",
                            "method": "post",
                            "params": "${{form.workflow_id}}"
                        },
                        "props":{
                            "title":"工作流ID查询"
                        }
                    },
                    "begin_ind": {
                        "control": "select",
                        "label": "起始节点标志",
                        "width": "200px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入起始节点标志"
                            }
                        ],
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        },
                        "events": {
                            "condition": {
                                "Y": {
                                    "control": {
                                        "job_rcv_method": {
                                            "disabled": true,
                                            "value": "A",
                                            "rules": [
                                                {
                                                    "required": true,
                                                    "message": "请选择任务签收方式"
                                                }
                                            ]
                                        }
                                    }
                                },
                                "N": {
                                    "control": {
                                        "job_rcv_method": {
                                            "disabled": false,
                                            "value": "",
                                            "rules": [
                                                {
                                                    "required": true,
                                                    "message": "请选择任务签收方式"
                                                }
                                            ]
                                        }
                                    }
                                }
                            }
                        }
                    }
                },
                {
                    "end_ind": {
                        "control": "select",
                        "label": "结束节点标志",
                        "width": "200px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入结束节点标志"
                            }
                        ],
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        }
                    },
                    "job_rcv_method": {
                        "control": "select",
                        "label": "任务签收方式",
                        "width": "200px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入任务签收方式"
                            }
                        ],
                        "dict": {
                            "dictType": "E_RECSTYLE",
                            "dictKey": [
                                "E_RECSTYLE"
                            ],
                            "format": "label"
                        }
                    }
                }
            ]
        }
    }
}
