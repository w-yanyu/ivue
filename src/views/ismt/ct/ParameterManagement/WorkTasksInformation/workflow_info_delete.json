{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "doRequest": {
                        "url": "/SUMP/call/RPCCall",
                        "method": "post",
                        "params": {
                            "servicecode": "ct2034",
                            "operater_ind": "D"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "workjob_id": {
                        "control": "string",
                        "label": "工作任务ID"
                    },
                    "workjob_desc": {
                        "control": "string",
                        "label": "工作任务描述"
                    }
                },
                {
                    "workflow_id": {
                        "control": "string",
                        "label": "工作流ID"
                    },
                    "begin_ind": {
                        "control": "string",
                        "label": "起始节点标志",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        }
                    },
                    "end_ind": {
                        "control": "string",
                        "label": "结束节点标志",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        }
                    },
                    "job_rcv_method": {
                        "control": "string",
                        "label": "任务签收方式",
                        "dict": {
                            "dictType": "E_RECSTYLE",
                            "dictKey": [
                                "E_RECSTYLE"
                            ],
                            "format": "label"
                        }
                    }
                }
            ]
        }
    }
}