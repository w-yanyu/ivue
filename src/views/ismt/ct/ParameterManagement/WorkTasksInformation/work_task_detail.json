{
    "module": "v-realonlyForm-btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "controls": [
                {
                    "workjob_id": {
                        "control": "string",
                        "label": "工作任务ID",
                        "width": "150px"
                    },
                    "workjob_desc": {
                        "control": "string",
                        "label": "工作任务描述",
                        "width": "150px",
                        "disabled": true
                    }
                },
                {
                    "workflow_id": {
                        "control": "string",
                        "label": "工作流ID",
                        "width": "150px"
                    },
                    "begin_ind": {
                        "control": "string",
                        "label": "起始节点标志",
                        "width": "150px",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        }
                    }
                },
                {
                    "end_ind": {
                        "control": "string",
                        "label": "结束节点标志",
                        "width": "150px",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        }
                    },
                    "job_rcv_method": {
                        "control": "string",
                        "label": "任务签收方式",
                        "width": "150px",
                        "dict": {
                            "dictType": "E_RECSTYLE",
                            "dictKey": [
                                "E_RECSTYLE"
                            ],
                            "format": "label"
                        }
                    }
                }
            ]
        }
    }
}