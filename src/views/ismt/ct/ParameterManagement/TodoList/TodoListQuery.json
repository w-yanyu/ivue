{
    "module": "v_search_btn_datagrid",
    "layout": {
        "datagrid": {
            "scope": "searchTable",
            "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "ct2018"
                }
            },
            "thead": [
                {
                    "type": "index"
                },
                {
                    "label": "创建人",
                    "prop": "creator_id"
                },
                {
                    "label": "申请日期",
                    "prop": "begin_time",
                    "type": "dateTimePicker"
                },
                {
                    "label": "交易名称",
                    "prop": "trxn_desc"
                },
                {
                    "label": "签收人",
                    "prop": "signer"
                },
                {
                    "label": "任务签收方式",
                    "prop": "job_rcv_method",
                    "dict": {
                        "dictType": "E_RECSTYLE",
                        "dictKey": [
                            "E_RECSTYLE"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "工作路线种类",
                    "prop": "route_type",
                    "dict": {
                        "dictType": "E_LINETYPE",
                        "dictKey": [
                            "E_LINETYPE"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "操作",
                    "btns": [
                        {
                            "type": "edit",
                            "label": "审批",
                            "toRequest": {
                                "url": "/views/ismt/ct/ParameterManagement/TodoList/todoListDetail.json",
                                "method": "get",
                                "width": "900px"
                            },
                            "props": {
                                "title": "审批工作流"
                            }
                        }
                    ]
                }
            ]
        }
    }
}