{
    "module": "v-inputDataForm-btn",
    "layout": {
        "form": {
            "initRequest": {
                "url": "/views/ismt/test/testNext.json",
                "method": ""
            },
            "doRequest": {
                "url": "/demo/submitData",
                "method": ""
            },
            "controls": [
                {
                    "id": {
                        "control": "text",
                        "label": "编号",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入编号"
                            }
                        ]
                    },
                    "name": {
                        "control": "text",
                        "label": "姓名",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入姓名"
                            }
                        ]
                    }
                },
                {
                    "sex": {
                        "control": "select",
                        "label": "性别",
                        "dict": {
                            "dictType": "MS_E_GENDER",
                            "dictKey": [
                                "MS_E_GENDER"
                            ],
                            "format": "label"
                        }
                    },
                    "age": {
                        "control": "text",
                        "label": "年龄"
                    }
                }
            ]
        },
        "alert": {
            "title": "备注",
            "description": [
                "表单初始化数据，获取后台数据，初始化数据填充到对应控件中！"
            ],
            "listStyle": "disc"
        }
    }
}