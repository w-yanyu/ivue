{
    "module": "v_search_btn_datagrid",
    "layout": {
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": false,
            "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "ct2018"
                }
            },
            "pagination": {
                "show": true
            },
            "thead": [
                {
                    "label": "待办事项ID",
                    "prop": "schedule_id"
                },
                {
                    "label": "工作任务ID",
                    "prop": "workjob_id"
                },
                {
                    "label": "工作任务描述",
                    "prop": "workjob_desc"
                },
                {
                    "label": "工作流实例ID",
                    "prop": "ince_id"
                },
                {
                    "label": "工作流ID",
                    "prop": "workflow_id"
                },
                {
                    "label": "任务签收方式",
                    "prop": "job_rcv_method",
                    "dict": {
                        "dictType": "E_RECSTYLE",
                        "dictKey": [
                            "E_RECSTYLE"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "签收人",
                    "prop": "signer"
                },
                {
                    "label": "工作路线种类",
                    "prop": "route_type",
                    "dict": {
                        "dictType": "E_LINETYPE",
                        "dictKey": [
                            "E_LINETYPE"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "操作",
                    "btns": [
                        {
                            "type": "edit",
                            "label": "修改",
                            "toRequest": {
                                "url": "/views/ismt/ct/ParameterManagement/TodoList/todoListDetail.json",
                                "method": "GET"
                            },
                            "props": {
                                "title": "工作流定义修改"
                            }
                        }
                    ]
                }
            ]
        }
    }
}