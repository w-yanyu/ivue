{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": "",
                    "doRequest": {
                        "url": "/SUMP/call/RPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "ct3270"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "cust_manager_id": {
                        "control": "lookup",
                        "edit": true,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入客户经理编号"
                            }
                        ],
                        "label": "客户经理编号",
                        "keyField": {
                            "cust_manager_id": "user_id",
                            "cust_manager_name": "user_name",
                            "branch_id": "branch_id"

                        },
                        "toRequest": {
                            "url": "/views/ismt/ct/user/user-manager-lookup.json",
                            "method": "post"
                        },
                        "props": {
                            "width": "80%",
                            "title": "用户信息查询"
                        },
                        "format": "label"

                    },
                    "cust_manager_name": {
                        "label": "客户经理姓名",
                        "control": "text",
                        "rules": [{
                            "required": true,
                            "message": "请输入客户经理姓名"
                        }]
                    },
                    "branch_id": {
                        "label": "机构号",
                        "control": "text",
                        "rules": [{
                            "required": true,
                            "message": "请输入机构号"
                        }]
                    },
                    "cust_manager_type": {
                        "label": "客户经理类型",
                        "control": "select",
                        "dict": {
                            "dictType": "CT_E_MANAGER_TYPE",
                            "dictKey": ["CT_E_MANAGER_TYPE"]
                        },
                        "rules": [{
                            "required": true,
                            "message": "请选择客户经理类型"
                        }]
                    },
                    "cust_manager_level": {
                        "label": "客户经理级别",
                        "control": "select",
                        "dict": {
                            "dictType": "CT_E_MANAGER_LEVEL",
                            "dictKey": ["CT_E_MANAGER_LEVEL"]
                        },
                        "rules": [{
                            "required": true,
                            "message": "请选择客户经理级别"
                        }]
                    }

                }
            ]
        }
    }
}
