{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "controls": [
                {
                    "cust_manager_id": {
                        "label": "客户经理编号",
                        "control": "text",
                        "rules": [{
                            "required": false,
                            "message": "请输入客户经理编号"
                        }]
                    },
                    "cust_manager_name": {
                        "label": "客户经理姓名",
                        "control": "text",
                        "rules": [{
                            "required": false,
                            "message": "请输入客户经理姓名"
                        }]
                    },
                    "branch_id": {
                        "label": "机构号",
                        "control": "text",
                        "rules": [{
                            "required": false,
                            "message": "请输入机构号"
                        }]
                    },
                    "cust_manager_type": {
                        "label": "客户经理类型",
                        "control": "select",
                        "dict": {
                            "dictType": "CT_E_MANAGER_TYPE",
                            "dictKey": ["CT_E_MANAGER_TYPE"]
                        }
                    },
                    "cust_manager_level": {
                        "label": "客户经理级别",
                        "control": "select",
                        "dict": {
                            "dictType": "CT_E_MANAGER_LEVEL",
                            "dictKey": ["CT_E_MANAGER_LEVEL"]
                        }
                    }
                }
            ]
        },
        "toolbar": {
            "btns": [
                {
                    "type": "add",
                    "target": "dialog",
                    "label": "新增",
                    "toRequest": {
                        "method": "GET",
                        "url": "/views/ismt/ct/custManager/customer_manager_add.json"
                    },
                    "props": {
                        "title": "客户经理信息新增"
                    }
                }
            ]
        },
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": false,
            "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "ct3273"
                }
            },
            "pagination": {
                "show": true
            },
            "thead": [
                {
                    "type": "index"
                },
                {
                    "label": "客户经理编号",
                    "prop": "cust_manager_id"
                },
                {
                    "label": "客户经理姓名",
                    "prop": "cust_manager_name"
                },
                {
                    "label": "机构号",
                    "prop": "branch_id"
                },
                {
                    "label": "客户经理类型",
                    "prop": "cust_manager_type",
                    "dict": {
                        "dictType": "CT_E_MANAGER_TYPE",
                        "dictKey": [
                            "CT_E_MANAGER_TYPE"
                        ],
                        "format": "value-label"
                    }
                },
                {
                    "label": "客户经理级别",
                    "prop": "cust_manager_level",
                    "dict": {
                        "dictType": "CT_E_MANAGER_LEVEL",
                        "dictKey": [
                            "CT_E_MANAGER_LEVEL"
                        ],
                        "format": "value-label"
                    }
                },
                {
                    "label": "是否有效",
                    "prop": "enable_ind",
                    "dict": {
                        "dictType": "MS_E_YESORNO",
                        "dictKey": [
                            "MS_E_YESORNO"
                        ],
                        "format": "value-label"
                    }
                },
                
                {
                    "label": "操作",
                    "fixed": "right",
                    "width": 220,
                    "align": "center",
                    "btns": [
                        {
                            "type": "edit",
                            "label": "维护",
                            "toRequest": {
                                "url": "/views/ismt/ct/custManager/customer_manager_update.json",
                                "method": "GET"
                            },
                            "props": {
                                "title": "角色维护"
                            }
                        },
                        {
                            "type": "delete",
                            "label": "删除",
                            "doRequest": {
                              "url": "/SUMP/icmcall/icmRPCCall",
                              "method": "POST",
                              "params": {
                                "servicecode": "ct3272"
                              }
                            }
                          }
                    ]
                }
            ]
        }
    }
}