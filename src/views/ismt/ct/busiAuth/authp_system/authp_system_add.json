{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "btns": [
        {
          "type": "submit",
          "label": "保存",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "ct2405",
              "appId":"${{params.appId}}"
            }
          }
        }
      ],
      "controls": [
          {
              "authp_system_code": {
                  "control": "text",
                  "label": "授权体系编号",
                  "rules": [
                      {
                          "required": true,
                          "message": "请输入授权体系编号"
                      },
                      {
                          "max": 64,
                          "message": "长度不能超过64个字符"
                      }
                  ]
              },
              "authp_system_name": {
                  "control": "text",
                  "label": "授权体系名称",
                  "rules": [
                      {
                          "required": true,
                          "message": "请输入授权体系名称"
                      },
                      {
                          "max": 500,
                          "message": "长度不能超过500个字符"
                      }
                  ]
              },
              "dimension_busi_type": {
                "control": "select",
                  "label": "授权类型",
                  "multiple": true,
                  "selectFirstValue":false,
                  "dict": {
                    "dictType": "CT_E_APPL_TYPE",
                    "dictKey": [
                        "CT_E_APPL_TYPE"
                    ],
                    "format": "label"
                },
                "rules": [
                  {
                    "required": true,
                    "message": "请选择授权类型"
                  }
                ]
            },
            "authp_system_status": {
              "control": "hidden",
                "label": "状态",
                "selectFirstValue":false,
                "dict": {
                  "dictType": "CT_E_AUTH_STATUS",
                  "dictKey": [
                      "CT_E_AUTH_STATUS"
                  ],
                  "format": "label"
              },
              "#rules": [
                  {
                    "required": true,
                    "message": "请选择授权状态"
                  }
                ]
            },
            "all_exe_flag": {
              "control": "select",
              "format": "label",
              "label": "是否全部执行标识",
              "dict": {
                "format": "label",
                "dictKey": [
                    "MS_E_YESORNO"
                ],
                "dictType": "MS_E_YESORNO"
              },
              "rules": [
                {
                  "required": true,
                  "message": "请输入是否全部执行标识"
                }
              ]
            },
            "effect_date": {
                "control": "dateTimePicker",
                "format": "yyyyMMdd",
                "label": "生效日期",
                "rules": [
                    {
                      "required": true,
                      "message": "请输入生效日期"
                    }
                ]
            },
            "invalid_date": {
                "control": "dateTimePicker",
                "label": "失效日期",
                "format": "yyyyMMdd",
                "rules": [
                    {
                      "required": true,
                      "message": "请输入失效日期"
                    }
                ]
            }
          }
      ]
    }
  }
}
