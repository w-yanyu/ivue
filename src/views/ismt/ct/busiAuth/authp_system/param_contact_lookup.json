{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "doRequest": {
          "url": "",
          "method": "POST",
          "params": {}
      },
      "controls": [
        {
          "field_name": {
            "control": "text",
            "label": "业务字段名称",
            "rules": [
              {
                "max": 64,
                "message": "长度不能超过64个字符"
              }
            ]
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ct2435",
          "authp_system_code": "${{params.authp_system_code}}",
          "appId":"${{params.appId}}"
        }
      },
      "pagination": {
        "show": true,
        "pageSize": 10
      },
      "thead": [
        {
          "label": "操作",
          "align": "center",
          "fixed": "left",
          "btns": [
              {
                  "type": "bringback",
                  "label": "选择"
              }
          ]
      },
        {
          "label": "业务指标名称",
          "prop": "dimension_name"
        },
        {
          "label": "字段名称",
          "prop": "field_name"
        },
        {
          "label": "返回值类型",
          "prop": "retrun_value_type",
          "dict": {
            "dictType": "CT_E_RESULT_TYPE",
            "dictKey": [
                "CT_E_RESULT_TYPE"
            ],
            "format": "label"
          }
      },
      {
          "label": "字符字段中文值",
          "prop": "string_en_value",
          "align": "center"

      }
      ]
    }
  }
}
