{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "controls": [],
            "btns": []  
        },
        "toolbar": {
            "btns": [
                {
                    "type": "add",
                    "label": "关联业务参数",
                    "toRequest": {
                        "method": "GET",
                        "url": "/views/ismt/ct/busiAuth/authp_system/param_input.json",
                        "params": {
                            "authp_system_code": "${{params.authp_system_code}}",  
                            "auth_version": "${{params.auth_version}}",                        
                            "appId":"${{params.appId}}"
                        }
                        
                    },
                    "props": {
                        "title": "关联业务参数"
                    }
                }
            ]
        },
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": true,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "ct2434",
                    "authp_system_code": "${{params.authp_system_code}}",
                    "auth_version": "${{params.auth_version}}",
                    "appId":"${{params.appId}}"
                }
            },
            "pagination": {
                "show": false,
                "pageSize": 10
            },
            "thead": [
                {
                    "label": "业务指标名称",
                    "prop": "dimension_name",
                    "align": "center"

                },
                {
                    "label": "字段名称",
                    "prop": "field_name",
                    "align": "center"

                },
                {
                    "label": "业务指标类型",
                    "prop": "dimension_type",
                    "dict": {
                      "dictType": "CT_E_DIMENSION_TYPE",
                      "dictKey": [
                          "CT_E_DIMENSION_TYPE"
                      ],
                      "format": "label"
                    }
                },
                {
                    "label": "返回值类型",
                    "prop": "retrun_value_type",
                    "dict": {
                      "dictType": "CT_E_RESULT_TYPE",
                      "dictKey": [
                          "CT_E_RESULT_TYPE"
                      ],
                      "format": "label"
                    }
                },
                {
                    "label": "字符字段中文值",
                    "prop": "string_en_value",
                    "align": "center"

                },
                {
                    "label": "操作",
                    "width": 250,
                    "align":"center",
                    "fixed": "right",
                    "btns": [
                        {
                            "type": "edit",
                            "label": "删除",
                            "doRequest": {
                                "url": "/SUMP/icmcall/icmRPCCall",
                                "method": "POST",
                                "params": {
                                    "servicecode": "ct2437",
                                    "authp_system_code": "${{params.authp_system_code}}",
                                    "auth_version": "${{params.auth_version}}",
                                    "appId":"${{params.appId}}"
                                }
                            },
                            "props": {
                                "title": "关联业务参数信息删除"
                            },
                            "confirm":{
                                "title":"提示信息",
                                "msg":"确认要撤回吗？",
                                "confirmButtonText":"确定",
                                "cancelButtonText":"取消"
                            }
                        }
                    ]
                }
            ]
            
        }
    }
  }