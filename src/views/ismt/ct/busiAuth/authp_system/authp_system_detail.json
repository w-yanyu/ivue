{
    "module": "v_inputDataForm_btn",
    "layout": {
      "form": {
        "initRequest": "${{params}}",
        "btns": [
          
        ],
        "controls": [
            {
              "authp_system_code": {
                "control": "string",
                "label": "授权体系编号",
                "width": "100px"
              },
              "auth_version": {
                "control": "string",
                "label": "授权体系版本",
                "width": "100px"
              },
              "authp_system_name": {
                "control": "string",
                "label": "授权体系名称",
                "width": "100px"
              },
              "dimension_busi_type": {
                "control": "select",
                "label": "授权类型",
                "disabled":"true",
                "width": "100px",
                "multiple": true,
                "dict": {
                  "dictType": "CT_E_APPL_TYPE",
                  "dictKey": [
                    "CT_E_APPL_TYPE"
                  ],
                  "format": "label"
                }
              },
              "authp_system_status": {
                "control": "select",
                "label": "状态",
                "disabled":"true",
                "width": "100px",
                "dict": {
                  "dictType": "CT_E_AUTH_STATUS",
                  "dictKey": [
                    "CT_E_AUTH_STATUS"
                  ],
                  "format": "label"
                }
              },
              "all_exe_flag": {
                "control": "select",
                "format": "label",
                "label": "是否全部执行标识",
                "disabled":"true",
                "width": "100px",
                "dict": {
                  "format": "label",
                  "dictKey": [
                      "MS_E_YESORNO"
                  ],
                  "dictType": "MS_E_YESORNO"
                }
              },
              "effect_date": {
                "control": "dateTimePicker",
                "format": "yyyyMMdd",
                "label": "生效日期",
                "disabled":"true",
                "width": "100px"
              },
              "invalid_date": {
                "control": "dateTimePicker",
                "format": "yyyyMMdd",
                "label": "失效日期",
                "disabled":"true",
                "width": "100px"
              }
            }
        ]
      }
    }
  }
  