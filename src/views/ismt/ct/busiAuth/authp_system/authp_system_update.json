{
    "module": "v_inputDataForm_btn",
    "layout": {
      "form": {
        "initRequest": "${{params}}",
        "btns": [
          {
            "type": "submit",
            "label": "提交",
            "icon": "",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2406",
                "appId":"${{params.appId}}"
              }
            }
          }
        ],
        "controls": [
            {
              "authp_system_code": {
                "control": "string",
                "label": "授权体系编号",
                "width": "100px"
              },
              "auth_version": {
                "control": "string",
                "label": "授权体系版本",
                "width": "100px"
              },
              "authp_system_name": {
                "control": "text",
                "label": "授权体系名称",
                "width": "100px",
                "rules": [
                  {
                    "required": true,
                    "message": "请输入授权体系名称"
                  },
                  {
                    "max": 500,
                    "message": "长度不能超过500个字符"
                  }
                ]
              },
              "dimension_busi_type": {
                "control": "select",
                "label": "授权类型",
                "width": "100px",
                "multiple": true,
                "dict": {
                  "dictType": "CT_E_APPL_TYPE",
                  "dictKey": [
                    "CT_E_APPL_TYPE"
                  ],
                  "format": "label"
                },
                "rules": [
                  {
                    "required": true,
                    "message": "请选择授权类型"
                  }
                ]
              },
              "authp_system_status": {
                "control": "select",
                "label": "状态",
                "width": "100px",
                "dict": {
                  "dictType": "CT_E_AUTH_STATUS",
                  "dictKey": [
                    "CT_E_AUTH_STATUS"
                  ],
                  "format": "label"
                },
                "rules": [
                  {
                    "required": true,
                    "message": "请选择状态"
                  }
                ]
              },
              "all_exe_flag": {
                "control": "select",
                "format": "label",
                "label": "是否全部执行标识",
                "width": "100px",
                "dict": {
                  "format": "label",
                  "dictKey": [
                      "MS_E_YESORNO"
                  ],
                  "dictType": "MS_E_YESORNO"
                },
                "rules": [
                  {
                    "required": true,
                    "message": "请输入是否全部执行标识"
                  }
                ]
              },
              "effect_date": {
                "control": "dateTimePicker",
                "format": "yyyyMMdd",
                "label": "生效日期",
                "width": "100px",
                "rules": [
                  {
                    "required": true,
                    "message": "请输入生效日期"
                  }
                ]
              },
              "invalid_date": {
                "control": "dateTimePicker",
                "format": "yyyyMMdd",
                "label": "失效日期",
                "width": "100px",
                "rules": [
                  {
                    "required": true,
                    "message": "请输入失效日期"
                  }
                ]
              }
            }
        ]
      }
    }
  }
  