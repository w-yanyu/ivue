{
    "module": "v_search_btn_datagrid",
    "layout": {
        "toolbar": {
            "btns": [
            ]
        },
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": true,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "ct2412",
                    "authp_system_code": "${{params.authp_system_code}}",
                    "auth_version": "${{params.auth_version}}",
                    "role_id": "${{params.role_id}}",
                    "appId":"${{params.appId}}"
                }
            },
            "pagination": {
                "show": true,
                "pageSize": 10
            },
            "thead": [
                {
                    "type": "index"
                },
                {
                    "label": "规则编号",
                    "prop": "authp_rule_code",
                    "width": 150

                },
                
                {
                    "label": "规则描述", 
                    "prop": "remark"
                }
            ]
            
        }
    }
  }