{
    "module": "v_inputDataForm_btn",
    "layout": {
      "form": {
        "initRequest": "${{params}}",
        "btns": [
          {
            "type": "submit",
            "label": "提交",
            "icon": "",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2407",
                "auth_version": "${{params.auth_version}}",
                "appId":"${{params.appId}}"
              }
            }
          }
        ],
        "controls": [
          {
            "authp_system_code": {
              "control": "string",
              "label": "授权体系编号",
              "width": "140px"
            },
            "authp_system_name": {
              "control": "string",
              "rules": [
                {
                  "max": 500,
                  "message": "长度不能超过500个字符"
                }
              ],
              "label": "授权体系名称",
              "width": "140px"
            }
          }
        ]
      }
    }
  }
  