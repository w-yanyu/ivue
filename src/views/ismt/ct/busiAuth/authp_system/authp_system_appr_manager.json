{
  "module": "v_search_btn_datagrid",
  "layout": {
      "form": {
          "doRequest": {
              "url": "",
              "method": "POST",
              "params": {}
          },
          "controls": [
              {
                "appId": {
                    "control": "select",
                    "label": "业务模块",
                    "selectFirstValue":true,
                    "dict": {
                        "dictType": "MS_APPID",
                        "dictKey": [
                            "MS_APPID"
                        ],
                        "format": "value-label"
                    },
                    "rules": [
                        {
                            "required": true,
                            "message": "请选择业务模块"
                        }]
                }
              },
              {
                  "authp_system_code": {
                      "control": "text",
                      "label": "授权体系编号",
                      "rules": [
                          {
                              "max": 64,
                              "message": "长度不能超过64个字符"
                          }
                      ]
                  },
                  "authp_system_name": {
                      "control": "text",
                      "label": "授权体系名称",
                      "rules": [
                          {
                              "max": 500,
                              "message": "长度不能超过500个字符"
                          }
                      ]
                  },
                  "dimension_busi_type": {
                    "control": "select",
                      "label": "授权类型",
                      "selectFirstValue":false,
                      "dict": {
                        "dictType": "CT_E_APPL_TYPE",
                        "dictKey": [
                            "CT_E_APPL_TYPE"
                        ],
                        "format": "label"
                    }
                  }
              }
          ]
      },
      "toolbar": {
          "btns": [
              
          ]
      },
     "datagrid": {
          "scope": "searchTable",
          "isInitSearch": false,
          "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                  "servicecode": "ct2408",
                  "appId":"${{form.appId}}",
                  "authp_system_status" : "4"
              }
          },
          "pagination": {
              "show": true,
              "pageSize": 10
          },
          "thead": [
              {
                  "type": "index"
              },
              {
                  "label": "授权体系编号",
                  "prop": "authp_system_code"
              },
              {
                "label": "授权体系版本",
                "prop": "auth_version"
                },
              {
                  "label": "授权体系名称",
                  "prop": "authp_system_name"
              },
              {
                  "label": "授权类型",
                  "prop": "dimension_busi_type_en"                  
              },
              {
                "label": "状态",
                "prop": "authp_system_status",
                "dict": {
                    "dictType": "CT_E_AUTH_STATUS",
                    "format": "label",
                    "dictKey": [
                        "CT_E_AUTH_STATUS"
                    ]
                },
                "format": "label"
              },
              {
                "label": "是否全部执行标识",
                "prop": "all_exe_flag",
                "dict": {
                    "format": "label",
                    "dictKey": [
                        "MS_E_YESORNO"
                    ],
                    "dictType": "MS_E_YESORNO"
                },
                "format": "label"
              },
              {
                  "prop": "effect_date",
                  "label": "生效日期"
              },
              {
                  "label": "失效日期",
                  "prop": "invalid_date"
              },
              {
                  "label": "操作",
                  "width": 200,
                  "align":"center",
                  "fixed": "right",
                  "btns": [
                      {
                        "type": "edit",
                        "label": "详情",
                        "toRequest": {
                            "url": "/views/ismt/ct/busiAuth/authp_system/authp_system_detail_steps.json",
                            "method": "GET",
                            "params": {
                              "appId":"${{form.appId}}"
                            }
                        },
                        "props": {
                            "title": "授权体系信息详情",
                            "width":"80%"
                        }
                      },
                      {
                        "type": "edit",
                        "label": "审核",
                        "toRequest": {
                            "url": "/views/ismt/ct/busiAuth/authp_system/authp_system_appr_steps.json",
                            "method": "GET",
                            "params": {
                              "appId":"${{form.appId}}"
                            }
                        },
                        "props": {
                            "title": "授权体系信息审核",
                            "width":"80%"
                        }
                      }
                  ]
              }
          ]
      }
  }
}