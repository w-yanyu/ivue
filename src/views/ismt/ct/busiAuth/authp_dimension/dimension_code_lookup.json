{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "doRequest": {
                "url": "",
                "method": "POST",
                "params": {}
            },
            "controls": [
                {
                    "dimension_code": {
                        "control": "text",
                        "label": "业务指标编码",
                        "rules": [
                            {
                                "max": 64,
                                "message": "长度不能超过64个字符"
                            }
                        ]
                    },
                    "dimension_name": {
                        "control": "text",
                        "label": "业务指标名称",
                        "rules": [
                            {
                                "max": 500,
                                "message": "长度不能超过500个字符"
                            }
                        ]
                    },
                    "dimension_type": {
                      "control": "string",
                        "label": "业务指标类型",
                        "selectFirstValue":false,
                        "dict": {
                          "dictType": "CT_E_DIMENSION_TYPE",
                          "dictKey": [
                              "CT_E_DIMENSION_TYPE"
                          ],
                          "format": "label"
                      }
                  },
                  "dimension_busi_type": {
                    "control": "select",
                      "label": "所属业务类型",
                      "selectFirstValue":false,
                      "dict": {
                        "dictType": "CT_E_APPL_TYPE",
                        "dictKey": [
                            "CT_E_APPL_TYPE"
                        ],
                        "format": "label"
                    }
                  }
                }
            ]
        },
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": false,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "ct2404",
                    "dimension_type": "${{params.dimension_type}}",
                    "appId":"${{params.appId}}"
                }
            },
            "pagination": {
                "show": true,
                "pageSize": 10
            },
            "thead": [
                {
                    "label": "操作",
                    "align": "center",
                    "fixed": "left",
                    "btns": [
                        {
                            "type": "bringback",
                            "label": "选择"
                        }
                    ]
                },
                {
                    "label": "业务指标编码",
                    "prop": "dimension_code"
                },
                {
                    "label": "业务指标名称",
                    "prop": "dimension_name"
                },
                
                {
                  "label": "所属业务类型",
                  "prop": "dimension_busi_type",
                  "dict": {
                      "dictType": "CT_E_APPL_TYPE",
                      "format": "label",
                      "dictKey": [
                          "CT_E_APPL_TYPE"
                      ]
                  },
                  "format": "label"
                },
                {
                    "prop": "achive_type",
                    "label": "获取方式",
                    "dict": {
                        "dictType": "CT_E_MANNER",
                        "dictKey": [
                            "CT_E_MANNER"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "服务路径",
                    "prop": "service_path"
                },
                {
                  "label": "所属表",
                  "prop": "belonging_table"
                },
                {
                    "label": "字段名称",
                    "prop": "field_name"
                },
                {
                  "label": "返回值类型",
                  "prop": "retrun_value_type",
                  "dict": {
                    "dictType": "CT_E_RESULT_TYPE",
                    "dictKey": [
                        "CT_E_RESULT_TYPE"
                    ],
                    "format": "label"
                  }
                },
                {
                  "label": "查询条件",
                  "prop": "query_condition"
                }
            ]
        }
    }
  }