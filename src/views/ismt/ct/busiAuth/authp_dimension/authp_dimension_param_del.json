{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "ct2427",
              "appId":"${{params.appId}}"
            }
          }
        }
      ],
      "controls": [
        {
          "field_code": {
            "control": "string",
            "label": "字段代码",
            "width": "140px"
          },
          "field_name": {
            "control": "string",
            "label": "字段名",
            "width": "140px"
          },
          "dimension_code": {
            "control": "string",
            "label": "业务指标编码",
            "width": "140px"
          },
          "retrun_value_type": {
            "control": "string",
            "label": "返回值类型",
            "width": "140px",
            "dict": {
              "dictType": "CT_E_RESULT_TYPE",
              "dictKey": [
                  "CT_E_RESULT_TYPE"
              ],
              "format": "label"
            }
          }
        }
      ]
    }
  }
}
