{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "controls": [],
            "btns": []  
        },
        "toolbar": {
            "btns": [
            ]
        },
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": true,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "ct2418",
                    "authp_system_code": "${{params.authp_system_code}}",
                    "authp_rule_code": "${{params.authp_rule_code}}",
                    "auth_version": "${{params.auth_version}}",
                    "appId":"${{params.appId}}"
                }
            },
            "pagination": {
                "show": true,
                "pageSize": 10
            },
            "thead": [
                {
                    "label": "机构号",
                    "prop": "branch_id",
                    "align": "center"
                },
                {
                    "label": "机构名称",
                    "prop": "branch_name",
                    "align": "center"
                }
            ]
        }
    }
  }