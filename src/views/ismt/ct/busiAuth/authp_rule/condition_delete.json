{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "ct2415",
              "appId":"${{params.appId}}"
            }
          }
        }
      ],
      "controls": [
        {
          "authp_system_code": {
            "control": "string",
            "label": "授权体系编号",
            "width": "140px"
          },
          "authp_rule_code": {
            "control": "string",
            "label": "规则编号",
            "width": "140px"
          },
          "dimension_code": {
            "control": "string",
            "label": "业务指标编码",
            "width": "140px"
          },
          "field_code": {
            "control": "string",
            "label": "字段代码",
            "width": "140px"
          }
        }
      ]
    }
  }
}
