{
    "module": "v-inputDataForm-btn",
    "layout": {
        "scope": "templateScope",
        "form": {
            "initRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "ct2409",
                    "authp_system_code":"${{params.authp_system_code}}",
                    "authp_version":"${{params.authp_version}}",
                    "appId":"${{params.appId}}"
                }
            },
            "btns": [],
            "controls": [
                {
                    "authp_system_code": {
                        "control": "string",
                        "label": "授权体系编号"
                    },
                    "authp_rule_code": {
                        "control": "string",
                        "label": "规则编号"
                    },
                    "top": {
                        "control": "dynamic",
                        "span": 24,
                        "toRequest": {
                            "method": "post",
                            "url": "/views/ismt/ct/busiAuth/authp_rule/authp_rule_steps.json",
                            "params": {
                                "authp_system_code": "${{form.authp_system_code}}",
                                "authp_rule_code": "${{form.authp_rule_code}}",
                                "appId":"${{params.appId}}"
                            }
                        }
                    }
                }
            ]
        }
    }
}