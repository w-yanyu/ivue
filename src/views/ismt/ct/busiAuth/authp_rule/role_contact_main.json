{
    "module": "v-inputDataForm-btn",
    "layout": {
        "scope": "templateScope",
        "form": {
            "btns": [],
            "controls": [
                {
                    "top": {
                        "control": "dynamic",
                        "span": 24,
                        "toRequest": {
                            "method": "post",
                            "url": "/views/ismt/ct/busiAuth/authp_rule/role_contact.json",
                            "params": {
                                "authp_system_code": "${{params.authp_system_code}}",
                                "authp_rule_code": "${{params.authp_rule_code}}",
                                "auth_version": "${{params.auth_version}}",
                                "appId":"${{params.appId}}"
                            }
                        },
                        "eventAfter": { 
                            "doParentCallback": true,
                            "doLastCallback": true
                          }
                    }
                }
            ]
        }
    }
}