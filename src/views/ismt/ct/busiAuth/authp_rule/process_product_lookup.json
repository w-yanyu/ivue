{
    "module": "v_search_btn_datagrid",
    "layout": {
      "datagrid": {
        "scope": "searchTable",
        "isInitSearch": true,
        "doRequest": {
          "url": "/SUMP/icmcall/icmRPCCall",
          "method": "POST",
          "params": {
            "servicecode": "bpm0061",
            "appId":"${{params.appId}}"
          }
        },
        "thead": [
          {
            "label": "流程产品编号",
            "prop": "wf_prod_no"
          },
          {
            "label": "流程产品名称",
            "prop": "wf_prod_name"
          },
          {
            "label": "流程定义名称",
            "prop": "wf_define_name"
          },
          {
            "label": "流程产品类型",
            "width": 120,
            "prop": "wf_prod_tp",
            "dict": {
              "dictType": "BPM_E_WFPDTP",
              "dictKey": [
                "BPM_E_WFPDTP"
              ],
              "format": "value-label"
            }
          },
  
          {
            "label":"版本号",
            "prop":"data_version",
            "width": 80
          },
          {
            "label":"修改日期",
            "prop":"data_update_time"
          },
          {
            "label":"修改人",
            "prop":"data_update_user"
          },
          {
            "label": "操作",
            "align": "center",
            "fixed": "right",
            "btns": [{
              "type": "bringback",
              "label": "选择"
            }]
          }
        ]
      }
    }
  }
  