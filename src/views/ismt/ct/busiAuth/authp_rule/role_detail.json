{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "controls": [],
            "btns": []  
        },
        "toolbar": {
            "btns": []
        },
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": true,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "ct2431",
                    "authp_system_code": "${{params.authp_system_code}}",
                    "auth_version": "${{params.auth_version}}",
                    "appId":"${{params.appId}}"
                }
            },
            "pagination": {
                "show": false,
                "pageSize": 10
            },
            "thead": [
                {
                    "label": "角色编号",
                    "prop": "role_id",
                    "align": "center"

                },
                {
                    "label": "关联审批角色名称",
                    "prop": "role_name",
                    "align": "center"

                }
            ]
            
        }
    }
  }