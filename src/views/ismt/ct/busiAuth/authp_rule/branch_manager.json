{
    "module": "lr_transferTable",
    "layout": {
      "toolbar": {
        "btns": [
            
        ]
    },
      "transfer": {
        "height":"400px",
        "left": {
          "filterable":true,
          "title": "可选机构",
          "placeholder": "请输入机构名称",
          "query":["page_template_no","branch_name"],
          "btn":{
            "label": "关联",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode":"ct2417",
                "authp_system_code": "${{params.authp_system_code}}",
                "authp_rule_code": "${{params.authp_rule_code}}",
                "auth_version": "${{params.auth_version}}",
                "appId":"${{params.appId}}"
              }
            }
          },
          "datagrid": {
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2419",
                "authp_system_code": "${{params.authp_system_code}}",
                "authp_rule_code": "${{params.authp_rule_code}}",
                "auth_version": "${{params.auth_version}}",
                "appId":"${{params.appId}}"
              }
            },
            "pagination": {
              "show": true
            },
            "thead": [
              {
                "label": "机构号",
                "prop": "branch_id",
                "align": "center"
              },
              {
                "label": "机构名称",
                "prop": "branch_name",
                "align": "center"
              }
            ]
          }
        },
        "right": {
          "title":"已选机构",
          "filterable":true,
          "placeholder": "请输入页面编号",
          "query":["page_template_no"],
          "btn":{
            "label":"删除",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode":"ct2421",
                "authp_system_code": "${{params.authp_system_code}}",
                "authp_rule_code": "${{params.authp_rule_code}}",
                "auth_version": "${{params.auth_version}}",
                "appId":"${{params.appId}}"
              }
            }
          },
          "datagrid": {
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2418",
                "authp_system_code": "${{params.authp_system_code}}",
                "authp_rule_code": "${{params.authp_rule_code}}",
                "auth_version": "${{params.auth_version}}",
                "appId":"${{params.appId}}"
              }
            },
            "pagination": {
              "show": true
            },
            "checked":"checked",
            "thead": [
              {
                "label": "机构号",
                "prop": "branch_id",
                "control": "text",
                "align": "center"
              },
              {
                "label": "机构名称",
                "prop": "branch_name",
                "align": "center"
              }
            ]
          }
        }
      }
    }
  }
  