{
    "module": "v_search_btn_datagrid",
    "layout": {
        "toolbar": {
            "btns": [
            ]
          },
        "datagrid": {
            "isInitSearch": true,
            "scope": "searchTable",
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "post",
                "params": {
                    "servicecode": "ct2337",
                    "announce_id":"${{params.announce_id}}",
                    "read_ind" : "0"
                }
            },
            "thead": [
                {
                    "label": "用户编号",
                    "prop": "user_id"
                },
                {
                    "label": "用户姓名",
                    "prop": "user_name"
                },
               
                {
                    "label": "查阅时间",
                    "prop": "read_time"
                }
            ],
            "pagination": {
                "show": true
            }
        }
    }
}