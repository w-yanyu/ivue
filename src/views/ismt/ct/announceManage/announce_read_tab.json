{
  "module": "dynamicTabs",
  "layout": {
    "tabs": {
      "tabHeader": [
        {
          "id": "tab1",
          "label": "已读 ",
          "closable": false,
          "disabled": false,
          "icon": "el-icon-date",
          "toRequest": {
            "url": "/views/ismt/ct/announceManage/announce_readed.json",
            "method": "post"
          }
        },
        {
          "id": "tab2",
          "label": "未读",
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "toRequest": {
            "url": "/views/ismt/ct/announceManage/announce_unread.json",
            "method": "post"
          }
        }
      ],
      "defaultActiveKey": "tab1",
      "position": "top",
      "styleClass": "card"
    }
  }
}

