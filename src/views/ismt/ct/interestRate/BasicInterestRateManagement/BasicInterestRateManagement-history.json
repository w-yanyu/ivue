{
    "module": "v_search_btn_datagrid",
    "layout": {
        "initRequest": "${{params}}",
        "toolbar": {
        },
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": false,
            "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "ct3257",
                    "interest_rate_sort":"${{params.interest_rate_sort}}"
                }
            },
            "thead": [
                {
                    "label": "法人代码",
                    "prop": "org_id"
                },
                {
                    "label": "基础利率类型",
                    "prop": "interest_rate_type",
                    "dict": {
                        "dictType": "CT_E_INTEREST_RATE_TYPE",
                        "dictKey": [
                            "CT_E_INTEREST_RATE_TYPE"
                        ],
                        "format": "value-label"
                    }
                },
                {
                    "label": "基础利率代码",
                    "prop": "interest_rate_code"
                },
                {
                    "label": "利率种类",
                    "prop": "interest_rate_category",
                    "dict": {
                        "dictType": "CT_E_INTEREST_RATE_CATEGORY",
                        "dictKey": [
                            "CT_E_INTEREST_RATE_CATEGORY"
                        ],
                        "format": "value-label"
                    }
                },
                {
                    "label": "币种",
                    "prop": "ccy_code",
                    "dict": {
                        "format": "value-label",
                        "dictKey": [
                            "MS_CURRENCY"
                        ],
                        "dictType": "MS_CURRENCY"
                    }
                },
                {
                    "label": "期限",
                    "prop": "term",
                    "dict": {
                        "dictType": "CT_E_TERM",
                        "dictKey": [
                            "CT_E_TERM"
                        ],
                        "format": "value-label"
                    }
                },
                {
                    "label": "利率名称",
                    "prop": "interest_rate_name"
                },
                {
                    "label": "利率性质",
                    "prop": "interest_rate_nature",
                    "dict": {
                        "dictType": "CT_E_INTEREST_RATE_UNIT",
                        "dictKey": [
                            "CT_E_INTEREST_RATE_UNIT"
                        ],
                        "format": "value-label"
                    }
                },
                {
                    "label": "基础利率",
                    "prop": "interest_rate_value"
                },
                {
                    "label": "浮动点数上限",
                    "prop": "upper_limit"
                },
                {
                    "label": "浮动点数下限",
                    "prop": "lower_limit"
                },
                {
                    "label": "生效日期",
                    "type": "dateTimePicker",
                    "format":"yyyy-MM-dd",
                    "prop": "effective_date"
                },
                {
                    "label": "生效时间",
                    "prop": "effective_time"
                },
                {
                    "label": "失效日期",
                    "type": "dateTimePicker",
                    "format":"yyyy-MM-dd",
                    "prop": "expiration_date"
                },
                {
                    "label": "失效时间",
                    "prop": "expiration_time"
                }
            ]
        }
    }
}