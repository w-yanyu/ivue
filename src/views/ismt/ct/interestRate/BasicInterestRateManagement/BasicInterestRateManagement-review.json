{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "controls": [
                {
                    "entry_seq": {
                        "control": "text",
                        "label": "录入流水号",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入录入流水号"
                            }
                        ]
                    }
                }
            ]
        },
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": false,
            "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "selcdl"

                }
            },
            "thead": [
                {
                    "label": "法人代码",
                    "prop": "corpno"
                },
                {
                    "label": "利率类型",
                    "prop": "interest_rate_type"
                },
                {
                    "label": "利率代码",
                    "prop": "interest_rate_code"
                },
                {
                    "label": "利率种类",
                    "prop": "interest_rate_category"
                },
                {
                    "label": "币种",
                    "prop": "ccy_code"
                },
                {
                    "label": "期限",
                    "prop": "term"
                },
                {
                    "label": "利率名称",
                    "prop": "interest_rate_name"
                },
                {
                    "label": "利率性质",
                    "prop": "interest_rate_nature"
                },
                {
                    "label": "利率",
                    "prop": "interest_rate_value"
                },
                {
                    "label": "浮动点数上限",
                    "prop": "upper_limit"
                },
                {
                    "label": "浮动点数下限",
                    "prop": "lower_limit"
                },
                {
                    "label": "生效日期",
                    "prop": "effective_date"
                },
                {
                    "label": "生效时间",
                    "prop": "effective_time"
                },
                {
                    "label": "基础利率序号",
                    "prop": "interest_rate_sort"
                },
                {
                    "label": "交易用户",
                    "prop": "trxn_teller"
                }
            ]
        }
    }
}
