{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": "",
                    "doRequest": {
                        "url": "/SUMP/call/RPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "ct3252"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "org_id": {
                        "control": "string",
                        "label": "法人代码",
                        "width": "150px"
                    },
                    "interest_rate_sort": {
                        "control": "string",
                        "label": "利率序号",
                        "width": "150px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入基础利率序号"
                            }
                        ]
                    }
                },
                {
                    "interest_rate_type": {
                        "control": "string",
                        "label": "利率类型",
                        "width": "150px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入利率类型"
                            }
                        ],
                        "dict": {
                            "dictType": "CT_E_INTEREST_RATE_TYPE",
                            "dictKey": [
                                "CT_E_INTEREST_RATE_TYPE"
                            ],
                            "format": "value-label"
                        }
                    },
                    "interest_rate_code": {
                        "control": "string",
                        "label": "利率代码",
                        "width": "150px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入基础利率代码"
                            }
                        ]
                    }
                },
                {
                    "interest_rate_category": {
                        "control": "string",
                        "label": "利率种类",
                        "width": "150px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入利率种类"
                            }
                        ],
                        "dict": {
                            "dictType": "CT_E_INTEREST_RATE_CATEGORY",
                            "dictKey": [
                                "CT_E_INTEREST_RATE_CATEGORY"
                            ],
                            "format": "value-label"
                        }
                    },
                    "ccy_code": {
                        "control": "string",
                        "label": "币种",
                        "width": "150px",
                        "dict": {
                            "dictType": "MS_CURRENCY",
                            "dictKey": [
                                "MS_CURRENCY"
                            ],
                            "format": "value-label"
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入币种"
                            }
                        ]
                    }
                },
                {
                    "term": {
                        "control": "string",
                        "label": "期限",
                        "width": "150px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入期限"
                            }
                        ],
                        "dict": {
                            "dictType": "CT_E_TERM",
                            "dictKey": [
                                "CT_E_TERM"
                            ],
                            "format": "value-label"
                        }
                    },
                    "interest_rate_name": {
                        "label": "利率名称",
                        "width": "150px",
                        "control": "text",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入利率名称"
                            }
                        ]
                    }
                },
                {
                    "interest_rate_nature": {
                        "control": "string",
                        "label": "利率性质",
                        "width": "150px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入利率性质"
                            }
                        ],
                        "dict": {
                            "dictType": "CT_E_INTEREST_RATE_UNIT",
                            "dictKey": [
                                "CT_E_INTEREST_RATE_UNIT"
                            ],
                            "format": "value-label"
                        }
                    },
                    "upper_limit": {
                        "control": "hidden",
                        "label": "浮动点数上限",
                        "width": "150px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入浮动点数上限"
                            }
                        ]
                    },

                    "lower_limit": {
                        "control": "hidden",
                        "label": "浮动点数下限",
                        "width": "150px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入浮动点数下限"
                            }
                        ]
                    },
                    "interest_rate_value": {
                        "control": "text",
                        "label": "利率",
                        "width": "150px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入基础利率"
                            }
                        ]
                    }
                },
                {
                    "effective_date": {
                        "control": "dateTimePicker",
                        "label": "生效日期",
                        "width": "150px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择生效日期"
                            }
                        ]
                    },
                    "effective_time": {
                        "control": "timePicker",
                        "label": "生效时间",
                        "valueFormat":"HHmmss",
                        "width": "150px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择生效时间"
                            }
                        ]
                    }
                },
                {
                    "entry_seq": {
                        "control": "hidden",
                        "label": "录入流水号",
                        "width": "150px"
                    },
                    "sort_no": {
                        "control": "hidden",
                        "label": "排序号",
                        "width": "150px"
                    }
                }
            ]
        }
    }
}
