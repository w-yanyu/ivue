{
  "module": "v_search_btn_datagrid",
  "pageId": "A0101",
  "layout": {
    "form": {
      "doRequest": {
        "url": "",
        "method": "POST",
        "params": {}
      },
      "controls": [
        {
          "brch_relation_code": {
            "control": "select",
            "label": "机构关系",
            "value": "01",
            "rules": [
              {
                "required": "true",
                "message": "请选择机构关系"
              }
            ],
            "dict": {
              "dictType": "CT_E_BRANCH_RELATION",
              "dictKey": [
                "CT_E_BRANCH_RELATION"
              ],
              "format": "label"
            }
          },
          "user_name": {
            "control": "text",
            "label": "用户姓名",
            "rules": [
              {
                "max": 100,
                "message": "长度不能超过100个字符"
              }
            ]
          }
        },
        {
          "user_status": {
            "control": "select",
            "dict": {
              "dictType": "CT_E_TELLERSTAS",
              "dictKey": [
                "CT_E_TELLERSTAS"
              ],
              "format": "label"
            },
            "label": "用户状态"
          },
          "branch_id": {
            "control": "text",
            "label": "机构号",
            "rules": [
              {
                "max": 10,
                "message": "长度不能超过10个字符"
              }
            ]
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {

          "type": "add",
          "target": "dialog",
          "label": "新增",
          "toRequest": {
            "method": "GET",
            "url": "/views/ismt/ct/user/user-information-add/user-info-add.json",
            "params": {
              "list01": []
            }
          },
          "props": {
            "title": "用户信息新增"
          }
        },
        {
          "type": "export",
          "label": "导出",
          "icon": "iconfont icon-daochu",
          "##doRequest": {
            "url": "/SUMP/rest/file/commExport",
            "method": "post",
            "params": {
              "user_id": "$g{{user_info.userCode}}",
              "servicecode": "ct2042",
              "exportname": "用户信息表"
            }
          },
          "toRequest": {
            "method": "GET",
            "url": "/views/ismt/ct/export/export_appl.json",
            "params": {
              "page_id": "/A01/A0101",
              "tran_code": "ct2042"
            }
          },
          "confirm": {
            "title": "提示信息",
            "msg": "确认导出吗？",
            "confirmButtonText": "确定",
            "cancelButtonText": "取消"
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/rest/auth/userManager",
        "method": "POST",
        "params": {
          "servicecode": "ct2002"
        }
      },
      "pagination": {
        "show": true,
        "pageSize": 10
      },
      "thead": [
        {
          "type": "index"
        },
        {
          "label": "用户ID",
          "prop": "user_id"
        },
        {
          "label": "用户姓名",
          "prop": "user_name"
        },
        {
          "label": "证件类型",
          "prop": "cert_type",
          "dict": {
            "dictType": "MS_E_CRET_TYPE",
            "dictKey": [
              "MS_E_CRET_TYPE"
            ],
            "format": "label"
          }
        },
        {
          "label": "证件号",
          "prop": "cert_no"
        },
        {
          "label": "机构号",
          "prop": "branch_id"
        },
        {
          "label": "所属系统",
          "prop": "belong_system_id",
          "isShow": false
        },
        {
          "label": "用户状态",
          "prop": "user_status",
          "dict": {
            "dictType": "CT_E_TELLERSTAS",
            "dictKey": [
              "CT_E_TELLERSTAS"
            ],
            "format": "label"
          }
        },


        {
          "label": "密码状态",
          "prop": "password_status",
          "dict": {
            "dictType": "CT_E_PASSWDSTAS",
            "dictKey": [
              "CT_E_PASSWDSTAS"
            ],
            "format": "label"
          }
        },

        {
          "label": "登录状态",
          "prop": "login_status",
          "dict": {
            "dictType": "CT_E_LOGSTAS",
            "dictKey": [
              "CT_E_LOGSTAS"
            ],
            "format": "label"
          }
        },

        {
          "label": "操作",
          "fixed": "right",
          "width": 250,
          "align": "center",
          "btns": [
            {
              "authId":"A010101",
              "type": "look",
              "label": "详情",
              "toRequest": {
                "url": "/views/ismt/ct/user/user-manager-detail.json",
                "method": "POST"
              },
              "props": {
                "title": "用户信息详情"
              }
            },
            {
              "type": "edit",
              "label": "维护",
              "toRequest": {
                "url": "/views/ismt/ct/user/user-information-update/user-info-update.json",
                "method": "POST"
              },
              "props": {
                "title": "用户信息维护"
              },
              "show": {
                "prop": "user_status",
                "is": [
                  "N"
                ]
              }
            },
            {
              "type": "look",
              "label": "绑定角色",
              "toRequest": {
                "url": "/views/ismt/ct/user/teller-role-maintain-lookup.json",
                "method": "POST",
                "params":{
                  "user_id":"${{current.user_id}}"
                }
              
              },
              "props": {
                "title": "绑定角色"
              },"show": {
                "prop": "user_status",
                "is": [
                  "N"
                ]
              }
            },
            {
              "type": "delete",
              "label": "强制签退",
              "doRequest": {
                "url": "/SUMP/rest/auth/forceLogout",
                "method": "POST",
                "params": {
                  "servicecode": "ct2008"
                }
              },
              "show": {
                "prop": "login_status",
                "is": [
                  "1"
                ]
              },
              "confirm": {
                "title": "提示信息",
                "msg": "确认强制签退？",
                "confirmButtonText": "确定",
                "cancelButtonText": "取消"
            }
            },
            {
              "type": "edit",
              "label": "解锁",
              "doRequest": {
                "url": "/SUMP/rest/auth/ulkpwd",
                "method": "post",
                "params": {
                  "servicecode": "ct2011"
                }
              },
              "show": {
                "prop": "password_status",
                "is": [
                  "F"
                ]
              }
            },
            {
              "type": "edit",
              "label": "停用",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "post",
                "params": {
                  "servicecode": "ct2503",
                  "staff_no":"${{current.staff_no}}"
                }
              },
              "confirm": {
                "title": "提示信息",
                "msg": "请确认是否停用该用户？",
                "confirmButtonText": "确定",
                "cancelButtonText": "取消"
              },
              "show": {
                "prop": "user_status",
                "is": [
                  "N"
                ]
              }
            },
            {
              "type": "edit",
              "label": "修改密码",
              "toRequest": {
                "url": "@updatePasswordNew",
                "method": "post",
                "params": {
                  "user_id": "${{current.user_id}}"
                }
              },
              "show": {
                "expression": "'${{current.user_id}}' == '$g{{user_info.userCode}}'"
              }
            },
            {
              "type": "edit",
              "label": "重置密码",
              "toRequest": {
                "url": "@ResetPasswordNew",
                "method": "post",
                "params": {
                  "user_id": "${{current.user_id}}"
                }
              },"show": {
                "prop": "user_status",
                "is": [
                  "N"
                ]
              }
            },
            
            {
              "type": "edit",
              "label": "解冻",
              "toRequest": {
                "url": "/views/ismt/ct/user/user-status-unfreeze/user-status-unfreeze.json",
                "method": "POST"
              },
              "props": {
                "title": "解冻"
              },
              "show": {
                "prop": "freeze_ind",
                "is": [
                  "Y"
                ]
              }
            }
          ]
        }
      ]
    }
  }
}