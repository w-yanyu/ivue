{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "controls": [
        {
          "user_id": {
            "control": "text",
            "label": "用户ID",
            "width": "110px",
            "rules": [
              {
                "required": true,
                "message": "请输入用户ID"
              },
              {
                "pattern": "^[0-9a-zA-Z]*$",
                "message": "请输入数字或者字母"
              },
              {
                "max": 32,
                "message": "长度不能超过8个字符"
              }
            ]
          },
          "user_name": {
            "control": "text",
            "label": "用户姓名",
            "width": "110px",
            "rules": [
              {
                "required": true,
                "message": "请输入用户姓名"
              },
              {
                "max": 100,
                "message": "长度不能超过100个字符"
              }
            ]
          }
        },
        {
          "gender": {
            "control": "select",
            "label": "性别",
            "width": "110px",
            "dict": {
              "dictType": "MS_E_GENDER",
              "dictKey": [
                "MS_E_GENDER"
              ],
              "format": "label"
            }
          },
          "e_mail": {
            "control": "text",
            "label": "电子邮箱",
            "width": "110px",
            "rules": [
              {
                "max": 80,
                "message": "长度不能超过80个字符"
              },
              {
                "pattern": "^[a-zA-Z0-9_-]+@[a-zA-Z0-9_-]+(\\.[a-zA-Z0-9_-]+)+$",
                "message": "邮箱格式错误"
              }
            ]
          }
        },
        {
          "user_depart": {
            "control": "text",
            "label": "用户部门",
            "width": "110px",
            "rules": [
              {
                "max": 200,
                "message": "长度不能超过200个字符"
              }
            ]
          },
          "staff_no": {
            "control": "text",
            "label": "员工编号",
            "width": "110px",
            "rules": [
              {
                "required": true,
                "message": "请输入员工编号"
              },
              {
                "max": 32,
                "message": "长度不能超过32个字符"
              }
            ]
          }
        },
        {
          "office_phone_no": {
            "control": "text",
            "label": "办公电话",
            "width": "110px",
            "rules": [
              {
                "max": 20,
                "message": "长度不能超过20个字符"
              },
              {
                "pattern": "^[0-9-]+$",
                "message": "办公电话格式错误"
              }
            ]
          },
          "mobile_no": {
            "control": "text",
            "label": "手机号",
            "width": "110px",
            "rules": [
              {
                "required": true,
                "message": "请输入手机号"
              },
              {
                "max": 11,
                "message": "长度不能超过11个字符"
              },
              {
                "pattern": "^[0-9-]+$",
                "message": "手机号格式错误"
              }
            ]
          }
        },
        {
          "office_adress": {
            "control": "text",
            "label": "办公地址",
            "width": "110px",
            "rules": [
              {
                "max": 100,
                "message": "长度不能超过100个字符"
              }
            ]
          },
          "exec_level": {
            "control": "select",
            "label": "行政级别",
            "width": "110px",
            "dict": {
              "dictType": "CT_E_EXECLEVEL",
              "dictKey": [
                "CT_E_EXECLEVEL"
              ],
              "format": "label"
            }
          }
        },
        {
          "tech_level": {
            "control": "select",
            "label": "技术职别",
            "width": "110px",
            "dict": {
              "dictType": "CT_E_TECHLEVEL",
              "dictKey": [
                "CT_E_TECHLEVEL"
              ],
              "format": "label"
            }
          },
          "cert_type": {
            "control": "select",
            "label": "证件类型",
            "width": "110px",
            "dict": {
              "dictType": "MS_E_CRET_TYPE",
              "dictKey": [
                "MS_E_CRET_TYPE"
              ],
              "format": "value-label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择证件类型"
              }
            ]
          }
        },
        {
          "cert_no": {
            "control": "text",
            "label": "证件号",
            "width": "110px",
            "rules": [
              {
                "required": true,
                "message": "请输入证件号"
              },
              {
                "max": 32,
                "message": "长度不能超过32个字符"
              }
            ]
          },
          "fprint_ind": {
            "control": "select",
            "initShow": false,
            "label": "指纹录入标志",
            "width": "110px",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            },
            "value": "0",
            "rules": [
              {
                "required": true,
                "message": "请输入指纹录入标志"
              }
            ]
          }
        }
      ]
    }
  }
}
