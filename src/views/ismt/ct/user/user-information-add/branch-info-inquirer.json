{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "controls": [
                {
                    "branch_id": {
                        "control": "text",
                        "label": "机构号",
                        "rules": [
                            {
                                "max": 10,
                                "message": "长度不能超过10个字符"
                            }
                        ]
                    },
                    "branch_name": {
                        "control": "text",
                        "label": "机构名称",
                        "rules": [
                            {
                                "max": 100,
                                "message": "长度不能超过100个字符"
                            }
                        ]
                    }
                },
                {
                    "branch_function_class": {
                        "control": "text",
                        "label": "机构职能分类",
                        "rules": [
                            {
                                "max": 8,
                                "message": "长度不能超过8个字符"
                            }
                        ]
                    },
                    "real_branch_ind": {
                        "control": "select",
                        "label": "实体机构标志",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        }
                    }
                }
            ]
        },
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": false,
            "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "ct2142",
                    "page_id": "/views/ismt/ct/branch/brannch-info-manager.json"
                }
            },
            "thead": [
                {
                    "type": "index"
                },
                {
                    "label": "机构号",
                    "prop": "branch_id"
                },
                {
                    "label": "机构名称",
                    "prop": "branch_name"
                },
                {
                    "label": "实体机构标志",
                    "prop": "real_branch_ind",
                    "dict": {
                        "dictType": "MS_E_YESORNO",
                        "dictKey": [
                            "MS_E_YESORNO"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "机构地址",
                    "prop": "branch_address"
                },
                {
                    "label": "机构电话",
                    "prop": "branch_phone"
                },
                {
                    "label": "邮政编码",
                    "prop": "postcode"
                },
                {
                    "label": "联系人",
                    "prop": "contacts_name"
                },
                {
                    "label": "联系人电话",
                    "prop": "contacts_phone"
                },
                {
                    "label": "机构职能分类",
                    "prop": "branch_function_class"
                },
                {
                    "label": "SWIFT代码",
                    "prop": "swift_no"
                },
                {
                    "label": "节假日代码",
                    "prop": "holiday_code"
                },
                {
                    "label": "操作",
                    "fixed": "left",
                    "btns": [
                        {
                            "type": "bringback",
                            "label": "选择"
                        }
                    ]
                }
            ]
        }
    }
}