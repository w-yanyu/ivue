{
  "module": "dynamicTabs",
  "layout": {
    "tabs": {
      "submitBtnPlace": "right",
      "doRequest": {
        "url": "/SUMP/rest/auth/userManager",
        "method": "POST",
        "params": {
          "servicecode": "ct2000",
          "system_id": "core"
        }
      },
      "scope": "templateScope",
      "initDataOnce": true,
      "tabHeader": [
        {
          "id": "baseInfo",
          "label": "用户基础信息",
          "closable": false,
          
          "disabled": false,
          "clickInit": false,
          "icon": "el-icon-date",
          "toRequest": {
            "url": "/views/ismt/ct/user/user-information-add/user-info-add-base.json",
            "method": "post"
          }
        },
        {
          "id": "UserAffiliation",
          "label": "用户隶属信息",
          "name":"user_belong",
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "clickInit": false,
          "toRequest": {
            "url": "/views/ismt/ct/user/user-information-add/user-info-add-Affiliation.json",
            "method": "post"
          }
        }
      ],
      "defaultActiveKey": "baseInfo",
      "position": "top",
      "styleClass": "card"
    }
  }
}