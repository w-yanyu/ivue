{
    "module": "v-form-btn-dynamic",
    "layout": {
        "form": {
            "controls": [
                {
                    "user_password": {
                        "control": "password",
                        "label": "原密码",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入原密码"
                            }
                        ]
                    }
                },
                {
                    "new_user_password": {
                        "control": "password",
                        "label": "新密码",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入新密码"
                            }
                        ]
                    }
                },
                {
                    "confirm_user_password": {
                        "control": "password",
                        "label": "确认密码",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入确认密码"
                            }
                        ]
                    }
                }
            ]
        },
        "toolbar": {
            "btns": [
                {
                    "type": "submit",
                    "label": "修改",
                    "doRequest": {
                        "url": "/SUMP/rest/auth/chgpwd",
                        "method": "post",
                        "params": {
                            "servicecode": "ct2009"
                        }
                    }
                },
                {
                    "type": "reset",
                    "btnStyle": "success",
                    "label": "清空"
                }
            ]
        }
    }
}