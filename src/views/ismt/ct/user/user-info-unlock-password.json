{
    "module": "v-form-btn-dynamic",
    "layout": {
        "form": {
            "controls": [
                {
                    "user_id": {
                        "control": "text",
                        "label": "用户ID",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入用户ID"
                            },
                            {
                                "max": 32,
                                "message": "长度不能超过32个字符"
                            }
                        ],
                        "events": {
                            "doRequest": {
                                "url": "/SUMP/rest/auth/userManager",
                                "method": "POST",
                                "params": {
                                    "servicecode": "ct2022",
                                    "user_id": "${{form.user_id}}"
                                },
                                "resultSet": {
                                    "user_name": "user_name"
                                }
                            }
                        }
                    },
                    "user_name": {
                        "control": "string",
                        "label": "用户姓名",
                        "rules": [
                            {
                                "max": 100,
                                "message": "长度不能超过100个字符"
                            }
                        ]
                    }
                }
            ]
        },
        "toolbar": {
            "btns": [
                {
                    "type": "submit",
                    "label": "解锁",
                    "unplain": true,
                    "doRequest": {
                        "url": "/SUMP//rest/auth/ulkpwd",
                        "method": "post",
                        "params": {
                            "servicecode": "ct2011"
                        }
                    }
                },
                {
                    "type": "reset",
                    "btnStyle": "success",
                    "label": "清空"
                }
            ]
        }
    }
}