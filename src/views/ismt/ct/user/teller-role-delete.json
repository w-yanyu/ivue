{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": "",
                    "doRequest": {
                        "url": "/SUMP/rest/auth/userRole",
                        "method": "POST",
                        "params": {
                            "servicecode": "ct2006",
                            "operater_ind": "D",
                            "system_id": ""
                        }
                    }
                }
            ],
            "controls": [
                {
                    "role_id": {
                        "control": "string",
                        "label": "角色ID",
                        "disabled": "false",
                        "rules": [
                            {
                                "max": 64,
                                "message": "长度不能超过64个字符"
                            }
                        ]
                    },
                    "role_name": {
                        "control": "string",
                        "label": "角色名称",
                        "disabled": "false",
                        "rules": [
                            {
                                "max": 100,
                                "message": "长度不能超过100个字符"
                            }
                        ]
                    }
                },
                {
                    "effect_date": {
                        "control": "dateTimePicker",
                        "disabled": "false",
                        "label": "生效日期",
                        "prop": "effect_date"
                    },
                    "expiry_date": {
                        "control": "dateTimePicker",
                        "disabled": "false",
                        "label": "失效日期",
                        "prop": "expiry_date"
                    }
                },
                {
                    "user_id":{
                        "control":"hidden",
                        "label":"用户id"
                    }
                }
            ]
        }
    }
}