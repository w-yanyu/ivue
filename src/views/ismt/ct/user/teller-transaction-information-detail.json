{
    "module": "v-realonlyForm-btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "controls": [
                {
                    "trxn_seq": {
                        "control": "string",
                        "label": "交易流水",
                        "width": "150px",
                        "rules": [
                            {
                                "max": 32,
                                "message": "长度不能超过32个字符"
                            }
                        ]
                    },
                    "trxn_date": {
                        "control": "dateTimePicker",
                        "label": "交易日期",
                        "width": "150px",
                        "disabled": true
                    }
                },
                {
                    "busi_seq": {
                        "control": "string",
                        "label": "业务流水",
                        "width": "150px",
                        "rules": [
                            {
                                "max": 32,
                                "message": "长度不能超过32个字符"
                            }
                        ]
                    },
                    "trxn_branch": {
                        "control": "string",
                        "label": "交易机构",
                        "width": "150px",
                        "rules": [
                            {
                                "max": 10,
                                "message": "长度不能超过10个字符"
                            }
                        ]
                    }
                },
                {
                    "trxn_desc": {
                        "control": "string",
                        "label": "交易描述",
                        "width": "150px",
                        "rules": [
                            {
                                "max": 100,
                                "message": "长度不能超过100个字符"
                            }
                        ]
                    },
                    "user_status": {
                        "control": "string",
                        "label": "用户状态",
                        "width": "150px"
                    }
                },
                {
                    "trxn_type": {
                        "control": "string",
                        "label": "交易类型",
                        "width": "150px",
                        "dict": {
                            "dictType": "MS_E_TRXNTYPE",
                            "dictKey": [
                                "MS_E_TRXNTYPE"
                            ],
                            "format": "label"
                        }
                    },
                    "password_date": {
                        "control": "string",
                        "label": "密码日期",
                        "width": "150px",
                        "type": "dateTimePicker"
                    }
                },
                {
                    "ct_trxn_code": {
                        "control": "string",
                        "label": "交易码",
                        "width": "150px",
                        "rules": [
                            {
                                "max": 10,
                                "message": "长度不能超过10个字符"
                            }
                        ]
                    },
                    "trxn_teller": {
                        "control": "string",
                        "label": "交易用户",
                        "width": "150px",
                        "rules": [
                            {
                                "max": 32,
                                "message": "长度不能超过32个字符"
                            }
                        ]
                    }
                },
                {
                    "trxn_branch": {
                        "control": "string",
                        "label": "交易机构",
                        "width": "150px"
                    },
                    "financial_trxn_sort": {
                        "control": "string",
                        "label": "金融交易序号",
                        "width": "150px"
                    }
                },
                {
                    "cash_trxn_ind": {
                        "control": "string",
                        "label": "现转标志",
                        "width": "150px",
                        "dict": {
                            "dictType": "E_CASHTRXN",
                            "dictKey": [
                                "E_CASHTRXN"
                            ],
                            "format": "label"
                        }
                    },
                    "trxn_ccy": {
                        "control": "string",
                        "label": "交易币种",
                        "width": "150px"
                    }
                },
                {
                    "trxn_amt": {
                        "control": "string",
                        "label": "交易金额",
                        "width": "150px",
                        "rules": [
                            {
                                "max": 21,
                                "message": "长度不能超过21个字符"
                            }
                        ]
                    },
                    "trxn_acct_no": {
                        "control": "string",
                        "label": "交易账号",
                        "width": "150px",
                        "rules": [
                            {
                                "max": 35,
                                "message": "长度不能超过35个字符"
                            }
                        ]
                    }
                },
                {
                    "trxn_acct_name": {
                        "control": "string",
                        "label": "账户名称",
                        "width": "150px"
                    },
                    "debit_credit": {
                        "control": "string",
                        "label": "借记信用卡标志",
                        "width": "150px",
                        "dict": {
                            "dictType": "MS_E_DEBITCREDIT",
                            "dictKey": [
                                "MS_E_DEBITCREDIT"
                            ],
                            "format": "label"
                        }
                    }
                },
                {
                    "counterparty_acct_no": {
                        "control": "string",
                        "label": "对手方账号",
                        "width": "150px"
                    },
                    "counterparty_acct_na": {
                        "control": "string",
                        "label": "对方账户名称",
                        "width": "150px"
                    }
                },
                {
                    "trxn_remark": {
                        "control": "string",
                        "label": "交易备注",
                        "width": "150px"
                    },
                    "customer_remark": {
                        "control": "string",
                        "label": "客户备注",
                        "width": "150px"
                    }
                },
                {
                    "trxn_status": {
                        "control": "string",
                        "label": "交易状态",
                        "width": "150px",
                        "dict": {
                            "dictType": "E_TRXNSTATUS",
                            "dictKey": [
                                "E_TRXNSTATUS"
                            ],
                            "format": "label"
                        }
                    },
                    "agent_org_id": {
                        "control": "string",
                        "label": "业务代办法人",
                        "width": "150px"
                    }
                },
                {
                    "check_ind": {
                        "control": "string",
                        "label": "检查标志",
                        "width": "150px",
                        "dict": {
                            "dictType": "E_TRXNSTATUS",
                            "dictKey": [
                                "E_TRXNSTATUS"
                            ],
                            "format": "label"
                        }
                    }
                }
            ]
        }
    }
}