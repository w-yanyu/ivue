{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initDataOnce": true,
            "controls": [
                {
                    "user_id": {
                        "control": "string",
                        "label": "用户ID",
                        "rules": [
                            {
                                "max": 32,
                                "message": "长度不能超过32个字符"
                            }
                        ]
                    },
                    "user_name": {
                        "control": "string",
                        "label": "用户姓名",
                        "rules": [
                            {
                                "max": 100,
                                "message": "长度不能超过100个字符"
                            }
                        ]
                    }
                },
                {
                    "gender": {
                        "control": "string",
                        "label": "性别",
                        "dict": {
                            "dictType": "MS_E_GENDER",
                            "dictKey": [
                                "MS_E_GENDER"
                            ],
                            "format": "label"
                        }
                    },
                    "e_mail": {
                        "control": "string",
                        "label": "电子邮箱",
                        "rules": [
                            {
                                "max": 100,
                                "message": "长度不能超过100个字符"
                            }
                        ]
                    }
                },
                {
                    "user_depart": {
                        "control": "string",
                        "label": "用户部门",
                        "rules":[
                            {
                                "max":200,
                                "message":"长度不能超过200个字符"
                            }
                        ]
                    },
                    "staff_no": {
                        "control": "string",
                        "label": "员工编号",
                        "rules":[
                            {
                                "max":32,
                                "message":"长度不能超过32个字符"
                            }
                        ]
                    }
                },
                {
                    "office_phone_no": {
                        "control": "string",
                        "label": "办公电话",
                        "rules": [
                            {
                                "max": 20,
                                "message": "长度不能超过20个字符"
                            }
                        ]
                    },
                    "office_adress": {
                        "control": "string",
                        "label": "办公地址",
                        "rules": [
                            {
                                "max": 100,
                                "message": "长度不能超过100个字符"
                            }
                        ]
                    }
                },
                {
                    "exec_level": {
                        "control": "select",
                        "label": "行政级别",
                        "dict": {
                            "dictType": "E_EXECLEVEL",
                            "dictKey": [
                                "E_EXECLEVEL"
                            ],
                            "format": "label"
                        }
                    },
                    "tech_level": {
                        "control": "select",
                        "label": "技术职别",
                        "dict": {
                            "dictType": "E_TECHLEVEL",
                            "dictKey": [
                                "E_TECHLEVEL"
                            ],
                            "format": "label"
                        }
                    }
                },
                {
                    "user_status": {
                        "control": "string",
                        "label": "用户状态",
                        "dict": {
                            "dictType": "CT_E_TELLERSTAS",
                            "dictKey": [
                                "CT_E_TELLERSTAS"
                            ],
                            "format": "label"
                        }
                    },
                    "fprint_ind": {
                        "control": "select",
                        "label": "指纹录入标志",
                        "initShow": false,
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        }
                    }
                }
            ]
        }
    }
}