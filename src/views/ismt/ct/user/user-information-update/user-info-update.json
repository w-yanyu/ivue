{
  "module": "dynamicTabs",
  "layout": {
    "tabs": {
      "doRequest": {
        "url": "/SUMP/rest/auth/userManager",
        "method": "POST",
        "params": {
          "servicecode": "ct2001",
          "system_id": "core"
        }
      },
      "scope": "templateScope",
      "initDataOnce": true,
      "submitBtnPlace": "right",
      "initRequest": {
        "url": "/SUMP/rest/auth/userManager",
        "method": "post",
        "params": {
          "servicecode": "ct2022",
          "user_id": "${{params.user_id}}"
        }
      },
      "tabHeader": [
        {
          "id": "baseInfo",
          "label": "用户基础信息",
          "closable": false,
          "disabled": false,
          "clickInit": false,
          "icon": "el-icon-date",
          "toRequest": {
            "url": "/views/ismt/ct/user/user-information-update/user-info-update-base.json",
            "method": "post"
          }
        },
        {
          "id": "UserAffiliation",
          "name": "user_belong",
          "label": "用户隶属信息",
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "clickInit": false,
          "toRequest": {
            "url": "/views/ismt/ct/user/user-information-update/user-info-update-Affiliation.json",
            "method": "post",
            "params": {

            }
          }
        }
      ],
      "defaultActiveKey": "baseInfo",
      "position": "top",
      "styleClass": "card"
    }
  }
}
