{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ct2155",
          "branch_id": "${{params.branch_id}}"

        }
      },
      "controls": [
        {
          "org_id": {
            "control": "string",
            "label": "法人代码",
            "prop": "org_id",
            "width": "150px"
          },
          "branch_id": {
            "control": "lookup",
            "edit": false,
            "label": "机构号",
            "prop": "branch_id",
            "rules": [
              {
                "required": true,
                "message": "请输入机构号"
              },
              {
                "max": 10,
                "message": "长度不能超过10个字符"
              }
            ],
            "keyField": {
              "branch_id": "branch_id",
              "org_id":"org_id",
              "branch_name": "branch_name"
            },
            "toRequest": {
              "url": "@UserBranchTreeManager",
              "method": "post",
              "params": {}
            },
            "props": {
              "title": "机构信息查询"
            }
          }
        },
        {
          "branch_name": {
            "control": "text",
            "label": "机构名称",
            "prop": "branch_name",
            "width": "150px"
          },
          "operater_ind": {
            "control": "string",
            "label": "操作标识",
            "width": "150px",
            "value": "M",
            "initShow":false
          }
        }
      ]
    }
  }
}
