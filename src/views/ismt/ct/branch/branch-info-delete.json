{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/call/RPCCall",
            "method": "POST",
            "params": {
              "servicecode": "ct2141",
              "user_id": "$g{{user_info.userCode}}",
              "operater_ind": "D"
            }
          }
        }
      ],
      "controls": [
        {
          "branch_id": {
            "control": "string",
            "label": "机构号",
            "width": "160px",
            "rules": [
              {
                "max": 10,
                "message": "长度不能超过10个字符"
              }
            ]
          },
          "brch_relation_code": {
            "control": "hidden",
            "label": "机构关系代码",
            "width": "160px"
          },
          "branch_name": {
            "control": "string",
            "label": "机构名称",
            "width": "160px",
            "rules": [
              {
                "max": 100,
                "message": "长度不能超过100个字符"
              }
            ]
          }
        },
        {
          "branch_address": {
            "control": "string",
            "label": "机构地址",
            "width": "160px",
            "rules": [
              {
                "max": 200,
                "message": "长度不能超过200个字符"
              }
            ]
          },
          "branch_mail": {
            "control": "string",
            "label": "机构邮箱",
            "width": "160px",
            "rules": [
              {
                "max": 32,
                "message": "长度不能超过32个字符"
              }
            ]
          }
        },
        {
          "branch_phone": {
            "control": "text",
            "label": "机构电话",
            "disabled":true,
            "width": "160px",
            "rules": [{
              "pattern": "^[0-9]*$",
              "message": "请输入正确电话格式"
            },
              {
                "max": 12,
                "message": "电话最大长度不超过12"
              }
            ]
          },
          "postcode": {
            "control": "text",
            "label": "邮政编码",
            "disabled":true,
            "width": "160px",
            "rules": [
              {
                "max": 10,
                "message": "长度不能超过10个字符"
              }
            ]
          }
        },
        {
          "real_branch_ind": {
            "control": "select",
            "label": "实体机构标志",
            "width": "160px",
            "disabled":true,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "contacts_name": {
            "control": "string",
            "label": "联系人",
            "width": "160px",
            "rules": [
              {
                "max": 100,
                "message": "长度不能超过100个字符"
              }
            ]
          }
        },
        {
          "contacts_phone": {
            "control": "string",
            "label": "联系人电话",
            "width": "160px",
            "rules": [
              {
                "max": 32,
                "message": "长度不能超过32个字符"
              }
            ]
          },
          "contacts_mail": {
            "control": "string",
            "label": "联系人邮箱",
            "width": "160px",
            "rules": [
              {
                "max": 32,
                "message": "长度不能超过32个字符"
              }
            ]
          }
        },
        {
          "branch_function_class": {
            "control": "string",
            "label": "机构职能分类",
            "rules": [
              {
                "max": 8,
                "message": "长度不能超过8个字符"
              }
            ],
            "width": "160px",
            "dict": {
              "dictType": "CT_E_BRCHTP",
              "dictKey": [
                "CT_E_BRCHTP"
              ],
              "format": "label"
            }
          },
          "swift_no": {
            "control": "string",
            "label": "SWIFT代码",
            "width": "160px",
            "rules": [
              {
                "max": 32,
                "message": "长度不能超过32个字符"
              }
            ]
          }
        },
        {
          "holiday_code": {
            "control": "text",
            "label": "节假日代码",
            "width": "160px",
            "disabled":true,
            "rules": [
              {
                "max": 8,
                "message": "长度不能超过8个字符"
              }
            ],
            "labelField": "drop_list_desc",
            "valueField": "drop_list_value",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ms1053",
                "dynamic_drop_list_type": "AP0002"
              }
            },
            "format": "label"
          },
          "agree_sign": {
            "control": "string",
            "label": "合同签署地",
            "width": "160px"
          }

        },
        {
          "branch_level": {
            "control": "remoteSelect",
            "label": "机构级别",
            "width": "160px",
            "disabled":true,
            "format": "label",
            "valueField": "dict_id",
            "labelField": "dict_name",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "dict_type": "CT_E_BRANCHLEVEL",
                "servicecode": "ct2205"
              }
            },
            "rules": [{
              "max": 10,
              "message": "长度不能超过10个字符"
            }]
          },
          "parent_brch_id": {
            "control": "text",
            "label": "上级机构",
            "width": "160px",
            "disabled":true,
            "rules": [
              {
                "max": 10,
                "message": "长度不能超过10个字符"
              },
              {
                "required": true,
                "message": "请选择上级机构代码"
              }
            ]
          }

        },
        {
          "branch_type": {
            "control": "select",
            "label": "部门类型",
            "width": "160px",
            "disabled":true,
            "rules": [
              {
                "max": 8,
                "message": "长度不能超过8个字符"
              }
            ],
            "dict": {
              "dictType": "CT_E_BRCHTP",
              "dictKey": [
                "CT_E_BRCHTP"
              ],
              "format": "label"
            }
          },
          "branch_stus": {
            "control": "select",
            "disabled": true,
            "label": "部门状态",
            "width": "160px",
            "rules": [
              {
                "max": 8,
                "message": "长度不能超过8个字符"
              }
            ],
            "dict": {
              "dictType": "CT_E_BRCHSTUS",
              "dictKey": [
                "CT_E_BRCHSTUS"
              ],
              "format": "label"
            }
          }

        },
        {
          "branch_city_no": {
            "control": "text",
            "label": "分行号",
            "width": "160px",
            "disabled":true,
            "rules": [
              {
                "max": 10,
                "message": "长度不能超过10个字符"
              }
            ]
          },
          "iscity": {
            "control": "select",
            "label": "是否展业城市",
            "disabled": true,
            "width": "160px",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          }
        },
        {
          "province": {
            "control": "text",
            "disabled": true,
            "label": "省",
            "width": "160px"
          },
          "city": {
            "control": "text",
            "disabled": true,
            "label": "市",
            "width": "160px"
          }
        },
        {
          "area": {
            "control": "string",
            "label": "区",
            "width": "160px"
          },
          "address": {
            "control": "string",
            "label": "详细地址",
            "width": "160px"
          }
        }
      ]
    }
  }
}
