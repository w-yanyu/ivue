{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "scope":"strForm",
      "initRequest": "${{params}}",

      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/call/RPCCall",
            "method": "POST",
            "params": {
              "servicecode": "ct2176"
            }
          }
        }
      ],
      "controls": [
        {
          "branch_id": {
            "control": "text",
            "label": "机构号",
            "width": "160px",
            "disabled": true
          },
          "related_branch_id": {
            "control": "text",
            "label": "映射机构号",
            "width": "160px",
            "disabled": true
          }

        },
        {
          "branch_mapping_type": {
            "control": "select",
            "label": "映射类型",
            "width": "160px",
            "dict": {
              "dictType": "CT_E_BRANCH_MAPPING_TYPE",
              "dictKey": [
                "CT_E_BRANCH_MAPPING_TYPE"
              ],
              "format": "label"
            }
          }
        }
      ]
    }
  }
}
