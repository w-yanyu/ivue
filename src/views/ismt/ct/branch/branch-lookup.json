{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "branch_id": {
            "control": "text",
            "label": "机构号",
            "rules": [
              {
                "max": 10,
                "message": "长度不能超过10个字符"
              }
            ]
          },
          "branch_name": {
            "control": "text",
            "label": "机构名称",
            "rules": [
              {
                "max": 100,
                "message": "长度不能超过100个字符"
              }
            ]
          }
        },
        {
          "branch_function_class": {
            "control": "select",
            "label": "机构职能分类",
            "rules": [
              {
                "max": 8,
                "message": "长度不能超过8个字符"
              }
            ],
            "dict": {
              "dictType": "CT_E_BRCHTP",
              "dictKey": [
                "CT_E_BRCHTP"
              ],
              "format": "label"
            }
          },
          "real_branch_ind": {
            "control": "select",
            "label": "实体机构标志",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/call/RPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ct2155"
        }
      },
      "thead": [
        {
          "type": "index",
          "align": "center"
        },
        {
          "label": "机构号",
          "prop": "branch_id",
          "align": "center"
        },
        {
          "label": "机构名称",
          "prop": "branch_name",
          "align": "center",
          "width": "130px"
        },
        {
          "label": "实体机构标志",
          "prop": "real_branch_ind",
          "align": "center",
          "dict": {
            "dictType": "MS_E_YESORNO",
            "dictKey": [
              "MS_E_YESORNO"
            ],
            "format": "label"
          }
        },
        {
          "label": "机构地址",
          "align": "center",
          "prop": "branch_address"
        },
        {
          "label": "机构电话",
          "align": "center",
          "prop": "branch_phone"
        },
        {
          "label": "机构邮箱",
          "align": "center",
          "prop": "branch_mail"
        },
        {
          "label": "邮政编码",
          "align": "center",
          "prop": "postcode"
        },
        {
          "label": "联系人",
          "align": "center",
          "prop": "contacts_name"
        },
        {
          "label": "联系人电话",
          "align": "center",
          "prop": "contacts_phone"
        },
        {
          "label": "联系人邮箱",
          "align": "center",
          "prop": "contacts_mail"
        },
        {
          "label": "机构职能分类",
          "align": "center",
          "prop": "branch_function_class",
          "dict": {
            "dictType": "CT_E_BRCHTP",
            "dictKey": [
              "CT_E_BRCHTP"
            ],
            "format": "label"
          }
        },
        {
          "label": "SWIFT代码",
          "align": "center",
          "prop": "swift_no"
        },
        {
          "label": "节假日代码",
          "align": "center",
          "prop": "holiday_code"
        },
        {
          "label": "是否展业城市",
          "prop": "iscity",
          "dict": {
            "dictType": "MS_E_YESORNO",
            "dictKey": [
              "MS_E_YESORNO"
            ],
            "format": "label"
          }
        },
        {
          "label": "省",
          "prop": "province"
        },
        {
          "label": "市",
          "prop": "city"
        },
        {
          "label": "区",
          "prop": "area"
        },
        {
          "label": "详细地址",
          "prop": "address"
        },
        {
          "label": "合同签署地",
          "prop": "agree_sign"
        },
        {
          "label": "操作",
          "align": "center",
          "fixed": "right",
          "btns": [
            {
              "type": "bringback",
              "label": "选择"
            }
          ]
        }
      ]
    }
  }
}
