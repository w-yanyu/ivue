{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "branch_id": {
            "control": "text",
            "label": "机构号",
            "rules": [
              {
                "max": 10,
                "message": "长度不能超过10个字符"
              }
            ]
          },
          "branch_name": {
            "control": "text",
            "label": "机构名称",
            "rules": [
              {
                "max": 100,
                "message": "长度不能超过100个字符"
              }
            ]
          }
        },
        {
          "branch_function_class": {
            "control": "select",
            "label": "机构职能分类",
            "rules": [
              {
                "max": 8,
                "message": "长度不能超过8个字符"
              }
            ],
            "dict": {
              "dictType": "CT_E_BRCHTP",
              "dictKey": [
                "CT_E_BRCHTP"
              ],
              "format": "label"
            }
          },
          "real_branch_ind": {
            "control": "select",
            "label": "实体机构标志",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "target": "dialog",
          "label": "新增",
          "toRequest": {
            "method": "GET",
            "url": "/views/ismt/ct/branch/branch-info-add.json"
          },
          "props": {
            "title": "机构信息新增"
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/call/RPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ct2155",
          "page_id": "/views/ismt/ct/branch/brannch-info-manager.json"
        }
      },
      "thead": [
        {
          "type": "index",
          "align": "center"
        },
        {
          "label": "机构号",
          "prop": "branch_id",
          "align": "center"
        },
        {
          "label": "机构名称",
          "prop": "branch_name",
          "align": "center",
          "width": "130px"
        },
        {
          "label": "实体机构标志",
          "prop": "real_branch_ind",
          "align": "center",
          "dict": {
            "dictType": "MS_E_YESORNO",
            "dictKey": [
              "MS_E_YESORNO"
            ],
            "format": "label"
          }
        },
        {
          "label": "机构地址",
          "align": "center",
          "prop": "branch_address"
        },
        {
          "label": "机构电话",
          "align": "center",
          "prop": "branch_phone"
        },
        {
          "label": "机构邮箱",
          "align": "center",
          "prop": "branch_mail"
        },
        {
          "label": "邮政编码",
          "align": "center",
          "prop": "postcode"
        },
        {
          "label": "联系人",
          "align": "center",
          "prop": "contacts_name"
        },
        {
          "label": "联系人电话",
          "align": "center",
          "prop": "contacts_phone"
        },
        {
          "label": "联系人邮箱",
          "align": "center",
          "prop": "contacts_mail"
        },
        {
          "label": "机构职能分类",
          "align": "center",
          "prop": "branch_function_class",
          "dict": {
            "dictType": "CT_E_BRCHTP",
            "dictKey": [
              "CT_E_BRCHTP"
            ],
            "format": "label"
          }
        },
        {
          "label": "SWIFT代码",
          "align": "center",
          "prop": "swift_no"
        },
        {
          "label": "节假日代码",
          "align": "center",
          "prop": "holiday_code"
        },
        {
          "label": "是否展业城市",
          "prop": "iscity",
          "dict": {
            "dictType": "MS_E_YESORNO",
            "dictKey": [
              "MS_E_YESORNO"
            ],
            "format": "label"
          }
        },
        {
          "label": "省",
          "prop": "province"
        },
        {
          "label": "市",
          "prop": "city"
        },
        {
          "label": "区",
          "prop": "area"
        },
        {
          "label": "详细地址",
          "prop": "address"
        },
        {
          "label": "合同签署地",
          "prop": "agree_sign"
        },
        {
          "label": "操作",
          "fixed": "right",
          "width": 200,
          "align":"center",
          "btns": [
            {
              "type": "look",
              "label": "详情",
              "toRequest": {
                "url": "/views/ismt/ct/branch/branch-info-detail.json",
                "method": "POST"
              },
              "props": {
                "title": "机构信息详情"
              }
            },
            {
              "type": "edit",
              "label": "维护",
              "toRequest": {
                "url": "/views/ismt/ct/branch/branch-info-update.json",
                "method": "GET"
              },
              "props": {
                "title": "机构信息维护"
              }
            },
            {
              "type": "edit",
              "label": "删除",
              "toRequest": {
                "url": "/views/ismt/ct/branch/branch-info-delete.json",
                "method": "GET"
              },
              "props": {
                "title": "机构信息删除"
              }
            },
            {
              "type": "edit",
              "label": "撤并",
              "toRequest": {
                "url": "/views/ismt/ct/branch/branch_revoke_step.json",
                "method": "GET"
              },
              "props": {
                "title": "机构撤并",
                "width":"80%",
                "height":"70%"
              },
              "show": {
                "expression": "'${{current.branch_stus}}' == '1'"
              }
            },
            {
              "type": "edit",
              "label": "撤并",
              "toRequest": {
                "url": "/views/ismt/ct/branch/branch_revoke_info_step.json",
                "method": "GET"
              },
              "props": {
                "title": "机构撤并",
                "width":"80%",
                "height":"70%"
              },
              "show": {
                "expression": "'${{current.branch_stus}}' == '3'"
              }
            },
            {
              "type": "edit",
              "label": "确认撤并",
              "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "ct2193",
                  "branch_id":"${{current.branch_id}}"
                }
              },
              "confirm":{
                "title":"提示信息",
                "msg":"确认撤并该机构吗？",
                "confirmButtonText":"确定",
                "cancelButtonText":"取消"
              },
              "show": {
                "expression": "'${{current.revoke_busi_flag}}' === '1' && '${{current.branch_stus}}' === '3'"
              }
            }
          ]
        }
      ]
    }
  }
}
