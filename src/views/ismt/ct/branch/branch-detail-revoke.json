{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "scope":"strForm",
      "initRequest": {
        "url": "/SUMP/call/RPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ct2194",
          "branch_id":"${{params.branch_id}}"
        }
      },
      "btns": [],
      "controls": [
        {
          "branch_id": {
            "control": "string",
            "label": "机构号",
            "width": "160px"
          },
          "branch_name": {
            "control": "string",
            "label": "机构名称",
            "width": "160px"
          }
        },
        {
          "revoke_no": {
            "control": "string",
            "label": "撤销编号",
            "width": "160px"
          },
          "branch_status": {
            "control": "string",
            "label": "机构状态",
            "width": "160px",
            "dict": {
              "dictType": "CT_E_BRCHSTUS",
              "dictKey": [
                "CT_E_BRCHSTUS"
              ],
              "format": "label"
            }
          }
        },
        {
          "brch_relation_code": {
            "control": "hidden",
            "label": "机构关系代码",
            "width": "160px",
            "dict": {
              "dictType": "CT_E_BRANCH_RELATION",
              "dictKey": [
                "CT_E_BRANCH_RELATION"
              ],
              "format": "label"
            }
          },
          "branch_reserve_flag": {
            "control": "string",
            "label": "机构是否保留",
            "width": "160px",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          }
        }
      ]
    }
  }
}
