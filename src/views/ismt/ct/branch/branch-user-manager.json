{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "doRequest": {
        "url": "",
        "method": "POST",
        "params": {}
      },
      "controls": [
        {
          "user_name": {
            "control": "text",
            "label": "用户姓名",
            "rules": [
              {
                "max": 100,
                "message": "长度不能超过100个字符"
              }
            ]
          },
          "user_status": {
            "control": "select",
            "dict": {
              "dictType": "CT_E_TELLERSTAS",
              "dictKey": [
                "CT_E_TELLERSTAS"
              ],
              "format": "label"
            },
            "label": "用户状态"
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {

          "type": "add",
          "target": "dialog",
          "label": "用户转移",
          "checkCountMin": 1,
          "toRequest": {
            "method": "GET",
            "url": "/views/ismt/ct/branch/branch-user-change.json",
            "params": {
              "brch_relation_code":"${{params.brch_relation_code}}"
            }
          },
          "props": {
            "title": "用户转移"
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/rest/auth/userManager",
        "method": "POST",
        "params": {
          "servicecode": "ct2002",
          "branch_id":"${{params.branch_id}}",
          "brch_relation_code":"${{params.brch_relation_code}}"
        }
      },
      "pagination": {
        "show": true,
        "pageSize": 10
      },
      "thead": [
        {
          "type": "selection",
          "align": "center",
          "width": "55",
          "fixed": "left"
        },
        {
          "label": "用户ID",
          "prop": "user_id"
        },
        {
          "label": "用户姓名",
          "prop": "user_name"
        },
        {
          "label": "证件类型",
          "prop": "cert_type",
          "dict": {
            "dictType": "MS_E_CRET_TYPE",
            "dictKey": [
              "MS_E_CRET_TYPE"
            ],
            "format": "label"
          }
        },
        {
          "label": "证件号",
          "prop": "cert_no"
        },
        {
          "label": "机构号",
          "prop": "branch_id"
        },
        {
          "label": "所属系统",
          "prop": "belong_system_id",
          "isShow": false
        },
        {
          "label": "用户状态",
          "prop": "user_status",
          "dict": {
            "dictType": "CT_E_TELLERSTAS",
            "dictKey": [
              "CT_E_TELLERSTAS"
            ],
            "format": "label"
          }
        },


        {
          "label": "密码状态",
          "prop": "password_status",
          "dict": {
            "dictType": "CT_E_PASSWDSTAS",
            "dictKey": [
              "CT_E_PASSWDSTAS"
            ],
            "format": "label"
          }
        },

        {
          "label": "登录状态",
          "prop": "login_status",
          "dict": {
            "dictType": "CT_E_LOGSTAS",
            "dictKey": [
              "CT_E_LOGSTAS"
            ],
            "format": "label"
          }
        }
      ]
    }
  }
}