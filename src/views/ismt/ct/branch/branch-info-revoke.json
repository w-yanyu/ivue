{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "scope":"strForm",
      "initRequest": "${{params}}",
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/call/RPCCall",
            "method": "POST",
            "params": {
              "servicecode": "ct2189"
            }
          }
        }
      ],
      "controls": [
        {
          "branch_id": {
            "control": "string",
            "label": "机构号",
            "width": "160px"
          },
          "branch_name": {
            "control": "string",
            "label": "机构名称",
            "width": "160px"
          }
        },
        {
          "brch_relation_code": {
            "control": "hidden",
            "label": "机构关系代码",
            "width": "160px"
          },
          "branch_status": {
            "control": "hidden",
            "label": "机构状态",
            "width": "160px"
          }
        },
        {
          "branch_reserve_flag": {
            "control": "select",
            "label": "机构是否保留",
            "width": "160px",
            "rules": [
              {
                "required": true,
                "message": "请选择机构是否保留"
              }
            ],
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          }
        }
      ]
    }
  }
}
