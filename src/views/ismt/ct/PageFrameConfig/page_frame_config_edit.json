{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "scope": "templateScope",
            "initRequest":"${{params}}",
            "initDataOnce":true,
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "post",
              "params":{
                "servicecode": "AS_ICMS_PB_UPFRME"
              }
            },
            "btns":[
				{
					"type":"submit",
					"label":"提交"
				}
			],
            "controls": [
                {
                    "frame_cd": {
                        "label": "框架编号",
                        "width": "130px",
                        "control": "string"
                        
                    },
                    "url": {
                        "label": "链接地址URL",
                        "control": "text",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入链接地址URL"
                            }
                        ]
                    },
                    "hdr_url": {
                        "label": "表头页面连接",
                        "control": "text",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入表头页面连接"
                            }
                        ]
                    },
                    "foot_url": {
                        "label": "表尾页面链接",
                        "control": "text",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入表尾页面链接"
                            }
                        ]
                    },
                    "frame_desc": {
                        "label": "框架描述",
                        "control": "text",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入框架描述"
                            }
                        ]
                    },
                    "is_enabled": {
                        "control": "select",
                        "label": "是否生效",
                        "dict": {
                            "dictType": "E_ISENBE",
                            "dictKey": [
                                "E_ISENBE"
                            ],
                            "format": "label"
                        },
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择是否生效"
                            }
                        ]
                    }
                }
            ]
        }
    }
}