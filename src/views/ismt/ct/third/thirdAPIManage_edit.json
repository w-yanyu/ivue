{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "ct2552"
        }
      },
      "controls": [
        {
          "api_name": {
            "control": "text",
            "label": "API名称",
            "span": 12,
            "width": "140px",
            "disabled": true,
            "rules": [
              {
                "required": false,
                "message": "请输入必须项"
              }
            ]
          },
          "api_desc": {
            "control": "text",
            "label": "API描述",
            "span": 12,
            "width": "140px",
            "rules": [
              {
                "required": true,
                "message": "请输入必须项"
              }
            ]
          },
          "key1": {
            "control": "text",
            "label": "请求密钥1",
            "span": 12,
            "width": "140px",
            "rules": [
              {
                "required": true,
                "message": "请输入必须项"
              }
            ]
          },
          "key2": {
            "control": "text",
            "label": "请求密钥2",
            "span": 12,
            "width": "140px",
            "rules": [
              {
                "required": true,
                "message": "请输入必须项"
              }
            ]
          },
          "api_status": {
            "control": "hidden",
            "label": "API状态",
            "span": 12,
            "width": "140px",
            "rules": [
              {
                "required": true,
                "message": "请输入必须项"
              }
            ],
            "dict": {
              "dictType": "CT_E_API_STATUS",
              "dictKey": [
                "CT_E_API_STATUS"
              ],
              "format": "label"
            }
          },
          "api_enable": {
            "control": "hidden",
            "label": "API连通性",
            "span": 12,
            "width": "140px",
            "rules": [
              {
                "required": true,
                "message": "请输入必须项"
              }
            ],
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "api_path": {
            "control": "text",
            "label": "请求路径",
            "span": 12,
            "width": "140px",
            "rules": [
              {
                "required": true,
                "message": "请输入必须项"
              }
            ]
          },
          "server_range": {
            "label": "微服务范围",
            "control": "select",
            "multiple": true,
            "span": 12,
            "width": "140px",
            "rules": [
              {
                "required": false,
                "message": "请输入必须项"
              }
            ],
            "dict": {
              "dictType": "MS_APPID",
              "dictKey": [
                "MS_APPID"
              ],
              "format": "value-label"
            }
          }
        }
      ]
    }
  }
}
