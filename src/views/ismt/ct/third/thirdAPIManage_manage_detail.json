{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pd0214",
          "apply_no": "${{params.apply_no}}",
          "cust_no": "${{params.cust_no}}",
          "due_item_no": "${{params.due_item_no}}"
        }
      },
      "thead": [
        {
          "type": "string",
          "label": "null",
          "prop": "null",
          "align": "center"
        },
        {
          "type": "string",
          "label": "API名称",
          "prop": "api_name",
          "align": "center"
        },
        {
          "type": "string",
          "label": "API描述",
          "prop": "api_desc",
          "align": "center"
        },
        {
          "type": "string",
          "label": "请求路径",
          "prop": "api_path",
          "align": "center"
        },
        {
          "type": "string",
          "label": "请求密钥1",
          "prop": "key1",
          "align": "center"
        },
        {
          "type": "string",
          "label": "请求密钥2",
          "prop": "key2",
          "align": "center"
        },
        {
          "type": "string",
          "label": "API状态",
          "prop": "api_status",
          "align": "center",
          "dict": {
            "dictType": "CT_E_API_STATUS",
            "dictKey": [
              "CT_E_API_STATUS"
            ],
            "format": "label"
          }
        },
        {
          "type": "string",
          "label": "API连通性",
          "prop": "api_enable",
          "align": "center",
          "dict": {
            "dictType": "MS_E_YESORNO",
            "dictKey": [
              "MS_E_YESORNO"
            ],
            "format": "label"
          }
        },
        {
          "type": "string",
          "label": "微服务范围",
          "prop": "server_range",
          "align": "center"
        },
        {
          "label": "操作",
          "align": "center",
          "width": "200px",
          "btns": [
            {
              "type": "look",
              "label": "详情",
              "toRequest": {
                "url": "@prodLayout",
                "method": "GET",
                "params": {
                  "apply_no": "${{params.apply_no}}",
                  "cust_no": "${{params.cust_no}}",
                  "due_item_no": "${{params.due_item_no}}"
                }
              },
              "props": {
                "width": "60%",
                "title": "授信产品详情"
              }
            }
          ]
        }
      ]
    }
  }
}
