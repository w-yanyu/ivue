{
    "module": "v-selectableTree",
    "layout": {
        "tree": {
            "span": 24,
            "idField": "menu_id",
            "pIdField": "menu_upper_id",
            "labelField": "menu_desc",
            "checkedField": "checked",
            "isLeafField": "isLeaf",
            "disabledField": "disabled",
            "showCheckbox": true,
            "showRadio": false,
            "halfCheckedAlias": "halfCheckedList",
            "expandAllLevel": false,
            "expandLevel": 0,
            "expandCheckedLevel": true,
            "checkStrictly": false,
            "showSeach": true,
            "iconField": "icon",
            "submitKeyAlias": "submitData",
            "initRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "ct2107",
                    "role_id": "${{params.role_id}}"
                }
            },
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "ct2061",
                    "role_id": "${{params.role_id}}"
                }
            }
        }
    }
}