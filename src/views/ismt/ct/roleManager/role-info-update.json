{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/call/RPCCall",
            "method": "POST",
            "params": {
              "servicecode": "ct2052",
              "page_id": "views/ismt/ct/roleManager/role-info-update.json",
              "user_id": "$g{{user_info.userCode}}",
              "operater_ind": "M"
            }
          }
        }
      ],
      "controls": [
        {
          "role_id": {
            "control": "string",
            "label": "角色ID",
            "width": "140px"
          },
          "role_name": {
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入角色名称"
              },
              {
                "max": 100,
                "message": "长度不能超过100个字符"
              }
            ],
            "label": "角色名称",
            "width": "140px"
          },
          "role_type": {
            "control": "select",
            "label": "角色类型",
            "width": "140px",
            "dict": {
              "dictType": "CT_E_ROLE_TYPE",
              "dictKey": [
                "CT_E_ROLE_TYPE"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择角色类型"
              }
            ]
          }
        }
      ]
    }
  }
}
