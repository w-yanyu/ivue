{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/call/RPCCall",
            "method": "POST",
            "params": {
              "servicecode": "ct2051",
              "bind_default_menu_ind": "1",
              "user_id": "$g{{user_info.userCode}}",
              "system_id": ""
            }
          }
        }
      ],
      "controls": [
        {
          "role_id": {
            "control": "text",
            "label": "角色ID",
            "width": "100px",
            "rules": [
              {
                "required": true,
                "message": "请输入角色ID"
              },
              {
                "max": 64,
                "message": "长度不能超过64个字符"
              }
            ]
          },
          "role_name": {
            "control": "text",
            "label": "角色名称",
            "width": "100px",
            "rules": [
              {
                "required": true,
                "message": "请输入角色名称"
              },
              {
                "max": 100,
                "message": "长度不能超过100个字符"
              }
            ]
          },
          "role_type": {
            "control": "select",
            "label": "角色类型",
            "width": "100px",
            "dict": {
              "dictType": "CT_E_ROLE_TYPE",
              "dictKey": [
                "CT_E_ROLE_TYPE"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择角色类型"
              }
            ]
          }
        }
      ]
    }
  }
}
