{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "ct2068",
                    "old_role_mutex_id": "${{params.role_mutex_id}}",
                    "system_id": ""
                }
            },
            "controls": [
                {
                    "role_id": {
                        "control": "string",
                        "label": "角色ID",
                        "rules": [
                            {
                                "required": "true",
                                "message": "请输入角色ID"
                            },
                            {
                                "max": 64,
                                "message": "长度不能超过64个字符"
                            }
                        ],
                        "dict": {
                            "dictType": "D_role_id",
                            "dictKey": [
                                "D_role_id"
                            ],
                            "labelField": "drop_list_desc",
                            "valueField": "drop_list_value",
                            "doRequest": {
                                "url": "/SUMP/call/RPCCall",
                                "method": "POST",
                                "params": {
                                    "servicecode": "ms9999",
                                    "dynamic_drop_list_type": "CT0001"
                                }
                            },
                            "format": "label"
                        }
                    },
                    "role_mutex_id": {
                        "control": "remoteSelect",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入角色互斥ID"
                            },
                            {
                                "max": 10,
                                "message": "长度不能超过10个字符"
                            }
                        ],
                        "label": "角色互斥ID",
                        "labelField": "drop_list_desc",
                        "valueField": "drop_list_value",
                        "doRequest": {
                            "url": "/SUMP/call/RPCCall",
                            "method": "POST",
                            "params": {
                                "servicecode": "ms9999",
                                "dynamic_drop_list_type": "CT0001"
                            }
                        },
                        "format": "label"
                    }
                }
            ]
        }
    }
}