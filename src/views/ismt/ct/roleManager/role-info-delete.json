{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/call/RPCCall",
            "method": "POST",
            "params": {
              "servicecode": "ct2052",
              "page_id": "views/ismt/ct/roleManager/role-info-update.json",
              "user_id": "$g{{user_info.userCode}}",
              "operater_ind": "D"
            }
          }
        }
      ],
      "controls": [
        {
          "role_id": {
            "control": "string",
            "label": "角色ID",
            "width": "140px"
          },
          "role_name": {
            "control": "string",
            "rules": [
              {
                "max": 100,
                "message": "长度不能超过100个字符"
              }
            ],
            "label": "角色名称",
            "width": "140px"
          }
        }
      ]
    }
  }
}
