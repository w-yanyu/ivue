{
    "module": "v_inputDataForm_btn",
    "layout": {
      "form": {
        "initRequest": "${{params}}",
        "btns": [
          {
            "type": "submit",
            "label": "提交",
            "icon": "",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct4103",
                "appId":"deciOnl"
              }
            }
          }
        ],
        "controls": [
            {
              "record_no": {
                "control": "string",
                "label": "记录编号",
                "width": "240px"
              },
              "eng_name": {
                "control": "string",
                "label": "字段英文名称",
                "width": "240px",
                "rules": [
                  {
                    "required": true,
                    "message": "请输入表中字段英文名称"
                  }
                ]
              },
              "is_dict": {
                "control": "select",
                "label": "是否业务字典",
                "width": "240px",
                "dict": {
                  "dictType": "MS_E_YESORNO",
                  "dictKey": [
                    "MS_E_YESORNO"
                  ],
                  "format": "label"
                },
                "rules": [
                  {
                    "required": true,
                    "message": "请选择是否业务字典"
                  }
                ],
                "showItem":{
                  "1": [
                    "dict_no"
                  ]
                }
              },
              "dict_no": {
                "control": "text",
                "label": "业务字典编号",
                "width": "240px",
                "rules": [
                  {
                    "required": true,
                    "message": "请输入业务字典编号"
                  }
                ]
              },
              "field_name": {
                "control": "text",
                "label": "字段名",
                "width": "240px",
                "rules": [
                  {
                    "required": true,
                    "message": "请输入字段名"
                  }
                ]
              },
              "auth_status": {
                "control": "select",
                "label": "授权状态",
                "width": "240px",
                "dict": {
                  "dictType": "CT_E_EXPAUTH_STATUS",
                  "dictKey": [
                    "CT_E_EXPAUTH_STATUS"
                  ],
                  "format": "label"
                },
                "rules": [
                  {
                    "required": true,
                    "message": "请选择授权状态"
                  }
                ]
              },
              "exp_para_tp": {
                "control": "select",
                "label": "导出参数类型",
                "width": "240px",
                "dict": {
                  "dictType": "CT_E_EXP_PARA_TP",
                  "dictKey": [
                    "CT_E_EXP_PARA_TP"
                  ],
                  "format": "label"
                },
                "rules": [
                  {
                    "required": true,
                    "message": "请选择导出参数类型"
                  }
                ],
                "showItem":{
                  "0":["req_para_value"]
                }
              },
              "req_para_value": {
                "control": "text",
                "label": "请求参数值",
                "width": "240px",
                "rules": [
                  {
                    "required": true,
                    "message": "请输入请求参数值"
                  }
                ]
              }
            }
        ]
      }
    }
  }
  