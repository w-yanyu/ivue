{
    "module": "v_inputDataForm_btn",
    "layout": {
      "form": {
        "initRequest": "${{params}}",
        "btns": [
          {
            "type": "submit",
            "label": "提交",
            "icon": "",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct4106",
                "appId":"deciOnl",
                "record_no" : "${{params.record_no}}"
              }
            }
          }
        ],
        "controls": [
            {
              "eng_name": {
                "control": "text",
                "label": "字段英文名称",
                "width": "240px",
                "rules": [
                  {
                    "required": true,
                    "message": "请输入表中字段英文名称"
                  }
                ]
              },
              "show_name": {
                "control": "text",
                "label": "导出展示名",
                "width": "240px",
                "rules": [
                  {
                    "required": true,
                    "message": "请输入导出展示名"
                  }
                ]
              }
            }
        ]
      }
    }
  }
  