{
    "module": "v_inputDataForm_btn",
    "layout": {
      "form": {
        "initRequest": "${{params}}",
        "btns": [
          {
            "type": "submit",
            "label": "提交",
            "icon": "",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct4100",
                "appId":"deciOnl"
              }
            }
          }
        ],
        "controls": [
            {
              "record_no": {
                "label": "记录编号",
                "control": "string"
              },
              "page_id": {
                "control": "string",
                "label": "菜单",
                "width": "240px"
              },
              "tran_code": {
                "control": "string",
                "label": "交易码",
                "width": "240px"
              },
              "up_limit": {
                "control": "string",
                "label": "导出数据上限",
                "width": "240px"
              },
              "exp_format": {
                "control": "string",
                "label": "导出格式",
                "width": "240px",
                "dict": {
                  "dictType": "CT_E_EXP_FORMAT",
                  "dictKey": [
                    "CT_E_EXP_FORMAT"
                  ],
                  "format": "label"
                },
                "showItem": {
                    "0": [
                      "page_num"
                    ]
                }
              },
              "page_num": {
                "control": "string",
                "label": "Excel分页记录数",
                "width": "240px"
              },
              "del_auth": {
                "control": "string",
                "label": "是否下放导出范围权限",
                "width": "240px",
                "dict": {
                  "dictType": "MS_E_YESORNO",
                  "dictKey": [
                    "MS_E_YESORNO"
                  ],
                  "format": "label"
                }
              },
              "auth_status": {
                "control": "string",
                "label": "授权状态",
                "width": "240px",
                "dict": {
                  "dictType": "CT_E_EXPAUTH_STATUS",
                  "dictKey": [
                    "CT_E_EXPAUTH_STATUS"
                  ],
                  "format": "label"
                }
              }
              }
        ]
      }
    }
  }
  