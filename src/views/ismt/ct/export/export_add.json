{
    "module": "v_inputDataForm_btn",
    "layout": {
      "form": {
        "initRequest": "${{params}}",
        "btns": [
          {
            "type": "submit",
            "label": "提交",
            "icon": "",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct4100",
                "appId":"deciOnl"
              }
            }
          }
        ],
        "controls": [
            {
              "flag": {
                "label": "flag",
                "control": "hidden",
                "showItem": {
                  "upd": [
                    "record_no"
                  ]
                }
              },
              "record_no": {
                "label": "记录编号",
                "control": "string"
              },
              "page_id": {
                "control": "text",
                "label": "菜单",
                "width": "240px",
                "rules": [
                  {
                    "required": true,
                    "message": "请输入菜单"
                  }
                ]
              },
              "tran_code": {
                "control": "text",
                "label": "交易码",
                "width": "240px",
                "rules": [
                  {
                    "required": true,
                    "message": "请输入交易码"
                  }
                ]
              },
              "up_limit": {
                "control": "text",
                "label": "导出数据上限",
                "width": "240px",
                "rules": [
                  {
                    "required": true,
                    "message": "请输入导出数据上限"
                  },
                  {
                    "pattern": "^[1-9]+[0-9]*$",
                    "message": "请输入正整数"
                  }
                ]
              },
              "exp_format": {
                "control": "select",
                "label": "导出格式",
                "width": "240px",
                "dict": {
                  "dictType": "CT_E_EXP_FORMAT",
                  "dictKey": [
                    "CT_E_EXP_FORMAT"
                  ],
                  "format": "label"
                },
                "rules": [
                  {
                    "required": true,
                    "message": "请选择导出格式"
                  }
                ],
                "showItem": {
                    "0": [
                      "page_num"
                    ]
                }
              },
              "page_num": {
                "control": "text",
                "label": "Excel分页记录数",
                "width": "240px",
                "rules": [
                  {
                    "required": true,
                    "message": "请输入Excel分页记录数"
                  },
                  {
                    "pattern": "^[1-9]+[0-9]*$",
                    "message": "请输入正整数"
                  }
                ]
              },
              "del_auth": {
                "control": "select",
                "label": "是否下放导出范围权限",
                "width": "240px",
                "dict": {
                  "dictType": "MS_E_YESORNO",
                  "dictKey": [
                    "MS_E_YESORNO"
                  ],
                  "format": "label"
                },
                "rules": [
                  {
                    "required": true,
                    "message": "请选择是否下放导出范围权限"
                  }
                ]
              },
              "auth_status": {
                "control": "select",
                "label": "授权状态",
                "width": "240px",
                "dict": {
                  "dictType": "CT_E_EXPAUTH_STATUS",
                  "dictKey": [
                    "CT_E_EXPAUTH_STATUS"
                  ],
                  "format": "label"
                },
                "rules": [
                  {
                    "required": true,
                    "message": "请选择授权状态"
                  }
                ]
              }
              }
        ]
      }
    }
  }
  