{
    "module": "v_search_btn_datagrid",
    "layout": {
        
        "toolbar": {
            "btns": [
                {
                    "type": "add",
                    "target": "dialog",
                    "label": "新增",
                    "toRequest": {
                        "method": "GET",
                        "url": "/views/ismt/ct/export/export_fieldRange_add.json",
                        "params": {
                            "record_no" : "${{params.record_no}}"
                        }
                    },
                    "props": {
                        "title": "新增智能业务数据导出范围配置"
                    },
                    "show": {
                        "expression": "'${{params.pageType}}' != 'detail'"
                    }
                }
            ]
        },
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": true,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "ct4107",
                    "appId":"deciOnl",
                    "record_no" : "${{params.record_no}}"
                }
            },
            "pagination": {
                "show": true,
                "pageSize": 10
            },
            "thead": [
                {
                    "type": "index"
                },
                {
                    "label": "记录编号",
                    "prop": "record_no"
                },
                {
                    "label": "字段英文名称",
                    "prop": "eng_name"
                },
                {
                  "label": "导出展示名称",
                  "prop": "show_name"
                },
                {
                    "label": "操作",
                    "width": 120,
                    "align":"center",
                    "fixed": "right",
                    "btns": [
                        {
                            "type": "edit",
                            "label": "维护",
                            "toRequest": {
                                "url": "/views/ismt/ct/export/export_fieldRange_upd.json",
                                "method": "GET",
                                "params": {
                                    "flag": "UPD"
                                }
                            },
                            "props": {
                                "title": "智能业务数据导出范围配置信息维护"
                            },
                            "show": {
                                "expression": "'${{params.pageType}}' != 'detail'"
                            }
                        },
                        {
                            "type": "edit",
                            "label": "删除",
                            "doRequest": {
                                "url": "/SUMP/icmcall/icmRPCCall",
                                "method": "post",
                                "params": {
                                    "servicecode": "ct4108"
                                }
                            },
                            "confirm": {
                                "title": "提醒",
                                "msg": "确定删除该记录？",
                                "confirmButtonText": "确定",
                                "cancelButtonText": "取消"
                            },
                            "show": {
                                "expression": "'${{params.pageType}}' != 'detail'"
                            }
                        }
                    ]
                }
            ]
        }
    }
  }