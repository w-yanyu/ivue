{
    "module": "v_search_btn_datagrid",
    "layout": {
        
        "toolbar": {
            "btns": [
                {
                    "type": "add",
                    "target": "dialog",
                    "label": "新增",
                    "toRequest": {
                        "method": "GET",
                        "url": "/views/ismt/ct/export/export_fieldDefine_add.json",
                        "params": {
                            "record_no" : "${{params.record_no}}"
                        }
                    },
                    "props": {
                        "title": "新增智能业务数据字段库定义信息"
                    },
                    "show": {
                        "expression": "'${{params.pageType}}' != 'detail'"
                    }
                }
            ]
        },
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": true,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "ct4105",
                    "appId":"deciOnl",
                    "record_no" : "${{params.record_no}}"
                }
            },
            "pagination": {
                "show": true,
                "pageSize": 10
            },
            "thead": [
                {
                    "type": "index"
                },
                {
                    "label": "记录编号",
                    "prop": "record_no"
                },
                {
                    "label": "字段英文名称",
                    "prop": "eng_name"
                },
                {
                    "label": "是否业务字典",
                    "prop": "is_dict",
                    "dict": {
                        "dictType": "MS_E_YESORNO",
                        "format": "label",
                        "dictKey": [
                            "MS_E_YESORNO"
                        ]
                    },
                    "format": "label"
                },
                {
                  "label": "业务字典编号",
                  "prop": "dict_no"
                },
                {
                    "label": "表中字段名",
                    "prop": "field_name"
                },
                {
                    "label": "授权状态",
                    "prop": "auth_status",
                    "dict": {
                        "dictType": "CT_E_EXPAUTH_STATUS",
                        "format": "label",
                        "dictKey": [
                            "CT_E_EXPAUTH_STATUS"
                        ]
                    }
                },
                {
                    "label": "导出参数类型",
                    "prop": "exp_para_tp",
                    "dict": {
                        "dictType": "CT_E_EXP_PARA_TP",
                        "format": "label",
                        "dictKey": [
                            "CT_E_EXP_PARA_TP"
                        ]
                    }
                },
                {
                    "label": "参数值",
                    "prop": "req_para_value"
                },
                {
                    "label": "操作",
                    "width": 120,
                    "align":"center",
                    "fixed": "right",
                    "btns": [
                        {
                            "type": "edit",
                            "label": "维护",
                            "toRequest": {
                                "url": "/views/ismt/ct/export/export_fieldDefine_upd.json",
                                "method": "GET"
                            },
                            "props": {
                                "title": "智能业务数据字段库定义信息维护"
                            },
                            "show": {
                                "expression": "'${{params.pageType}}' != 'detail'"
                            }
                        },
                        {
                            "type": "edit",
                            "label": "删除",
                            "doRequest": {
                                "url": "/SUMP/icmcall/icmRPCCall",
                                "method": "post",
                                "params": {
                                    "servicecode": "ct4104"
                                }
                            },
                            "confirm": {
                                "title": "提醒",
                                "msg": "确定删除该记录？",
                                "confirmButtonText": "确定",
                                "cancelButtonText": "取消"
                            },
                            "show": {
                                "expression": "'${{params.pageType}}' != 'detail'"
                            }
                        }
                    ]
                }
            ]
        }
    }
  }