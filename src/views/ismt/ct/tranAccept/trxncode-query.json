{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "controls": [{
                "trxn_code": {
                    "control": "text",
                    "label": "柜面交易码",
                    "rules": [{
                        "max": 20,
                        "message": "长度不能超过10个字符"
                    }]
                },
                "trxn_code_name": {
                    "control": "text",
                    "label": "柜面交易码名称",
                    "rules": [{
                        "max": 100,
                        "message": "长度不能超过100个字符"
                    }]
                }
            }]
        },
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": false,
            "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "ct2075"
                }
            },
            "thead": [{
                    "label": "柜面交易码",
                    "prop": "trxn_code"
                },
                {
                    "label": "柜面交易码名称",
                    "prop": "trxn_code_name"
                },
                {
                    "label": "操作",
                    "fixed": "left",
                    "btns": [{
                        "type": "bringback",
                        "label": "选择"
                    }]
                }
            ]
        }
    }
}