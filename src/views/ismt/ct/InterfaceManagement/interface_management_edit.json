{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "scope": "templateScope",
            "initRequest":"${{params}}",
            "initDataOnce":true,
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "post",
              "params":{
                "servicecode": "AS_ICMS_PB_UPSRFC"
              }
            },
            "btns":[
				{
					"type":"submit",
					"label":"提交"
				}
			],
            "controls": [
                {
                    "ifaceno": {
                        "label": "接口编号",
                        "width": "130px",
                        "control": "string",
                        "span": 12
                    },
                    
                    "ifacena": {
                        "label": "接口名称",
                        "control": "text",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入接口名称"
                            }
                        ]
                    },
                    "ifacetp": {
                        "control": "select",
                        "label": "接口状态",
                        "dict": {
                            "dictType": "E_IFACTP",
                            "dictKey": [
                                "E_IFACTP"
                            ],
                            "format": "label"
                        },
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择接口状态"
                            }
                        ]
                        
                    },
                    "ifasyscd": {
                        "label": "接口交互系统编号",
                        "control": "text",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入接口交互系统编号"
                            }
                        ]
                    }
                }
            ]
        }
    }
}