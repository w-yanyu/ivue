{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "controls": [
                {
                    "ifaceno": {
                        "label": "接口编号",
                        "control": "text",
                        "rules": [
                            {
                                "max": 20,
                                "message": "长度不能超过20个字符"
                            }
                        ]
                    }
                }
            ],
            "foldLineNumber": 3
        },
        "toolbar": {
            "btns": [
                {
                    "type": "add",
                    "target": "dialog",
                    "label": "新增",
                    "toRequest": {
                        "method": "POST",
                        "url": "/views/ismt/ct/InterfaceManagement/interface_management_add.json"
                    },
                    "props": {
                        "width": "65%",
                        "title": "接口信息新增页面"
                    }
                }
            ]
        },
        "datagrid": {
            "isInitSearch": false,
            "scope": "searchTable",
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "post",
                "params": {
                    "servicecode": "AS_ICMS_PB_SESRFC"
                }
            },
            "thead": [
                {
                    "label": "序号",
                    "type": "index"
                },
                {
                    "label": "接口编号",
                    "prop": "ifaceno"
                },
                {
                    "label": "接口名称",
                    "prop": "ifacena"
                },
                {
                    "label": "接口状态",
                    "prop": "ifacetp",
                    "type": "select",
                    "dict": {
                        "dictType": "E_IFACTP",
                        "dictKey": [
                            "E_IFACTP"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "接口交互系统编号",
                    "prop": "ifasyscd"
                },
                {
                    "label": "操作",
                    "fixed": "right",
                    "width": 200,
                    "align":"center",
                    "btns": [
                     
                        {
                            "type": "edit",
                            "label": "维护",
                            "toRequest": {
                                "url": "/views/ismt/ct/InterfaceManagement/interface_management_edit.json",
                                "method": "post"
                            },
                            "props": {
                                "width": "65%",
                                "title": "接口信息维护页面"
                            }
                        },
                       
                        {
                            "type": "delete",
                            "label": "删除",
                            "doRequest": {
                                "url": "/SUMP/icmcall/icmRPCCall",
                                "method": "post",
                                "params": {
                                    "servicecode": "AS_ICMS_PB_DESRFC"
                                }
                            }
                        }
                    ]
                }
            ],
            "pagination": {
                "show": true
            }
        }
    }
}