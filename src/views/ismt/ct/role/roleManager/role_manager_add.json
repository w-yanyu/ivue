{
    "module": "v_inputDataForm_btn",
    "layout": {
      "form": {
        "initRequest": {
          "url": "/SUMP/icmcall/icmRPCCall",
          "method": "POST",
          "params": {
            "servicecode": "ct3200",
            "role_id": "${{params.role_id}}"
          }
        },
        "controls": [
          {
            "role_id": {
              "control": "text",
              "label": "角色ID",
              "width": "100px",
              "rules": [
                {
                  "required": true,
                  "message": "请输入角色ID"
                },
                {
                  "max": 64,
                  "message": "长度不能超过64个字符"
                }
              ]
            },
            "role_name": {
              "control": "text",
              "label": "角色名称",
              "width": "100px",
              "rules": [
                {
                  "required": true,
                  "message": "请输入角色名称"
                },
                {
                  "max": 100,
                  "message": "长度不能超过100个字符"
                }
              ]
            },
            "role_type": {
              "control": "select",
              "label": "角色类型",
              "width": "100px",
              "dict": {
                "dictType": "CT_E_ROLE_TYPE",
                "dictKey": [
                  "CT_E_ROLE_TYPE"
                ],
                "format": "label"
              },
              "rules": [
                {
                  "required": true,
                  "message": "请选择角色类型"
                }
              ]
            }
          }
        ]
      }
    }
  }
  