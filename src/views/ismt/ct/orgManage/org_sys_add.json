{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "ct2322"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "system_id": {
                        "control": "text",
                        "label": "系统号",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "系统号"
                            }
                        ]
                    },
                    "system_name": {
                        "control": "text",
                        "label": "系统名称",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "系统名称"
                            }
                        ]
                    }
                },
                {
                    "is_inited": {
                        "control": "select",
                        "label": "是否初始化",
                        "width": "130px",
                        "default": 0,
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "value-label"
                        },
                        "rules": [
                            {
                                "required": true
                            }
                        ]
                    }
                }
            ]
        }
    }
}