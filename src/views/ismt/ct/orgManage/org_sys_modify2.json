{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "appId":"deciOnl",
                            "servicecode": "ms2352"
                        }
                    }
                }
            ],

            "controls": [
                {
                    "target_org_id": {
                        "control": "text",
                        "label": "新增法人号",
                        "width": "130px",
                        "value":"${{params.org_id}}",
                        "disabled":true,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入新增法人号"
                            }
                        ]
                    },
                    "is_inited": {
                        "control": "select",
                        "disabled": true,
                        "label": "是否初始化",
                        "width": "130px",
                        "default": 0,
                        "dict": {
                        "dictType": "MS_E_YESORNO",
                        "dictKey": [
                          "MS_E_YESORNO"
                        ],
                        "format": "value-label"
                        },
                        "rules": [
                            {
                                "required": true
                            }
                        ]
                    }
                },
                {
                    "system_id": {
                        "control": "text",
                        "label": "子系统号",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "子系统号"
                            }
                        ]
                    }
                }
            ]
        }
    }
}
