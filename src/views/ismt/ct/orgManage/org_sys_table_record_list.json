{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "isInitSearch": true,
      "pagination": {
        "show": false
      },
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "ct2347",
          "busi_seq": "${{params.busi_seq}}",
          "system_id": "${{params.system_id}}"
        }
      },
      "thead": [
        {
          "type": "string",
          "label": "表名",
          "align": "center",
          "prop": "table_name"
        },
        {
          "type": "string",
          "label": "表描述",
          "align": "center",
          "prop": "table_desc"
        },
        {
          "type": "string",
          "label": "替换字段信息",
          "align": "center",
          "prop": "column_list"
        },
        {
          "align": "center",
          "prop": "org_field_name",
          "type": "text",
          "label": "法人字段名称",
          "nowrap": true,
          "dict": {
            "dictType": "domain_code",
            "dictKey": ["section"],
            "format": "[value]label",
            "valueField": "domain_code",
            "labelField": "domain_name",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct3302"
              }
            }
          }
        },
          {
            "align": "center",
            "prop": "system_id",
            "type": "text",
            "label": "法人字段名称",
            "nowrap": true,
            "dict": {
              "dictType": "system_id",
              "dictKey": ["section"],
              "format": "[value]label",
              "valueField": "system_id",
              "labelField": "system_name",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "ct2321",
                  "length": 50
                }
              }
            }}   ,

          {
            "type": "string",
            "label": "处理状态",
            "align": "center",
            "prop": "tran_state",
            "dict": {
              "dictType": "COM_E_BAT_TRAN_STATE",
              "dictKey": [
                "COM_E_BAT_TRAN_STATE"
              ],
              "format": "label"
            }
          }
        ]
      }
    }
  }