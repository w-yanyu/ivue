{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "scope": "formData",
      "controls": [
        {
          "target_org_id": {
            "control": "text",
            "label": "新增法人号",
            "value":"${{params.org_id}}",
            "disabled": true
          },
          "system_id": {
            "control": "select",
            "label": "子系统",
            "dict": {
                "dictType": "MS_APPID",
                "dictKey": [
                    "MS_APPID"
                ],
                "format": "value-label"
            }
          },
          "is_inited": {
              "control": "select",
              "label": "是否初始化",
              "selectFirstValue":false,
              "dict": {
                  "dictType": "MS_E_YESORNO",
                  "dictKey": [
                      "MS_E_YESORNO"
                  ],
                  "format": "value-label"
              }
          }
        }
      ]
    },

    "toolbar": {
      "btns": [
        {
          "type": "add",
          "icon": "el-icon-circle-plus",
          "target": "dialog",
          "label": "新增",
          "toRequest": {
            "method": "GET",
            "url": "/views/ismt/ct/orgManage/org_sys_add2.json",
            "params": {
              "appId":"deciOnl",
              "org_id":"${{params.org_id}}"
            }
          },
          "props": {
            "title": "新增初始化系统"
          }
        }
      ]
    },

    "datagrid": {
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "appId":"deciOnl",
          "servicecode": "ms2351"
        }
      },
      "thead": [
        {
          "type":"string",
          "label": "新增法人号",
          "align":"center",
          "prop": "target_org_id"
        },
        {
          "type":"string",
          "label": "子系统",
          "align":"center",
          "prop": "system_id",
          "dict": {
            "dictType": "MS_APPID",
            "dictKey": [
                "MS_APPID"
            ],
            "format": "value-label"
          }
        },
        {
          "type":"string",
          "label": "是否初始化",
          "align":"center",
          "width":"200%",
          "prop": "is_inited",
          "dict":{
            "dictType": "MS_E_YESORNO",
            "dictKey": [
              "MS_E_YESORNO"
            ],
            "format": "value-label"

          }
        },
        {
          "label": "操作",
          "fixed": "right",
          "width":"250%",
          "align":"center",
          "btns": [
            {
              "type": "custom",
              "label": "初始化脚本管理",
              "toRequest": {
                "url": "/views/ismt/ct/orgManage/org_sys_file_management.json",
                "method": "POST",
                "params":{
                  "target_org_id":"${{current.target_org_id}}"
                }
              },
              "props": {
                "title": "系统初始化脚本管理",
                "width": "65%"
              }
            },
            {
                "type": "look",
                "label": "删除",
                "show": {"expression":"'${{current.is_inited}}'=== '0'"},
                "doRequest": {
                    "url": "/SUMP/icmcall/icmRPCCall",
                    "method": "POST",
                    "params": {
                        "appId":"deciOnl",
                        "servicecode": "ms2354"
                    }
                },
                "confirm": {
                    "title": "确定删除",
                    "confirmButtonText": "确定",
                    "cancelButtonText": "取消"
                }
            },
            {
                "type": "look",
                "label": "刷新",
                "doRequest": {
                    "url": "/SUMP/icmcall/icmRPCCall",
                    "method": "POST",
                    "params": {
                        "servicecode": "ms0012",
                        "appId": "deciOnl"
                    }
                }
            }
          ]
        }
      ]
    }
  }
}
