{
    "module": "v_realonlyForm_btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "controls": [
                {
                    "org_id": {
                        "control": "text",
                        "label": "法人号",
                        "disabled":true,
                        "width": "130px"
                    },
                    "ccy_code": {
                        "control": "select",
                        "disabled":true,
                        "label": "默认币种",
                        "width": "130px",
                        "dict": {
                        "dictType": "MS_SETT_CURRENCY",
                        "dictKey": [
                            "MS_SETT_CURRENCY"
                        ],
                        "format": "value-label"
                        }
                    }
                },
                {
                    "org_name": {
                        "control": "text",
                        "label": "法人名称",
                        "disabled":true,
                        "width": "130px"
                    },
                    "org_address": {
                        "control": "text",
                        "label": "法人地址",
                        "disabled":true,
                        "width": "130px"
                    }
                },
                {
                    "upper_org": {
                        "control": "text",
                        "label": "上级法人",
                        "disabled":true,
                        "width": "130px"
                    },
                    "multi_ccy_flag": {
                        "control": "select",
                        "disabled":true,
                        "label": "多币种标志",
                        "width": "130px",
                        "dict": {
                        "dictType": "MS_E_YESORNO",
                        "dictKey": [
                          "MS_E_YESORNO"
                        ],
                        "format": "value-label"
                        },
                        "showItem": {
                          "1": [
                            "trxn_ccy"
                          ]
                        }
                    }
                },
                {
                    "org_level": {
                        "control": "select",
                        "disabled":true,
                        "label": "法人级别",
                        "width": "130px",
                        "dict": {
                        "dictType": "MS_E_CORPLEVEL",
                        "dictKey": [
                          "MS_E_CORPLEVEL"
                        ],
                        "format": "label"
                        }
                    },
                    "system_date": {
                        "control": "dateTimePicker",
                        "label": "系统日期",
                        "width": "130px",
                        "align": "center",
                        "disabled":true
                    }
                },
                {
                    "trxn_ccy": {
                        "control": "checkbox",
                        "span": "24",
                        "label": "支持币种",
                        "disabled":true,
                        "value":"156",
                        "dict": {
                          "dictType": "MS_SETT_CURRENCY",
                          "dictKey": ["MS_SETT_CURRENCY"],
                          "labelField": "dictName",
                          "valueField": "dictId"
                        }
                      }
                }        
            ]
        }
    }
}