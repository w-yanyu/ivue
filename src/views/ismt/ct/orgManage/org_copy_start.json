{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "appId": "deciOnl",
                            "servicecode": "ms0011"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "org_id": {
                        "control": "remoteSelect",
                        "label": "被复制法人号",
                        "valueField": "org_id",
