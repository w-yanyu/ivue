{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "scope": "formData",
            "initRequest": "${{params}}",
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "ct3301"
                }
            },
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": ""
                }
            ],
            "controls": [
                {
                    "domain_code": {
                        "control": "text",
                        "label": "字段列",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入字段列"
                            }
                        ]
                    },
                    "domain_name": {
                        "control": "text",
                        "label": "字段名称",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入字典名称"
                            }
                        ]
                    }
                },
                {
                    "data_type": {
                        "control": "select",
                        "label": "数据类型",
                        "width": "130px",
                        "dict": {
                            "dictType": "CT_E_DATA_TYPE",
                            "dictKey": [
                                "CT_E_DATA_TYPE"
                            ],
                            "format": "label"
                        },
                        "rules": [
                            {
                                "required": false,
                                "message": "请选择值类型"
                            }
                        ]
                    },
                    "data_size": {
                        "control": "text",
                        "label": "数据长度",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入数据长度"
                            }
                        ]
                    }
                },
                {
                    "data_precision": {
                        "control": "text",
                        "label": "数据精度",
                        "width": "130px",
                        "rules": [
                            {
                                "required": false,
                                "message": "请输入数据精度"
                            }
                        ]
                    },
                    "interest_rate_desc": {
                        "control": "text",
                        "label": "说明信息",
                        "width": "130px"
                    }
                }
            ]
        }
    }
}