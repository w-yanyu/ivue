{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ct3305"
        }
      },
      "controls": [
        {
          "data_perm_name": {
            "control": "text",
            "label": "权限名称",
            "span": 18,
            "rules": [
              {
                "required": true,
                "message": "请输入权限名称！"
              },
              {
                "max": 20,
                "message": "权限名称不超过20个字符！"
              }
            ]
          }
        },
        {
          "is_dynamic": {
            "label": "动态权限",
            "control": "switch",
            "span": 18,
            "activeValue": "1",
            "inactiveValue": "0",
            "value": "0"
          }
        },
        {
          "interest_rate_desc": {
            "control": "textarea",
            "label": "说明",
            "span": 18,
            "rules": [
              {
                "max": 256,
                "message": "不超过256个字符！"
              }
            ]
          }
        }
      ]
    }
  }
}
