{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "btns": [],
      "controls": [
        {
          "column_operator": {
            "span": 18,
            "control": "select",
            "label": "列连接符",
            "value": "${{params.column_operator}}",
            "rules": [
              {
                "required": true,
                "message": "请选择"
              }
            ],
            "dict": {
              "format": "label",
              "dictKey": ["CT_COLUMN_OPERATOR"],
              "dictType": "CT_COLUMN_OPERATOR"
            },
            "showItem": {
              "isNull": [
                ""
              ],
              "isNotNull": [
                ""
              ],
              "$default": [
                "rule_value"
              ]
            }
          }
        },
        {
          "rule_value": {
            "initShow": false,
            "span": 18,
            "control": "text",
            "label": "规则值",
            "value": "${{params.rule_value}}",
            "rules": [
              {
                "required": true,
                "message": "请输入"
              },
              {
                "message": "最长不超过4000个字符！",
                "max": 4000
              }
            ]
          }
        }
      ]
    }
  }
}
