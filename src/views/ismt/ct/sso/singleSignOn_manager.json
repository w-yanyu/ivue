{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "controls": [
              {
                "system_name": {
                  "control": "text",
                  "label": "系统名称",
                  "span": 8
                }
              }
            ]
          },
        "toolbar": {
            "btns": [
                {
                    "type": "add",
                    "target": "addlog",
                    "label": "新增",
                    "toRequest": {
                      "method": "GET",
                      "url": "/views/ismt/ct/sso/singleSignOn_add.json.json"
                    },
                    "props": {
                      "width": "60%",
                      "title": "维护单点登录系统"
                    }
                  } 
            ]
        },
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": true,
            "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "ct3402"
                }
            },
            "pagination": {
                "show": true,
                "pageSize": 10
            },
            "thead": [
                {
                    "type": "index"
                },
                {
                    "label": "系统编号",
                    "prop": "system_id"
                },
                {
                    "label": "系统名称",
                    "prop": "system_name"
                },
                {
                    "label": "跳转地址",
                    "prop": "redirect_url"
                },{
                    "label": "操作",
                    "align": "center",
                    "width": "250px",
                    "fixed": "right",
                    "btns": [
                      
                      {
                        "type": "look",
                        "label": "维护",
                        "toRequest": {
                          "url": "/views/ismt/ct/sso/singleSignOn_upd.json",
                          "method": "GET",
                          "params": {
                          }
                        },
                        "props": {
                          "width": "60%",
                          "title": "维护单点登录系统"
                        }
                      },

                      {
                        "type": "custom",
                        "label": "删除",
                        "doRequest": {
                          "url": "/SUMP/icmcall/icmRPCCall",
                          "method": "POST",
                          "params": {
                            "servicecode": "ct3405"
                          }
                        },
                        "confirm": {
                          "title": "删除",
                          "msg": "确定删除吗？",
                          "confirmButtonText": "确定",
                          "cancelButtonText": "取消"
                        }
                      },
                      {
                        "type": "look",
                        "label": "详情",
                        "toRequest": {
                          "url": "/views/ismt/ct/sso/singleSignOn_detail.json",
                          "method": "GET",
                          "params": {
                          }
                        },
                        "props": {
                          "width": "60%",
                          "title": "单点登录系统详情"
                        }
                      }
                    ]
                  }
               
            ]
        }
    }
}