{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "ct3403"
        }
      },
      "btns": [],
      "controls": [
        {
          "system_id": {
            "control": "text",
            "label": "系统编号",
            "disabled":true
           
          },
          "system_name": {
            "control": "text",
            "label": "系统名称",
            "disabled":true
          },
          "redirect_url": {
            "control": "text",
            "label": "跳转地址",
            "disabled":true
          },
          "system_icon_url": {
            "control": "text",
            "label": "展示图标",
            "disabled":true
          },
          
          "additional_param": {
            "control": "text",
            "label": "附加参数",
            "disabled":true
          },
          "enable_ind": {
            "control": "select",
            "label": "是否可用",
            "disabled":true,
           
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          }
        }
      ]
    }
  }
}