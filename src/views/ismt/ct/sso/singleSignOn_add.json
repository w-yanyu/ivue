{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "ct3403"
        }
      },
      "controls": [
        {
          "system_id": {
            "control": "text",
            "label": "系统编号",
            "rules": [
              {
                "required": true,
                "message": "请输入系统编号"
              }
            ]
          },
          "system_name": {
            "control": "text",
            "label": "系统名称",
            "rules": [
              {
                "required": true,
                "message": "请输入系统名称"
              }
            ]
          },
          "redirect_url": {
            "control": "text",
            "label": "跳转地址",
            "rules": [
              {
                "required": true,
                "message": "请输入跳转地址"
              }
            ]
          },
          "system_icon_url": {
            "control": "text",
            "label": "展示图标"
          },
          "additional_param": {
            "control": "text",
            "label": "附加参数"
          },
          "enable_ind": {
            "control": "select",
            "label": "是否可用",
            "rules": [
              {
                "required": true,
                "message": "请输入必须项"
              }
            ],
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          }
         
        }
      ]
    }
  }
}