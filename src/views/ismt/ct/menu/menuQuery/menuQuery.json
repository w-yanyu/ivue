{
    "module": "v_search_btn_datagrid",
    "pageId": "A0301",
    "layout": {
        "form": {
            "controls": [
                {
                    "menu_id": {
                        "control": "text",
                        "label": "菜单编号",
                        "rules": [
                            {
                                "max": 10,
                                "message": "长度不能超过10个字符"
                            }
                        ]
                    },
                    "menu_code": {
                        "control": "text",
                        "label": "菜单码",
                        "rules": [
                            {
                                "max": 32,
                                "message": "长度不能超过32个字符"
                            }
                        ]
                    }
                },
                {
                    "menu_group": {
                        "authId":"A030101",
                        "control": "text",
                        "label": "菜单组别",
                        "rules": [
                            {
                                "max": 2,
                                "message": "长度不能超过2个字符"
                            }
                        ]
                    }
                }
            ]
        },
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": false,
            "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "ct3104"
                }
            },
            "pagination": {
                "show": true
            },
            "thead": [
                {
                    "type": "index"
                },
                {
                    "label": "菜单编号",
                    "prop": "menu_id"
                },
                {
                    "label": "菜单码",
                    "prop": "menu_code"
                },
                {
                    "label": "菜单描述",
                    "prop": "menu_desc"
                },
                {
                    "label": "菜单组别",
                    "prop": "menu_group"
                },
                {
                    "label": "上级菜单编号",
                    "prop": "menu_upper_id"
                },
                {
                    "label": "默认菜单标志",
                    "prop": "menu_default_ind",
                    "dict": {
                        "dictType": "MS_E_YESORNO",
                        "dictKey": [
                            "MS_E_YESORNO"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "页面路径",
                    "prop": "page_id"
                },
                {
                    "label": "参数列表",
                    "prop": "params"
                }
            ]
        }
    }
}