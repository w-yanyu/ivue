{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "controls": [
                {
                    "menu_code": {
                        "label": "菜单编号",
                        "control": "string",
                        "rules": [
                            {
                                "max": 32,
                                "message": "长度不能超过32个字符"
                            }
                        ]
                    },
                    "menu_id": {
                        "label": "菜单编号",
                        "control": "string",
                        "rules": [
                            {
                                "max": 10,
                                "message": "长度不能超过10个字符"
                            }
                        ]
                    },
                    "menu_upper_id": {
                        "label": "父级",
                        "control": "text",
                        "rules": [
                            {
                                "max": 10,
                                "message": "长度不能超过10个字符"
                            }
                        ]
                    },
                    "page_id": {
                        "label": "菜单URL",
                        "control": "text",
                        "rules": [
                            {
                                "max": 32,
                                "message": "长度不能超过32个字符"
                            }
                        ]
                    },
                    "iconfg": {
                        "label": "菜单图标",
                        "control": "text"
                    },
                    "menu_desc": {
                        "label": "菜单名称",
                        "control": "text",
                        "rules": [
                            {
                                "max": 100,
                                "message": "长度不能超过100个字符"
                            }
                        ]
                    },
                    "sortno": {
                        "label": "排序",
                        "control": "text"
                    },
                    "authType": {
                        "label": "权限类型",
                        "control": "text"
                    }
                }
            ],
            "initRequest": {
                "url": "/SUMP/rest/auth/single-menu",
                "method": "POST"
            },
            "doRequest": {
                "url": "/SUMP/rest/auth/updAuth",
                "method": "POST"
            }
        }
    }
}