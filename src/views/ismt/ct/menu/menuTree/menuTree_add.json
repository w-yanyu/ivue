{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/call/RPCCall",
            "method": "POST",
            "params": {
              "servicecode": "ct3101"
            }
          }
        }
      ],
      "controls": [
        {
          "menu_id": {
            "control": "text",
            "rules": [
              {
                "message": "请输入菜单编号",
                "required": false
              }
            ],
            "label": "菜单编号"
          },
          "title": {
            "control": "hidden",
            "rules": [
              {
                "message": "请输入title",
                "required": false
              }
            ],
            "label": "title"
          },
          "menu_code": {
            "control": "text",
            "rules": [
              {
                "message": "请输入菜单码",
                "required": true
              }
            ],
            "label": "菜单码"
          },
          "menu_upper_id": {
            "control": "text",
            "disabled": true,
            "value": "${{params.menu_id}}",
            "rules": [
              {
                "message": "请输入上级菜单编号",
                "required": false
              }
            ],
            "label": "上级菜单编号"
          },
          "menu_desc": {
            "control": "text",
            "rules": [
              {
                "message": "请输入菜单名称",
                "required": false
              }
            ],
            "label": "菜单名称",
            "events": {
              "condition": {
                "$default": {
                  "control": {
                    "title": {
                      "value": "[${{form.menu_id}}]${{form.menu_desc}}"
                    }
                  }
                }
              }
            }
          },
          "menu_group": {
            "control": "text",
            "rules": [
              {
                "message": "请输入菜单组别",
                "required": false
              }
            ],
            "label": "菜单组别"
          },
          "page_display_scene": {
            "control": "text",
            "rules": [
              {
                "message": "请输入菜单图标",
                "required": false
              }
            ],
            "label": "菜单图标"
          },
          "page_id": {
            "control": "text",
            "rules": [
              {
                "message": "请输入页面路径",
                "required": false
              }
            ],
            "label": "页面路径"
          },
          "menu_show_ind": {
            "dict": {
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "dictType": "MS_E_YESORNO"
            },
            "control": "select",
            "label": "菜单显示标志"
          },
          "menu_default_ind": {
            "dict": {
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "dictType": "MS_E_YESORNO"
            },
            "control": "select",
            "label": "默认菜单标志"
          },
          "sort_no": {
            "control": "inputNumber",
            "label": "菜单序号",
            "rules": [
              {
                "message": "请输入菜单序号",
                "required": true
              }
            ]
          },
          "channel_list": {
            "dict": {
              "dictKey": [
                "E_CHANNEL_LIST"
              ],
              "dictType": "E_CHANNEL_LIST"
            },
            "control": "select",
            "multiple": true,
            "format": "value-label",
            "label": "可显示渠道"
          },
          "params": {
            "rules": [
              {
                "message": "请输入参数列表",
                "required": false
              }
            ],
            "control": "text",
            "label": "参数列表"
          }
        }
      ]
    },
    "alert": {
      "span": 8,
      "listStyle": "disc",
      "description": [
        "<p style='color:red; text-align: left'>参数列表样例：</p>",
        "<p style='text-align: left'>    单值传参：param1=123;param2=345</p>",
        "<p style='text-align: left'>    多值传参：param1=123|234|abc;param2=678;</p>"
      ]
    }
  }
}
