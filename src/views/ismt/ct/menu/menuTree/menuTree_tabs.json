{
  "module": "dynamicTabs",
  "layout": {
    "form": {
          },
    "tabs": {
      "tabHeader": [
        {
          "id": "tab1",
          "label": "菜单信息编辑",
          "closable": false,
          "disabled": false,
          "icon": "el-icon-date",
          "toRequest": {
            "url": "/views/ismt/ct/menu/menuTree/menuTree_update.json",
            "method": "post"
          }
        },
        {
          "id": "tab2",
          "label": "控件管理",
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "hide": "${{params.ishide}}",
          "toRequest": {
            "url": "/views/ismt/ct/control/form_control_manager.json",
            "method": "post"
          }
        }
      ],
      "defaultActiveKey": "tab1",
      "position": "top",
      "styleClass": "card"
    }
  }
}
