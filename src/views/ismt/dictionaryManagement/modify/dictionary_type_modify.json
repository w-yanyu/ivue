{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "scope": "formData",
            "initRequest": "${{params}}",
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "ct3318"
                }
            },
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": ""
                }
            ],
            "controls": [
                {
                    "dict_type": {
                        "control": "string",
                        "label": "字典类型",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入字典类型"
                            }
                        ]
                    },
                    "dict_name": {
                        "control": "text",
                        "label": "字典名称",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入字典名称"
                            }
                        ]
                    }
                },
                {
                    "ms_appid": {
                        "control": "text",
                        "label": "业务模块",
                        "width": "130px"
                    },
                    "value_type": {
                        "control": "select",
                        "label": "值类型",
                        "width": "130px",
                        "dict": {
                            "dictType": "CT_E_VALUE_TYPE",
                            "dictKey": [
                                "CT_E_VALUE_TYPE"
                            ],
                            "format": "label"
                        },
                        "rules": [
                            {
                                "required": false,
                                "message": "请选择值类型"
                            }
                        ]
                    }
                },
                {
                    "interest_rate_desc": {
                        "control": "text",
                        "label": "说明信息",
                        "width": "130px"
                    }
                }
            ]
        }
    }
}