{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "scope": "formData",
            "initRequest": "${{params}}",
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "ct2206",
                    "recdver": "${{params.recdver}}"
                }
            },
            "btns": [{
                "type": "submit",
                "label": "提交",
                "icon": ""
            }],
            "controls": [
                {
                    "dict_type": {
                        "control": "string",
                        "label": "字典类型",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入字典类型"
                            }
                        ]
                    },
                    "dict_id": {
                        "control": "string",
                        "label": "字典id",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入字典id"
                            }
                        ]
                    }
                },
                {
                    "dict_name": {
                        "control": "text",
                        "label": "字典名称",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入字典名称"
                            }
                        ]
                    },
                    "parent_dict_type": {
                        "control": "text",
                        "label": "上一级字典类型",
                        "width": "130px"
                    }
                },
                {
                    "parent_dict_id": {
                        "control": "text",
                        "label": "上一级字典id",
                        "width": "130px"
                    },
                    "status": {
                        "control": "select",
                        "label": "是否有效",
                        "width": "130px",
                        "name":"status",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        },
                        "rules": [
                            {
                                "required": false,
                                "message": "请选择是否有效"
                            }
                        ]
                    }
                },
                {
                    "sort_no": {
                        "control": "text",
                        "label": "字典编号",
                        "width": "130px",
                        "name": "bal_attributes"
                        
                    },
                    "dict_type_name": {
                        "control": "text",
                        "label": "字典类型名称",
                        "width": "130px"
                    }
                }
            ]
        }
    }
}