{
    "module": "v-form-editableDataGrid-btn",
    "layout": {
        "form": {
            "controls": [
            ]
        },
        "toolbar": {
            "btns": [
                {
                    "type": "add",
                    "label": "新增",
                    "icon": "el-icon-plus"
                }
            ]
        },
        "datagrid": {
            "pagination": {
                "show": false
            },
            "thead": [
                {
                    "prop": "dict_type",
                    "type": "text",
                    "label": "字典类型"
                },
                {
                    "prop": "dict_id",
                    "type": "text",
                    "width": "80px",
                    "label": "字典id"
                },
                {
                    "prop": "dict_name",
                    "type": "text",
                    "label": "字典名称"
                },
                {
                    "prop": "parent_dict_type",
                    "type": "text",
                    "width": "100px",
                    "label": "父级字典类型"
                },
                {
                    "prop": "parent_dict_id",
                    "type": "text",
                    "width": "90px",
                    "label": "父级字典id"
                },
                {
                    "prop": "status",
                    "type": "select",
                    "label": "是否有效",
                    "dict": {
                        "dictType": "MS_E_YESORNO",
                        "dictKey": [
                            "MS_E_YESORNO"
                        ],
                        "format": "label"
                    }
                },
                {
                    "prop": "parent_dict_id",
                    "type": "text",
                    "width": "80px",
                    "label": "字典编号"
                },
                {
                    "prop": "dict_type_name",
                    "type": "text",
                    "width": "100px",
                    "label": "字典类型名称"
                },
                {
                    "label": "操作",
                    "width": "70px",
                    "btns": [
                        {
                            "type": "delete",
                            "label": "删除",
                            "icon": "el-icon-close"
                        }
                    ]
                }
            ]
        },
        "footer": {
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": "el-icon-tickets",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "ct2208"
                        }
                    }
                }
            ]
        }
    }
}