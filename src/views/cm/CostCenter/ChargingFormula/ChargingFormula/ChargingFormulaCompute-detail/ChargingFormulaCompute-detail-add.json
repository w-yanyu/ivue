{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": "",
                    "doRequest": {
                        "url": "/SUMP/call/RPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "adcmdt"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "chrgfm": {
                        "control": "text",
                        "label": "计费公式代码"
                    },
                    "brchno": {
                        "control": "text",
                        "label": "机构号"
                    }
                },
                {
                    "crcycd": {
                        "control": "text",
                        "label": "币种"
                    },
                    "limiam": {
                        "control": "text",
                        "label": "区间金额下限"
                    }
                },
                {
                    "cufetp": {
                        "control": "select",
                        "label": "计费类型",
                        "dict": {
                            "dictType": "E_CUFETP",
                            "dictKey": [
                                "E_CUFETP"
                            ],
                            "format": "label"
                        }
                    },
                    "chrgrt": {
                        "control": "text",
                        "label": "收费比例"
                    }
                },
                {
                    "pecgam": {
                        "control": "text",
                        "label": "计费单价"
                    },
                    "cgmnam": {
                        "control": "text",
                        "label": "最低金额"
                    }
                },
                {
                    "cgmxam": {
                        "control": "text",
                        "label": "最高金额"
                    },
                    "remark": {
                        "control": "text",
                        "label": "备注信息"
                    }
                }
            ]
        }
    }
}