{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": "",
                    "doRequest": {
                        "url": "/SUMP/call/RPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "adcmdf"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "fmunam": {
                        "control": "text",
                        "label": "公式名称"
                    },
                    "filebs": {
                        "control": "text",
                        "label": "文件依据"
                    }  
                },
                {
                    "efctdt": {
                        "control": "dateTimePicker",
                        "label": "生效日期",
                        "format":"yyyy年MM月dd日"
                    },
                    "inefdt": {
                        "control": "dateTimePicker",
                        "label": "失效日期",
                        "format":"yyyy年MM月dd日"
                    }
                },
                {
                    "chrgfm": {
                        "control": "text",
                        "label": "计费公式代码"
                    }
                }
            ]
        }
    }
}