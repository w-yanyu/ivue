{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "controls": [
                {
                    "chrgcd": {
                        "control": "text",
                        "label": "费种代码"
                    },
                    "scencd": {
                        "control": "text",
                        "label": "场景代码"
                    }
                }
               
            ]
        },
        "toolbar": {
            "btns": [
                {
                    "type": "add",
                    "target": "dialog",
                    "label": "新增",
                    "toRequest": {
                        "method": "GET",
                        "url": "/views/cm/CostCenter/SceneManagement/SceneCharging/SceneChargingManagement-add.json",
                        "params": {
                            
                        }
                    },
                    "props": {
                        "title": "新增场景计费管理"
                    }
                },
                {
                    "type": "export",
                    "label": "导出",
                    "doRequest": {
                        "method": "",
                        "url": ""
                    }
                }
            ]
        },
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": false,
            "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "qrsecd"
                }
            },
            "thead": [
                {
                    "label": "场景代码",
                    "prop": "scencd"
                },
                {
                    "label": "场景名称",
                    "prop": "scends"
                },
                {
                    "label": "费种代码",
                    "prop": "chrgcd"
                },
                {
                    "label": "费种代码名称",
                    "prop": "chrgna"
                },
                {
                    "label": "顺序号",
                    "prop": "seqnum"
                },
                {
                    "label": "备注",
                    "prop": "remark"
                },
                {
                    "label": "操作",
                    "fixed": "right",
                    "width": 200,
                    "align": "center",
                    "btns": [
                        {
                            "type": "edit",
                            "label": "修改",
                            "toRequest": {
                                "url": "/views/cm/CostCenter/SceneManagement/SceneCharging/SceneChargingManagement-update.json",
                                "method": "GET"
                            },
                            "props": {
                                "title": "修改场景计费管理"
                            }
                        },
                        {
                            "type": "edit",
                            "label": "删除",
                            "toRequest": {
                                "url": "/views/cm/CostCenter/SceneManagement/SceneCharging/SceneChargingManagement-delete.json",
                                "method": "GET"
                            },
                            "props": {
                                "title": "删除场景计费管理"
                            }
                        }
                    ]
                }
            ]
        }
    }
}