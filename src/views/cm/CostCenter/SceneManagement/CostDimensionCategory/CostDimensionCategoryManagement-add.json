                {
                    "type": "submit",
                    "label": "提交",
                    "icon": "",
                    "doRequest": {
                        "url": "/SUMP/call/RPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "addime"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "dimena": {
