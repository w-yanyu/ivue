{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": "",
                    "doRequest": {
                        "url": "/SUMP/call/RPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "dlsedf"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "evetcd": {
                        "control": "text",
                        "label": "事件编号",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入事件编号"
                            }
                        ]
                    }
                },
                {
                    "evetna": {
                        "control": "text",
                        "label": "事件名称",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入事件名称"
                            }
                        ]
                    }
                },
                {
                    "remark": {
                        "control": "text",
                        "label": "备注",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入备注"
                            }
                        ]
                    }
                }
            ]
        }
    }
}

