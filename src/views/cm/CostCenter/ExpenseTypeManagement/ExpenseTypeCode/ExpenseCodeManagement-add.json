{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": "",
                    "doRequest": {
                        "url": "/SUMP/call/RPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "adchdf"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "chrgna": {
                        "control": "text",
                        "label": "费用代码名称"
                    },
                    "cgpyrv": {
                        "control": "select",
                        "label": "费用收付标志",
                        "dict": {
                            "dictType": "E_CGPYRV",
                            "dictKey": [
                                "E_CGPYRV"
                            ],
                            "format": "label"
                        }
                    }  
                },
                {
                    "fetype": {
                        "control": "select",
                        "label": "费种大类",
                        "dict": {
                            "dictType": "E_FETYPE",
                            "dictKey": [
                                "E_FETYPE"
                            ],
                            "format": "label"
                        }
                    },
                    "crcycd": {
                        "control": "text",
                        "label": "币种"
                    }
                },
                {
                    "mndecm": {
                        "control": "text",
                        "label": "最低小数位数"
                    },
                    "carrtp": {
                        "control": "select",
                        "label": "四舍五入方式",
                        "dict": {
                            "dictType": "E_CARRTP",
                            "dictKey": [
                                "E_CARRTP"
                            ],
                            "format": "label"
                        }
                    }
                },
                {
                    "lysptp": {
                        "control": "select",
                        "label": "分层方式",
                        "dict": {
                            "dictType": "E_LYSPTP",
                            "dictKey": [
                                "E_LYSPTP"
                            ],
                            "format": "label"
                        }
                    },
                    "cufesr": {
                        "control": "select",
                        "label": "计费金额来源",
                        "dict": {
                            "dictType": "E_CUFESR",
                            "dictKey": [
                                "E_CUFESR"
                            ],
                            "format": "label"
                        }
                    }
                },
                {
                    "felytp": {
                        "control": "select",
                        "label": "计费分层取值类型",
                        "dict": {
                            "dictType": "E_FELYTP",
                            "dictKey": [
                                "E_FELYTP"
                            ],
                            "format": "label"
                        }
                    },
                    "isfavo": {
                        "control": "select",
                        "label": "是否允许优惠",
                        "dict": {
                            "dictType": "E_ISFAVO",
                            "dictKey": [
                                "E_ISFAVO"
                            ],
                            "format": "label"
                        }
                    }
                },
                {
                    "mnfvrt": {
                        "control": "text",
                        "label": "最低优惠比例"
                    },
                    "mxfvrt": {
                        "control": "text",
                        "label": "最高优惠比例"
                    }
                },
                {
                    "fedive": {
                        "control": "select",
                        "label": "收费分润标志",
                        "dict": {
                            "dictType": "E_FEDIVE",
                            "dictKey": [
                                "E_FEDIVE"
                            ],
                            "format": "label"
                        }
                    },
                    "chrgsr": {
                        "control": "select",
                        "label": "收费金额来源",
                        "dict": {
                            "dictType": "E_CHRGSR",
                            "dictKey": [
                                "E_CHRGSR"
                            ],
                            "format": "label"
                        }
                    }
                },
                {
                    "cjsign": {
                        "control": "select",
                        "label": "县辖维护标志",
                        "dict": {
                            "dictType": "E_CJSIGN",
                            "dictKey": [
                                "E_CJSIGN"
                            ],
                            "format": "label"
                        }
                    },
                    "efctdt": {
                        "control": "dateTimePicker",
                        "label": "生效日期",
                        "format":"yyyy年MM月dd日"
                    }
                },
                {
                    "inefdt": {
                        "control": "dateTimePicker",
                        "label": "失效日期",
                        "format":"yyyy年MM月dd日"
                    }
                }
            ]
        }
    }
}