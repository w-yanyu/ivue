{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "controls": [
                {
                    "chrgcd": {
                        "control": "text",
                        "label": "费用代码"
                    },
                    "chrgna": {
                        "control": "text",
                        "label": "费用代码名称"
                    }
                },
                {
                    "cgpyrv": {
                        "control": "text",
                        "label": "费用收付标志"
                    },
                    "fetype": {
                        "control": "select",
                        "label": "费种大类",
                        "dict": {
                            "dictType": "E_FETYPE",
                            "dictKey": [
                                "E_FETYPE"
                            ],
                            "format": "label"
                        }
                    }
                }
            ]
        },
        "toolbar": {
            "btns": [
                {
                    "type": "add",
                    "target": "dialog",
                    "label": "新增",
                    "toRequest": {
                        "method": "GET",
                        "url": "/views/cm/CostCenter/ExpenseTypeManagement/ExpenseTypeCode/ExpenseCodeManagement-add.json"
                    },
                    "props": {
                        "title": "费种代码新增"
                    }
                }
            ]
        },
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": false,
            "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "qrchdf",
                    "page_id": ""
                }
            },
            "thead": [
                {
                    "type": "index"
                },
                {
                    "label": "费种代码",
                    "prop": "chrgcd"
                },
                {
                    "label": "费种代码名称",
                    "prop": "chrgna"
                },
                {
                    "label": "费用收付标志",
                    "prop": "cgpyrv"
                },
                {
                    "label": "费种大类",
                    "prop": "fetype",
                    "dict": {
                        "dictType": "E_FETYPE",
                        "dictKey": [
                            "E_FETYPE"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "币种",
                    "prop": "crcycd"
                },
                {
                    "label": "最低小数位数",
                    "prop": "mndecm"
                },
                {
                    "label": "四舍五入方式",
                    "prop": "carrtp"
                },
                {
                    "label": "分层方式",
                    "prop": "lysptp"
                },
                {
                    "label": "收费分层取值类型",
                    "prop": "felytp"
                },
                {
                    "label": "是否允许优惠",
                    "prop": "isfavo"
                },
                {
                    "label": "最高优惠比例",
                    "prop": "mxfvrt"
                },
                {
                    "label": "最低优惠比例",
                    "prop": "mnfvrt"
                },
                {
                    "label": "收费分润标志",
                    "prop": "fedive"
                },
                {
                    "label": "收费金额涞源",
                    "prop": "chrgsr"
                },
                {
                    "label": "县辖维护标志",
                    "prop": "cjsign"
                },
                {
                    "label": "生效日期",
                    "prop": "efctdt"
                },
                {
                    "label": "失效日期",
                    "prop": "inefdt"
                },
                {
                    "label": "操作",
                    "fixed": "right",
                    "width": 200,
                    "align":"center",
                    "btns": [
                        {
                            "type": "edit",
                            "label": "维护",
                            "toRequest": {
                                "url": "",
                                "method": "GET"
                            },
                            "props": {
                                "title": "机构信息维护"
                            }
                        },
                        {
                            "type": "edit",
                            "label": "删除",
                            "toRequest": {
                                "url": "",
                                "method": "GET"
                            },
                            "props": {
                                "title": "机构信息删除"
                            }
                        }
                    ]
                }
            ]
        }
    }
}