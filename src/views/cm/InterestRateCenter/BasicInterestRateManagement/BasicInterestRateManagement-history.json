{
    "module": "v_search_btn_datagrid",
    "layout": {
        "initRequest": "${{params}}",
        "toolbar": {
        },
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": false,
            "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "selrhs",
                    "rfircd":"${{params.rfircd}}"
                }
            },
            "thead": [
                {
                    "label": "法人代码",
                    "prop": "corpno"
                },
                {
                    "label": "基础利率类型",
                    "prop": "rfirtp",
                    "dict": {
                        "dictType": "E_RFIRTP",
                        "dictKey": [
                            "E_RFIRTP"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "基础利率代码",
                    "prop": "rfircd"
                },
                {
                    "label": "利率种类",
                    "prop": "intrkd",
                    "dict": {
                        "dictType": "E_INTRTY",
                        "dictKey": [
                            "E_INTRTY"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "币种",
                    "prop": "crcycd",
                    "dict": {
                        "format": "label",
                        "dictKey": [
                            "MS_CURRENCY"
                        ],
                        "dictType": "MS_CURRENCY"
                    }
                },
                {
                    "label": "期限",
                    "prop": "rfirtm",
                    "dict": {
                        "dictType": "E_TERMCD",
                        "dictKey": [
                            "E_TERMCD"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "利率名称",
                    "prop": "intrna"
                },
                {
                    "label": "利率性质",
                    "prop": "intrfg",
                    "dict": {
                        "dictType": "E_INRATE",
                        "dictKey": [
                            "E_INRATE"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "基础利率",
                    "prop": "baseir"
                },
                {
                    "label": "浮动点数上限",
                    "prop": "flmxsc"
                },
                {
                    "label": "浮动点数下限",
                    "prop": "flmnsc"
                },
                {
                    "label": "生效日期",
                    "prop": "efctdt"
                },
                {
                    "label": "生效时间",
                    "prop": "efcttm"
                },
                {
                    "label": "失效日期",
                    "prop": "inefdt"
                },
                {
                    "label": "失效时间",
                    "prop": "ineftm"
                }
            ]
        }
    }
}