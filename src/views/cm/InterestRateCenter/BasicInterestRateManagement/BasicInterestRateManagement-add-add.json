{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "bringBack": true,
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": ""
                }
            ],
            "controls": [
                {
                    "rfirtm": {
                        "control": "select",
                        "label": "期限",
                        "dict": {
                            "dictType": "E_TERMCD",
                            "dictKey": [
                              "E_TERMCD"
                            ],
                            "format": "label"
                          },
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择期限"
                            }
                        ]
                    },
                    "intrfg": {
                        "control": "select",
                        "label": "利率性质",
                        "dict": {
                            "dictType": "E_INRATE",
                            "dictKey": [
                              "E_INRATE"
                            ],
                            "format": "label"
                          },
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择利率性质" 
                            }
                        ]
                    }
                },
                {
                    "baseir": {
                        "control":  "text",
                        "label": "基础利率",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入基础利率"
                            }
                        ]
                    },
                    "flmxsc": {
                        "control": "text",
                        "label": "浮动点数上限",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入浮动点数上限"
                            }
                        ]
                    }
                },
                {
                    "flmnsc": {
                        "control": "text",
                        "label": "浮动点数下限",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入浮动点数下限"
                            }
                        ]
                    },
                    "efctdt": {
                        "control": "dateTimePicker",
                        "label": "生效日期",
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择生效日期"
                            }
                        ]
                    }
                },
                {
                    "efcttm": {
                        "control": "timePicker",
                        "label": "生效时间",
                        "valueFormat":"HHmmss",
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择生效时间"
                            }
                        ]
                    }
                }
            ]
        }
    }
}
