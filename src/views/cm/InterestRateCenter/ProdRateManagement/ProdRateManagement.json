{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "controls": [
                {
                    "crdtcd": {
                        "control": "text",
                        "label":  "项目编号",
                        "rules": [
                            {
                                "max": 20,
                                "message": "长度不能超过20个字符"
                            }
                        ]
                    },
                    "custlabel": {
                        "control": "select",
                        "label":  "客群",
                        "dict": {
                          "dictType": "E_CUSLAB",
                          "dictKey": [
                            "E_CUSLAB"
                          ],
                          "format": "label",
                        "labelField":"dictName",
                        "valueField":"dictId"
                      }
                    }
                },
                {
                    
                    "areacode": {
                        "control": "text",
                        "label":  "区域中心代码",
                        "rules": [
                            {
                                "max": 100,
                                "message": "长度不能超过100个字符"
                            }
                        ]
                    }
                }
            ]
        },
        "toolbar": {
            "btns": [
                {
                  "type": "add",
                  "target": "dialog",
                  "label": "新增",
                  "toRequest": {
                      "method": "GET",
                      "url": "/views/cm/InterestRateCenter/ProdRateManagement/ProdRateManagement_add.json"
                  },
                  "props": {
                      "width": "75%",
                      "title":  "项目差异化信息新增"
                  }
              }
            ]
        }
        ,
        "datagrid": {
            "isInitSearch": false,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "initProd"
                }
            },
            "pagination": {
                "pageSizes": [
                    10,
                    15,
                    20
                ],
                "pageSize": 10
            },
            "thead": [
                {
                    "label": "项目编号",
                    "prop": "crdtcd"
                },
                {
                    "label":"项目名称",
                    "prop": "prodna"
                },
                {
                    "label":  "客群",
                    "prop": "cust_label",
                    "dict": {
                      "dictType": "E_CUSLAB",
                      "dictKey": [
                        "E_CUSLAB"
                      ],
                      "format": "label",
                    "labelField":"dictName",
                    "valueField":"dictId"
                  }
                },
                {
                    "label":  "区域中心代码",
                    "prop": "area_code"
                },
                {
                    "label":  "区域中心名称",
                    "prop": "area_name"
                },
                {
                    "label":  "定价方案编号",
                    "prop": "rate_code"
                },
                {
                    "label":  "定价方案名称",
                    "prop": "rate_name"
                },
                {
                  "label": "操作",
                  "fixed": "right",
                  "width": 200,
                  "align":"center",
                  "btns": [
                    {
                        "type": "look",
                        "label": "定价方案详情",
                        "toRequest": {
                            "url": "/views/cm/InterestRateCenter/ProdRateManagement/LookRatePlanRequest.json",
                            "method": "POST"
                        },
                        "props": {
                            "width": "75%",
                            "title": "利率详情信息"
                          }
                    },
                    {
                      "type": "edit",
                      "label": "维护",
                      "toRequest": {
                        "url": "/views/cm/InterestRateCenter/ProdRateManagement/ProdRateManagement_update.json",
                        "method": "POST"
                      },
                      "props": {
                        "width": "75%",
                        "title": "项目差异化信息维护"
                      }
                    },  
                    {
                        "type": "delete",
                        "label": "删除",
                        "doRequest": {
                            "url": "/SUMP/call/RPCCall",
                            "method": "POST",
                            "params": {
                                "servicecode": "deleteprod"
                            }
                        }
                    }
                  ]
                }
            ]
        },
        "alert": {
          "span": 8,
            "title": "客群类别说明：",
            "description": [
                "A-A类; B-B类; ALL-全部。"
            ],
            "listStyle": "disc"
        }
    }
}