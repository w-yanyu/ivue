{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "updatemnplan",
                    "planlist":"[${{form}}]",
                    "rate_code":"rate_code"
                }
            },
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": ""
                }
            ],
            "controls": [
                {
                    "subTerm": {
                        "control": "select",
                        "label": "期数",
                        "dict": {
                            "dictType": "E_PERIOD",
                            "dictKey": [
                                "E_PERIOD"
                            ],
                            "format": "value"
                        }
                    },
                    "subRate": {
                        "control": "text",
                        "label": "分段利率"
                        
                    },
                    "mark":{
                        "control": "textarea",
                        "label": "备注信息"
                    }
                },
                {
                    "onlyno": {
                        "control": "hidden",
                        "label": "编号",
                        "value":"${{params.onlyno}}"
                       
                    }
                }
               

            ]
        }
    }
}
