{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "insertplan",
                    "prioif":"[${{form}}]"
                }
            },
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "url":"/views/cm/InterestRateCenter/RatePlanManagement/RatePlanMnManagement_add.json"
                }
            ],
            "controls": [
                {
                    "rate_code": {
                        "control": "text",
                        "label": "定价方案编号",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入定价方案编号号"
                            },
                            {
                                "max": 20,
                                "message": "长度不能超过20个字符"
                            }
                        ]
                    },
                    "rate_name": {
                        "control":  "text",
                        "label": "定价方案名称"
                    }
                    
                },
                {
                    
                    "periods": {
                        "control": "select",
                        "label": "期数",
                        "dict": {
                            "dictType": "E_PERIOD",
                            "dictKey": [
                                "E_PERIOD"
                            ],
                            "format": "value"
                        }
                        
                    },
                    "interest_rate": {
                        "control": "text",
                        "label": "利率",
                        "rules": [
                            {
                                "required": true,
                                "message": "新增时利率不得为空"
                            }
                        ]

                    }
                   
                },
                {
                    "repayment_type": {
                        "control": "select",
                        "label": "还款方式",
                        "dict": {
                            "dictType": "E_SCHDTD",
                            "dictKey": [
                                "E_SCHDTD"
                            ],
                            "format": "label"
                        }
                    },
                    "subsidy_flag": {
                        "control": "select",
                        "label": "是否贴息",
                        "dict": {
                            "dictType": "E_SUBFLG",
                            "dictKey": [
                                "E_SUBFLG"
                            ],
                            "format": "label"
                        }
                    }
                }

            ]
        }
    }
}