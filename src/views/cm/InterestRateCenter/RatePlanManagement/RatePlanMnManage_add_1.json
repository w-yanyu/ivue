{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "scope": "formData",
            "initRequest": "${{params}}",
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "insertPlanMn"
                }
                },
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": ""
                }
            ],
            "controls": [
                {
                    "rate_code": {
                        "control": "text",
                        "label": "定价方案编号",
                        "disabled": true
                    }, 
                    "periods": {
                        "control": "select",
                        "label": "期数",
                        "dict": {
                            "dictType": "E_PERIOD",
                            "dictKey": [
                                "E_PERIOD"
                            ],
                            "format": "value"
                        }
                    }
                },
                {
                    
                    "period": {
                        "control": "select",
                        "label": "期数",
                        "dict": {
                            "dictType": "E_PERIOD",
                            "dictKey": [
                                "E_PERIOD"
                            ],
                            "format": "value"
                        }
                    },
                    "interest_rate": {
                        "control": "text",
                        "label": "M+N模式利率"
                    },
                    "mark":{
                        "control": "textarea",
                        "label": "备注信息"
                    }
                   
                }

            ]
        }
    }
}
