{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "controls": [  
                {
                    "rate_code": {
                        "control": "hidden",
                        "label":  "定价方案编号",
                        "value":"${{params.rate_code}}"
                    },
                    "rate_name": {
                        "control": "hidden",
                        "value":"${{params.rate_name}}",
                        "label":  "利率"
                    }
                }
            ],
            "btns":[],
            "a":"${{params.rate_name}}"
        },
            
        "datagrid": {
            "isInitSearch": true,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "selRateInfo"
                }
            },
            "pagination": {
                "pageSizes": [
                    10,
                    15,
                    20
                ],
                "pageSize": 10
            },
            "thead": [
                {
                    "label":  "定价方案编号",
                    "prop": "rate_code"
                },
                {
                    "label":  "定价方案名称",
                    "prop": "rate_name"
                },
                {
                    "label":  "还款方式",
                    "prop": "repayment_type",
                    "dict": {
                        "dictType": "E_SCHDTP",
                        "dictKey": [
                            "E_SCHDTP"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label":  "期数",
                    "prop": "periods"
                },
                {
                    "label":  "利率",
                    "prop": "interest_rate"
                }
                ]
                  
            
        }
    }
}