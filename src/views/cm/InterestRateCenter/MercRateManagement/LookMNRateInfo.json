{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "controls": [
                {    "rate_code": {
                    "control": "text",
                    "value":"${{params.rate_code}}",
                    "label":  "定价方案编号"
                },
                    "rate_name": {
                        "control": "text",
                        "value":"${{params.rate_name}}",
                        "label":  "定价方案名称"
                    }
                   
                }
               
            ],
            "btns": []
        },
      
            
        "datagrid": {
            "isInitSearch": true,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    
                    "servicecode": "mnformerc"
                }
            },
            
            "thead": [
                {
                    "label":  "定价方案编号",
                    "prop": "rate_code"
                },
                {
                    "label":  "定价方案名称",
                    "prop": "rate_name"
                },
                {
                    "label":  "期数",
                    "prop": "subTerm"
                },
                {
                    "label":  "利率",
                    "prop": "subRate"
                }
            ]
        }
    }
}
