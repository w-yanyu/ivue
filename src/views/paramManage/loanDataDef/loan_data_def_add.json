{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/call/RPCCall",
            "method": "POST",
            "params": {
              "servicecode": "ap0000",
              "dtbs_oprn_flg": "0",
              "parm_tab_nm": "klnp_agnc_info",
              "data_pkg": "${{current}}"
            }
          }
        }
      ],
      "controls": [
        {
          "modu_code": {
            "control": "text",
            "label": "模块代码",
            "width": "100px",
            "rules": [
              {
                "required": true,
                "message": "请输入模块代码"
              },
              {
                "max": 20,
                "message": "长度不能超过20个字符"
              }
            ]
          },
          "data_class_code": {
            "control": "text",
            "label": "数据类代码",
            "width": "100px",
            "rules": [
              {
                "required": true,
                "message": "请输入数据类代码"
              },
              {
                "max": 20,
                "message": "长度不能超过20个字符"
              }
            ]
          }
        },{
          "data_code": {
            "control": "text",
            "label": "数据代码",
            "width": "100px",
            "rules": [
              {
                "required": true,
                "message": "请输入数据代码"
              },
              {
                "max": 20,
                "message": "长度不能超过20个字符"
              }
            ]
          },
          "data_nm": {
            "control": "text",
            "label": "数据名称",
            "width": "100px",
            "rules": [
              {
                "max": 100,
                "message": "长度不能超过100个字符"
              }
            ]
          }
        },
        {
          "data_comt": {
            "control": "text",
            "label": "数据描述",
            "width": "100px",
            "rules": [
              {
                "max": 80,
                "message": "长度不能超过80个字符"
              }
            ]
          },
          "serl_num_long": {
            "control": "text",
            "label": "序号",
            "width": "100px",
            "rules": [
              {
                "max": 16,
                "message": "长度不能超过16个字符"
              }
            ]
          }
        }
      ]
    }
  }
}
