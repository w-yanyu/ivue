{
  "module": "v-realonlyForm-btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "controls": [
        {
          "adv_rpymt_fine_num": {
            "control": "string",
            "label": "提前还款罚金编号"
          },
          "adv_rpymt_fine_nm": {
            "control": "string",
            "label": "提前还款罚金名称"
          }
        },
        {
          "eff_dt": {
            "control": "string",
            "label": "生效日期",
            "disabled": true
          },
          "invld_dt": {
            "control": "string",
            "label": "失效日期",
            "disabled": true
          }
        },
        {
          "max_fine_amt": {
            "control": "string",
            "label": "最大罚金金额"
          },
          "min_fine_amt": {
            "control": "string",
            "label": "最大罚金金额"
          }
        },
        {
          "fine_caln_tp": {
            "control": "string",
            "label": "罚金计算类型",
            "dict": {
              "dictType": "LN_E_FINE_CALN_TP",
              "dictKey": [
                "LN_E_FINE_CALN_TP"
              ],
              "format": "label"
            }
          },
          "int_dys": {
            "label": "利息天数",
            "control": "string"
          }
        },
        {
          "strt_dt_rule": {
            "control": "string",
            "label": "起始日期规则",
            "dict": {
              "dictType": "LN_E_STRT_DT_RULE",
              "dictKey": [
                "LN_E_STRT_DT_RULE"
              ],
              "format": "label"
            }
          },
          "fixd_amt": {
            "control": "string",
            "label": "固定金额"
          }
        },
        {
          "layer_val_rule": {
            "control": "string",
            "label": "分层取值规则",
            "dict": {
              "dictType": "LN_E_LAYER_VAL_RULE",
              "dictKey": [
                "LN_E_LAYER_VAL_RULE"
              ],
              "format": "label"
            }
          },
          "serl_num_long": {
            "control": "string",
            "label": "序号",
            "rules": [
              {
                "max": 8,
                "message": "长度不能超过8个字符"
              }
            ]
          }
        }
      ]
    }
  }
}
