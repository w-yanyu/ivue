{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/call/RPCCall",
            "method": "POST",
            "params": {
              "servicecode": "ap0000",
              "parm_tab_nm": "klnp_adv_rpymt_fine",
              "dtbs_oprn_flg": "0",
              "data_pkg": "${{current}}"
            }
          }
        }
      ],
      "controls": [
        {
          "adv_rpymt_fine_num": {
            "control": "text",
            "label": "提前还款罚金编号",
            "rules": [
              {
                "required": true,
                "message": "请输入提前还款罚金编号"
              },
              {
                "max": 10,
                "message": "长度不能超过10个字符"
              }
            ]
          },
          "adv_rpymt_fine_nm": {
            "control": "text",
            "label": "提前还款罚金名称",
            "rules": [
              {
                "max": 80,
                "message": "长度不能超过80个字符"
              }
            ]
          }
        },
        {
          "eff_dt": {
            "control": "dateTimePicker",
            "label": "生效日期",
            "rules": [
              {
                "max": 8,
                "message": "长度不能超过8个字符"
              }
            ]
          },
          "invld_dt": {
            "control": "dateTimePicker",
            "label": "失效日期",
            "rules": [
              {
                "max": 8,
                "message": "长度不能超过8个字符"
              }
            ]
          }
        },
        {
          "max_fine_amt": {
            "control": "currency",
            "label": "最大罚金金额"
          },
          "min_fine_amt": {
            "control": "currency",
            "label": "最小 罚金金额"
          }
        },
        {
          "fine_caln_tp": {
            "control": "select",
            "label": "罚金计算类型",
            "dict": {
              "dictType": "LN_E_FINE_CALN_TP",
              "dictKey": [
                "LN_E_FINE_CALN_TP"
              ],
              "format": "label"
            }
          },
          "int_dys": {
            "label": "利息天数",
            "control": "inputNumber"
          }
        },
        {
          "strt_dt_rule": {
            "control": "select",
            "label": "起始日期规则",
            "dict": {
              "dictType": "LN_E_STRT_DT_RULE",
              "dictKey": [
                "LN_E_STRT_DT_RULE"
              ],
              "format": "label"
            }
          },
          "fixd_amt": {
            "control": "currency",
            "label": "固定金额"
          }
        },
        {
          "layer_val_rule": {
            "control": "select",
            "label": "分层取值规则",
            "dict": {
              "dictType": "LN_E_LAYER_VAL_RULE",
              "dictKey": [
                "LN_E_LAYER_VAL_RULE"
              ],
              "format": "label"
            }
          },
          "serl_num_long": {
            "control": "text",
            "label": "序号",
            "rules": [
              {
                "max": 8,
                "message": "长度不能超过8个字符"
              }
            ]
          }
        }
      ]
    }
  }
}
