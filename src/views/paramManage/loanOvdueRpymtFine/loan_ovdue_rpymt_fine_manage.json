{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "ovdue_rpymt_fine_num": {
            "control": "text",
            "label": "逾期还款罚金编号",
            "rules": [
              {
                "max": 10,
                "message": "长度不能超过10个字符"
              }
            ]
          },
          "ovdue_rpymt_fine_nm": {
            "control": "text",
            "label": "逾期还款罚金名称",
            "rules": [
              {
                "max": 80,
                "message": "长度不能超过80个字符"
              }
            ]
          }
        },
        {
          "eff_dt": {
            "control": "dateTimePicker",
            "label": "生效日期",
            "rules": [
              {
                "max": 8,
                "message": "长度不能超过8个字符"
              }
            ]
          },
          "invld_dt": {
            "control": "dateTimePicker",
            "label": "失效日期",
            "rules": [
              {
                "max": 8,
                "message": "长度不能超过8个字符"
              }
            ]
          }
        },
        {
          "fine_chrg_rule": {
            "control": "select",
            "label": "罚金收取规则",
            "dict": {
              "dictType": "LN_E_FINE_CHRG_RULE",
              "dictKey": [
                "LN_E_FINE_CHRG_RULE"
              ],
              "format": "label"
            }
          },
          "fine_caln_meth": {
            "control": "select",
            "label": "罚金计算方法",
            "dict": {
              "dictType": "LN_E_FINE_CALN_METH",
              "dictKey": [
                "LN_E_FINE_CALN_METH"
              ],
              "format": "label"
            }
          }
        },
        {
          "ovdue_rpymt_fine_base_num": {
            "control": "select",
            "label": "逾期还款罚金基数",
            "dict": {
              "dictType": "LN_E_OVDUE_RPYMT_FINE_BASE_NUM",
              "dictKey": [
                "LN_E_OVDUE_RPYMT_FINE_BASE_NUM"
              ],
              "format": "label"
            }
          },
          "grace_prd_chrg_fine_rule": {
            "control": "select",
            "label": "宽限期收取罚金规则",
            "dict": {
              "dictType": "LN_E_GRACE_PRD_CHRG_FINE_RULE",
              "dictKey": [
                "LN_E_GRACE_PRD_CHRG_FINE_RULE"
              ],
              "format": "label"
            }
        }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "target": "dialog",
          "label": "新增",
          "toRequest": {
            "method": "GET",
            "url": "/views/paramManage/loanOvdueRpymtFine/loan_ovdue_rpymt_fine_add.json"
          },
          "props": {
            "title": "新增"
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ap0000",
          "parm_tab_nm": "klnp_ovdue_rpymt_fine",
          "dtbs_oprn_flg": "3",
          "data_pkg": "${{current}}"
        }
      },
      "thead": [
        {
          "label": "逾期还款罚金编号",
          "prop": "ovdue_rpymt_fine_num",
          "align": "center"
        },
        {
          "label": "逾期还款罚金名称",
          "prop": "ovdue_rpymt_fine_nm",
          "align": "center"
        },
        {
          "label": "生效日期",
          "prop": "eff_dt",
          "align": "center"
        },
        {
          "label": "失效日期",
          "prop": "invld_dt",
          "align": "center"
        },
        {
          "label": "最大罚金金额",
          "prop": "max_fine_amt",
          "align": "center"
        },
        {
          "label": "最小罚金金额",
          "prop": "min_fine_amt",
          "align": "center"
        },
        {
          "label": "罚金收取规则",
          "prop": "fine_chrg_rule",
          "align": "center",
          "dict": {
            "dictType": "LN_E_FINE_CHRG_RULE",
            "dictKey": [
              "LN_E_FINE_CHRG_RULE"
            ],
            "format": "label"
          }
        },
        {
          "label": "罚金计算方法",
          "prop": "fine_caln_meth",
          "align": "center",
          "dict": {
            "dictType": "LN_E_FINE_CALN_METH",
            "dictKey": [
              "LN_E_FINE_CALN_METH"
            ],
            "format": "label"
          }
        },
        {
          "label": "罚金金额/百分比",
          "prop": "fine_amt_pcnt",
          "align": "center"
        },
        {
          "label": "逾期还款罚金基数",
          "prop": "ovdue_rpymt_fine_base_num",
          "align": "center",
          "dict": {
            "dictType": "LN_E_OVDUE_RPYMT_FINE_BASE_NUM",
            "dictKey": [
              "LN_E_OVDUE_RPYMT_FINE_BASE_NUM"
            ],
            "format": "label"
          }
        },
        {
          "label": "宽限期收取罚金规则",
          "prop": "grace_prd_chrg_fine_rule",
          "align": "center",
          "dict": {
            "dictType": "LN_E_GRACE_PRD_CHRG_FINE_RULE",
            "dictKey": [
              "LN_E_GRACE_PRD_CHRG_FINE_RULE"
            ],
            "format": "label"
          }
        },
        {
          "label": "序号",
          "prop": "serl_num_long",
          "align": "center"
        },
        {
          "label": "操作",
          "fixed": "right",
          "width": 300,
          "align": "center",
          "btns": [
            {
              "type": "look",
              "label": "详情",
              "toRequest": {
                "url": "/views/paramManage/loanOvdueRpymtFine/loan_ovdue_rpymt_fine_info.json",
                "method": "POST"
              },
              "props": {
                "width": "50%",
                "title": "信息详情"
              }
            },
            {
              "type": "edit",
              "label": "维护",
              "toRequest": {
                "url": "/views/paramManage/loanOvdueRpymtFine/loan_ovdue_rpymt_fine_update.json",
                "method": "GET"
              },
              "props": {
                "width": "50%",
                "title": "信息维护"
              }
            },
            {
              "type": "custom",
              "label": "删除",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "ap0000",
                  "parm_tab_nm": "klnp_ovdue_rpymt_fine",
                  "dtbs_oprn_flg": "2",
                  "data_pkg": {
                    "ovdue_rpymt_fine_num":"${{current.ovdue_rpymt_fine_num}}"
                  }
                }
              },
              "confirm": {
                "title": "确定删除",
                "confirmButtonText": "确定",
                "cancelButtonText": "取消"
              }
            }
          ]
        }
      ]
    }
  }
}
