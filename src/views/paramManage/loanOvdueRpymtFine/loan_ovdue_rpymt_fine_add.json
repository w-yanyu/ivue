{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/call/RPCCall",
            "method": "POST",
            "params": {
              "servicecode": "ap0000",
              "parm_tab_nm": "klnp_ovdue_rpymt_fine",
              "dtbs_oprn_flg": "0",
              "data_pkg": "${{current}}"
            }
          }
        }
      ],
      "controls": [
        {
          "ovdue_rpymt_fine_num": {
            "control": "text",
            "label": "逾期还款罚金编号",
            "rules": [
              {
                "max": 10,
                "message": "长度不能超过10个字符"
              },
              {
                "required": true,
                "message": "请输入逾期还款罚金编号"
              }
            ]
          },
          "ovdue_rpymt_fine_nm": {
            "control": "text",
            "label": "逾期还款罚金名称",
            "rules": [
              {
                "max": 80,
                "message": "长度不能超过80个字符"
              }
            ]
          }
        },
        {
          "eff_dt": {
            "control": "dateTimePicker",
            "label": "生效日期",
            "rules": [
              {
                "max": 8,
                "message": "长度不能超过8个字符"
              }
            ]
          },
          "invld_dt": {
            "control": "dateTimePicker",
            "label": "失效日期",
            "rules": [
              {
                "max": 8,
                "message": "长度不能超过8个字符"
              }
            ]
          }
        },
        {
          "max_fine_amt": {
            "control": "currency",
            "label": "最大罚金金额"
          },
          "min_fine_amt": {
            "control": "currency",
            "label": "最小罚金金额"
          }
        },
        {
          "fine_chrg_rule": {
            "control": "select",
            "label": "罚金收取规则",
            "dict": {
              "dictType": "LN_E_FINE_CHRG_RULE",
              "dictKey": [
                "LN_E_FINE_CHRG_RULE"
              ],
              "format": "label"
            }
          },
          "fine_caln_meth": {
            "control": "select",
            "label": "罚金计算方法",
            "dict": {
              "dictType": "LN_E_FINE_CALN_METH",
              "dictKey": [
                "LN_E_FINE_CALN_METH"
              ],
              "format": "label"
            }
          }
        },
        {
          "fine_amt_pcnt": {
            "control": "inputNumber",
            "label": "罚金金额/百分比",
            "controls": false,
            "min": 0
          },
          "ovdue_rpymt_fine_base_num": {
            "control": "select",
            "label": "逾期还款罚金基数",
            "dict": {
              "dictType": "LN_E_OVDUE_RPYMT_FINE_BASE_NUM",
              "dictKey": [
                "LN_E_OVDUE_RPYMT_FINE_BASE_NUM"
              ],
              "format": "label"
            }
          }
        },{
          "grace_prd_chrg_fine_rule": {
            "control": "select",
            "label": "宽限期收取罚金规则",
            "dict": {
              "dictType": "LN_E_GRACE_PRD_CHRG_FINE_RULE",
              "dictKey": [
                "LN_E_GRACE_PRD_CHRG_FINE_RULE"
              ],
              "format": "label"
            }
          },
          "serl_num_long": {
            "control": "inputNumber",
            "label": "序号",
            "controls": false,
            "min": 0
          }
        }
      ]
    }
  }
}
