{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "scope":"strForm",
      "initRequest": "${{params}}",
      "doRequest": {
      },
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/call/RPCCall",
            "method": "POST",
            "params": {
              "servicecode": "ap0000",
              "dtbs_oprn_flg": "1",
              "parm_tab_nm": "klnp_prod_grp",
              "data_pkg":"${{current}}"
            }
          }
        }
      ],
      "controls": [
        {
          "prod_grp_code": {
            "control": "text",
            "label": "产品组代码",
            "width": "100px",
            "disabled": true,
            "rules": [
              {
                "required": true,
                "message": "请输入模块代码"
              },
              {
                "max": 30,
                "message": "长度不能超过20个字符"
              }
            ]
          },
          "prod_grp_nm": {
            "control": "text",
            "label": "产品组名称",
            "width": "100px",
            "rules": [
              {
                "max": 100,
                "message": "长度不能超过100个字符"
              }
            ]
          }
        },
        {
          "serl_num_long": {
            "control": "text",
            "label": "序号",
            "width": "100px"
          }
        }
      ]
    }
  }
}
