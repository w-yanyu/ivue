{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/call/RPCCall",
            "method": "POST",
            "params": {
              "servicecode": "ap0000",
              "parm_tab_nm": "klnp_adv_rpymt_fine_layer",
              "dtbs_oprn_flg": "0",
              "data_pkg": "${{current}}"
            }
          }
        }
      ],
      "controls": [
        {
          "adv_rpymt_fine_num": {
            "control": "text",
            "label": "提前还款罚金编号",
            "rules": [
              {
                "required": true,
                "message": "请输入提前还款罚金编号"
              },
              {
                "max": 10,
                "message": "长度不能超过10个字符"
              }
            ]
          },
          "numrcl_val_serl_num": {
            "control": "inputNumber",
            "label": "数值序号",
            "controls": false,
            "min": 0,
            "rules": [
              {
                "required": true,
                "message": "请输入数值序号"
              }
            ]
          }
        },
        {
          "matu_pcnt": {
            "control": "inputNumber",
            "label": "期限百分比",
            "controls": false,
            "min": 0
          },
          "adv_rpymt_fine_base_num": {
            "control": "select",
            "label": "提前还款罚金基数",
            "dict": {
              "dictType": "LN_E_ADV_RPYMT_FINE_BASE_NUM",
              "dictKey": [
                "LN_E_ADV_RPYMT_FINE_BASE_NUM"
              ],
              "format": "label"
            }
          }
        },
        {
          "fine_caln_meth": {
            "control": "select",
            "label": "罚金计算方法",
            "dict": {
              "dictType": "LN_E_FINE_CALN_METH",
              "dictKey": [
                "LN_E_FINE_CALN_METH"
              ],
              "format": "label"
            }
          },
          "fine_amt_pcnt": {
            "control": "inputNumber",
            "label": "罚金金额/百分比",
            "controls": false,
            "min": 0
          }
        },
        {
          "serl_num_long": {
            "control": "inputNumber",
            "label": "序号",
            "controls": false,
            "min": 0
          }
        }
      ]
    }
  }
}
