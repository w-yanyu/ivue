{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "ta7505",
                    "rule_id": "${{params.busi_code}}",
                    "outlistid": "listnm"
                }
            },
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": "",
                    "doRequest": {
                        "url": "/SUMP/call/RPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "ta7532"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "busi_code": {
                        "control": "lookup",
                        "width": "130px",
                        "disabled": true,
                        "edit": true,
                        "rules": [
                            {
                                "required": true,
                                "message": "业务编码"
                            }
                        ],
                        "label": "业务编码",
                        "keyField": {
                            "busi_code": "busi_code",
                            "acct_busi_catg": "acct_busi_catg",
                            "busi_code_comt": "busi_code_comt",
                            "acct_num_serl_num": "acct_num_serl_num",
                            "busi_acctg_code": "busi_acctg_code",
                            "acct_id_code": "acct_id_code",
                            "cptl_bal_charic": "cptl_bal_charic",
                            "bal_upd_mode": "bal_upd_mode",
                            "bal_chk_flg": "bal_chk_flg",
                            "bal_thrhd_min_amt": "bal_thrhd_min_amt",
                            "bal_thrhd_max_amt": "bal_thrhd_max_amt",
                            "cr_manu_acctg_prms": "cr_manu_acctg_prms",
                            "db_manu_acctg_prms": "db_manu_acctg_prms",
                            "end_dy_btch_open_acct_flg": "end_dy_btch_open_acct_flg",
                            "int_caln_flg": "int_caln_flg",
                            "acrl_flg": "acrl_flg",
                            "vld_flg": "vld_flg"
                        },
                        "toRequest": {
                            "url": "/views/paramManage/loanAcInfo/fund_business_info.json"
                        },
                        "props": {
                            "width": "80%",
                            "title": "业务编码查询"
                        },
                        "format": "label"
                    },
                    "acct_busi_catg": {
                        "control": "select",
                        "label": "账户业务种类",
                        "disabled": true,
                        "width": "130px",
                        "dict": {
                            "dictType": "AC_E_ACCT_BUSI_CATG",
                            "dictKey": [
                                "AC_E_ACCT_BUSI_CATG"
                            ],
                            "format": "label"
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择账户业务种类"
                            }
                        ]
                    }
                },
                {
                    "busi_code_comt": {
                        "label": "业务编码说明",
                        "control": "textarea",
                        "width": "130px",
                        "span": 12,
                        "autosize": {
                            "minRows": 3,
                            "maxRows": 5
                        },
                        "maxlength": 600,
                        "showWordLimit": true
                    },
                    "acct_num_serl_num": {
                        "control": "text",
                        "width": "130px",
                        "label": "账号序号",
                        "disabled": true,
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择账号序号"
                            }
                        ]
                    }
                },
                {
                    "busi_acctg_code": {
                        "control": "text",
                        "width": "130px",
                        "label": "核算代码",
                        "disabled": true,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入核算代码"
                            }
                        ]
                    },
                    "aplbl_ccy_list": {
                        "control": "select",
                        "width": "130px",
                        "label": "适用币种列表",
                        "multiple": true,
                        "valueDataType": "string",
                        "dict": {
                            "dictType": "MS_CURRENCY",
                            "dictKey": [
                                "MS_CURRENCY"
                            ],
                            "format": "label"
                        }
                    }
                },
                {
                    "acct_id_code": {
                        "control": "text",
                        "width": "130px",
                        "label": "账户标识码",
                        "disabled": true
                    },
                    "cptl_bal_charic": {
                        "control": "select",
                        "width": "130px",
                        "label": "资金余额性质",
                        "dict": {
                            "dictType": "AC_E_TEMP_RECPT_PYMT_BUSI_CATG",
                            "dictKey": [
                                "AC_E_TEMP_RECPT_PYMT_BUSI_CATG"
                            ],
                            "format": "label"
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择资金余额性质"
                            }
                        ]
                    }
                },
                {
                    "bal_upd_mode": {
                        "control": "select",
                        "label": "余额更新模式",
                        "width": "130px",
                        "dict": {
                            "dictType": "AC_E_BAL_UPD_MODE",
                            "dictKey": [
                                "AC_E_BAL_UPD_MODE"
                            ],
                            "format": "label"
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择余额更新模式"
                            }
                        ]
                    },
                    "bal_chk_flg": {
                        "control": "select",
                        "label": "余额检查标志",
                        "width": "130px",
                        "dict": {
                            "dictType": "AC_E_BAL_CHK_FLG",
                            "dictKey": [
                                "AC_E_BAL_CHK_FLG"
                            ],
                            "format": "label"
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择余额检查标志"
                            }
                        ]
                    }
                },
                {
                    "bal_thrhd_min_amt": {
                        "control": "text",
                        "label": "余额阈值最低金额",
                        "width": "130px",
                        "disabled": true
                    },
                    "bal_thrhd_max_amt": {
                        "control": "text",
                        "label": "余额阈值最高金额",
                        "width": "130px",
                        "disabled": true
                    }
                },
                {
                    "db_manu_acctg_prms": {
                        "control": "select",
                        "label": "借方手工记账许可",
                        "width": "130px",
                        "dict": {
                            "dictType": "AC_E_MANU_ACCTG_PRMS",
                            "dictKey": [
                                "AC_E_MANU_ACCTG_PRMS"
                            ],
                            "format": "label"
                        }
                    },
                    "cr_manu_acctg_prms": {
                        "control": "select",
                        "label": "贷方手工记账许可",
                        "width": "130px",
                        "dict": {
                            "dictType": "AC_E_MANU_ACCTG_PRMS",
                            "dictKey": [
                                "AC_E_MANU_ACCTG_PRMS"
                            ],
                            "format": "label"
                        }
                    }
                },
                {
                    "aplbl_org_list": {
                        "control": "remoteSelect",
                        "label": "适用机构列表",
                        "multiple": true,
                        "valueDataType": "string",
                        "width": "130px",
                        "labelField": "branch_name",
                        "valueField": "branch_id",
                        "doRequest": {
                            "url": "/SUMP/call/RPCCall",
                            "method": "POST",
                            "params": {
                                "servicecode": "ct2173"
                            }
                        },
                        "format": "label"
                    },
                    "end_dy_btch_open_acct_flg": {
                        "control": "select",
                        "label": "日终批开标志",
                        "width": "130px",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择日终批开标志"
                            }
                        ]
                    }
                },
                {
                    "prod_code": {
                        "control": "text",
                        "label": "产品代码",
                        "width": "130px"
                    },
                    "dtbs_oprn_flg": {
                        "control": "select",
                        "label": "数据库操作标志",
                        "value": "1",
                        "width": "130px",
                        "disabled": true,
                        "dict": {
                            "dictType": "AC_E_DTBS_OPRN_FLG",
                            "dictKey": [
                                "AC_E_DTBS_OPRN_FLG"
                            ],
                            "format": "label"
                        }
                    }
                },
                {
                    "int_caln_flg": {
                        "control": "select",
                        "label": "计息标志",
                        "width": "130px",
                        "value": "0",
                        "disabled": true,
                        "initShow": false,
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        }
                    },
                    "acrl_flg": {
                        "control": "select",
                        "label": "计提标志",
                        "width": "130px",
                        "value": "0",
                        "disabled": true,
                        "initShow": false,
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        }
                    }
                },
                {
                    "vld_flg": {
                        "control": "select",
                        "label": "有效标志",
                        "width": "130px",
                        "value": "1",
                        "disabled": true,
                        "initShow": false,
                        "dict": {
                            "dictType": "AC_E_VLD_FLG",
                            "dictKey": [
                                "AC_E_VLD_FLG"
                            ],
                            "format": "label"
                        }
                    }
                }
            ]
        }
    }
}