                        "url": "/SUMP/call/RPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "ta7500",
                            "acct_busi_catg": "10"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "busi_code": {
                        "control": "lookup",
                        "width": "130px",
                        "edit": true,
                        "rules": [
                            {
                                "required": true,
                                "message": "业务编码"
                            }
                        ],
                        "label": "业务编码",
                        "keyField": {
                            "busi_code": "busi_code",
                            "acct_nm": "busi_code_comt",
                            "acct_num_serl_num": "acct_num_serl_num",
                            "bal_upd_mode": "bal_upd_mode",
                            "bal_chk_flg": "bal_chk_flg",
                            "bal_thrhd_min_amt": "bal_thrhd_min_amt",
                            "bal_thrhd_max_amt": "bal_thrhd_max_amt",
                            "cr_manu_acctg_prms": "cr_manu_acctg_prms",
                            "db_manu_acctg_prms": "db_manu_acctg_prms"
                        },
                        "toRequest": {
                            "url": "/views/paramManage/loanAcInfo/fund_business_info.json"
                        },
                        "props": {
                            "width": "80%",
                            "title": "业务编码查询"
                        },
                        "format": "label"
                    },
                    "acct_nm": {
                        "control": "text",
                        "label": "账户名称",
                        "width": "130px"
                    }
                },
                {
                    "ccy_code_num": {
                        "control": "select",
                        "width": "130px",
                        "label": "货币代号",
                        "dict": {
                            "dictType": "MS_CURRENCY",
                            "dictKey": [
                                "MS_CURRENCY"
                            ],
                            "format": "label"
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择货币代号"
                            }
                        ]
                    },
                    "intrl_acct_tp": {
                        "control": "select",
                        "label": "内部账户类型",
                        "value": "2",
                        "width": "130px",
                        "dict": {
                            "dictType": "AC_E_INTRL_ACCT_TP",
                            "dictKey": [
                                "AC_E_INTRL_ACCT_TP"
                            ],
                            "format": "label"
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择内部账户类型"
                            }
                        ]
                    }
                },
                {
                    "acct_num_serl_num": {
                        "control": "text",
                        "label": "账号序号",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入账号序号"
                            }
                        ]
                    },
                    "bal_upd_mode": {
                        "control": "select",
                        "label": "余额更新模式",
                        "width": "130px",
                        "dict": {
                            "dictType": "AC_E_BAL_UPD_MODE",
                            "dictKey": [
                                "AC_E_BAL_UPD_MODE"
                            ],
                            "format": "label"
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择余额更新模式"
                            }
                        ]
                    }
                },
                {
                    "bal_chk_flg": {
                        "control": "select",
                        "label": "余额检查标志",
                        "width": "130px",
                        "dict": {
                            "dictType": "AC_E_BAL_CHK_FLG",
                            "dictKey": [
                                "AC_E_BAL_CHK_FLG"
                            ],
                            "format": "label"
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择余额检查标志"
                            }
                        ]
                    },
                    "bal_thrhd_min_amt": {
                        "control": "text",
                        "label": "余额阈值最低金额",
                        "width": "130px",
                        "disabled": true
                    }
                },
                {
                    "bal_thrhd_max_amt": {
                        "control": "text",
                        "label": "余额阈值最高金额",
                        "width": "130px",
                        "disabled": true
                    },
                    "cr_manu_acctg_prms": {
                        "control": "select",
                        "label": "贷方手工记账许可",
                        "width": "130px",
                        "dict": {
                            "dictType": "AC_E_MANU_ACCTG_PRMS",
                            "dictKey": [
                                "AC_E_MANU_ACCTG_PRMS"
                            ],
                            "format": "label"
                        }
                    }
                },
                {
                    "db_manu_acctg_prms": {
                        "control": "select",
                        "label": "借方手工记账许可",
                        "width": "130px",
                        "dict": {
                            "dictType": "AC_E_MANU_ACCTG_PRMS",
                            "dictKey": [
                                "AC_E_MANU_ACCTG_PRMS"
                            ],
                            "format": "label"
                        }
                    },
                    "acct_oprtg_org": {
                        "control": "text",
                        "label": "序号",
                        "width": "130px",
                        "value": "985000",
                        "disabled": true,
                        "initShow": false
                    }
                }
            ]
        }
    }
}