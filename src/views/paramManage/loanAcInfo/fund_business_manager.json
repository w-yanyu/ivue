{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "controls": [
                {
                    "acct_busi_catg": {
                        "control": "select",
                        "label": "账户业务种类",
                        "value": "10",
                        "width": "130px",
                        "dict": {
                            "dictType": "AC_E_ACCT_BUSI_CATG",
                            "dictKey": [
                                "AC_E_ACCT_BUSI_CATG"
                            ],
                            "format": "label"
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "账户业务种类不能为空"
                            }
                        ]
                    },
                    "busi_code": {
                        "control": "text",
                        "label": "业务编码",
                        "width": "130px"
                    }
                },
                {
                    "busi_code_comt": {
                        "label": "业务编码说明",
                        "control": "textarea",
                        "width": "130px",
                        "span": 12,
                        "autosize": {
                            "minRows": 3,
                            "maxRows": 5
                        },
                        "maxlength": 600,
                        "showWordLimit": true
                    },
                    "acct_num_serl_num": {
                        "control": "text",
                        "width": "130px",
                        "label": "账号序号"
                    }
                },
                {
                    "busi_acctg_code": {
                        "control": "text",
                        "width": "130px",
                        "label": "核算代码"
                    },
                    "cptl_bal_charic": {
                        "control": "select",
                        "width": "130px",
                        "label": "核算性质",
                        "dict": {
                            "dictType": "AC_E_TEMP_RECPT_PYMT_BUSI_CATG",
                            "dictKey": [
                                "AC_E_TEMP_RECPT_PYMT_BUSI_CATG"
                            ],
                            "format": "label"
                        }
                    }
                },
                {
                    "bal_chk_flg": {
                        "control": "select",
                        "width": "130px",
                        "label": "余额检查标志",
                        "dict": {
                            "dictType": "AC_E_BAL_CHK_FLG",
                            "dictKey": [
                                "AC_E_BAL_CHK_FLG"
                            ],
                            "format": "label"
                        }
                    },
                    "bal_upd_mode": {
                        "control": "select",
                        "width": "130px",
                        "label": "余额更新模式",
                        "dict": {
                            "dictType": "AC_E_BAL_UPD_MODE",
                            "dictKey": [
                                "AC_E_BAL_UPD_MODE"
                            ],
                            "format": "label"
                        }
                    }
                }
            ]
        },
        "toolbar": {
            "btns": [
                {
                    "type": "add",
                    "target": "dialog",
                    "label": "新增",
                    "toRequest": {
                        "method": "GET",
                        "url": "/views/paramManage/loanAcInfo/fund_business_add.json"
                    },
                    "props": {
                        "title": "新增"
                    }
                }
            ]
        },
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": false,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "ta7505",
                    "outlistid": "listnm"
                }
            },
            "thead": [
                {
                    "label": "账户业务种类",
                    "prop": "acct_busi_catg",
                    "align": "center",
                    "dict": {
                        "dictType": "AC_E_ACCT_BUSI_CATG",
                        "dictKey": [
                            "AC_E_ACCT_BUSI_CATG"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "业务编码",
                    "prop": "busi_code",
                    "align": "center"
                },
                {
                    "label": "业务编码说明",
                    "prop": "busi_code_comt",
                    "align": "center"
                },
                {
                    "label": "账号序号",
                    "prop": "acct_num_serl_num",
                    "align": "center"
                },
                {
                    "label": "核算代码",
                    "prop": "busi_acctg_code",
                    "align": "center"
                },
                {
                    "label": "账户标识码",
                    "prop": "acct_id_code",
                    "align": "center"
                },
                {
                    "label": "核算性质",
                    "prop": "cptl_bal_charic",
                    "align": "center",
                    "dict": {
                        "dictType": "AC_E_TEMP_RECPT_PYMT_BUSI_CATG",
                        "dictKey": [
                            "AC_E_TEMP_RECPT_PYMT_BUSI_CATG"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "余额更新模式",
                    "prop": "bal_upd_mode",
                    "align": "center",
                    "dict": {
                        "dictType": "AC_E_BAL_UPD_MODE",
                        "dictKey": [
                            "AC_E_BAL_UPD_MODE"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "余额检查标志",
                    "prop": "bal_chk_flg",
                    "align": "center",
                    "dict": {
                        "dictType": "AC_E_BAL_CHK_FLG",
                        "dictKey": [
                            "AC_E_BAL_CHK_FLG"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "余额阈值最低金额",
                    "prop": "bal_thrhd_min_amt",
                    "align": "center"
                },
                {
                    "label": "余额阈值最高金额",
                    "prop": "bal_thrhd_max_amt",
                    "align": "center"
                },
                {
                    "label": "借方手工记账许可",
                    "prop": "db_manu_acctg_prms",
                    "align": "center",
                    "dict": {
                        "dictType": "AC_E_MANU_ACCTG_PRMS",
                        "dictKey": [
                            "AC_E_MANU_ACCTG_PRMS"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "贷方手工记账许可",
                    "prop": "cr_manu_acctg_prms",
                    "align": "center",
                    "dict": {
                        "dictType": "AC_E_MANU_ACCTG_PRMS",
                        "dictKey": [
                            "AC_E_MANU_ACCTG_PRMS"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "日终批开标志",
                    "prop": "end_dy_btch_open_acct_flg",
                    "align": "center",
                    "dict": {
                        "dictType": "MS_E_YESORNO",
                        "dictKey": [
                            "MS_E_YESORNO"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "有效标志",
                    "prop": "vld_flg",
                    "align": "center",
                    "dict": {
                        "dictType": "AC_E_VLD_FLG",
                        "dictKey": [
                            "AC_E_VLD_FLG"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "操作",
                    "align": "center",
                    "fixed": "right",
                    "btns": [
                        {
                            "type": "look",
                            "label": "详情",
                            "toRequest": {
                                "url": "/views/paramManage/loanAcInfo/fund_business_manager_info.json",
                                "method": "POST"
                            },
                            "props": {
                                "width": "70%",
                                "title": "资金业务编码参数详情"
                            }
                        },
                        {
                            "type": "edit",
                            "label": "维护",
                            "toRequest": {
                                "url": "/views/paramManage/loanAcInfo/fund_business_update.json",
                                "method": "GET"
                            },
                            "props": {
                                "width": "70%",
                                "title": "暂收暂付账户信息维护"
                            }
                        },
                        {
                            "type": "custom",
                            "label": "删除",
                            "doRequest": {
                                "url": "/SUMP/icmcall/icmRPCCall",
                                "method": "POST",
                                "params": {
                                    "servicecode": "ta7532",
                                    "dtbs_oprn_flg": 2
                                }
                            },
                            "confirm": {
                                "title": "确定删除",
                                "confirmButtonText": "确定",
                                "cancelButtonText": "取消"
                            }
                        }
                    ]
                }
            ]
        }
    }
}