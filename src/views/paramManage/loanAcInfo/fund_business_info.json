            "control": "text",
            "width": "130px",
            "label": "核算代码"
          },
          "cptl_bal_charic": {
            "control": "select",
            "width": "130px",
            "label": "核算性质",
            "dict": {
              "dictType": "AC_E_TEMP_RECPT_PYMT_BUSI_CATG",
              "dictKey": [
                "AC_E_TEMP_RECPT_PYMT_BUSI_CATG"
              ],
              "format": "label"
