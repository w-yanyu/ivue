    "form": {
      "controls": [
        {
          "acct_busi_catg": {
            "control": "select",
            "label": "账户业务种类",
            "value": "10",
            "width": "130px",
            "dict": {
              "dictType": "AC_E_ACCT_BUSI_CATG",
              "dictKey": [
                "AC_E_ACCT_BUSI_CATG"
              ],
              "format": "label"
            }
          },
          "busi_code": {
            "control": "text",
            "label": "业务编码",
            "width": "130px"
          }
        },
        {
          "busi_code_comt": {
            "label": "业务编码说明",
            "control": "textarea",
            "width": "130px",
            "span": 12,
            "autosize": {
              "minRows": 3,
              "maxRows": 5
            },
            "maxlength": 600,
            "showWordLimit": true
          },
          "acct_num_serl_num": {
            "control": "text",
            "width": "130px",
            "label": "账号序号"
          }
        },
        {
