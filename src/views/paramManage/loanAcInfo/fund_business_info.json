    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ta7505",
          "outlistid": "listnm"
        }
      },
      "thead": [
        {
          "label": "账户业务种类",
          "prop": "acct_busi_catg",
          "align": "center",
          "dict": {
            "dictType": "AC_E_ACCT_BUSI_CATG",
            "dictKey": [
              "AC_E_ACCT_BUSI_CATG"
            ],
            "format": "label"
          }
