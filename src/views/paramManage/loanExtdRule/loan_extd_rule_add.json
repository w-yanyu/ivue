{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/call/RPCCall",
            "method": "POST",
            "params": {
              "servicecode": "ap0000",
              "parm_tab_nm": "klnp_extd_rule",
              "dtbs_oprn_flg": "0",
              "data_pkg": "${{current}}"
            }
          }
        }
      ],
      "controls": [
        {
          "extd_rule_num": {
            "control": "text",
            "label": "展期规则编号",
            "rules": [
              {
                "max": 10,
                "message": "长度不能超过10个字符"
              },{
                "required": true,
                "message": "请输入展期规则编号"
              }
            ]
          },
          "extd_rule_dsc": {
            "control": "text",
            "label": "展期规则描述",
            "rules": [
              {
                "max": 80,
                "message": "长度不能超过80个字符"
              }
            ]
          }
        },
        {
          "bchmk_int_rate_val_rule": {
            "control": "select",
            "label": "基准利率取值规则",
            "dict":{
              "dictType": "LN_E_BCHMK_INT_RATE_VAL_RULE",
              "dictKey": [
                "LN_E_BCHMK_INT_RATE_VAL_RULE"
              ],
              "format": "label"
            }
          },
          "extd_int_rate_flt_ctrl": {
            "control": "select",
            "label": "展期利率浮动控制",
            "dict":{
              "dictType": "LN_E_EXTD_INT_RATE_FLT_CTRL",
              "dictKey": [
                "LN_E_EXTD_INT_RATE_FLT_CTRL"
              ],
              "format": "label"
            }
          }
        },
        {
          "extd_int_rate_flt_val_ctrl": {
            "control": "select",
            "label": "展期利率浮动值控制",
            "dict": {
              "dictType": "LN_E_EXTD_INT_RATE_FLT_VAL_CTRL",
              "dictKey": [
                "LN_E_EXTD_INT_RATE_FLT_VAL_CTRL"
              ],
              "format": "label"
            }
          },
          "serl_num_long": {
            "control": "inputNumber",
            "label": "序号",
            "controls": false,
            "min": 0
          }
        }
      ]
    }
  }
}
