{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "agnc_rgon_num": {
            "control": "text",
            "label": "代理区域编号",
            "rules": [
              {
                "max": 10,
                "message": "长度不能超过10个字符"
              }
            ]
          },
          "agnc_dsc": {
            "control": "text",
            "label": "代理描述",
            "rules": [
              {
                "max": 80,
                "message": "长度不能超过80个字符"
              }
            ]
          }
        },
        {
          "clnt_dept_acct_num": {
            "control": "text",
            "label": "委托人存款账号",
            "rules": [
              {
                "max": 35,
                "message": "长度不能超过35个字符"
              }
            ]
          },
          "clnt_dept_sub_acct_serl_num": {
            "control": "text",
            "label": "委托人存款子账户序号",
            "rules": [
              {
                "max": 80,
                "message": "长度不能超过80个字符"
              }
            ]
          }
        },
        {
          "entstd_dept_busi_code": {
            "control": "text",
            "label": "委托存款账户业务编码",
            "rules": [
              {
                "max": 60,
                "message": "长度不能超过60字符"
              }
            ]
          },
          "entstd_dept_acct_num": {
            "control": "text",
            "label": "委托存款账号",
            "rules": [
              {
                "max": 35,
                "message": "长度不能超过35个字符"
              }
            ]
          }
        },
        {
          "entstd_dept_sub_acct_serl_num": {
            "control": "text",
            "label": "委托存款子账户序号",
            "rules": [
              {
                "max": 8,
                "message": "长度不能超过8字符"
              }
            ]
          },
          "prcpl_ret_pstg_acct_num": {
            "control": "text",
            "label": "本金归还入账账号",
            "rules": [
              {
                "max": 35,
                "message": "长度不能超过35个字符"
              }
            ]
          }
        },
        {
          "prcpl_pstg_sub_acct_serl_num": {
            "control": "text",
            "label": "本金归还入账子账户序号",
            "rules": [
              {
                "max": 35,
                "message": "长度不能超过35字符"
              }
            ]
          },
          "int_ret_pstg_acct_num": {
            "control": "text",
            "label": "利息归还入账账号",
            "rules": [
              {
                "max": 35,
                "message": "长度不能超过8个字符"
              }
            ]
          }
        },
        {
          "int_ret_pstg_sub_acct_serl_num": {
            "control": "text",
            "label": "利息归还入账子账户序号",
            "rules": [
              {
                "max": 8,
                "message": "长度不能超过8个字符"
              }
            ]
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "target": "dialog",
          "label": "新增",
          "toRequest": {
            "method": "GET",
            "url": "/views/paramManage/loanAgncInfo/loan_agnc_info_add.json"
          },
          "props": {
            "title": "新增"
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ap0000",
          "parm_tab_nm": "klnp_agnc_info",
          "dtbs_oprn_flg": "3",
          "data_pkg": "${{current}}"
        }
      },
      "thead": [
        {
          "label": "代理区域编号",
          "prop": "agnc_rgon_num",
          "align": "center"
        },
        {
          "label": "代理序号",
          "prop": "agnc_serl_num",
          "align": "center"
        },
        {
          "label": "代理描述",
          "prop": "agnc_dsc",
          "align": "center"
        },
        {
          "label": "委托人存款账号",
          "prop": "clnt_dept_acct_num",
          "align": "center"
        },   {
          "label": "委托人存款子账户序号",
          "prop": "clnt_dept_sub_acct_serl_num",
          "align": "center"
        },   {
          "label": "委托存款账户业务编码",
          "prop": "entstd_dept_busi_code",
          "align": "center"
        },
        {
          "label": "委托存款账号",
          "prop": "entstd_dept_acct_num",
          "align": "center"
        },
        {
          "label": "委托存款子账户序号",
          "prop": "entstd_dept_sub_acct_serl_num",
          "align": "center"
        },
        {
          "label": "本金归还入账账号",
          "prop": "prcpl_ret_pstg_acct_num",
          "align": "center"
        },
        {
          "label": "本金归还入账子账户序号",
          "prop": "prcpl_pstg_sub_acct_serl_num",
          "align": "center"
        },
        {
          "label": "利息归还入账账号",
          "prop": "int_ret_pstg_acct_num",
          "align": "center"
        },
        {
          "label": "利息归还入账子账户序号",
          "prop": "int_ret_pstg_sub_acct_serl_num",
          "align": "center"
        },
        {
          "label": "序号",
          "prop": "serl_num_long",
          "align": "center"
        },
        {
          "label": "操作",
          "fixed": "right",
          "width": 300,
          "align": "center",
          "btns": [
            {
              "type": "look",
              "label": "详情",
              "toRequest": {
                "url": "/views/paramManage/loanAgncInfo/loan_agnc_info_info.json",
                "method": "POST"
              },
              "props": {
                "width": "50%",
                "title": "信息详情"
              }
            },
            {
              "type": "edit",
              "label": "维护",
              "toRequest": {
                "url": "views/paramManage/loanAgncInfo/loan_agnc_info_update.json",
                "method": "GET"
              },
              "props": {
                "width": "50%",
                "title": "信息维护"
              }
            },
            {
              "type": "custom",
              "label": "删除",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "ap0000",
                  "parm_tab_nm": "klnp_agnc_info",
                  "dtbs_oprn_flg": "2",
                  "data_pkg": {
                    "agnc_rgon_num":"${{current.agnc_rgon_num}}",
                    "agnc_serl_num": "${{current.agnc_serl_num}}"
                  }
                }
              },
              "confirm": {
                "title": "确定删除",
                "confirmButtonText": "确定",
                "cancelButtonText": "取消"
              }
            }
          ]
        }
      ]
    }
  }
}
