{
  "module": "v-realonlyForm-btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "controls": [
        {
          "parm_nm": {
            "control": "string",
            "label": "参数名称",
            "width": "100px",
            "rules": [
              {
                "required": true,
                "message": "请输入模块代码"
              },
              {
                "max": 100,
                "message": "长度不能超过100个字符"
              }
            ]
          },
          "parm_val": {
            "control": "string",
            "label": "参数值",
            "width": "100px"
          }
        },{
          "parm_data": {
            "control": "string",
            "label": "参数数据",
            "width": "100px"
          },
          "spare_data_80": {
            "control": "string",
            "label": "备用数据",
            "width": "100px"
          }
        },
        {
          "data_comt": {
            "control": "string",
            "label": "参数说明",
            "width": "100px"
          },
          "serl_num_long": {
            "control": "string",
            "label": "序号",
            "width": "100px"
          }
        }
      ]
    }
  }
}
