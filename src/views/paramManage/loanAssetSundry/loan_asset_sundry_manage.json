{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "parm_nm": {
            "control": "text",
            "label": "参数名称",
            "rules": [
              {
                "max": 100,
                "message": "长度不能超过100个字符"
              }
            ]
          },
          "parm_val": {
            "control": "text",
            "label": "参数值",
            "rules": [
              {
                "max": 100,
                "message": "长度不能超过100个字符"
              }
            ]
          }
        },
        {
          "parm_data": {
            "control": "text",
            "label": "参数数据",
            "rules": [
              {
                "max": 100,
                "message": "长度不能超过100个字符"
              }
            ]
          },
          "spare_data_80": {
            "control": "text",
            "label": "备用数据",
            "rules": [
              {
                "max": 80,
                "message": "长度不能超过80个字符"
              }
            ]
          }
        },
        {
          "parm_comt": {
            "control": "text",
            "label": "参数说明",
            "rules": [
              {
                "max": 600,
                "message": "长度不能超过600个字符"
              }
            ]
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "target": "dialog",
          "label": "新增",
          "toRequest": {
            "method": "GET",
            "url": "/views/paramManage/loanAssetSundry/loan_asset_sundry_add.json"
          },
          "props": {
            "title": "新增"
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ap0000",
          "parm_tab_nm": "klnp_asset_sundry",
          "dtbs_oprn_flg": "3",
          "data_pkg": "${{current}}"
        }
      },
      "thead": [
        {
          "label": "参数名称",
          "prop": "parm_nm",
          "align": "center"
        },
        {
          "label": "参数值",
          "prop": "parm_val",
          "align": "center"
        },
        {
          "label": "参数数据",
          "prop": "parm_data",
          "align": "center"
        },
        {
          "label": "备用数据",
          "prop": "spare_data_80",
          "align": "center"
        },
        {
          "label": "参数说明",
          "prop": "parm_comt",
          "align": "center"
        },
        {
          "label": "序号",
          "prop": "serl_num_long",
          "align": "center"
        },
        {
          "label": "操作",
          "fixed": "right",
          "width": 300,
          "align": "center",
          "btns": [
            {
              "type": "look",
              "label": "详情",
              "toRequest": {
                "url": "/views/paramManage/loanAssetSundry/loan_asset_sundry_info.json",
                "method": "POST"
              },
              "props": {
                "width": "50%",
                "title": "基础产品信息详情"
              }
            },
            {
              "type": "edit",
              "label": "维护",
              "toRequest": {
                "url": "/views/paramManage/loanAssetSundry/loan_asset_sundry_update.json",
                "method": "GET"
              },
              "props": {
                "width": "50%",
                "title": "基础产品信息维护"
              }
            },
            {
              "type": "custom",
              "label": "删除",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "ap0000",
                  "parm_tab_nm": "klnp_asset_sundry",
                  "dtbs_oprn_flg": "2",
                  "data_pkg": {
                    "parm_nm":"${{current.parm_nm}}",
                    "parm_val": "${{current.parm_val}}"
                  }
                }
              },
              "confirm": {
                "title": "确定删除",
                "confirmButtonText": "确定",
                "cancelButtonText": "取消"
              }
            }
          ]
        }
      ]
    }
  }
}
