{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/call/RPCCall",
            "method": "POST",
            "params": {
              "servicecode": "ap0000",
              "dtbs_oprn_flg": "0",
              "parm_tab_nm": "klnp_asset_sundry",
              "data_pkg": "${{current}}"
            }
          }
        }
      ],
      "controls": [
        {
          "parm_nm": {
            "control": "text",
            "label": "参数名称",
            "width": "100px",
            "rules": [
              {
                "required": true,
                "message": "请输入模块代码"
              },
              {
                "max": 100,
                "message": "长度不能超过100个字符"
              }
            ]
          },
          "parm_val": {
            "control": "text",
            "label": "参数值",
            "width": "100px",
            "rules": [
              {
                "required": true,
                "message": "请输入数据类代码"
              },
              {
                "max": 100,
                "message": "长度不能超过100个字符"
              }
            ]
          }
        },{
          "parm_data": {
            "control": "text",
            "label": "参数数据",
            "width": "100px",
            "rules": [
              {
                "max": 100,
                "message": "长度不能超过100个字符"
              }
            ]
          },
          "spare_data_80": {
            "control": "text",
            "label": "备用数据",
            "width": "100px",
            "rules": [
              {
                "max": 100,
                "message": "长度不能超过100个字符"
              }
            ]
          }
        },
        {
          "data_comt": {
            "control": "text",
            "label": "参数说明",
            "width": "100px",
            "rules": [
              {
                "max": 600,
                "message": "长度不能超过600个字符"
              }
            ]
          },
          "serl_num_long": {
            "control": "text",
            "label": "序号",
            "width": "100px"
          }
        }
      ]
    }
  }
}
