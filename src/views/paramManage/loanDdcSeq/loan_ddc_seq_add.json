{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/call/RPCCall",
            "method": "POST",
            "params": {
              "servicecode": "ap0000",
              "dtbs_oprn_flg": "0",
              "parm_tab_nm": "klnp_ddc_seq",
              "data_pkg": "${{current}}"
            }
          }
        }
      ],
      "controls": [
        {
          "ddc_mny_parm_num": {
            "control": "text",
            "label": "扣款参数编号",
            "rules": [
              {
                "required": true,
                "message": "请输入扣款参数编号"
              },
              {
                "max": 10,
                "message": "长度不能超过10个字符"
              }
            ]
          },
          "ddc_mny_parm_dsc": {
            "control": "text",
            "label": "扣款参数描述",
            "rules": [
              {
                "max": 300,
                "message": "长度不能超过300个字符"
              }
            ]
          }
        },
        {
          "loan_form": {
            "control": "select",
            "label": "贷款形态",
            "dict": {
              "dictType": "LN_E_LOAN_FORM",
              "dictKey": [
                "LN_E_LOAN_FORM"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择贷款形态"
              }
            ]
          },
          "imprmt_loan_flg": {
            "control": "select",
            "label": "减值贷款标志",
            "dict": {
              "dictType": "LN_E_IMPRMT_LOAN_FLG",
              "dictKey": [
                "LN_E_LOAN_FORM"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择减值贷款标志"
              }
            ]
          }
        },
        {
          "acd_non_acd_ste": {
            "control": "select",
            "label": "应计非应计状态",
            "dict": {
              "dictType": "LN_E_ACD_NON_ACD_STE",
              "dictKey": [
                "LN_E_ACD_NON_ACD_STE"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择应计非应计状态"
              }
            ]
          },
          "rpymt_attr": {
            "control": "select",
            "label": "还款属性",
            "dict": {
              "dictType": "LN_E_RPYMT_ATTR",
              "dictKey": [
                "LN_E_RPYMT_ATTR"
              ],
              "format": "label"
            }
          }
        },
        {
          "rpymt_ste_rule": {
            "control": "select",
            "label": "还款状态规则",
            "dict": {
              "dictType": "LN_E_RPYMT_STE_RULE",
              "dictKey": [
                "LN_E_RPYMT_STE_RULE"
              ],
              "format": "label"
            }
          },
          "rpymt_seq": {
            "control": "text",
            "label": "还款顺序",
            "rules": [
              {
                "max": 20,
                "message": "长度不能超过20个字符"
              }
            ]
          }
        },
        {
          "data_comt": {
            "control": "text",
            "label": "数据说明",
            "rules": [
              {
                "max": 80,
                "message": "长度不能超过80个字符"
              }
            ]
          },
          "serl_num_long": {
            "control": "text",
            "label": "序号"
          }
        }
      ]
    }
  }
}
