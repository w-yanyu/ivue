{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/call/RPCCall",
            "method": "POST",
            "params": {
              "servicecode": "ap0000",
              "parm_tab_nm": "klnp_extd_rule_layer",
              "dtbs_oprn_flg": "0",
              "data_pkg": "${{current}}"
            }
          }
        }
      ],
      "controls": [
        {
          "extd_rule_num": {
            "control": "text",
            "label": "展期规则编号",
            "rules": [
              {
                "max": 10,
                "message": "长度不能超过10个字符"
              },{
                "required": true,
                "message": "请输入展期规则编号"
              }
            ]
          },
          "numrcl_val_serl_num": {
            "control": "inputNumber",
            "label": "数值序号",
            "controls": false,
            "min": 0,
            "rules": [
            {
                "required": true,
                "message": "请输入数值序号"
              }
            ]
          }
        },
        {
          "extd_rule_matu": {
            "control": "text",
            "label": "展期规则期限(月)",
            "controls": false,
            "min": 0
          },
          "extd_ctrl_way": {
            "control": "select",
            "label": "展期控制方式",
            "dict":{
              "dictType": "LN_E_EXTD_CTRL_WAY",
              "dictKey": [
                "LN_E_EXTD_CTRL_WAY"
              ],
              "format": "label"
            }
          }
        },
        {
          "matu_ctrl_catg": {
            "control": "select",
            "label": "期限控制种类",
            "dict": {
              "dictType": "LN_E_MATU_CTRL_CATG",
              "dictKey": [
                "LN_E_MATU_CTRL_CATG"
              ],
              "format": "label"
            }
          },
          "extd_ctrl_matu": {
            "control": "text",
            "label": "展期控制期限(月)",
            "controls": false,
            "min": 0
          }
        },
        {
          "extd_max_matu": {
            "control": "text",
            "label": "展期最大期限(月)",
            "controls": false,
            "min": 0
          },
          "serl_num_long": {
            "control": "inputNumber",
            "label": "序号",
            "controls": false,
            "min": 0
          }
        }
      ]
    }
  }
}
