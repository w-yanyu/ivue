{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "extd_rule_num": {
            "control": "text",
            "label": "展期规则编号",
            "rules": [
              {
                "max": 10,
                "message": "长度不能超过10个字符"
              }
            ]
          },
          "extd_ctrl_way": {
            "control": "select",
            "label": "展期控制方式",
            "dict":{
              "dictType": "LN_E_EXTD_CTRL_WAY",
              "dictKey": [
                "LN_E_EXTD_CTRL_WAY"
              ],
              "format": "label"
            }
          }
        },
        {
          "matu_ctrl_catg": {
            "control": "select",
            "label": "期限控制种类",
            "dict":{
              "dictType": "LN_E_MATU_CTRL_CATG",
              "dictKey": [
                "LN_E_MATU_CTRL_CATG"
              ],
              "format": "label"
            }
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "target": "dialog",
          "label": "新增",
          "toRequest": {
            "method": "GET",
            "url": "/views/paramManage/loanExtdRuleLayer/loan_extd_rule_layer_add.json"
          },
          "props": {
            "title": "新增"
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ap0000",
          "parm_tab_nm": "klnp_extd_rule_layer",
          "dtbs_oprn_flg": "3",
          "data_pkg": "${{current}}"
        }
      },
      "thead": [
        {
          "label": "展期规则编号",
          "prop": "extd_rule_num",
          "align": "center"
        },
        {
          "label": "数值序号",
          "prop": "numrcl_val_serl_num",
          "align": "center"
        },
        {
          "label": "展期规则期限(月)",
          "prop": "extd_rule_matu",
          "align": "center"
        },
        {
          "label": "展期控制方式",
          "prop": "extd_ctrl_way",
          "align": "center",
          "dict": {
            "dictType": "LN_E_EXTD_CTRL_WAY",
            "dictKey": [
              "LN_E_EXTD_CTRL_WAY"
            ],
            "format": "label"
          }
        },
        {
          "label": "期限控制种类",
          "prop": "matu_ctrl_catg",
          "align": "center",
          "dict": {
            "dictType": "LN_E_MATU_CTRL_CATG",
            "dictKey": [
              "LN_E_MATU_CTRL_CATG"
            ],
            "format": "label"
          }
        },
        {
          "label": "展期控制期限(月)",
          "prop": "extd_rule_matu",
          "align": "center"
        },
        {
          "label": "展期最大期限(月)",
          "prop": "extd_max_matu",
          "align": "center"
        },
        {
          "label": "序号",
          "prop": "serl_num_long",
          "align": "center"
        },
        {
          "label": "操作",
          "fixed": "right",
          "width": 300,
          "align": "center",
          "btns": [
            {
              "type": "look",
              "label": "详情",
              "toRequest": {
                "url": "/views/paramManage/loanExtdRuleLayer/loan_extd_rule_layer_info.json",
                "method": "POST"
              },
              "props": {
                "width": "50%",
                "title": "信息详情"
              }
            },
            {
              "type": "edit",
              "label": "维护",
              "toRequest": {
                "url": "/views/paramManage/loanExtdRuleLayer/loan_extd_rule_layer_update.json",
                "method": "GET"
              },
              "props": {
                "width": "50%",
                "title": "信息维护"
              }
            },
            {
              "type": "custom",
              "label": "删除",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "ap0000",
                  "parm_tab_nm": "klnp_extd_rule_layer",
                  "dtbs_oprn_flg": "2",
                  "data_pkg": {
                    "extd_rule_num":"${{current.extd_rule_num}}",
                    "numrcl_val_serl_num":"${{current.numrcl_val_serl_num}}"
                  }
                }
              },
              "confirm": {
                "title": "确定删除",
                "confirmButtonText": "确定",
                "cancelButtonText": "取消"
              }
            }
          ]
        }
      ]
    }
  }
}
