{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "scope": "formData",
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": "",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "materiaicf",
                            "typeofdate": "FJ"
                        }
                    }
                }
            ],
            "controls": [
                {
                  "parentpostidid": {
                    "label": "岗位",
                    "control": "remoteSelect",
                    "valueField": "postid",
                    "labelField": "postspecies",
                    "doRequest": {
                      "url": "/SUMP/icmcall/icmRPCCall",
                      "method": "post",
                      "params": {
                        "servicecode": "selmateriaicf",
                        "typeofdate": "GW",
                        "isparentnode": "Y",
                        "length": 100
                      }
                    },
                    "rules": [
                      {
                        "required": true,
                        "message": "请选择岗位"
                      }
                    ]
                  },

                  "postspecies": {
                    "control": "lookup",
                    "label": "附件名称",
                    "multiple": true,
                    "#valueDataType": "string",
                    "toRequest": {
                      "url": "/views/mc/materialcf/materialcf_annex.json",
                      "method": "get",
                      "params": {},
                      "props": {
                        "width": "85%",
                        "title": "多选",
                        "height": "300px"
                      }
                    },
                    "events":{
                      "set":{
                        "postid":"postid",
                        "postspecies":"postspecies"
                      }
                    },
                    "#keyField":  {
                      "postid":"postid",
                      "postspecies":"postspecies"
                    },
                    "rules": [
                      {
                        "required": true,
                        "message": "请选择附件"
                      }
                    ]
                  }

                },
              {
                "postid": {
                  "control": "select",
                  "label": "附件id",
                  "multiple": true,
                  "width": 100,
                  "disabled": true
                }
              }

            ]
        }
    }
}
