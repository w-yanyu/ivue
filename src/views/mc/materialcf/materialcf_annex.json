{
  "module": "v-search-btn-datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "textR": {
            "control": "text",
            "label": "附件"
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "bringback",
          "label": "选择"
        }
      ]
    },
    "datagrid": {
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "selmateriaicf",
          "typeofdate": "FJ",
          "isparentnode": "Y",
          "length": 1000
        }
      },
      "thead": [
        {
          "type": "selection",
          "width": "55"
        },
        {
          "type": "index",
          "label": "序号"
        },
        {
          "label": "附件id",
          "prop": "postid"
        },
        {
          "label": "附件名称",
          "prop": "postspecies"
        },
        {
          "label": "附件类型",
          "prop": "issupplement",
          "dict": {
            "dictKey": [
              "E_DATEFILEOFTYPE"
            ],
            "dictType": "E_DATEFILEOFTYPE",
            "format": "label"
          }
        }
      ],
      "pagination": {
        "show": false
      }
    }
  }
}
