{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "typeofdate": {
            "control": "select",
            "label": "基础数据类型",
            "dict": {
              "dictKey": [
                "E_DATEOFTYPE"
              ],
              "dictType": "E_DATEOFTYPE",
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择数据类型"
              }
            ]
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "target": "dialog",
          "label": "新增基础数据配置",
          "toRequest": {
            "method": "GET",
            "url": "/views/mc/materialcfbase/materialcf_addbasedate.json"
          },
          "props": {
            "title": "新增基础数据配置"
          }
        }

      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "selmateriaicf",
          "isparentnode": "Y"
        }
      },
      "thead": [
        {
          "label": "序号",
          "type": "index",
          "align": "center"
        },
        {
          "label": "基础数据id",
          "prop": "postid",
          "align": "center"
        },
        {
          "label": "基础数据名称",
          "prop": "postspecies",
          "align": "center"
        },
        {
          "label": "基础数据类型",
          "prop": "typeofdate",
          "align": "center",
          "dict": {
            "dictKey": [
              "E_DATEOFTYPE"
            ],
            "dictType": "E_DATEOFTYPE",
            "format": "label"
          }
        },
        {
          "label": "附件类型",
          "prop": "issupplement",
          "align": "center",
          "dict": {
            "dictKey": [
              "E_DATEFILEOFTYPE"
            ],
            "dictType": "E_DATEFILEOFTYPE",
            "format": "label"
          }
        },
        {
          "label": "操作",
          "fixed": "right",
          "align": "center",
          "btns": [
            {
              "type": "delete",
              "label": "删除",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "delmateriaicf"
                }
              }
            },
            {
              "type": "edit",
              "label": "修改",
              "toRequest": {
                "url": "/views/mc/materialcfbase/materialcf_upbasedate.json",
                "method": "post"
              },
              "props": {
                "width": "65%",
                "title": "修改"
              }
            }
          ]
        }
      ]
    }
  }
}
