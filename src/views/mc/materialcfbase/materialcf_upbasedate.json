{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
          "initRequest": "${{params}}",
            "scope": "formData",
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": "",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "upmateriaicf"

                        }
                    }
                }
            ],
            "controls": [
                {

                  "postid": {
                    "label": "数据值",
                    "control": "text",
                    "disabled": true
                  },
                  "postspecies": {
                    "label": "数据名称",
                    "control": "text"
                  },
                  "isparentnode": {
                    "label": "是否为基础数据",
                    "control": "hidden"
                  },
                  "issupplement": {
                    "label": "文件附件类型",
                    "control": "select",
                    "dict": {
                      "dictKey": [
                        "E_DATEFILEOFTYPE"
                      ],
                      "dictType": "E_DATEFILEOFTYPE",
                      "format": "label"
                    }
                  }
                }

            ]
        }
    }
}
