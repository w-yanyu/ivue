{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "applno": {
            "control": "text",
            "label": "申请编号"
          },
          "applid": {
            "control": "text",
            "label": "进件编号"
          }
        },
        {
          "custNa": {
            "control": "text",
            "label": "客户姓名"
          },
          "custNo": {
            "control": "text",
            "label": "身份证号"
          }
        },
        {

          "taskid": {
            "control": "text",
            "label": "当前流程"
          },
          "curWfrlId": {
            "control": "text",
            "label": "客户经理"
          }
        },
        {
          "stappdt": {
            "control": "dateTimePicker",
            "label": "进件开始时间"
          },
          "endappdt": {
            "control": "dateTimePicker",
            "label": "进件结束时间"
          }
        },

        {
          "usercd": {
            "control": "text",
            "label": "当前执行人"
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "custom",
          "target": "dialog",

          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "delDateVerification",
              "selectName": "dataVerification"
            }
          },

          "label": "取消案件认领",
          "confirm":{
            "title":"提示信息",
            "msg":"确认取消案件认领吗？",
            "confirmButtonText":"确定",
            "cancelButtonText":"取消"
          }

        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "selDataVerification"
        }
      },
      "thead": [
        {
          "label": "序号",
          "type": "selection",
          "align": "center"
        },
        {
          "label": "进件编号",
          "prop": "applid",
          "align": "center"
        },
        {
          "label": "客户姓名",
          "prop": "custna",
          "align": "center"
        },
        {
          "label": "产品名称",
          "prop": "prodna",
          "align": "center"
        },
        {
          "label": "授信申请额度",
          "prop": "postid",
          "align": "center"
        },
        {
          "label": "授信申请额度",
          "prop": "applam",
          "align": "center"
        },
        {
          "label": "贷款期数",
          "prop": "ppvtnr",
          "align": "center"
        },
        {
          "label": "客户经理",
          "prop": "ppvtnr",
          "align": "center"
        },
        {
          "label": "当前执行人",
          "prop": "usercd",
          "align": "center"
        },
        {
          "label": "当前流程",
          "prop": "taskna",
          "align": "center"
        },
        {
          "label": "状态",
          "prop": "applst",
          "align": "center"
        },
        {
          "label": "进件时间",
          "prop": "appldt",
          "align": "center"
        }
      ]
    }
  }
}
