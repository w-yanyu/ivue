{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "taskid": {
            "control": "text",
            "label": "当前流程"
          },
          "applid": {
            "control": "text",
            "label": "申请编号"
          },
          "usercd": {
            "control": "text",
            "label": "当前执行人"
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "custom",
          "target": "dialog",
          "label": "案件分派",
          "toRequest": {
            "url": "/views/mc/caseassignment/caseassignment_distribution.json",
            "method": "POST"
          }

        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "selCaseAssignment",
          "caseType": "N"
        }
      },
      "thead": [
        {
          "label": "序号",
          "type": "selection",
          "align": "center"
        },
        {
          "label": "进件编号",
          "prop": "applid",
          "align": "center"
        },
        {
          "label": "客户姓名",
          "prop": "custna",
          "align": "center"
        },
        {
          "label": "产品名称",
          "prop": "prodna",
          "align": "center"
        },
        {
          "label": "产品编号",
          "prop": "prodcd",
          "align": "center"
        },
        {
          "label": "授信申请额度",
          "prop": "applam",
          "align": "center"
        },
        {
          "label": "贷款期数",
          "prop": "ppvtnr",
          "align": "center"
        },
        {
          "label": "客户经理",
          "prop": "ppvtnr",
          "align": "center"
        },
        {
          "label": "当前执行人",
          "prop": "usercd",
          "align": "center"
        },
        {
          "label": "当前流程",
          "prop": "taskna",
          "align": "center"
        },
        {
          "label": "状态",
          "prop": "applst",
          "align": "center",
          "dict": {
            "dictType": "E_SXAPPLST",
            "dictKey": [
              "E_SXAPPLST"
            ],
            "format": "label"
          }
        },
        {
          "label": "进件时间",
          "prop": "appldt",
          "align": "center"
        }
      ]
    }
  }
}
