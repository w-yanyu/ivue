{
  "module": "v_search_btn_datagrid",
  "layout": {

    "toolbar": {
      "btns": [
        {
          "type": "custom",
          "target": "dialog",
          "label": "提交",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "delCaseAssignment",
              "taskIdList": "${{params.selectedData}}"
            }
          }

        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "selAllUser",
          "prodcd": "${{params.selectedData}}"
        }
      },
      "thead": [
        {
          "label": "序号",
          "type": "radio",
          "align": "center"
        },
        {
          "label": "操作人员编号",
          "prop": "opridd",
          "align": "center"
        },
        {
          "label": "操作人员姓名",
          "prop": "oprnam",
          "align": "center"
        }

      ]
    }
  }
}
