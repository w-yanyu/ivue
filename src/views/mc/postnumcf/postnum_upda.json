{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "scope": "formData",
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "postNumcfg",
              "delDateType": "M"
            }
          }
        }
      ],
      "controls": [
        {

          "postna": {
            "label": "岗位名称",
            "control": "text",
            "disabled": true
          },
          "postid": {
            "label": "岗位id",
            "control": "hidden"
          },
          "postnum": {
            "label": "岗位认领笔数",
            "control": "inputNumber",
            "stepStrictly": 1
          }
        }

      ]
    }
  }
}
