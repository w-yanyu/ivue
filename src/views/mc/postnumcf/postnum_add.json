{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "scope": "formData",
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "postNumcfg",
              "delDateType": "A"
            }
          }
        }
      ],
      "controls": [
        {

          "postid": {
            "label": "岗位",
            "control": "remoteSelect",
            "valueField": "postid",
            "labelField": "postspecies",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "post",
              "params": {
                "servicecode": "selmateriaicf",
                "typeofdate": "GW",
                "isparentnode": "Y",
                "length": 100
              }
            }
          },
          "postnum": {
            "label": "岗位认领笔数",
            "control": "inputNumber",
            "stepStrictly": 1,
            "rules": [
              {
                "required": true,
                "message": "请输入笔数"
              }
            ]
          }
        }

      ]
    }
  }
}
