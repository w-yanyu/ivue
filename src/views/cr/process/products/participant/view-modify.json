{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "CRMSBPMTS0034",
                    "wfpart_cd": "${{params.wfpart_cd}}",
                    "trans_type": "SELECT"
                }
            },
            "btns": [{
                "type": "submit",
                "label": "提交",
                "icon": "",
                "doRequest": {
                    "url": "/SUMP/icmcall/icmRPCCall",
                    "method": "POST",
                    "params": {
                        "servicecode": "CRMSBPMTS0034",
                        "wfpart_cd": "${{params.wfpart_cd}}",
                        "trans_type": "UPDATE"
                    }
                }
            }],
            "controls": [{
                "wfpart_name": {
                    "control": "string",
                    "label": "配置名称",
                    "width": "130px",
                    "rules": [{
                        "required": true,
                        "message": "请输入配置名称"
                    }]
                },
                "wfprod_cd": {
                    "control": "string",
                    "label": "产品编号",
                    "width": "130px",
                    "rules": [{
                        "required": true,
                        "message": "请输入产品编号"
                    }]
                },
                "wfprod_name": {
                    "control": "string",
                    "label": "产品名称",
                    "width": "130px",
                    "rules": [{
                        "required": true,
                        "message": "请输入产品名称"
                    }]
                },
                "task_def_id": {
                    "control": "string",
                    "label": "活动定义id",
                    "width": "130px",
                    "rules": [{
                        "required": true,
                        "message": "请输入活动定义id"
                    }]
                },
                "task_def_name": {
                    "control": "string",
                    "label": "活动定义名称",
                    "width": "130px",
                    "rules": [{
                        "required": true,
                        "message": "请输入活动定义名称"
                    }]
                },
                "part_type": {
                    "control": "select",
                    "label": "参与人类型",
                    "width": "130px",
                    "dict": {
                        "dictType": "BPM_E_PART_TYPE",
                        "dictKey": [
                            "BPM_E_PART_TYPE"
                        ],
                        "format": "label"
                    },
                    "rules": [{
                        "required": true,
                        "message": "请选择参与人类型"
                    }]
                },
                "bankorg_id": {
                    "control": "text",
                    "label": "法人代码",
                    "width": "130px",
                    "rules": [{
                        "required": true,
                        "message": "请输入法人代码"
                    }]
                },
                "select_type": {
                    "control": "select",
                    "label": "处理方式",
                    "width": "130px",
                    "dict": {
                        "dictType": "BPM_E_SELECT_TYPE",
                        "dictKey": [
                            "BPM_E_SELECT_TYPE"
                        ],
                        "format": "label"
                    },
                    "rules": [{
                        "required": true,
                        "message": "请选择处理方式"
                    }]
                },
                "brch_type": {
                    "control": "select",
                    "label": "机构类型",
                    "width": "130px",
                    "dict": {
                        "dictType": "BPM_E_BRCH_TYPE",
                        "dictKey": [
                            "BPM_E_BRCH_TYPE"
                        ],
                        "format": "label"
                    },
                    "rules": [{
                        "required": true,
                        "message": "请选择机构类型"
                    }]
                },
                "filter_brch_ind": {
                    "control": "lookup",
                    "label": "处理机构",
                    "width": "130px",
                    "edit": true,
                    "keyField": {
                        "filter_brch_ind": "branch_id" },
                    "toRequest": {
                        "url": "/views/product/availSellProductMange/add/defaultorginazation.json",
                        "method": "post"
                    },
                    "multiple": false,
                    "props": {
                        "title": "机构选择"
                    },
                    "rules": [
                        {
                            "required": true,
                            "message": "默认机构不得为空"
                        },
                        {
                            "max": 100,
                            "message": "长度不能超过100个字符"
                        }
                    ] 
                },
                "wfrole_cd": {
                    "control": "text",
                    "label": "处理角色",
                    "width": "130px",
                    "rules": [{
                            "required": true,
                            "message": "请输入处理角色"
                        },
                        {
                            "max": 50,
                            "message": "长度不能超过50个字符"
                        }
                    ]
                },
                "user_cd": {
                    "control": "lookup",
                    "label": "处理用户",
                    "width": "130px",
                    "edit": true,
                    "keyField": {
                        "user_cd": "user_id" },
                    "toRequest": {
                        "url": "/views/product/availSellProductMange/add/customermanager.json",
                        "method": "post"
                    },
                    "multiple": false,
                    "props": {
                        "title": "用户选择"
                    },
                    "rules": [
                        {
                            "required": true,
                            "message": "客户经理不得为空"
                        },
                        {
                            "max": 100,
                            "message": "长度不能超过100个字符"
                        }
                    ] 
                },
                "ver": {
                    "control": "string",
                    "label": "版本号",
                    "width": "130px",
                    "rules": [{
                            "required": true,
                            "message": "请输入版本号"
                        },
                        {
                            "max": 50,
                            "message": "长度不能超过50个字符"
                        }
                    ]
                },
                "remark": {
                    "control": "textarea",
                    "label": "备注",
                    "width": "130px",
                    "maxlength": 500,
                    "showWordLimit": true
                }
            }]
        }
    }
}