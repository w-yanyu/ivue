{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "scope": "templateScope",
      "initRequest": "${{params}}",
      "initDataOnce": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "bpm0042",
          "appId":"${{params.appId}}"
        }
      },
      "btns": [
        {
          "type": "submit",
          "label": "提交"
        }
      ],
      "controls": [
        {
          "frame_cd": {
            "label": "框架编号",
            "control": "text",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入框架编号"
              }
            ]
          },
          "frame_desc": {
            "label": "框架名称",
            "control": "text",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入框架描述"
              }
            ]
          },
          "main_url": {
            "label": "页面链接地址",
            "control": "text",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入页面链接地址"
              }
            ]
          },
          "hdr_url": {
            "label": "表头链接地址",
            "control": "text",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入表头链接地址"
              }
            ]
          },
          "foot_url": {
            "label": "表尾链接地址",
            "control": "text",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入表尾链接地址"
              }
            ]
          },
          "is_enabled": {
            "control": "select",
            "label": "是否生效",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            },
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请选择是否生效"
              }
            ]
          }
        }
      ]
    }
  }
}
