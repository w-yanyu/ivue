{
  "module": "v_search_btn_datagrid",
  "layout": {

    "toolbar": {
      "btns": [
        {
          "type": "add",
          "target": "dialog",
          "label": "新增",
          "toRequest": {
            "method": "POST",
            "url": "/views/msap/bpm/frameManagement/node_config_add.json",
            "params": {
              "frame_cd": "${{params.frame_cd}}",
              "appId":"${{params.appId}}"
            }
          },
          "props": {
            "width": "65%",
            "title": "框架节点基本信息录入"
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": true,
      "initDataOnce": true,
      "scope": "searchTable",
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "bpm0048",
          "frame_cd": "${{params.frame_cd}}",
          "appId":"${{params.appId}}"
        }
      },
      "thead": [
        {
          "label": "序号",
          "type": "index"
        },
        {
          "label": "框架编号",
          "prop": "frame_cd",
          "align": "center"
        },
        {
          "label": "功能编号",
          "prop": "func_cd",
          "align": "center"
        },
        {
          "label": "菜单名称",
          "prop": "tree_menu_name",
          "align": "center"
        },
        {
          "label": "页面路径",
          "prop": "vue_path",
          "align": "center"
        },
        {
          "label": "菜单序号",
          "prop": "tree_seq",
          "align": "center"
        },
        {
          "prop": "is_enabled",
          "control": "select",
          "label": "是否有效",
          "align": "center",
          "dict": {
            "dictType": "MS_E_YESORNO",
            "dictKey": [
              "MS_E_YESORNO"
            ],
            "format": "label"
          }
        },
        {
          "label": "是否子框架",
          "prop": "child_frame_flag",
          "type": "select",
          "align": "center",
          "dict": {
            "dictType": "MS_E_YESORNO",
            "dictKey": [
              "MS_E_YESORNO"
            ],
            "format": "label"
          }
        },
        {
          "label": "操作",
          "fixed": "right",
          "width": 200,
          "align": "center",
          "btns": [
            {
              "type": "edit",
              "label": "维护",
              "toRequest": {
                "url": "/views/msap/bpm/frameManagement/node_config_edit.json",
                "method": "post",
                "params": {
                  "appId":"${{params.appId}}"
                }
              },
              "props": {
                "width": "65%",
                "title": "维护"
              }
            },
            {
              "type": "delete",
              "label": "删除",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "post",
                "params": {
                  "servicecode": "bpm0047",
                  "appId":"${{params.appId}}"
                }
              }
            }
          ]
        }
      ],
      "pagination": {
        "show": true
      }
    }
  }
}
