{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "appId": {
              "control": "select",
              "label": "业务模块",
              "selectFirstValue":true,
              "dict": {
                  "dictType": "MS_APPID",
                  "dictKey": [
                      "MS_APPID"
                  ],
                  "format": "value-label"
              },
              "rules": [
                  {
                      "required": true,
                      "message": "请选择业务模块"
                  }]
          }
        },{
          "frame_cd": {
            "label": "框架编号",
            "control": "text",
            "rules": [
              {
                "max": 30,
                "message": "长度不能超过30个字符"
              }
            ]
          },
          "frame_desc": {
            "label": "框架名称",
            "control": "text",
            "rules": [
              {
                "max": 50,
                "message": "长度不能超过50个字符"
              }
            ]
          }
        }
      ],
      "foldLineNumber": 3
    },
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "target": "dialog",
          "label": "新增",
          "toRequest": {
            "method": "POST",
            "url": "/views/msap/bpm/frameManagement/page_frame_config_add.json",
            "params": {
              "appId":"${{form.appId}}"
            }
          },
          "props": {
            "width": "65%",
            "title": "页面框架基本信息录入"
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": false,
      "scope": "searchTable",
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "bpm0044",
          "appId":"${{form.appId}}"
        }
      },
      "thead": [
        {
          "label": "序号",
          "type": "index",
          "align": "center"
        },
        {
          "label": "框架编号",
          "prop": "frame_cd",
          "align": "center"
        },
        {
          "label": "框架描述",
          "prop": "frame_desc",
          "align": "center"
        },
        {
          "label": "页面链接地址",
          "prop": "main_url",
          "align": "center"
        },
        {
          "label": "表头链接地址",
          "prop": "hdr_url",
          "align": "center"
        },
        {
          "label": "表尾链接地址",
          "prop": "foot_url",
          "align": "center"
        },
        {
          "label": "是否生效",
          "prop": "is_enabled",
          "type": "select",
          "align": "center",
          "dict": {
            "dictType": "MS_E_YESORNO",
            "dictKey": [
              "MS_E_YESORNO"
            ],
            "format": "label"
          }
        },
        {
          "label": "操作",
          "fixed": "right",
          "width": 200,
          "align": "center",
          "btns": [
            {
              "type": "edit",
              "label": "节点配置",
              "toRequest": {
                "url": "/views/msap/bpm/frameManagement/node_config.json",
                "method": "post",
                "params": {
                  "appId":"${{form.appId}}"
                }
              },
              "props": {
                "width": "70%",
                "title": "节点配置"
              }
            },
            {
              "type": "edit",
              "label": "维护",
              "toRequest": {
                "url": "/views/msap/bpm/frameManagement/page_frame_config_edit.json",
                "method": "post",
                "params": {
                  "appId":"${{form.appId}}"
                }
              },
              "props": {
                "width": "65%",
                "title": "页面框架基本信息修改"
              }
            },
            {
              "type": "delete",
              "label": "删除",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "post",
                "params": {
                  "servicecode": "bpm0043",
                  "appId":"${{form.appId}}"
                }
              }
            }
          ]
        }
      ],
      "pagination": {
        "show": true
      }
    }
  }
}
