{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "bpm0046",
          "appId":"${{params.appId}}"
        }
      },
      "btns": [
        {
          "type": "submit",
          "label": "提交"
        }
      ],
      "controls": [
        {
          "frame_cd": {
            "label": "框架编号",
            "control": "string",
            "value": "${{params.frame_cd}}",
            "width": "130px"
          },
          "func_cd": {
            "label": "功能编号",
            "control": "string",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入功能编号"
              }
            ]
          },
          "vue_path": {
            "label": "页面路径",
            "control": "text",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入页面路径"
              }
            ]
          },
          "tree_menu_name": {
            "label": "菜单名称",
            "control": "text",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入菜单名称"
              }
            ]
          },
          "tree_seq": {
            "label": "菜单序号",
            "control": "text",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入菜单名称"
              }
            ]
          },
          "is_enabled": {
            "control": "select",
            "label": "是否有效",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            },
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请选择是否有效"
              }
            ]
          },
          "child_frame_flag": {
            "control": "select",
            "label": "是否子框架",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            },
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请选择是否子框架"
              }
            ]
          }
        }
      ]
    }
  }
}
