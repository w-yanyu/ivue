{
    "module": "v_search_btn_datagrid",
    "layout": {
        "toolbar": {
            "btns": []
        },
        "datagrid": {
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "us0203",
                    "cust_no": "${{params.cust_no}}",
                  "belong_flash":"0"
                }
            },
            "thead": [{
                    "label": "客户名称",
                    "align": "center",
                    "prop": "cust_name"
                },
                {
                    "label": "客户英文名称",
                    "align": "center",
                    "prop": "cust_english_name"
                },
                {
                    "label": "曾用名",
                    "align": "center",
                    "prop": "cust_befor_name"
                },
                {
                    "label": "证件类型",
                    "align": "center",
                    "prop": "cert_type",
                    "dict": {
                        "dictType": "MS_E_CRET_TYPE",
                        "dictKey": [
                            "MS_E_CRET_TYPE"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "证件号码",
                    "align": "center",
                    "prop": "cert_no"
                },
                {
                    "label": "客户类别",
                    "align": "center",
                    "prop": "cust_type",
                    "dict": {
                        "dictType": "US_E_CUST_TYPE",
                        "dictKey": [
                            "US_E_CUST_TYPE"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "注册地址",
                    "align": "center",
                    "prop": "addr_name"
                },
                {
                    "label": "组织机构代码",
                    "align": "center",
                    "prop": "org_code"
                },
                {
                    "label": "信用机构代码",
                    "align": "center",
                    "prop": "credit_org_code"
                },
                {
                    "label": "客户号",
                    "align": "center",
                    "prop": "cust_no"
                }
            ]
        }
    }
}
