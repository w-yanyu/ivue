{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "controls": [{
                    "cust_name": {
                        "control": "text",
                        "label": "客户名称"
                    },
                    "appl_id": {
                        "control": "text",
                        "label": "申请编号"
                    }
                }
            ]
        },
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": true,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "cr1013"
                }
            },
            "thead": [{
                    "label": "申请书编号",
                    "prop": "appl_id",
                    "width": 160,
                    "align": "center"
                },
                {
                    "label": "客户名称",
                    "prop": "cust_name",
                    "width": 160,
                    "align": "center"
                },
                {
                    "label": "评级类型",
                    "prop": "temp_type",
                    "align": "center",
                    "dict": {
                        "dictType": "CR_E_TEMP_TYPE",
                        "dictKey": [
                            "CR_E_TEMP_TYPE"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "评级日期",
                    "prop": "eval_dt",
                    "align": "center",
                    "format": "yyyy-MM-dd"
                },
                {
                    "label": "报表日期",
                    "prop": "rept_dt",
                    "align": "center",
                    "format": "yyyy-MM-dd"
                },
                {
                    "label": "当前步骤",
                    "prop": "cur_step",
                    "align": "center"
                },
                {
                    "label": "有效起始日期",
                    "prop": "beff_dt",
                    "align": "center",
                    "format": "yyyy-MM-dd"
                },
                {
                    "label": "有效到期日期",
                    "prop": "eeff_dt",
                    "align": "center",
                    "format": "yyyy-MM-dd"
                },
                {
                    "label": "年度评级结果",
                    "prop": "rank_id",
                    "align": "center"
                },
                {
                    "label": "评级机构",
                    "prop": "brch_no",
                    "align": "center"
                },
                {
                    "label": "操作",
                    "fixed": "right",
                    "width": 130,
                    "align": "center",
                    "btns": [{
                            "type": "look",
                            "label": "详情",
                            "toRequest": {
                                "url": "@CRWaitToDoDeal",
                                "method": "GET",
                                "target": "navTab",
                                "params": {
                                    
                                }
                            },
                            "props": {
                                "title": "信用评级查询详情"
                            }
                        }

                    ]
                }
            ]
        }
    }
}