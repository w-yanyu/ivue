{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "scope": "templateScope",
            "initRequest": "${{params.methcd}}",
            "initDataOnce": true,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "post",
                "params": {
                    "servicecode": "AS_PLMS_CP_UPMTPA",
                    "methcd": "${{params.methcd}}"
                }
            },
            "btns": [{
                "type": "submit",
                "label": "提交"
            }],
            "controls": [{
                "methcd": {
                    "label": "方法编号",
                    "width": "130px",
                    "control": "string",
                    "span": 12
                },
                "sortno": {
                    "label": "参数序号",
                    "control": "string",
                    "width": "130px",
                    "rules": [{
                        "required": true,
                        "message": "请输入参数序号"
                    }]
                },
                "cansfl": {
                    "control": "select",
                    "label": "参数分类",
                    "dict": {
                        "dictType": "BPM_E_CANSFL",
                        "dictKey": [
                            "BPM_E_CANSFL"
                        ],
                        "format": "label"
                    },
                    "width": "130px",
                    "rules": [{
                        "required": true,
                        "message": "请选择参数分类"
                    }]
                },

                "canslx": {
                    "control": "select",
                    "label": "参数类型",
                    "dict": {
                        "dictType": "BPM_E_CANSLX",
                        "dictKey": [
                            "BPM_E_CANSLX"
                        ],
                        "format": "label"
                    },
                    "width": "130px",
                    "rules": [{
                        "required": true,
                        "message": "请选择参数类型"
                    }]
                },
                "gdcans": {
                    "control": "select",
                    "label": "固定参数名",
                    "dict": {
                        "dictType": "BPM_E_INCSXX",
                        "dictKey": [
                            "BPM_E_INCSXX"
                        ],
                        "format": "label"
                    },
                    "width": "130px",
                    "rules": [{
                        "required": true,
                        "message": "请选择固定参数名"
                    }]
                }
            }]
        }
    }
}