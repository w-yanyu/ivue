{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "scope": "templateScope",
            "initRequest": "${{params}}",
            "initDataOnce": true,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "post",
                "params": {
                    "servicecode": "CRMSCPTS0045"
                }
            },
            "btns": [{
                "type": "submit",
                "label": "提交"
            }],
            "controls": [{
                "ysjnum": {
                    "label": "元数据编号",
                    "control": "string",
                    "width": "130px"
                },
                "gettyp": {
                    "control": "select",
                    "label": "获取方式",
                    "dict": {
                        "dictType": "BPM_E_GETTYP",
                        "dictKey": [
                            "BPM_E_GETTYP"
                        ],
                        "format": "label"
                    },
                    "width": "130px",
                    "rules": [{
                        "required": true,
                        "message": "请选择获取方式"
                    }],
                    "showItem": {
                        "0": [
                            "biaomc",
                            "ziduan",
                            "chxutj",
                            "gensql",
                            "tjzxsq"
                        ],
                        "1": [
                            "retuzd",
                            "methnm"
                        ]
                    },
                    "events": {
                        "condition": {
                            "0": {
                                "control": {
                                    "biaomc": {
                                        "rules": [{
                                            "required": true,
                                            "message": "请输入表名"
                                        }]
                                    },

                                    "ziduan": {
                                        "rules": [{
                                            "required": true,
                                            "message": "请输入字段名"
                                        }]
                                    }
                                },
                                "chxutj": {
                                    "rules": [{
                                        "required": true,
                                        "message": "请输入查询条件"
                                    }]
                                },
                                "tjzxsq": {
                                    "rules": [{
                                        "required": true,
                                        "message": "请输入统计执行SQL"
                                    }]
                                }

                            },
                            "1": {
                                "control": {
                                    "methnm": {
                                        "rules": [{
                                            "required": true,
                                            "message": "请输入方法名"
                                        }]
                                    },

                                    "retuzd": {
                                        "rules": [{
                                            "required": true,
                                            "message": "请输入方法返回字段"
                                        }]
                                    }
                                }
                            }
                        }
                    }
                },
                "ysjnam": {
                    "label": "元数据名称",
                    "control": "text",
                    "width": "130px",
                    "rules": [{
                        "required": true,
                        "message": "请输入元数据名称"
                    }]
                },
                "dattyp": {
                    "control": "select",
                    "label": "数据类型",
                    "dict": {
                        "dictType": "BPM_E_DATTYP",
                        "dictKey": [
                            "BPM_E_DATTYP"
                        ],
                        "format": "label"
                    },
                    "width": "130px",
                    "rules": [{
                        "required": true,
                        "message": "请选择数据类型"
                    }]
                },

                "methnm": {
                    "label": "方法名",
                    "control": "text",
                    "width": "130px",
                    "initShow": false,
                    "span": 12
                },
                "retuzd": {
                    "label": "方法返回字段",
                    "width": "130px",
                    "control": "text",
                    "initShow": false,
                    "span": 12
                },


                "biaomc": {
                    "label": "表名",
                    "control": "text",
                    "width": "130px",
                    "initShow": false,
                    "span": 12
                },
                "ziduan": {
                    "label": "字段名",
                    "width": "130px",
                    "control": "text",
                    "initShow": false,
                    "span": 12
                },
                "chxutj": {
                    "label": "查询条件",
                    "control": "text",
                    "width": "130px",
                    "initShow": false,
                    "span": 12
                },
                "gensql": {
                    "label": "SQL预览",
                    "control": "string",
                    "width": "130px",
                    "initShow": false,
                    "span": 12
                },
                "tjzxsq": {
                    "label": "统计执行SQL",
                    "width": "130px",
                    "control": "text",
                    "initShow": false,
                    "span": 12
                },
                "sjfenl": {
                    "control": "hidden",
                    "width": "130px",
                    "initShow": false,
                    "span": 12
                }
            }]
        }
    }
}