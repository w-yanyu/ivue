{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "scope": "formData",
            "initRequest": "${{params}}",
            "btns": [{
                    "type": "submit",
                    "icon": "el-icon-check",
                    "label": "保存",
                    "doRequest": {
                        "method": "POST",
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "params": {
                            "servicecode": "cr0015",
                            "temp_id": "${{params.temp_id}}"
                        }
                    }
                },
                {
                    "type": "reset",
                    "icon": "el-icon-close",
                    "label": "重置"
                }
            ],
            "controls": [{
                    "temp_name": {
                        "control": "text",
                        "label": "模版名称",
                        "placeholder": "请输入",
                        "rules": [{
                            "required": true,
                            "message": "请输入"
                        }]
                    },
                    "temp_type": {
                        "control": "select",
                        "label": "模版类型",
                        "placeholder": "请选择",
                        "dict": {
                            "dictType": "CR_E_TEMP_TYPE",
                            "dictKey": [
                                "CR_E_TEMP_TYPE"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": true,
                            "message": "请选择"
                        }]
                    }
                },
                {
                    "temp_kind": {
                        "control": "select",
                        "label": "模版种类",
                        "placeholder": "请选择",
                        "dict": {
                            "dictType": "CR_E_TEMP_KIND",
                            "dictKey": [
                                "CR_E_TEMP_KIND"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": true,
                            "message": "请选择"
                        }]
                    },
                    "fincl_rate": {
                        "control": "text",
                        "label": "财务因素所占权重比(%)",
                        "placeholder": "请输入",
                        "rules": [{
                            "required": true,
                            "message": "请输入"
                        }]
                    }
                },
                {
                    "unfincl_rate": {
                        "control": "text",
                        "label": "非财务因素所占权重比(%)",
                        "placeholder": "请输入",
                        "rules": [{
                            "required": true,
                            "message": "请输入"
                        }]
                    },
                    "point_diff": {
                        "control": "text",
                        "label": "财务得分与非财务得分分差为多少评级结果无效",
                        "placeholder": "请输入",
                        "rules": [{
                            "required": true,
                            "message": "请输入"
                        }]
                    }
                },
                {
                    "fincl_strtegy_id": {
                        "control": "lookup",
                        "edit": true,
                        "label": "财务指标策略编码",
                        "keyField": {
                          "fincl_strtegy_id": "scoreCardCode"
                        },
                        "toRequest": {
                          "url": "/views/cr/params/template/strtegy_id_lookup.json",
                          "method": "post"
                        },
                        "props": {
                          "title": "请选择"
                        },
                        "rules": [{
                            "required": true,
                            "message": "请选择"
                        }]
                      }
                },
                {
                    "unfincl_strtegy_id": {
                        "control": "lookup",
                        "edit": true,
                        "label": "非财务指标策略编码",
                        "keyField": {
                            "unfincl_strtegy_id": "scoreCardCode"
                          },
                          "toRequest": {
                            "url": "/views/cr/params/template/strtegy_id_lookup.json",
                            "method": "post"
                          },
                        "placeholder": "请选择",
                        "rules": [{
                            "required": true,
                            "message": "请选择"
                        }]
                    }
                },
                {
                    "dt_term": {
                        "control": "inputNumber",
                        "label": "有效期期限(月)",
                        "placeholder": "请输入",
                        "rules": [{
                            "required": true,
                            "message": "请输入"
                        }]
                    }
                },
                {
                    "remark": {
                        "control": "textarea",
                        "label": "备注",
                        "placeholder": "请输入",
                        "rows": 4,
                        "span": 24
                    }
                }
            ]
        }
    }
}
