{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initDataOnce":true,
            "autoReset": true,
            "scope": "formData",
            "initRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "cr0080",
                  "temp_id": "${{params.temp_id}}"
                }
            },
            "btns": [{
                    "type": "submit",
                    "icon": "el-icon-check",
                    "label": "保存",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "cr0033",
                            "check_id": "${{current.check_id}}",
                            "temp_id": "${{params.temp_id}}"
                        }
                    }
                },
                {
                    "type": "delete",
                    "icon": "el-icon-close",
                    "label": "重置",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "cr0034",
                            "check_id": "${{current.check_id}}"
                        }
                    },
                    "show": {
                        "expression": "'${{params.is_valid}}' == '0'"
                    }

                }
            ],
            "controls": [

                {
                    "zichan_flag": {
                        "control": "select",
                        "label": "是否检查资产负债表",
                        "placeholder": "请选择",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": true,
                            "message": "请选择"
                        }]
                    },
                    "sunyi_flag": {
                        "control": "select",
                        "label": "是否检查损益表",
                        "placeholder": "请选择",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": true,
                            "message": "请选择"
                        }]
                    }
                },
                {
                    "cash_flag": {
                        "control": "select",
                        "label": "是否检查现金流量表",
                        "placeholder": "请选择",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": true,
                            "message": "请选择"
                        }]
                    },
                    "shouru_flag": {
                        "control": "select",
                        "label": "是否检查收入支出",
                        "placeholder": "请选择",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": true,
                            "message": "请选择"
                        }]
                    },
                    "fincl_year": {
                        "control": "text",
                        "label": "财务报表连续份数",
                        "rules": [{
                            "required": true,
                            "message": "请输入"
                        }]
                    },
                    "check_id": {
                        "control": "hidden",
                        "label": "检查项编码",
                        "disabled": true
                    },
                    "check_freq": {
                        "control": "select",
                        "label": "检查类型",
                        "dict": {
                            "dictType": "CR_E_RPTPERDTP",
                            "dictKey": [
                                "CR_E_RPTPERDTP"
                            ],
                            "format": "label"
                        },
                        "disabled": true
                    }
                }
            ]
        }
    }
}
