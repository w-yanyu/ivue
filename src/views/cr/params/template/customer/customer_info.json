{
    "module": "v_search_btn_datagrid",
    "layout": {
        "toolbar": {
            "btns": [{
                "type": "add",
                "target": "dialog",
                "label": "新增",
                "toRequest": {
                    "method": "GET",
                    "url": "/views/cr/params/template/customer/customer_add.json",
                    "params": {
                        "temp_id": "${{params.temp_id}}"
                    }
                },
                "show": {
                    "expression": "'${{params.is_valid}}' == '0'"
                },
                "props": {
                    "width": "65%",
                    "title": "客户关联新增"
                }
            }]
        },
        "datagrid": {
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "cr0021",
                    "temp_id": "${{params.temp_id}}"
                }
            },
            "pagination": {
                "pageSizes": [
                    10,
                    15,
                    20,
                    50,
                    100
                ],
                "pageSize": 10
            },
            "thead": [{
                    "type": "selection",
                    "align": "center",
                    "width": "55"
                },
                {
                    "type": "index",
                    "align": "center",
                    "label": "序号"
                },
                {
                    "label": "关联编码",
                    "align": "center",
                    "prop": "crel_id"
                },
                {
                    "label": "是否上市公司",
                    "align": "center",
                    "prop": "is_listed_company",
                    "dict": {
                        "dictType": "MS_E_YESORNO",
                        "dictKey": [
                            "MS_E_YESORNO"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "是否集团公司",
                    "align": "center",
                    "prop": "is_group_company",
                    "dict": {
                        "dictType": "MS_E_YESORNO",
                        "dictKey": [
                            "MS_E_YESORNO"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "客户类型",
                    "align": "center",
                    "prop": "cust_type",
                    "dict": {
                        "dictType": "US_E_CUST_TYPE",
                        "dictKey": [
                            "US_E_CUST_TYPE"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "企业规模",
                    "align": "center",
                    "prop": "corp_size",
                    "dict": {
                        "dictType": "CR_E_CORP_SIZE",
                        "dictKey": [
                            "CR_E_CORP_SIZE"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "操作",
                    "width": "260px",
                    "align": "center",
                    "btns": [{
                            "type": "edit",
                            "label": "详情",
                            "icon": "el-icon-view",
                            "toRequest": {
                                "url": "/views/cr/params/template/customer/customer_detail.json",
                                "method": "GET"
                            },
                            "props": {
                                "width": "65%",
                                "title": "客户关联详情"
                            }
                        },
                        {
                            "type": "edit",
                            "icon": "el-icon-edit",
                            "label": "维护",
                            "toRequest": {
                                "url": "/views/cr/params/template/customer/customer_modify.json",
                                "method": "GET"
                            },
                            "show": {
                                "expression": "'${{params.is_valid}}' == '0'"
                            },
                            "props": {
                                "width": "65%",
                                "title": "客户关联维护"
                            }
                        },
                        {
                            "type": "delete",
                            "label": "删除",
                            "icon": "el-icon-close",
                            "doRequest": {
                                "url": "/SUMP/icmcall/icmRPCCall",
                                "method": "delete",
                                "params": {
                                    "servicecode": "cr0024",
                                    "crel_id": "${{current.crel_id}}"
                                }
                            },
                            "show": {
                                "expression": "'${{params.is_valid}}' == '0'"
                            }
                        }
                    ]
                }
            ]
        }
    }
}