{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "scope": "formData",
            "initRequest": "${{params}}",
            "btns": [{
                    "type": "submit",
                    "icon": "el-icon-check",
                    "label": "保存",
                    "doRequest": {
                        "method": "POST",
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "params": {
                            "servicecode": "cr0023",
                            "temp_id": "${{params.temp_id}}",
                            "crel_id": "${{params.crel_id}}"
                        }
                    }
                },
                {
                    "type": "reset",
                    "icon": "el-icon-close",
                    "label": "重置"
                }
            ],
            "controls": [{
                    "is_listed_company": {
                        "control": "select",
                        "label": "是否上市公司",
                        "placeholder": "请选择",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": false,
                            "message": "请选择"
                        }]
                    },
                    "is_group_company": {
                        "control": "select",
                        "label": "是否集团公司",
                        "placeholder": "请选择",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": false,
                            "message": "请选择"
                        }]
                    }
                },
                {
                  "cust_type": {
                      "control": "select",
                      "label": "客户类型",
                      "placeholder": "请选择",
                      "dict": {
                          "dictType": "US_E_CUST_TYPE",
                          "dictKey": [
                              "US_E_CUST_TYPE"
                          ],
                          "format": "label"
                      },
                      "rules": [{
                          "required": true,
                          "message": "请选择"
                      }]
                  },
                    "corp_size": {
                        "control": "select",
                        "label": "企业规模",
                        "placeholder": "请选择",
                        "dict": {
                            "dictType": "CR_E_CORP_SIZE",
                            "dictKey": [
                                "CR_E_CORP_SIZE"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": false,
                            "message": "请选择"
                        }]
                    }
                }
            ]
        }
    }
}