{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {

            "scope": "formData",
            "btns": [{
                    "type": "submit",
                    "icon": "el-icon-check",
                    "label": "保存",
                    "doRequest": {
                        "method": "POST",
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "params": {
                            "servicecode": "cr0022",
                            "temp_id": "${{params.temp_id}}"
                        }
                    }
                },
                {
                    "type": "reset",
                    "icon": "el-icon-close",
                    "label": "重置"
                }
            ],
            "controls": [{
                    "education": {
                        "control": "select",
                        "label": "学历",
                        "placeholder": "请选择",
                        "dict": {
                            "dictType": "MS_E_MAXEDU",
                            "dictKey": [
                                "MS_E_MAXEDU"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": true,
                            "message": "请选择"
                        }]
                    },
                    "cust_type": {
                        "control": "select",
                        "label": "客户类型",
                        "placeholder": "请选择",
                        "dict": {
                            "dictType": "US_E_CUST_TYPE",
                            "dictKey": [
                                "US_E_CUST_TYPE"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": true,
                            "message": "请选择"
                        }]
                    }
                },
                {
                    "sex_tp": {
                        "control": "select",
                        "label": "性别",
                        "placeholder": "请选择",
                        "dict": {
                            "dictType": "MS_E_GENDER",
                            "dictKey": [
                                "MS_E_GENDER"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": true,
                            "message": "请选择"
                        }]
                    },
                    "marr_state": {
                        "control": "select",
                        "label": "婚姻状态",
                        "placeholder": "请选择",
                        "dict": {
                            "dictType": "MS_E_MARITA",
                            "dictKey": [
                                "MS_E_MARITA"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": true,
                            "message": "请选择"
                        }]
                    }
                }
            ]
        }
    }
}
