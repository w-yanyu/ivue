{
  "module": "v-realonlyForm-btn",
  "layout": {
    "form": {
      "scope": "formData",
      "initRequest": "${{params}}",
      "controls": [
        {
          
          "cust_type": {
            "control": "string",
            "label": "客户类型",
            "placeholder": "请选择",
            "dict": {
              "dictType": "US_E_CUST_TYPE",
              "dictKey": [
                "US_E_CUST_TYPE"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择"
              }
            ]
          },
          "sex_tp": {
            "control": "string",
            "label": "性别",
            "placeholder": "请选择",
            "dict": {
              "dictType": "MS_E_GENDER",
              "dictKey": [
                "MS_E_GENDER"
              ],
              "format": "label"
            }
          }
        },
        {
          "education": {
            "control": "string",
            "label": "学历",
            "placeholder": "请选择",
            "dict": {
              "dictType": "MS_E_MAXEDU",
              "dictKey": [
                "MS_E_MAXEDU"
              ],
              "format": "label"
            }
          },
          "marr_state": {
            "control": "string",
            "label": "婚姻状态",
            "placeholder": "请选择",
            "dict": {
              "dictType": "MS_E_MARITA",
              "dictKey": [
                "MS_E_MARITA"
              ],
              "format": "label"
            }
          }
        }
      ]
    }
  }
}
