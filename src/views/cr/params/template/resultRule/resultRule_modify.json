{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "scope": "formData",
            "initRequest": "${{params}}",
            "btns": [{
                    "type": "submit",
                    "icon": "el-icon-check",
                    "label": "保存",
                    "doRequest": {
                        "method": "POST",
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "params": {
                            "servicecode": "cr0053",
                            "adj_id": "${{params.adj_id}}",
                            "temp_id": "${{params.temp_id}}"
                        }
                    }
                },
                {
                    "type": "reset",
                    "icon": "el-icon-close",
                    "label": "重置"
                }
            ],
            "controls": [{
                    "adj_name": {
                        "control": "text",
                        "label": "规则名称",
                        "placeholder": "请输入",
                        "rules": [{
                            "required": true,
                            "message": "请输入"
                        }]
                    },
                    "req_flag": {
                        "control": "select",
                        "label": "是否硬性要求",
                        "placeholder": "请选择",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": true,
                            "message": "请选择"
                        }]
                    }
                },
                {
                    "adj_type": {
                        "control": "select",
                        "label": "调整种类",
                        "placeholder": "请选择",
                        "dict": {
                            "dictType": "CR_E_ADJ_TYPE",
                            "dictKey": [
                                "CR_E_ADJ_TYPE"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": true,
                            "message": "请选择"
                        }]
                    },
                    "adj_path": {
                        "control": "text",
                        "label": "方法路径",
                        "placeholder": "请输入",
                        "rules": [{
                            "required": true,
                            "message": "请输入"
                        }]
                    }
                },
                {
                    "adj_level": {
                        "control": "select",
                        "label": "调整级别",
                        "placeholder": "请选择",
                        "dict": {
                            "dictType": "CR_E_ADJ_LEVEL",
                            "dictKey": [
                                "CR_E_ADJ_LEVEL"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": true,
                            "message": "请选择"
                        }]
                    },
                    "adj_max": {
                        "control": "select",
                        "label": "可调整最大级别",
                        "placeholder": "请选择",
                        "dict": {
                            "dictType": "CR_E_ADJ_LEVEL",
                            "dictKey": [
                                "CR_E_ADJ_LEVEL"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": true,
                            "message": "请选择"
                        }]
                    }
                },
                {
                    "adj_min": {
                        "control": "select",
                        "label": "可调整最小级别",
                        "placeholder": "请选择",
                        "dict": {
                            "dictType": "CR_E_ADJ_LEVEL",
                            "dictKey": [
                                "CR_E_ADJ_LEVEL"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": true,
                            "message": "请选择"
                        }]
                    }
                }
            ]
        }
    }
}