{
    "module": "v-realonlyForm-btn",
    "layout": {
        "form": {
            "scope": "formData",
            "initRequest": "${{params}}",
            "controls": [{
                    "temp_name": {
                        "control": "string",
                        "label": "模版名称",
                        "placeholder": "请输入",
                        "rules": [{
                            "message": "请输入"
                        }]
                    },
                    "temp_type": {
                        "control": "string",
                        "label": "模版类型",
                        "placeholder": "请选择",
                        "dict": {
                            "dictType": "CR_E_TEMP_TYPE",
                            "dictKey": [
                                "CR_E_TEMP_TYPE"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "message": "请选择"
                        }],
                        "showItem": {
                            "0": [],
                            "1": [
                                "fincl_strtegy_id"
                            ],
                            "2": [
                                "fincl_strtegy_id"
                            ]
                        }
                    }
                },
                {
                    "temp_kind": {
                        "control": "string",
                        "label": "模版种类",
                        "placeholder": "请选择",
                        "dict": {
                            "dictType": "CR_E_TEMP_KIND",
                            "dictKey": [
                                "CR_E_TEMP_KIND"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "message": "请选择"
                        }]
                    },
                    "fincl_rate": {
                        "control": "string",
                        "label": "财务因素所占权重比(%)",
                        "placeholder": "请输入",
                        "rules": [{
                            "message": "请输入"
                        }]
                    }
                },
                {
                    "unfincl_rate": {
                        "control": "string",
                        "label": "非财务因素所占权重比(%)",
                        "placeholder": "请输入",
                        "rules": [{
                            "message": "请输入"
                        }]
                    },
                    "point_diff": {
                        "control": "string",
                        "label": "财务得分与非财务得分分差为多少评级结果无效",
                        "placeholder": "请输入",
                        "rules": [{
                            "message": "请输入"
                        }]
                    }
                },
                {
                    "fincl_strtegy_id": {
                        "control": "lookup",
                        "disabled": true,
                        "edit": true,
                        "label": "财务指标策略编码",
                        "keyField": {
                          "fincl_strtegy_id": "scoreCardCode"
                        },
                        "toRequest": {
                          "url": "/views/cr/params/template/strtegy_id_lookup.json",
                          "method": "post"
                        },
                        "props": {
                          "title": "请选择"
                        },
                        "rules": [{
                            "required": true,
                            "message": "请选择"
                        }]
                      }
                },
                {
                    "unfincl_strtegy_id": {
                        "control": "lookup",
                        "disabled": true,
                        "edit": true,
                        "label": "非财务指标策略编码",
                        "keyField": {
                            "unfincl_strtegy_id": "scoreCardCode"
                          },
                          "toRequest": {
                            "url": "/views/cr/params/template/strtegy_id_lookup.json",
                            "method": "post"
                          },
                        "placeholder": "请选择",
                        "rules": [{
                            "required": true,
                            "message": "请选择"
                        }]
                    }
                },
                {
                    "dt_term": {
                        "control": "inputNumber",
                        "disabled": true,
                        "label": "有效期期限(月)",
                        "placeholder": "请输入",
                        "rules": [{
                            "message": "请输入"
                        }]
                    }
                },
                {
                    "remark": {
                        "control": "textarea",
                        "disabled": true,
                        "label": "备注",
                        "placeholder": "请输入",
                        "rows": 4,
                        "span": 24
                    }
                }
            ]
        }
    }
}
