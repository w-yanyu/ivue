{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "scope": "formData",
            "initRequest": "${{params}}",
            "btns": [{
                    "type": "submit",
                    "icon": "el-icon-check",
                    "label": "保存",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "cr0062",
                            "temp_id": "${{params.temp_id}}",
                            "flproper_id": "${{params.flproper_id}}"
                        }
                    }
                },
                {
                    "type": "reset",
                    "icon": "el-icon-close",
                    "label": "重置"
                }
            ],
            "controls": [{
                    "role_id": {
                        "control": "hidden",
                        "label": "流程角色id"
                    }
                },
                {
                    "role_name": {
                        "control": "string",
                        "edit": true,
                        "label": "流程角色",
                        "span": 10
                    },
                    "read_level": {
                        "control": "select",
                        "label": "查看级别",
                        "span": 10,
                        "placeholder": "请选择",
                        "dict": {
                            "dictType": "CR_E_READ_LEVEL",
                            "dictKey": [
                                "CR_E_READ_LEVEL"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": true,
                            "message": "请选择"
                        }]
                    }
                },
                {
                    "rank_up": {
                        "control": "text",
                        "label": "可上调级数",
                        "span": 10,
                        "placeholder": "请输入整数级别",
                        "rules": [{
                            "required": true,
                            "message": "请输入整数级别"
                        }]
                    },
                    "rank_down": {
                        "control": "text",
                        "label": "可下调级数",
                        "span": 10,
                        "placeholder": "请输入整数级别",
                        "rules": [{
                            "required": true,
                            "message": "请输入整数级别"
                        }]
                    }
                }
            ]
        }
    }
}