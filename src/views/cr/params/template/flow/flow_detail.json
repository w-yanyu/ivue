{
  "module": "v-realonlyForm-btn",
  "layout": {
    "form": {
      "scope": "formData",
      "initRequest": "${{params}}",
      "controls": [
        {
          "role_id": {
            "control": "hidden",
            "label": "流程角色id"
          }
        },
        {
          "role_name": {
            "control": "string",
            "edit": true,
            "label": "流程角色",
            "span": 10
          },
          "read_level": {
           "control": "string",
            "label": "查看级别",
            "span": 10,
            "placeholder": "请选择",
            "dict": {
              "dictType": "CR_E_READ_LEVEL",
              "dictKey": [
                "CR_E_READ_LEVEL"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择"
              }
            ]
          }
        },
        {
          "rank_up": {
           "control": "string",
            "label": "可上调级数",
            "span": 10,
            "placeholder": "请输入整数级别",
            "rules": [
              {
                "required": true,
                "message": "请输入整数级别"
              }
            ]
          },
          "rank_down": {
           "control": "string",
            "label": "可下调级数",
            "span": 10,
            "placeholder": "请输入整数级别",
            "rules": [
              {
                "required": true,
                "message": "请输入整数级别"
              }
            ]
          }
        }
      ]
    }
  }
}
