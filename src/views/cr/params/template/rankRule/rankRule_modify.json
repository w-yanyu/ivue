{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "scope": "formData",
            "initRequest": "${{params}}",
            "btns": [{
                    "type": "submit",
                    "icon": "el-icon-check",
                    "label": "保存",
                    "doRequest": {
                        "method": "POST",
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "params": {
                            "servicecode": "cr0043",
                            "temp_id": "${{params.temp_id}}",
                            "level_rule_id": "${{params.level_rule_id}}"
                        }
                    }
                },
                {
                    "type": "reset",
                    "icon": "el-icon-close",
                    "label": "重置"
                }
            ],
            "controls": [{
                    "level_rule_rank": {
                        "control": "select",
                        "label": "级别",
                        "dict": {
                            "dictType": "CR_E_ADJ_LEVEL",
                            "dictKey": [
                                "CR_E_ADJ_LEVEL"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": true,
                            "message": "请选择级别"
                        }]
                    },
                    "credit_limit": {
                        "control": "text",
                        "label": "可授信额度",
                        "rules": [{
                            "required": true,
                            "message": "请输入"
                        }]
                    }
                },
                {
                    "sc_low": {
                        "control": "text",
                        "label": "分数下限",
                        "rules": [{
                            "required": true,
                            "message": "请输入"
                        }]
                    },
                    "sc_high": {
                        "control": "text",
                        "label": "分数下上限",
                        "rules": [{
                            "required": true,
                            "message": "请输入"
                        }]
                    }
                },
                {
                    "remark": {
                        "control": "textarea",
                        "label": "描述",
                        "span": 24,
                        "autosize": { "minRows": 3, "maxRows": 5 },
                        "rules": [{
                            "required": true,
                            "message": "请输入"
                        }]
                    }
                }
            ]
        }
    }
}