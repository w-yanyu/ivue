{
  "module": "v-realonlyForm-btn",
  "layout": {
    "form": {
      "scope":"formData",
      "initRequest": "${{params}}",
      "controls": [
        {
          "level_rule_rank": {
            "control": "string",
            "label": "级别",
            "dict": {
              "dictType": "CR_E_ADJ_LEVEL",
              "dictKey": [
                "CR_E_ADJ_LEVEL"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message":"请选择级别"
              }
            ]
          },
          "credit_limit": {
            "control": "string",
            "label": "可授信额度",
            "rules": [
              {
                "required": true,
                "message":"请输入"
              }
            ]
          }
        },
        {
          "sc_low": {
            "control": "string",
            "label": "分数下限",
            "rules": [
              {
                "required": true,
                "message":"请输入"
              }
            ]
          },
          "sc_high": {
            "control": "string",
            "label": "分数下上限",
            "rules": [
              {
                "required": true,
                "message":"请输入"
              }
            ]
          }
        },
        {
          "remark": {
            "control": "textarea",
            "disabled": true,
            "label": "描述",
            "span": 24,
            "autosize":{"minRows": 3,"maxRows":5},
            "rules": [
              {
                "required": true,
                "message":"请输入"
              }
            ]
          }
        }
      ]
    }
  }
}
