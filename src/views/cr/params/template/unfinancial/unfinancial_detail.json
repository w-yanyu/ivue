{
    "module": "v-form-editableDataGrid-btn",
    "layout": {
        "form": {
            "scope": "formData",
            "initRequest": "${{params}}",
            "controls": [{
                    "metric_id": {
                        "control": "string",
                        "label": "指标编码",
                        "rules": [{
                            "required": true,
                            "message": "请输入"
                        }]
                    },
                    "weight": {
                        "control": "string",
                        "label": "权重(%)",
                        "rules": [{
                            "required": true,
                            "message": "请输入"
                        }]
                    }
                },
                {
                    "metric_type": {
                        "control": "string",
                        "label": "指标类型",
                        "dict": {
                            "dictType": "CR_E_UNFINCL_METRIC_TYPE",
                            "dictKey": [
                                "CR_E_UNFINCL_METRIC_TYPE"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": true,
                            "message": "请输入"
                        }]
                    },
                    "metric_name": {
                        "control": "string",
                        "label": "指标名称",
                        "rules": [{
                            "required": true,
                            "message": "请输入"
                        }]
                    }
                },
                {
                    "is_valid": {
                        "label": "状态",
                        "control": "string",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": true,
                            "message": "请输入"
                        }]
                    }
                }
            ]
        },
        "datagrid": {
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "cr0075",
                    "metric_id": "${{params.metric_id}}"
                }
            },
            "pagination": {
               "pageStart":0,
                "pageSize": 10
            },
            "allData": true,
            "thead": [{
                    "type": "selection",
                    "align": "center",
                    "width": "55",
                    "fixed": "left",
                    "show": {
                        "prop": "changkuan",
                        "is": [
                            "0"
                        ]
                    }
                },
                {
                    "prop": "domain_key",
                    "type": "text",
                    "align": "center",
                    "label": "评级指标域值",
                    "rules": [{
                        "required": true,
                        "message": "请输入"
                    }]
                },
                {
                    "prop": "domain_name",
                    "type": "text",
                    "align": "center",
                    "label": "评级指标域名称",
                    "rules": [{
                        "required": true,
                        "message": "请输入"
                    }]
                },
                {
                    "prop": "domain_index",
                    "type": "text",
                    "align": "center",
                    "label": "显示顺序",
                    "rules": [{
                        "required": true,
                        "message": "请输入"
                    }]
                }
            ]
        }
    }
}