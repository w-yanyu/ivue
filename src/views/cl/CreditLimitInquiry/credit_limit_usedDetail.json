{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "controls": [],
            "btns": []
        },
        "toolbar": {
            "btns": []
        },
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": true,
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cl0041",
                "limit_no" : "${{params.limit_no}}"
              }
            },
            "thead": [
                {
                    "label": "额度编号",
                    "align": "center",
                    "prop": "limit_no"
                },
                {
                    "label": "客户编号",
                    "align": "center",
                    "prop": "cust_no"
                },
                {
                    "label": "交易流水",
                    "align": "center",
                    "prop": "tran_seq"
                },
                {
                    "label": "交易金额",
                    "align": "center",
                    "prop": "trxn_amt"
                },
                {
                    "label": "交易币种",
                    "align": "center",
                    "prop": "tran_ccy",
                    "dict": {
                        "dictType": "MS_CURRENCY",
                        "dictKey": [
                          "MS_CURRENCY"
                        ],
                        "format": "label"
                      }
                },
                {
                    "label": "交易时间",
                    "align": "center",
                    "prop": "trxn_time"
                },
                {
                    "label": "增减标志",
                    "align": "center",
                    "prop": "add_sub_flag",
                    "dict": {
                        "dictType": "CL_E_ADD_SUB_FLAG",
                        "dictKey": [
                          "CL_E_ADD_SUB_FLAG"
                        ],
                        "format": "label"
                      }
                }
            ]
        }
    }
}