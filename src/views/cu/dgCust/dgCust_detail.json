{
  "module": "v-inputDataForm-btn",
  "layout": {
    "scope": "templateScope",
    "form": {
      "btns":[],
      "controls": [
        {
          "top": {
            "control": "dynamic",
            "span": 24,
            "toRequest": {
              "url": "/views/cu/dgCust/dgCust_detail_top.json",
              "method": "GET",
              "params": {
                "props": "${{params}}"
              }
            }
          }
        },
        {
          "bottom": {
            "control":"dynamic",
            "span":24,
            "toRequest":{
              "url":"/views/cu/dgCust/dgCust_detail_buttom.json",
              "method": "GET",
              "params": {
                "custid": "${{params.custid}}"
              }
            }
          }
        }
      ]
    }
  }
}
