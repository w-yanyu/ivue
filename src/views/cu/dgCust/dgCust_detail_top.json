{
  "module": "v-realonlyForm-btn",
  "layout": {
    "form": {
      "controls": [
        {
          "custna": {
            "control": "string",
            "label": "客户名称",
            "span": 12,
            "value": "${{params.props.custna}}",
            "disabled": true
          },
          "cuenna": {
            "control": "string",
            "label": "客户英文名称",
            "span": 12,
            "value": "${{params.props.cuenna}}"

          },
          "cuodna": {
            "control": "string",
            "label": "曾用名",
            "span": 12,
            "value": "${{params.props.cuodna}}"
          }
        },
        {
          "table_name": {
            "control": "string",
            "label": "客户类型",
            "span": 12,
            "value": "对公客户"
          },

          "custty": {
            "span": 12,
            "label": "对公客户类别",
            "control": "select",
            "value": "${{params.props.custty}}",
            "disabled": true,
            "options": [
              {
                "label": "个人借款客户",
                "value": "1"
              },
              {
                "label": "对公借款客户",
                "value": "2"
              },
              {
                "label": "对公担保客户",
                "value": "3"
              },
              {
                "label": "个人担保客户",
                "value": "4"
              },
              {
                "label": "集团客户",
                "value": "5"
              },
              {
                "label": "个人股东客户",
                "value": "7"
              },
              {
                "label": "对公其他客户",
                "value": "8"
              },
              {
                "label": "法人关联客户",
                "value": "9"
              },
              {
                "label": "同业客户",
                "value": "11"
              }
            ]
          },
          "cuprop": {
            "span": 12,
            "control": "select",
            "disabled": true,
            "label": "客户性质",
            "value": "${{params.props.cuprop}}",
            "options": [
              {
                "label": "企业法人",
                "value": "01"
              },
              {
                "label": "企业非法人",
                "value": "02"
              },
              {
                "label": "个体工商户",
                "value": "03"
              },
              {
                "label": "事业单位",
                "value": "04"
              },
              {
                "label": "其他组织机构",
                "value": "99"
              }
            ]
          }
        },
        {
          "addrna": {
            "control": "string",
            "label": "注册地址",
            "span": 12,
            "value": "${{params.props.addrna}}"
          },
          "certtp": {
            "control": "string",
            "label": "证件类型",
            "span": 12,
            "value": "${{params.props.certtp}}"
          },
          "certno": {
            "control": "string",
            "label": "证件号码",
            "span": 12,
            "value": "${{params.props.certno}}"
          },
          "corgcd": {
            "span": 12,
            "control": "text",
            "label": "组织机构号",
            "value": "${{params.props.orgcd}}",
            "disabled": true
          }
        },
        {
        "culncd": {
          "span": 12,
          "control": "text",
          "label": "贷款卡编号",
          "value": "${{params.props.lncd}}",
          "disabled": true
        },
          "orgdit": {
            "control": "string",
            "span": 12,
            "label": "信用机构代码",
            "value": "${{params.props.orgdit}}"
          }
        }
      ]
    }
  }
}
