{
    "module": "v_search_btn_datagrid",
    "layout": {
        "datagrid": {
            "isInitSearch": true,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "IUMSCUTS0009",
                    "custid": "${{params.custid}}"
                }
            },
            "thead": [{
                "type": "string",
                "label": "客户号",
                "prop": "custno",
                "align": "center"
            }, {
                "type": "string",
                "label": "账号类型",
                "prop": "acctno_type",
                "align": "center"
            }, {
                "type": "string",
                "label": "备注信息",
                "prop": "remark",
                "align": "center"
            }]
        }
    }
}