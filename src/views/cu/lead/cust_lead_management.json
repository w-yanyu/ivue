{
  "module": "v_search_btn_datagrid",
  "layout": {
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "target": "dialog",
          "label": "新增",
          "toRequest": {
            "method": "GET",
            "url": "/views/cu/lead/cust_lead_add.json",
            "params": {
              "custid": "${{params.custid}}"
            }
          },
          "show": {
            "expression": "'${{params.show_flash}}' == '1'"
          },
          "props": {
            "width": "75%",
            "title": "新增实际控制人"
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "IUMSCUTS0024",
          "custid": "${{params.custid}}"
        }
      },
      "thead": [
        {
          "type": "string",
          "label": "实际控制人",
          "prop": "infact",
          "align": "center"
        },
        {
          "type": "select",
          "label": "实际控制人类型",
          "prop": "lead_type",
          "align": "center",
          "dict": {
            "dictType": "E_LETYPE",
            "dictKey": [
              "E_LETYPE"
            ],
            "format": "label"
          }
        },
        {
          "type": "string",
          "label": "客户名称",
          "prop": "custna",
          "align": "center"
        },
        {
          "type": "select",
          "label": "证件类型",
          "prop": "cert_type",
          "align": "center",
          "dict": {
            "dictType": "E_PUBLIC",
            "dictKey": [
              "E_PUBLIC "
            ],
            "format": "label"
          }
        },
        {
          "type": "string",
          "label": "证件号码",
          "prop": "cert_no",
          "align": "center"
        },
        {
          "type": "string",
          "label": "组织机构号代码",
          "prop": "corgcd",
          "align": "center"
        },
        {
          "type": "string",
          "label": "有效状态",
          "prop": "statss",
          "align": "center",
          "dict": {
            "dictType": "E_YES_",
            "dictKey": [
              "E_YES_"
            ],
            "format": "label"
          }
        },
        {
          "type": "string",
          "label": "流水号",
          "prop": "lead_id",
          "align": "center"
        },
        {
          "label": "操作",
          "align": "center",
          "fixed": "right",
          "width": 160,
          "btns": [
            {
              "type": "look",
              "label": "详情",
              "toRequest": {
                "url": "/views/cu/lead/cust_lead_detail.json",
                "method": "POST"
              },
              "props": {
                "width": "75%",
                "title": "实际控制人信息详情"
              }
            },
            {
              "type": "edit",
              "label": "维护",
              "toRequest": {
                "url": "/views/cu/lead/cust_lead_modify.json",
                "method": "GET"
              },
              "show": {
                "expression": "'${{params.show_flash}}' == '1'"
              },
              "props": {
                "width": "75%",
                "title": "实际控制人信息维护"
              }
            },
            {
              "type": "delete",
              "label": "删除",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "IUMSCUTS0025",
                  "trantp": "DELETE"
                }
              },
              "show": {
                "expression": "'${{params.show_flash}}' == '1'"
              }
            }
          ]
        }
      ]
    }
  }
}
