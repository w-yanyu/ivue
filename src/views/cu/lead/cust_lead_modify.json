{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": "",
                    "doRequest": {
                        "url": "/SUMP/call/nodeModify",
                        "method": "POST",
                        "params": {
                            "servicecode": "IUMSCUTS0025",
                            "trantp": "UPDATE"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "custna": {
                        "control": "text",
                        "label": "实际控制人客户名称",
                        "disabled": true,
                        "width": "160px"
                    },
                    "lead_id": {
                        "control": "hidden",
                        "label": "流水号",
                        "width": "160px",
                        "disabled": true
                    },
                    "lead_type": {
                        "control": "select",
                        "label": "实际控制人类型",
                        "disabled": true,
                        "width": "160px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择实际控制人类型"
                            }
                        ],
                        "dict": {
                            "dictType": "E_LETYPE",
                            "dictKey": [
                                "E_LETYPE"
                            ],
                            "format": "label"
                        },
                        "showItem": {
                            "2": [
                                "corgcd"
                            ],
                            "9": [
                                "corgcd"
                            ]
                        }
                    }
                },
                {
                    "infact": {
                        "control": "hidden",
                        "label": "实际控制人ID",
                        "width": "160px"
                    }
                },
                {
                    "cert_type": {
                        "control": "select",
                        "label": "证件类型",
                        "width": "160px",
                        "disabled": true,
                        "dict": {
                            "dictType": "E_PUBLIC",
                            "dictKey": [
                                "E_PUBLIC"
                            ],
                            "format": "label"
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择证件类型"
                            }
                        ]
                    },
                    "cert_no": {
                        "control": "text",
                        "label": "证件号码",
                        "width": "160px",
                        "disabled": true,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入证件号码"
                            }
                        ]
                    }
                },
                {
                    "custid": {
                        "control": "hidden",
                        "label": "客户id",
                        "disabled": true
                    },
                    "corgcd": {
                        "control": "text",
                        "label": "组织机构代码",
                        "disabled": true,
                        "width": "160px",
                        "rules": [
                            {
                                "max": 30,
                                "pattern": "^[0-9]+$"
                            }
                        ]
                    },
                    "statss": {
                        "control": "select",
                        "label": "是否有效",
                        "width": "160px",
                        "dict": {
                            "dictType": "E_YES_",
                            "dictKey": [
                                "E_YES_"
                            ],
                            "format": "label"
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择是否有效"
                            }
                        ]
                    }
                }
            ]
        }
    }
}
