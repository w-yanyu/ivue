{
    "module": "v_search_btn_datagrid",
    "layout": {
      
      "toolbar": {
        "btns": [
          
        ]
      },
      "datagrid": {
        "scope": "searchTable",
        "isInitSearch": true,
        "doRequest": {
          "url": "/SUMP/icmcall/icmRPCCall",
          "method": "POST",
          "params": {
            "servicecode": "wa2153",
            "custid": "${{params.custid}}"
          }
        },
        "thead": [
            {
              "label": "预警批次",
              "width": "200px",
              "prop": "task_id",
              "align": "center"
            },
            {
              "label": "预警日期",
              "width": "200px",
              "prop": "warn_date",
              "align": "center",
              "control": "dateTimePicker",
              "format":"yyyyMMdd"
            },
            {
              "label": "案件编号",
              "width": "200px",
              "prop": "case_id",
              "align": "center"
            },
            {
              "label": "预警来源",
              "width": "200px",
              "prop": "warn_source",
              "align": "center"
            },
            {
              "label": "预警级别",
              "width": "200px",
              "prop": "risk_level_code",
              "align": "center"
            },
            {
              "label": "处置措施",
              "align": "center",
              "width": "200px",
              "prop": "method_code",
              "dict": {
                "dictType": "E_METHOD_CD",
                "dictKey": [
                  "E_METHOD_CD"
                ],
                "format": "label"
              }
            },
            {
              "label": "是否入催",
              "align": "center",
              "width": "200px",
              "prop": "ahead_pl_ind",
              "dict": {
                "dictType": "MS_E_YESORNO",
                "dictKey": [
                  "MS_E_YESORNO"
                ],
                "format": "label"
              }
            },
            {
              "label": "移除日期",
              "width": "200px",
              "prop": "romove_date",
              "align": "center",
              "control": "dateTimePicker",
              "format":"yyyyMMdd"
            },
            {
              "label": "预警处理人",
              "width": "200px",
              "prop": "warn_user_id",
              "align": "center"
            }

        ]
      }
    }
  }
  