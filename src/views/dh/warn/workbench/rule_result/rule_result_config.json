{
    "module": "lr_transferTable",
    "layout": {
        "transfer": {
            "height": "500px",
            "left": {
                "filterable": false,
                "title": "可选策略",
                "btn": {
                    "label": "授权",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "post",
                        "params": {
                            "servicecode": "wa2158"
                        }
                    }
                },
                "datagrid": {
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "wa2156"
                        }
                    },
                    "checked": "checked",
                    "pagination": {
                        "show": true,
                        "pageSizes": [5, 10, 15, 20],
                        "pageSize": 5
                    },
                    "thead": [{
                            "prop": "rule_id",
                            "label": "规则ID"
                        },
                        {
                            "prop": "rule_desc",
                            "label": "规则描述"

                        },
                        {
                            "prop": "result_score",
                            "label": "风险分数"

                        }
                    ]
                }
            },
            "right": {
                "filterable": false,
                "title": "已选策略",
                "placeholder": "请输入角色名称",
                "btn": {
                    "label": "取消授权",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "post",
                        "params": {
                            "servicecode": "wa2159"
                        }
                    }
                },
                "datagrid": {
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "post",
                        "params": {
                            "servicecode": "wa2157"
                        }
                    },
                    "thead": [{
                            "prop": "rule_id",
                            "label": "规则ID"
                        },
                        {
                            "prop": "rule_desc",
                            "label": "规则描述"

                        },
                        {
                            "prop": "result_score",
                            "label": "风险分数"

                        }
                    ]
                }
            }
        }
    }
}