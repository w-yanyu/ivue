      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "wa1104"
        }
      },
      "thead": [
        {
          "label": "产品编码",
          "prop": "product_code",
          "align": "center"
        },
        {
          "label": "产品名称",
          "prop": "product_name",
          "align": "center"
        },
        {
          "label": "基础产品ID",
          "prop": "bprdid",
          "align": "center"
        },
        {
          "label": "源产品ID",
          "prop": "srcpid",
          "align": "center"
        },
        {
          "label": "操作",
          "align": "center",
          "fixed": "right",
          "btns": [{
            "type": "bringback",
            "label": "选择"
          }]
        }
      ]
    }
  }
