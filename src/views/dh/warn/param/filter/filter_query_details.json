{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "name": "detail",
            "initRequest": "${{params}}",
            "btns": [],
            "controlsGroup": [
                {
                    "title": "筛选策略基本信息",
                    "tooltip": "",
                    "titleIcon": "",
                    "showHint": "",
                    "accordion": false,
                    "expand": true,
                    "controls": [
                        {
                            "strategy_name": {
                                "label": "策略名称",
                                "control": "text",
                                "span": "8",
                                "width": "25px",
                                "format": "label",
                                "disabled": "true",
                                "rules": [
                                    {
                                        "required": false,
                                        "message": "请输入策略名称"
                                    }
                                ]
                            },
                            "validity": {
                                "label": "是否启用",
                                "control": "select",
                                "disabled": "true",
                                "span": "8",
                                "width": "25px",
                                "multiple": false,
                                "dict": {
                                    "dictType": "MS_E_YESORNO",
                                    "dictKey": [
                                        "MS_E_YESORNO"
                                    ],
                                    "format": "label"
                                },
                                "rules": [
                                    {
                                        "required": false,
                                        "message": "请选择是否启用"
                                    }
                                ]
                            },
                            "check_style": {
                                "label": "抽取方式",
                                "control": "select",
                                "disabled": "true",
                                "span": "8",
                                "width": "25px",
                                "multiple": false,
                                "dict": {
                                    "dictType": "E_CHOOSE_TYPE",
                                    "dictKey": [
                                        "E_CHOOSE_TYPE"
                                    ],
                                    "format": "label"
                                },
                                "rules": [
                                    {
                                        "required": false,
                                        "message": "请选择抽取方式"
                                    }
                                ]
                            }
                        },
                        {
                            "check_number": {
                                "label": "抽取数量",
                                "disabled": "true",
                                "span": "8",
                                "width": "25px",
                                "control": "text",
                                "format": "label",
                                "rules": [
                                    {
                                        "required": false,
                                        "message": "请选择抽取数量"
                                    }
                                ]
                            },
                            "product_code": {
                                "label": "产品编码",
                                "span": "8",
                                "width": "25px",
                                "control": "lookup",
                                "edit": true,
                                "disabled": "true",
                                "keyField": {
                                    "product_code": "product_code"
                                },
                                "toRequest": {
                                    "url": "/views/dh/warn/param/filter/tranferProduct.json",
                                    "method": "post"
                                },
                                "props": {
                                    "title": "选择产品编码"
                                },
                                "format": "label"
                            },
                            "cooperation_org": {
                                "label": "合作机构",
                                "control": "text",
                                "disabled": "true",
                                "width": "25px",
                                "format": "label",
                                "span": "8"
                            },
                            "cooperation_shop": {
                                "label": "合作门店",
                                "control": "text",
                                "format": "label",
                                "disabled": "true",
                                "span": "8",
                                "width": "25px"
                            }
                        }
                    ]
                },
                {
                    "title": "筛选策略借据信息",
                    "tooltip": "",
                    "titleIcon": "",
                    "showHint": "",
                    "expand": true,
                    "controls": [
                        {
                            "credit_granting_start": {
                                "label": "授信区间(元)",
                                "control": "currency",
                                "disabled": "true",
                                "width": "25px",
                                "suffix": "元",
                                "span": "5",
                                "thousand": ","
                            },
                            "credit_granting_end": {
                                "label": "至",
                                "width": "25px",
                                "control": "currency",
                                "disabled": "true",
                                "suffix": "元",
                                "span": "7",
                                "thousand": ","
                            },
                            "loan_amount_start": {
                                "label": "贷款金额(元)",
                                "width": "25px",
                                "control": "currency",
                                "disabled": "true",
                                "span": "5",
                                "thousand": ","
                            },
                            "loan_amount_end": {
                                "label": "至",
                                "width": "25px",
                                "span": "3",
                                "control": "currency",
                                "disabled": "true",
                                "thousand": ","
                            },
                            "loan_balance_start": {
                                "label": "贷款余额(元)",
                                "width": "25px",
                                "control": "currency",
                                "disabled": "true",
                                "span": "5",
                                "thousand": ","
                            },
                            "loan_balance_end": {
                                "label": "至",
                                "width": "25px",
                                "control": "currency",
                                "disabled": "true",
                                "span": "7",
                                "thousand": ","
                            },
                            "loan_period_start": {
                                "label": "贷款期数(天)",
                                "control": "text",
                                "width": "25px",
                                "disabled": "true",
                                "span": "5",
                                "format": "label"
                            },
                            "loan_period_end": {
                                "label": "至",
                                "control": "text",
                                "disabled": "true",
                                "width": "25px",
                                "span": "3",
                                "format": "label"
                            },
                            "overdue_amount_start": {
                                "label": "逾期金额(元)",
                                "span": "5",
                                "control": "currency",
                                "disabled": "true",
                                "suffix": "元",
                                "width": "25px",
                                "thousand": ","
                            },
                            "overdue_amount_end": {
                                "label": "至",
                                "span": "7",
                                "control": "currency",
                                "disabled": "true",
                                "suffix": "元",
                                "width": "25px",
                                "thousand": ","
                            },
                            "overdue_period_start": {
                                "label": "逾期期数(天)",
                                "control": "text",
                                "disabled": "true",
                                "span": "5",
                                "width": "25px",
                                "format": "label"
                            },
                            "overdue_period_end": {
                                "label": "至",
                                "control": "text",
                                "disabled": "true",
                                "span": "3",
                                "width": "25px",
                                "format": "label"
                            }
                        },
                        {
                            "termfm_type": {
                                "label": "贷款期数类型",
                                "control": "select",
                                "multiple": false,
                                "disabled": "true",
                                "span": "8",
                                "width": "25px",
                                "dict": {
                                    "dictType": "E_TERMFM_TYPE",
                                    "dictKey": [
                                        "E_TERMFM_TYPE"
                                    ],
                                    "format": "label"
                                }
                            },
                            "dataRange": {
                                "control": "dateTimePicker",
                                "label": "贷款放款日期",
                                "disabled": "true",
                                "span": "8",
                                "width": "25px",
                                "type": "daterange",
                                "startPlaceholder": "起始日期",
                                "endPlaceholder": "到期日期",
                                "startField": "loan_grant_date_start",
                                "endField": "loan_grant_date_end",
                                "valueFormat": "yyyMMdd"
                            },
                            "asset_classify": {
                                "label": "资产分类",
                                "control": "select",
                                "disabled": "true",
                                "multiple": false,
                                "span": "8",
                                "width": "25px",
                                "dict": {
                                    "dictType": "E_CLASSIFY",
                                    "dictKey": [
                                        "E_CLASSIFY"
                                    ],
                                    "format": "label"
                                }
                            },
                            "excl_freezing_ind": {
                                "label": "是否排除冻结",
                                "disabled": "true",
                                "control": "select",
                                "multiple": false,
                                "span": "8",
                                "width": "25px",
                                "dict": {
                                    "dictType": "MS_E_YESORNO",
                                    "dictKey": [
                                        "MS_E_YESORNO"
                                    ],
                                    "format": "label"
                                }
                            },
                            "strategy_id": {
                                "label": "策略id",
                                "disabled": "true",
                                "control": "hidden",
                                "format": "label",
                                "value": "1"
                            },
                            "register_province": {
                                "label": "户籍地址(省)",
                                "control": "hidden",
                                "span": "10",
                                "edit": true,
                                "disabled": "true",
                                "keyField": {
                                    "register_province": "product_code"
                                },
                                "toRequest": {
                                    "url": "/pte/json/v_search_btn_datagrid/add.json",
                                    "method": "get",
                                    "params": {
                                        "btnDdd": "${{form.province}}",
                                        "btnname": "静态"
                                    }
                                },
                                "props": {
                                    "title": "选择产品编码"
                                },
                                "format": "label"
                            },
                            "register_city": {
                                "label": "户籍地址",
                                "control": "hidden",
                                "format": "label",
                                "doRequest": {
                                    "url": "/pte/json/v_search_btn_datagrid/add.json",
                                    "method": "get",
                                    "params": {
                                        "register_province": "${{form.province}}",
                                        "register_city": "${{form.city}}",
                                        "btnname": "静态"
                                    }
                                }
                            }
                        }
                    ]
                }
            ]
        }
    }
}