{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "wa1502",
              "trantp": "INSERT",
              "risk_type": "${{params.risk_type}}"
            }
          }
        }
      ],
      "controls": [
        {
          "risk_rate_code": {
            "control": "text",
            "label": "风险级别代码",
            "width": "130px",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请输入"
              },
              {
                "pattern": "^[\\w\\u4E00-\\u9FA5]{1,20}$",
                "message": "请输入长度在1至20由汉字，字母，数字或下划线组成的字符串"
              }
            ]
          },
          "risk_rate_name": {
            "control": "text",
            "label": "风险级别名称",
            "width": "130px",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请输入"
              },
              {
                "pattern": "^[\\w\\u4E00-\\u9FA5]{1,20}$",
                "message": "请输入长度在1至20由汉字，字母，数字或下划线组成的字符串"
              }
            ]
          }
        },
        {
          "risk_rate_signal": {
            "control": "select",
            "label": "风险信号",
            "width": "130px",
            "span": 12,
            "placeholder": "请选择",
            "dict": {
              "dictType": "E_RISKSIGNAL",
              "dictKey": [
                "E_RISKSIGNAL"
              ],
              "format": "value-label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择风险信号"
              }
            ]
          },
          "priority": {
            "control": "text",
            "label": "优先级",
            "width": "130px",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请输入优先级"
              },
              {
                "pattern": "^[0-9]*$",
                "message": "请输入整数"
              }
            ]
          }
        },
        {
          "first_operator": {
            "control": "select",
            "label": "风险分值区间(初值)",
            "placeholder": "请选择",
            "width": "130px",
            "span": 12,
            "dict": {
              "dictType": "E_RELATIONOPERATOR",
              "dictKey": [
                "E_RELATIONOPERATOR"
              ],
              "format": "label ${{current.second_number}}"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择区间符号"
              }
            ]
          },
          "first_number": {
            "control": "text",
            "width": "130px",
            "span": 10,
            "label": "",
            "rules": [
              {
                "required": true,
                "message": "请输入初始值"
              },
              {
                "pattern": "^[0-9]*$",
                "message": "请输入整数"
              }
            ]
          }
        },
        {
          "second_operator": {
            "control": "select",
            "label": "风险分值区间(末值)",
            "placeholder": "请选择",
            "width": "130px",
            "span": 12,
            "dict": {
              "dictType": "E_RELATIONOPERATOR",
              "dictKey": [
                "E_RELATIONOPERATOR"
              ],
              "format": "label ${{current.second_number}}"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择区间符号"
              }
            ]
          },
          "second_number": {
            "control": "text",
            "width": "130px",
            "span": 10,
            "label": "",
            "rules": [
              {
                "required": true,
                "message": "请输入结束值"
              },
              {
                "pattern": "^[0-9]*$",
                "message": "请输入整数"
              },
              {
                "expression": " ${{form.second_number}} >=  ${{form.first_number}} ",
                "message": "风险分值区间末值不能小于初值"
              }
            ]
          }
        },
        {
          "auto_deal_ind": {
            "control": "select",
            "label": "是否自动预警处置",
            "width": "130px",
            "span": 12,
            "placeholder": "请选择",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "value-label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择"
              }
            ]
          },
          "method_code": {
            "control": "select",
            "label": "借款人默认处置方式",
            "width": "130px",
            "span": 12,
            "placeholder": "请选择",
            "dict": {
              "dictType": "E_METHOD_CD",
              "dictKey": [
                "E_METHOD_CD"
              ],
              "format": "value-label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择处置方式"
              }
            ]
          }
        },
        {
          "risk_rate_desc": {
            "control": "textarea",
            "width": "130px",
            "label": "风险级别描述:",
            "span": 24,
            "maxLength": 200,
            "autosize": false,
            "rows": 4
          }
        }
      ]
    }
  }
}