{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "initDataOnce": true,
            "btns": [],
            "controls": [
                {
                    "risk_rate_code": {
                        "control": "text",
                        "label": "风险级别代码",
                        "disabled": true,
                        "width": "130px",
                        "span": 12,
                        "rules": [
                            {
                                "required": false,
                                "message": "请输入"
                            },
                            {
                                "pattern": "^[\\w\\u4E00-\\u9FA5]{1,20}$",
                                "message": "请输入长度在1至20由汉字，字母，数字或下划线组成的字符串"
                            }
                        ]
                    },
                    "risk_rate_name": {
                        "control": "text",
                        "disabled": true,
                        "label": "风险级别名称",
                        "width": "130px",
                        "span": 12,
                        "rules": [
                            {
                                "required": false,
                                "message": "请输入"
                            },
                            {
                                "pattern": "^[\\w\\u4E00-\\u9FA5]{1,20}$",
                                "message": "请输入长度在1至20由汉字，字母，数字或下划线组成的字符串"
                            }
                        ]
                    }
                },
                {
                    "risk_rate_signal": {
                        "control": "select",
                        "disabled": true,
                        "label": "风险信号",
                        "width": "130px",
                        "span": 12,
                        "placeholder": "请选择",
                        "dict": {
                            "dictType": "E_RISKSIGNAL",
                            "dictKey": [
                                "E_RISKSIGNAL"
                            ],
                            "format": "value-label"
                        },
                        "rules": [
                            {
                                "required": false,
                                "message": "请选择风险信号"
                            }
                        ]
                    },
                    "priority": {
                        "control": "text",
                        "label": "优先级",
                        "disabled": true,
                        "width": "130px",
                        "span": 12,
                        "rules": [
                            {
                                "required": false,
                                "message": "请输入优先级"
                            },
                            {
                                "pattern": "^[0-9]*$",
                                "message": "请输入整数"
                            }
                        ]
                    }
                },
                {
                    "first_operator": {
                        "control": "select",
                        "label": "风险分值区间(初值)",
                        "placeholder": "请选择",
                        "disabled": true,
                        "width": "130px",
                        "span": 12,
                        "dict": {
                            "dictType": "E_RELATIONOPERATOR",
                            "dictKey": [
                                "E_RELATIONOPERATOR"
                            ],
                            "format": "label ${{current.second_number}}"
                        },
                        "rules": [
                            {
                                "required": false,
                                "message": "请选择区间符号"
                            }
                        ]
                    },
                    "first_number": {
                        "control": "text",
                        "label": "",
                        "disabled": true,
                        "width": "130px",
                        "span": 10,
                        "rules": [
                            {
                                "required": false,
                                "message": "请输入初始值"
                            },
                            {
                                "pattern": "^[0-9]*$",
                                "message": "请输入整数"
                            }
                        ]
                    }
                },
                {
                    "second_operator": {
                        "control": "select",
                        "disabled": true,
                        "label": "风险分值区间(末值)",
                        "placeholder": "请选择",
                        "width": "130px",
                        "span": 12,
                        "dict": {
                            "dictType": "E_RELATIONOPERATOR",
                            "dictKey": [
                                "E_RELATIONOPERATOR"
                            ],
                            "format": "label ${{current.second_number}}"
                        },
                        "rules": [
                            {
                                "required": false,
                                "message": "请选择区间符号"
                            }
                        ]
                    },
                    "second_number": {
                        "control": "text",
                        "disabled": true,
                        "width": "130px",
                        "span": 10,
                        "label": "",
                        "rules": [
                            {
                                "required": false,
                                "message": "请输入初始值"
                            },
                            {
                                "pattern": "^[0-9]*$",
                                "message": "请输入整数"
                            }
                        ]
                    }
                },
                {
                    "auto_deal_ind": {
                        "control": "select",
                        "disabled": true,
                        "label": "是否自动预警处置",
                        "placeholder": "请选择",
                        "width": "130px",
                        "span": 12,
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "value-label"
                        },
                        "rules": [
                            {
                                "required": false,
                                "message": "请选择"
                            }
                        ]
                    },
                    "method_code": {
                        "control": "select",
                        "disabled": true,
                        "label": "借款人默认处置方式",
                        "placeholder": "请选择",
                        "width": "130px",
                        "span": 12,
                        "dict": {
                            "dictType": "E_METHOD_CD",
                            "dictKey": [
                                "E_METHOD_CD"
                            ],
                            "format": "value-label"
                        },
                        "rules": [
                            {
                                "required": false,
                                "message": "请选择处置方式"
                            }
                        ]
                    }
                },
                {
                    "risk_rate_desc": {
                        "control": "textarea",
                        "disabled": true,
                        "label": "风险级别描述:",
                        "width": "140px",
                        "span": 24,
                        "maxLength": 200,
                        "autosize": false,
                        "rows": 4
                    }
                }
            ]
        }
    }
}