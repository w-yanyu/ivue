{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "scope": "formData",
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "wa1302"
                        }
                    }
                }
            ],
            "controlsGroup": [
                {
                    "title": "预警任务单基本信息",
                    "tooltip": "",
                    "titleIcon": "",
                    "showHint": "",
                    "accordion": false,
                    "expand": true,
                    "controls": [
                        {
                            "task_name": {
                                "control": "text",
                                "label": "预警任务名称",
                                "width": "130px",
                                "rules": [
                                    {
                                        "required": true
                                    }
                                ],
                                "span": "11"
                            },
                            "validity": {
                                "control": "select",
                                "label": "是否启用",
                                "width": "130px",
                                "span": "10",
                                "rules": [
                                    {
                                        "required": true,
                                        "message": "请选择是否启用"
                                    }
                                ],
                                "dict": {
                                    "dictType": "MS_E_YESORNO",
                                    "dictKey": [
                                        "MS_E_YESORNO"
                                    ],
                                    "format": "label"
                                }
                            },
                            "batch_desc": {
                                "label": "描述",
                                "width": "130px",
                                "placeholder": "(300个字以内,若没有请填“无”)",
                                "control": "textarea",
                                "span": "24",
                                "autosize": {
                                    "minRows": 4
                                },
                                "maxlength": 300,
                                "showWordLimit": true
                            }
                        }
                    ]
                },
                {
                    "title": "预警任务单详细信息",
                    "tooltip": "",
                    "titleIcon": "",
                    "showHint": "",
                    "expand": true,
                    "controls": [
                        {
                            "screen_style": {
                                "control": "select",
                                "label": "预警对象筛选方式",
                                "width": "130px",
                                "span": "10",
                                "rules": [
                                    {
                                        "required": true,
                                        "message": "请输入预警对象筛选方式"
                                    }
                                ],
                                "dict": {
                                    "dictType": "E_SCREEN_STYLE",
                                    "dictKey": [
                                        "E_SCREEN_STYLE"
                                    ],
                                    "format": "label"
                                },
                                "showItem": {
                                    "S": [
                                        "strategy_id"
                                    ]
                                }
                            },
                            "strategy_id": {
                                "label": "策略编号",
                                "control": "lookup",
                                "edit": true,
                                "width": "130px",
                                "rules": [
                                    {
                                        "required": true,
                                        "message": "请输入策略编号"
                                    }
                                ],
                                "span": "10",
                                "disable": "true",
                                "initShow": false,
                                "keyField": {
                                    "strategy_id": "strategy_id"
                                },
                                "toRequest": {
                                    "url": "/views/dh/warn/param/worklist/worklist_filter.json",
                                    "method": "post"
                                },
                                "props": {
                                    "title": "选择筛选策略"
                                },
                                "format": "label"
                            },
                            "rule_group_no": {
                                "control": "remoteSelect",
                                "label": "预警包规则编号",
                                "width": "130px",
                                "span": "10",
                                "placeholder": "请选择",
                                "labelField": "strategy_name_cn",
                                "valueField": "strategy_code",
                                "doRequest": {
                                    "url": "/SUMP/icmcall/icmRPCCall",
                                    "method": "POST",
                                    "params": {
                                        "servicecode": "CRMSXPTS0017"
                                    }
                                },
                                "format": "value-label",
                                "rules": [
                                    {
                                        "required": true,
                                        "message": "请选择预警包规则"
                                    }
                                ],
                                "events": {
                                    "emit": "finstrtegy_desc"
                                }
                            },
                            "warn_rate_num": {
                                "control": "text",
                                "label": "预警频率",
                                "width": "130px",
                                "rules": [
                                    {
                                        "required": true,
                                        "message": "请输入预警频率"
                                    }
                                ],
                                "span": "10"
                            },
                            "warn_rate_style": {
                                "control": "select",
                                "label": "",
                                "width": "15px",
                                "span": "1",
                                "rules": [
                                    {
                                        "required": true,
                                        "message": "请选择预警频率周期"
                                    }
                                ],
                                "dict": {
                                    "dictType": "E_RATE_STAYLE",
                                    "dictKey": [
                                        "E_RATE_STAYLE"
                                    ],
                                    "format": "label"
                                }
                            }
                        },
                        {
                            "first_warn_time": {
                                "control": "dateTimePicker",
                                "label": "首次预警时间",
                                "width": "130px",
                                "rules": [
                                    {
                                        "required": true,
                                        "message": "请输入预警频率周期"
                                    }
                                ],
                                "span": "10",
                                "format": "yyyyMMdd"
                            },
                            "scheme_id": {
                                "control": "select",
                                "label": "工单分配规则",
                                "width": "130px",
                                "span": "10",
                                "rules": [
                                    {
                                        "required": true,
                                        "message": "请选择工单分配规则"
                                    }
                                ],
                                "dict": {
                                    "dictType": "E_ASSIGNED_STYLE",
                                    "dictKey": [
                                        "E_ASSIGNED_STYLE"
                                    ],
                                    "format": "label"
                                }
                            }
                        }
                    ]
                }
            ]
        }
    }
}