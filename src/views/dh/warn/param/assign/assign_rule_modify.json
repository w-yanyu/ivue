{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "scope": "formData",
            "btns": [{
                "type": "submit",
                "label": "保存",
                "doRequest": {
                    "url": "/SUMP/icmcall/icmRPCCall",
                    "method": "POST",
                    "params": {
                        "servicecode": "wa1204"

                    }
                }
            }],
            "controls": [
                {
                    "assign_rule_id": {
                        "control": "hidden",
                        "label": "案件分配规则编号",
                        "width": "130px",
                        "span": "10"
                    },
                    "assign_code": {
                        "control": "text",
                        "label": "预警案件分配编码",
                        "width": "130px",
                        "rules": [{
                            "required": true
                        }],
                        "span": "10"
                    },
                    "assign_obj": {
                        "control": "text",
                        "label": "预警案件分配对象",
                        "width": "130px",
                        "rules": [{
                            "required": true
                        }],
                        "span": "10"
                    }
                    
                },
                {
                    "scheme_id": {
                        "control": "text",
                        "label": "案件分配方案编号",
                        "width": "130px",
                        "disabled":true,
                        "value":"${{params.scheme_id}}",
                        "rules": [{
                            "required": true
                        }],
                        "span": "10"
                    },
                    "is_valid": {
                        "control": "select",
                        "label": "是否有效",
                        "width": "130px",
                        "span": "10",
                        "rules": [{
                            "required": true
                        }],
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        }
                    }
                }
                
            ]
        }
    }
}