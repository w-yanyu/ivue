{
  "module": "v-realonlyForm-btn",
  "layout": {
    "form": {
      "name": "detail",
      "scope": "template_print",
      "initRequest": "${{params}}",
      "btns": [
        {
          "type": "print",
          "icon": "el-icon-printer",
          "label": "打印",
          "css": "",
          "documentTitle": "打印放款凭证",
          "headerStyle": "font-weight：600;",
          "maxWidth": 1400,
          "printType": "html",
          "printable": "template_print",
          "scanStyles": true,
          "gridStyle": "border: 2px solid #3971A5;",
          "header": "",
          "style": ""
        },
        {
          "type": "close",
          "icon": "el-icon-close",
          "label": "关闭"
        }
      ],
      "controls": [
        {
          "lncfno": {
            "label": "借据号",
            "control": "string",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请输入借据号"
              }
            ]
          },
          "prodcd": {
            "label": "贷款产品",
            "control": "string",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请输入贷款产品"
              }
            ]
          },
          "old_acct_no": {
            "label": "原账号",
            "span": 12,
            "control": "string",
            "rules": [
              {
                "required": true,
                "message": "请输入原账号"
              }
            ]
          },
          "new_acct_no": {
            "label": "变更后账号",
            "span": 12,
            "control": "string",
            "rules": [
              {
                "required": true,
                "message": "请输入变更后账号"
              }
            ]
          }
        }
      ]
    }
  }
}
