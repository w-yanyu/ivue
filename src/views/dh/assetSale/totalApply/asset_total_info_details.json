{
  "module": "v-realonlyForm-btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "PLMSKGTS1009",
          "appt_id": "${{params.appt_id}}",
          "sale_gather_id": "${{params.sale_gather_id}}",
          "sale_id": "${{params.sale_id}}"
        }
      },
      "controls": [{
        "cust_id": {
          "control": "hidden"
        },
        "acc_brch_cd": {
          "control": "string",
          "label": "账务机构",
          "span": 7
        },
        "sale_amt": {
          "control": "string",
          "span": 7,
          "label": "出售金额(元)"
        },
        "save_acct": {
          "span": 7,
          "control": "string",
          "label": "存款账号"
        },
        "save_acct_name": {
          "span": 7,
          "control": "string",
          "label": "存款户名"
        },
        "save_acct_bank": {
          "span": 7,
          "control": "string",
          "label": "开户银行"
        }
      }]
    }
  }
}
