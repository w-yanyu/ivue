{
  "module": "v-realonlyForm-btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "PLMSKGTS1008",
          "params": {
            "appl_id": "${{params.appl_id}}",
            "asset_sale_id": "${{params.asset_sale_id}}",
            "sale_id": "${{params.sale_id}}"
          }
        }
      },
      "controls": [{
        "cust_id": {
          "control": "hidden"
        },
        "ref_no": {
          "control": "string",
          "label": "业务编号",
          "span": 9
        },
        "asset_origin_price": {
          "control": "string",
          "span": 6,
          "label": "转让资产原价"
        },
        "evl_price_type": {
          "control": "string",
          "label": "评估方式",
          "span": 6,
          "dict": {
            "dictType": "E_EVALUATION_WAY",
            "dictKey": [
              "E_EVALUATION_WAY"
            ],
            "format": "label"
          }
        },
        "sale_price": {
          "span": 9,
          "control": "string",
          "label": "转让价格"
        },
        "recv_usr_name": {
          "span": 6,
          "control": "string",
          "label": "受让人"
        },
        "sale_dt": {
          "span": 6,
          "control": "string",
          "label": "转让日期"
        }
      }]
    }
  }
}
