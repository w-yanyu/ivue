{
  "module": "v-form-btn-dynamic",
  "layout": {
    "form": {
      "name":"detail",
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "kg0014",
          "sale_id": "${{params.sale_id}}"
        }
      },
      
      "controls": [
        
        {
          "appl_id": {
            "control": "textarea",
            "disabled": true,
            "label": "申请编号",
            "width": "130px",
            "span":"10"
          },
          "sale_id": {
            "control": "textarea",
            "disabled": true,
            "label": "转让编码",
            "width": "130px",
            "span":"10"
          },
          "ref_no": {
            "control": "textarea",
            "disabled": true,
            "label": "业务编号",
            "width": "130px",
            "span":"10"
          },
          "recv_usr_name": {
            "control": "textarea",
            "disabled": true,
            "label": "受让方",
            "width": "130px",
            "span":"10"
          },
          "asset_origin_price": {
            "control": "currency",
            "disabled": true,
            "label": "转让资产原价(元)",
            "width": "130px",
            "span":"10"
          },
          "evl_price_type": {
            "control": "select",
            "disabled": true,
            "label": "评估方式",
            "width": "130px",
            "span":"10",
            "dict": {
              "dictType": "E_EVALUATION_WAY",
              "dictKey": [
                "E_EVALUATION_WAY"
              ]
            }
          },
          "sale_price": {
            "control": "currency",
            "disabled": true,
            "label": "转让价格(元)",
            "width": "130px",
            "span":"10"
          },
          "sale_dt": {
            "control": "dateTimePicker",
            "disabled": true,
            "label": "转让时间",
            "width": "130px",
            "span":"10",
            "format": "yyyyMMdd"
          },
          "apply_user": {
            "control": "textarea",
            "disabled": true,
            "label": "申请人",
            "width": "130px",
            "span":"10"
          },
          "apply_date": {
            "control": "dateTimePicker",
            "disabled": true,
            "label": "申请时间",
            "width": "130px",
            "span":"10",
            "format": "yyyyMMdd"
          },
          "cust_id": {
            "control": "textarea",
            "disabled": true,
            "label": "客户编码",
            "width": "130px",
            "span":"10"
          }
        }
      ]
    }
  }
}

