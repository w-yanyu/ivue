{
    "module": "v_search_btn_datagrid",
    "layout": {
      "form": {
        "controls": [
          {
            "flowno": {
              "control": "text",
              "label": "流程申请号",
              "rules": [
                {
                  "max": 18,
                  "message": "长度不能超过18个字符"
                }
              ]
            },
            "lncfno": {
              "control": "text",
              "label": "借据号",
              "rules": [
                {
                  "max": 80,
                  "message": "长度不能超过80个字符"
                }
              ]
            }
          },
          {
            "applid": {
              "control": "text",
              "label": "申请号",
              "rules": [
                {
                  "max": 80,
                  "message": "长度不能超过80个字符"
                }
              ]
            }
          }
        ]
      },
     
      "datagrid": {
        "scope": "searchTable",
        "isInitSearch": true,
        "doRequest": {
          "url": "/SUMP/icmcall/icmRPCCall",
          "method": "POST",
          "params": {
            "servicecode": "PLMSKGTS0098",
            "lncfno": "${{current.lncfno}}",
             "flowno": "${{current.flowno}}",
             "applid": "${{current.applid}}"
          }
        },
        "thead": [
          {
            "label": "流程申请号",
            "prop": "flowno",
            "align": "center"
          },
          {
            "label": "借据号",
            "prop": "lncfno",
            "align": "center"
          },
          {
            "label": "申请号",
            "prop": "applid",
            "align": "center"
          },
          {
            "label": "来源渠道",
            "prop": "srcchl",
            "align": "center"
          },
          {
            "label": "客户号",
            "prop": "custno",
            "align": "center"
          },
          {
            "label": "错误信息",
            "prop": "exception_msg",
            "align": "center"
          },
          {
            "label": "解锁状态",
            "prop": "lock_status",
            "align": "center",
            "dict": {
              "dictType": "E_LOCKST",
              "dictKey": [
                "E_LOCKST"
              ],
              "format": "label",
              "labelField": "dictName",
              "valueField": "dictId"
            }
          },
      
        {
            "label": "操作",
            "fixed": "right",
            "width": 100,
            "align": "center",
            "btns": [
                {
                    "type": "edit",
                    "label": "解锁",
                    "doRequest": {
                      "url": "/SUMP/icmcall/icmRPCCall",
                      "method": "POST",
                      "params": {
                        "servicecode": "PLMSKGTS0099",
                        "flowNo": "${{current.flowno}}"
                
                      }
                    },
                    "props": {
                      "title": "解锁"
                    }
                  }
            ]
          }
        ]
      }
    }
  }