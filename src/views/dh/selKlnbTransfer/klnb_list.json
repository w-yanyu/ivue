{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "controls": [
        {
          "accountDuration": {
            "label": "账龄",
            "control": "select",
            "multiple": true,
            "dict": {
              "dictKey": [
                "E_ACCDURATION"
              ],
              "dictType": "E_ACCDURATION",
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请输入账龄"
              }
            ]
          },
          "productType": {
            "label": "产品大类",
            "control": "select",
            "multiple": true,
            "dict": {
              "dictKey": [
                "E_PRODUCTTYPE"
              ],
              "dictType": "E_PRODUCTTYPE",
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请输入产品大类"
              }
            ]
          }
        },
        {
          "redu_inte_max": {
            "label": "减免利息上限(%)",
            "control": "inputNumber",
            "value": "${{params.redu_Inte_max}}",
            "min": 0,
            "max": 100,
            "controlsPosition": "right",
            "rules": [
              {
                "required": true,
                "message": "请输入减免利息上限"
              }
            ]
          },
          "redu_pena_inte_max": {
            "label": "减免罚息上限(%)",
            "control": "inputNumber",
            "value": "${{params.redu_pena_Inte_max}}",
            "min": 0,
            "max": 100,
            "controlsPosition": "right",
            "rules": [
              {
                "required": true,
                "message": "请输入减免罚息上限"
              }
            ]
          },
          "redu_cost_max": {
            "label": "减免费用上限(%)",
            "control": "inputNumber",
            "min": 0,
            "max": 100,
            "controlsPosition": "right",
            "rules": [
              {
                "required": true,
                "message": "请输入减免费用上限"
              }
            ]
          }
        }
      ],
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "PLMSKGTS0090",
              "paraId": "${{params.para_id}}"
            }
          }
        }
      ]
    }
  }
}