{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "lncfno": {
            "control": "text",
            "label": "借据号"
          },
          "custno": {
            "control": "hidden",
            "label": "客户号",
            "value":"${{params.custno}}"
        }
      }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "CORELNTS9105"
        }
      },
      "thead": [
        {
          "label": "操作",
          "fixed": "left",
          "btns": [
            {
              "type": "bringback",
              "label": "选择"
            }
          ]
        },
        {
          "label": "贷款借据号",
          "prop": "lncfno"
        },
        {
          "label": "还款账号",
          "prop": "rpacno"
        },
        {
          "label": "贷款产品",
          "prop": "prod_no"
        },
        {
          "label": "客户姓名",
          "prop": "acctna"
        },
        {
          "label": "借据金额",
          "prop": "lncfam"
        }
      ]
    }
  }
}
