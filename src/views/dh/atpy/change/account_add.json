{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "name": "detail",
      "btns": [
        {
          "type": "submit",
          "icon": "el-icon-check",
          "label": "保存",
          "doRequest": {
            "method": "POST",
            "url": "/SUMP/icmcall/icmRPCCall",
            "params": {
              "servicecode": "kg0002",
              "trantp": "INSERT"
            }
          }
        },
        {
          "type": "reset",
          "icon": "el-icon-close",
          "label": "重置"
        }
      ],
      "controls": [
        {
          "cust_id": {
            "label": "cust_id",
            "control": "hidden"
          },
          "custna": {
            "label": "custna",
            "control": "hidden"
          },
          "idtfno": {
            "label": "证件号码",
            "control": "hidden"
          },
          "cntrno": {
            "label": "合同号",
            "control": "hidden"
          }
        },
        {
          "lncfno": {
            "label": "借据号",
            "control": "lookup",
            "align":"center",
            "edit": false,
            "keyField": {
              "lncfno": "lncfno",
              "cust_id": "custno",
              "prodcd": "prodcd",
              "old_acct_no": "rpacno",
              "old_acct_name": "acctna",
              "old_acct_type": "clssst",
              "new_acct_name": "acctna",
              "cntrno": "cntrno",
              "idtfno": "idtfno",
              "custna": "acctna"
            },
            "toRequest": {
              "url": "/views/dh/atpy/change/duebill_no_lookup.json",
              "method": "post"
            },
            "rules": [
              {
                "required": true,
                "message": "请输入借据号"
              }
            ]
          },
          "prodcd": {
            "label": "贷款产品",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入贷款产品"
              }
            ]
          },
          "old_acct_no": {
            "label": "原账号",
            "control": "text",
            "disabled": true,
            "rules": [
              {
                "max": 19,
                "required": true,
                "message": "请输入原账号, 长度不超过19位"
              }
            ]
          },
          "new_acct_no": {
            "label": "变更后账号",
            "control": "text",
            "rules": [
              {
                "max": 19,
                "required": true,
                "message": "请输入变更后账号, 长度不超过19位"
              }
            ]
          },
          "old_acct_name": {
            "label": "原账户名称",
            "control": "string"
          },
          "new_acct_name": {
            "label": "变更后账户名称",
            "control": "string",
            "rules": [
              {
                "required": true,
                "message": "请输入变更后账户名称"
              }
            ]
          },
          "old_acct_type": {
            "label": "变更前账户类型",
            "control": "select",
            "value": "0",
            "disabled":true,
            "dict": {
              "dictType": "E_PAYACT",
              "dictKey": [
                "E_PAYACT"
              ],
              "format": "label"
            }
          },
          "new_acct_type": {
            "label": "变更后账户类型",
            "control": "select",
            "value": "0",
            "disabled":true,
            "dict": {
              "dictType": "E_PAYACT",
              "dictKey": [
                "E_PAYACT"
              ],
              "format": "label"
            }
          },
          "acct_ccy": {
            "label": "币种",
            "control": "select",
            "disabled":true,
            "value": "CNY",
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": [
                "MS_CURRENCY"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择币种"
              }
            ]
          },
          "acct_nt_tt_flag": {
            "label": "自动付款类型",
            "control": "select",
            "dict": {
              "dictType": "E_AUTO_PAY_TYPE",
              "dictKey": [
                "E_AUTO_PAY_TYPE"
              ],
              "format": "label"
            }
          },
          "val_dt": {
            "label": "处理日期",
            "control": "dateTimePicker",
            "format": "yyyyMMdd",
            "rules": [
              {
                "required": true,
                "message": "请输入处理日期"
              }
            ]
          },
          "exp_dt": {
            "label": "到期日",
            "control": "dateTimePicker",
            "format": "yyyyMMdd",
            "rules": [
              {
                "required": true,
                "message": "请输入到期日"
              }
            ]
          },
          "pri_no": {
            "label": "支付优先级",
            "control": "select",
            "dict": {
              "dictType": "E_PAY_PRIORITY",
              "dictKey": [
                "E_PAY_PRIORITY"
              ],
              "format": "label"
            }
          },
          "sign_flag": {
            "label": "是否签约",
            "control": "select",
            "dict": {
              "dictType": "E_YES___",
              "dictKey": [
                "E_YES___"
              ],
              "format": "label"
            }
          },
          "remark": {
            "label": "备注",
            "control": "textarea",
            "span": 24,
            "autosize": {
              "minRows": 3,
              "maxRows": 5
            },
            "maxlength": 100,
            "showWordLimit": true
          }
        }
      ]
    }
  }
}
