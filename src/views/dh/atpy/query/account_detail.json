{
    "module": "v-realonlyForm-btn",
    "layout": {
        "form": {
            "name": "detail",
            "initRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "kg0008",
                    "qutaid": "${{params.instr_id}}"
                }
            },
            "controls": [{
                "lncfno": {
                    "label": "借据号",
                    "control": "string",
                    "rules": [{
                        "required": true,
                        "message": "请输入借据号"
                    }]
                },
                "prodcd": {
                    "label": "贷款产品",
                    "control": "string",
                    "rules": [{
                        "required": true,
                        "message": "请输入贷款产品"
                    }]
                },
                "old_acct_no": {
                    "label": "原账号",
                    "control": "string",
                    "rules": [{
                        "required": true,
                        "message": "请输入原账号"
                    }]
                },
                "new_acct_no": {
                    "label": "变更后账号",
                    "control": "string",
                    "rules": [{
                        "required": true,
                        "message": "请输入变更后账号"
                    }]
                },
                "old_acct_brch_cd": {
                    "label": "变更前所在支行",
                    "control": "hidden"
                },
                "old_acct_brch_name": {
                    "label": "变更前支行名称",
                    "control": "string",
                    "rules": [{
                        "required": true,
                        "message": "请输入变更前支行名称"
                    }]
                },
                "new_acct_brch_cd": {
                    "label": "变更后所在支行",
                    "control": "hidden"
                },
                "new_acct_brch_name": {
                    "label": "变更后支行名称",
                    "control": "string",
                    "rules": [{
                        "required": true,
                        "message": "请输入变更后支行名称"
                    }]
                },
                "old_acct_name": {
                    "label": "原账户名称",
                    "control": "string",
                    "rules": [{
                        "required": true,
                        "message": "请输入原账户名称"
                    }]
                },
                "new_acct_name": {
                    "label": "变更后账户名称",
                    "control": "string",
                    "rules": [{
                        "required": true,
                        "message": "请输入变更后账户名称"
                    }]
                },
                "old_acct_type": {
                    "label": "变更前账户类型",
                    "control": "string",
                    "dict": {
                        "dictType": "E_PAYACT",
                        "dictKey": [
                            "E_PAYACT"
                        ],
                        "format": "label"
                    }
                },
                "new_acct_type": {
                    "label": "变更后账户类型",
                    "control": "string",
                    "dict": {
                        "dictType": "E_PAYACT",
                        "dictKey": [
                            "E_PAYACT"
                        ],
                        "format": "label"
                    }
                },
                "acct_ccy": {
                    "label": "币种",
                    "control": "string",
                    "dict": {
                        "dictType": "MS_CURRENCY",
                        "dictKey": [
                            "MS_CURRENCY"
                        ],
                        "format": "label"
                    }
                },
                "acct_nt_tt_flag": {
                    "label": "自动付款类型",
                    "control": "string",
                    "dict": {
                        "dictType": "E_AUTO_PAY_TYPE",
                        "dictKey": [
                            "E_AUTO_PAY_TYPE"
                        ],
                        "format": "label"
                    }
                },
                "val_dt": {
                    "label": "处理日期",
                    "control": "string",
                    "rules": [{
                        "required": true,
                        "message": "请输入处理日期"
                    }]
                },
                "exp_dt": {
                    "label": "到期日",
                    "control": "string",
                    "rules": [{
                        "required": true,
                        "message": "请输入到期日"
                    }]
                },
                "pri_no": {
                    "label": "支付优先级",
                    "control": "string",
                    "dict": {
                        "dictType": "E_PAY_PRIORITY",
                        "dictKey": [
                            "E_PAY_PRIORITY"
                        ],
                        "format": "label"
                    }
                },
                "sign_flag": {
                    "label": "是否签约",
                    "control": "string",
                    "dict": {
                        "dictType": "E_YES___",
                        "dictKey": [
                            "E_YES___"
                        ],
                        "format": "label"
                    }
                },
                "remark": {
                    "label": "备注",
                    "disabled": true,
                    "control": "textarea",
                    "span": 24,
                    "autosize": {
                        "minRows": 3,
                        "maxRows": 5
                    },
                    "maxlength": 100,
                    "showWordLimit": true
                }
            }]
        }
    }
}