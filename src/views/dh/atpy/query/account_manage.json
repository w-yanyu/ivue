{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "controls": [{
                "lncfno": {
                    "label": "借据号",
                    "control": "text"
                },
                "new_acct_no": {
                    "control": "text",
                    "label": "账号"
                }
            }]
        },
        "toolbar": {
            "btns": [

            ]
        },
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": true,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "kg0006"
                }
            },
            "thead": [{
                    "type": "selection",
                    "align": "center"
                },
                {
                    "label": "借据号",
                    "width": "160px",
                    "prop": "lncfno",
                    "align": "center"
                },
                {
                    "label": "贷款产品",
                    "prop": "prodcd",
                    "align": "center"
                },
                {
                    "label": "还款账号",
                    "prop": "new_acct_no",
                    "align": "center",
                    "width": "160px"
                },
                {
                    "label": "账户类型",
                    "prop": "new_acct_type",
                    "align": "center",
                    "dict": {
                        "dictType": "E_PAYACT",
                        "dictKey": [
                            "E_PAYACT"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "币种",
                    "prop": "acct_ccy",
                    "dict": {
                        "dictType": "MS_CURRENCY",
                        "dictKey": [
                            "MS_CURRENCY"
                        ],
                        "format": "label"
                    },
                    "align": "center"
                },
                {
                    "label": "新账户名",
                    "align": "center",
                    "prop": "new_acct_name"
                },
                {
                    "label": "自动付款状态",
                    "align": "center",
                    "prop": "acct_nt_tt_flag",
                    "dict": {
                        "dictType": "E_AUTO_PAY_TYPE",
                        "dictKey": [
                            "E_AUTO_PAY_TYPE"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "支付优先级",
                    "align": "center",
                    "prop": "pri_no",
                    "dict": {
                        "dictType": "E_PAY_PRIORITY",
                        "dictKey": [
                            "E_PAY_PRIORITY"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "账务机构",
                    "align": "center",
                    "prop": "new_acct_brch_name"
                },
                {
                    "label": "审批状态",
                    "align": "center",
                    "prop": "atpy_status",
                    "dict": {
                        "dictType": "E_ALTER_STATUS",
                        "dictKey": [
                            "E_ALTER_STATUS"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "操作",
                    "fixed": "right",
                    "width": 200,
                    "align": "center",
                    "btns": [

                        {
                            "type": "look",
                            "label": "查看详情",
                            "toRequest": {
                                "url": "@DHWaitToWithDeal",
                                "method": "GET",
                                "target": "navTab",
                                "params": {
                                    "instr_id": "${{current.instr_id}}",
                                    "appl_id": "${{current.appl_id}}",
                                    "proc_inst_id": "${{current.proc_inst_id}}"
                                }
                            },
                            "props": {
                                "title": "查看详情"
                            }
                        }
                    ]
                }
            ]
        }
    }
}