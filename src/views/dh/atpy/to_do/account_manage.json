{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "qryPayAcctTodo",
          "appl_id": "${{params.applid}}"
        }
      },
      "thead": [
        {
          "type": "selection",
          "align": "left"
        },
        {
          "label": "借据号",
          "prop": "lncfno",
          "align": "center"
        },
        {
          "label": "贷款产品",
          "prop": "prodcd",
          "align": "center"
        },
        {
          "label": "还款账号",
          "prop": "new_acct_no",
          "align": "center"
        },
        {
          "label": "账户类型",
          "prop": "new_acct_type",
          "dict": {
            "dictType": "E_PAYACT",
            "dictKey": [
              "E_PAYACT"
            ],
            "format": "label"
          },
          "align": "center"
        },
        {
          "label": "币种",
          "prop": "acct_ccy",
          "align": "center",
          "dict": {
            "dictType": "MS_CURRENCY",
            "dictKey": [
              "MS_CURRENCY"
            ],
            "format": "label"
          }
        },
        {
          "label": "当前处理步骤",
          "prop": "handle_node",
          "align": "center"
        },
        {
          "label": "发起人",
          "align": "center",
          "prop": "start_usr"
        },
        {
          "label": "发起机构",
          "align": "center",
          "prop": "start_org"
        },
        {
          "label": "发起时间",
          "align": "center",
          "prop": "data_create_time"
        },
        {
          "label": "操作",
          "fixed": "right",
          "width": 200,
          "align":"center",
          "btns": [
            {
              "type": "edit",
              "label": "查看",
              "toRequest": {
                "url": "/views/dh/atpy/change/account_detail.json",
                "method": "GET",
                "params": {

                }
              },
              "props": {
                "width": "65%",
                "title": "查看"
              }
            }
          ]
        }
      ]
    }
  }
}
