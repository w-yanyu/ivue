{
  "module": "v_search_btn_datagrid",

  "layout": {
    "form": {
      "scope": "formData",
      "controls": [
        {
          "cust_id": {
            "control": "text",
            "label": "客户编号"
          },
          "template_cd": {
            "control": "text",
            "label": "模板编号"
          }
        },
        {
          "tel_no": {
            "control": "text",
            "label": "电话号码"
          },
          "send_date": {
            "control": "dateTimePicker",
            "label": "发送时间"
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "qrySmsHis"
        }
      },
      "thead": [
        {
          "label": "短信流水号",
          "align":"center",
          "prop": "sms_id"
        },
        {
          "label": "客户编号",
          "align":"center",
          "prop": "cust_id"
        },
        {
          "label": "模板代码",
          "align":"center",
          "prop": "template_cd"
        },
        {
          "label": "发送状态",
          "align":"center",
          "prop": "send_status",
          "dict":{
            "dictType": "E_SENDSTATUS",
            "dictKey": [
              "E_SENDSTATUS"
            ],
            "format": "label"

          }
        },
        {
          "label": "电话号码",
          "align":"center",
          "prop": "tel_no"
        },
        { 
          "label": "发送时间",
          "align":"center",
          "prop": "send_date"
        },
        {
          "label": "短信内容",
          "align":"center",
          "width": "260px",
          "prop": "sms_content"
        }
      ]
    }
  }
}
