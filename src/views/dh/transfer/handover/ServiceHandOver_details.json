{
  "module": "v-form-btn-dynamic",
  "layout": {
    "form": {
      "initRequest":"${{params}}",
      "scope": "formData",
      "controls": [
        {
          "simwf_name": {
            "label": "移交产品",
            "control": "lookup",
            "width": "130px",
            "span": "10",
            "disabled": true,
            "edit": true,
            "keyField": {
              "simwf_name": "simwf_name",
              "simwf_cd": "simwf_cd",
              "cust_tx_flag": "cust_tx_flag",
              "accting_flag": "accting_flag",
              "trans_busi_flag": "trans_busi_flag",
              "trans_brch_flag": "trans_brch_flag"
            },
            "toRequest": {
              "url": "/views/dh/transfer/handover/tranferProduct.json",
              "method": "post"
            },
            "props": {
              "title": "选择移交产品"
            },
            "format": "label"
            
          }
        },
        {
          "simwf_cd": {
            "control": "hidden",
            "label": "移交产品编码",
            "width": "130px",
            "span":"10"
          },
          "accting_flag": {
            "control": "select",
            "label": "是否移交账务",
            "width": "130px",
            "disabled": true,
            "span":"10",
            "dict": {
              "dictType": "E_YES___",
              "dictKey": [
                "E_YES___"
              ],
              "format": "label"
            }
          }
        },
        {
          "cust_tx_flag": {
            "control": "select",
            "label": "是否移交客户",
            "width": "130px",
            "disabled": true,
            "span":"10",
            "dict": {
              "dictType": "E_YES___",
              "dictKey": [
                "E_YES___"
              ],
              "format": "label"
            }
          }
        },
        {
          "trans_busi_flag": {
            "control": "select",
            "label": "是否移交业务",
            "width": "130px",
            "span": "10",
            "disabled": true,
            "dict": {
              "dictType": "E_YES___",
              "dictKey": [
                "E_YES___"
              ],
              "format": "label"
            }
          }
        },
        {
          "trans_brch_flag": {
            "control": "select",
            "label": "是否跨机构移交",
            "width": "130px",
            "span":"10",
            "disabled": true,
            "dict": {
              "dictType": "E_YES___",
              "dictKey": [
                "E_YES___"
              ],
              "format": "label"
            }
          }
        },
        {
          "remark": {
            "label": "备注",
            "control": "textarea",
            "width": "130px",
            "span":"10",
            "disabled": true,
            "autosize":{
              "minRows": 2
            },
            "rules": [
              {
                "max": "200",
                "message": "长度不能超过200个字符"
              }
            ]
          }
        }
      ]
    }
  }
}

