{
  "module": "v_search_btn_datagrid",
  "layout": {

    "toolbar": {
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "qrculist",
          "appl_id":"${{params.appl_id}}"
        }
      },
      "thead": [
        {
          "type": "index",
          "align": "center"
        },
        {
          "label": "客户编号",
          "prop": "transfer_no",
          "align": "center"
        },
        {
          "label": "客户经理",
          "prop": "old_usr_name",
          "align": "center"
        },
        {
          "label": "移交后客户经理",
          "prop": "new_usr_name",
          "align": "center"
        },
        {
          "label": "原机构",
          "prop": "old_brch_cd",
          "align": "center"
        },
        {
          "label": "新机构",
          "prop": "new_brch_cd",
          "align": "center"
        }

      ]
    }
  }
}
