{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "lncfno": {
            "control": "text",
            "label": "借据号"
          },
          "custno": {
            "control": "hidden",
            "label": "客户号",
            "value":"${{params.custno}}"
        }
      }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "CORELNTS2006"
        }
      },
      "thead": [
        {
          "label": "操作",
          "fixed": "left",
          "btns": [
            {
              "type": "bringback",
              "label": "选择"
            }
          ]
        },
        {
          "label": "贷款借据号",
          "prop": "lncfno"
        },
        {
          "label": "客户姓名",
          "prop": "acctna"
        },
        {
          "label": "开户日期",
          "prop": "initdt"
        },
        {
          "label": "到期日期",
          "prop": "matudt"
        },
        {
          "label": "贷款金额",
          "prop": "lncfam"
        },
        {
          "label": "正常本金",
          "prop": "lnnpbl"
        },
        {
          "label": "逾期本金",
          "prop": "lnopbl"
        },
        {
          "label": "呆滞本金",
          "prop": "lndpbl"
        },
        {
          "label": "呆账本金",
          "prop": "lnbpbl"
        },
        {
          "label": "应收应计利息",
          "prop": "ysyjni"
        }
        ,
        {
          "label": "催收应计利息",
          "prop": "csyjni"
        }
        ,
        {
          "label": "应收欠息",
          "prop": "ysxxoi"
        }
        ,
        {
          "label": "催收欠息",
          "prop": "csxxoi"
        }
        ,
        {
          "label": "应收应计罚息",
          "prop": "ysyjpi"
        }
        ,
        {
          "label": "催收应计罚息",
          "prop": "csyjpi"
        }
        ,
        {
          "label": "应收罚息",
          "prop": "ysxxpi"
        }
        ,
        {
          "label": "催收罚息",
          "prop": "csxxpi"
        }
      ]
    }
  }
}
