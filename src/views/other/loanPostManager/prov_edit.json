{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "scope": "templateScope",
            "initRequest":"${{params}}",
            "initDataOnce":true,

            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "post",
              "params":{
                "servicecode": "PLMSKGSS0007",
                    "bbjtdm": "${{params.bbjtdm}}"
              }
            },
            "btns":[
				{
					"type":"submit",
					"label":"提交"
				}
			],
            "controls": [
                {
                    "bbjtdm": {
                        "label": "拨备计提代码",
                        "control": "string",
                        "width": "130px"
                    },
                    "bbjtmc": {
                        "label": "拨备计提名称",
                        "control": "string",
                        "width": "130px"
                    },
                    "bbjtbl": {
                        "label": "拨备计提比例",
                        "control": "inputNumber",
                        "precision":8,
                        "min": 0,
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入拨备计提比例"
                            }
                        ],
                        "controls": false,
                        "position": "left"
                    },
                    "bbztbl": {
                        "label": "拨备增提比例",
                        "control": "inputNumber",
                        "precision":8,
                        "min": 0,
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入拨备增提比例"
                            }
                        ],
                        "controls": false,
                        "position": "left"
                    },
                    "matmin": {
                        "label": "逾期天数最小值",
                        "control": "text",
                        "width": "130px",
                        "min": 0,
                        "controls": false,
                        "position": "left"
                        ,
                        "rules": [
                            {
                                "type": "number",
                                "message": "填写整数类型"
                            }
                        ]
                    },
                    "matmax": {
                        "label": "逾期天数最大值",
                        "control": "text",
                        "min": 0,
                        "width": "130px",
                        "controls": false,
                        "position": "left",
                        "rules": [
                            {
                                "type": "number",
                                "message": "填写整数类型"
                            }
                        ]
                    },
                    "firstl": {
                        "label": "拨备优先级",
                        "control": "text",
                        "min": 0,
                        "width": "130px",
                        "controls": false,
                        "position": "left",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入拨备优先级"
                            },
                            {
                                "type": "number",
                                "message": "填写整数类型"
                            }
                        ]
                    }
                }
            ]
        }
    }
}
