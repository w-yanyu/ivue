{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "controls": [
                {
                  "lncfno": {
                    "control": "text",
                    "label": "借据号"
                  },
                  "custna": {
                    "control": "text",
                    "label": "客户名称"
                  },
                  "cardno": {
                    "control": "text",
                    "label": "证件号码",
                    "rules": [
                      {
                        "max": 20,
                        "message": "长度不能超过20个字符"
                      }
                    ]
                  },
                  "status": {
                    "control": "select",
                    "label": "发送状态",
                    "align":"center",
                    "dict": {
                        "dictType": "E_SMSSTATUS",
                        "dictKey": [
                        "E_SMSSTATUS"
                        ],
                        "format": "label"
                    }
                  }
                }
            ]
        },
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": false,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "PLMSKGTS0052"
                }
            },
            "thead": [
                {
                    "label": "申请编号",
                    "prop": "applid",
                    "align":"center",
                    "visible": "true"
                },
                {
                    "label": "借据号",
                    "prop": "lncfno",
                    "align":"center",
                    "visible": "true"
                },
                {
                    "label": "客户名称",
                    "prop": "custna",
                    "align":"center"
                },
                {
                    "label": "证件号码",
                    "prop": "cardno",
                    "align":"center"
                },
                {
                    "label": "手机号码",
                    "prop": "mobile",
                    "align":"center"
                }, 
                {
                    "label": "业务类型",
                    "prop": "lncatp",
                    "align":"center",
                    "dict": {
                        "dictType": "E_LNCATP2",
                        "dictKey": [
                            "E_LNCATP2"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "短信发送状态",
                    "prop": "status",
                    "align":"center",
                    "dict": {
                        "dictType": "E_SMSSTATUS",
                        "dictKey": [
                            "E_SMSSTATUS"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "操作",
                    "fixed": "right",
                    "align": "center",
                    "btns": [
                        {
                            "type": "edit",
                            "label": "重新发送",
                            "doRequest": {
                                "url": "/SUMP/icmcall/icmRPCCall",
                                "method": "POST",
                                "params": {
                                 "servicecode": "PLMSKGTS0051"
                                }
                            },
                            "confirm": {
                                "title": "重新发送",
                                "msg": "确定重新发送短信？",
                                "confirmButtonText": "确定",
                                "cancelButtonText": "取消"
                                },
                            "props": {
                                "title": "重新发送",
                                "width":"80%"
                            }
                        }
                    ]
                }
              ]
        }
    }
}
