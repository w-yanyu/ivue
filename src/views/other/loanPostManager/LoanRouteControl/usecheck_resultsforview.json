{
  "module": "v_search_btn_datagrid",
  "layout": {
      "form": {
          "controls": [
            {
              "area_code": {
                "control": "text",
                "label": "区域中心",
                "rules": [
                  {
                    "max": 20,
                    "message": "长度不能超过30个字符"
                  }
                ]
              },
              "lncfno": {
                "control": "text",
                "label": "借据号",
                "rules": [
                  {
                    "max": 20,
                    "message": "长度不能超过30个字符"
                  }
                ]
              },
              "custna": {
                "control": "text",
                "label": "客户名称",
                "rules": [
                  {
                    "max": 20,
                    "message": "长度不能超过30个字符"
                  }
                ]
              },
              "idtfno": {
                "control": "text",
                "label": "证件号码",
                "rules": [
                  {
                    "max": 20,
                    "message": "长度不能超过30个字符"
                  }
                ]
              },
              "phone_no": {
                "control": "text",
                "label": "手机号码",
                "rules": [
                  {
                    "max": 20,
                    "message": "长度不能超过30个字符"
                  }
                ]
              },
              "loan_date": {
                "control": "text",
                "label": "放款时间",
                "rules": [
                  {
                    "max": 20,
                    "message": "长度不能超过30个字符"
                  }
                ]
              },
              "loan_amt": {
                "control": "text",
                "label": "放款金额",
                "rules": [
                  {
                    "max": 20,
                    "message": "长度不能超过30个字符"
                  }
                ]
              },
              "clssst": {
                "control": "select",
                "label": "贷款状态",
                "rules": [
                  {
                    "max": 20,
                    "message": "长度不能超过30个字符"
                  }
                ]
              },
              "acctst": {
                "control": "text",
                "label": "贷款余额",
                "rules": [
                  {
                    "max": 20,
                    "message": "长度不能超过30个字符"
                  }
                ]
              },
              "check_result": {
                "control": "text",
                "label": "用途检查结果",
                "rules": [
                  {
                    "max": 20,
                    "message": "长度不能超过30个字符"
                  }
                ]
              }
            }
          ]
        },

      "datagrid": {
          "isInitSearch": false,
          "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST", 
              "params": {
                  "servicecode": "PLMSKGSS0093"
                  
              }
          },
          "thead": [
              {
                  "type": "text",
                  "label": "区域中心",
                  "prop": "area_code",
                  "align": "center"
              },
              {
                  "type": "text",
                  "label": "借据号",
                  "prop": "lncfno",
                  "align": "center"
              },
              {
                 "type": "text",
                 "label": "客户名称",
                 "prop": "custna",
                 "align": "center"
            },
             {
                "type": "text",
                "label": "证件号码",
                "prop": "idtfno",
                "align": "center"
             },
             {
                "type": "text",
                "label": "手机号码",
                "prop": "phone_no",
                "align": "center"
              },
             {
               "type": "text",
               "label": "放款时间",
               "prop": "loan_date",
               "align": "center"
             },
             {
               "type": "text",
               "label": "放款金额",
               "prop": "loan_amt",
               "align": "center"
             },
             {
               "type": "text",
               "label": "贷款状态",
               "prop": "clssst",
               "align": "center"
             },
             {
              "type": "text",
              "label": "贷款余额",
              "prop": "acctst",
              "align": "center"
            },
            {
              "type": "text",
              "label": "逾期天数",
              "prop": "ovdays",
              "align": "center"
            },
            {
              "type": "text",
              "label": "用途检查结果",
              "prop": "check_result",
              "align": "center"
            }
          ]
      }
  }
}