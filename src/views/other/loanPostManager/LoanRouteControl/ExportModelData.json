{
  "module": "v_form_btn_dynamic",
  "layout": {
      "form": {
          "scope": "formData",
        
          "controls": [
              {
                  "upload": {
                      "control": "dynamic",
                     
                      "span": 6,
                      "toRequest": {
                          "url": "@SingleUpload",
                          "method": "get",
                          "params": {
                              "formKey": "upload",
                              "formLabel": "上传用途标记数据文件"
                          }
                      }
                  }
              }
          ]
      },
      "toolbar": {
          "btns": [
               {
                  "type": "submit",
                  "label": "导入",
                  "unplain":true,
                  "icon": "",
                  "doRequest": {
                      "url": "/SUMP/icmcall/icmRPCCall",
                      "method": "POST",
                      "params": {
                          "servicecode": "PLMSKGSS0094",
                          "upload": "${{formData.upload}}"
                      }
                  }
              }
          ]
      }              
  }
}