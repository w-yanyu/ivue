{
  "module": "v_search_btn_datagrid",
  "layout": {
      "form": {
          "controls": [
            {
              "lncfno": {
                "control": "text",
                "label": "借据号",
                "rules": [
                  {
                    "max": 20,
                    "message": "长度不能超过30个字符"
                  }
                ]
              },
              "custna": {
                "control": "text",
                "label": "客户名称",
                "rules": [
                  {
                    "max": 20,
                    "message": "长度不能超过30个字符"
                  }
                ]
              },
              "check_result": {
                "control": "select",
                "label": "用途检查结果",
                "dict": {
                  "dictType": "E_CHECKRES",
                  "dictKey": [
                    "E_CHECKRES"
                  ],
                  "format": "label"
                },
                "rules": [
                  {
                    "max": 20,
                    "message": "长度不能超过30个字符"
                  }
                ]
              },
              "checker": {
                "control": "text",
                "label": "检查人",
                "rules": [
                  {
                    "max": 20,
                    "message": "长度不能超过30个字符"
                  }
                ]
              },
              "check_time": {
                "control": "text",
                "label": "检查时间",
                "rules": [
                  {
                    "max": 20,
                    "message": "长度不能超过30个字符"
                  }
                ]
              },
              "check_way": {
                "control": "select",
                "label": "检查方式",
                "dict": {
                  "dictType": "E_CHECKWAY",
                  "dictKey": [
                    "E_CHECKWAY"
                  ],
                  "format": "label"
                },
                "rules": [
                  {
                    "max": 20,
                    "message": "长度不能超过30个字符"
                  }
                ]
              },
              "remark": {
                "control": "text",
                "label": "备注",
                "rules": [
                  {
                    "max": 20,
                    "message": "长度不能超过30个字符"
                  }
                ]
              }
            }
          ]
        },
        "toolbar": {
          
          "btns": [
            {
              "type": "export",
              "label": "用途检查标记数据导入",
              "toRequest": {
                "method": "GET",
                "url": "/views/other/loanPostManager/LoanRouteControl/ExportModelData.json"
              },
              "props": {
                "title": "用途检查标记数据导入"
              }
            },
            
            {
              "type": "export",
              "label": "导出模板",
              "icon": "iconfont icon-daochu",
              "doRequest": {
                  "url": "/SUMP/icmcall/plms/exportModelRouteCheckLoge",
                  "method": "post",
                  "params": {}
              },
              "confirm":{
                "title":"提示信息",
                "msg":"确认导出吗？",
                "confirmButtonText":"确定",
                "cancelButtonText":"取消"
              }
          }
          ]
      },
      "datagrid": {
          "isInitSearch": false,
          "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST", 
              "params": {
                  "servicecode": "PLMSKGSS0092"
                  
              }
          },
          "thead": [
              {
                  "type": "text",
                  "label": "借据号",
                  "prop": "lncfno",
                  "align": "center"
              },
              {
                  "type": "text",
                  "label": "客户姓名",
                  "prop": "custna",
                  "align": "center"
              },
              {
                 "type": "text",
                 "label": "贷款形态",
                 "prop": "clssst",
                 "align": "center",
                 "dict":{ 
                  "dictType": "E_LOANST",
                  "dictKey": [
                    "E_LOANST"
                  ],
                  "format": "label"
                }
            },
             {
                "type": "text",
                "label": "贷款借据状态",
                "prop": "loan_lon_status",
                "align": "center",
                "dict":{ 
                  "dictType": "E_LNCFST",
                  "dictKey": [
                    "E_LNCFST"
                  ],
                  "format": "label"
                }
             },
             {
                "type": "text",
                "label": "现贷余额",
                "prop": "acctst",
                "align": "center"
              },
             {
               "type": "text",
               "label": "用途检查结果",
               "prop": "check_result",
               "align": "center",
               "dict":{ 
                "dictType": "E_CHECKRES",
                "dictKey": [
                  "E_CHECKRES"
                ],
                "format": "label"
              }
             },
             {
               "type": "text",
               "label": "检查人",
               "prop": "checker",
               "align": "center"
             },
             {
               "type": "text",
               "label": "检查时间",
               "prop": "check_time",
               "align": "center"
             },
             {
              "type": "text",
              "label": "检查方式",
              "prop": "check_way",
              "align": "center",
              "dict": {
                "dictType": "E_CHECKWAY",
                "dictKey": [
                  "E_CHECKWAY"
                ],
                "format": "label"
              }
            },           {
              "type": "text",
              "label": "备注",
              "prop": "remark",
              "align": "center"
            }
          ]
      }
  }
}