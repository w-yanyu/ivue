{
	"module":"v_search_btn_datagrid",
	"layout":{
		"form":{
			"controls":[
				{
					"acctno":{
						"label":"贷款账号",
						"control":"text",
						"name":"acctno",
						"rules":[
							{
								"required":false
							},
							{
								"max":40,
								"message":"长度不能超过40个字符"
							}
						]
					},
					"transq":{
						"label":"交易流水",
						"control":"text",
						"name":"transq",
						"rules":[
							{
								"required":false
							},
							{
								"max":32,
								"message":"长度不能超过32个字符"
							}
						]
					}
				}
				
			]
		},
		"datagrid":{
			"isInitSearch":false,
			"scope":"searchTable",
			"doRequest":{
				"url":"/SUMP/icmcall/icmRPCCall",
				"method":"POST",
				"params":{
					"servicecode":"PLMSKGSS0004"
				}
			},
			"pagination":{
				"show":true
			},
			"thead":[
				{
					"label":"拨备计提代码",
					"prop":"bbjtdm"
				},
				{
					"label":"拨备计提名称",
					"prop":"bbjtmc"
				},
				{
					"label":"五级分类级别",
					"prop":"clevel"
				},
			
				{
					"label":"拨备计提比例",
					"prop":"bbjtbl"
				},
				{
					"label":"拨备增提比例",
					"prop":"bbztbl"
				},
			
				{
					"label":"逾期天数下限",
					"prop":"matmin"
				},
				{
					"label":"逾期天数上限",
					"prop":"matmax"
				},
				{
					"label":"拨备优先级",
					"prop":"firstl"
				}
			]
		}
	}
}