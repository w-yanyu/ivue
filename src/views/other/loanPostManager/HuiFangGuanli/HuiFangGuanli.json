{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "batchnum": {
            "control": "text",
            "label": "批次号",
            "rules": [
              {
                "max": 18,
                "message": "长度不能超过18个字符"
              }
            ]
          },
          "lncfno": {
            "control": "text",
            "label": "借据号",
            "rules": [
              {
                "max": 80,
                "message": "长度不能超过80个字符"
              }
            ]
          }
        },
        {
          "acctna": {
            "control": "text",
            "label": "客户名称",
            "rules": [
              {
                "max": 80,
                "message": "长度不能超过80个字符"
              }
            ]
          },
          "phoneno": {
            "control": "text",
            "label": "手机号码",
            "rules": [
              {
                "max": 80,
                "message": "长度不能超过80个字符"
              }
            ]
          }
        },
        {
          "prodna": {
            "control": "text",
            "label": "产品名称",
            "rules": [
              {
                "max": 80,
                "message": "长度不能超过80个字符"
              }
            ]
          },
          "call_result": {
            "control": "select",
            "label": "通话结果",
            "dict": {
              "dictType": "E_TONGHJG",
              "dictKey": [
                "E_TONGHJG"
              ],
              "format": "label",
              "labelField": "dictName",
              "valueField": "dictId"
            }
          }
        },
        {
          "start_times": {
            "control": "dateTimePicker",
            "label": "开始回访日期",
            "format": "yyyy-MM-dd"
          },
          "end_times": {
            "control": "dateTimePicker",
            "label": "结束回访日期",
            "format": "yyyy-MM-dd"
          }
        },
        {
          "call_type": {
            "control": "select",
            "label": "通话类型",
            "dict": {
              "dictType": "E_TONGHLX",
              "dictKey": [
                "E_TONGHLX"
              ],
              "format": "label",
              "labelField": "dictName",
              "valueField": "dictId"
            }
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "export",
          "label": "回访结果导入",
          "toRequest": {
            "method": "GET",
            "url": "/views/other/loanPostManager/HuiFangGuanli/huifang.json"
          },
          "props": {
            "title": "回访结果导入"
          }
        },
        {
          "type": "export",
          "label": "导出模板",
          "icon": "iconfont icon-daochu",
          "doRequest": {
              "url": "/SUMP/icmcall/plms/exportModelDaihou",
              "method": "post",
              "params": {}
          },
          "confirm":{
            "title":"提示信息",
            "msg":"确认导出吗？",
            "confirmButtonText":"确定",
            "cancelButtonText":"取消"
          }
      }
      ]
    },

    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "PLMSKGTS0058"
        }
      },
      "thead": [
        {
          "label": "批次号",
          "prop": "batchnum",
          "align": "center"
        },
        {
          "label": "借据号",
          "prop": "lncfno",
          "align": "center"
        },
        {
          "label": "客户名称",
          "prop": "acctna",
          "align": "center"
        },
        {
          "label": "手机号码",
          "prop": "phoneno",
          "align": "center"
        },
        {
          "label": "产品名称",
          "prop": "prodna",
          "align": "center"
        },
        {
          "label": "放款日期",
          "prop": "lona_date",
          "align": "center"
        },
        {
          "label": "放款金额",
          "prop": "loanamt",
          "align": "center"
        },
        {
          "label": "贷款用途",
          "prop": "loan_purpose",
          "align": "center",
          "dict": {
            "dictType": "E_DAIKYTU",
            "dictKey": [
              "E_DAIKYTU"
            ],
            "format": "label",
            "labelField": "dictName",
            "valueField": "dictId"
          }
        },

        {
          "label": "回访完成时间",
          "prop": "complete_time",
          "align": "center"
        },
        {
          "label": "通话结果",
          "prop": "call_result",
          "align": "center",
             "dict": {
            "dictType": "E_TONGHJG",
            "dictKey": [
              "E_TONGHJG"
            ],
            "format": "label",
            "labelField": "dictName",
            "valueField": "dictId"
          }
        },
        {
          "label": "通话类型",
          "prop": "call_type",
          "align": "center",
            "dict": {
              "dictType": "E_TONGHLX",
              "dictKey": [
                "E_TONGHLX"
              ],
              "format": "label",
              "labelField": "dictName",
              "valueField": "dictId"
            }
        },
        {
          "label": "收费情况",
          "prop": "charge_situation",
          "align": "center",
          "dict": {
            "dictType": "E_SHOUFQK",
            "dictKey": [
              "E_SHOUFQK"
            ],
            "format": "label",
            "labelField": "dictName",
            "valueField": "dictId"
          }
        },
        {
          "label": "培训结果",
          "prop": "train_result",
          "align": "center",
          "dict": {
            "dictType": "E_PEIXJG",
            "dictKey": [
              "E_PEIXJG"
            ],
            "format": "label",
            "labelField": "dictName",
            "valueField": "dictId"
          }
        }
      ]
    }
  }
}
