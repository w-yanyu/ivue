{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "controls": [
                {
                  "applid": {
                    "control": "text",
                    "label": "申请编号"
                  },
                  "lncfno": {
                    "control": "text",
                    "label": "借据号"
                  }
                }
            ]
        },
        "toolbar": {
            "btns": [
            ]
        },
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": false,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "PLMSKGTS0056"
                }
            },
            "thead": [
                {
                "label": "申请号",
                "prop": "applid",
                "align":"center",
                "visible": "true"
                },  {
                    "label": "借据号",
                    "prop": "lncfno",
                    "align":"center",
                    "visible": "true"
                },  {
                    "label": "客户名称",
                    "prop": "custna",
                    "align":"center"
                },{
                    "label": "校验结果",
                    "prop": "checkStatus",
                    "align":"center",
                    "dict": {
                        "dictType": "E_CHECK_STATUS",
                        "dictKey": [
                            "E_CHECK_STATUS"
                        ],
                        "format": "label"
                    }
                },  {
                  "label": "业务类型",
                  "prop": "lncatp",
                  "align":"center",
                  "dict": {
                      "dictType": "E_LNCATP2",
                      "dictKey": [
                          "E_LNCATP2"
                      ],
                      "format": "label"
                  }
                },  {
                    "label": "创建时间",
                    "prop": "gmt_create",
                    "align":"center"
                }
            ]
        }
    }
}
