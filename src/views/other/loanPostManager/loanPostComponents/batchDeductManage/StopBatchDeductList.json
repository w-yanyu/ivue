{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "controls": [
                {
                    "brchno": {
                        "control": "remoteSelect",
                        "label": "区域中心",
                        "labelField": "brachName",
                        "valueField": "branchNo",
                        "doRequest": {
                          "url": "/SUMP/icmcall/icmRPCCall",
                          "method": "POST",
                          "params": {
                            "servicecode": "PLMSKGTS0135"
                          }
                        },
                        "format": "label"
                    },
                    "lncfno": {
                        "control": "text",
                        "label": "借据号"
                    },
                    "custna": {
                        "control": "text",
                        "label": "客户名称"
                    },
                    "cardno": {
                        "control": "text",
                        "label": "证件号码",
                        "rules": [
                        {
                            "max": 20,
                            "message": "长度不能超过20个字符"
                        }
                        ]
                    }
                }
            ]
        },
        "toolbar": {
            "btns": [
                {
                    "type": "custom",
                    "label":  "停批恢复",  
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                          "servicecode": "PLMSKGTS0078",
                          "stopBatchDeductList":"${{current.selectedData}}"
                        }
                    },
                    "confirm": {
                        "title": "停批恢复",
                        "msg": "确定恢复选中的停批申请么？",
                        "confirmButtonText": "确定",
                        "cancelButtonText": "取消"
                    }
                }
            ]
        },
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": false,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "PLMSKGTS0076"
                }
            },
            "thead": [
                {
                    "type" :  "selection",
                    "width": "55"
                },
                {
                    "label": "借据号",
                    "prop": "lncfno",
                    "align":"center",
                    "visible": "true"
                },
                {
                    "label": "客户名称",
                    "prop": "custna",
                    "align":"center"
                },
                {
                    "label": "证件号码",
                    "prop": "cardno",
                    "align":"center"
                },
                {
                    "label": "现贷余额",
                    "prop": "acctbl",
                    "align":"center"
                },  {
                    "label": "逾期天数",
                    "prop": "ovdays",
                    "align":"center"
                },  {
                    "label": "审批通过时间",
                    "prop": "stopBatchStart",
                    "align":"center"
                },  {
                    "label": "停批截止时间",
                    "prop": "stopBatchEnd",
                    "align":"center"
                },{
                    "label": "生效状态",
                    "prop": "isEffective",
                    "align":"center",
                    "dict": {
                        "dictType": "E_EFFECTIVE",
                        "dictKey": [
                            "E_EFFECTIVE"
                        ],
                        "format": "label"
                    }
                },  {
                    "label": "案件状态",
                    "prop": "applyStatus",
                    "align":"center",
                    "dict": {
                        "dictType": "E_APPLYSTATUS",
                        "dictKey": [
                            "E_APPLYSTATUS"
                        ],
                        "format": "label"
                    }
                }

              ]
        }
    }
}
