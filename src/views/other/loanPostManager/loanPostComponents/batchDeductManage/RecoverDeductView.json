{
    "module": "v_search_btn_datagrid",
    "layout": {
        
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": true,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "PLMSKGTS0075",
                    "applid": "${{params.applid}}"
                }
            },
            "thead": [
                {
                    "label": "借据号",
                    "prop": "lncfno",
                    "align":"center",
                    "visible": "true"
                },
                {
                    "label": "客户名称",
                    "prop": "custna",
                    "align":"center"
                },
                {
                    "label": "证件号码",
                    "prop": "cardno",
                    "align":"center"
                },
                {
                    "label": "现贷余额",
                    "prop": "acctbl",
                    "align":"center"
                },  {
                    "label": "逾期天数",
                    "prop": "ovdays",
                    "align":"center"
                },  {
                    "label": "停批通过时间",
                    "prop": "stopBatchStart",
                    "align":"center"
                },  {
                    "label": "停批截止时间",
                    "prop": "stopBatchEnd",
                    "align":"center"
                }

              ]
        }
    }
}
