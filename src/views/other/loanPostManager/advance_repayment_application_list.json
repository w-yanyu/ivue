{
	"module":"v_search_btn_datagrid",
	"layout":{
		"form":{
			"controls":[
				{
					"acctno":{
						"label":"贷款账号",
						"control":"text",
						"name":"acctno",
						"rules":[
							{
								"required":false
							},
							{
								"max":40,
								"message":"长度不能超过40个字符"
							}
						]
					},
					"transq":{
						"label":"交易流水",
						"control":"text",
						"name":"transq",
						"rules":[
							{
								"required":false
							},
							{
								"max":32,
								"message":"长度不能超过32个字符"
							}
						]
					}
				}
				
			]
		},
		"datagrid":{
			"isInitSearch":false,
			"scope":"searchTable",
			"doRequest":{
				"url":"/SUMP/icmcall/icmRPCCall",
				"method":"POST",
				"params":{
					"servicecode":"PLMSKGTS0023"
				}
			},
			"pagination":{
				"show":true
			},
			"thead":[

				{
					"label":"交易流水",
					"prop":"transq"
				},
				{
					"label":"交易日期",
					"prop":"trandt",
					"type":"dateTimePicker"
				},
				{
					"label":"贷款借据号",
					"prop":"lncfno"
				},
				{
					"label":"贷款账号",
					"prop":"acctno"
				},
				{
					"label":"事件编号",
					"prop":"evetna"
				},
				{
					"label":"减免方式",
					"prop":"remthd"
				},
				{
					"label":"上传url",
					"prop":"upload"
				},
				{
					"label":"是否急件",
					"prop":"urgent"
				},
				{
					"label":"备注信息",
					"prop":"remark"
				},
				{
					"label":"减免金额",
					"prop":"dtsfam"
                }
			]
		}
	}
}