{
    "module": "v_form_btn_dynamic",
    "layout": {
        "form": {
            "scope": "formData",
            "formSetTitle": "批量核销",
            "controls": [
                {
                   
                  
                    "upload": {
                        "control": "dynamic",
                        "isShowChildForm": true,
                        "span": 6,
                        "toRequest": {
                            "url": "@CancelUpload",
                            "method": "get",
                            "params": {
                                "formKey": "upload",
                                "formLabel": "上传核销文件"

                            }
                        }
                    }
                }
            ]
        },
        "toolbar": {
            "btns": [
                {
                    "type": "submit",
                    "label": "导入",
                    "unplain":true,
                    "icon": "",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "PLMSKGSS0012"
                        }
                    }
                },
                {
                    "type": "export",
                    "label": "导出模板",
                    "icon": "iconfont icon-daochu",
                    "doRequest": {
                        "url": "/SUMP/icmcall/plms/exportModel",
                        "method": "post",
                        "params": {}
                    },
                    "confirm":{
                      "title":"提示信息",
                      "msg":"确认导出吗？",
                      "confirmButtonText":"确定",
                      "cancelButtonText":"取消"
                    }
                }
            ]
        }
    }
}