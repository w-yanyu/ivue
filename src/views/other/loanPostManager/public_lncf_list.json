{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "CORELNTS2005",
          "idtfno": "${{params.idtfno}}",
          "prodcd": "${{params.prodcd}}"
        }
      },
      "thead": [
        {
          "label": "序号",
          "type": "index"
        },
        {
          "label": "客户姓名",
          "prop": "custna"
        },
        {
          "label": "证件类型",
          "prop": "idtftp",
          "dict": {
            "dictType": "E_IDTFTP",
            "dictKey": [
              "E_IDTFTP"
            ],
            "format": "label"
          }
        },
        {
          "label": "证件号码",
          "prop": "idtfno"
        },
        {
          "label": "借据号",
          "prop": "lncfno"
        },
        {
          "label": "产品名称",
          "prop": "prodna"
        },
        {
          "label": "放款日期",
          "prop": "initdt"
        },
        {
          "label": "到期日期",
          "prop": "matudt"
        },
        {
          "label": "贷款金额(元)",
          "prop": "lncfam"
        },
        {
          "label": "剩余金额(元)",
          "prop": "crrpbl"
        },
        {
          "label": "贷款状态",
          "prop": "acctst",
          "dict": {
            "dictType": "E_ACCTST",
            "dictKey": [
              "E_ACCTST"
            ],
            "format": "label"
          }
        },
        {
          "label": "贷款形态",
          "prop": "clssst",
          "dict": {
            "dictType": "E_CLSSST",
            "dictKey": [
              "E_CLSSST"
            ],
            "format": "label"
          }
        }
      ]
    }
  }
}