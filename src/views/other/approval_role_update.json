{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "AS_ICMS_CM_UPWFRL",
                    "operater_ind": "M"
                }
            },
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": "",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "AS_ICMS_CM_UPWFRL",
                            "operater_ind": "M"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "wfrlid": {
                        "control": "string",
                        "label": "审批角色编号",
                        "width": "160px",
                        "rules": [
                            {
                                "max": 10,
                                "message": "长度不能超过10个字符"
                            }
                        ]
                    },
                    "wfrlnm": {
                        "control": "text",
                        "label": "审批角色名称",
                        "width": "160px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入审批角色名称"
                            },
                            {
                                "max": 100,
                                "message": "长度不能超过100个字符"
                            }
                        ]
                    }
                },
                {
                    "rlauth": {
                        "control": "select",
                        "label": "审批层级",
                        "rules": [
                            {
                                "max": 8,
                                "message": "长度不能超过8个字符"
                            }
                        ],
                        "width": "160px",
                        "dict": {
                            "dictType": "E_RLAUTH",
                            "dictKey": [
                                "E_RLAUTH"
                            ],
                            "format": "label"
                        }
                    },
                    "remark": {
                        "control": "text",
                        "label": "备注",
                        "width": "160px",
                        "rules": [
                            {
                                "max": 100,
                                "message": "长度不能超过100个字符"
                            }
                        ]
                    }
                }
                
            ]
        }
    }
}