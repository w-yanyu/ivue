{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "controls": [
                {
                    "trandt": {
                        "label": "交易日期",
                        "control": "dateTimePicker"
                    },
                    "lncfno": {
                        "label": "借据号",
                        "control": "text"
                    }
                }
            ]
        },
        "toolbar": {
            "btns": []
        },
        "datagrid": {
            "isInitSearch": false,
            "scope": "searchTable",
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "qrlnkb"
                }
            },
            "pagination": {
                "show": true
            },
            "thead": [
                {
                    "label": "外部交易日期",
                    "prop": "trandt"
                },
                {
                    "label": "贷款借据号",
                    "prop": "lncfno"
                },
                {
                    "label": "渠道代码",
                    "prop": "servtp"
                },
                {
                    "label": "外部交易码",
                    "prop": "prcsco"
                },
                {
                    "label": "对账日期",
                    "prop": "chckdt"
                },
                {
                    "label": "对账流水",
                    "prop": "chcksq"
                },
                {
                    "label": "账号",
                    "prop": "acctno"
                },
                {
                    "label": "对方账号",
                    "prop": "toacct"
                },
                {
                    "label": "交易金额",
                    "prop": "tranam"
                },
                {
                    "label": "账户余额",
                    "prop": "balanc"
                },
                {
                    "label": "可用余额",
                    "prop": "balabl"
                },
                {
                    "label": "错误码",
                    "prop": "errod"
                },
                {
                    "label": "描述",
                    "prop": "desc"
                }
            ]
        }
    }
}