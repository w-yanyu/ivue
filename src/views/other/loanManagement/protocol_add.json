{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "post",
                "params": {
                  "servicecode": "AS_ICMS_PD_HTXXNR",
                  "trantp":"INSERT"
                }
              },
              "btns":[
				{
					"type":"submit",
					"label":"提交"
				}
			],
            "controls": [
                {
                    "htmctc": {
                        "label": "合同名称",
                        "control": "text",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入合同名称"
                            }
                        ]
                    },
                    "htlxtc": {
                        "control": "select",
                        "label": "合同类型",
                        "dict": {
                            "dictType": "E_HTLXTC",
                            "dictKey": [
                                "E_HTLXTC"
                            ],
                            "format": "label"
                        },
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择合同类型"
                            }
                        ]
                    }
                },
                {
                    "htfltc": {
                        "control": "select",
                        "label": "合同状态",
                        "dict": {
                            "dictType": "E_HTFLTC",
                            "dictKey": [
                                "E_HTFLTC"
                            ],
                            "format": "label"
                        },
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择合同状态"
                            }
                        ]
                    }
                },
                {
                    "wbnrtc": {
                        "initShow": true,
                        "label": "协议内容",
                        "control": "mce",
                        "span": 24,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入模板内容"
                            }
                        ]
                    }
                }
            ]
        }
    }
}