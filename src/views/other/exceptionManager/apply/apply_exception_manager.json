{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "scope": "formData",
            "controls": [
                {
                    "idtfno": {
                        "control": "text",
                        "label": "客户证件号码",
                        "name": "idtfno"
                    },
                    "prodna": {
                        "control": "text",
                        "label": "产品名称",
                        "name": "prodna"
                    }
                },
                {
                    "applid": {
                        "control": "hidden",
                        "label": "申请编号",
                        "name": "applid"
                    }
                }
            ]
        },
        "datagrid": {
            "isInitSearch": false,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "AS_ICMS_PB_QRCLEX"
                }
            },
            "pagination": {
                "show": true
            },
            "thead": [
                {
                    "type": "index",
                    "align": "center"
                },
                {
                    "label": "申请编号",
                    "prop": "applid",
                    "align": "center"
                },
                {
                    "label": "授信产品编号",
                    "prop": "prodcd",
                    "align": "center"
                },
                {
                    "label": "授信产品名称",
                    "prop": "prodna",
                    "align": "center"
                },
                {
                    "label": "客户名称",
                    "prop": "custna",
                    "align": "center"
                },
                {
                    "label": "证件类型",
                    "prop": "idtype",
                    "align": "center"
                },
                {
                    "label": "证件号码",
                    "prop": "idtfno",
                    "align": "center"
                },
                {
                    "label": "申请时间",
                    "prop": "appldt",
                    "align": "center"
                },
                {
                    "label": "申请环节",
                    "prop": "aplink",
                    "align": "center"
                },
                {
                    "label": "操作",
                    "fixed": "right",
                    "align": "center",
                    "btns": [
                        {
                            "type": "edit",
                            "label": "处理",
                            "toRequest": {
                                "url": "/views/other/exceptionManager/apply/apply_exception_process.json",
                                "method": "POST",
                                "params": {
                                    "servicecode": "AS_ICMS_BPM_COMTSK"
                                }
                            }
                        }
                    ]
                }
            ]
        }
    }
}