{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "AS_ICMS_PB_QRQNOD",
                    "remosq": "${{params.remosq}}",
                    "gmt_create": "${{params.gmt_create}}"
                }
            },
            "btns": [],
            "controls": [
                {
                    "channelNbr": {
                        "span": "10",
                        "control": "string",
                        "label": "渠道号"
                    },
                    "respStatus": {
                        "span": "10",
                        "control": "string",
                        "label": "返回状态"
                    }
                },
                {
                    "respCode": {
                        "span": "10",
                        "control": "string",
                        "label": "返回状态码"
                    },
                    "respMessage": {
                        "span": "10",
                        "control": "string",
                        "label": "返回信息"
                    }
                },
                {
                    "overallTransNbr": {
                        "span": "10",
                        "control": "string",
                        "label": "支付平台流水号"
                    },
                    "transDate": {
                        "span": "10",
                        "control": "string",
                        "label": "交易时间"
                    }
                },
                {
                    "oriRtxnStatus": {
                        "span": "10",
                        "control": "string",
                        "label": "原交易状态"
                    },
                    "oriRtxnMessage": {
                        "span": "10",
                        "control": "string",
                        "label": "原交易错误信息"
                    }
                }
            ]
        }
    }
}