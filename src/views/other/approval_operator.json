{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "controls": [
                {
                    "opridd": {
                        "control": "text",
                        "label": "操作员编号",
                        "rules": [
                            {
                                "max": 10,
                                "message": "长度不能超过10个字符"
                            }
                        ]
                    },
                    "oprnam": {
                        "control": "text",
                        "label": "操作员姓名",
                        "rules": [
                            {
                                "max": 100,
                                "message": "长度不能超过100个字符"
                            }
                        ]
                    }
                }
            ]
        },      
        "toolbar": {
            "btns": [
                {
                    "type": "add",
                    "target": "dialog",
                    "label": "新增",
                    "toRequest": {
                        "method": "GET",
                        "url": "/views/other/approval_operator_add.json"
                    },
                    "props": {
                        "title": "审批角色新增"
                    }
                }
            ]
        },
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": false,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "AS_ICMS_CM_SEWFOP",
                    "page_id": "/views/other/approval_operator.json"
                }
            },
            "thead": [               
                {
                    "label": "操作员编号",
                    "prop": "opridd"
                },
                {
                    "label": "操作员姓名",
                    "prop": "oprnam"
                },
                {
                    "label": "所属机构",
                    "prop": "orgnam"
                },
                {
                    "label": "操作",
                    "fixed": "right",
                    "width": 200,
                    "align":"center",
                    "btns": [                       
                        {
                            "type": "custom",
                            "label": "删除",
                            "doRequest": {
                                "url": "/SUMP/icmcall/icmRPCCall",
                                "method": "POST",
                                "params":{
                                    "servicecode":"AS_ICMS_CM_DEWFOP",
                                    "jiaoyzht":"${{form.wfrlid}}"
                                }
                            },
                            "confirm": {
                                "title": "确定删除",
                                "confirmButtonText": "确定",
                                "cancelButtonText": "取消"
                            }
                        }
                    ]
                }
            ]
        }
    }
}