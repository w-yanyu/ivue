{
    "module": "v-form-editableDataGrid-btn",
    "layout": {
        "form": {
            "controls": [
                {
                    "dataid": {
                        "control": "select",
                        "label": "数据类型",
                        "placeholder": " ",
                        "dict": {
                            "dictType": "E_FILETP",
                            "format": "label",
                            "dictKey": [
                                "E_FILETP"
                            ]
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择数据类型"
                            }
                        ]
                    },
                    "busseq": {
                        "control": "text",
                        "label": "业务流水",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入业务流水"
                            }
                        ]
                    }
                },
                {
                    "acctdt": {
                        "control": "dateTimePicker",
                        "label": "业务日期",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入业务日期"
                            }
                        ]
                    }
                },
                {
                    "source": {
                        "control": "hidden",
                        "label": "来源系统",
                        "value": "BMS"
                    },
                    "target": {
                        "control": "hidden",
                        "label": "目标系统",
                        "value": "ICMS"
                    }
                }
            ]
        },
        "toolbar": {
            "btns": [
                {
                    "type": "add",
                    "label": "新增",
                    "icon": "el-icon-plus"
                }
            ]
        },
        "datagrid": {
            "pagination": {
                "show": false
            },
            "thead": [
                {
                    "type": "index",
                    "label": "序号"
                },
                {
                    "prop": "filenm",
                    "type": "text",
                    "label": "文件名"
                },
                {
                    "prop": "flpath",
                    "type": "text",
                    "label": "文件路径"
                },
                {
                    "prop": "params",
                    "type": "text",
                    "label": "附加参数"
                },
                {
                    "label": "操作",
                    "width": "280px",
                    "btns": [
                        {
                            "type": "delete",
                            "label": "删除",
                            "icon": "el-icon-close"
                        }
                    ]
                }
            ]
        },
        "footer": {
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "smtbat"
                        }
                    }
                },
                {
                    "type": "reset",
                    "label": "重置",
                    "icon": ""
                  }
            ]
        }
    }
}