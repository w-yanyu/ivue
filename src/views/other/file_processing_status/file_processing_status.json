{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "controls": [
                {
                    "branch_id": {
                        "control": "dateTimePicker",
                        "label": "交易日期"
                    },
                    "branch_name": {
                        "control": "text",
                        "label": "批量批次号",
                        "rules": [
                            {
                                "max": 100,
                                "message": "长度不能超过100个字符"
                            }
                        ]
                    }
                },
                {
                    "branch_function_class": {
                        "control": "text",
                        "label": "交易流水号"
                    },
                    "real_branch_ind": {
                        "control": "select",
                        "label": "发送对象",
                        "dict": {
                            "dictType": "E_SENDDX",
                            "dictKey": [
                                "E_SENDDX"
                            ],
                            "format": "label"
                        }
                    }
                },
                {
                    "real_branch_ind": {
                        "control": "select",
                        "label": "文件状态",
                        "dict": {
                            "dictType": "E_FILEZT",
                            "dictKey": [
                                "E_FILEZT"
                            ],
                            "format": "label"
                        }
                    },
                    "文件类型": {
                        "control": "select",
                        "label": "文件类型",
                        "dict": {
                            "dictType": "E_FILELX",
                            "dictKey": [
                                "E_FILELX"
                            ],
                            "format": "label"
                        }
                    }
                }
            ]
        },
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": false,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "AS_ICMS_JK_QRRECD",
                    "page_id": "/views/other/file_processing_status/file_processing_status.json"
                }
            },
            "thead": [
                {
                    "label": "选择",
                    "prop": "pljypich"
                },
                {
                    "label": "交易日期",
                    "prop": "exdate"
                },
                {
                    "label": "批量批次号",
                    "prop": "pljypich"
                },
                {
                    "label": "交易流水号",
                    "prop": "exresq"
                },
                {
                    "label": "交易处理状态",
                    "dict": {
                        "dictType": "E_WJCLZT",
                        "dictKey": [
                            "E_WJCLZT"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "是否已下载",
                    "prop": "isdown",
                    "dict": {
                        "dictType": "E_YES___",
                        "dictKey": [
                            "E_YES___"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "文件批次号",
                    "prop": "pichao"
                },
                {
                    "label": "发送对象",
                    "prop": "senddx",
                    "dict": {
                        "dictType": "E_SENDDX",
                        "dictKey": [
                            "E_SENDDX"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "文件名",
                    "prop": "filena"
                },
                {
                    "label": "文件状态",
                    "prop": "filezt",
                    "dict": {
                        "dictType": "E_FILEZT",
                        "dictKey": [
                            "E_FILEZT"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "文件类型",
                    "prop": "filetp",
                    "width": 150,
                    "dict": {
                        "dictType": "E_FILELX",
                        "dictKey": [
                            "E_FILELX"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "文件生成日期",
                    "prop": "fuwbiaoz"
                },
                {
                    "label": "批次编号",
                    "prop": "jyksshij"
                },
                {
                    "label": "文件处理归属",
                    "prop": "jyjsshij"
                },
                {
                    "label": "操作",
                    "fixed": "right",
                    "width": 100,
                    "align":"center",
                    "btns": [
                        {
                            "type": "edit",
                            "label": "修改",
                            "toRequest": {
                                "url": "/views/other/file_processing_status/file_processing_statu_edit.json",
                                "method": "POST"
                            },
                            "props": {
                                "width": "70%",
                                "title": "文件状态修改"
                            }
                        }
                    ]
                }
            ]
        }
    }
}