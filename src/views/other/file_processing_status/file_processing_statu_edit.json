{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": "",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "AS_ICMS_JK_UPRECD"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "exdate": {
                        "control": "string",
                        "label": "交易日期",
                        "width": "160px",
                        "rules": [
                            {
                                "max": 10,
                                "message": "长度不能超过10个字符"
                            }
                        ]
                    },
                    "pljypich": {
                        "control": "text",
                        "label": "批量批次号",
                        "width": "160px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入批量批次号"
                            },
                            {
                                "max": 100,
                                "message": "长度不能超过100个字符"
                            }
                        ]
                    }
                },
                {
                    "exresq": {
                        "control": "text",
                        "label": "交易流水号",
                        "width": "160px",
                        "rules": [
                            {
                                "max": 200,
                                "message": "长度不能超过200个字符"
                            }
                        ]
                    },
                    "wjclzt": {
                        "control": "select",
                        "label": "交易处理状态",
                        "width": "160px",
                        "rules": [
                            {
                                "max": 32,
                                "message": "长度不能超过32个字符"
                            }
                        ],
                        "dict": {
                            "dictType": "E_WJCLZT",
                            "dictKey": [
                                "E_WJCLZT"
                            ],
                            "format": "label"
                        }
                    }
                },
                {
                    "isdown": {
                        "control": "select",
                        "label": "是否已下载",
                        "width": "160px",
                        "rules": [
                            {
                                "max": 32,
                                "message": "长度不能超过32个字符"
                            }
                        ],
                        "dict": {
                            "dictType": "E_YES___",
                            "dictKey": [
                                "E_YES___"
                            ],
                            "format": "label"
                        }
                    },
                    "pichao": {
                        "control": "text",
                        "label": "文件批次号",
                        "width": "160px",
                        "rules": [
                            {
                                "max": 10,
                                "message": "长度不能超过10个字符"
                            }
                        ]
                    }
                },
                {
                    "senddx": {
                        "control": "select",
                        "label": "发送对象",
                        "width": "160px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入实体机构标志"
                            }
                        ],
                        "dict": {
                            "dictType": "E_SENDDX",
                            "dictKey": [
                                "E_SENDDX"
                            ],
                            "format": "label"
                        }
                    },
                    "filena": {
                        "control": "text",
                        "label": "文件名",
                        "width": "160px",
                        "rules": [
                            {
                                "max": 100,
                                "message": "长度不能超过100个字符"
                            }
                        ]
                    }
                },
                {
                    "filezt": {
                        "control": "select",
                        "label": "文件状态",
                        "width": "160px",
                        "rules": [
                            {
                                "max": 32,
                                "message": "长度不能超过32个字符"
                            }
                        ],
                        "dict": {
                            "dictType": "E_FILEZT",
                            "dictKey": [
                                "E_FILEZT"
                            ],
                            "format": "label"
                        }
                    },
                    "filetp": {
                        "control": "select",
                        "label": "文件类型",
                        "width": "160px",
                        "rules": [
                            {
                                "max": 32,
                                "message": "长度不能超过32个字符"
                            }
                        ],
                        "dict": {
                            "dictType": "E_FILELX",
                            "dictKey": [
                                "E_FILELX"
                            ],
                            "format": "label"
                        }
                    }
                },
                {
                    "mkdate": {
                        "control": "dateTimePicker",
                        "label": "文件生成日期",
                        "rules": [
                            {
                                "max": 8,
                                "message": "长度不能超过8个字符"
                            }
                        ],
                        "width": "160px"
                    },
                    "picino": {
                        "control": "text",
                        "label": "批次编号",
                        "width": "160px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入批次编号"
                            },
                            {
                                "max": 32,
                                "message": "长度不能超过32个字符"
                            }
                        ]
                    }
                },
                {
                    "wjclgs": {
                        "control": "text",
                        "label": "文件处理归属",
                        "width": "160px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入节假日代码"
                            },
                            {
                                "max": 8,
                                "message": "长度不能超过8个字符"
                            }
                        ]
                    }
                }
            ]
        }
    }
}