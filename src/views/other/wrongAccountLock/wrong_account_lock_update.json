{
    "module": "v-inputDataForm-btn",
    "layout": {
        "form": {
            "scope": "formData",
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "uplnwl"
                }
            },
            "controls": [
                {
                    "trandt": {
                        "control": "text",
                        "label": "交易日期",
                        "disabled": "true",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入交易日期"
                            }
                        ]
                    },
                    "lncfno": {
                        "control": "text",
                        "label": "借据号",
                        "disabled": "true",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入借据号"
                            }
                        ]
                    }
                },
                {
                    "lockst": {
                        "control": "select",
                        "label": "锁定状态",
                        "dict": {
                            "dictType": "E_YES___",
                            "dictKey": [
                                "E_YES___"
                            ],
                            "format": "label"
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入锁定状态"
                            }
                        ]
                    },
                    "transq": {
                        "control": "text",
                        "label": "锁定交易流水",
                        "disabled": "true",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入锁定交易流水"
                            }
                        ]
                    }
                }
            ]
        }
    }
}