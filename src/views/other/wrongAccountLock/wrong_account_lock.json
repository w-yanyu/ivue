{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "controls": [
                {
                    "trandt": {
                        "label": "交易日期",
                        "control": "dateTimePicker"
                    },
                    "lncfno": {
                        "label": "借据号",
                        "control": "text"
                    }
                },
                {
                    "lockst": {
                        "label": "锁定状态",
                        "control": "select",
                        "dict": {
                            "dictType": "E_YES___",
                            "format": "label",
                            "dictKey": [
                                "E_YES___"
                            ]
                        }
                    },
                    "pljioyma": {
                        "label": "批量交易码",
                        "control": "text"
                    }
                }
            ]
        },
        "toolbar": {
            "btns": []
        },
        "datagrid": {
            "isInitSearch": false,
            "scope": "searchTable",
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "qrlnwl"
                }
            },
            "pagination": {
                "show": true
            },
            "thead": [
                {
                    "label": "trandt",
                    "prop": "交易日期"
                },
                {
                    "label": "贷款借据号",
                    "prop": "lncfno"
                },
                {
                    "label": "锁定交易流水",
                    "prop": "transq"
                },
                {
                    "label": "锁定时间",
                    "prop": "lockdt"
                },
                {
                    "label": "解锁时间",
                    "prop": "unlkdt"
                },
                {
                    "label": "解锁交易流水",
                    "prop": "unlksq"
                },
                {
                    "label": "锁定状态",
                    "prop": "lockst",
                    "dict": {
                        "dictType": "E_YES___",
                        "dictKey": [
                            "E_YES___"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "批量交易组标识号",
                    "prop": "pljyzbsh"
                },
                {
                    "label": "批量交易码",
                    "prop": "pljioyma"
                },
                {
                    "label": "操作",
                    "btns": [
                        {
                            "type": "edit",
                            "label": "错锁更新",
                            "toRequest": {
                                "url": "/views/other/wrongAccountLock/wrong_account_lock_update.json",
                                "method": "get"
                            }
                        }
                    ]
                }
            ]
        }
    }
}