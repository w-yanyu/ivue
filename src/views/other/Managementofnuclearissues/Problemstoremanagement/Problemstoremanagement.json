{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "controls": [
                {
                    "ruleid": {
                        "label": "问题名称",
                        "control": "text",
                        "rules": [
                            {
                                "max": "5",
                                "message": "长度不能超过5个字符"
                            }
                        ]
                    },
                    "rudesc": {
                        "label": "创建机构",
                        "control": "text",
                        "rules": [
                            {
                                "max": "30",
                                "message": "长度不能超过30个字符"
                            }
                        ]
                    }
                }
            ]
        },
        "toolbar": {
            "btns": [
                {
                    "type": "add",
                    "target": "dialog",
                    "label": "新增",
                    "toRequest": {
                        "method": "GET",
                        "url": "/views/other/Managementofnuclearissues/Problemstoremanagement/addprobleminfo.json"
                    },
                    "props": {
                        "title": "问题信息新增"
                    }
                }
            ]
        },
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": false,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "recurd",
                    "method":"queryAll"
                }
            },
            "thead": [
                {
                    "type": "index"
                },
                {
                    "prop": "ruleid",
                    "label": "选择",
                    "align": "center"
                },
                {
                    "prop": "rudesc",
                    "label": "问题名称",
                    "align": "center"
                },
                {
                    "prop": "rudate",
                    "label": "问题类型",
                    "align": "center"
                },
                {
                    "prop": "freque",
                    "dict": {
                        "format": "label",
                        "dictKey": [
                            "E_FREQUE"
                        ],
                        "dictType": "E_FREQUE"
                    },
                    "label": "人工答案",
                    "align": "center"
                },
                {
                    "prop": "fachio",
                    "dict": {
                        "format": "label",
                        "dictKey": [
                            "E_FACHIO"
                        ],
                        "dictType": "E_FACHIO"
                    },
                    "label": "元数据名称",
                    "align": "center"
                },
                {
                    "prop": "flowpa",
                    "dict": {
                        "format": "label",
                        "dictKey": [
                            "MS_E_YESORNO"
                        ],
                        "dictType": "MS_E_YESORNO"
                    },
                    "label": "创建机构",
                    "align": "center"
                },
                {
                    "prop": "pronumber",
                    "label": "是否有效",
                    "align": "center"
                },
                {
                    "prop": "pronumber",
                    "label": "备注",
                    "align": "center"
                },
                {
                    "prop": "pronumber",
                    "label": "创建员",
                    "align": "center"
                },
                {
                    "prop": "pronumber",
                    "label": "创建时间",
                    "align": "center"
                },
                {
                    "prop": "pronumber",
                    "label": "修改员",
                    "align": "center"
                },
                {
                    "prop": "pronumber",
                    "label": "修改时间",
                    "align": "center"
                },
                {
                    "label": "",
                    "fixed": "right",
                    "width": "200",
                    "align": "center",
                    "btns": [
                   
                        
                           
                        
                    ]
                }
            ]
        }
