{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": "",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "recurd",
                            "method":"add"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "ruleid": {
                        "label": "问题名称",
                        "control": "text",
                        "rules": [
                            {
                                "max": "5",
                                "message": "长度不能超过5个字符"
                            }
                        ]
                    },
                    "rudesc": {
                        "label": "问题类型",
                        "control": "text",
                        "rules": [
                            {
                                "max": "30",
                                "message": "长度不能超过30个字符"
                            }
                        ]
                    }
                },
                {
                    "rudate": {
                        "label": "元数据名称",
                        "control": "dateTimePicker",
                        "rules": [
                            {
                                "max": "10",
                                "message": "长度不能超过10个字符"
                            }
                        ]
                    },
                    "freque": {
                        "label": "人工答案",
                        "control": "select",
                        "dict": {
                            "format": "label",
                        
                            "dictType": "E_FREQUE"
                        }
                    }
                },
                {
                    "fachio": {
                        "label": "是否有效",
                        "control": "select",
                        "dict": {
                            "format": "label",
                            "dictKey": [
                                "E_FACHIO"
                            ],
                            "dictType": "E_FACHIO"
                        }
                    },
                    "flowpa": {
                        "label": "备注",
                        "control": "select",
                        "dict": {
                            "format": "label",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "dictType": "MS_E_YESORNO"
                        },
                        "events": {
                            "condition": {
                                "Y": {
                                    "control": {
                                        "pronumber": {
                                            "value": "KGRD_AU_2"
                                        }
                                    }
                                },
                                "N": {
                                    "control": {
                                        "pronumber": {
                                            "value": ""
                                        }
                                    }
                                }
                            } 
                        }
                    }
                }
               
                
            ]
        }
    }
}
