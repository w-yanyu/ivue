{
    "module": "v-realonlyForm-btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "controls": [
                {
                    "acctno": {
                        "control": "string",
                        "label": "账号",
                        "width": "160px",
                        "rules": [
                            {
                                "max": 20,
                                "message": "长度不能超过10个字符"
                            }
                        ]
                    }
                }
            ]
        },
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": false,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "010INTS2010",
                    "page_id": "/views/other/internal_transaction_details_query.json"
                }
            },
            "thead": [               
                {
                    "label": "交易日期",
                    "prop": "trandt"
                },
                {
                    "label": "币种",
                    "prop": "crcycd"
                },
                {
                    "label": "记账类型",
                    "prop": "acuttp"
                },
                {
                    "label": "借贷标志",
                    "prop": "amntcd"
                },
                {
                    "label": "交易金额",
                    "prop": "tranam"
                },
                {
                    "label": "本次余额",
                    "prop": "tranbl"
                }               
                ,
                {
                    "label": "对方账号",
                    "prop": "toacct"
                },
                {
                    "label": "核算机构",
                    "prop": "brchno"
                },
                {
                    "label": "时间戳",
                    "prop": "tmstmp"
                },
                {
                    "label": "交易流水",
                    "prop": "transq"
                },
                {
                    "label": "记账用户",
                    "prop": "tranus"
                } 
                ,
                {
                    "label": "复核用户",
                    "prop": "ckbsus"
                },
                {
                    "label": "授权用户",
                    "prop": "auttel"
                },
                {
                    "label": "事件说明",
                    "prop": "evtsig"
                }
            ]
        }
    }
}