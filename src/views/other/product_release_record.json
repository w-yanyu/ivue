{
    "module": "v-realonlyForm-btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "controls": [
                {
                    "crdtcd": {
                        "control": "string",
                        "label": "账号",
                        "width": "160px",
                        "rules": [
                            {
                                "max": 20,
                                "message": "长度不能超过10个字符"
                            }
                        ]
                    },"verson": {
                        "control": "string",
                        "label": "账号",
                        "width": "160px",
                        "rules": [
                            {
                                "max": 20,
                                "message": "长度不能超过10个字符"
                            }
                        ]
                    },
                    "sheltp": {
                        "control": "hide",                       
                        "value":"01"
                    }
                }
            ]
        },
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": false,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "AS_ICMS_PD_QRSHIN",
                    "page_id": "/views/other/product_release_record.json"
                }
            },
            "thead": [               
                {
                    "label": "授信产品编号",
                    "prop": "prodcd"
                },
                {
                    "label": "可售产品编号",
                    "prop": "crdtcd"
                },
                {
                    "label": "产品名称",
                    "prop": "prodna"
                },
                {
                    "label": "版本号",
                    "prop": "verson"
                },
                {
                    "label": "操作类型",
                    "prop": "sheltp"
                },
                {
                    "label": "返回结果",
                    "prop": "result"
                }  ,
                {
                    "label": "操作员",
                    "prop": "tranus"
                },
                {
                    "label": "操作时间",
                    "prop": "gmt_create"
                }            
            ]
        }
    }
}