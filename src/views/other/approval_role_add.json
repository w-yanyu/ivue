{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": "",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "AS_ICMS_CM_ADWFRL",
                            "page_id": "/views/other/approval_role_add.json"
                        }                       
                    }
                }
            ],
            "controls": [
                {
                    "wfrlnm": {
                        "control": "text",
                        "label": "审批角色名称",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入审批角色名称"
                            },
                            {
                                "max": 10,
                                "message": "长度不能超过10个字符"
                            }
                        ]
                    }, 
                    "rlauth": {
                        "control": "select",
                        "label": "审批层级",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择审批层级"
                            }
                        ],
                        "dict": {
                            "dictType": "E_RLAUTH",
                            "dictKey": [
                                "E_RLAUTH"
                            ],
                            "format": "label"
                        }
                    }                  
                },
                {
                    "remark": {
                        "control": "text",
                        "label": "备注",
                        "width": "130px",
                        "rules": [                           
                            {
                                "max": 100,
                                "message": "长度不能超过100个字符"
                            }
                        ]
                    }
                }
            ]
        }
    }
}