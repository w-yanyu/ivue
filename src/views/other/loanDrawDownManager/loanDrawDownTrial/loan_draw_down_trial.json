{
    "module": "v_form_btn_dynamic",
    "layout": {
        "form": {
            "scope": "formData",
            "formSetTitle": "放款试算",
            "formResultTitle": "试算结果",
            "align":"center",
            "controls": [
                {
                    "cardno": {
                        "span": "8",
                        "control": "text",
                        "label": "交易卡号",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入交易卡号"
                            }
                        ]
                    },
                    "prodcd": {
                        "span": "8",
                        "control": "text",
                        "label": "产品代码",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入产品代码"
                            }
                        ]
                    },
                    "tranam": {
                        "span": "8",
                        "control": "text",
                        "label": "交易金额",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入交易金额"
                            }
                        ]
                    }
                },
                {
                    "crcycd": {
                        "span": "8",
                        "control": "select",
                        "label": "币种",
                        "dict": {
                            "dictType": "E_CRCYCD_HS",
                            "format": "label",
                            "dictKey": [
                                "E_CRCYCD_HS"
                            ]
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择币种"
                            }
                        ]
                    },
                    "period": {
                        "span": "8",
                        "control": "text",
                        "label": "贷款期限",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入贷款期限"
                            }
                        ]
                    },
                    "schdtp": {
                        "span": "8",
                        "label": "还款方式",
                        "control": "select",
                        "dict": {
                            "dictType": "E_SCHDTP",
                            "format": "label",
                            "dictKey": [
                                "E_SCHDTP"
                            ]
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择还款方式"
                            }
                        ]
                    }
                },
                {
                    "irdyfg": {
                        "span": "8",
                        "label": "年/月利率标识",
                        "control": "select",
                        "dict": {
                            "dictType": "E_IRDYFG",
                            "format": "label",
                            "dictKey": [
                                "E_IRDYFG"
                            ]
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择年/月利率标识"
                            }
                        ]
                    },
                    "lnrtir": {
                        "span": "8",
                        "control": "text",
                        "label": "执行利率",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入执行利率"
                            }
                        ]
                    },
                    "repyfm": {
                        "span": "8",
                        "label": "还款周期",
                        "control": "text",
                        "name": "repyfm",
                        "#suffix": {
                            "title": "还款周期",
                            "width": 400,
                            "options": [
                                {
                                    "value": "Y",
                                    "label": "年"
                                },
                                {
                                    "value": "M",
                                    "label": "月"
                                },
                                {
                                    "value": "W",
                                    "label": "周"
                                },
                                {
                                    "value": "D",
                                    "label": "日"
                                }
                            ]
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入还款周期"
                            }
                        ]
                    }
                },
                {
                    "loandt": {
                        "span": "8",
                        "control": "dateTimePicker",
                        "label": "放款日期"
                    },
                    "matudt": {
                        "span": "8",
                        "control": "dateTimePicker",
                        "label": "到期日期"
                    }
                }
            ]
        },
        "toolbar": {
            "btns": [
                {
                    "type": "submit",
                    "label": "试算",
                    "unplain":true,
                    "icon": "",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "AS_ICMS_JK_HXFKSS"
                        }
                    },
                    "toRequest": {
                        "url": "/views/other/loanDrawDownManager/loanDrawDownTrial/loan_draw_down_trial_result_page.json",
                        "method": "POST"
                    }
                },
                {
                    "type": "reset",
                    "btnStyle": "success",
                    "icon": "",
                    "label": "重置"
                }
            ]
        }
    }
}
