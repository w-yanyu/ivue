{
    "module": "v-form-editableDataGrid-btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "controls": [
                {
                    "totamo": {
                        "control": "text",
                        "label": "还款总额"
                    }
                }
            ]
        },
        "datagrid": {
            "pagination": {
                "show": false
            },
            "thead": [
                {
                    "label": "借据号",
                    "prop": "lncfno",
                    "align": "center"
                },
                {
                    "label": "还款期数",
                    "prop": "termno",
                    "align": "center"
                },
                {
                    "label": "正常本金",
                    "prop": "lnnpbl",
                    "align": "center"
                },
                {
                    "label": "初始利息",
                    "prop": "initin",
                    "align": "center"
                },
                {
                    "label": "应收费用",
                    "prop": "ysxxfe",
                    "align": "center"
                },
                {
                    "label": "总金额",
                    "prop": "totalm",
                    "align": "center"
                },
                {
                    "label": "应还罚息",
                    "prop": "ysyjpi",
                    "align": "center"
                },
                {
                    "label": "是否逾期",
                    "prop": "overdu",
                    "align": "center"
                },
                {
                    "label": "应收欠息",
                    "prop": "ysxxoi",
                    "align": "center"
                },
                {
                    "label": "逾期本金",
                    "prop": "lnopbl",
                    "dict": {
                        "dictType": "E_YES___",
                        "dictKey": [
                            "E_YES___"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "放款日期",
                    "prop": "initdt",
                    "align": "center"
                }
            ]
        }
    }
}