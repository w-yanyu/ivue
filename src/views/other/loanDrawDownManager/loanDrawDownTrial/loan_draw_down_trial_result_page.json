{
    "module": "dynamicTabs",
    "layout": {
        "tabs": {
            "scope": "templateScope",
            "initDataOnce": true,
            "#initRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "post",
                "params": {
                    "servicecode": "AS_ICMS_JK_HXFKSS"
                }
            },
            "btns": [],
            "tabHeader": [
                {
                    "id": "baseInfo",
                    "label": "基础信息",
                    "closable": false,
                    "disabled": false,
                    "icon": "el-icon-date",
                    "toRequest": {
                        "url": "/views/other/loanDrawDownManager/loanDrawDownTrial/loan_draw_down_trial_result.json",
                        "method": "get"
                    }
                },
                {
                    "id": "plans",
                    "label": "还款计划列表",
                    "icon": "el-icon-date",
                    "closable": false,
                    "disabled": false,
                    "toRequest": {
                        "url": "/views/other/loanDrawDownManager/loanDrawDownTrial/repayment_plan_grid.json",
                        "method": "get"
                    }
                }
            ],
            "defaultActiveKey": "baseInfo",
            "position": "top",
            "styleClass": "card"
        }
    }
}