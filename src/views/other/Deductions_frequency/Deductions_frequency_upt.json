{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": "",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "CORELNTS9276",
                            "method": "edit"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "prodcd": {
                        "label": "产品代码",
                        "control": "text",
                        "width": "130px",
                        "disabled": true,
                        "rules": [
                            {
                                "max": "18",
                                "message": "长度不能超过18个字符"
                            },
                            {
                                "required": true,
                                "message": "请输入产品代码"
                            }
                        ]
                    },
                    "decatp": {
                        "label": "扣款卡类别",
                        "width": "130px",
                        "control": "select",
                        "dict": {
                            "dictType": "E_DECATP",
                            "dictKey": [
                                "E_DECATP"
                            ],
                            "format": "label"
                        }
                    }
                },
                {
                    "delost": {
                        "label": "扣款借据状态",
                        "control": "select",
                        "width": "130px",
                        "dict": {
                            "dictType": "E_DELOST",
                            "dictKey": [
                                "E_DELOST"
                            ],
                            "format": "label"
                        }
                    },
                    "defreq": {
                        "label": "扣款频率类型",
                        "control": "select",
                        "width": "130px",
                        "dict": {
                            "dictType": "E_DEFREQ",
                            "dictKey": [
                                "E_DEFREQ"
                            ],
                            "format": "label"
                        }
                    }
                },
                {
                    "fixday": {
                        "label": "固定日",
                        "control": "text",
                        "width": "130px",
                        "rules": [
                            {
                                "max": "128",
                                "message": "长度不能超过128个字符"
                            }
                        ]
                    }
                }
            ]
        }
    }
}