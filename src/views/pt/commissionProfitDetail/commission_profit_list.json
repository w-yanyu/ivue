{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls":
      [
        {
          "sale_prod_cd": {
            "label": "可售产品编号",
            "edit":true,
            "control": "text",
            "valueField": "sale_prod_cd"
          },
          "part_no": {
            "control": "text",
            "label": "合作方编号",
            "valueField": "part_no"
          }
        }
      ]
    },
    "datagrid": {
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "pt1001"
        }
      },
      "thead": [
        {
          "type": "index",
          "label": "序号"
        },
        {
          "label": "合作方名称",
          "prop": "center_name",
          "align": "center"
        },
        {
          "label": "合作方编号",
          "prop": "part_no",
          "align": "center"
        },
        {
          "label":"可售产品编号",
          "prop": "sale_prod_name",
          "align": "center"
        },
        {
          "label":"可售产品编号",
          "prop": "sale_prod_cd",
          "align": "center"
        },
        {
          "label":  "结算日期",
          "prop": "billing_date",
          "align": "center"
        },
        {
          "label":  "分润/分佣金额",
          "prop": "profit_amt",
          "align": "center",
          "format": {
            "thousand": ","
          }
        },
        {
          "label": "分润基准",
          "prop": "profit_baseline",
          "dict": {
            "dictType": "PT_E_PROFIT_BASELINE",
            "dictKey": ["PT_E_PROFIT_BASELINE"],
            "format": "label",
            "align": "center"
          }

        }
      ]
    }
  }
}
