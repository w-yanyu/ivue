{
    "module": "d-steps",
    "layout": {
        "steps": {
            "direction": "horizontal",
            "alignCenter": true,
            "finishStatus": "success",
            "simpleType": false,
            "nextStepType": "validate",
            "skip": true,
            "list": [
                {
                    "title": "基本信息展示",
                    "icon": "el-icon-edit",
                  "toRequest": {
                    "url": "/views/pt/pt_consult_uphold/pt_consult_uphole_updatatab1.json",
                    "method": "post",
                    "params": {
                      "part_protocol_no": "${{params.part_protocol_no}}"
                      
                    }
                  },
                  "doRequest": {
                    "url": "/SUMP/icmcall/icmRPCCall",
                    "method": "POST",
                    "params": {
                      "servicecode": "pt0003",
                      "part_protocol_no": "${{params.part_protocol_no}}"
                    }
                  }
                },

                {
                    "title": "敞口配置",
                    "icon": "el-icon-edit",
                  "toRequest": {
                    "url": "/views/pt/pt_consult_uphold/pt_consult_uphole_openconfig.json",
                    "params": {
                      "part_protocol_no": "${{params.part_protocol_no}}",
                      "part_project_no": "${{params.part_project_no}}"

                    }
                  },
                  "doRequest": {
                    "url": "/SUMP/icmcall/icmRPCCall",
                    "method": "POST",
                    "params": {
                      "servicecode": "pt0010",
                      "part_protocol_no": "${{params.part_protocol_no}}"
                    }
                  }
                },


                {
                    "title": "分佣分润协议",
                    "icon": "el-icon-edit",
                  "toRequest": {
                    "url": "/views/pt/pt_consult_uphold/pt_consult_uphole_updfenrun.json",
                    "params": {
                      "part_protocol_no": "${{params.part_protocol_no}}",
                      "part_no": "${{params.part_no}}"


                    }
                  },
                  "doRequest": {
                    "url": "/SUMP/icmcall/icmRPCCall",
                    "method": "POST",
                    "params": {
                      "servicecode": "pt0007",
                      "part_protocol_no": "${{params.part_protocol_no}}"
                    }
                  }
                },
                {
                    "title": "引流协议",
                    "icon": "el-icon-edit",
                  "toRequest": {
                    "url": "/views/pt/pt_consult_uphold/pt_consult_uphole_upddrainage.json",
                    "params": {
                      "part_protocol_no": "${{params.part_protocol_no}}"
                    }
                  },
                  "doRequest": {
                    "url": "/SUMP/icmcall/icmRPCCall",
                    "method": "POST",
                    "params": {
                      "servicecode": "pt0102",
                      "part_protocol_no": "${{params.part_protocol_no}}"
                    }
                  }
                },
                {
                    "title": "代偿协议",
                    "icon": "el-icon-edit",
                  "toRequest": {
                    "url": "/views/pt/pt_consult_uphold/pt_consult_uphole_updcompensatory.json",
                    "params": {
                      "part_protocol_no": "${{params.part_protocol_no}}",
                      "part_no": "${{params.part_no}}"

                    }
                  },
                  "doRequest": {
                    "url": "/SUMP/icmcall/icmRPCCall",
                    "method": "POST",
                    "params": {
                      "servicecode": "pt0212",
                      "compensate_rule_no": "${{params.part_protocol_no}}"
                    }
                  }
                },
                {
                    "title": "贴息协议",
                    "icon": "el-icon-edit",
                  "toRequest": {
                    "url": "/views/pt/pt_consult_uphold/pt_consult_uphole_updinst.json",
                    "params": {
                      "part_protocol_no": "${{params.part_protocol_no}}",
                      "part_no": "${{params.part_no}}"

                    }
                  },
                  "doRequest": {
                    "url": "/SUMP/icmcall/icmRPCCall",
                    "method": "POST",
                    "params": {
                      "servicecode": "pt0112",
                      "part_protocol_no": "${{params.part_protocol_no}}"
                    }
                  }
                }, {
                "title": "协议文件",
                "icon": "el-icon-edit",
                "toRequest": {
                  "url": "@DocumentAgreementupd",
                  "params": {
                    "part_protocol_no": "${{params.part_protocol_no}}",
                    "part_no": "${{params.part_no}}",
                    "partner_name": "${{params.partner_name}}"
                  }
                }
              }
            ]
        }
    }
}
