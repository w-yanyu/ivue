{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "scope": "templateScope",
      "initDataOnce":true,
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "qrcosz",
          "seqnum": "${{params.seqnum}}"
        }
      },

      "btns":[
        {
          "type": "submit",
          "label": "保存",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "upcosc",
              "seqnum": "${{params.seqnum}}"
            }
          }
        }
      ],

      "controls": [
        {
          "partsz": {
            "control": "text",
            "label": "合作方规模金额(元)",
            "width": "50%",
            "rules": [
              {
                "required": true,
                "message": "合作方规模金额(元)"
              },
              {
                "max": 100,
                "message": "长度不能超过10个字符"
              }
            ]
          },
          "guartp": {
            "control": "select",
            "label": "担保类型",
            "width": "50%",

            "rules": [
              {
                "required": true,
                "message": "担保类型"
              }
            ],
            "dict": {
              "dictType": "E_GUARTY",
              "dictKey": [
                "E_GUARTY"
              ],
              "format": "label"
            }
          }

        },
        {
          "guarpr": {
            "control": "text",
            "label": "担保比例(%)",
            "width": "50%",
            "rules": [
              {
                "required": true,
                "message": "担保比例(%)"
              },
              {
                "max": 10,
                "message": "长度不能超过10个字符"
              }
            ]
          },
          "islimt": {
            "control": "select",
            "label": "是否允许超额",
            "width": "50%",
            "rules": [
              {
                "required": true,
                "message": "是否允许超额"
              },
              {
                "max": 100,
                "message": "长度不能超过10个字符"
              }
            ],
            "dict": {
              "dictType": "E_BTFLAG",
              "dictKey": [
                "E_BTFLAG"
              ],
              "format": "label"
            }
          }
        },
        {
          "exprop": {
            "control": "text",
            "label": "超额比例(%)",
            "width": "50%",
            "rules": [
              {
                "required": true,
                "message": "超额比例(%)不得为空"
              }
            ]
          },
          "datmax": {
            "control": "text",
            "label": "当日最大放款金额",
            "width": "50%",
            "rules": [
              {
                "required": true,
                "message": "当日最大放款金额"
              }

            ]
          }
        },
        {
          "maxnum": {
            "control": "text",

            "label": "当日最大放款笔数",
            "width": "50%",
            "rules": [
              {
                "required": true,
                "message": "当日最大放款笔数"
              },
              {
                "max": 100,
                "message": "长度不能超过100个字符"
              }
            ]
          },
          "sinmax": {
            "control": "text",
            "label": "单笔最大放款金额",
            "width": "50%",
            "rules": [
              {
                "required": true,
                "message": "单笔最大放款金额"
              }
            ]
          }
        },
        {
          "sinmin": {
            "control": "text",
            "label": "单笔最小放款金额",
            "width": "50%",
            "rules": [
              {
                "required": true,
                "message": "单笔最小放款金额："
              }
            ]
          }

        }


      ]
    }
  }
}
