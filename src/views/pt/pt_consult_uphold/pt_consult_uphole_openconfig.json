{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initDataOnce":true,
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pt0008",
          "part_protocol_no": "${{params.part_protocol_no}}"
        }
      },
      "btns": [
        {
          "type": "submit",
          "label": "保存",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "pt0010",
              "part_project_no": "${{params.part_project_no}}"
            }
          }
        }
      ],
      "controlsGroup": [
        {
          "title": "基本信息",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "expand": true,
          "controls": [
            {
              "part_protocol_no": {
                "control": "text",
                "label": "合作方协议编号",

                "span": 20,
                "disabled": "true",
                "value": "${{params.part_protocol_no}}",
                "rules": [
                  {
                    "required": true,
                    "message": "合作方协议编号不得为空"
                  }
                ]
              }

            }

          ]
        },
        {
          "title": "授信管控",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "expand": true,
          "controls": [

            {
              "daily_credit_check": {
                "control": "select",
                "label": "当日授信检查",
                "width": "120px",
                "span": 12,
                "dict": {
                  "format": "label",
                  "dictKey": [
                      "MS_E_YESORNO"
                  ],
                  "dictType": "MS_E_YESORNO"
                }
              },
              "daily_credit_line": {
                "control": "text",
                "label": "当日授信额度",
                "width": "120px",
                "span": 12,
                "disabled": "false"
              }

            },
            {
              "monthly_credit_check": {
                "control": "select",
                "label": "当月授信检查",
                "width": "120px",
                "span": 12,
                "dict": {
                  "format": "label",
                  "dictKey": [
                      "MS_E_YESORNO"
                  ],
                  "dictType": "MS_E_YESORNO"
                }
              },
              "monthly_credit_line": {
                "control": "text",
                "label": "当月授信额度",
                "width": "120px",
                "span": 12,
                "disabled": "false"
              }

            },
            {
              "yearly_credit_check": {
                "control": "select",
                "label": "当年授信检查",
                "width": "120px",
                "span": 12,
                "dict": {
                  "format": "label",
                  "dictKey": [
                      "MS_E_YESORNO"
                  ],
                  "dictType": "MS_E_YESORNO"
                }
              },
              "yearly_credit_line": {
                "control": "text",
                "label": "当年授信额度",
                "width": "120px",
                "span": 12,
                "disabled": "false"
              }

            }

          ]
        },
        {
          "title": "贷款管控",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "expand": true,
          "controls": [
            {
              "daily_total_loan_check": {
                "control": "select",
                "label": "当日贷总检查",
                "width": "120px",
                "span": 12,
                "dict": {
                  "format": "label",
                  "dictKey": [
                    "MS_E_YESORNO"
                  ],
                  "dictType": "MS_E_YESORNO"
                }
              },
              "daily_total_loan_balance": {
                "control": "text",
                "label": "当日贷总额度",
                "width": "120px",
                "span": 12,
                "disabled": "false"
              },
              "monthly_total_loan_check": {
                "control": "select",
                "label": "当月贷总检查",
                "width": "120px",
                "span": 12,
                "dict": {
                  "format": "label",
                  "dictKey": [
                      "MS_E_YESORNO"
                  ],
                  "dictType": "MS_E_YESORNO"
                }
              },
              "monthly_total_loan_line": {
                "control": "text",
                "label": "当月贷总额度",
                "width": "120px",
                "span": 12,
                "disabled": "false"
              }
            },
            {
              "yearly_total_loan_check": {
                "control": "select",
                "label": "当年贷总检查",
                "width": "120px",
                "span": 12,
                "dict": {
                  "format": "label",
                  "dictKey": [
                      "MS_E_YESORNO"
                  ],
                  "dictType": "MS_E_YESORNO"
                }
              },
              "yearly_total_loan_line": {
                "control": "text",
                "label": "当年贷总额度",
                "width": "120px",
                "span": 12,
                "disabled": "false"
              }
            }



          ]
        },
        {
          "title": "其他信息",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "expand": true,
          "controls": [
            {
              "protocol_status": {
                "control": "select",
                "label": "协议有效状态",
                "width": "120px",
                "span": 12,
                "dict": {
                  "format": "label",
                  "dictKey": [
                      "MS_E_YESORNO"
                  ],
                  "dictType": "MS_E_YESORNO"
                }
              },
              "warn_value_percent": {
                "control": "text",
                "label": "预警阀值百分比",
                "width": "120px",
                "span": 12
              }
            },
            {
              "mobile_no": {
                "control": "text",
                "label": "预警联系人手机号码",
                "width": "120px",
                "span": 12
              },
              "contact_person_name": {
                "control": "text",
                "label": "预警联系人名称",
                "width": "120px",
                "span": 12
              }
            },
            {
              "email": {
                "control": "text",
                "label": "e-mail地址",
                "width": "120px",
                "span": 12
              }

            }

          ]
        }



      ]
      }
  }
}
