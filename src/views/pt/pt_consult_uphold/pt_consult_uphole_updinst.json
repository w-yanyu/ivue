{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {

       "initDataOnce":true,
       "initRequest": {
         "url": "/SUMP/icmcall/icmRPCCall",
         "method": "POST",
         "params": {
           "servicecode": "pt0110",
           "part_protocol_no": "${{params.part_protocol_no}}"
         }
       },

      "btns": [
        {
          "type": "submit",
          "label": "保存",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "pt0112",
              "discount_rule_no": "${{params.discount_rule_no}}"
            }
          }
        }
      ],
      "controls": [
          {
            "part_protocol_no": {
              "control": "text",
              "label": "合作方协议编号",
              "width": "50%",
              "disabled": true,
              "value": "${{params.part_protocol_no}}",
              "rules": [
                {
                  "required": true,
                  "message": "合作方协议编号不得为空"
                }
              ]
            },
            "discount_cust_type": {
              "control": "select",
              "label": "贴息使用客户类型",
              "width": "50%",
              "dict": {
                "dictType": "PT_E_DISCOUNT_CUST_TYPE",
                "dictKey": [
                  "PT_E_DISCOUNT_CUST_TYPE"
                ],
                "format": "label"
              }
            }
          },
          {

            "discount_loan_use": {
              "control": "select",
              "label": "贴息贷款资金用途",
              "width": "50%",
              "dict": {
                "dictType": "PT_E_DISCOUNT_LOAN_USE",
                "dictKey": [
                  "PT_E_DISCOUNT_LOAN_USE"
                ],
                "format": "label"
              }
            },
            "claim_time": {
              "control": "select",
              "disabled": false,
              "label": "补贴申领时点",
              "width": "50%",
              "dict": {
                "dictType": "PT_E_DISCOUNT_COUNT_VALUE",
                "dictKey": [
                  "PT_E_DISCOUNT_COUNT_VALUE"
                ],
                "format": "label"
              }
            }
          },
          {
            "calculation_method": {
              "control": "select",
              "label": "补贴金额计算方式",
              "width": "50%",
              "dict": {
                "dictType": "PT_E_CALCULATION_METHOD",
                "dictKey": [
                  "PT_E_CALCULATION_METHOD"
                ],
                "format": "label"
              }
            },
            "discount_count_value": {
              "control": "text",
              "label": "贴息计算数值",
              "width": "50%",
              "rules": [
                {
                  "required": true,
                  "message": "合作方协议编号不得为空"
                }
              ]
            }

          },
          {
            "discount_deduction_account": {
              "control": "lookup",
              "edit": true,
              "label": "贴息扣款账户",
              "width": "50%",
              "keyField": {
                "discount_deduction_account": "acct_no",
                "part_no": "cust_no",
                "cert_no": "cert_no",
                "cert_type": "cert_type",
                "cust_type": "cust_type",
                "corp_size": "corp_size",
                "ind_credit": "ind_credit"
              },

              "toRequest": {
                "url": "/views/us/entManager/custfinanceManager/partAcctManager/partAcct_lookup.json",
                "method": "GET",
                "params": {
                  "cust_no": "${{params.part_no}}"
                }
              },
              "props": {
                "width": "50%",
                "title": "查询账户"
              },
              "rules": [{
                "required": true,
                "message": "请选择"
              }]
            },
            "discount_back_account": {
              "control": "lookup",
              "edit": true,
              "label": "贴息退回账户",
              "width": "50%",
              "keyField": {
                "discount_back_account": "acct_no",
                "part_no": "cust_no",
                "cert_no": "cert_no",
                "cert_type": "cert_type",
                "cust_type": "cust_type",
                "corp_size": "corp_size",
                "ind_credit": "ind_credit"
              },

              "toRequest": {
                "url": "/views/us/entManager/custfinanceManager/partAcctManager/partAcct_lookup.json",
                "method": "GET",
                "params": {
                  "cust_no": "${{params.part_no}}"
                }
              },
              "props": {
                "width": "50%",
                "title": "查询账户"
              },
              "rules": [{
                "required": true,
                "message": "请选择"
              }]
            }
          }
        ]
      }
  }
}
