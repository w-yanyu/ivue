{
  "module": "dynamicTabs",
  "layout": {
    "tabs": {

      "tabHeader": [
        {
          "id": "tab1",
          "label": "基本信息展示",
          "closable": false,
          "disabled": false,
          "icon": "el-icon-date",
          "toRequest": {
            "url": "/views/pm/pm_consult_uphold/pm_consult_uphole_updatatab1.json",
            "method": "post",
            "params": {
              "ofpacd": "${{params.ofpacd}}",
              "seqnum": "${{params.seqnum}}"

            }
          }
        },
        {
          "id": "tab2",
          "label": "规模管理信息",
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "toRequest": {
            "url": "/views/pm/pm_consult_uphold/pm_consult_uphole_updatatab2.json",
            "method": "post"
          }
        },

        {
          "id": "tab3",
          "label": "敞口配置",
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "toRequest": {
            "url": "/views/pm/pm_consult_uphold/pm_consult_uphole_openconfig.json"
          }
        },
        {
          "id": "tab4",
          "label": "协议文件",
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "toRequest": {
            "url": "@DocumentAgreement"
          }
        },
        {
          "id": "tab5",
          "label": "提交审核",
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "toRequest": {
            "url": "/views/pm/pm_consult_uphold/pm_consult_uphole_submit.json",
            "method": "post"
          }
        }
      ],
      "defaultActiveKey": "tab1",
      "position": "top",
      "styleClass": "card"
    }
  }
}
