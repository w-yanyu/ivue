{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {


      "btns": [
        {
          "type": "submit",
          "label": "保存",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "upcoag",
              "seqnum": "${{params.seqnum}}"
            }
          }
        }
      ],
      "controls": [
        {
          "cortno1": {
            "control": "text",
            "label": "子协议ID",
            "width": "50%",
            "disabled": true,
            "value": "${{params.ofpacd}}",
            "rules": [
              {
                "required": true,
                "message": "合作方编号"
              },
              {
                "max": 100,
                "message": "长度不能超过10个字符"
              }
            ]
          },
          "ofpana": {
            "control": "text",
            "label": "回购协议ID",
            "width": "50%",
            "disabled": true,
            "value": "${{params.ofpana}}",
            "rules": [
              {
                "required": true,
                "message": "合作方名称"
              },
              {
                "max": 100,
                "message": "长度不能超过100个字符"
              }
            ]
          }

        },
        {
          "pronam": {
            "control": "text",
            "label": "回购账户",
            "width": "50%",
            "value": "${{params.pronam}}",
            "rules": [
              {
                "required": true,
                "message": "协议名称不得为空"
              },
              {
                "max": 30,
                "message": "长度不能超过30个字符"
              }
            ]
          },
          "procor": {
            "control": "select",
            "label": "回购条件",
            "width": "50%",
            "value": "${{params.procor}}",
            "rules": [
              {
                "required": true,
                "message": "请输入保证金账户"
              }

            ],
            "dict": {
              "dictType": "E_LEGALER",
              "dictKey": [
                "E_LEGALER"
              ],
              "format": "label"
            }
          }
        },



        {



          "blbrch": {
            "control": "select",
            "label": "回购条件阈值",
            "width": "50%",
            "value": "${{params.blbrch}}",
            "rules": [
              {
                "required": true,
                "message": "请输入所属机构"
              }
            ],
            "dict": {
              "dictType": "E_BLBRCH",
              "dictKey": [
                "E_BLBRCH"
              ],
              "format": "label"
            }
          }
        }
      ]
    }
  }
}
