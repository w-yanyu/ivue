{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "project_template_no": {
            "label": "合作项目模版编号",
            "control": "text",
            "span": 12
          },
          "project_mould_name": {
            "label": "合作项目模版名称",
            "control": "text",
            "span": 12
          }

        }
      ]
    },
    "toolbar": {
    },
    "datagrid": {
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "pt0095"
        }
      },
      "thead": [
        {
          "label": "合作项目模版编号",
          "prop": "project_template_no",
          "align": "center"
        },
        {
          "label": "合作项目模版名称",
          "prop": "project_mould_name",
          "align": "center"
        },
        {
          "label": "合作项目类型",
          "prop": "part_project_type",
          "align": "center",
          "dict": {
            "dictType": "PT_E_PART_PROJECT_TYPE",
            "dictKey": [
              "PT_E_PART_PROJECT_TYPE"
            ],
            "format": "label"
          }
        },
        {
          "label": "检查资料列表编号",
          "prop": "data_list_no",
          "align": "center"
        },
        {
          "label": "操作",
          "align": "center",
          "btns": [
            {
              "type": "bringback",
              "label": "选择"
            }
          ]
        }
      ]
    }
  }
}
