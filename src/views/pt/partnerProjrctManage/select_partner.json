{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "part_no": {
            "label": "合作方编号",
            "control": "text"
          },
          "center_name": {
            "label": "合作方名称",
            "control": "text"
          }
        }
      ]
    },
    "toolbar": {
    },
    "datagrid": {
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "center_status":"1",
          "servicecode": "pt0040"
        }
      },
      "thead": [
        {
          "label": "合作方编号",
          "prop": "part_no",
          "align": "center"
        },
        {
          "label": "合作方名称",
          "prop": "center_name",
          "align": "center"
        },
        {
          "label": "客户编号",
          "prop": "cust_no",
          "align": "center"
        },
        {
          "label": "操作",
          "align": "center",
          "btns": [
            {
              "type": "bringback",
              "label": "选择"
            }
          ]
        }
      ]
    }
  }
}
