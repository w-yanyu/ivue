{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "scope": "strForm",
      "controls":
      [
        {
          "part_project_no": {
            "control": "text",
            "label":  "合作项目编号",
            "span": 8
          },
          "part_project_name": {
            "control": "text",
            "label": "合作项目名称",
            "span": 8
          },
          "part_project_type": {
            "control": "select",
            "span": 8,
            "label": "合作项目类型",
            "dict": {
              "dictType": "PT_E_PART_PROJECT_TYPE",
              "dictKey": [
                "PT_E_PART_PROJECT_TYPE"
              ],
              "format": "label"
            }
          }
        }
      ]
    },
    "datagrid": {
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "pt0303",
          "approve_status": "01"
        }
      },
      "thead": [
        {
          "type": "index",
          "label": "序号"
        },
        {
          "label": "合作项目编号",
          "prop": "part_project_no"
        },
        {
          "label": "合作项目名称",
          "prop": "part_project_name"
        },
        {
          "label":  "合作项目描述",
          "prop": "part_project_describe"
        },
        {
          "label": "合作项目类型",
          "prop": "part_project_type",
          "dict": {
            "dictType": "PT_E_PART_PROJECT_TYPE",
            "dictKey": ["PT_E_PART_PROJECT_TYPE"],
            "format": "label"
          }

        },
        {
          "label":  "项目生效日期",
          "prop": "effect_date"
        },
        {
          "label":  "项目失效日期",
          "prop": "expiry_date"
        },
        {
          "label": "项目状态",
          "prop": "proj_status",
          "dict": {
            "dictType": "PT_E_STATUS",
            "dictKey": ["PT_E_STATUS"],
            "format": "label"
          }

        },
        {
          "label": "预警状态",
          "prop": "warn_status",
          "dict": {
            "dictType": "PT_E_WARN_STATUS",
            "dictKey": ["PT_E_WARN_STATUS"],
            "format": "label"
          }

        },
        {
          "label": "审批状态",
          "prop": "approve_status",
          "dict": {
            "dictType": "PT_E_APPROVE_STATUS",
            "dictKey": [
              "PT_E_APPROVE_STATUS"
            ],
            "format": "label"
          }

        },
        {
          "label": "操作",
          "width": "260px",
          "fixed": "right",
          "btns": [
            {
              "type": "look",
              "label":  "审批",
              "toRequest": {
                "url": "/views/pt/partnerProjrctManage/approve.json",
                "method": "POST"
              },
              "props": {
                "width": "30%",
                "title":  "审批"
              },
              "show": {
                "prop": "approve_status",
                "is": [
                  "01"
                ]
              }
            },
            {
              "type": "look",
              "fixed": "right",
              "label": "详情",
              "toRequest": {
                "url": "@projectAddLayout",
                "method": "GET",
                "params": {
                  "tranType": "detail",
                  "part_project_no": "${{current.part_project_no}}",
                  "project_template_no":"${{current.project_template_no}}"
                }
              },
              "props": {
                "width": "65%",
                "title":  "查看详情"
              }
            },
            {
              "type": "look",
              "label": "审核记录",
              "toRequest": {
                "url": "/views/pt/partnertemplateManage/apprRecordManage/apprRecord_list.json",
                "method": "GET",
                "params": {
                  "sale_prod_cd": "${{current.part_project_no}}",
                  "mould_code": "3"
                  
                }
              },
              "props": {
                "width": "65%",
                "title":  "查看审核记录"
              }
            }
          ]
        }
      ]
    }
  }
}
