{
  "module": "v_realonlyForm_btn",
  "layout": {
    "form": {
      "scope": "templateScope",
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pt0304",
          "part_project_no": "${{params.part_project_no}}",
          "version": "${{params.version}}"
        }
      },
      "btns": [],
      "controls": [
        {
          "part_project_no": {
            "control": "string",
            "disabled": true,
            "label": "合作项目编号",
            "span": 8,
            "width": "120px"
          },
          "part_project_name": {
            "control": "string",
            "label": "合作项目名称",
            "span": 8,
            "width": "120px"
          },
          "part_project_type": {
            "value": "${{params.part_project_type}}",
            "label": "项目类型",
            "disabled": true,
            "span": 8,
            "width": "120px",
            "control": "string",
            "dict": {
              "dictType": "PT_E_PART_PROJECT_TYPE",
              "dictKey": [
                "PT_E_PART_PROJECT_TYPE"
              ],
              "format": "value-label"
            }
          }
        },
        {
          "effect_date": {
            "label": "生效日期",
            "span": 8,
            "width": "120px",
            "control": "string",
            "format": "yyyyMMdd"
          },
          "expiry_date": {
            "label": "失效日期",
            "span": 8,
            "width": "120px",
            "control": "string",
            "format": "yyyyMMdd"
          }
        },
        {

          "part_project_describe": {
            "label": "合作项目描述",
            "control": "textarea",
            "width": "120px",
            "span": 24,
            "disabled": true,
            "autosize": {
              "minRows": 3,
              "maxRows": 5
            },
            "maxlength": 500,
            "showWordLimit": true
          },
          "project_template_no": {
            "control": "hidden",
            "label": "项目模版编号",
            "span": 8
          }
        }
      ]
    }
  }
}
