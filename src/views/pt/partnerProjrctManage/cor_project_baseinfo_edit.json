{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "scope": "templateScope",
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pt0304",
          "part_project_no": "${{params.part_project_no}}"
        }
      },
      "btns": [
        {
          "type": "submit",
          "label": "保存",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "pt0301",
              "part_project_no": "${{params.part_project_no}}"
            }
          },
          "eventAfter": {
            "doLastCallback": false
          }
        }
      ],
      "controls": [
        {
          "part_project_no": {
            "control": "text",
            "disabled": true,
            "label": "合作项目编号",
            "span": 8,
            "width": "120px",
            "rules": [
              {
                "required": true,
                "message": "可售产品名称不得为空"
              },
              {
                "max": 80,
                "message": "长度不能超过80个字符"
              }
            ]
          },
          "part_project_name": {
            "control": "text",
            "label": "合作项目名称",
            "span": 8,
            "width": "120px",
            "rules": [
              {
                "required": true,
                "message": "合作项目名称不得为空"
              },
              {
                "max": 80,
                "message": "长度不能超过80个字符"
              }
            ]
          },
          "part_project_type": {
            "value": "${{params.part_project_type}}",
            "label": "项目类型",
            "disabled": true,
            "span": 8,
            "width": "120px",
            "control": "select",
            "dict": {
              "dictType": "PT_E_PART_PROJECT_TYPE",
              "dictKey": [
                "PT_E_PART_PROJECT_TYPE"
              ],
              "format": "value-label"
            },
            "rules": [
              {
                "required": true,
                "message": "项目类型不得为空"
              }
            ]
          }
        },
        {
          "effect_date": {
            "label": "生效日期",
            "span": 8,
            "width": "120px",
            "control": "dateTimePicker",
            "format": "yyyyMMdd",
            "rules": [
              {
                "required": true,
                "message": "请输入生效日期"
              }
            ]
          },
          "expiry_date": {
            "label": "失效日期",
            "span": 8,
            "width": "120px",
            "control": "dateTimePicker",
            "format": "yyyyMMdd",
            "rules": [
              {
                "required": true,
                "message": "请输入失效日期"
              }
            ]
          }
        },
        {
          "part_project_describe": {
            "label": "合作项目描述",
            "control": "textarea",
            "width": "140px",
            "placeholder": "(500个字以内,若没有请填“无”)",
            "span": 24,
            "width": "120px",
            "autosize": {
              "minRows": 3,
              "maxRows": 5
            },
            "maxlength": 500,
            "showWordLimit": true
          },
          "project_template_no": {
            "control": "hidden",
            "label": "项目模版编号",
            "span": 8
          },
          "version": {
            "control": "hidden",
            "label": "版本号",
            "value": "${{params.version}}",
            "span": 8
          }
        }
      ]
    }
  }
}
