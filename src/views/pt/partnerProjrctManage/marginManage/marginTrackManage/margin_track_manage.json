{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pt0337",
          "margin_protocol_no": "${{params.margin_protocol_no}}",
          "project_version": "${{params.project_version}}"
        }
      },
      "thead": [
        {
          "type": "string",
          "label": "调整轨迹编号",
          "prop": "track_no",
          "align": "center"
        },
        {
          "type": "string",
          "label": "保证金操作类型",
          "prop": "margin_oper_type",
          "align": "center",
          "dict": {
            "dictType": "PT_E_MARGIN_OPER_TYPE",
            "dictKey": [
              "PT_E_MARGIN_OPER_TYPE"
            ],
            "format": "label"
          }
        },
        {
          "type": "string",
          "label": "存入/取出金额",
          "prop": "oper_amt",
          "align": "center"
        },
        {
          "type": "string",
          "label": "调整金额币种",
          "prop": "track_ccy_code",
          "align": "center",
          "dict": {
            "dictType": "MS_CURRENCY",
            "dictKey": [
              "MS_CURRENCY"
            ],
            "format": "label"
          }
        },
        {
          "type": "string",
          "label": "调整轨迹登记时间",
          "prop": "track_register_time",
          "align": "center"
        },
        {
          "type": "string",
          "label": "调整轨迹备注",
          "prop": "track_remark",
          "align": "center"
        },
        {
          "type": "string",
          "label": "合作方编号",
          "prop": "part_no",
          "align": "center"
        },
        {
          "type": "string",
          "label": "合作方名称",
          "prop": "center_name",
          "align": "center"
        },
        {
          "type": "string",
          "label": "可售产品编号",
          "prop": "sale_prod_cd",
          "align": "center"
        },
        {
          "type": "string",
          "label": "可售产品名称",
          "prop": "sale_prod_name",
          "align": "center"
        },
        {
          "type": "string",
          "label": "合作项目编号",
          "prop": "part_project_no",
          "align": "center"
        },
        {
          "type": "string",
          "label": "合作项目名称",
          "prop": "part_project_name",
          "align": "center"
        },
        {
          "type": "string",
          "label": "合作项目版本",
          "prop": "project_version",
          "align": "center"
        },
        {
          "type": "string",
          "label": "保证金协议编号",
          "prop": "margin_protocol_no",
          "align": "center"
        },
        {
          "type": "string",
          "label": "保证金账号",
          "prop": "guaranty_no",
          "align": "center"
        },
        {
          "type": "string",
          "label": "保证金账户余额",
          "prop": "margin_acct_balance",
          "align": "center"
        },
        {
          "type": "string",
          "label": "币种",
          "prop": "ccy_code",
          "align": "center",
          "dict": {
            "dictType": "MS_CURRENCY",
            "dictKey": [
              "MS_CURRENCY"
            ],
            "format": "label"
          }
        },
        {
          "type": "string",
          "label": "登记时间",
          "prop": "register_time",
          "align": "center"
        },
        {
          "type": "string",
          "label": "保证金状态",
          "prop": "margin_status",
          "align": "center",
          "dict": {
            "dictType": "PT_E_MARGIN_STATUS",
            "dictKey": [
              "PT_E_MARGIN_STATUS"
            ],
            "format": "label"
          }
        }
      ]
    }
  }
}
