{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "scope": "strForm",
      "controls":
      [
        {
          "part_project_no": {
            "control": "text",
            "label":  "合作项目编号",
            "span": 8
          },
          "part_project_name": {
            "control": "text",
            "label": "合作项目名称",
            "span": 8
          },
          "part_project_type": {
            "control": "select",
            "span": 8,
            "label": "合作项目类型",
            "dict": {
              "dictType": "PT_E_PART_PROJECT_TYPE",
              "dictKey": [
                "PT_E_PART_PROJECT_TYPE"
              ],
              "format": "label"
            }
          }
        }
      ]
    },
    "datagrid": {
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "pt0321"
        }
      },
      "thead": [
        {
          "type": "index",
          "label": "序号"
        },
        {
          "label": "合作项目编号",
          "prop": "part_project_no"
        },
        {
          "label": "合作项目名称",
          "prop": "part_project_name"
        },
        {
          "label":  "合作项目描述",
          "prop": "part_project_describe"
        },
        {
          "label": "合作项目类型",
          "prop": "part_project_type",
          "dict": {
            "dictType": "PT_E_PART_PROJECT_TYPE",
            "dictKey": ["PT_E_PART_PROJECT_TYPE"],
            "format": "label"
          }

        },
        {
          "label":  "项目生效日期",
          "prop": "effect_date"
        },
        {
          "label":  "项目失效日期",
          "prop": "expiry_date"
        },
        {
          "label": "项目状态",
          "prop": "proj_status",
          "dict": {
            "dictType": "PT_E_STATUS",
            "dictKey": ["PT_E_STATUS"],
            "format": "label"
          }

        },
        {
          "label": "预警状态",
          "prop": "warn_status",
          "dict": {
            "dictType": "PT_E_WARN_STATUS",
            "dictKey": ["PT_E_WARN_STATUS"],
            "format": "label"
          }

        },
        {
          "label": "数据版本",
          "prop": "version"
        },
        {
          "label": "操作",
          "fixed": "right",
          "width": 200,
          "align": "center",
          "btns": [
            {
              "type": "bringback",
              "label": "选择"
            }
          ]
        }
      ]
    }
  }
}
