{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "scope": "templateScope",
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pt0315",
          "part_project_no": "${{params.part_project_no}}",
          "version": "${{params.version}}"
        }
      },
      "btns": [],
      "controls": [
        {
          "area_access": {
            "control": "text",
            "label": "地区准入",
            "span": 8,
            "rules": [
              {
                "required": true,
                "message": "请输入地区准入"
              }
            ]
          },
          "industry_access": {
            "control": "text",
            "label": "行业准入",
            "span": 8,
            "rules": [
              {
                "required": true,
                "message": "请输入行业准入"
              }
            ]
          },
          "min_age": {
            "label": "最低年龄",
            "span": 8,
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入年龄"
              }
            ]
          }
        },
        {
          "mar_age": {
            "control": "text",
            "label": "最高年龄",
            "span": 8
          },
          "work_tp": {
            "control": "select",
            "label": "职业",
            "span": 8,
            "dict": {
              "dictType": "MS_E_WORKTP",
              "dictKey": [
                "MS_E_WORKTP"
              ],
              "format": "label",
              "rules": [
                {
                  "required": true,
                  "message": "请选择职业"
                }
              ]

            }
          },
          "min_education": {
            "label": "最低学历",
            "control": "select",
            "span": 8,
            "dict": {
              "dictType": "MS_E_MAXEDU",
              "dictKey": [
                "MS_E_MAXEDU"
              ],
              "format": "label",
              "rules": [
                {
                  "required": true,
                  "message": "请选择最低学历"
                }
              ]
            }
          }
        },
        {
          "check_type": {
            "label": "检查控制类型",
            "span": 8,
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入检查控制类型"
              }
            ]
          },
          "servicecode": {
            "label": "交易码",
            "span": 8,
            "value": "pt0316",
            "control": "hidden"
          },
          "version": {
            "label": "数据版本",
            "span": 8,
            "value":  "${{params.version}}",
            "control": "hidden"
          }
        }
      ]
    }
  }
}
