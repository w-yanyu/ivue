{
  "module": "v_realonlyForm_btn",
  "layout": {
    "form": {
      "scope": "templateScope",
      "initRequest": "${{params}}",
      "controls": [
        {
          "car_dealer_name": {
            "control": "string",
            "label": "汽车经销商名称",
            "span": 8,
            "rules": [
              {
                "required": true,
                "message": "汽车经销商名称不得为空"
              },
              {
                "max": 80,
                "message": "长度不能超过80个字符"
              }
            ]
          },
          "offipart_area": {
            "control": "string",
            "label": "合作区域中心码",
            "span": 8,
            "rules": [
              {
                "required": true,
                "message": "合作区域中心码不得为空"
              },
              {
                "max": 80,
                "message": "长度不能超过80个字符"
              }
            ]
          },
          "part_brand": {
            "label": "合作车品牌",
            "span": 8,
            "control": "string",
            "rules": [
              {
                "required": true,
                "message": "合作车品牌不得为空"
              },
              {
                "max": 80,
                "message": "长度不能超过80个字符"
              }
            ]
          }
        },
        {
          "part_car_series": {
            "control": "string",
            "label": "合作车系",
            "span": 8,
            "rules": [{
              "required": true,
              "message": "合作车系不能为空!"
            }]
          },
          "part_info": {
            "control": "textarea",
            "span": 16,
            "label": "合作4S店信息",
            "width": "140px",
            "placeholder": "(500个字以内,若没有请填“无”)",
            "align": "left",
            "autosize": {
              "minRows": 3,
              "maxRows": 5
            },
            "disabled": true,
            "maxlength": 500,
            "showWordLimit": true
          }
        }
      ]
    }
  }
}
