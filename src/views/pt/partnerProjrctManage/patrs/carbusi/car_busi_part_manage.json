{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "scope": "strForm",
      "controls":
      [
        {
          "car_dealer_name": {
            "control": "text",
            "label": "汽车经销商名称",
            "span": 8
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "target": "dialog",
          "label": "新增",
          "toRequest": {
            "method": "GET",
            "url": "/views/pt/partnerProjrctManage/patrs/carbusi/car_busi_part_add.json",
            "params": {
              "part_project_no": "${{params.part_project_no}}",
              "version": "${{params.version}}"
            }
          },
          "props": {
            "width": "65%",
            "title":  "汽车经销商信息新增"
          }
        }
      ]
    },
    "datagrid": {
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "pt0313",
          "part_project_no": "${{params.part_project_no}}",
          "version": "${{params.version}}"
        }
      },
      "thead": [
        {
          "type": "index",
          "label": "序号"
        },
        {
          "label": "汽车经销商名称",
          "prop": "car_dealer_name"
        },
        {
          "label": "合作区域中心码",
          "prop": "offipart_area"
        },
        {
          "label":"合作4S店信息",
          "prop": "part_info"
        },
        {
          "label":  "合作车品牌",
          "prop": "part_brand"
        },
        {
          "label":  "合作车系",
          "prop": "part_car_series"
        },
        {
          "label": "操作",
          "width": "260px",
          "btns": [
            {
              "type": "edit",
              "label":  "维护",
              "toRequest": {
                "url": "/views/pt/partnerProjrctManage/patrs/carbusi/car_busi_part_md.json",
                "method": "Get"
              },
              "props": {
                "width": "65%",
                "title":  "汽车经销商信息维护"
              }
            },
            {
              "type": "custom",
              "label": "删除",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "pt0314"
                }
              },
              "confirm": {
                "title": "确定删除",
                "confirmButtonText": "确定",
                "cancelButtonText": "取消"
              }
            },
            {
              "type": "look",
              "label": "详情",
              "toRequest": {
                "url": "/views/pt/partnerProjrctManage/patrs/carbusi/car_busi_part_detail.json",
                "method": "GET"
              },
              "props": {
                "width": "65%",
                "title":  "查看详情"
              }
            }
          ]
        }
      ]
    }
  }
}
