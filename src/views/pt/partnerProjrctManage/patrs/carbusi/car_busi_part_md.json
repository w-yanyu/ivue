{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "scope": "templateScope",
      "initRequest": "${{params}}",
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pt0311",
          "part_project_no": "${{params.part_project_no}}",
          "car_dealer_no": "${{params.car_dealer_no}}",
          "version": "${{params.version}}"
        }
      },
      "controls": [
        {
          "car_dealer_name": {
            "control": "text",
            "label": "汽车经销商名称",
            "span": 8,
            "rules": [
              {
                "required": true,
                "message": "汽车经销商名称不得为空"
              },
              {
                "max": 80,
                "message": "长度不能超过80个字符"
              }
            ]
          },
          "offipart_area": {
            "control": "text",
            "label": "合作区域中心码",
            "span": 8,
            "rules": [
              {
                "required": true,
                "message": "合作区域中心码不得为空"
              },
              {
                "max": 80,
                "message": "长度不能超过80个字符"
              }
            ]
          },
          "part_brand": {
            "label": "合作车品牌",
            "span": 8,
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "合作车品牌不得为空"
              },
              {
                "max": 80,
                "message": "长度不能超过80个字符"
              }
            ]
          }
        },
        {
          "part_car_series": {
            "control": "text",
            "label": "合作车系",
            "span": 8,
            "rules": [{
              "required": true,
              "message": "合作车系不能为空!"
            }]
          },
          "part_info": {
            "control": "textarea",
            "label": "合作4S店信息",
            "width": "140px",
            "placeholder": "(500个字以内,若没有请填“无”)",
            "align": "left",
            "autosize": {
              "minRows": 3,
              "maxRows": 5
            },
            "maxlength": 500,
            "showWordLimit": true
          }
        }
      ]
    }
  }
}
