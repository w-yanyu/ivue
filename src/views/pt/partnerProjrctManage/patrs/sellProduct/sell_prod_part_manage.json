{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "scope": "strForm",
      "controls":
      [
        {
          "sale_prod_cd": {
            "control": "text",
            "label": "可售产品编号",
            "span": 12
          },
          "sale_prod_name": {
            "control": "text",
            "label": "可售产品名称",
            "span": 12
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "target": "dialog",
          "label": "新增",
          "toRequest": {
            "method": "GET",
            "url": "/views/pt/partnerProjrctManage/patrs/sellProduct/sell_prod_part_add.json",
            "params": {
              "part_project_no": "${{params.part_project_no}}",
              "version": "${{params.version}}"
            }
          },
          "props": {
            "width": "65%",
            "title":  "可售产品信息新增"
          }
        }
      ]
    },
    "datagrid": {
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "pt0317",
          "part_project_no": "${{params.part_project_no}}",
          "version": "${{params.version}}"
        }
      },
      "thead": [
        {
          "type": "index",
          "label": "序号"
        },
        {
          "label": "可售产品编号",
          "prop": "sale_prod_cd"
        },
        {
          "label": "可售产品名称",
          "prop": "sale_prod_name"
        },
        {
          "label":"可售产品类型",
          "prop": "prod_type_name"
        },
        {
          "label":  "产品版本",
          "prop": "prod_version"
        },
        {
          "label": "资料检查列表名称",
          "prop": "check_filelist_nm"

        },
        {
          "label":  "资料检查列表编号",
          "prop": "data_list_no"
        },
        {
          "label": "操作",
          "width": "260px",
          "btns": [
            {
              "type": "edit",
              "label":  "维护",
              "fixed":"right",
              "toRequest": {
                "url": "/views/pt/partnerProjrctManage/patrs/sellProduct/sell_prod_part_md.json",
                "method": "Get"
              },
              "props": {
                "width": "65%",
                "title":  "可售产品信息维护"
              }
            },
            {
              "type": "custom",
              "label": "删除",
              "fixed":"right",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "pt0318",
                  "mainten_tp": "2"
                }
              },
              "confirm": {
                "title": "确定删除",
                "confirmButtonText": "确定",
                "cancelButtonText": "取消"
              }
            },
            {
              "type": "look",
              "label": "详情",
              "fixed":"right",
              "toRequest": {
                "url": "/views/pt/partnerProjrctManage/patrs/sellProduct/sell_prod_part_detail.json",
                "method": "GET"
              },
              "props": {
                "width": "65%",
                "title":  "查看详情"
              }
            }
          ]
        }
      ]
    }
  }
}
