{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "scope": "templateScope",
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pt0318",
          "mainten_tp": "0",
          "part_project_no": "${{params.part_project_no}}",
          "version": "${{params.version}}"
        }
      },
      "controls": [
        {
          "sale_prod_cd": {
            "control": "lookup",
            "label": "可售产品编号",
            "edit": false,
            "span": 8,
            "rules": [
              {
                "required": true,
                "message": "请输入可售产品编号"
              },
              {
                "max": 80,
                "message": "长度不能超过80个字符"
              }
            ],
            "keyField": {
              "sale_prod_cd": "sale_prod_cd",
              "sale_prod_name": "sale_prod_name",
              "prod_version": "prod_version",
              "prod_type_name": "prod_type_name"
            },
            "toRequest": {
              "url": "/views/pt/partnerProjrctManage/patrs/sellProduct/sell_prod_look_up.json",
              "method": "post"
            },
            "props": {
              "title": "选择可售产品"
            }
          },
          "sale_prod_name": {
            "control": "text",
            "label": "可售产品名称",
            "span": 8,
            "disabled": true,
            "rules": [
              {
                "required": true,
                "message": "可售产品名称不得为空"
              },
              {
                "max": 80,
                "message": "长度不能超过80个字符"
              }
            ]
          },
          "prod_type_name": {
            "label": "可售产品类型",
            "span": 8,
            "disabled": true,
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "可售产品类型不得为空"
              },
              {
                "max": 80,
                "message": "长度不能超过80个字符"
              }
            ]
          }
        },
        {
          "prod_version": {
            "control": "text",
            "label": "产品版本",
            "span": 8,
            "disabled": true,
            "rules": [{
              "required": true,
              "message": "产品版本不能为空!"
            }]
          },
          "check_filelist_nm": {
            "control": "lookup",
            "label": "资料检查列表名称",
            "edit": false,
            "span": 8,
            "keyField": {
              "data_list_no": "doc_check_list_no",
              "check_filelist_nm": "doc_check_list_desc"
            },
            "toRequest": {
              "url": "/views/pd/prod/templatePage/process/childPage/docChecklist_lookup.json",
              "method": "post"
            },
            "props": {
              "title": "选择资料检查列表"
            }
          },
          "data_list_no": {
            "control": "text",
            "label": "资料检查列表编号",
            "span": 8,
            "disabled": true,
            "rules": [{
              "required": true,
              "message": "产品版本不能为空!"
            }]
          }
        }
      ]
    }
  }
}
