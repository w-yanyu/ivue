{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "pt0430",
          "part_protocol_no": "${{params.part_protocol_no}}",
          "version": "${{params.version}}"
        }
      },
      "btns": [],
      "controls": [
        {
          "premium_protocol_no": {
            "control": "hidden",
            "label": "保费代收协议编号",
            "width": "50px",
            "span": 8
          },
          "premium_basis": {
            "control": "select",
            "label": "保费收取依据",
            "disabled": true,
            "width": "50px",
            "span": 8,
            "dict": {
              "dictType": "PT_E_PREMIUM_BASIS",
              "dictKey": [
                "PT_E_PREMIUM_BASIS"
              ],
              "format": "label"
            },
            "showItem": {
              "0": [],
              "1": [
                "premium_way",
                "premium_base",
                "ccy_code",
                "execution_inst_rate",
                "billing_frequency",
                "billing_date",
                "billing_period_unit",
                "period_value"
              ]
            },
            "events": {
              "condition": {
                "0": {
                  "control": {
                    "premium_way": {
                      "value": "",
                      "rules": [
                        {
                          "required": false
                        }
                      ]
                    },
                    "premium_base": {
                      "value": ""
                    },
                    "ccy_code": {
                      "value": "",
                      "rules": [
                        {
                          "required": false
                        }
                      ]
                    },
                    "execution_inst_rate": {
                      "value": ""
                    },
                    "billing_frequency": {
                      "value": ""
                    },
                    "billing_date": {
                      "value": ""
                    },
                    "billing_period_unit": {
                      "value": ""
                    },
                    "period_value": {
                      "value": ""
                    }
                  }
                }
              }
            }
          },
          "premium_way": {
            "control": "select",
            "label": "保费收取方式",
            "width": "50px",
            "span": 8,
            "disabled": true,
            "dict": {
              "dictType": "PT_E_PREMIUM_WAY",
              "dictKey": [
                "PT_E_PREMIUM_WAY"
              ],
              "format": "label"
            }
          },
          "premium_base": {
            "control": "select",
            "label": "保费计算基准",
            "width": "50px",
            "span": 8,
            "disabled": true,
            "dict": {
              "dictType": "PT_E_COMPUTE_BASE",
              "dictKey": [
                "PT_E_COMPUTE_BASE"
              ],
              "format": "label"
            },
            "showItem": {
              "$default": [],
              "1": [
                "ccy_code"
              ]
            }
          },
          "ccy_code": {
            "control": "select",
            "label": "币种",
            "width": "50px",
            "span": 8,
            "disabled": true,
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": [
                "MS_CURRENCY"
              ]
            }
          },
          "execution_inst_rate": {
            "control": "currency",
            "label": "执行费率(%)",
            "width": "50px",
            "span": 8,
            "disabled": true,
            "format": {
              "thousand": ",",
              "decimal": 2
            }
          },
          "billing_frequency": {
            "control": "select",
            "label": "结算频率",
            "width": "50px",
            "span": 8,
            "disabled": true,
            "dict": {
              "dictType": "PT_E_BILLING_PREQ",
              "dictKey": [
                "PT_E_BILLING_PREQ"
              ],
              "format": "label"
            },
            "showItem": {
              "1": [
                "billing_date"
              ],
              "2": [
                "billing_period_unit",
                "period_value"
              ]
            }
          },
          "billing_date": {
            "control": "dateTimePicker",
            "label": "结算日期",
            "width": "50px",
            "span": 8,
            "disabled": true,
            "valueFormat": "yyyyMMdd"
          },
          "billing_period_unit": {
            "control": "select",
            "label": "结算周期单位",
            "width": "50px",
            "span": 8,
            "disabled": true,
            "dict": {
              "dictType": "MS_E_CYCLETYPE",
              "dictKey": [
                "MS_E_CYCLETYPE"
              ],
              "format": "label"
            }
          },
          "period_value": {
            "control": "text",
            "label": "周期值",
            "width": "50px",
            "span": 8,
            "disabled": true
          },
          "premium_acct_no": {
            "control": "text",
            "label": "保费资金账号",
            "width": "50px",
            "span": 8,
            "disabled": true
          },
          "premium_acct_name": {
            "control": "text",
            "label": "保费资金账户名",
            "width": "50px",
            "span": 8,
            "disabled": true
          }
        }
      ]
    }
  }
}
