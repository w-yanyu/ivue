{
  "module": "v_realonlyForm_btn",
  "layout": {
    "form": {
      "scope": "templateScope",
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
         "servicecode": "pt0101",
          "part_project_no": "${{params.part_project_no}}",
          "version": "${{params.version}}"
        }
      },
      "controls": [
        {
          "channel_part_medol": {
            "control": "select",
            "label": "渠道合作模式",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请选择渠道合作模式"
              }
            ],
            "dict": {
              "dictType": "PT_E_CHANNEL_PART_MEDOL",
              "dictKey": [
                "PT_E_CHANNEL_PART_MEDOL"
              ],
              "format": "label",
              "rules": [
                {
                  "required": true,
                  "message": "请选择职业"
                }
              ]

            }

          },
          "lee_payment": {
            "control": "text",
            "label": "放款支付方",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "放款支付方不得为空"
              }
            ],
            "dict": {
              "dictType": "PT_E_LEE_PAYMENT",
              "dictKey": [
                "PT_E_LEE_PAYMENT"
              ],
              "format": "label",
              "rules": [
                {
                  "required": true,
                  "message": "请选择职业"
                }
              ]

            }
          }

        },
        {
          "repayment_deduction": {
            "control": "text",
            "label": "还款划扣方",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "还款划扣方不得为空"
              }
            ],
            "dict": {
              "dictType": "PT_E_LEE_PAYMENT",
              "dictKey": [
                "PT_E_LEE_PAYMENT"
              ],
              "format": "label",
              "rules": [
                {
                  "required": true,
                  "message": "请选择职业"
                }
              ]

            }
          },
          "push_repayment_list": {
            "control": "select",
            "label": "推送还款列表",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "推送还款列表不得为空"
              }
            ],
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"

            },
            "showItem": {


              "1": [
                "pre_repayment_day"
              ],
              "0": []
            }
          }
        },
        {
          "pre_repayment_day": {
            "control": "text",
            "label": "提前推送还款列表天数",
            "disabled": true,
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "提前推送还款列表天数不得为空"
              },
              {
                "max": 80,
                "message": "长度不能超过80个字符"
              }
            ]
          }
        }
      ]
    }
  }
}
