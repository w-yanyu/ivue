{
  "module": "v_realonlyForm_btn",
  "layout": {
    "form": {
      "scope": "templateScope",
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pt0100",
          "part_project_no": "${{params.part_project_no}}",
          "version": "${{params.version}}"
        }
      },
      "controls": [
        {
          "channel_part_medol": {
            "control": "string",
            "label": "渠道合作模式",
            "span": 8,
            "dict": {
              "dictType": "PT_E_CHANNEL_PART_MEDOL",
              "dictKey": [
                "PT_E_CHANNEL_PART_MEDOL"
              ],
              "format": "label"

            }

          },
          "lee_payment": {
            "control": "string",
            "label": "放款支付方",
            "span": 8,
            "dict": {
              "dictType": "PT_E_LEE_PAYMENT",
              "dictKey": [
                "PT_E_LEE_PAYMENT"
              ],
              "format": "label"

            }
          },
          "repayment_deduction": {
            "control": "string",
            "label": "还款划扣方",
            "span": 8,
            "dict": {
              "dictType": "PT_E_LEE_PAYMENT",
              "dictKey": [
                "PT_E_LEE_PAYMENT"
              ],
              "format": "label"

            }
          }
        },
        {
          "push_repayment_list": {
            "control": "string",
            "label": "推送还款列表",
            "span": 8,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"

            },
            "showItem": {
              "1": [
                "pre_repayment_day"
              ],
              "0": []
            }
          },
          "pre_repayment_day": {
            "control": "string",
            "label": "提前推送还款列表天数",
            "span": 8
          }
        }
      ]
    }
  }
}
