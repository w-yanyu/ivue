{
  "module": "v_search_btn_datagrid",
  "layout": {

    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pt0055",
          "part_no": "${{params.part_no}}"
        }
      },
      "thead": [
        {
          "type": "index",
          "width": 50,
          "label": "序号"
        },
        {
          "label": "合作账户简称",
          "prop": "center_account_name",
          "align": "center"
        },
        {
          "label": "合作方账户账号",
          "prop": "part_center_no",
          "align": "center"
        },
        {
          "label": "合作方账户名称",
          "prop": "part_center_name",
          "align": "center"
        },
        {
          "label": "操作",
          "align": "center",
          "width":"200px",
          "btns": [
            {
              "type": "bringback",
              "label": "选择"
            }
          ]
        }
      ]
    }
  }
}
