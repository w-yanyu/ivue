{
  "module": "v_realonlyForm_btn",
  "layout": {
    "form": {
      "initDataOnce":true,
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pt0110",
          "part_protocol_no": "${{params.part_protocol_no}}",
          "version": "${{params.version}}"
        }
      },
      "btns": [
      ],
      "controls": [
        {
          "discount_cust_type": {
            "control": "string",
            "width": "50%",
            "label": "贴息使用客户类型",
            "dict": {
              "dictType": "PT_E_DISCOUNT_CUST_TYPE",
              "dictKey": [
                "PT_E_DISCOUNT_CUST_TYPE"
              ],
              "format": "label"
            }
          },
          "discount_loan_use": {
            "control": "string",
            "width": "50%",
            "label": "贴息贷款资金用途",
            "dict": {
              "dictType": "PT_E_DISCOUNT_LOAN_USE",
              "dictKey": [
                "PT_E_DISCOUNT_LOAN_USE"
              ],
              "format": "label"
            }
          }
        },
        {
          "discount_time": {
            "control": "string",
            "width": "50%",
            "label": "补贴申领时点",
            "dict": {
              "dictType": "PT_E_DISCOUNT_COUNT_VALUE",
              "dictKey": [
                "PT_E_DISCOUNT_COUNT_VALUE"
              ],
              "format": "label"
            }
          },
          "calculation_method": {
            "control": "string",
            "width": "50%",
            "label": "补贴金额计算方式",
            "dict": {
              "dictType": "PT_E_CALCULATION_METHOD",
              "dictKey": [
                "PT_E_CALCULATION_METHOD"
              ],
              "format": "label"

            }
          }
        },
        {
          "discount_count_value": {
            "control": "string",
            "width": "50%",
            "label": "贴息计算数值"
          }
        },
        {
          "discount_deduction_account": {
            "control": "string",
            "width": "50%",
            "label": "贴息扣款账户"
          },
          "discount_back_account": {
            "control": "string",
            "width": "50%",
            "label": "贴息退回账户"
          }
        },
        {
          "dicount_loan_min_amt": {
            "control": "currency",
            "label": "贴息贷款最小金额",
            "decimal": "2",
            "width": "50%",
            "align": "left",
            "disabled": true,
            "format": {
              "thousand": ","
            }
          },
          "dicount_loan_max_amt": {
            "control": "currency",
            "label": "贴息贷款最大金额",
            "decimal": "2",
            "width": "50%",
            "align": "left",
            "disabled": true,
            "format": {
              "thousand": ","
            }
          }
        }
      ]
    }
  }
}
