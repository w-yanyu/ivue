              "daily_total_loan_check": {
                "control": "select",
                "label": "当日贷总检查",
                "width": "120px",
                "span": 12,
                "disabled": true,
                "dict": {
                  "format": "label",
                  "dictKey": [
                    "MS_E_YESORNO"
                  ],
                  "dictType": "MS_E_YESORNO"
                }
              },
              "daily_total_loan_balance": {
                "control": "string",
                "label": "当日贷总额度",
                "width": "120px",
                "disabled": true,
                "span": 12,
                "disabled": "false"
              }
            },
            {

              "monthly_total_loan_check": {
                "control": "select",
                "label": "当月贷总检查",
                "width": "120px",
                "disabled": true,
                "span": 12,
                "dict": {
                  "format": "label",
                  "dictKey": [
                    "MS_E_YESORNO"
                  ],
                  "dictType": "MS_E_YESORNO"
                }
              },
              "monthly_total_loan_line": {
                "control": "string",
                "label": "当月贷总额度",
                "width": "120px",
                "disabled": true,
                "span": 12,
                "disabled": "false"
              }
            },
            {
              "yearly_total_loan_check": {
                "control": "select",
                "label": "当年贷总检查",
                "width": "120px",
                "disabled": true,
                "span": 12,
                "dict": {
                  "format": "label",
                  "dictKey": [
                    "MS_E_YESORNO"
                  ],
                  "dictType": "MS_E_YESORNO"
                }
              },
              "yearly_total_loan_line": {
                "control": "string",
                "disabled": true,
                "label": "当年贷总额度",
                "width": "120px",
                "span": 12,
                "disabled": "false"
              }
            }



          ]
        },
        {
          "title": "其他信息",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "expand": true,
          "controls": [
            {
              "protocol_status": {
                "control": "select",
                "label": "协议有效状态",
                "width": "120px",
                "disabled": true,
                "span": 12,
                "dict": {
                  "format": "label",
                  "dictKey": [
                    "MS_E_YESORNO"
