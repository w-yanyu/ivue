{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "scope": "templateScope",
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pt0008",
          "part_protocol_no": "${{params.part_protocol_no}}",
          "version": "${{params.version}}"
        }
      },
      "btns": [
      ],
      "controlsGroup": [
        {
          "title": "授信管控",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "expand": true,
          "controls": [

            {
              "daily_credit_check": {
                "control": "select",
                "label": "当日授信检查",
                "width": "120px",
                "disabled": true,
                "span": 12,
                "dict": {
                  "format": "label",
                  "dictKey": [
                    "MS_E_YESORNO"
                  ],
                  "dictType": "MS_E_YESORNO"
                }
              },
              "daily_credit_line": {
                "control": "string",
                "label": "当日授信额度",
                "width": "120px",
                "disabled": true,
                "span": 12,
                "disabled": "false"
              }

            },
            {
              "monthly_credit_check": {
                "control": "select",
                "label": "当月授信检查",
                "width": "120px",
                "disabled": true,
                "span": 12,
                "dict": {
                  "format": "label",
                  "dictKey": [
                    "MS_E_YESORNO"
                  ],
                  "dictType": "MS_E_YESORNO"
                }
              },
              "monthly_credit_line": {
                "control": "string",
                "label": "当月授信额度",
