{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "pt0442",
          "part_protocol_no": "${{params.part_protocol_no}}",
          "version": "${{params.version}}"
        }
      },
      "btns": [
        {
          "type": "submit",
          "icon": "el-icon-check",
          "label": "保存",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "post",
            "params": {
              "servicecode": "pt0443",
              "part_protocol_no": "${{params.part_protocol_no}}",
              "part_project_no": "${{params.part_project_no}}",
              "version": "${{params.version}}"
            }
          },
          "eventAfter": {
            "doLastCallback": false
          }
        }
      ],
      "controls": [
        {
          "cooperation_protocol_no": {
            "control": "hidden",
            "label": "合作业务协议编号",
            "width": "50px",
            "span": 8
          },
          "guarantee_amount": {
            "control": "currency",
            "label": "担保额规模",
            "width": "50px",
            "span": 8,
            "format": {
              "thousand": ",",
              "decimal": 2
            },
            "rules": [
              {
                "required": true,
                "message": "请选择"
              }
            ]
          },
          "warning_amt_value": {
            "control": "currency",
            "label": "额度预警阈值",
            "width": "50px",
            "span": 8,
            "format": {
              "thousand": ",",
              "decimal": 2
            },
            "rules": [
              {
                "required": true,
                "message": "请选择"
              }
            ]
          },
          "ccy_code": {
            "control": "select",
            "label": "币种",
            "width": "50px",
            "span": 8,
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": [
                "MS_CURRENCY"
              ]
            },
            "rules": [
              {
                "required": true,
                "message": "请选择币种"
              }
            ]
          },
          "max_loan_rate": {
            "control": "currency",
            "label": "贷款利率最大值(%)",
            "width": "50px",
            "span": 8,
            "format": {
              "thousand": ",",
              "decimal": 2
            }
          },
          "min_loan_rate": {
            "control": "currency",
            "label": "贷款利率最小值(%)",
            "width": "50px",
            "span": 8,
            "format": {
              "thousand": ",",
              "decimal": 2
            }
          },
          "month_rate_coll_day": {
            "control": "text",
            "label": "每月贷款利息结算日(号)",
            "width": "50px",
            "span": 8,
            "rules": [
              {
                "required": true,
                "message": "请选择"
              },
              {
                "type":"number",
                "message":"请输入整数"
              }
            ]
          },
          "bad_rate": {
            "control": "currency",
            "label": "不良率上限（%）",
            "width": "50px",
            "span": 8,
            "format": {
              "thousand": ",",
              "decimal": 2
            }
          },
          "start_date": {
            "control": "dateTimePicker",
            "label": "开始日期",
            "width": "50px",
            "span": 8,
            "valueFormat": "yyyyMMdd",
            "rules": [
              {
                "required": true,
                "message": "请填写开始日期"
              }
            ]
          },
          "end_date": {
            "control": "dateTimePicker",
            "label": "结束日期",
            "width": "50px",
            "span": 8,
            "valueFormat": "yyyyMMdd",
            "rules": [
              {
                "required": true,
                "message": "请填写结束日期"
              }
            ]
          }
        }
      ]
    }
  }
}
