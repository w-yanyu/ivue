{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "pt0442",
          "part_protocol_no": "${{params.part_protocol_no}}",
          "version": "${{params.version}}"
        }
      },
      "btns": [],
      "controls": [
        {
          "cooperation_protocol_no": {
            "control": "hidden",
            "label": "合作业务协议编号",
            "width": "50px",
            "span": 8
          },
          "part_protocol_no": {
            "control": "hidden",
            "label": "合作方协议编号",
            "width": "50px",
            "span": 8
          },
          "part_project_no": {
            "control": "hidden",
            "label": "合作项目编号",
            "width": "50px",
            "span": 8
          },
          "version": {
            "control": "hidden",
            "label": "版本",
            "width": "50px",
            "span": 8
          },
          "guarantee_amount": {
            "control": "currency",
            "label": "担保额规模",
            "width": "50px",
            "span": 8,
            "disabled": true,
            "format": {
              "thousand": ",",
              "decimal": 2
            }
          },
          "warning_amt_value": {
            "control": "currency",
            "label": "额度预警阈值",
            "width": "50px",
            "span": 8,
            "disabled": true,
            "format": {
              "thousand": ",",
              "decimal": 2
            }
          },
          "ccy_code": {
            "label": "币种",
            "control": "select",
            "width": "50px",
            "span": 8,
            "disabled": false,
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": [
                "MS_CURRENCY"
              ]
            }
          },
          "max_loan_rate": {
            "control": "currency",
            "label": "贷款利率最大值",
            "width": "50px",
            "span": 8,
            "disabled": true,
            "format": {
              "thousand": ",",
              "decimal": 2
            }
          },
          "min_loan_rate": {
            "control": "currency",
            "label": "贷款利率最小值",
            "width": "50px",
            "span": 8,
            "disabled": true,
            "format": {
              "thousand": ",",
              "decimal": 2
            }
          },
          "month_rate_coll_day": {
            "control": "string",
            "label": "每月贷款利息结算日(号)",
            "width": "50px",
            "span": 8
          },
          "bad_rate": {
            "control": "currency",
            "label": "不良率上限（%）",
            "width": "50px",
            "span": 8,
            "disabled": true,
            "format": {
              "thousand": ",",
              "decimal": 2
            }
          },
          "start_date": {
            "control": "dateTimePicker",
            "label": "开始日期",
            "width": "50px",
            "span": 8,
            "valueFormat": "yyyyMMdd",
            "disabled": true
          },
          "end_date": {
            "control": "dateTimePicker",
            "label": "结束日期",
            "width": "50px",
            "span": 8,
            "valueFormat": "yyyyMMdd",
            "disabled": true
          }
        }
      ]
    }
  }
}
