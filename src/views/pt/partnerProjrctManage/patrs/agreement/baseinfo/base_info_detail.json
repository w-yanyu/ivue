{
  "module": "v_realonlyForm_btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pt0115",
          "part_project_no": "${{params.part_project_no}}",
          "protocol_template_no": "${{params.protocol_template_no}}"
        }
      },

      "btns": [
      ],
      "controls": [

        {
          "partner_name": {
            "control": "string",
            "label": "合作方名称",
            "width": "50%"
            },
          "part_method": {
            "control": "string",
            "label": "合作方式",
            "width": "50%",
            "dict": {
              "dictType": "PT_E_PART_METHOD",
              "dictKey": [
                "PT_E_PART_METHOD"
              ],
              "format": "label"
            }
          }
        },
        {
          "part_no": {
            "control": "hidden",
            "label": "合作方编号",
            "width": "50%"
          },
          "protocol_name": {
            "control": "string",
            "label": "协议名称",
            "width": "50%"
          },
          "protocol_status": {
            "control": "string",
            "label": "协议有效状态",
            "width": "50%",
            "value": "${{params.procor}}",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          }
        },
        {
          "effect_date": {
            "control": "string",
            "label": "生效日期",
            "width": "50%"
          },
          "expiry_date": {
            "control": "string",
            "label": "失效日期",
            "width": "50%"
          }
        },
        {
          "continued_day_advan": {
            "control": "string",
            "label": "续签提示提前天数",
            "width": "50%"
          },
          "original_protocol_no": {
            "control": "string",
            "label": "原协议编号",
            "width": "50%"
          }
        },
        {
          "is_exposure_control": {
            "control": "string",
            "label": "是否敞口管控",
            "width": "50%",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "is_margin": {
            "control": "string",
            "label": "是否有保证金",
            "width": "50%",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          }
        },

        {

          "is_commission": {
            "control": "string",
            "label": "是否分佣分润",
            "width": "50%",
            "value": "${{params.procor}}",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },

          "protocol_addr1": {
            "control": "string",
            "label": "协议地址1",
            "width": "50%"
          }
        },
        {
          "protocol_addr2": {
            "control": "string",
            "label": "协议地址2",
            "width": "50%"
          },
          "protocol_addr3": {
            "control": "string",
            "label": "协议地址3",
            "width": "50%"
          }
        }
      ]
    }
  }
}
