{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "pt0436",
          "part_protocol_no": "${{params.part_protocol_no}}",
          "version": "${{params.version}}"
        }
      },
      "btns": [],
      "controls": [
        {
          "margin_protocol_no": {
            "control": "hidden",
            "label": "保证金协议编号",
            "width": "50px",
            "span": 8
          },
          "margin_payment_way": {
            "control": "select",
            "disabled": true,
            "label": "保证金缴纳方式",
            "width": "50px",
            "span": 8,
            "dict": {
              "dictType": "PT_E_MARGIN_PAYMENT_WAY",
              "dictKey": [
                "PT_E_MARGIN_PAYMENT_WAY"
              ],
              "format": "label"
            },
            "showItem": {
              "1": [],
              "2": [
                "payment_percent"
              ]
            },
            "events": {
              "condition": {
                "1": {
                  "control": {
                    "payment_percent": {
                      "value": ""
                    }
                  }
                }
              }
            }
          },
          "payment_percent": {
            "control": "currency",
            "label": "收取比例(%)",
            "width": "50px",
            "span": 8,
            "disabled": true,
            "format": {
              "thousand": ",",
              "decimal": 2
            }
          },
          "first_margin_amt": {
            "control": "currency",
            "label": "首次保证金金额",
            "width": "50px",
            "span": 8,
            "disabled": true,
            "format": {
              "thousand": ",",
              "decimal": 2
            }
          },
          "ccy_code": {
            "control": "select",
            "label": "币种",
            "width": "50px",
            "span": 8,
            "disabled": true,
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": [
                "MS_CURRENCY"
              ]
            }
          },
          "guaranty_no": {
            "control": "text",
            "label": "保证金账号",
            "width": "50px",
            "span": 8,
            "disabled": true
          },
          "margin_acct_name": {
            "control": "text",
            "label": "保证金账户名",
            "width": "50px",
            "span": 8,
            "disabled": true
          },
          "warning_way": {
            "control": "select",
            "label": "预警方式",
            "width": "50px",
            "span": 8,
            "disabled": true,
            "dict": {
              "dictType": "PT_E_WARNING_TYPE",
              "dictKey": [
                "PT_E_WARNING_TYPE"
              ],
              "format": "label"
            }
          },
          "warning_amt_value": {
            "control": "currency",
            "label": "预警金额/比例",
            "width": "50px",
            "span": 8,
            "disabled": true,
            "format": {
              "thousand": ",",
              "decimal": 2
            }
          }
        }
      ]
    }
  }
}
