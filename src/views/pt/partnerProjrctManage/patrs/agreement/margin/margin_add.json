{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "pt0436",
          "part_protocol_no": "${{params.part_protocol_no}}",
          "version": "${{params.version}}"
        }
      },
      "btns": [
        {
          "type": "submit",
          "icon": "el-icon-check",
          "label": "保存",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "post",
            "params": {
              "servicecode": "pt0437",
              "part_protocol_no": "${{params.part_protocol_no}}",
              "part_project_no": "${{params.part_project_no}}",
              "version": "${{params.version}}"
            }
          },
          "eventAfter": {
            "doLastCallback": false
          }
        }
      ],
      "controls": [
        {
          "margin_protocol_no": {
            "control": "hidden",
            "label": "保证金协议编号",
            "width": "50px",
            "span": 8
          },
          "margin_payment_way": {
            "control": "select",
            "label": "保证金缴纳方式",
            "width": "50px",
            "span": 8,
            "dict": {
              "dictType": "PT_E_MARGIN_PAYMENT_WAY",
              "dictKey": [
                "PT_E_MARGIN_PAYMENT_WAY"
              ],
              "format": "label"
            },
            "showItem": {
              "1": [],
              "2": [
                "payment_percent"
              ]
            },
            "events": {
              "condition": {
                "1": {
                  "control": {
                    "payment_percent": {
                      "value": ""
                    }
                  }
                }
              }
            },
            "rules": [
              {
                "required": true,
                "message": "请选择"
              }
            ]
          },
          "payment_percent": {
            "control": "currency",
            "label": "收取比例(%)",
            "width": "50px",
            "span": 8,
            "format": {
              "thousand": ",",
              "decimal": 2
            },
            "rules": [
              {
                "required": true,
                "message": "请填写收取比例"
              }
            ]
          },
          "first_margin_amt": {
            "control": "currency",
            "label": "首次保证金金额",
            "width": "50px",
            "span": 8,
            "format": {
              "thousand": ",",
              "decimal": 2
            },
            "rules": [
              {
                "required": true,
                "message": "请填写首次保证金金额"
              }
            ]
          },
          "ccy_code": {
            "control": "select",
            "label": "币种",
            "width": "50px",
            "span": 8,
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": [
                "MS_CURRENCY"
              ]
            },
            "rules": [
              {
                "required": true,
                "message": "请选择币种"
              }
            ]
          },
          "guaranty_no": {
            "control": "lookup",
            "label": "保证金账号",
            "width": "50px",
            "span": 8,
            "keyField": {
              "guaranty_no": "part_center_no",
              "margin_acct_name": "part_center_name"
            },
            "toRequest": {
              "url": "/views/pt/partnerProjrctManage/patrs/agreement/commissionProfit/account_lookup.json",
              "method": "GET",
              "params": {
                "part_no": "${{params.part_no}}"
              }
            },
            "props": {
              "width": "50%",
              "title": "查询账户"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择"
              }
            ]
          },
          "margin_acct_name": {
            "control": "text",
            "label": "保证金账户名",
            "width": "50px",
            "span": 8,
            "disabled": true
          },
          "warning_way": {
            "control": "select",
            "label": "预警方式",
            "width": "50px",
            "span": 8,
            "dict": {
              "dictType": "PT_E_WARNING_TYPE",
              "dictKey": [
                "PT_E_WARNING_TYPE"
              ],
              "format": "label"
            }
          },
          "warning_amt_value": {
            "control": "currency",
            "label": "预警金额/比例",
            "width": "50px",
            "span": 8,
            "format": {
              "thousand": ",",
              "decimal": 2
            }
          }
        }
      ]
    }
  }
}
