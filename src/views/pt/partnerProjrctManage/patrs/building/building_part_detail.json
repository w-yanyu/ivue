{
  "module": "v_realonlyForm_btn",
  "layout": {
    "form": {
      "scope": "templateScope",
      "initRequest": "${{params}}",
      "controls": [
        {
          "house_name": {
            "control": "string",
            "label": "楼盘小区名称",
            "width": "50px",
            "span": 8
          },
          "house_addr": {
            "label": "楼盘小区地址",
            "width": "50px",
            "span": 8,
            "control": "string"
          },
          "covered_area": {
            "control": "string",
            "label": "占地面积",
            "width": "50px",
            "span": 8
          }
        },
        {

          "built_area": {
            "control": "string",
            "label": "建筑面积",
            "disabled": true,
            "width": "50px",
            "span": 8
          },
          "univlc_avg": {
            "label": "平均单价",
            "width": "50px",
            "span": 8,
            "control": "string"
          },
          "built_year": {
            "label": "建筑年份",
            "width": "50px",
            "span": 8,
            "control": "string"
          }
        },
        {

          "green_rate": {
            "width": "50px",
            "span": 8,
            "label": "绿化率(%)",
            "control": "string"
          },
          "plot_ratio": {
            "control": "string",
            "label": "容积率(%)",
            "width": "50px",
            "span": 8
          },
          "is_new_house": {
            "label": "是否新房",
            "width": "50px",
            "span": 8,
            "control": "string",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "value-label"
            }
          }
        },
        {
          "head_annotations_loan_whethe": {
            "label": "是否总行已批住房开发贷款项目",
            "width": "50px",
            "span": 8,
            "control": "string",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "value-label"
            }
          },
          "handle_estate_properties_whethe": {
            "label": "是否已办理房产“大证”现房楼盘",
            "width": "50px",
            "span": 8,
            "control": "string",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "value-label"
            }
          },
          "urban_properties_whethe": {
            "label": "是否城区楼盘",
            "width": "50px",
            "span": 8,
            "control": "string",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "value-label"
            }
          }
        },
        {
          "margin_deposit_ratio": {
            "control": "string",
            "label": "保证金缴存比例(%)",
            "width": "50px",
            "span": 8,
            "controls": false
          },
          "opening_fund_whethe": {
            "label": "是否开立预售资金监管账户",
            "width": "50px",
            "span": 8,
            "control": "string",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "value-label"
            }
          },
          "stage_guarantee_whethe": {
            "label": "开发商是否提供阶段性担保",
            "width": "50px",
            "span": 8,
            "control": "string",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "value-label"
            }
          }
        },
        {
          "houses_number": {
            "label": "房屋总套数",
            "width": "50px",
            "span": 8,
            "control": "string"
          },
          "risk_rate_type": {
            "label": "风险评级分类",
            "width": "50px",
            "span": 8,
            "control": "string",
            "dict": {
              "dictType": "PT_E_RISK_RATE_TYPE",
              "dictKey": [
                "PT_E_RISK_RATE_TYPE"
              ],
              "format": "value-label"
            }
          },
          "part_house_no": {
            "label": "合作楼号",
            "width": "50px",
            "span": 8,
            "control": "string"
          }
        },
        {
          "house_special": {
            "label": "楼盘特色",
            "width": "50px",
            "span": 24,
            "control": "textarea",
            "placeholder": "(500个字以内,若没有请填“无”)",
            "align": "left",
            "disabled": true,
            "autosize": {
              "minRows": 3,
              "maxRows": 5
            },
            "maxlength": 500,
            "showWordLimit": true
          }
        }
      ]
    }
  }
}
