{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "scope": "strForm",
      "controls":
      [
        {
          "house_name": {
            "control": "text",
            "label": "楼盘小区名称",
            "span": 8
          }
        }
      ]
    },
    "datagrid": {
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "pt0308",
          "part_project_no": "${{params.part_project_no}}",
          "version": "${{params.version}}"
        }
      },
      "thead": [
        {
          "type": "index",
          "label": "序号"
        },
        {
          "label": "楼盘小区名称",
          "prop": "house_name"
        },
        {
          "label": "楼盘小区地址",
          "prop": "house_addr"
        },
        {
          "label":"占地面积",
          "prop": "covered_area"
        },
        {
          "label":  "建筑面积",
          "prop": "built_area"
        },
        {
          "label":  "平均单价",
          "prop": "univlc_avg"
        },
        {
          "label": "建筑年份",
          "prop": "built_year"

        },
        {
          "label":  "绿化率(%)",
          "prop": "green_rate"
        },
        {
          "label":  "容积率(%)",
          "prop": "plot_ratio"
        },
        {
          "label":  "是否新房",
          "prop": "is_new_house",
          "dict": {
            "dictType": "MS_E_YESORNO",
            "dictKey": [
              "MS_E_YESORNO"
            ],
            "format": "value-label"
          }
        },
        {
          "label":  "是否总行已批住房开发贷款项目",
          "prop": "head_annotations_loan_whethe",
          "dict": {
            "dictType": "MS_E_YESORNO",
            "dictKey": [
              "MS_E_YESORNO"
            ],
            "format": "value-label"
          }
        },
        {
          "label":  "是否已办理房产“大证”现房楼盘",
          "prop": "handle_estate_properties_whethe",
          "dict": {
            "dictType": "MS_E_YESORNO",
            "dictKey": [
              "MS_E_YESORNO"
            ],
            "format": "value-label"
          }
        },
        {
          "label":  "是否城区楼盘",
          "prop": "urban_properties_whethe",
          "dict": {
            "dictType": "MS_E_YESORNO",
            "dictKey": [
              "MS_E_YESORNO"
            ],
            "format": "value-label"
          }
        },
        {
          "label":  "保证金缴存比例(%)",
          "prop": "margin_deposit_ratio"
        },
        {
          "label":  "是否开立预售资金监管账户",
          "prop": "opening_fund_whethe",
          "dict": {
            "dictType": "MS_E_YESORNO",
            "dictKey": [
              "MS_E_YESORNO"
            ],
            "format": "value-label"
          }
        },
        {
          "label":  "开发商是否提供阶段性担保",
          "prop": "stage_guarantee_whethe",
          "dict": {
            "dictType": "MS_E_YESORNO",
            "dictKey": [
              "MS_E_YESORNO"
            ],
            "format": "value-label"
          }
        },
        {
          "label":  "房屋总套数",
          "prop": "houses_number"
        },
        {
          "label":  "合作楼号",
          "prop": "part_house_no"
        },
        {
          "label":  "楼盘特色",
          "prop": "house_special"
        },

        {
          "label": "操作",
          "width": "260px",
          "btns": [
            {
              "type": "look",
              "label": "详情",
              "toRequest": {
                "url": "/views/pt/partnerProjrctManage/patrs/building/building_part_detail.json",
                "method": "GET"
              },
              "props": {
                "width": "65%",
                "title":  "查看详情"
              }
            }
          ]
        }
      ]
    }
  }
}
