{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "scope": "templateScope",
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pt0306",
          "part_project_no": "${{params.part_project_no}}",
          "version": "${{params.version}}"
        }
      },
      "controls": [
        {
          "house_name": {
            "control": "lookup",
            "label": "楼盘小区名称",
            "width": "50px",
            "span": 8,
            "placeholder": "请选择楼盘小区名称",
            "edit": false,
            "keyField": {
              "house_name": "house_name",
              "house_id": "house_id",
              "house_addr": "house_addr",
              "built_area": "area_msg",
              "univlc_avg": "avg_prise"
            },
            "toRequest": {
              "url": "/views/pt/partnerProjrctManage/patrs/building/building_part_lookup.json",
              "method": "POST"
            },
            "props": {
              "title": "选择楼盘信息"
            },
            "rules": [
              {
                "required": true,
                "message": "楼盘小区名称名称不得为空"
              },
              {
                "max": 80,
                "message": "长度不能超过80个字符"
              }
            ]
          },
          "house_id": {
            "label": "楼盘编号",
            "width": "50px",
            "span": 8,
            "control": "hidden"
          },
          "house_addr": {
            "label": "楼盘小区地址",
            "width": "50px",
            "span": 8,
            "control": "text",
            "disabled": true,
            "rules": [
              {
                "required": true,
                "message": "楼盘小区地址不得为空"
              },
              {
                "max": 80,
                "message": "长度不能超过80个字符"
              }
            ]
          },
          "built_area": {
            "control": "text",
            "label": "建筑面积",
            "width": "50px",
            "span": 8,
            "disabled": true,
            "rules": [
              {
                "required": false,
                "message": "建筑面积不得为空"
              }
            ]
          }
        },
        {
          "covered_area": {
            "control": "text",
            "label": "占地面积",
            "width": "50px",
            "span": 8,
            "rules": [
              {
                "required": false,
                "message": "占地面积不能为空!"
              }
            ]
          },
          "univlc_avg": {
            "label": "平均单价",
            "width": "50px",
            "span": 8,
            "control": "currency",
            "disabled": true,
            "rules": [
              {
                "required": false,
                "message": "请输入平均单价"
              }
            ]
          },
          "built_year": {
            "label": "建筑年份",
            "width": "50px",
            "span": 8,
            "control": "dateTimePicker",
            "type": "year",
            "format": "yyyy",
            "rules": [
              {
                "required": false,
                "message": "请输入建筑年份"
              }
            ]
          }
        },
        {
          "green_rate": {
            "width": "50px",
            "span": 8,
            "label": "绿化率(%)",
            "control": "inputNumber",
            "max": 100,
            "min": 0,
            "controls": false,
            "rules": [
              {
                "required": false,
                "message": "绿化率不得为空"
              }
            ]
          },
          "plot_ratio": {
            "control": "inputNumber",
            "label": "容积率(%)",
            "width": "50px",
            "span": 8,
            "controls": false,
            "rules": [
              {
                "required": false,
                "message": "容积率不得为空"
              }
            ]
          },
          "is_new_house": {
            "label": "是否新房",
            "width": "50px",
            "span": 8,
            "control": "select",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "value-label"
            }
          }
        },
        {
          "head_annotations_loan_whethe": {
            "label": "是否总行已批住房开发贷款项目",
            "width": "50px",
            "span": 8,
            "control": "select",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "value-label"
            }
          },
          "handle_estate_properties_whethe": {
            "label": "是否已办理房产“大证”现房楼盘",
            "width": "50px",
            "span": 8,
            "control": "select",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "value-label"
            }
          },
          "urban_properties_whethe": {
            "label": "是否城区楼盘",
            "width": "50px",
            "span": 8,
            "control": "select",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "value-label"
            }
          }
        },
        {
          "margin_deposit_ratio": {
            "control": "inputNumber",
            "label": "保证金缴存比例(%)",
            "width": "50px",
            "span": 8,
            "controls": false
          },
          "opening_fund_whethe": {
            "label": "是否开立预售资金监管账户",
            "width": "50px",
            "span": 8,
            "control": "select",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "value-label"
            }
          },
          "stage_guarantee_whethe": {
            "label": "开发商是否提供阶段性担保",
            "width": "50px",
            "span": 8,
            "control": "select",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "value-label"
            }
          }
        },
        {
          "houses_number": {
            "label": "房屋总套数",
            "width": "50px",
            "span": 8,
            "control": "text"
          },
          "risk_rate_type": {
            "label": "风险评级分类",
            "width": "50px",
            "span": 8,
            "control": "select",
            "dict": {
              "dictType": "PT_E_RISK_RATE_TYPE",
              "dictKey": [
                "PT_E_RISK_RATE_TYPE"
              ],
              "format": "value-label"
            }
          },
          "part_house_no": {
            "label": "合作楼号",
            "width": "50px",
            "span": 8,
            "control": "text"
          }
        },
        {
          "house_special": {
            "control": "textarea",
            "label": "楼盘特色",
            "width": "50px",
            "span": 24,
            "placeholder": "(500个字以内,若没有请填“无”)",
            "align": "left",
            "autosize": {
              "minRows": 3,
              "maxRows": 5
            },
            "maxlength": 500,
            "showWordLimit": true
          }
        }
      ]
    }
  }
}
