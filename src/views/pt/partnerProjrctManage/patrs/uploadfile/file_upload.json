{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ar0201",
          "apply_no": "${{params.apply_no}}"
        }
      },
      "pagination": {
        "pageSizes": [
          10,
          15,
          20,
          50,
          100
        ],
        "pageSize": 10
      },
      "thead": [
        {
          "label": "序号",
          "type": "index",
          "align": "center"
        },
        {
          "label": "资料编号",
          "prop": "doc_no",
          "width": "300px",
          "align": "center"
        },
        {
          "label": "资料描述",
          "prop": "doc_name",
          "align": "center"
        },
        {
          "label": "是否必需",
          "prop": "is_must",
          "width": "150px",
          "align": "center",
          "dict": {
            "dictType": "MS_E_YESORNO",
            "dictKey": [
              "MS_E_YESORNO"
            ],
            "format": "label"
          }
        },
        {
          "label": "上传标志",
          "prop": "upload_flag",
          "width": "150px",
          "align": "center",
          "dict": {
            "dictType": "MS_E_YESORNO",
            "dictKey": [
              "MS_E_YESORNO"
            ],
            "format": "label"
          }
        },
        {
          "label": "操作",
          "fixed": "right",
          "width": 200,
          "align": "center",
          "btns": [
            {
              "type": "edit",
              "label": "资料管理",
              "toRequest": {
                "url": "/views/icms/docManager/docApplyCheck/file_manage.json",
                "method": "POST",
                "params": {
                  "doc_no": "${{current.doc_no}}",
                  "apply_no": "${{current.apply_no}}"
                }
              },
              "props": {
                "width": "65%",
                "title": "资料管理"
              }
            }
          ]
        }
      ]
    }
  }
}
