{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest":"${{params}}",
      "btns": [],
      "controlsGroup": [
        {
          "title": "客户信息",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "accordion": false,
          "expand": true,
          "controls": [
            {
              "cust_no": {
                "control": "text",
                "label": "客户编号",
                "width": "140px",
                "span": 8,
                "disabled": true
              },
              "cust_name": {
                "control": "text",
                "label": "客户姓名",
                "width": "140px",
                "span": 8,
                "disabled": true
              },
              "cert_type": {
                "control": "select",
                "label": "证件类型",
                "width": "140px",
                "span": 8,
                "dict": {
                  "dictType": "MS_E_CRET_TYPE",
                  "dictKey": [
                    "MS_E_CRET_TYPE"
                  ],
                  "format": "label"
                },
                "disabled": true
              },
              "cert_no": {
                "control": "text",
                "label": "证件号码",
                "width": "140px",
                "span": 8,
                "disabled": true
              },
              "mobile_no": {
                "control": "text",
                "label": "手机号码",
                "width": "140px",
                "span": 8,
                "disabled": true
              }
            }
          ]
        },
        {
          "title": "定制信息",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "accordion": false,
          "expand": true,
          "controls": [
            {
              "ccy_code": {
                "control": "select",
                "label": "币种",
                "width": "140px",
                "span": 8,
                "dict": {
                  "dictType": "MS_CURRENCY",
                  "dictKey": [
                    "MS_CURRENCY"
                  ],
                  "format": "label"
                },
                "disabled": true
              },
              "loan_term": {
                "control": "text",
                "label": "贷款期限（月）",
                "width": "140px",
                "span": 8,
                "suffix": "M",
                "disabled": true
              },
              "max_credit_amt": {
                "control": "currency",
                "label": "最大授信额度（元）",
                "width": "140px",
                "span": 8,
                "format": {
                  "thousand": ",",
                  "decimal": "2"
                },
                "disabled": true
              },
              "min_credit_amt": {
                "control": "currency",
                "label": "最小授信额度（元）",
                "width": "140px",
                "span": 8,
                "format": {
                  "thousand": ",",
                  "decimal": "2"
                },
                "disabled": true
              },
              "credit_amt": {
                "control": "currency",
                "label": "授信额度（元）",
                "width": "140px",
                "span": 8,
                "format": {
                  "thousand": ",",
                  "decimal": "2"
                },
                "disabled": true
              },
              "interest_rate": {
                "control": "currency",
                "label": "年利率（%）",
                "width": "140px",
                "span": 8,
                "format": {
                  "thousand": ",",
                  "decimal": "2"
                },
                "disabled": true
              },
              "repay_type_list": {
                "control": "select",
                "label": "还款方式",
                "width": "140px",
                "span": 24,
                "multiple": true,
                "disabled": true,
                "dict": {
                  "dictType": "MS_E_REPAY_WAY",
                  "dictKey": [
                    "MS_E_REPAY_WAY"
                  ],
                  "format": "label"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
