{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest":"${{params}}",
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "pt0449",
          "part_protocol_no": "${{params.part_protocol_no}}",
          "part_project_no": "${{params.part_project_no}}",
          "version": "${{params.version}}",
          "protocol_whitelist_no": "${{params.protocol_whitelist_no}}",
          "cust_type": "10"
        }
      },
      "controlsGroup": [
        {
          "title": "客户信息",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "accordion": false,
          "expand": true,
          "controls": [
            {
              "cust_no": {
                "control": "lookup",
                "label": "客户编号",
                "width": "140px",
                "span": 8,
                "edit": true,
                "disabled": true,
                "keyField": {
                  "cust_no": "cust_no",
                  "cust_name": "cust_name",
                  "cert_type": "cert_type",
                  "cert_no": "cert_no",
                  "mobile_no": "mobile_no"
                },
                "toRequest": {
                  "url": "/views/us/cust_lookup_for_private.json",
                  "method": "post"
                },
                "multiple": false,
                "props": {
                  "title": "客户选择"
                }
              },
              "cust_name": {
                "control": "text",
                "label": "客户姓名",
                "width": "140px",
                "span": 8,
                "disabled": true
              },
              "cert_type": {
                "control": "select",
                "label": "证件类型",
                "width": "140px",
                "span": 8,
                "disabled": true,
                "dict": {
                  "dictType": "US_E_PRI_CRET_TYPE",
                  "dictKey": [
                    "US_E_PRI_CRET_TYPE"
                  ],
                  "format": "label"
                }
              },
              "cert_no": {
                "control": "text",
                "label": "证件号码",
                "width": "140px",
                "span": 8,
                "disabled": true
              },
              "mobile_no": {
                "control": "text",
                "label": "手机号码",
                "width": "140px",
                "span": 8,
                "rules": [
                  {
                    "required": true,
                    "message": "请输入手机号码"
                  }
                ]
              }
            }
          ]
        },
        {
          "title": "定制信息",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "accordion": false,
          "expand": true,
          "controls": [
            {
              "ccy_code": {
                "control": "select",
                "label": "币种",
                "width": "140px",
                "span": 8,
                "dict": {
                  "dictType": "MS_CURRENCY",
                  "dictKey": [
                    "MS_CURRENCY"
                  ],
                  "format": "label"
                }
              },
              "loan_term": {
                "control": "text",
                "label": "贷款期限（月）",
                "width": "140px",
                "span": 8,
                "suffix": "M"
              },
              "max_credit_amt": {
                "control": "currency",
                "label": "最大授信额度（元）",
                "width": "140px",
                "span": 8,
                "format": {
                  "thousand": ",",
                  "decimal": "2"
                }
              },
              "min_credit_amt": {
                "control": "currency",
                "label": "最小授信额度（元）",
                "width": "140px",
                "span": 8,
                "format": {
                  "thousand": ",",
                  "decimal": "2"
                }
              },
              "credit_amt": {
                "control": "currency",
                "label": "授信额度（元）",
                "width": "140px",
                "span": 8,
                "format": {
                  "thousand": ",",
                  "decimal": "2"
                }
              },
              "interest_rate": {
                "control": "currency",
                "label": "年利率（%）",
                "width": "140px",
                "span": 8,
                "format": {
                  "thousand": ",",
                  "decimal": "2"
                }
              },
              "repay_type_list": {
                "control": "select",
                "label": "还款方式",
                "width": "140px",
                "span": 24,
                "multiple": true,
                "dict": {
                  "dictType": "MS_E_REPAY_WAY",
                  "dictKey": [
                    "MS_E_REPAY_WAY"
                  ],
                  "format": "label"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
