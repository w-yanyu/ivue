{
  "module": "d-steps",
  "layout": {
    "steps": {
      "direction": "horizontal",
      "alignCenter": true,
      "finishStatus": "success",
      "simpleType": false,
      "nextStepType": "validate",
      "skip": true,
      "list": [
        {
          "title": "基本信息展示",
          "icon": "el-icon-edit",
          "toRequest": {
            "url": "/views/pm/pm_consult_uphold/pm_consult_uphole_updatatab1.json",
            "method": "post",
            "params": {
              "ofpacd": "${{params.ofpacd}}",
              "seqnum": "${{params.seqnum}}"

            }
          },
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "upcoag",
              "seqnum": "${{params.seqnum}}"
            }
          }
        },
        {
          "title": "提交审核",
          "icon": "el-icon-edit",
          "toRequest": {
            "url": "/views/pm/pm_consult_uphold/pm_consult_uphole_submit.json",
            "method": "post",
            "params": {
              "ofpacd": "${{params.ofpacd}}",
              "seqnum": "${{params.seqnum}}"

            }
          },
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "chagin",
              "ofpacd": "${{params.ofpacd}}",
              "seqnum": "${{params.seqnum}}",
              "statusxy":"02"
            }
          }
        }
      ]
    }
  }
}
