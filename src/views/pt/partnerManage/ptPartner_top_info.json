{
  "module": "dynamicTabs",
  "layout": {
    "tabs": {
      "scope": "formDate",
      "tabHeader": [
        {
          "id": "partnerInfo",
          "label": "合作方信息",
          "closable": false,
          "disabled": false,
          "icon": "el-icon-date",
          "toRequest": {
            "url": "/views/pt/partnerManage/ptPartner_info.json",
            "method": "GET",
            "params": {
              "part_no": "${{params.part_no}}",
              "center_object_type":"${{params.center_object_type}}",
              "cust_no":"${{params.cust_no}}",
              "center_cert_type":"${{params.center_cert_type}}",
              "center_cert_no":"${{params.center_cert_no}}",
              "center_name":"${{params.center_name}}",
              "center_brand":"${{params.center_brand}}",
              "center_status":"${{params.center_status}}",
              "warn_status":"${{params.warn_status}}",
              "start_date":"${{params.start_date}}",
              "take_effect_date":"${{params.take_effect_date}}",
              "invalid_date":"${{params.invalid_date}}"
            }
          }
        }
      ],
      "defaultActiveKey": "partnerInfo",
      "position": "top",
      "styleClass": "card"
    }
  }
}
