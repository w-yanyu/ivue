                    }
                  }
                },
                {
                    "title": "代偿协议",
                    "forbidBack": true,
                  "scope": "baseInfo5",
                    "icon": "el-icon-edit",
                  "toRequest": {
                    "url": "/views/pt/pt_consult_uphold/pt_consult_uphole_addcompensatory.json",
