                      "part_protocol_no": "${{newPart.baseInfo4.part_protocol_no}}",
                      "part_no": "${{newPart.baseInfo.part_no}}"

                    }
                  },
                  "doRequest": {
                    "url": "/SUMP/icmcall/icmRPCCall",
                    "method": "POST",
                    "params": {
                      "servicecode": "pt0121"
                    }
                  }
                },

                {
                    "title": "贴息协议",
                    "forbidBack": true,
                  "scope": "baseInfo6",
                    "icon": "el-icon-edit",
                  "toRequest": {
                    "url": "/views/pt/pt_consult_uphold/pt_consult_uphole_addinst.json",
