                    }
                  },
                  "doRequest": {
                    "url": "/SUMP/icmcall/icmRPCCall",
                    "method": "POST",
                    "params": {
                      "servicecode": "pt0006"
                    }
                  }
                },
                {
                    "title": "引流协议",
                    "forbidBack": true,
                  "scope": "baseInfo4",
