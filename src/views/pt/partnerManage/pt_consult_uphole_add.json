                    "method": "post",
                    "params": {
                    }
                  },
                  "doRequest": {
                    "url": "/SUMP/icmcall/icmRPCCall",
                    "method": "POST",
                    "params": {
                      "servicecode": "pt0002"
                    },
                    "resultSet": {
                      "part_protocol_no": "part_protocol_no"
                    }
                  }
                },

                {
                    "title": "合作体系",
                    "forbidBack": true,
                     "scope": "baseInfo1",
