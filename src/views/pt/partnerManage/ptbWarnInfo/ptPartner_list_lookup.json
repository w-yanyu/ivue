{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "controls": [
                {
                  "part_no": {
                    "control": "text",
                    "label": "合作方编号"
        
                  },
                  "cust_no": {
                    "control": "text",
                    "label": "客户编号"
                  }
                },
                {
                  "center_name": {
                    "control": "text",
                    "label": "合作方名称"
                  },
                  "center_cert_type": {
                    "control": "select",
                    "label": "合作方证件类型",
                     "dict": {
                       "dictType": "MS_E_CRET_TYPE",
                       "dictKey": [
                         "MS_E_CRET_TYPE"
                       ],
                       "format": "label"
                     }
                  }
                },
                {
                  "center_cert_no": {
                    "control": "text",
                    "label": "合作方证件号码"
        
                  },
                  "center_status": {
                    "control": "select",
                    "label": "合作方状态",
                     "dict": {
                       "dictType": "PT_E_CENTER_STATUS",
                       "dictKey": [
                         "PT_E_CENTER_STATUS"
                       ],
                       "format": "label"
                     }
                  }
        
                },
                {
                  "warn_status": {
                    "control": "select",
                    "label": "预警状态",
                     "dict": {
                       "dictType": "PT_E_WARN_STATUS",
                       "dictKey": [
                         "PT_E_WARN_STATUS"
                       ],
                       "format": "label"
                     }
                  }
                }
        
              ]
        },
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": false,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "center_status":"1",
                    "servicecode": "pt0040"
                }
            },
            "thead": [

                {
                    "type": "index"
                  },
                  {
                    "label": "合作方编号",
                    "prop": "part_no",
                    "align":"center"
                  },
                  {
                    "label":"合作方对象类型",
                    "prop": "center_object_type",
                    "align":"center",
                    "dict": {
                      "dictType": "PT_E_CENTER_OBJECT_TYPE",
                      "dictKey": [
                        "PT_E_CENTER_OBJECT_TYPE"
                      ],
                      "format": "label"
                    }
                  },
                  {
                    "label": "客户编号",
                    "prop": "cust_no",
                    "align":"center"
                  },
                  {
                    "label": "合作方证件类型",
                    "prop": "center_cert_type",
                    "align":"center",
                    "dict": {
                      "dictType": "MS_E_CRET_TYPE",
                      "dictKey": [
                        "MS_E_CRET_TYPE"
                      ],
                      "format": "label"
                    }
                  },
                  {
                    "label": "合作方证件号码",
                    "prop": "center_cert_no",
                    "align":"center"
                  },
                  {
                    "label": "合作方名称",
                    "prop": "center_name",
                    "align":"center"
                  },
                  {
                    "label": "合作方品牌",
                    "prop": "center_brand",
                    "align":"center"
                  },
                  {
                    "label": "合作方状态",
                    "prop": "center_status",
                    "align":"center",
                    "dict": {
                      "dictType": "PT_E_CENTER_STATUS",
                      "dictKey": [
                        "PT_E_CENTER_STATUS"
                      ],
                      "format": "label"
                    }
                  },
                  {
                    "label": "预警状态",
                    "prop": "warn_status",
                    "align":"center",
                    "dict": {
                      "dictType": "PT_E_WARN_STATUS",
                      "dictKey": [
                        "PT_E_WARN_STATUS"
                      ],
                      "format": "label"
                    }
                  },
                  {
                    "label": "登记日期",
                    "prop": "start_date",
                    "align":"center"
                  },
                  {
                    "label": "生效日期",
                    "prop": "take_effect_date",
                    "align":"center"
                  },
                  {
                    "label": "失效日期",
                    "prop": "invalid_date",
                    "align":"center"
                  },               
                  {
                    "label": "操作",
                    "align": "center",
                    "fixed": "right",
                    "btns": [
                        {
                            "type": "bringback",
                            "label": "选择"
                        }
                    ]
                  }
            ]
        }
    }
}