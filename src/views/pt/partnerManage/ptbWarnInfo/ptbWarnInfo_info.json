{
    "module": "v-realonlyForm-btn",
    "layout": {
        "form": {
            "scope": "formData",
            "initRequest": "${{params}}",
            "controls": [
                {
                    "seq_no": {
                        "control": "hidden",
                        "label": "流水号",
                        "placeholder": "请输入",
                        "rules": [{
                            "required": true,
                            "message": "请输入"
                        }]
                    },
                    "warn_busi_type": {
                        "control": "select",
                        "label": "预警业务类型",
                        "placeholder": "请选择",
                        "disabled":true,
                        "dict": {
                            "dictType": "PT_E_WARN_BUSI_TYPE",
                            "dictKey": [
                                "PT_E_WARN_BUSI_TYPE"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": true,
                            "message": "请选择"
                        }]
                    },
                    "warn_code": {
                        "control": "text",
                        "label": "预警码",
                        "width":"130px",
                        "disabled":true,
                        "placeholder": "请输入",
                        "rules": [{
                            "required": true,
                            "message": "请输入"
                        }]
                    }
                },
                {
                    "warn_describe": {
                        "control": "text",
                        "label": "预警描述",
                        "disabled":true,
                        "value": "${{params.part_no}}",
                        "placeholder": "请输入"
                    },
                    "warn_check_serv": {
                        "control": "text",
                        "label": "预警检查服务",
                        "width":"130px",
                        "disabled":true,
                        "placeholder": "请输入",
                        "rules": [{
                            "required": true,
                            "message": "请输入"
                        }]
                    }
                },
                {
                    "part_project_no": {
                        "control": "text",
                        "label": "合作项目编号",
                        "disabled":true,
                        "placeholder": "请输入",
                        "rules": [{
                            "required": true,
                            "message": "请输入"
                        }]
                    },
                    "part_protocol_no": {
                        "control": "text",
                        "label": "合作方协议编号",
                        "disabled":true,
                        "width":"130px",
                        "placeholder": "请输入",
                        "rules": [{
                            "required": true,
                            "message": "请输入"
                        }]
                    }
                },
                {
                    "part_no": {
                        "control": "text",
                        "label": "合作方编号",
                        "placeholder": "请输入",
                        "disabled":true,
                        "rules": [{
                            "required": true,
                            "message": "请输入"
                        }]
                    },
                    "warn_time": {
                        "label": "预警时间",
                        "disabled":true,
                        "width": "130px",
                        "control": "dateTimePicker",
                        "format": "yyyyMMdd",
                        "rules": [{
                            "required": true,
                            "message": "请输入登记日期"
                        }]
                    }
                },
                {
                    "warn_action": {
                        "control": "text",
                        "label": "预警动作",
                        "placeholder": "请输入",
                        "disabled": true,
                        "rules": [{
                            "required": true,
                            "message": "请输入"
                        }]
                    }
                }


            ]
        }
    }
}
