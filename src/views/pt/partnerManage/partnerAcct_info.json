{
    "module": "v-realonlyForm-btn",
    "layout": {
        "form": {
            "scope": "formData",
            "initRequest": "${{params}}",
            "controls": [
                {
                    "center_account_no": {
                        "label": "合作方结算账户",
                        "control": "hidden",
                        "disabled": true
                    },
                    "center_account_name": {
                        "control": "string",
                        "label": "合作账户简称",
                        "placeholder": "请输入",
                        "rules": [{
                            "message": "请输入"
                        }]
                    },
                    "account_status": {
                        "control": "string",
                        "label": "账户状态",
                        "placeholder": "请选择",
                        "dict": {
                            "dictType": "PT_E_ACCOUNT_STATUS",
                            "dictKey": [
                                "PT_E_ACCOUNT_STATUS"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "message": "请选择"
                        }]
                    }
                },
                {
                    "part_no": {
                        "control": "string",
                        "label": "合作方账号",
                        "disabled": true
                    },
                    "part_center_no": {
                        "control": "string",
                        "label": "合作方账户账号",
                        "placeholder": "请输入",
                        "rules": [{
                            "message": "请输入"
                        }]
                    }
                },
                {
                    "part_center_name": {
                        "control": "string",
                        "label": "合作方账户名称",
                        "placeholder": "请输入",
                        "rules": [{
                            "message": "请输入"
                        }]
                    },
                    "open_bank_no": {
                        "control": "string",
                        "label": "开户行号",
                        "placeholder": "请输入",
                        "rules": [{
                            "message": "请输入"
                        }]
                    }
                },
                {
                  "open_bank_name": {
                      "control": "string",
                      "label": "开户行名",
                      "placeholder": "请输入",
                      "rules": [{
                          "message": "请输入"
                      }]
                  },
                    "is_my_bank_no": {
                        "control": "string",
                        "label": "是否我行账户",
                        "placeholder": "请选择",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "message": "请选择"
                        }]
                    }
                }


            ]
        }
    }
}
