{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
          "scope": "templateScope",
          "initRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "pt0170",
                "part_no": "${{params.part_no}}"
                
              }
          },
          "initDataOnce":true,
  
  
            "btns": [
 
            ],
            "controls": [
                {
                    "part_no": {
                        "control": "text",
                        "label": "合作方账号",
                        "width": "130px",
                        "disabled":true
                    }

                },
                {
                    "cust_no": {
                        "control": "text",
                        "label": "客户编号",
                        "width": "130px",
                        "disabled":true
                    },
                    "center_cert_type": {
                        "control": "select",
                        "label": "合作方证件类型",
                        "width": "130px",
                        "dict": {
                            "dictType": "MS_E_CRET_TYPE",
                            "dictKey": [
                                "MS_E_CRET_TYPE"
                            ],
                            "format": "label"
                        },
                        "disabled":true
                    }
                },
                {
                  "center_cert_no": {
                      "control": "text",
                      "label": "合作方证件号码",
                      "width": "130px",
                      "disabled":true
                  },
                  "center_name": {
                      "control": "text",
                      "label": "合作方名称",
                      "width": "130px",
                      "disabled":true
                  }
    
                },
                {
                    "center_brand": {
                        "control": "text",
                        "label": "合作方品牌",
                        "width": "130px",
                        "disabled":true
                    },
                    "center_status": {
                        "control": "select",
                        "label": "合作方状态",
                        "disabled":true,
                        "width": "130px",
                        "dict": {
                            "dictType": "PT_E_CENTER_STATUS",
                            "dictKey": [
                                "PT_E_CENTER_STATUS"
                            ],
                            "format": "label"
                        }
    
                    }
                },
                {
                    "warn_status": {
                        "control": "select",
                        "label": "预警状态",
                        "disabled":true,
                        "width": "130px",
                        "dict": {
                            "dictType": "PT_E_WARN_STATUS",
                            "dictKey": [
                                "PT_E_WARN_STATUS"
                            ],
                            "format": "label"
                        }
    
                    },
                    "start_date": {
                        "label": "登记日期",
                        "width": "130px",
                        "control": "dateTimePicker",
                        "disabled":true,
                        "editable": true,
                        "format": "yyyyMMdd"
                    }
    
                },
                {
                  "take_effect_date": {
                      "label": "生效日期",
                      "width": "130px",
                      "control": "dateTimePicker",
                      "editable": true,
                      "disabled":true,
                      "format": "yyyyMMdd"
                  },
                  "invalid_date": {
                      "label": "失效日期",
                      "width": "130px",
                      "disabled":true,
                      "control": "dateTimePicker",
                      "editable": true,
                      "format": "yyyyMMdd"
                  }
    
                }
    
             ]
        }
    }
  }
  