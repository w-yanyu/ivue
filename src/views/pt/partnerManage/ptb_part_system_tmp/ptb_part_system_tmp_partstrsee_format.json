            "control": "text",
            "label": "商户所属机构",
            "disabled": true,
            "span": 8,
            "rules": [
              {
                "max": 200,
                "message": "长度不能超过200个字符"
              },
              {
                "required": false,
                "message": "请输入商户所属机构"
              }
            ]
          },
          "register_address": {
            "control": "areaCascade",
            "label": "注册地址",
            "disabled":true,
            "span":8,
            "rules": [
              {
                "max": 100,
                "message": "长度不能超过100个字符"
              },
              {
                "required": false,
                "message": "请输入网点详细地址"
              }
            ]
          }
        },
        
        {
          "acct_no": {
            "control": "hidden",
            "label": "清算账号",
            "disabled": true,
            "span": 8,
            "rules": [
              {
                "max": 40,
                "message": "长度不能超过40个字符"
              },
              {
                "required": false,
                "message": "请输入清算账号"
              }
            ]
          },
          "acct_name": {
            "control": "hidden",
            "label": "清算账号名称",
            "disabled": true,
            "span": 8,
            "rules": [
              {
                "max": 130,
                "message": "长度不能超过130个字符"
              },
              {
                "required": false,
                "message": "请输入清算账号名称"
              }
            ]
          },
          "bank_no": {
            "control": "hidden",
            "label": "联行号",
            "disabled": true,
            "span": 8,
            "rules": [
              {
                "max": 30,
                "message": "长度不能超过30个字符"
              },
              {
                "required": false,
                "message": "请输入清算账号名称"
              }
            ]
          }
        },
