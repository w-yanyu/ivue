          "open_bank_city": {
            "control": "hidden",
            "label": "开户行城市",
            "disabled": true,
            "span": 8,
            "rules": [
              {
                "max": 100,
                "message": "长度不能超过100个字符"
              },
              {
                "required": false,
                "message": "请输入开户行城市"
              }
            ]
