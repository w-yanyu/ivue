{
  "module": "v-realonlyForm-btn",
  "layout": {
    "initReqeust": "${{params}}",
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pt0193",
          "part_no":"${{params.part_no}}",
          "node_no": "${{params.part_no}}"
        }
      },
      "controls": [
        {
          "node_no": {
            "control": "string",
            "label": "节点编号",
            "span": 8
          },
          "node_name": {
            "control": "string",
            "label": "节点名称",
            "span": 8
          },
          "part_no": {
            "control": "string",
            "label": "合作方账号",
            "span": 8
          }
        },
        {
          "node_level": {
            "control": "hidden",
            "label": "节点层级",
            "span": 8
          },
          "parent_node_no": {
            "control": "hidden",
            "label": "父级节点",
            "span": 8
          },
          "is_leaf": {
            "control": "hidden",
            "label": "是否叶子节点",
            "span": 8
          }
        },
        {
          "cooperative_subject_name": {
            "control": "string",
            "label": "合作主体名称",
            "disabled": true,
            "span": 8,
            "rules": [
              {
                "max": 66,
                "message": "长度不能超过66个字符"
              },
              {
                "required": false,
                "message": "请输入合作主体名称"
              }
            ]
          },
          "partner_cret_no": {
            "control": "text",
            "label": "合作方证件号码",
            "disabled": true,
            "span": 8,
            "rules": [
              {
                "max": 32,
                "message": "长度不能超过20个字符"
              },
              {
                "required": false,
                "message": "请输入合作方证件号码"
              }
            ]
          },
          "issue_admin": {
            "control": "text",
            "label": "发证机关",
            "disabled": true,
            "span": 8,
            "rules": [
              {
                "max": 100,
                "message": "长度不能超过100个字符"
              },
              {
                "required": false,
                "message": "请输入发证机关"
              }
            ]
          }
        },
        {
          "cret_start_date": {
            "control": "text",
            "label": "证件签发日期",
            "disabled": true,
            "span": 8,
            "rules": [
              {
                "max": 8,
                "message": "长度不能超过8个字符"
              },
              {
                "required": false,
                "message": "请输入证件签发日期"
              }
            ]
          },
          "cret_end_date": {
            "control": "text",
            "label": "证件到期日期",
            "disabled": true,
            "span": 8,
            "rules": [
              {
                "max": 8,
                "message": "长度不能超过8个字符"
              },
              {
                "required": false,
                "message": "请输入证件到期日期"
              }
            ]
          },
          "business_license_no": {
            "control": "text",
            "label": "营业执照号码",
            "disabled": true,
            "span": 8,
            "rules": [
              {
                "max": 20,
                "message": "长度不能超过20个字符"
              },
              {
                "required": false,
                "message": "请输入营业执照号码"
              }
            ]
