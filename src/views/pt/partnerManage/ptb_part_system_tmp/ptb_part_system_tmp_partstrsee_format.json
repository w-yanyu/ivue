          "area_code": {
            "control": "hidden",
            "label": "绩效归属区域中心",
            "disabled": true,
            "span": 8
          },
          "area_name": {
            "control": "hidden",
            "label": "绩效归属区域中心名称",
            "disabled": true,
            "span": 8
          },
          "offipart_area": {
            "control": "hidden",
            "label": "合作区域中心",
            "disabled": true,
            "span": 8
          },
          "offipart_area_name": {
            "control": "hidden",
            "label": "合作区域中心名称",
            "disabled": true,
            "span": 8
          }
        },
        {
          
          "seqnum": {
            "control": "hidden",
            "label": "序号"
          }
        }
      ]
    }
  }
}