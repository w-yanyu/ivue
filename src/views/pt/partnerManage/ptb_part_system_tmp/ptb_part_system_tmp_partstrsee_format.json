              {
                "required": false,
                "message": "请输入开户行名称"
              }
            ]
          },
          "open_acct_bank_no": {
            "control": "hidden",
            "label": "开户行行号",
            "disabled": true,
            "span": 8,
            "rules": [
              {
                "max": 30,
                "message": "长度不能超过30个字符"
              },
              {
                "required": false,
                "message": "请输入开户行行号"
              }
            ]
          },
          "open_bank_province": {
            "control": "hidden",
            "label": "开户行省份",
            "disabled": true,
            "span": 8,
            "rules": [
