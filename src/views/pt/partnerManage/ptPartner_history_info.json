{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/call/RPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pt0050",
          "part_no": "${{params.part_no}}"
        }
      },
      "thead": [
        {
          "type":"string",
          "label": "合作方变更id",
          "align":"center",
          "prop": "seq_no"
        },
        {
          "type":"string",
          "label": "合作方编号",
          "align":"center",
          "prop": "part_no"
        },
        {
          "type":"string",
          "label": "合作方对象类型",
          "align":"center",
          "prop": "center_object_type",
          "dict": {
            "dictType": "PT_E_CENTER_OBJECT_TYPE",
            "dictKey": [
              "PT_E_CENTER_OBJECT_TYPE"
            ],
            "format": "label"
          }
        },
        {
          "type":"string",
          "label": "客户编号",
          "align":"center",
          "prop": "cust_no"
        },
        {
          "type":"string",
          "label": "合作方证件类型",
          "align":"center",
          "prop": "center_cert_type",
          "dict": {
            "dictType": "MS_E_CRET_TYPE",
            "dictKey": [
              "MS_E_CRET_TYPE"
            ],
            "format": "label"
          }
        },
        {
          "type":"string",
          "label": "合作方证件号码",
          "align":"center",
          "prop": "center_cert_no"
        },
        {
          "type":"string",
          "label": "合作方名称",
          "align":"center",
          "prop": "center_name"
        },
        {
          "type":"string",
          "label": "合作方品牌",
          "align":"center",
          "prop": "center_brand"
        },
        {
          "type":"string",
          "label": "合作方状态",
          "align":"center",
          "prop": "center_status",
          "dict": {
            "dictType": "PT_E_CENTER_STATUS",
            "dictKey": [
              "PT_E_CENTER_STATUS"
            ],
            "format": "label"
          }
        },
        {
          "type":"string",
          "label": "登记日期",
          "align":"center",
          "prop": "start_date"
        },
        {
          "type":"string",
          "label": "生效日期",
          "align":"center",
          "prop": "take_effect_date"
        },
        {
          "type":"string",
          "label": "失效日期",
          "align":"center",
          "prop": "invalid_date"
        }
      ]
    }
  }
}
