{
    "module": "dynamicTabs",
    "layout": {
        "tabs": {
          "scope": "templateScope",
          "tabHeader": [
            {
              "id": "partneracctInfo",
              "label": "合作方结算账户信息",
              "closable": false,
              "disabled": false,
              "icon": "el-icon-date",
              "toRequest":{
                "url":"/views/pt/partnerManage/partnerAcct_list.json",
                "method": "post",
                "params": {
                  "part_no": "${{params.part_no}}",
                  "show_flash":"${{params.show_flash}}"
                }
              }
            }
          ],
          "defaultActiveKey": "partneracctInfo",
          "position": "top",
          "styleClass": "card"
        }

    }
  }
