{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "controls": [
        {
          "part_no": {
            "control": "hidden",
            "label": "合作方编号",
            "value": "${{params.part_no}}"
          }
        },
        {
          "aaa": {
            "span": 24,
            "control": "dynamic",
            "toRequest": {
              "url": "/views/pt/partnerManage/partnerAcct_list.json",
              "params": {
                "part_no": "${{params.part_no}}",
                "show_flash": "1"
               
              }
            }
          }
        }
      ]
    }
  }
}
