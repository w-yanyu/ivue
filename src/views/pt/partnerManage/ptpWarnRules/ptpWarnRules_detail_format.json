{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
          "scope": "templateScope",
          "initRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "pt0190",
                "part_no": "${{params.part_no}}"
                
              }
          },
          "initDataOnce":true,
  
  
            "btns": [
 
            ],
            "controls": [
                {
                    "part_no": {
                        "control": "hidden",
                        "label": "合作方编号",
                        "width": "130px",
                        "value": "${{params.part_no}}"
                    }
                },
                {
                    "loan_warn_type": {
                        "control": "select",
                        "label": "贷款预警类型",
                        "disabled":true,
                        "dict": {
                            "dictType": "PT_E_LOAN_WRAN_TYPE",
                            "dictKey": [
                                "PT_E_LOAN_WRAN_TYPE"
                            ],
                            "format": "label"
                        }
                    },
                    "warn_value": {
                        "control": "text",
                        "label": "预警值",
                        "disabled":true
                    }
                },
                {
                        "teller_list": {
                            "label": "用户编号",
                            "control": "text",
                            "disabled":true
                                
                        }
                    ,
                    
                        "role_list": {
                            "control": "text",
                            "label": "角色编号",
                            "disabled":true
                            
                        }
                    },{
                    "message_list": {
                        "control": "text",
                        "label": "预警短信手机",
                        "disabled":true
                    }
    
                }
    
             ]
        }
    }
  }
  