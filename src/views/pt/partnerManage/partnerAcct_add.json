{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "scope": "formData",
            "btns": [{
                    "type": "submit",
                    "label": "保存",
                    "doRequest": {
                        "method": "POST",
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "params": {
                            "servicecode": "pt0052",
                            "part_no": "${{params.part_no}}",
                            "version": "${{params.version}}"
                        }
                    }
                },
                {
                    "type": "reset",
                    "icon": "el-icon-close",
                    "label": "重置"
                }
            ],
            "controls": [
                {

                    "center_account_name": {
                        "control": "text",
                        "label": "合作账户简称",
                        "placeholder": "请输入",
                        "rules": [{
                            "required": true,
                            "message": "请输入"
                        }]
                    },
                    "account_status": {
                        "control": "select",
                        "label": "账户状态",
                        "placeholder": "请选择",
                        "value":"1",
                        "disabled":true,
                        "dict": {
                            "dictType": "PT_E_ACCOUNT_STATUS",
                            "dictKey": [
                                "PT_E_ACCOUNT_STATUS"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": true,
                            "message": "请选择"
                        }]
                    }
                },
                {
                    "part_no": {
                        "control": "text",
                        "label": "合作方编号",
                        "value": "${{params.part_no}}",
                        "placeholder": "请输入",
                        "disabled":true
                    },
                    "part_center_no": {
                        "control": "text",
                        "label": "合作方账户账号",
                        "placeholder": "请输入",
                        "rules": [{
                            "required": true,
                            "message": "请输入"
                        }]
                    }
                },
                {
                    "part_center_name": {
                        "control": "text",
                        "label": "合作方账户名称",
                        "placeholder": "请输入",
                        "rules": [{
                            "required": true,
                            "message": "请输入合作方账户名称"
                        }]
                    },
                    "open_bank_no": {
                        "control": "text",
                        "label": "开户行号",
                        "placeholder": "请输入",
                        "rules": [{
                            "required": true,
                            "message": "请输入"
                        }]
                    }
                },
                {
                    "open_bank_name": {
                        "control": "text",
                        "label": "开户行名",
                        "placeholder": "请输入",
                        "rules": [{
                            "required": true,
                            "message": "请输入"
                        }]
                    },
                    "is_my_bank_no": {
                        "control": "select",
                        "label": "是否我行账户",
                        "placeholder": "请选择",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": true,
                            "message": "请选择"
                        }]
                    }
                }


            ]
        }
    }
}
