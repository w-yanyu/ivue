              "node_no": {
                "control": "string",
                "label": "节点编号",
                "span": 6,
                "rules": [
                  {
                    "max": 30,
                    "message": "长度不能超过30个字符"
                  }
                ]
              },
              "node_name": {
                "control": "string",
                "label": "节点名称",
                "span": 6,
                "rules": [
                  {
                    "max": 80,
                    "message": "长度不能超过80个字符"
                  }
                ]
              },
              "part_no": {
                "control": "string",
                "label": "合作方账号",
