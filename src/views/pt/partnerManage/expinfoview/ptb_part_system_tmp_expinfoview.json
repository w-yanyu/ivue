{
  "module": "lr_tree_dynamic",
  "layout": {
    "##initReqeust": "${{params}}",
    "tree": {
      "scope": "productParam",
      "title": "敞口信息",
      "lazy": false,
      "idField": "node_no",
      "pIdField": "parent_node_no",
      "labelField": "node_name",
      "isLeafField": "is_leaf",
      "nodeTypeField": "node_level",
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "belorg":"${{params.belorg}}",
          "part_no":"${{params.part_no}}",
          "seqnum":"${{params.seqnum}}",
          "servicecode": "pt0171"
        }
      },
      "#root": {
        "rootNode": {
          "name": "伙伴体系",
          "value": "-1",
          "icon": "xitongguanli",
          "disabled": true

        }
      },
      "nodeTypes": {
        "1": {
          "icon": "el-icon-news",
          "children": [
            "node_no",
            "node_name"
          ],
          "toRequest": {
            "method": "get",
            "url": "/views/pt/partnerManage/expinfoview/ptb_part_system_tmp_expinfosee.json",
            "params": {
              "operater_ind": "M",
              "part_no":"${{params.part_no}}",
              "prndid": "-1"
            }
          },
          "props": {
            "title": "敞口详情",
            "width":"70%"
          },
          "contextMenu": [
            {
              "type": "custom",
              "label": "敞口信息更新",
              "childNodeType": "1",
              "icon": "el-icon-circle-plus-outline",
              "params": {
                "operater_ind": "M",
                "part_no":"${{params.part_no}}",
                "node_no": "${{params.node_no}}"

              },
              "toRequest": {
                "url": "/views/pt/partnerManage/expinfoview/ptb_part_system_tmp_expinfoadd1.json",
                "target": "dialog"

              },
              "props": {
                "title": "敞口信息更新",
                "width":"70%"
              },
              "eventAfter": {
                "callback": "renderTree"
              }
            }
          ]
        },
        "2": {
          "icon": "el-icon-news",
          "children": [
            "node_no",
            "node_name"
          ],
          "toRequest": {
            "method": "get",
            "url": "/views/pt/partnerManage/expinfoview/ptb_part_system_tmp_expinfosee2.json",
            "params": {
              "operater_ind": "M",
              "part_no":"${{params.part_no}}",
              "prndid": "${{productParam.prndid}}"
            }
          },
          "props": {
            "title": "合作伙伴体系详情"
          },
          "contextMenu": [
            {
              "type": "edit",
              "label": "节点信息更新",
              "childNodeType": "2",
              "icon": "el-icon-circle-plus-outline",
              "params": {
                "operater_ind": "M",
                "part_no":"${{params.part_no}}"

              },
              "toRequest": {
                "url": "/views/pt/partnerManage/expinfoview/ptb_part_system_tmp_expinfoadd1.json",
                "target": "dialog"

              },
              "props": {
                "title": "节点信息更新",
                "width":"70%"
              },
              "eventAfter": {
                "callback": "renderTree"
              }
            }
          ]
        },
        "3": {
          "icon": "el-icon-news",
          "children": [
            "node_no",
            "node_name"
          ],
          "toRequest": {
            "method": "get",
            "url": "/views/pt/partnerManage/expinfoview/ptb_part_system_tmp_expinfosee2.json",
            "params": {
              "operater_ind": "M",
              "##part_no":"${{params.part_no}}",
              "##prndid": "${{productParam.prndid}}"
            }
          },
          "props": {
            "title": "合作伙伴体系详情"
          },
          "contextMenu": [

            {
              "type": "custom",
              "label": "节点信息更新",
              "childNodeType": "3",
              "icon": "el-icon-circle-plus-outline",
              "params": {
                "operater_ind": "M",
                "part_no":"${{params.part_no}}",
                "node_no": "${{params.node_no}}"

              },
              "toRequest": {
                "url": "/views/pt/partnerManage/expinfoview/ptb_part_system_tmp_expinfoadd1.json",
                "target": "dialog"
              },
              "props": {
                "title": "节点信息更新",
                "width":"70%"
              },
              "eventAfter": {
                "callback": "renderTree"
              }
            }
          ]
        }
      }
    }
  }
}
