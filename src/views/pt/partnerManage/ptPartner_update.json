{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
          "scope": "templateScope",
          "initRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "pt0170",
                "part_no": "${{params.part_no}}",
                "version": "${{params.version}}"
              }
          },
          "initDataOnce":true,
  
  
            "btns": [
 
            ],
            "controls": [
                {
                    "part_no": {
                        "control": "text",
                        "label": "合作方编号",
                        "width": "130px",
                        "rules": [{
                                "required": true,
                                "message": "请输入合作方账号"
                            },
                            {
                                "max": 200,
                                "message": "长度不能超过200个字符"
                            }
                        ],
                        "disabled":true
                    },
                    "version": {
                        "control": "hidden",
                        "label": "版本"
                    }

                },
                {
                    "cust_no": {
                        "control": "text",
                        "label": "客户编号",
                        "width": "130px",
                        "rules": [{
                                "required": false,
                                "message": "请输入客户编号"
                            },
                            {
                                "max": 80,
                                "message": "长度不能超过80个字符"
                            }
                        ],
                        "disabled":true
                    },
                    "center_cert_type": {
                        "control": "select",
                        "label": "合作方证件类型",
                        "width": "130px",
                        "dict": {
                            "dictType": "MS_E_CRET_TYPE",
                            "dictKey": [
                                "MS_E_CRET_TYPE"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": true,
                            "message": "请选择合作方证件类型"
                        }],
                        "disabled":true
                    }
                },
                {
                  "center_cert_no": {
                      "control": "text",
                      "label": "合作方证件号码",
                      "width": "130px",
                      "rules": [{
                              "required": true,
                              "message": "请输入合作方证件号码"
                          },
                          {
                              "max": 30,
                              "message": "长度不能超过30个字符"
                          }
                      ],
                      "disabled":true
                  },
                  "center_name": {
                      "control": "text",
                      "label": "合作方名称",
                      "width": "130px",
                      "rules": [{
                              "required": true,
                              "message": "请输入合作方名称"
                          },
                          {
                              "max": 30,
                              "message": "长度不能超过30个字符"
                          }
                      ],
                      "disabled":true
                  }
    
                },
                {
                    "center_brand": {
                        "control": "text",
                        "label": "合作方品牌",
                        "width": "130px",
                        "rules": [{
                                "required": true,
                                "message": "请输入合作方品牌"
                            },
                            {
                                "max": 30,
                                "message": "长度不能超过30个字符"
                            }
                        ]
                    },
                    "warn_status": {
                        "control": "select",
                        "label": "预警状态",
                        "disabled":true,
                        "width": "130px",
                        "dict": {
                            "dictType": "PT_E_WARN_STATUS",
                            "dictKey": [
                                "PT_E_WARN_STATUS"
                            ],
                            "format": "label"
                        }
    
                    }

                },

                {
                  "take_effect_date": {
                      "label": "生效日期",
                      "width": "130px",
                      "control": "dateTimePicker",
                      "editable": true,
                      "format": "yyyyMMdd",
                      "rules": [{
                          "required": true,
                          "message": "请输入生效日期"
                      }]
                  },
                  "invalid_date": {
                      "label": "失效日期",
                      "width": "130px",
                      "control": "dateTimePicker",
                      "editable": true,
                      "format": "yyyyMMdd",
                      "rules": [{
                          "required": true,
                          "message": "请输入失效日期"
                      }]
                  }
    
                },
                {

                    "start_date": {
                        "label": "登记日期",
                        "width": "130px",
                        "control": "dateTimePicker",
                        "editable": true,
                        "format": "yyyyMMdd",
                        "rules": [{
                            "required": true,
                            "message": "请输入登记日期"
                        }]
                    }
                }
    
             ]
        }
    }
  }
  