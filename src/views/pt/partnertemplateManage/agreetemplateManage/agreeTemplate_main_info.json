{
  "module": "v-inputDataForm-btn",
  "layout": {
    "scope": "templateScope",
    "form": {
      "btns":[],
      "controls": [
          {
            "top": {
              "control":"dynamic",
              "span":24,
              "toRequest":{
                "url":"/views/pt/partnertemplateManage/agreetemplateManage/agreeTemplate_top_info.json",
                "method": "GET",
                "params": {
                  "protocol_template_no":"${{params.protocol_template_no}}",
                  "agree_type":"${{params.agree_type}}",
                  "mould_name":"${{params.mould_name}}",
                  "mould_describe":"${{params.mould_describe}}",
                  "mould_status":"${{params.mould_status}}",
                  "effect_date":"${{params.effect_date}}",
                  "expiry_date":"${{params.expiry_date}}",
                  "mould_section":"${{params.mould_section}}",
                  "version":"${{params.version}}",
                  "servicecode": "pt0076"
                }
              }
            },
            "bottom": {
              "control":"dynamic",
              "span":24,
              "toRequest":{
                "url":"/views/pt/partnertemplateManage/agreetemplateManage/agreeTemplate_bottom_info.json",
                "method": "post",
                "params": {
                  "protocol_template_no":"${{params.protocol_template_no}}",
                  "version":"${{params.version}}",
                  "show_flash":"${{params.show_flash}}",
                  "servicecode": "pt0083"
                }
              }
            }
          }
      ]
    }
  }
}
