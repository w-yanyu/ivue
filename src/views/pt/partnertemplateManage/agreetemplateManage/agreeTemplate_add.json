{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initDataOnce": true,
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pt0076",
          "protocol_template_no": "${{params.protocol_template_no}}",
          "version": "${{params.version}}"
        }
      },
      "controls": [
        {
          "protocol_template_no": {
            "control": "hidden",
            "label": "协议模版编号",
            "value": "${{params.protocol_template_no}}",
            "disabled": true
          },
          "version": {
            "control": "hidden",
            "label": "协议模版编号",
            "value": "${{params.version}}",
            "disabled": true
          },
          "agree_type": {
            "control": "select",
            "label": "协议类型",
            "width": "130px",
            "dict": {
              "dictType": "PT_E_AGREE_TYPE",
              "dictKey": [
                "PT_E_AGREE_TYPE"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择协议类型"
              }
            ]
          },
          "mould_name": {
            "control": "text",
            "label": "模版名称",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入模版名称"
              },
              {
                "max": 200,
                "message": "长度不能超过200个字符"
              }
            ]
          }
        },
        {
          "effect_date": {
            "label": "模版生效日期",
            "width": "130px",
            "control": "dateTimePicker",
            "editable": true,
            "format": "yyyyMMdd",
            "rules": [
              {
                "required": true,
                "message": "请输入模版生效日期"
              }
            ]
          },
          "expiry_date": {
            "label": "模版失效日期",
            "width": "130px",
            "control": "dateTimePicker",
            "editable": true,
            "format": "yyyyMMdd",
            "rules": [
              {
                "required": true,
                "message": "请输入模版失效日期"
              }
            ]
          }
        },
        {
          "mould_section": {
            "control": "text",
            "label": "模版管理部门",
            "width": "130px"
          }
        },
        {
          "mould_describe": {
            "label": "模版描述",
            "control": "textarea",
            "width": "130px",
            "span": "24",
            "autosize": {
              "minRows": 4
            },
            "rules": [
              {
                "required": false,
                "message": "请输入模版描述"
              },
              {
                "required": false,
                "max": 200,
                "message": "长度不能超过200个字符"
              }
            ]
          }
        }
      ]
    }
  }
}
