{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "controls": [{
                    "protocol_template_no": {
                        "control": "text",
                        "label": "协议模版编号"
                    },
                    "mould_name": {
                        "control": "text",
                        "label": "模版名称"
                    }
                },
                {

                    "mould_status": {
                        "control": "select",
                        "label": "模版状态",
                        "dict": {
                          "dictType": "PT_E_STATUS",
                          "dictKey": [
                            "PT_E_STATUS"
                          ],
                          "format": "value-label"
                        }
                    }
                }]
        },
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": true,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "pt0077"
                    

                }
            },
            "thead": [{
                    "type":"string",
                    "align":"center",
                    "label": "协议模版编号",
                    "prop": "protocol_template_no"

                },
                {
                    "type":"string",
                    "align":"center",
                    "label": "模版名称",
                    "prop": "mould_name"
                },
                {
                    "type":"string",
                    "align":"center",
                    "label": "协议类型",
                    "prop": "agree_type",
                    "dict": {
                      "dictType": "PT_E_AGREE_TYPE",
                      "dictKey": [
                        "PT_E_AGREE_TYPE"
                      ],
                      "format": "value-label"
                    }
                },
                {
                    "type":"string",
                    "align":"center",
                    "label": "模版状态",
                    "prop": "mould_status",
                    "dict": {
                      "dictType": "PT_E_STATUS",
                      "dictKey": [
                        "PT_E_STATUS"
                      ],
                      "format": "value-label"
                    }
                },
                {
                    "type":"string",
                    "align":"center",
                    "label": "模版生效日期",
                    "prop": "effect_date"
                },
                {
                    "type":"string",
                    "align":"center",
                    "label": "模版失效日期",
                    "prop": "expiry_date"
                },
                {
                    "type":"string",
                    "align":"center",
                    "label": "模版管理部门",
                    "prop": "mould_section"
                },
                {
                    "label": "操作",
                    "align": "center",
                    "fixed": "right",
                    "btns": [{
                        "type": "bringback",
                        "label": "选择"
                    }]
                }
            ]
        }
    }
}
