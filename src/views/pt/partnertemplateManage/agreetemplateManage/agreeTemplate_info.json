{
  "module": "v-realonlyForm-btn",
  "layout": {
    "form": {
      "scope": "formData",
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "##servicecode": "pt0076",
          "servicecode": "${{params.servicecode}}",
          "protocol_template_no": "${{params.protocol_template_no}}",
          "version": "${{params.version}}"
        }
      },
      "controls": [
        {
          "protocol_template_no": {
            "control": "string",
            "label": "协议模版编号",
            "width": "130px",
            "disabled": true
          }
        },
        {
          "mould_name": {
            "control": "string",
            "label": "模版名称",
            "width": "130px",
            "disabled": true
          },
          "agree_type": {
            "control": "string",
            "label": "协议类型",
            "width": "130px",
            "dict": {
              "dictType": "PT_E_AGREE_TYPE",
              "dictKey": [
                "PT_E_AGREE_TYPE"
              ],
              "format": "label"
            },
            "disabled": true
          }
        },
        {
          "mould_status": {
            "control": "string",
            "label": "模版状态",
            "width": "130px",
            "dict": {
              "dictType": "PT_E_STATUS",
              "dictKey": [
                "PT_E_STATUS"
              ],
              "format": "label"
            },
            "disabled": true
          },
          "effect_date": {
            "control": "string",
            "label": "模版生效日期",
            "width": "130px",
            "disabled": true
          }
        },
        {
          "expiry_date": {
            "control": "string",
            "label": "模版失效日期",
            "width": "130px",
            "disabled": true
          },
          "mould_section": {
            "control": "string",
            "label": "模版管理部门",
            "width": "130px",
            "disabled": true
          }
        },
        {
          "mould_describe": {
            "disabled": true,
            "label": "模版描述",
            "control": "textarea",
            "width": "130px",
            "span": "24",
            "autosize": {
              "minRows": 4
            },
            "##rules": [
              {
                "required": false,
                "message": "请输入模版描述"
              },
              {
                "max": 200,
                "message": "长度不能超过200个字符"
              }
            ]
          }
        }
      ]
    }
  }
}
