{
  "module": "d-steps",
  "layout": {
    "steps": {
      "scope": "newPart",
      "direction": "horizontal",
      "alignCenter": true,
      "finishStatus": "success",
      "simpleType": false,
      "nextStepType": "validate",
      "skip": false,
      "list": [
        {
          "title": "合作项目模版",
          "scope": "baseInfo",
          "icon": "el-icon-edit",
          "toRequest": {
            "url": "/views/pt/partnertemplateManage/projecttemplateManage/projectTemplate_add.json",
            "method": "post",
            "params": {
              "project_template_no": "${{newPart.baseInfo1.project_template_no}}",
              "version": "${{newPart.baseInfo1.version}}"
            }
          },
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "pt0090"
            },
            "resultSet": {
              "project_template_no": "project_template_no",
              "version":"version"
            }
          }
        },
        {
          "title": "项目模版关联部件",
          "##forbidBack": true,
          "scope": "baseInfo1",
          "icon": "el-icon-edit",
          "toRequest": {
            "url": "/views/pt/partnertemplateManage/projecttemplaterelapartsManage/projecttemplaterelaParts_middle_list.json",
            "params": {
              "servicecode": "pt0133",
              "project_template_no": "${{newPart.baseInfo.project_template_no}}",
              "version":"${{newPart.baseInfo.version}}"
            }
          },
          "doRequest": {
            "url": "/pte/json/layout/success.json",
            "method": "post",
            "eventAfter": {
              "doParentCallback": true
            }
          }
        },
        {
          "title": "项目模版关联协议模版",
          "##forbidBack": true,
          "scope": "baseInfo3",
          "icon": "el-icon-edit",
          "toRequest": {
            "url": "/views/pt/partnertemplateManage/projecttemplaterelaagreeManage/projecttemplaterelaAgree_middle_list.json",
            "params": {
              "project_template_no": "${{newPart.baseInfo1.project_template_no}}",
              "project_mould_name": "${{newPart.baseInfo.project_mould_name}}",
              "version": "${{newPart.baseInfo1.version}}",
              "servicecode": "pt0143"

            }
          },
          "doRequest": {
            "url": "/pte/json/layout/success.json",
            "method": "post",
            "eventAfter": {
              "doParentCallback": true
            }
          }
        },
        {
          "title": "项目模版管理流程产品",
          "##forbidBack": true,
          "scope": "baseInfo4",
          "icon": "el-icon-edit",
          "toRequest": {
            "url": "/views/pt/partnertemplateManage/CooperatItemFlowRelaManage/CooperatItemFlowRela_middle_list.json",
            "params": {
              "project_template_no": "${{newPart.baseInfo3.project_template_no}}",
              "version": "${{newPart.baseInfo3.version}}",
              "servicecode": "pt0153"
            }
          },
          "doRequest": {
            "url": "/pte/json/layout/success.json",
            "method": "post",
            "eventAfter": {
              "doParentCallback": true
            }
          }
        },
        {
          "title": "提交审核",
          "##forbidBack": true,
          "scope": "baseInfo2",
          "icon": "el-icon-edit",
          "toRequest": {
            "url": "/views/pt/partnertemplateManage/projecttemplateManage/projectTemplate_submit.json",
            "params": {
              "project_template_no": "${{newPart.baseInfo4.project_template_no}}",
              "version": "${{newPart.baseInfo4.version}}",
              "approve_status": "01"
            }
          },
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "pt0074",
              "sale_prod_cd": "${{newPart.baseInfo4.project_template_no}}",
              "version": "${{newPart.baseInfo4.version}}",
              "approve_comments": "1",
              "approve_link": "02",
              "approve_status": "01",
              "mould_code": "2"
            }
          }
        }
      ]
    }
  }
}
