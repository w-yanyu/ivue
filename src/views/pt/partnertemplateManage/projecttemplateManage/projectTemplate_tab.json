{
  "module": "dynamicTabs",
  "layout": {
    "tabs": {
      "tabHeader": [
        {
          "id": "baseInfo",
          "label": "项目模版信息",
          "closable": false,
          "disabled": false,
          "icon": "el-icon-date",
          "toRequest": {
            "url": "/views/pt/partnertemplateManage/projecttemplateManage/projectTemplate_info.json",
            "method": "POST"
          }
        },
        {
          "id": "baseInfo2",
          "label": "项目模版关联部件",
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "toRequest": {
            "url": "/views/pt/partnertemplateManage/projecttemplaterelapartsManage/projecttemplaterelaParts_list.json",
            "method": "get",
            "params": {
              "project_template_no": "${{params.project_template_no}}",
              "servicecode": "pt0133"
             }
          }
        },
        {
          "id": "baseInfo3",
          "label": "项目模版关联协议模版",
          "closable": false,
          "disabled": false,
          "icon": "el-icon-date",
          "toRequest": {
            "url": "/views/pt/partnertemplateManage/projecttemplaterelaagreeManage/projecttemplaterelaAgree_list.json",
            "method": "get",
            "params": {
              "project_template_no": "${{params.project_template_no}}",
              "servicecode": "pt0143"
             }
          }
        },
        {
          "id": "baseInfo4",
          "label": "项目模版管理流程产品",
          "closable": false,
          "disabled": false,
          "icon": "el-icon-date",
          "toRequest": {
            "url": "/views/pt/partnertemplateManage/CooperatItemFlowRelaManage/CooperatItemFlowRela_info.json",
            "method": "get",
            "params": {
              "project_template_no": "${{params.project_template_no}}",
              "servicecode": "pt0153"
             }
          }
        }

      ],
      "defaultActiveKey": "baseInfo",
      "position": "top",
      "styleClass": "card"
    }
  }
}
