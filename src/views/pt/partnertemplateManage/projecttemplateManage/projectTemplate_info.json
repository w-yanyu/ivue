{
    "module": "v-realonlyForm-btn",
    "layout": {
        "form": {
            "scope": "formData",
            "initRequest": "${{params}}",
            "controls": [
                {
                    "project_template_no": {
                        "control": "string",
                        "label": "项目模版编号",
                        "disabled": true
                    },
                    "project_mould_name": {
                        "control": "string",
                        "label": "项目模版名称",
                        "disabled": true
                    }

                },
                {

                    "part_project_type": {
                        "control": "string",
                        "label": "合作项目类型",
                        "dict": {
                            "dictType": "PT_E_PART_PROJECT_TYPE",
                            "dictKey": [
                                "PT_E_PART_PROJECT_TYPE"
                            ],
                            "format": "label"
                        },
                        "disabled": true
                    },
                    "mould_status": {
                        "control": "string",
                        "label": "项目模版状态",
                        "dict": {
                            "dictType": "PT_E_STATUS",
                            "dictKey": [
                                "PT_E_STATUS"
                            ],
                            "format": "label"
                        },
                        "disabled": true
                    }

                },
                {

                    "effective_date": {
                        "control": "string",
                        "label": "生效日期",
                        "disabled": true
                    },
                    "expiry_date": {
                        "control": "string",
                        "label": "失效日期",
                        "disabled": true
                    }
                },
                {
                  "check_filelist_nm": {
                    "control": "string",
                    "label": "资料检查列表名称"
                  },
                    "project_section": {
                        "control": "string",
                        "label": "项目模版管理部门",
                        "disabled": true
                    }
                },{
                    "project_mould_describe": {
                        "control": "textarea",
                        "width": "25px",
                        "span": "24",
                        "label": "项目模版描述",
                        "disabled": true
                    }
                }

            ]
        }
    }
}
