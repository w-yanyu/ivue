{
  "module": "dynamicTabs",
  "layout": {
    "tabs": {
      "scope": "formDate",
      "tabHeader": [
        {
          "id": "baseInfo",
          "label": "项目模版基础信息",
          "closable": false,
          "disabled": false,
          "icon": "el-icon-date",
          "toRequest": {
            "url": "/views/pt/partnertemplateManage/projecttemplateManage/projectTemplate_info.json",
            "method": "post",
            "params": {
              "project_template_no": "${{params.project_template_no}}"
            }
          }
        },
        {
          "id": "partsinfo",
          "label": "部件信息",
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "toRequest": {
            "url": "/views/pt/partnertemplateManage/partsManage/ptParts_info.json",
            "method": "post",
            "params": {
              "project_template_no": "${{params.project_template_no}}"
            }
          }
        },
        {
          "id": "agreetemplateinfo",
          "label": "协议模版信息",
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "toRequest": {
            "url": "/views/pt/partnertemplateManage/agreetemplateManage/agreeTemplate_info.json",
            "method": "post",
            "params": {
              "project_template_no": "${{params.project_template_no}}"
            }
          }
        }
      ],
      "defaultActiveKey": "baseInfo",
      "position": "top",
      "styleClass": "card"
    }
  }
}
