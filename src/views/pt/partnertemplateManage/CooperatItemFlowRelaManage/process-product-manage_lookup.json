
{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "appId": {
            "control": "string",
            "label": "业务模块",
            "selectFirstValue":false,
            "dict": {
                "dictType": "MS_APPID",
                "dictKey": [
                    "MS_APPID"
                ],
                "format": "value-label"
            },
            "value":"ptOnl"
        }
        },
        {
          "wf_prod_name": {
            "control": "text",
            "label": "流程产品名称",
            "rules": [
              {
                "max": 50,
                "message": "长度不能超过10个字符"
              }
            ]
          },
          "wf_prod_no": {
            "control": "text",
            "label": "流程产品编号",
            "width": 100,
            "rules": [
              {
                "max": 100,
                "message": "长度不能超过100个字符"
              }
            ]
          }
        }
      ]
    },

    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "bpm0061",
          "appId":"${{form.appId}}"
        }
      },
      "thead": [
        {
          "label": "流程产品编号",
          "prop": "wf_prod_no"
        },
        {
          "label": "流程产品名称",
          "prop": "wf_prod_name"
        },
        {
          "label": "流程定义名称",
          "prop": "wf_define_name"
        },
        {
          "label": "流程产品类型",
          "width": 120,
          "prop": "wf_prod_tp",
          "dict": {
            "dictType": "BPM_E_WFPDTP",
            "dictKey": [
              "BPM_E_WFPDTP"
            ],
            "format": "value-label"
          }
        },

        {
          "label":"版本号",
          "prop":"data_version",
          "width": 80
        },
        {
          "label": "操作",
          "align": "center",
          "fixed": "right",
          "btns": [{
              "type": "bringback",
              "label": "选择"
          }]
        }


      ]
    }
  }
}
