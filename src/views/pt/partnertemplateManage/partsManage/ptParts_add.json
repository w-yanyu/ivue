{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "doRequest": {
            "url": "/SUMP/call/RPCCall",
            "method": "POST",
            "params": {
              "servicecode": "pt0060",
              "page_id": "/views/pt/partnertemplateManage/partsManage/ptParts_add.json"
            }
          }
        }
      ],
      "controls": [
        {
          "parts_no": {
            "control": "hidden",
            "label": "部件编号",
            "width": "130px"
          },
          "parts_name": {
            "control": "text",
            "label": "部件名称",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          },
          "parts_class": {
            "control": "select",
            "label": "部件种类",
            "width": "130px",
            "dict": {
              "dictType": "PT_E_PARTS_CLASS",
              "dictKey": [
                "PT_E_PARTS_CLASS"
              ],
              "format": "value-label"
            },
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ],
            "showItem": {
              "1": [
                "agree_parts_type"
              ],
              "2": [
                "cooperate_project_type"
              ]
            },
            "events": {
              "condition": {
                "1": {
                  "control": {
                    "agree_parts_type": {
                      "control": "select",
                      "label": "协议部件类型",
                      "width": "130px",
                      "value": "",
                      "dict": {
                        "dictType": "PT_E_AGREE_PARTS_TYPE",
                        "dictKey": [
                          "PT_E_AGREE_PARTS_TYPE"
                        ],
                        "format": "value-label"
                      }
                    },
                    "cooperate_project_type":{
                      "value": ""
                    }
                  }
                },
                "2": {
                  "control": {
                    "cooperate_project_type": {
                      "control": "select",
                      "label": "项目部件类型",
                      "width": "130px",
                      "value": "",
                      "dict": {
                        "dictType": "PT_E_COOPERATE_PROJECT_TYPE",
                        "dictKey": [
                          "PT_E_COOPERATE_PROJECT_TYPE"
                        ],
                        "format": "value-label"
                      }
                    },
                    "agree_parts_type":{
                      "value": ""
                    }
                  }
                }
              }
            }
          }
        },
        {
          "agree_parts_type": {
            "control": "select",
            "label": "协议部件类型",
            "width": "130px",
            "dict": {
              "dictType": "PT_E_AGREE_PARTS_TYPE",
              "dictKey": [
                "PT_E_AGREE_PARTS_TYPE"
              ],
              "format": "value-label"
            },
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ],
            "value": ""
          },
          "cooperate_project_type": {
            "control": "select",
            "label": "项目部件类型",
            "width": "130px",
            "dict": {
              "dictType": "PT_E_COOPERATE_PROJECT_TYPE",
              "dictKey": [
                "PT_E_COOPERATE_PROJECT_TYPE"
              ],
              "format": "value-label"
            },
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ],
            "value": ""
          }
        },
        {
          "parts_status": {
            "control": "select",
            "label": "部件状态",
            "width": "130px",
            "dict": {
              "dictType": "PT_E_STATUS",
              "dictKey": [
                "PT_E_STATUS"
              ],
              "format": "value-label"
            },
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ],
            "disabled": true,
            "value": "1"
          },
          "add_url": {
            "control": "text",
            "label": "新增页面路径",
            "width": "130px"
          }
        },
        {
          "list_url": {
            "control": "hidden",
            "label": "列表页面路径",
            "width": "130px"
          },
          "is_choice": {
            "control": "hidden",
            "label": "是否支持多选",
            "width": "130px",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "value-label"
            },
            "#rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          }
        },
        {
          "update_url": {
            "control": "text",
            "label": "维护页面路径",
            "width": "130px"
          },
          "info_url": {
            "control": "text",
            "label": "详情页面路径",
            "width": "130px"
          }
        }
      ]
    }
  }
}