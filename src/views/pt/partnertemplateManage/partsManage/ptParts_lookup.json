{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "controls": [{
                    "parts_no": {
                        "control": "text",
                        "label": "部件编号"
                    },
                    "parts_class": {
                        "control": "select",
                        "label": "部件种类",
                        "disabled": true,
                        "value":"${{params.parts_class}}",
                        "dict": {
                          "dictType": "PT_E_PARTS_CLASS",
                          "dictKey": [
                            "PT_E_PARTS_CLASS"
                          ],
                          "format": "value-label"
                        }
                    }
                },
                {
                  "parts_name": {
                      "control": "text",
                      "label": "部件名称"
                  },
                    "parts_status": {
                        "control": "select",
                        "label": "部件状态",
                        "disabled": true,
                        "dict": {
                          "dictType": "PT_E_STATUS",
                          "dictKey": [
                            "PT_E_STATUS"
                          ],
                          "format": "value-label"
                        },
                        "value":"${{params.parts_status}}"
                    }
                }]
        },
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": true,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "pt0063",
                    "cust_type": "${{params.cust_type}}"

                }
            },
            "thead": [{
                    "type":"string",
                    "align":"center",
                    "label": "部件编号",
                    "prop": "parts_no"

                },
                {
                    "type":"string",
                    "align":"center",
                    "label": "部件名称",
                    "prop": "parts_name"
                },
                {
                    "type":"string",
                    "align":"center",
                    "label": "部件种类",
                    "prop": "parts_class",
                    "dict": {
                      "dictType": "PT_E_PARTS_CLASS",
                      "dictKey": [
                        "PT_E_PARTS_CLASS"
                      ],
                      "format": "value-label"
                    }
                },
                {
                    "type":"string",
                    "align":"center",
                    "label": "协议部件类型",
                    "prop": "agree_parts_type",
                    "dict": {
                      "dictType": "PT_E_AGREE_PARTS_TYPE",
                      "dictKey": [
                        "PT_E_AGREE_PARTS_TYPE"
                      ],
                      "format": "value-label"
                    }
                },
                {
                    "type":"string",
                    "align":"center",
                    "label": "项目部件类型",
                    "prop": "cooperate_project_type",
                    "dict": {
                      "dictType": "PT_E_COOPERATE_PROJECT_TYPE",
                      "dictKey": [
                        "PT_E_COOPERATE_PROJECT_TYPE"
                      ],
                      "format": "value-label"
                    }
                },
                {
                    "type":"string",
                    "align":"center",
                    "label": "部件状态",
                    "prop": "parts_status",
                    "dict": {
                      "dictType": "PT_E_STATUS",
                      "dictKey": [
                        "PT_E_STATUS"
                      ],
                      "format": "value-label"
                    }
                },              
                {
                    "label": "操作",
                    "align": "center",
                    "width": 160,
                    "fixed": "right",
                    "btns": [{
                        "type": "bringback",
                        "label": "选择"
                    },
                    {
                      "type": "look",
                      "label": "预览",
                      "toRequest": {
                          "url": "${{current.info_url}}",
                          "method": "POST"
                      },
                      "props": {
                          "title": "页面预览"
                      }
                  }
                  ]
                }
            ]
        }
    }
}
