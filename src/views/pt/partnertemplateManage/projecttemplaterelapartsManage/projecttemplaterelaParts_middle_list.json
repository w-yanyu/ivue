{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "controls": [
        {
          "project_template_no": {
            "control": "hidden",
            "label": "项目模版编号",
            "value": "${{params.project_template_no}}"
          },
          "version": {
            "control": "hidden",
            "label": "版号",
            "value": "${{params.version}}"
          }
        },
        {
          "aaa": {
            "span": 24,
            "control": "dynamic",
            "toRequest": {
              "url": "/views/pt/partnertemplateManage/projecttemplaterelapartsManage/projecttemplaterelaParts_list.json",
              "params": {
                "project_template_no": "${{params.project_template_no}}",
                "version": "${{params.version}}",
                "servicecode":  "${{params.servicecode}}",
                "show_flash": "1"
               
              }
            }
          }
        }
      ]
    }
  }
}
