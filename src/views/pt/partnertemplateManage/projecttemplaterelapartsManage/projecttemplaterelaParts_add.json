{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "btns": [
        {
          "type": "submit",
          "label": "保存",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "pt0130"
            }
          }
        }
      ],
      "controls": [
          {
              "project_template_no": {
                  "control": "string",
                  "label": "项目模版编号",
                  "disabled": true,
                  "value":"${{params.project_template_no}}"

              },
              "parts_no": {
                  "control": "lookup",
                  "edit": true,
                  "rules": [{
                      "required": true,
                      "message": "请选择部件"
                  }],
                  "label": "部件编号",
                  "keyField": {
                      "parts_no": "parts_no",
                      "cooperate_project_type": "cooperate_project_type",
                      "is_choice":"is_choice"
                      
                  },
                  "toRequest": {
                      "url": "/views/pt/partnertemplateManage/partsManage/ptParts_lookup.json",
                      "method": "post",
                      "params": {
                          "protocol_template_no": "${{formData.protocol_template_no}}",
                          "parts_class": "2",
                          "parts_status":"1"


                      }
                  },
                  "props": {
                      "width": "80%",
                      "title": "查询部件"
                  },
                  "format": "label"
              }
          },
          {
              "cooperate_project_type": {
                  "control": "select",
                  "label": "项目部件类型",
                  "disabled": true,
                  "placeholder": "请选择",
                  "dict": {
                      "dictType": "PT_E_COOPERATE_PROJECT_TYPE",
                      "dictKey": [
                          "PT_E_COOPERATE_PROJECT_TYPE"
                      ],
                      "format": "label"
                  },
                  "rules": [{
                      "required": true,
                      "message": "请选择"
                  }]
              },
              "is_must": {
                  "control": "select",
                  "label": "是否必填",
                  "placeholder": "请选择",
                  "dict": {
                      "dictType": "MS_E_YESORNO",
                      "dictKey": [
                          "MS_E_YESORNO"
                      ],
                      "format": "label"
                  },
                  "rules": [{
                      "required": true,
                      "message": "请选择"
                  }]
              }
          },

          {

              "is_choice": {
                  "control": "hidden",
                  "label": "是否可多选",
                 
                  "placeholder": "请选择",
                  "dict": {
                      "dictType": "MS_E_YESORNO",
                      "dictKey": [
                          "MS_E_YESORNO"
                      ],
                      "format": "label"
                  },
                  "rules": [{
                      "required": true,
                      "message": "请选择"
                  }]
              },
              "version": {
                "control": "hidden",
                "label": "版本号",
                "value": "${{params.version}}"
            }
          }

      ]
    }
  }
}
