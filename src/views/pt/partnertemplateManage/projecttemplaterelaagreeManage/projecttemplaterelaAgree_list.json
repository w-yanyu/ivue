{
  "module": "v_search_btn_datagrid",
  "layout": {
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "target": "dialog",
          "label": "新增",
          "toRequest": {
            "method": "GET",
            "url": "/views/pt/partnertemplateManage/projecttemplaterelaagreeManage/projecttemplaterelaAgree_add.json",
            "params": {
              "project_template_no": "${{params.project_template_no}}",
              "project_mould_name": "${{params.project_mould_name}}",
              "version": "${{params.version}}"
            }
          },
          "show": {
            "expression": "'${{params.show_flash}}' == '1'"
          },
          "props": {
            "title": "新增合作项目模版协议模版关联",
            "width": "75%"
          }
        },
        {
          "type": "add",
          "label": "排序",
          "toRequest": {
            "url": "@projSAgreeTemplatePageSort",
            "method": "GET",
            "params": {
              "project_template_no": "${{params.project_template_no}}",
              "version": "${{params.version}}",
              "servicecode": "${{params.servicecode}}"
            }
          },
          "props": {
            "width": "75%",
            "title": "合作项目模版协议模版关联排序"
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "##servicecode": "pt0143",
          "servicecode": "${{params.servicecode}}",
          "project_template_no": "${{params.project_template_no}}",
          "version": "${{params.version}}"
        }
      },
      "thead": [
        {
          "type": "index",
          "width": 50,
          "label": "序号"
        },
        {
          "label": "项目模版编号",
          "prop": "project_template_no",
          "align": "center",
          "disabled": true
        },
        {
          "label": "项目模版名称",
          "prop": "project_mould_name",
          "align": "center",
          "disabled": true
        },
        {
          "label": "协议模版编号",
          "prop": "protocol_template_no",
          "align": "center"
        },
        {
          "label": "协议模版名称",
          "prop": "mould_name",
          "align": "center"
        },
        {
          "label": "协议类型",
          "prop": "agree_type",
          "align": "center",
          "dict": {
            "dictType": "PT_E_AGREE_TYPE",
            "dictKey": [
              "PT_E_AGREE_TYPE"
            ],
            "labelField": "dictName",
            "valueField": "dictId"
          }
        },
        {
          "label": "是否必填",
          "prop": "is_must",
          "align": "center",
          "dict": {
            "dictType": "MS_E_YESORNO",
            "dictKey": [
              "MS_E_YESORNO"
            ],
            "labelField": "dictName",
            "valueField": "dictId"
          }
        },
        {
          "label": "协议模板版本",
          "prop": "protocol_template_version",
          "align": "center"
        },
        {
          "label": "操作",
          "fixed": "right",
          "width": 160,
          "align": "center",
          "btns": [
            {
              "type": "look",
              "label": "详情",
              "toRequest": {
                "url": "/views/pt/partnertemplateManage/agreetemplateManage/agreeTemplate_info.json",
                "method": "POST",
                "params": {
                  "protocol_template_no": "${{current.protocol_template_no}}",
                  "version": "${{current.version}}",
                  "servicecode": "pt0078"
                }
              },
              "props": {
                "width": "75%",
                "title": "协议模版详情"
              }
            },
            {
              "type": "edit",
              "label": "维护",
              "toRequest": {
                "url": "/views/pt/partnertemplateManage/projecttemplaterelaagreeManage/projecttemplaterelaAgree_update.json",
                "method": "GET",
                "params": {
                  "project_mould_name": "${{params.project_mould_name}}"
                }
              },
              "show": {
                "expression": "'${{params.show_flash}}' == '1'"
              },
              "props": {
                "width": "75%",
                "title": "合作项目模版协议模版关联维护"
              }
            },
            {
              "type": "custom",
              "label": "删除",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "pt0142"
                }
              },
              "confirm": {
                "title": "确定删除",
                "confirmButtonText": "确定",
                "cancelButtonText": "取消"
              },
              "show": {
                "expression": "'${{params.show_flash}}' == '1'"
              }
            }
          ]
        }
      ]
    }
  }
}
