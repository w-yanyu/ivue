{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "btns": [
        {
          "type": "submit",
          "label": "保存",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "pt0140"
            }
          }
        }
      ],
      "controls": [
        {
          "project_template_no": {
            "control": "string",
            "disabled": true,
            "label": "项目模版编号",
            "value": "${{params.project_template_no}}"
          },
          "project_mould_name": {
            "control": "hidden",
            "label": "项目模版名称",
            "value": "${{params.project_mould_name}}"
          },
          "protocol_template_no": {
            "control": "lookup",
            "edit": true,
            "label": "协议模版编号",
            "keyField": {
              "protocol_template_no": "protocol_template_no",
              "agree_type": "agree_type",
              "mould_name": "mould_name",
              "protocol_template_version": "version"
            },
            "toRequest": {
              "url": "/views/pt/partnertemplateManage/agreetemplateManage/agreeTemplate_lookup.json",
              "method": "GET",
              "params": {}
            },
            "props": {
              "width": "80%",
              "title": "查询协议模版"
            },
            "format": "label",
            "rules": [
              {
                "required": true,
                "message": "请选择协议模版"
              }
            ]
          },
          "mould_name": {
            "control": "hidden",
            "label": "协议模版编号"
          },
          "protocol_template_version": {
            "control": "hidden",
            "label": "协议模版版本"
          }
        },
        {
          "agree_type": {
            "control": "select",
            "label": "协议类型",
            "disabled": true,
            "placeholder": "请选择",
            "dict": {
              "dictType": "PT_E_AGREE_TYPE",
              "dictKey": [
                "PT_E_AGREE_TYPE"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择"
              }
            ]
          },
          "is_must": {
            "control": "select",
            "label": "是否必填",
            "placeholder": "请选择",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择"
              }
            ]
          }
        },
        {
          "is_choice": {
            "control": "hidden",
            "label": "是否可多选",
            "placeholder": "请选择",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            },
            "value":"1"
          },
          "version": {
            "control": "hidden",
            "label": "版本号",
            "value": "${{params.version}}"
          }
        }
      ]
    }
  }
}
