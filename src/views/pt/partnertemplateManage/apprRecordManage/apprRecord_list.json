{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pt0500",
          "sale_prod_cd": "${{params.sale_prod_cd}}",
          "mould_code": "${{params.mould_code}}"
        }
      },
      "thead": [
        {
          "type": "string",
          "align": "center",
          "label": "处理人",
          "prop": "data_create_user"
        },
        {
          "type": "string",
          "align": "center",
          "label": "环节",
          "prop": "approve_link",
          "dict": {
            "dictType": "PT_E_APPROVE_LINK",
            "dictKey": [
              "PT_E_APPROVE_LINK"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "处理时间",
          "prop": "data_create_time"
        },
        {
          "type": "string",
          "align": "center",
          "label": "处理意见",
          "prop": "approve_comments",
          "dict": {
            "dictType": "PT_E_APPROVE_TP",
            "dictKey": [
              "PT_E_APPROVE_TP"
            ]
          }
        }
      ]
    }
  }
}
