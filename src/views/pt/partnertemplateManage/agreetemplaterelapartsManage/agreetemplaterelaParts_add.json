{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "scope": "formData",
            "btns": [
                {
                    "type": "submit",
                    "label": "保存",
                    "doRequest": {
                        "method": "POST",
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "params": {
                            "servicecode": "pt0080",
                            "protocol_template_no": "${{params.protocol_template_no}}"
                        }
                    }
                },
                {
                    "type": "reset",
                    "icon": "el-icon-close",
                    "label": "重置"
                }
            ],
            "controls": [
                {
                    "protocol_template_no": {
                        "control": "hidden",
                        "label": "协议模版编号",
                        "value": "${{params.protocol_template_no}}"
                    },
                    "version": {
                        "control": "hidden",
                        "label": "版本号",
                        "value": "${{params.version}}"
                    }
                },
                {
                    "parts_no": {
                        "control": "lookup",
                        "edit": true,
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择部件"
                            }
                        ],
                        "label": "部件编号",
                        "keyField": {
                            "parts_no": "parts_no",
                            "agree_parts_type": "agree_parts_type",
                            "is_choice":"is_choice"
                        },
                        "toRequest": {
                            "url": "/views/pt/partnertemplateManage/partsManage/ptParts_lookup.json",
                            "method": "post",
                            "params": {
                                "protocol_template_no": "${{formData.protocol_template_no}}",
                                "parts_class": "1",
                                "parts_status":"1"
                            }
                        },
                        "props": {
                            "width": "80%",
                            "title": "查询部件"
                        },
                        "format": "label"
                    },
                    "agree_parts_type": {
                        "control": "select",
                        "label": "协议部件类型",
                        "placeholder": "请选择",
                        "disabled": true,
                        "dict": {
                            "dictType": "PT_E_AGREE_PARTS_TYPE",
                            "dictKey": [
                                "PT_E_AGREE_PARTS_TYPE"
                            ],
                            "format": "label"
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择"
                            }
                        ]
                    }
                },
                {
                    "is_must": {
                        "control": "select",
                        "label": "是否必填",
                        "placeholder": "请选择",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择"
                            }
                        ]
                    },
                    "is_choice": {
                        "control": "hidden",
                        "label": "是否可多选",
                       
                        "placeholder": "请选择",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择"
                            }
                        ]
                    }
                }
            ]
        }
    }
}