{
    "module": "v-realonlyForm-btn",
    "layout": {
        "form": {
            
            "initRequest": {
                    "url": "/SUMP/call/RPCCall",
                    "method": "POST",
                    "params": {
                      "servicecode": "pt0064",
                      "parts_no":"${{params.parts_no}}"
                    }
                  },
            "controls": [
                {
                    "parts_no": {
                        "control": "string",
                        "label": "部件编号",
                        "width": "130px"
                    },
                    "parts_name": {
                        "control": "string",
                        "label": "部件名称",
                        "width": "130px"
                    }
                },
                {
                    "parts_class": {
                        "control": "string",
                        "label": "部件种类",
                        "width": "130px",
                        "dict": {
                          "dictType": "PT_E_PARTS_CLASS",
                          "dictKey": [
                            "PT_E_PARTS_CLASS"
                          ],
                          "format": "value-label"
                        }
                    },
                    "agree_parts_type": {
                        "control": "string",
                        "label": "协议部件类型",
                        "width": "130px",
                        "dict": {
                          "dictType": "PT_E_AGREE_PARTS_TYPE",
                          "dictKey": [
                            "PT_E_AGREE_PARTS_TYPE"
                          ],
                          "format": "value-label"
                        }
                    }
                },
                {
                    "cooperate_project_type": {
                        "control": "string",
                        "label": "项目部件类型",
                        "width": "130px",
                        "dict": {
                          "dictType": "PT_E_COOPERATE_PROJECT_TYPE",
                          "dictKey": [
                            "PT_E_COOPERATE_PROJECT_TYPE"
                          ],
                          "format": "value-label"
                        }
                    },
                    "parts_status": {
                        "control": "string",
                        "label": "部件状态",
                        "width": "130px",
                        "dict": {
                          "dictType": "PT_E_STATUS",
                          "dictKey": [
                            "PT_E_STATUS"
                          ],
                          "format": "value-label"
                        }
                    }
                },
                {
                    "add_url": {
                        "control": "string",
                        "label": "新增页面路径",
                        "width": "130px"
                    },
                    "update_url": {
                        "control": "string",
                        "label": "维护页面路径",
                        "width": "130px"
                    }
                },
                {
                  "info_url": {
                      "control": "string",
                      "label": "详情页面路径",
                      "width": "130px"
                  }
                }
            ]
        }
    }
}
