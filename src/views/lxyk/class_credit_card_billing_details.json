{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "smilr_cr_card_loan_acct_num": {
            "control": "hidden",
            "value": "${{params.smilr_cr_card_loan_acct_num}}",
            "label": "类信用卡贷款账号"
          },
          "bill_prds": {
            "control": "text",
            "label": "账期"
          },
          "txn_evt": {
            "control": "select",
            "label": "交易事件",
            "dict": {
              "dictType": "LN_E_TXN_EVT",
              "dictKey": [
                "LN_E_TXN_EVT"
              ],
              "format": "label"
            }
          }
        },
        {
          "strt_dt_8": {
            "control": "dateTimePicker",
            "label": "起始日期",
            "type": "date",
            "unlinkPanels": true,
            "format": "yyyyMMdd",
            "disabledDate": {
              "startRange": "19000101",
              "endRange": "99991231"
            }
          },
          "matu_dt": {
            "control": "dateTimePicker",
            "label": "到期日期",
            "type": "date",
            "unlinkPanels": true,
            "format": "yyyyMMdd",
            "disabledDate": {
              "startRange": "19000101",
              "endRange": "99991231"
            }
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ln3998",
          "outlistid": "listnm"
        }
      },
      "thead": [
        {
          "label": "类信用卡贷款账号",
          "prop": "smilr_cr_card_loan_acct_num",
          "align": "center"
        },
        {
          "label": "贷款借据号",
          "prop": "loan_due_bill_num",
          "align": "center"
        },
        {
          "label": "账期",
          "prop": "bill_prds",
          "align": "center"
        },
        {
          "label": "明细序号",
          "prop": "detl_serl_num",
          "align": "center"
        },
        {
          "label": "交易金额",
          "prop": "txn_amt",
          "align": "center"
        },
        {
          "label": "交易事件",
          "prop": "txn_evt",
          "align": "center",
          "dict": {
            "dictType": "LN_E_TXN_EVT",
            "dictKey": [
              "LN_E_TXN_EVT"
            ],
            "format": "label"
          }
        },
        {
          "label": "交易事件说明",
          "prop": "txn_evt_comt",
          "align": "center"
        },
        {
          "label": "摘要描述",
          "prop": "abst_dsc",
          "align": "center"
        },
        {
          "label": "贷款入账账号",
          "prop": "loan_pstg_acct_num",
          "align": "center"
        },
        {
          "label": "贷款入账子账户序号",
          "prop": "loan_pstg_sub_acct_serl_num",
          "align": "center"
        },
        {
          "label": "交易机构",
          "prop": "txn_org",
          "align": "center"
        },
        {
          "label": "交易用户",
          "prop": "txn_tlr",
          "align": "center"
        },
        {
          "label": "交易码",
          "prop": "txn_code",
          "align": "center"
        },
        {
          "label": "第三方交易码",
          "prop": "third_party_txn_code",
          "align": "center"
        },
        {
          "label": "交易流水号",
          "prop": "txn_rung_num_32",
          "align": "center"
        },
        {
          "label": "全局流水号",
          "prop": "glbl_rung_num",
          "align": "center"
        },
        {
          "label": "交易日期",
          "prop": "txn_dt_8",
          "align": "center"
        },
        {
          "label": "还款账号",
          "prop": "rpymt_acct_num",
          "align": "center"
        },
        {
          "label": "还款子账户序号",
          "prop": "rpymt_sub_acct_serl_num",
          "align": "center"
        },
        {
          "label": "分期标志",
          "prop": "istlmt_flg",
          "align": "center",
          "dict": {
            "dictType": "MS_E_YESORNO",
            "dictKey": [
              "MS_E_YESORNO"
            ],
            "format": "label"
          }
        },
        {
          "label": "分期借据号",
          "prop": "istlmt_due_bill_num",
          "align": "center"
        },
        {
          "label": "冲正标志",
          "prop": "rvrs_flg",
          "align": "center",
          "dict": {
            "dictType": "E_RVRS_FLG",
            "dictKey": [
              "E_RVRS_FLG"
            ],
            "format": "label"
          }
        },
        {
          "label": "被冲正标志",
          "prop": "be_rvrsd_flg",
          "align": "center",
          "dict": {
            "dictType": "E_STRK_BAL_ADD_ACCT_FLG",
            "dictKey": [
              "E_STRK_BAL_ADD_ACCT_FLG"
            ],
            "format": "label"
          }
        },
        {
          "label": "错账原日期",
          "prop": "err_acct_orig_dt",
          "align": "center"
        },
        {
          "label": "错账原用户流水号",
          "prop": "err_acct_orig_tlr_rung_num",
          "align": "center"
        },
        {
          "label": "原错账全局流水号",
          "prop": "orig_err_acct_glbl_rung_num",
          "align": "center"
        }
      ]
    }
  }
}