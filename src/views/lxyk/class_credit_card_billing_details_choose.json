{
  "module": "v_search_btn_datagrid",
  "layout": {
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "label": "提交",
          "doRequest": {
            "url": "/SUMP/call/RPCCall",
            "method": "POST",
            "params": {
              "servicecode": "ln3976",
              "smilr_cr_card_loan_acct_num": "${{params.smilr_cr_card_loan_acct_num}}",
              "cust_num": "${{form.cust_num}}",
              "cust_nm": "${{form.cust_nm}}",
              "ret_goods_amt": "${{form.ret_goods_amt}}",
              "cptl_src_acct_num": "${{form.cptl_src_acct_num}}",
              "cptl_src_sub_acct_serl_num": "${{form.cptl_src_sub_acct_serl_num}}"
            }
          }
        }
      ]
    },
    "datagrid": {
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ln3998",
          "smilr_cr_card_loan_acct_num": "${{params.smilr_cr_card_loan_acct_num}}",
          "txn_evt": "${{params.txn_evt}}",
          "outlistid": "listnm"
        }
      },
      "thead": [
        {
          "type": "selection"
        },
        {
          "label": "类信用卡贷款账号",
          "prop": "smilr_cr_card_loan_acct_num",
          "align": "center"
        },
        {
          "label": "贷款借据号",
          "prop": "loan_due_bill_num",
          "align": "center"
        },
        {
          "label": "账期",
          "prop": "bill_prds",
          "align": "center"
        },
        {
          "label": "明细序号",
          "prop": "detl_serl_num",
          "align": "center"
        },
        {
          "label": "交易金额",
          "prop": "txn_amt",
          "align": "center"
        },
        {
          "label": "交易事件",
          "prop": "txn_evt",
          "align": "center",
          "dict": {
            "dictType": "E_TRANSACTION_EVENT",
            "dictKey": [
              "E_TRANSACTION_EVENT"
            ],
            "format": "label"
          }
        },
        {
          "label": "交易事件说明",
          "prop": "txn_evt_comt",
          "align": "center"
        },
        {
          "label": "摘要描述",
          "prop": "abst_dsc",
          "align": "center"
        },
        {
          "label": "贷款入账账号",
          "prop": "loan_pstg_acct_num",
          "align": "center"
        },
        {
          "label": "贷款入账子账户序号",
          "prop": "loan_pstg_sub_acct_serl_num",
          "align": "center"
        },
        {
          "label": "交易机构",
          "prop": "txn_org",
          "align": "center"
        },
        {
          "label": "交易码",
          "prop": "txn_code",
          "align": "center"
        },
        {
          "label": "交易流水号",
          "prop": "txn_rung_num_32",
          "align": "center"
        },
        {
          "label": "全局流水号",
          "prop": "glbl_rung_num",
          "align": "center"
        },
        {
          "label": "交易日期",
          "prop": "txn_dt_8",
          "align": "center"
        },
        {
          "label": "还款账号",
          "prop": "rpymt_acct_num",
          "align": "center"
        },
        {
          "label": "还款子账户序号",
          "prop": "rpymt_sub_acct_serl_num",
          "align": "center"
        },
        {
          "label": "分期标志",
          "prop": "istlmt_flg",
          "align": "center",
          "dict": {
            "dictType": "MS_E_YESORNO",
            "dictKey": [
              "MS_E_YESORNO"
            ],
            "format": "label"
          }
        },
        {
          "label": "分期借据号",
          "prop": "istlmt_due_bill_num",
          "align": "center"
        },
        {
          "label": "冲正标志",
          "prop": "rvrs_flg",
          "align": "center",
          "dict": {
            "dictType": "E_RVRS_FLG",
            "dictKey": [
              "E_RVRS_FLG"
            ],
            "format": "label"
          }
        },
        {
          "label": "被冲正标志",
          "prop": "be_rvrsd_flg",
          "align": "center",
          "dict": {
            "dictType": "E_STRK_BAL_ADD_ACCT_FLG",
            "dictKey": [
              "E_STRK_BAL_ADD_ACCT_FLG"
            ],
            "format": "label"
          }
        }
      ]
    }
  }
}