{
  "module": "v_search_btn_datagrid",
  "layout": {
    "pageId": "D0107",
    "form": {
      "title": "查询条件",
      "controls": [
        {
          "prod_code": {
            "control": "text",
            "label": "产品代码"
          },
          "prod_nm": {
            "control": "text",
            "label": "产品名称"
          }
        },
        {
          "loan_prod_ste": {
            "control": "select",
            "label": "贷款产品状态",
            "dict": {
              "dictType": "LN_E_LOAN_PROD_STE",
              "dictKey": [
                "LN_E_LOAN_PROD_STE"
              ],
              "format": "label"
            }
          },
          "loan_obj": {
            "control": "select",
            "label": "贷款对象",
            "dict": {
              "dictType": "LN_E_LOAN_OBJ",
              "dictKey": [
                "LN_E_LOAN_OBJ"
              ],
              "format": "label"
            }
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ln3004",
          "outlistid": "listnm"
        }
      },
      "thead": [
        {
          "label": "产品代码",
          "prop": "prod_code",
          "align": "center"
        },
        {
          "label": "产品名称",
          "prop": "prod_nm",
          "align": "center"
        },
        {
          "label": "贷款对象",
          "prop": "loan_obj",
          "align": "center",
          "dict": {
            "dictType": "LN_E_LOAN_OBJ",
            "dictKey": [
              "LN_E_LOAN_OBJ"
            ],
            "format": "label"
          }
        },
        {
          "label": "贷款处理模式",
          "prop": "loan_deal_mode",
          "align": "center",
          "isShow": false,
          "dict": {
            "dictType": "E_LOAN_DEAL_MODE",
            "dictKey": [
              "E_LOAN_DEAL_MODE"
            ],
            "format": "label"
          }
        },
        {
          "label": "生效日期",
          "prop": "eff_dt",
          "align": "center"
        },
        {
          "label": "失效日期",
          "prop": "invld_dt",
          "align": "center"
        },
        {
          "label": "贷款产品状态",
          "prop": "loan_prod_ste",
          "align": "center",
          "dict": {
            "dictType": "LN_E_LOAN_PROD_STE",
            "dictKey": [
              "LN_E_LOAN_PROD_STE"
            ],
            "format": "label"
          }
        },
        {
          "label": "货币代号",
          "prop": "ccy_code_num",
          "align": "center",
          "dict": {
            "dictType": "MS_CURRENCY",
            "dictKey": [
              "MS_CURRENCY"
            ],
            "format": "label"
          }
        },
        {
          "label": "操作",
          "fixed": "right",
          "width": 130,
          "align": "center",
          "btns": [
            {
              "type": "bringback",
              "label": "选择"
            }
          ]
        }
      ]
    }
  }
}