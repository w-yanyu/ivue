{
    "module": "v_search_btn_datagrid",
    "layout": {
        "datagrid": {
            "isInitSearch": true,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "ln3996",
                    "smilr_cr_card_loan_acct_num": "${{params.smilr_cr_card_loan_acct_num}}",
                    "istlmt_tp": "${{params.istlmt_tp}}",
                    "istlmt_prds": "${{params.istlmt_prds}}",
                    "outlistid": "lstSmilrCrCardTrialOutInfo",
                    "this_bill_istlmt_amt": "${{params.prcpl_smtn}}",
                    "addList": "${{params.goodsList}}"
                }
            },
            "thead": [
                {
                    "label": "账期",
                    "prop": "bill_prds",
                    "align": "center",
                    "disabled": true
                },
                {
                    "label": "账单起始日期",
                    "prop": "bill_strt_dt",
                    "align": "center"
                },
                {
                    "label": "账单到期日期",
                    "prop": "bill_matu_dt",
                    "align": "center"
                },
                {
                    "label": "账单还款日期",
                    "prop": "bill_rpymt_dt",
                    "align": "center"
                },
                {
                    "label": "账单宽限日期",
                    "prop": "bill_grace_dt",
                    "align": "center"
                },
                {
                    "label": "本期应还总金额",
                    "prop": "this_prd_need_rpymt_totl_amt",
                    "align": "center"
                },
                {
                    "label": "本期应还本金",
                    "prop": "this_prd_need_rpymt_prcpl",
                    "align": "center"
                },
                {
                    "label": "本期应还利息",
                    "prop": "this_prd_need_rpymt_int",
                    "align": "center"
                },
                {
                    "label": "本期应还费用",
                    "prop": "this_prd_need_rpymt_fee",
                    "align": "center"
                },
                {
                    "label": "剩余金额",
                    "prop": "rsdl_amt",
                    "align": "center"
                }
            ]
        }
    }
}