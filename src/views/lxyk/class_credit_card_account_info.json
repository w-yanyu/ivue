{
  "module": "v_realonlyForm_btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ln3997",
          "smilr_cr_card_loan_acct_num": "${{params.smilr_cr_card_loan_acct_num}}"
        }
      },
      "controls": [
        {
          "loan_bill_prd": {
            "control": "text",
            "label": "账单周期",
            "width": "100px",
            "disabled": true
          },
          "rpymt_prd": {
            "control": "text",
            "label": "还款周期",
            "width": "100px",
            "disabled": true
          }
        },
        {
          "rpymt_bill_dt_intrv_dys": {
            "control": "text",
            "label": "还款日账单日间隔天数",
            "width": "100px",
            "disabled": true
          },
          "cur_bill_prds": {
            "control": "text",
            "label": "当前账期",
            "width": "100px",
            "disabled": true
          }
        },
        {
          "cur_loan_bill_dt": {
            "control": "text",
            "label": "当前出账日期",
            "width": "100px",
            "disabled": true
          },
          "cur_rpymt_dt": {
            "control": "text",
            "label": "当前还款日期",
            "width": "100px",
            "disabled": true
          }
        },
        {
          "last_prd_bill_dt": {
            "control": "text",
            "label": "上期出账日期",
            "width": "100px",
            "disabled": true
          },
          "next_prd_bill_dt": {
            "control": "text",
            "label": "下期出账日期",
            "width": "100px",
            "disabled": true
          }
        },
        {
          "have_grace_prd_flg": {
            "control": "select",
            "label": "是否有宽限期",
            "width": "100px",
            "disabled": true,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "grace_prd_dys": {
            "control": "text",
            "label": "宽限期天数",
            "width": "100px",
            "disabled": true
          }
        },
        {
          "wthr_alw_cnsp_istlm": {
            "control": "select",
            "label": "是否允许消费分期",
            "width": "100px",
            "disabled": true,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "cnsp_istlmt_prod_code": {
            "control": "text",
            "label": "消费分期产品代码",
            "width": "100px",
            "disabled": true
          }
        },
        {
          "wthr_alw_txn_istlmt": {
            "control": "select",
            "label": "是否允许交易分期",
            "width": "100px",
            "disabled": true,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "txn_istlmt_prod_code": {
            "control": "text",
            "label": "交易分期产品代码",
            "width": "100px",
            "disabled": true
          }
        },
        {
          "wthr_alw_bill_istlmt": {
            "control": "select",
            "label": "是否允许账单分期",
            "width": "100px",
            "disabled": true,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "bill_istlmt_prod_code": {
            "control": "text",
            "label": "账单分期产品代码",
            "width": "100px",
            "disabled": true
          }
        },
        {
          "wthr_alw_pstp_rpymt": {
            "control": "select",
            "label": "是否允许延期还款",
            "width": "100px",
            "disabled": true,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "pstp_prod_code": {
            "control": "text",
            "label": "延期产品代码",
            "width": "100px",
            "disabled": true
          }
        },
        {
          "pstp_rpymt_chrg_code": {
            "control": "text",
            "label": "延期还款收费代码",
            "width": "100px",
            "disabled": true
          }
        },
        {
          "istlmt_min_amt_rstct": {
            "control": "text",
            "label": "分期最低金额限制",
            "width": "100px",
            "disabled": true
          },
          "istlmt_max_amt_rstct": {
            "control": "text",
            "label": "分期最高金额限制",
            "width": "100px",
            "disabled": true
          }
        },
        {
          "pstp_min_amt_rstct": {
            "control": "text",
            "label": "延期最低金额限制",
            "width": "100px",
            "disabled": true
          },
          "pstp_max_amt_rstc": {
            "control": "text",
            "label": "延期最高金额限制",
            "width": "100px",
            "disabled": true
          }
        },
        {
          "auto_rpymt_set": {
            "control": "select",
            "label": "自动还款设置",
            "width": "100px",
            "disabled": true,
            "dict": {
              "dictType": "E_AUTO_RPYMT_SET",
              "dictKey": [
                "E_AUTO_RPYMT_SET"
              ],
              "format": "label"
            }
          },
          "wthr_alw_min_rpymt_amt": {
            "control": "select",
            "label": "是否允许最小还款额还款",
            "width": "100px",
            "disabled": true,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          }
        },
        {
          "min_rpymt_caln_pcnt": {
            "control": "text",
            "label": "最低还款计算比例（%）",
            "width": "100px",
            "disabled": true
          },
          "smilr_cr_card_rpymt_seq_num": {
            "control": "text",
            "label": "类信用卡还款顺序编号",
            "width": "100px",
            "disabled": true
          }
        },
        {
          "cnsp_due_bill_int_caln_way": {
            "control": "select",
            "label": "消费借据计息方式",
            "width": "100px",
            "disabled": true,
            "dict": {
              "dictType": "E_CNSP_DUE_BILL_INT_CALN_WAY",
              "dictKey": [
                "E_CNSP_DUE_BILL_INT_CALN_WAY"
              ],
              "format": "label"
            }
          },
          "cnsp_due_bill_deal_mode": {
            "control": "select",
            "label": "消费借据处理模式",
            "width": "100px",
            "disabled": true,
            "dict": {
              "dictType": "E_CNSP_DUE_BILLDEAL_MODE",
              "dictKey": [
                "E_CNSP_DUE_BILLDEAL_MODE"
              ],
              "format": "label"
            }
          }
        },
        {
          "cnsp_nrl_int_rate": {
            "control": "text",
            "label": "消费正常利率",
            "width": "100px",
            "disabled": true
          },
          "cnsp_pnly_int_rate": {
            "control": "text",
            "label": "消费罚息利率",
            "width": "100px",
            "disabled": true
          }
        },
        {
          "istlmt_nrl_int_rate": {
            "control": "text",
            "label": "分期正常利率",
            "width": "100px",
            "disabled": true
          },
          "istlmt_pnly_int_rate": {
            "control": "text",
            "label": "分期罚息利率",
            "width": "100px",
            "disabled": true
          }
        },
        {
          "pstp_nrl_int_rate": {
            "control": "text",
            "label": "延期正常利率",
            "width": "100px",
            "disabled": true
          },
          "pstp_pnly_int_rate": {
            "control": "text",
            "label": "延期罚息利率",
            "width": "100px",
            "disabled": true
          }
        }
      ]
    }
  }
}