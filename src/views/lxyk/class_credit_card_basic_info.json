{
  "module": "v_realonlyForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "controls": [
        {
          "smilr_cr_card_loan_acct_num": {
            "control": "text",
            "label": "类信用卡贷款账号",
            "width": "100px",
            "disabled": true
          },
          "cust_num": {
            "control": "text",
            "label": "客户号",
            "width": "100px",
            "disabled": true
          }
        },
        {
          "cust_nm": {
            "control": "text",
            "label": "客户名称",
            "width": "100px",
            "disabled": true
          },
          "ccy_code_num": {
            "control": "select",
            "label": "货币代号",
            "width": "100px",
            "disabled": true,
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": [
                "MS_CURRENCY"
              ],
              "format": "label"
            }
          }
        },
        {
          "prod_code": {
            "control": "text",
            "label": "产品代码",
            "width": "100px",
            "disabled": true
          },
          "prod_nm": {
            "control": "text",
            "label": "产品名称",
            "width": "100px",
            "disabled": true
          }
        },
        {
          "cnsp_prod_code": {
            "control": "hidden",
            "label": "消费产品代码",
            "width": "100px",
            "disabled": true,
            "events": {
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "ln3271",
                  "prod_code": "${{params.cnsp_prod_code}}"
                },
                "resultSet": {
                  "cnsp_prod_nm": "prod_nm"
                }
              }
            }
          },
          "cnsp_prod_nm": {
            "control": "text",
            "label": "消费产品名称",
            "width": "100px",
            "disabled": true
          },
          "acctg_org": {
            "control": "text",
            "label": "账务机构",
            "width": "100px",
            "disabled": true
          }
        },
        {
          "oprtg_org": {
            "control": "text",
            "label": "营业机构",
            "width": "100px",
            "disabled": true
          },
          "open_acct_dt": {
            "control": "text",
            "label": "开户日期",
            "width": "100px",
            "disabled": true
          }
        },
        {
          "matu": {
            "control": "text",
            "label": "期限",
            "width": "100px",
            "disabled": true
          },
          "matu_dt": {
            "control": "text",
            "label": "到期日期",
            "width": "100px",
            "disabled": true
          }
        },
        {
          "loan_pstg_acct_num": {
            "control": "text",
            "label": "贷款入账账号",
            "width": "100px",
            "disabled": true
          },
          "loan_pstg_sub_acct_serl_num": {
            "control": "text",
            "label": "贷款入账子账户序号",
            "width": "100px",
            "disabled": true
          }
        },
        {
          "rpymt_acct_num": {
            "control": "text",
            "label": "还款账号",
            "width": "100px",
            "disabled": true
          },
          "rpymt_sub_acct_serl_num": {
            "control": "text",
            "label": "还款子账户序号",
            "width": "100px",
            "disabled": true
          }
        },
        {
          "slbl_prod_code": {
            "control": "text",
            "label": "可售产品代码",
            "width": "100px",
            "disabled": true
          },
          "slbl_prod_nm": {
            "control": "text",
            "label": "可售产品名称",
            "width": "100px",
            "disabled": true
          }
        },
        {
          "open_acct_org": {
            "control": "text",
            "label": "开户机构",
            "width": "100px",
            "disabled": true
          },
          "open_acct_tlr_num": {
            "control": "text",
            "label": "开户用户号",
            "width": "100px",
            "disabled": true
          }
        },
        {
          "cncl_acct_dt": {
            "control": "text",
            "label": "销户日期",
            "width": "100px",
            "disabled": true
          },
          "pre_cncl_acct_dt": {
            "control": "text",
            "label": "预销户日期",
            "width": "100px",
            "disabled": true
          }
        },
        {
          "smilr_cr_card_acct_ste": {
            "control": "select",
            "label": "类信用卡账户状态",
            "width": "100px",
            "disabled": true,
            "dict": {
              "dictType": "E_SMILR_CR_CARD_ACCT_STE",
              "dictKey": [
                "E_SMILR_CR_CARD_ACCT_STE"
              ],
              "format": "label"
            }
          }
        }
      ]
    }
  }
}