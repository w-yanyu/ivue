{
  "module": "v-form-btn-dynamic",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "formSetTitle": "试算条件",
      "formResultTitle": "试算结果",
      "controls": [
        {
          "smilr_cr_card_loan_acct_num": {
            "control": "string",
            "value": "${{params.smilr_cr_card_loan_acct_num}}",
            "label": "类信用卡贷款账号"
          },
          "this_bill_istlmt_amt": {
            "control": "currency",
            "label": "本账单分期金额",
            "rules": [
              {
                "required": true,
                "message": "请输入分期金额"
              }
            ]
          }
        },
        {
          "istlmt_tp": {
            "control": "select",
            "label": "分期类型",
            "dict": {
              "dictType": "E_ISTLMT_TP",
              "dictKey": [
                "E_ISTLMT_TP"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择分期类型"
              }
            ]
          },
          "istlmt_prds": {
            "control": "select",
            "label": "分期期数",
            "dict": {
              "dictType": "E_ISTLMT_PRDS",
              "dictKey": [
                "E_ISTLMT_PRDS"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择分期期数"
              }
            ]
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "submit",
          "label": "分期试算",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "post",
            "params": {
              "servicecode": "ln3996"
            }
          },
          "toRequest": {
            "url": "/views/lxyk/class_credit_card_staging_trial.json",
            "method": "get",
            "params": {
              "params": "${{formData}}"
            }
          }
        },
        {
          "type": "reset",
          "btnStyle": "success",
          "label": "重置"
        }
      ]
    }
  }
}
