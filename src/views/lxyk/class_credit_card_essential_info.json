{
  "module": "v_realonlyForm_btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ln3999",
          "smilr_cr_card_loan_acct_num": "${{params.smilr_cr_card_loan_acct_num}}"
        }
      },
      "controls": [
        {
          "smilr_cr_card_loan_acct_num": {
            "control": "text",
            "label": "类信用卡贷款账号",
