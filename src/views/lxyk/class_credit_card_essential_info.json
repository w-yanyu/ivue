        {
          "smilr_cr_card_acct_ste": {
            "control": "select",
            "label": "类信用卡账户状态",
            "width": "100px",
            "disabled": true,
            "dict": {
              "dictType": "E_SMILR_CR_CARD_ACCT_STE",
              "dictKey": [
                "E_SMILR_CR_CARD_ACCT_STE"
              ],
              "format": "label"
            }
          },
          "total_bill_prds": {
            "control": "text",
            "label": "总账期",
            "width": "100px",
            "disabled": true
