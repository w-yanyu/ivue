{
  "module": "v-realonlyForm-btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "ar0048",
          "apply_no": "${{params.apply_no}}"
        }
      },
      "controls": [
        {
          "order_form_num": {
            "control": "text",
            "disabled": true,
            "label": "订单号",
            "span": 12
          },
          "payment_dt": {
            "control": "dateTimePicker",
            "format": "yyyy-MM",
            "valueFormat": "yyyyMM",
            "disabled": true,
            "label": "订单所属账期",
            "span": 12
          }
        },
        {
          "refund_account_period": {
            "control": "dateTimePicker",
            "format": "yyyy-MM",
            "valueFormat": "yyyyMM",
            "disabled": true,
            "label": "退款入账账期",
            "span": 12
          },
          "refund_account_date": {
            "control": "dateTimePicker",
            "format": "yyyy-MM-dd",
            "valueFormat": "yyyyMMdd",
            "disabled": true,
            "label": "退款入账日期",
            "span": 12
          }
        },
        {
          "refund_acct_num": {
            "control": "text",
            "disabled": true,
            "label": "退款入账账号",
            "span": 12
          },
          "order_amount": {
            "control": "currency",
            "disabled": true,
            "label": "订单金额",
            "format": {
              "thousand": ","
            },
            "span": 12
          }
        },
        {
          "refund_date": {
            "control": "dateTimePicker",
            "format": "yyyy-MM-dd",
            "valueFormat": "yyyyMMdd",
            "disabled": true,
            "label": "退款日期",
            "span": 12
          },
          "refund_amt": {
            "control": "currency",
            "disabled": true,
            "label": "退款金额",
            "format": {
              "thousand": ","
            },
            "span": 12
          }
        },
        {
          "remks": {
            "control": "textarea",
            "disabled": true,
            "label": "退款原因",
            "rows": 6,
            "span": 12
          }
        }
      ]
    }
  }
}