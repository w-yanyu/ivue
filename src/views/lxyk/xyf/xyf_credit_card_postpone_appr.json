{
  "module": "v-realonlyForm-btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "ar0052",
          "apply_no": "${{params.apply_no}}"
        }
      },
      "controls": [
        {
          "smilr_cr_card_loan_acct_num": {
            "control": "text",
            "disabled": true,
            "label": "类信用卡贷款账号",
            "span": 12
          },
          "bill_prds": {
            "control": "dateTimePicker",
            "format": "yyyy-MM",
            "valueFormat": "yyyyMM",
            "disabled": true,
            "label": "账期",
            "span": 12
          }
        },
        {
          "pstp_amt": {
            "control": "currency",
            "disabled": true,
            "label": "延期金额",
            "format": {
              "thousand": ","
            },
            "span": 12
          },
          "pstp_rpymt_chrg_code": {
            "control": "text",
            "disabled": true,
            "label": "延期还款收费代码",
            "span": 12
          }
        },
        {
          "remks": {
            "control": "textarea",
            "disabled": true,
            "label": "延期原因",
            "rows": 6,
            "span": 12
          }
        }
      ]
    }
  }
}