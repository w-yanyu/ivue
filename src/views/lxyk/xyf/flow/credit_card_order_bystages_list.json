{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ar0068",
          "apply_no": "${{params.apply_no}}"
        }
      },
      "thead": [
        {
          "prop": "smilr_cr_card_loan_acct_num",
          "align": "center",
          "label": "类信用卡贷款账号"
        },
        {
          "label": "订单编号",
          "prop": "order_form_num",
          "align": "center"
        },
        {
          "prop": "cust_no",
          "type": "text",
          "label": "客户编号"
        },
        {
          "prop": "cust_name",
          "type": "text",
          "label": "客户名称"
        },
        {
          "label": "订单金额",
          "prop": "order_amount",
          "align": "center"
        },
        {
          "label": "分期期数",
          "prop": "istlmt_prds",
          "align": "center"
        },
        {
          "label": "消费日期",
          "prop": "pay_date",
          "align": "center"
        }
      ]
    }
  }
}