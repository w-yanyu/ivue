{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "smilr_cr_card_loan_acct_num": {
            "control": "hidden",
            "value": "${{params.smilr_cr_card_loan_acct_num}}",
            "label": "类信用卡贷款账号"
          },
          "txn_evt": {
            "control": "hidden",
            "label": "退款方式",
            "value": "TK"
          },
          "bill_prds": {
            "type": "month",
            "control": "dateTimePicker",
            "format": "yyyyMM",
            "label": "账期"
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "syln0011",
          "outlistid": "list01"
        }
      },
      "thead": [
        {
          "label": "类信用卡账号",
          "prop": "smilr_cr_card_loan_acct_num",
          "align": "center"
        },{
          "label": "账期",
          "prop": "bill_prds",
          "align": "center"
        },
        {
          "label": "明细序号",
          "prop": "detl_serl_num",
          "align": "center"
        },
        {
          "label": "退款金额",
          "prop": "txn_amt",
          "format": {
            "thousand": ","
          },
          "align": "center"
        },
        {
          "label": "交易事件",
          "prop": "txn_evt",
          "align": "center",
          "dict": {
            "dictType": "LN_E_TXN_EVT",
            "dictKey": [
              "LN_E_TXN_EVT"
            ],
            "format": "label"
          }
        },
        {
          "label": "交易日期",
          "prop": "txn_dt_8",
          "align": "center"
        }
      ]
    }
  }
}