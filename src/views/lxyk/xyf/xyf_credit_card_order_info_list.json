{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ar0058",
          "outlistid": "orderInfoList",
          "order_form_num": "${{params.order_form_num}}",
          "apply_no": "${{params.apply_no}}"
        }
      },
      "thead": [
        {
          "label": "订单编号",
          "prop": "order_form_num",
          "align": "center"
        },
        {
          "prop": "product_id",
          "type": "text",
          "label": "商品编号"
        },
        {
          "prop": "product_nm",
          "type": "text",
          "label": "商品名称"
        },
        {
          "prop": "product_univalence",
          "type": "currency",
          "label": "商品单价(元)"
        },
        {
          "prop": "product_count",
          "type": "inputNumber",
          "label": "商品数量",
          "min": 1
        },
        {
          "prop": "total_money",
          "type": "currency",
          "label": "总金额(元)"
        }
      ]
    }
  }
}
