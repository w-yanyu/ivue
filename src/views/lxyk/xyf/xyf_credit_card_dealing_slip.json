{
  "module": "dynamicTabs",
  "layout": {
    "tabs": {
      "tabHeader": [
        {
          "id": "01",
          "label": "消费记录",
          "closable": false,
          "disabled": false,
          "icon": "el-icon-date",
          "toRequest": {
            "url": "/views/lxyk/xyf/xyf_credit_card_order_list.json",
            "method": "post",
            "params": {
              "cust_no": "${{params.cust_num}}"
            }
          }
        },
        {
          "id": "02",
          "label": "还款记录",
          "closable": false,
          "disabled": false,
          "icon": "el-icon-date",
          "toRequest": {
            "url": "/views/lxyk/xyf/xyf_credit_card_payment_history.json",
            "method": "post"
          }
        },
        {
          "id": "03",
          "label": "退款记录",
          "closable": false,
          "disabled": false,
          "icon": "el-icon-date",
          "toRequest": {
            "url": "/views/lxyk/xyf/xyf_credit_card_refund_record.json",
            "method": "post"
          }
        },
        {
          "id": "04",
          "label": "分期记录",
          "closable": false,
          "disabled": false,
          "icon": "el-icon-date",
          "toRequest": {
            "url": "/views/lxyk/xyf/xyf_credit_card_record_by_stage.json",
            "method": "post",
            "params":{
              "cust_num":"${{params.cust_num}}",
              "cust_nm":"${{params.cust_nm}}"
            }
          }
        }
      ],
      "defaultActiveKey": "01",
      "position": "top",
      "styleClass": "card"
    }


  }
}
