{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "scope": "form",
      "btns": [
        {
          "type": "lxykCreateAccountStartBtn",
          "label": "申请开户",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "ar0035"
            }
          }
        }
      ],
      "controlsGroup": [
        {
          "title": "基本信息",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "accordion": false,
          "expand": true,
          "controls": [
            {
              "cust_no": {
                "control": "hidden",
                "label": "客户编号"
              },
              "cust_name": {
                "control": "lookup",
                "label": "客户姓名",
                "edit": true,
                "span": 12,
                "width": "50px",
                "keyField": {
                  "cust_no": "cust_no",
                  "cust_name": "cust_name",
                  "cert_type": "cert_type",
                  "cert_no": "cert_no",
                  "cert_day": "cert_day",
                  "card_end_day": "card_end_day",
                  "issuing_addr": "issuing_addr",
                  "country_code": "country_code",
                  "education": "education",
                  "is_work": "is_work",
                  "unit_nature": "unit_nature",
                  "year_income": "year_income",
                  "marr_state": "marr_state",
                  "spouse_name": "spouse_name",
                  "spouse_cert_type": "spouse_cert_type",
                  "spouse_cert_no": "spouse_cert_no",
                  "mobile_no": "mobile_no"
                },
                "toRequest": {
                  "url": "/views/us/cust_lookup_for_private.json",
                  "method": "post"
                },
                "multiple": false,
                "props": {
                  "title": "客户选择"
                }
              },
              "mobile_no": {
                "control": "text",
                "label": "手机号码",
                "span": 12,
                "width": "50px",
                "rules": [
                  {
                    "message": "请输入手机号码",
                    "required": true
                  }
                ]
              },
              "cert_type": {
                "control": "select",
                "label": "证件类型",
                "span": 12,
                "width": "50px",
                "dict": {
                  "dictType": "MS_E_CRET_TYPE",
                  "dictKey": [
                    "MS_E_CRET_TYPE"
                  ],
                  "format": "label"
                },
                "rules": [
                  {
                    "message": "请输入证件类型",
                    "required": true
                  }
                ]
              },
              "cert_no": {
                "control": "text",
                "label": "证件号码",
                "span": 12,
                "width": "50px",
                "rules": [
                  {
                    "message": "请输入证件号码",
                    "required": true
                  }
                ]
              },
              "cert_day": {
                "control": "dateTimePicker",
                "label": "证件生效日",
                "span": 12,
                "width": "50px",
                "editable": true,
                "format": "yyyyMMdd",
                "rules": [
                  {
                    "message": "请输入证件生效日",
                    "required": false
                  }
                ]
              },
              "card_end_day": {
                "control": "dateTimePicker",
                "label": "证件到期日",
                "editable": true,
                "format": "yyyyMMdd",
                "span": 12,
                "width": "50px",
                "rules": [
                  {
                    "message": "请输入证件到期日",
                    "required": false
                  }
                ]
              },
              "issuing_addr": {
                "control": "areaCascade",
                "label": "签发地址",
                "span": 12,
                "width": "50px",
                "rules": [
                  {
                    "message": "请输入签发地址",
                    "required": true
                  }
                ]
              },
              "country_code": {
                "control": "select",
                "label": "国籍",
                "span": 12,
                "width": "50px",
                "dict": {
                  "dictType": "US_E_INCTRY",
                  "dictKey": [
                    "US_E_INCTRY"
                  ]
                },
                "rules": [
                  {
                    "message": "请输入国籍",
                    "required": true
                  }
                ]
              }
            }
          ]
        },
        {
          "title": "学历工作信息",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "accordion": false,
          "expand": true,
          "controls": [
            {
              "education": {
                "control": "select",
                "label": "最高学历",
                "span": 12,
                "width": "50px",
                "dict": {
                  "dictType": "MS_E_MAXEDU",
                  "dictKey": [
                    "DICT_NAME"
                  ],
                  "labelField": "dictName",
                  "valueField": "dictId"
                }
              },
              "is_work": {
                "control": "select",
                "label": "是否有工作单位",
                "span": 12,
                "width": "50px",
                "dict": {
                  "dictType": "MS_E_YESORNO",
                  "dictKey": [
                    "MS_E_YESORNO"
                  ],
                  "format": "label"
                }
              },
              "unit_nature": {
                "control": "select",
                "label": "单位性质",
                "span": 12,
                "width": "50px",
                "dict": {
                  "dictType": "US_E_NUIT_NATURE",
                  "dictKey": [
                    "US_E_NUIT_NATURE"
                  ],
                  "format": "label"
                }
              },
              "year_income": {
                "control": "currency",
                "label": "年收入",
                "span": 12,
                "width": "50px",
                "rules": [
                  {
                    "message": "请输入年收入",
                    "required": true
                  }
                ]
              }
            }
          ]
        },
        {
          "title": "婚姻状况",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "accordion": false,
          "expand": true,
          "controls": [
            {
              "marr_state": {
                "control": "select",
                "label": "婚姻状态",
                "span": 12,
                "width": "50px",
                "placeholder": "请选择",
                "dict": {
                  "dictType": "MS_E_MARITA",
                  "dictKey": [
                    "MS_E_MARITA"
                  ],
                  "format": "label"
                },
                "rules": [
                  {
                    "message": "请输入婚姻状态",
                    "required": true
                  }
                ]
              },
              "spouse_name": {
                "control": "text",
                "label": "配偶姓名",
                "span": 12,
                "width": "50px"
              },
              "spouse_cert_type": {
                "control": "select",
                "label": "配偶证件类型",
                "span": 12,
                "width": "50px",
                "dict": {
                  "dictType": "MS_E_CRET_TYPE",
                  "dictKey": [
                    "MS_E_CRET_TYPE"
                  ],
                  "format": "label"
                }
              },
              "spouse_cert_no": {
                "control": "text",
                "label": "配偶证件号码",
                "span": 12,
                "width": "50px"
              }
            }
          ]
        },
        {
          "title": "账号信息",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "accordion": false,
          "expand": true,
          "controls": [
            {
              "loan_acctg_no": {
                "control": "text",
                "label": "贷款入账账号",
                "span": 12,
                "width": "50px",
                "rules": [
                  {
                    "message": "请输入贷款入账账号",
                    "required": true
                  }
                ]
              },
              "repay_acct": {
                "control": "text",
                "label": "还款账号",
                "span": 12,
                "width": "50px",
                "rules": [
                  {
                    "message": "请输入还款入账账号",
                    "required": true
                  }
                ]
              }
            }
          ]
        },
        {
          "title": "类信用卡产品",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "accordion": false,
          "expand": true,
          "controls": [
            {
              "sale_prod_cd": {
                "control": "lookup",
                "label": "类信用卡产品编号",
                "span": 12,
                "width": "50px",
                "keyField": {
                  "sale_prod_cd": "sale_prod_cd",
                  "sale_prod_name": "sale_prod_name"
                },
                "toRequest": {
                  "url": "/views/pd/prod/sale_product_lookup_for_type.json",
                  "method": "get",
                  "params": {
                    "prod_type": "LXYKDK"
                  }
                },
                "multiple": false,
                "props": {
                  "title": "类信用卡产品选择"
                },
                "rules": [
                  {
                    "required": true,
                    "message": "请输入类信用卡产品编号"
                  }
                ]
              },
              "sale_prod_name": {
                "control": "text",
                "label": "类信用卡产品名称",
                "disabled": true,
                "span": 12,
                "width": "50px",
                "rules": [
                  {
                    "required": true,
                    "message": "请输入类信用卡产品名称"
                  }
                ]
              },
              "loan_term": {
                "control": "text",
                "label": "期限(月)",
                "span": 12,
                "value": 36,
                "disabled": true,
                "width": "50px",
                "rules": [
                  {
                    "required": true,
                    "message": "请输入业务期限"
                  },
                  {
                    "message": "请输入正确期限",
                    "type": "number"
                  }
                ]
              }
            }
          ]
        }
      ]
    }
  }
}
