{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "scope": "strForm",
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/call/RPCCall",
            "method": "POST",
            "params": {
              "servicecode": "ln3970"
            }
          }
        }
      ],
      "controls": [
        {
          "loan_deal_mode": {
            "control": "hidden",
            "label": "贷款处理模式",
            "value": "1",
            "width": "100px",
            "dict": {
              "dictType": "E_LOAN_DEAL_MODE",
              "dictKey": [
                "E_LOAN_DEAL_MODE"
              ],
              "format": "label"
            }
          },
          "smilr_cr_card_loan_acct_num": {
            "control": "text",
            "label": "类信用卡贷款账号",
            "width": "100px"
          },
          "ccy_code_num": {
            "label": "货币代号",
            "control": "select",
            "width": "100px",
            "value": "156",
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": [
                "MS_CURRENCY"
              ],
              "format": "label"
            }
          }
        },
        {
          "slbl_prod_nm": {
            "control": "lookup",
            "label": "可售产品名称",
            "width": "100px",
            "edit": true,
            "keyField": {
              "slbl_prod_nm": "prod_nm",
              "slbl_prod_code": "prod_code"
            },
            "toRequest": {
              "url": "/views/lxyk/class_credit_card_manage_choose.json",
              "method": "get"
            },
            "props": {
              "title": "选择类信用卡产品",
              "width": "75%"
            },
            "format": "label"
          },
          "slbl_prod_code": {
            "control": "hidden",
            "label": "可售产品代码",
            "width": "100px"
          },
          "prod_nm": {
            "control": "lookup",
            "label": "产品名称",
            "width": "100px",
            "edit": true,
            "keyField": {
              "prod_code": "prod_code",
              "prod_nm": "prod_nm"
            },
            "toRequest": {
              "url": "/views/product/productModel/prod_list_qry.json",
              "method": "post"
            },
            "props": {
              "title": "请选择"
            },
            "format": "label"
          },
          "prod_code": {
            "control": "hidden",
            "label": "产品代码",
            "width": "100px"
          } 
        },
        {
          "open_acct_dt": {
            "control": "dateTimePicker",
            "label": "开户日期",
            "width": "100px",
            "type": "date",
            "unlinkPanels": true,
            "format": "yyyyMMdd",
            "disabledDate": {
              "startRange": "19000101",
              "endRange": "99991231"
            }
          },
          "matu": {
            "control": "timeLimit",
            "label": "期限",
            "width": "100px",
            "disabled": true
          }
        },
        {
          "matu_dt": {
            "control": "dateTimePicker",
            "label": "到期日期",
            "type": "date",
            "width": "100px",
            "unlinkPanels": true,
            "format": "yyyyMMdd",
            "disabledDate": {
              "startRange": "19000101",
              "endRange": "99991231"
            }
          },
          "genl_lmt": {
            "control": "currency",
            "label": "通用额度",
            "width": "100px",
            "format": "label",
            "rules": [
              {
                "expression": "${{form.basic_lmt}} >=  0  ",
                "message": "通用额度不能小于零"
              }
            ]
          }
        },
        {
          "basic_lmt": {
            "control": "currency",
            "label": "基础额度",
            "width": "100px",
            "format": "label",
            "rules": [
              {
                "expression": "${{form.basic_lmt}} >=  0  ",
                "message": "基础额度不能小于零"
              }
            ]
          },
          "temp_lmt": {
            "control": "currency",
            "label": "临时额度",
            "width": "100px",
            "format": "label",
            "rules": [
              {
                "expression": "${{form.temp_lmt}} >=  0  ",
                "message": "临时额度不能小于零"
              }
            ]
          }
        },
        {
          "istlmt_lmt": {
            "control": "currency",
            "label": "分期额度",
            "width": "100px",
            "format": "label",
            "rules": [
              {
                "expression": "${{form.istlmt_lmt}} >=  0  ",
                "message": "分期额度不能小于零"
              }
            ]
          },
          "frz_lmt": {
            "control": "currency",
            "width": "100px",
            "label": "冻结额度",
            "format": "label",
            "rules": [
              {
                "expression": "${{form.istlmt_lmt}} >=  0  ",
