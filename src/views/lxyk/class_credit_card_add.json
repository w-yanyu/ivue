          }
        },
        {
          "cust_num": {
            "control": "text",
            "label": "客户号",
            "width": "100px"
          },
          "cust_nm": {
            "control": "text",
            "label": "客户名称",
            "width": "100px"
          }
        },
        {
          "rpymt_acct_num": {
            "control": "text",
            "label": "还款账号",
            "width": "100px"
          },
          "rpymt_sub_acct_serl_num": {
            "control": "text",
            "label": "还款子账户序号",
            "width": "100px"
          }
