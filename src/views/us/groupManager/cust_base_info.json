{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "us0605",
                    "cust_no": "${{params.id}}"
                }
            },
            "scope": "formData",
            "btns": [],
            "controls": [
                {
                    "cust_name": {
                        "control": "text",
                        "label": "客戶名称",
                        "width": "130px",
                        "disabled": true
                    }
                },
                {
                    "cust_english_name": {
                        "control": "text",
                        "label": "客户英文名称",
                        "disabled": true,
                        "width": "130px"
                    }
                },
                {
                    "cust_no": {
                        "control": "text",
                        "label": "客戶编号",
                        "width": "130px",
                        "disabled": true
                    }
                },
                {
                    "cust_type": {
                        "control": "select",
                        "label": "客户类别",
                        "width": "130px",
                        "disabled": true,
                        "dict": {
                            "dictType": "US_E_CUST_TYPE",
                            "dictKey": [
                                "US_E_CUST_TYPE"
                            ],
                            "format": "label"
                        }
                    }
                },
                {
                    "open_acct_branch": {
                        "control": "text",
                        "label": "开户行",
                        "width": "130px",
                        "disabled": true
                    }
                }
            ]
        }
    }
}
