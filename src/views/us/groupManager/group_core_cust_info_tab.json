{
    "module": "dynamicTabs",
    "layout": {
        "tabs": {
            "scope": "templateScope",
            "submitBtnPlace": "right",
            "initDataOnce": false,
            "tabHeader": [{
                    "id": "baseInfo",
                    "label": "客户信息",
                    "closable": false,
                    "disabled": false,
                    "icon": "el-icon-date",
                    "toRequest": {
                        "url": "/views/us/groupManager/cust_base_info.json",
                        "method": "post"
                    }
                },
                {
                    "id": "groupRela",
                    "label": "集团客户关系",
                    "icon": "el-icon-date",
                    "closable": false,
                    "disabled": false,
                    "toRequest": {
                        "url": "/views/us/groupManager/group_cust_query.json",
                        "method": "post"
                    }
                },
                {
                    "id": "groupStructure",
                    "label": "组织架构图谱",
                    "icon": "el-icon-date",
                    "closable": false,
                    "disabled": false,
                    "toRequest": {
                        "url": "@groupCustStructure",
                        "method": "get",
                        "params": {
                            "cust_no": "${{params.cust_no}}"
                        }
                    }
                }
               
            ],
            "defaultActiveKey": "baseInfo",
            "duty_type": "top",
            "styleClass": "card"
        }
    }
}
