{
  "module": "lr-tree-dynamic",
  "initRequest": "${{params}}",
  "layout": {
    "tree": {
      "title": "集团客户",
      "lazy": false,
      "hideDynamicArea": false,
      "idField": "id",
      "pIdField": "pId",
      "labelField": "title",
      "isLeafField": "isLeaf",
      "nodeTypeField": "type",
      "####expandId": [
        "-1",
        "1QY",
        "2JY",
        "3CW",
        "4ZD"
      ],
      "expandId": [
        "-1"
      ],
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POSt",
        "params": {
          "servicecode": "ct2105",
          "grupno": "${{params.grupno}}"
        }
      },
      "root": {
        "rootNode": {
          "name": "集团客户信息",
          "value": "-1"
        },"toRequest": {
          "url": "/views/us/groupManager/group_cust_rela_tree.json",
          "params": {
            "cust_no": "${{params.cust_no}}",
            "show_flash": "${{params.show_flash}}",
            "method": "post"
          }
        }
      },
      "rootTypes": [
        "dept"
      ],
      "nodeTypes": {
        "qy01": {
          "children": [
            "qy01"
          ],
          "toRequest": {
            "url": "/views/us/groupManager/group_base_info.json",
            "params": {
              "cust_no": "${{params.cust_no}}",
              "show_flash": "${{params.show_flash}}",
              "method": "post"
            }
          }
        },
        "qy02": {
          "children": [
            "qy02"
          ],
          "toRequest": {
            "url": "/views/us/groupManager/group_cust_rela_tree.json",
            "params": {
              "cust_no": "${{params.cust_no}}",
              "show_flash": "${{params.show_flash}}"
            }
          }
        },
        "qy03": {
          "children": [
            "qy03"
          ],
          "toRequest": {
            "url": "/views/us/entManager/ratingManager/rating_list.json",
            "params": {
              "cust_no": "${{params.cust_no}}",
              "show_flash": "${{params.show_flash}}"
            }
          }
        },"jy01": {
          "children": [
            "jy01"
          ],
          "toRequest": {
            "url": "/views/us/entManager/entCustBusiManager/corporateHandlerManager/corporateHandler_tab.json",
            "params": {
              "cust_no": "${{params.cust_no}}",
              "show_flash": "${{params.show_flash}}"
            }
          }
        },
        "jy02": {
          "children": [
            "jy02"
          ],
          "toRequest": {
            "url": "@groupCustTree",
            "params": {
              "cust_no": "${{params.cust_no}}",
              "show_flash": "${{params.show_flash}}"
            }
          }
        },
        "jy03": {
          "children": [
            "jy03"
          ],
          "toRequest": {
            "url": "/views/us/entManager/entCustBusiManager/corporateManager/corpEquitManager/corpEquit_list.json",
            "params": {
              "cust_no": "${{params.cust_no}}",
              "show_flash": "${{params.show_flash}}"
            }
          }
        },"cw01": {
          "children": [
            "cw01"
          ],
          "toRequest": {
            "url": "@corporateCustomerFinancialStatements",
            "params": {
              "cust_no": "${{params.cust_no}}",
              "show_flash": "${{params.show_flash}}"
            }
          }
        }
      }
    }
  }
}
