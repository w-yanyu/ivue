{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "us0600",
                    "cust_no": "${{params.cust_no}}"
                }
            },
            "scope": "formData",
            "btns": [],
            "controls": [
                {
                    "cust_name": {
                        "control": "text",
                        "label": "客戶名称",
                        "width": "130px",
                        "disabled": true
                    },"cust_no": {
                        "control": "text",
                        "label": "客户编号",
                        "width": "130px",
                        "disabled": true
                    }
                },
                {
                    "is_virtual":{
                        "control":"select",
                        "label": "是否虚拟集团",
                        "align": "center",
                        "dict": {
                          "dictType": "MS_E_YESORNO",
                          "dictKey": [
                            "MS_E_YESORNO"
                          ],
                          "labelField": "dictName",
                          "valueField": "dictId"
                        },
                        "width": "130px",
                        "disabled": true
                    },"remark":{
                        "control":"text",
                        "label": "备注",
                        "align": "center",
                        "width": "130px",
                        "disabled": true
                    } 
                }
            ]
        }
    }
}
