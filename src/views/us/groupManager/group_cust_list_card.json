{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "cust_name": {
            "control": "text",
            "label": "集团客户名称",
            "rules": [
              {
                "max": 30,
                "message": "长度不能超过30个字符"
              }
            ]
          },
       
          "cust_no": {
            "control": "text",
            "label": "客户编号",
            "rules": [
              {
                "max": 32,
                "message": "长度不能超过32个字符"
              }
            ]
            
          }
        }
      ]
    },
    "toolbar": {
    },
    "datagrid": {
      "scope": "searchTable",
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "us0600"
        }
      },
      "thead": [
        {
          "type": "index",
          "label": "序号"
        },
        {
          "label": "客户号",
          "prop": "cust_no",
          "align": "center"
        },
        {
          "label": "客户名称",
          "prop": "cust_name",
          "align": "center"
        },

        {
          "label": "开户机构",
          "prop": "open_acct_branch"
          
        },

        {
          "label": "操作",
          "align": "center",
          "btns": [
            {
              "type": "look",
              "label": "维护",
              "toRequest": {
                "url": "/views/us/groupManager/group_cust_rela_tree.json",
                "method": "POST"
              },
              "props": {
                "width": "85%",
                "title": "集团客户维护"
              }
            }
          ]
        }
      ]
    }
  }
}
