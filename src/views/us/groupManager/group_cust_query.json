{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "us0606",
                  "cust_no": "${{params.id}}"
                }
              },
            "scope": "formData",
            "btns": [],
            "controls": [
                {
                    "cust_name": {
                        "control": "text",
                        "width": "130px",
                        "disabled":true,
                        "label": "客户名称"
                    },
                    "cust_no": {
                        "control": "text",
                        "label": "客户编号",
                        "width": "130px",
                        "disabled":true
                    }
                },
                {
                    "cust_up_name": {
                        "control": "text",
                        "label": "上级客户名称",
                        "width": "130px",
                        "disabled":true
                    },
                    "cust_up_no": {
                        "control": "text",
                        "label": "上级客户编号",
                        "width": "130px",
                        "disabled":true
                    }
                },
                {
                    "group_name": {
                        "control": "text",
                        "width": "130px",
                        "disabled":true,
                        "label": "集团客户名称"
                    },
                    "group_no": {
                        "control": "text",
                        "label": "集团客户编号",
                        "width": "130px",
                        "disabled":true
                    }
                },
                {
                    "is_group_credit_amt": {
                        "control": "select",
                        "label": "是否占用集团授信额度",
                        "width": "130px",
                        "disabled":true,
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "message": "请选择是否占用集团授信额度"
                        }]
                    },
                    "shareholding_ratio": {
                        "label": "持股比例",
                        "control": "text",
                        "width": "130px",
                        "suffix": "%",
                        "disabled":true
                    }
                },
                {
                    "is_core_subcompany": {
                        "control": "select",
                        "label": "是否核心子公司",
                        "width": "130px",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        },
                        "disabled":true
                    },
                    "group_relation": {
                        "control": "select",
                        "label": "关联关系",
                        "width": "130px",
                        "dict": {
                            "dictType": "US_E_GROUP_RELATION",
                            "dictKey": [
                                "US_E_GROUP_RELATION"
                            ],
                            "format": "label"
                        },
                        "disabled":true
                    }

                },
                {
                    "is_controller": {
                        "control": "select",
                        "label": "是否集团实际控制人",
                        "width": "130px",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        },
                        "disabled":true
                    },
                    "remark": {
                        "control": "text",
                        "label": "备注",
                        "width": "130px",
                        "disabled":true
                    }
                }
            ]
        }
    }
}
