{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "scope": "formData",
            "initRequest": "${{params}}",
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "us0828"
                }
            },
            "btns": [
                {
                    "type": "submit",
                    "label": "提交"
                }
            ],
            "controls": [
                {
                    "cust_name": {
                        "control": "lookup",
                        "edit": true,
                        "width": "130px",
                        "label": "客戶名称",
                       
                        "keyField": {
                            "cust_name": "cust_name",
                            "cust_no": "cust_no",
                            "cert_no": "cert_no",
                            "cert_type": "cert_type",
                            "cust_type": "cust_type",
                            "sex_tp": "sex_tp",
                            "mobile_no":"phone_no"
                        },

                        "toRequest": {
                            "url": "/views/us/cust_lookup.json",
                            "method": "GET"

                        },
                        "props": {
                            "width": "80%",
                            "title": "查询关联客户名称"
                        },
                        "format": "label",
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择"
                            }
                        ]
                    },
                    "cust_no": {
                        "label": "客户编号",
                        "control": "text",
                        "width": "130px",
                        
                        "format": "label"
                    },
                    "cert_type": {
                        "label": "证件类型",
                        "control": "select",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择证件类型"
                            }
                        ],
                        "dict": {
                            "dictType": "MS_E_CRET_TYPE",
                            "dictKey": [
                                "MS_E_CRET_TYPE"
                            ],
                            "format": "label"
                        }
                    },
                    "cert_no": {
                        "control": "text",
                        "label": "证件号码",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入证件号码"
                            }
                        ]
                    },
                    "mobile_no": {
                        "control": "text",
                        "label": "手机号码",
                        "width": "130px"
                    },
                    "elec_acct_no": {
                        "control": "text",
                        "label": "电子账号",
                        "width": "130px"
                    },
                    "risk_list_type": {
                        "label": "名单类型",
                        "control": "select",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择名单类型"
                            }
                        ],
                        "dict": {
                            "dictType": "US_E_RISK_LIST_TYPE",
                            "dictKey": [
                                "US_E_RISK_LIST_TYPE"
                            ],
                            "format": "label"
                        },
                        "events": {
                            "condition": {
                                "1": {
                                    "control": {
                                        "risk_list_name": {
                                            "value": "黑名单"
                                        }
                                    }
                                },
                                "2": {
                                    "control": {
                                        "risk_list_name": {
                                            "value": "白名单"
                                        }
                                    }
                                },
                                "3": {
                                    "control": {
                                        "risk_list_name": {
                                            "value": "黑名单"
                                        }
                                    }
                                },
                                "4": {
                                    "control": {
                                        "risk_list_name": {
                                            "value": "黑名单"
                                        }
                                    }
                                },
                                "5": {
                                    "control": {
                                        "risk_list_name": {
                                            "value": "灰名单"
                                        }
                                    }
                                },
                                "6": {
                                    "control": {
                                        "risk_list_name": {
                                            "value": "灰名单"
                                        }
                                    }
                                },
                                "7": {
                                    "control": {
                                        "risk_list_name": {
                                            "value": "灰名单"
                                        }
                                    }
                                },
                                "8": {
                                    "control": {
                                        "risk_list_name": {
                                            "value": "灰名单"
                                        }
                                    }
                                },
                                "9": {
                                    "control": {
                                        "risk_list_name": {
                                            "value": "灰名单"
                                        }
                                    }
                                },
                                "10": {
                                    "control": {
                                        "risk_list_name": {
                                            "value": "黑名单"
                                        }
                                    }
                                },
                                "11": {
                                    "control": {
                                        "risk_list_name": {
                                            "value": "白名单"
                                        }
                                    }
                                },
                                "12": {
                                    "control": {
                                        "risk_list_name": {
                                            "value": "白名单"
                                        }
                                    }
                                },
                                "13": {
                                    "control": {
                                        "risk_list_name": {
                                            "value": "灰名单"
                                        }
                                    }
                                },
                                "14": {
                                    "control": {
                                        "risk_list_name": {
                                            "value": "黑名单"
                                        }
                                    }
                                },
                                "15": {
                                    "control": {
                                        "risk_list_name": {
                                            "value": "黑名单"
                                        }
                                    }
                                },
                                "16": {
                                    "control": {
                                        "risk_list_name": {
                                            "value": "白名单"
                                        }
                                    }
                                },
                                "17": {
                                    "control": {
                                        "risk_list_name": {
                                            "value": "灰名单"
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "risk_list_reason": {
                        "control": "text",
                        "label": "上名单原因",
                        "width": "130px"
                    },
                    "risk_list_name": {
                        "control": "text",
                        "label": "名单名称",
                        "disabled": true,
                        "width": "130px"
                    },
                    "risk_list_status": {
                        "label": "名单状态",
                        "control": "select",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择名单状态"
                            }
                        ],
                        "dict": {
                            "dictType": "US_E_STATE",
                            "dictKey": [
                                "US_E_STATE"
                            ],
                            "format": "label"
                        }
                    },
                    "risk_list_from": {
                        "label": "来源系统",
                        "control": "select",
                        "width": "130px",
                        "value": "2",
                        "rules": [
                            {
                                "message": "请选择来源系统"
                            }
                        ],
                        "dict": {
                            "dictType": "US_E_RISK_LIST_FROM",
                            "dictKey": [
                                "US_E_RISK_LIST_FROM"
                            ],
                            "format": "label"
                        }
                    },
                    "expiry_date": {
                        "control": "dateTimePicker",
                        "label": "失效日期",
                        "format": "yyyyMMdd",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入失效日期"
                            }
                        ]
                    },
                    "effect_date": {
                        "control": "dateTimePicker",
                        "label": "入库日期",
                        "format": "yyyyMMdd",
                        "width": "130px",
                        "rules": [
