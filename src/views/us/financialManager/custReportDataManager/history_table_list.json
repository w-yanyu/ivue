{
    "module": "v_search_btn_datagrid",
    "layout": {
        "datagrid": {
            "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "post",
                "params": {
                    "servicecode": "us0933",
                    "rpt_type": "${{params.rpt_type}}",
                    "cust_no": "${{params.cust_no}}",
                    "tplt_id": "${{params.tplt_id}}"
                }
            },
            "thead": [{
                    "label": "客户编号",
                    "prop": "cust_no",
                    "align": "center"
                },
                {
                    "label": "报表行业类型",
                    "prop": "indu_type",
                    "align": "center",
                    "dict": {
                        "dictType": "US_E_INDU_TYPE",
                        "dictKey": [
                            "US_E_INDU_TYPE"
                        ],
                        "labelField": "dictName",
                        "valueField": "dictId"
                    }
                },
                {
                    "label": "报表类型",
                    "prop": "rpt_type",
                    "align": "center",
                    "dict": {
                        "dictType": "US_E_RPT_TYPE",
                        "dictKey": [
                            "US_E_RPT_TYPE"
                        ],
                        "labelField": "dictName",
                        "valueField": "dictId"
                    }
                },
                {
                    "label": "报表期限类型",
                    "prop": "rpt_perd_type",
                    "align": "center",
                    "dict": {
                        "dictType": "US_E_RPT_PERD_TYPE",
                        "dictKey": [
                            "US_E_RPT_PERD_TYPE"
                        ],
                        "labelField": "dictName",
                        "valueField": "dictId"
                    }
                },
                {
                    "label": "是否合并报表",
                    "prop": "is_unit_flag",
                    "align": "center",
                    "dict": {
                        "dictType": "MS_E_YESORNO",
                        "dictKey": [
                            "MS_E_YESORNO"
                        ],
                        "labelField": "dictName",
                        "valueField": "dictId"
                    }
                },
                {
                    "label": "是否审计",
                    "prop": "is_check_flag",
                    "align": "center",
                    "dict": {
                        "dictType": "MS_E_YESORNO",
                        "dictKey": [
                            "MS_E_YESORNO"
                        ],
                        "labelField": "dictName",
                        "valueField": "dictId"
                    }
                },
                {
                    "label": "报表合并口径",
                    "prop": "unit_way",
                    "align": "center",
                    "dict": {
                        "dictType": "US_E_UNIT_WAY",
                        "dictKey": [
                            "US_E_UNIT_WAY"
                        ],
                        "labelField": "dictName",
                        "valueField": "dictId"
                    }
                },
                {
                    "label": "报表日期",
                    "prop": "rpt_date",
                    "align": "center",
                    "format": "yyyy/M/d"
                },
                {
                    "label": "备注",
                    "prop": "remark",
                    "align": "center"
                },
                {
                    "label": "操作",
                    "fixed": "right",
                    "align": "center",
                    "btns": [{
                        "type": "bringback",
                        "label": "选择"
                    }]
                }
            ]
        }
    }
}
