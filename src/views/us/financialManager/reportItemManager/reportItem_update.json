{
    "module": "v-inputDataForm-btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "btns": [{
                "type": "submit",
                "label": "提交",
                "doRequest": {
                    "url": "/SUMP/call/RPCCall",
                    "method": "post",
                    "params": {
                        "servicecode": "us0902"

                    }
                }
            }],
            "controls": [{
                    "item_code": {
                        "control": "hidden",
                        "label": "科目编号",
                        "disabled": true,
                        "rules": [{
                                "max": 20,
                                "message": "长度不能超过20个字符"
                            },
                            {
                                "message": "科目编号不能为空",
                                "required": true
                            }
                        ]
                    },
                    "item_name": {
                        "control": "text",
                        "label": "科目名称",
                        "rules": [{
                                "max": 100,
                                "message": "长度不能超过100个字符"
                            },
                            {
                                "message": "科目名称不能为空",
                                "required": true
                            }
                        ]
                    }
                },
                {
                    "item_type": {
                        "control": "select",
                        "label": "科目类型",
                        "align": "center",
                        "dict": {
                            "dictType": "US_E_ITEM_TYPE",
                            "dictKey": [
                                "US_E_ITEM_TYPE"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": true,
                            "message": "请选择科目类型"
                        }]
                    },
                    "show_flag": {
                        "control": "select",
                        "label": "显示标志",
                        "align": "center",
                        "dict": {
                            "dictType": "US_E_SHOW_FLAG",
                            "dictKey": [
                                "US_E_SHOW_FLAG"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": true,
                            "message": "请选择显示标志"
                        }]
                    }
                },
                {
                    "parent_item_code": {
                        "control": "text",
                        "label": "上级科目编号",
                        "rules": [{
                            "max": 20,
                            "message": "长度不能超过20个字符"
                        }]
                    },
                    "auto_flag": {
                        "control": "select",
                        "label": "自动计算标志",
                        "align": "center",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": true,
                            "message": "请选择显示标志"
                        }]
                    }
                }
            ]
        }
    }
}
