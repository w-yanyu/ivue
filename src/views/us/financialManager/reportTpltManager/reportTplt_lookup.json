{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "controls": [{

                "indu_type": {
                    "control": "select",
                    "label": "报表行业类型",
                    "dict": {
                        "dictType": "US_E_INDU_TYPE",
                        "dictKey": [
                            "US_E_INDU_TYPE"
                        ],
                        "labelField": "dictName",
                        "valueField": "dictId"
                    }
                },
                "rpt_type": {
                    "control": "select",
                    "label": "报表类型",
                    "dict": {
                        "dictType": "US_E_RPT_TYPE",
                        "dictKey": [
                            "US_E_RPT_TYPE"
                        ],
                        "labelField": "dictName",
                        "valueField": "dictId"
                    }
                }
            }]
        },
        "toolbar": {
            "btns": [{
                "type": "add",
                "label": "新增",
                "target": "dialog",
                "toRequest": {
                    "method": "GET",
                    "url": "/views/us/financialManager/reportTpltManager/reportTplt_add.json"
                },
                "props": {
                    "width": "80%",
                    "title": "财务报表模板新增"
                }
            }]
        },
        "datagrid": {
            "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "post",
                "params": {
                    "servicecode": "us0910",
                   
                    "rpt_type": "${{params.rpt_type}}"
                }
            },
            "thead": [{
                    "label": "报表模板名称",
                    "prop": "tplt_name"
                },
                {
                    "label": "报表模板编号",
                    "prop": "tplt_id"
                },
                {
                    "label": "报表行业类型",
                    "prop": "indu_type",
                    "align": "center",
                    "dict": {
                        "dictType": "US_E_INDU_TYPE",
                        "dictKey": [
                            "US_E_INDU_TYPE"
                        ],
                        "labelField": "dictName",
                        "valueField": "dictId"
                    }
                },
                {
                    "label": "报表类型",
                    "prop": "rpt_type",
                    "align": "center",
                    "dict": {
                        "dictType": "US_E_RPT_TYPE",
                        "dictKey": [
                            "US_E_RPT_TYPE"
                        ],
                        "labelField": "dictName",
                        "valueField": "dictId"
                    }
                },
                {
                    "label": "是否停用",
                    "prop": "is_halt_flag",
                    "align": "center",
                    "dict": {
                        "dictType": "MS_E_YESORNO",
                        "dictKey": [
                            "MS_E_YESORNO"
                        ],
                        "labelField": "dictName",
                        "valueField": "dictId"
                    }
                },


                {
                    "label": "操作",
                    "fixed": "right",
                    "width": "140px",
                    "align": "center",
                    "btns": [{
                        "type": "bringback",
                        "label": "选择"
                    }]
                }
            ]
        }
    }
}