{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "ctrct_num": {
            "control": "text",
            "span": 12,
            "label": "合同编号",
            "format": "label",
            "rules": [
              {
                "max": 80,
                "message": "长度不能超过80个字符"
              }
            ]
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "syln0002",
          "outlistid": "lnCpstnQryType",
          "cust_num": "${{params.cust_num}}"
        }
      },
      "thead": [
        {
          "label": "序号",
          "width": "50px",
          "type": "index",
          "align": "center"
        },
        {
          "align": "center",
          "label": "借据号",
          "prop": "loan_due_bill_num",
          "width": "250px"
        },
        {
          "align": "center",
          "label": "合同编号",
          "prop": "ctrct_num",
          "width": "250px"
        },
        {
          "align": "center",
          "label": "客户号",
          "prop": "cust_num",
          "width": "250px"
        },
        {
          "align": "center",
          "width": "100px",
          "label": "可售产品编号",
          "prop": "slbl_prod_code",
          "width": "150px"
        },
        {
          "align": "center",
          "label": "可售产品名称",
          "width": "120px",
          "prop": "slbl_prod_nm",
          "width": "200px"
        },
        {
          "align": "center",
          "width": "100px",
          "label": "放款日期",
          "prop": "open_acct_dt"
        },
        {
          "align": "center",
          "width": "100px",
          "label": "到期日期",
          "prop": "matu_dt"
        },
        {
          "align": "center",
          "width": "140px",
          "label": "借据金额(元)",
          "prop": "due_bill_amt",
          "format": {
            "thousand": ","
          }
        },
        {
          "align": "center",
          "width": "140px",
          "label": "合同金额(元)",
          "prop": "ctrct_amt",
          "format": {
            "thousand": ","
          }
        },
        {
          "align": "center",
          "width": "80px",
          "label": "贷款形态",
          "prop": "loan_form",
          "dict": {
            "dictType": "LN_E_LOAN_FORM",
            "dictKey": [
              "LN_E_LOAN_FORM"
            ],
            "format": "label"
          }
        },
        {
          "align": "center",
          "width": "120px",
          "label": "贷款账户状态",
          "prop": "loan_acct_ste",
          "dict": {
            "dictType": "LN_E_LOAN_ACCT_STE",
            "dictKey": [
              "LN_E_LOAN_ACCT_STE"
            ],
            "format": "label"
          }
        },
        {
          "label": "操作",
          "fixed": "right",
          "align": "center",
          "btns": [
            {
              "type": "bringback",
              "label": "选择"
            }
          ]
        }
      ]
    }
  }
}
