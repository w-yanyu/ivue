{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "controls": [
        {
          "cust_no": {
            "control": "hidden",
            "label": "客户编号",
            "value": "${{params.cust_no}}",
            "disabled": true
          },
          "cust_code": {
            "control": "hidden",
            "label": "客户内码",
            "value": "${{params.cust_code}}",
            "disabled": true
          },
          "contact_person_name": {
            "control": "string",
            "width": "10px",
            "label": "联系人姓名"
          },
          "contact": {
            "control": "string",
            "width": "10px",
            "label": "联系人关系",
            "dict": {
              "dictType": "US_E_CONTACT",
              "dictKey": [
                "US_E_CONTACT"
              ],
              "format": "label"
            }
          },
          "is_home_member": {
            "control": "select",
            "width": "10px",
            "label": "是否为家庭成员",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "labelField": "dictName",
              "valueField": "dictId",
              "format": "label"
            },
            "disabled": true
          },
          "is_emergency_contact": {
            "control": "select",
            "width": "10px",
            "label": "是否紧急联系人",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "labelField": "dictName",
              "valueField": "dictId",
              "format": "label"
            },
            "disabled": true
          },
          "cert_type": {
            "control": "string",
            "width": "10px",
            "label": "证件类型",
            "dict": {
              "dictType": "MS_E_CRET_TYPE",
              "dictKey": [
                "MS_E_CRET_TYPE"
              ],
              "labelField": "label",
              "valueField": "value"
            }
          },
          "cert_no": {
            "control": "string",
            "width": "10px",
            "label": "证件号码"
          },
          "work_addr_details": {
            "control": "string",
            "title": "请选择工作单位地址",
            "label": "工作单位地址",
            "width": "10px"
          },
          "mobile_no": {
            "control": "string",
            "width": "10px",
            "label": "手机",
            "align": "center"
          },
          "contact_unit": {
            "control": "string",
            "width": "10px",
            "label": "联系单位",
            "align": "center"
          },
          "phone_no": {
            "control": "string",
            "width": "10px",
            "label": "联系电话",
            "align": "center"
          },
          "remark": {
            "label": "备注",
            "control": "textarea",
            "disabled": true,
            "width": "10px",
            "span": "24",
            "autosize": {
              "minRows": 4
            }
          }
        }
      ]
    }
  }
}
