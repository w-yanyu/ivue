{
    "module": "v_search_btn_datagrid",
    "layout": {
        "toolbar": {
            "btns": [
                {
                    "type": "add",
                    "label": "新增",
                    "toRequest": {
                        "url": "/views/us/privateManage/baseInfo/custIdentityInfo/custIdentityInfo_add.json",
                        "method": "post",
                        "params": {
                            "cust_no": "${{params.cust_no}}"
                        },
                        "props": {
                            "width": "75%",
                            "title": "客户证件信息新增"
                        }
                    },
                  "show": {
                    "expression": "'${{params.show_flash}}' == '1'"
                  }
                }
            ]
        },
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": true,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "us0021",
                    "cust_no": "${{params.cust_no}}"
                }
            },
            "pagination": {
                "pageSizes": [
                    10,
                    15,
                    20
                ],
                "pageSize": 10
            },
            "thead": [
                {
                    "type": "index",
                    "label": "序号",
                    "align": "center"
                },
                {
                    "label": "证件类型",
                    "prop": "cert_type",
                    "align": "center",
                    "dict": {
                        "dictType": "MS_E_CRET_TYPE",
                        "dictKey": [
                            "MS_E_CRET_TYPE"
                        ],
                        "labelField": "dictName",
                        "valueField": "dictId"
                    }
                },
                {
                    "label": "证件号码",
                    "prop": "cert_no",
                    "align": "center"
                },
                {
                    "label": "操作",
                    "align": "center",
                    "width": 180,
                    "btns": [
                        {
                            "type": "look",
                            "label": "详情",
                            "toRequest": {
                                "url": "/views/us/privateManage/baseInfo/custIdentityInfo/custIdentityInfo_info.json",
                                "method": "get",
                                "params": {}
                            },
                            "props": {
                                "width": "75%",
                                "title": "客户证件信息详情"
                            }
                        },
                        {
                            "type": "edit",
                            "label": "维护",
                            "toRequest": {
                                "url": "/views/us/privateManage/baseInfo/custIdentityInfo/custIdentityInfo_modity.json",
                                "method": "post",
                                "params": {
                                    "cust_no": "${{params.cust_no}}"
                                }
                            },
                          "show": {
                            "expression": "'${{params.show_flash}}' == '1'"
                          },
                            "props": {
                                "width": "75%",
                                "title": "客户证件信息维护"
                            }
                        },
                        {
                            "type": "custom",
                            "label": "删除",
                            "doRequest": {
                                "url": "/SUMP/icmcall/icmRPCCall",
                                "method": "POST",
                                "params": {
                                    "servicecode": "us0024",
                                    "cust_no": "${{params.cust_no}}"
                                }
                            },
                          "show": {
                            "expression": "'${{params.show_flash}}' == '1'"
                          },
                            "confirm": {
                                "title": "确定删除",
                                "confirmButtonText": "确定",
                                "cancelButtonText": "取消"
                            }
                        }
                    ]
                }
            ]
        }
    }
}
