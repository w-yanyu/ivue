{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "us0022",
              "cust_no": "${{params.cust_no}}"
            }
          }
        }
      ],
      "controls": [
        {
          "cust_no": {
            "control": "hidden",
            "label": "客户号",
            "value": "${{params.cust_no}}"
          },
          "cert_type": {
            "control": "select",
            "width": "140px",
            "label": "证件类型",
            "value": "",
            "dict": {
              "dictType": "MS_E_CRET_TYPE",
              "dictKey": [
                "MS_E_CRET_TYPE"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "证件类型不能为空"
              }
            ]
          },
          "cert_no": {
            "control": "text",
            "width": "140px",
            "label": "证件号码",
            "rules": [
              {
                "required": true,
                "message": "证件号码不能为空"
              }
            ]
          },
          "is_indv": {
            "control": "select",
            "width": "140px",
            "label": "是否个人证件",
            "value": "",
            "rules": [
              {
                "required": true,
                "message": "请选择"
              }
            ],
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          }
        }
      ]
    }
  }
}
