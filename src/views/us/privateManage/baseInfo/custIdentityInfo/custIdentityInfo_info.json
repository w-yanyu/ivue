{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "btns": [],
      "controls": [
        {
          "person_no": {
            "control": "hidden",
            "label": "证件序号",
            "span": 19,
            "value": "${{params.person_no}}"
          },
          "cust_no": {
            "control": "hidden",
            "label": "客户号",
            "span": 19,
            "value": "${{params.cust_no}}"
          },
          "cert_type": {
            "control": "select",
            "width": "140px",
            "label": "证件类型",
            "disabled": true,
            "value": "${{params.cert_type}}",
            "dict": {
              "dictType": "MS_E_CRET_TYPE",
              "dictKey": [
                "MS_E_CRET_TYPE"
              ],
              "labelField": "dictName",
              "valueField": "dictId"
            }
          },
          "cert_no": {
            "control": "text",
            "width": "140px",
            "label": "证件号码",
            "value": "${{params.cert_no}}",
            "disabled": true
          },
          "is_indv": {
            "control": "select",
            "disabled": true,
            "width": "140px",
            "label": "是否个人证件",
            "value": "${{params.is_indv}}",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          }
        }
      ]
    }
  }
}
