    {
        "module": "v_inputDataForm_btn",
        "layout": {
            "form": {
                "initRequest": "${{params}}",
                "controls": [
                    {
                        "cust_no": {
                            "control": "hidden",
                            "label": "当前客户编号",
                            "disabled": true
                        },
                        "start_date": {
                            "control": "dateTimePicker",
                            "label": "起始日期:",
                            "format": "yyyyMMdd",
                            "disabled": true,
                            "width": "10px"
                        },
                        "end_date_type": {
                            "control": "select",
                            "label": "到期日类型:",
                            "disabled": true,
                            "width": "10px",
                            "align": "center",
                            "dict": {
                                "dictType": "US_E_END_DATA_TYPE",
                                "dictKey": [
                                    "US_E_END_DATA_TYPE"
                                ],
                                "format": "label"
                            },
                            "events": {
                                "condition": {
                                    "1": {
                                        "control": {
                                            "end_date": {
                                                "rules": [
                                                    {
                                                        "required": false
                                                    }
                                                ]
                                            }
                                        }
                                    },
                                    "2": {
                                        "control": {
                                            "end_date": {}
                                        }
                                    }
                                }
                            }
                        },
                        "end_date": {
                            "control": "dateTimePicker",
                            "label": "终止日期:",
                            "disabled": true,
                            "width": "10px",
                            "format": "yyyyMMdd"
                        },
                        "duty_type": {
                            "control": "select",
                            "label": "职务:",
                            "disabled": true,
                            "width": "10px",
                            "align": "center",
                            "dict": {
                                "dictType": "US_E_DUTY_TYPE",
                                "dictKey": [
                                    "US_E_DUTY_TYPE"
                                ],
                                "format": "label"
                            }
                        },
                        "employer": {
                            "control": "text",
                            "label": "单位:",
                            "align": "center",
                            "disabled": true,
                            "width": "10px"
                        },
                        "remark": {
                            "label": "备注",
                            "control": "textarea",
                            "width": "10px",
                            "span": "24",
                            "disabled": true,
                            "autosize": {
                                "minRows": 4
                            }
                        }
                    }
                ],
                "btns": []
            }
        }
    }
