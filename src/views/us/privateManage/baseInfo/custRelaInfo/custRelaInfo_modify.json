{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "us0049"
        }
      },
      "controls": [
        {
          "relate_serial_no": {
            "control": "hidden",
            "label": "关联序号",
            "width": "10px",
            "disabled": true
          },
          "cust_no": {
            "control": "hidden",
            "label": "客户序号",
            "width": "10px",
            "disabled": true
          },
          "cust_name": {
            "control": "hidden",
            "label": "客户名称",
            "width": "10px",
            "disabled": true
          },
          "rela_cust_name": {
            "control": "string",
            "label": "对方客户名称",
            "width": "10px",
            "disabled": true,
            "span": "12"
          },
          "rela_cust_no": {
            "control": "hidden",
            "width": "10px",
            "label": "对方客户号",
            "rules": [
              {
                "max": 19,
                "message": "长度不能超过19个字符"
              },
              {
                "required": true,
                "message": "关联企业不能为空"
              }
            ]
          },
          "rela_type": {
            "control": "select",
            "width": "10px",
            "label": "关联关系",
            "align": "center",
            "value": "1",
            "dict": {
              "dictType": "US_E_RELA_TYPE",
              "dictKey": [
                "US_E_RELA_TYPE"
              ],
              "format": "label",
              "include": [
                "1",
                "5",
                "4",
                "9"
              ]
            },
            "rules": [
              {
                "required": true,
                "message": "关联关系不能为空"
              }
            ]
          },
          "rela_cv_type": {
            "control": "select",
            "width": "10px",
            "label": "逆向关联关系",
            "align": "center",
            "value": "1",
            "dict": {
              "dictType": "US_E_RELA_TYPE",
              "dictKey": [
                "US_E_RELA_TYPE"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "关联关系不能为空"
              }
            ]
          },
          "real_status": {
            "control": "select",
            "width": "10px",
            "label": "状态",
            "align": "center",
            "dict": {
              "dictType": "US_E_RELA_STATUS",
              "dictKey": [
                "US_E_RELA_STATUS"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "状态不能为空"
              }
            ]
          },
          "rela_desc": {
            "label": "关联关系描述",
            "control": "textarea",
            "width": "10px",
            "span": "24",
            "autosize": {
              "minRows": 2
            },
            "rules": [
              {
                "max": 100,
                "message": "长度不能超过100个字符"
              }
            ]
          },
          "remark": {
            "label": "备注",
            "control": "textarea",
            "width": "10px",
            "span": "24",
            "autosize": {
              "minRows": 4
            },
            "rules": [
              {
                "max": 100,
                "message": "长度不能超过100个字符"
              }
            ]
          }
        }
      ]
    }
  }
}
