{
    "module": "lr-tree-dynamic",
    "initRequest": "${{params}}",
    "layout": {
        "tree": {
            "title": "关联关系检索",
            "lazy": false,
            "hideDynamicArea": false,
            "idField": "id",
            "pIdField": "pid",
            "labelField": "title",
            "isLeafField": "isLeaf",
            "nodeTypeField": "type",
            "expandId": [
                "-1",
                "1QY-1",
                "2JY-1",
                "3CW-1",
                "4ZD-1"
            ],
            "initRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POSt",
                "params": {
                    "servicecode": "us0051",
                    "cust_no": "${{params.cust_no}}"
                }
            },
            "root": {
                "rootNode": {
                    "name": "关联客户检索",
                    "value": "-1"
                }
            },
            "rootTypes": [
                "dept"
            ],
            "nodeTypes": {
                "leaf01": {
                    "children": [
                        "leaf01"
                    ],
                    "toRequest": {
                        "url": "/views/us/privateManage/baseInfo/custRelaInfo/custRelaInfo_base_info.json",
                        "params": {
                            "cust_no": "${{params.cust_no}}",
                            "method": "post"
                        }
                    }
                }
            }
        }
    }
}
