{
    "module": "dynamicTabs",
    "layout": {
        "tabs": {
            "tabHeader": [{
                    "id": "tab1",
                    "label": "手动关联关系",
                    "closable": false,
                    "disabled": false,
                    "icon": "el-icon-date",
                    "toRequest": {
                        "url": "/views/us/privateManage/baseInfo/custRelaInfo/custRelaInfo_manage.json",
                        "method": "POST",
                        "cust_no": "${{params.cust_no}}",
                        "cust_name": "${{params.cust_name}}"

                    }
                },
                {
                    "id": "tab2",
                    "label": "关联关系检索",
                    "icon": "el-icon-date",
                    "closable": false,
                    "disabled": false,
                    "toRequest": {
                        "url": "/views/us/privateManage/baseInfo/custRelaInfo/custRelaInfo_tree.json",
                        "method": "POST",
                        "cust_no": "${{params.cust_no}}"
                    }
                }
            ],
            "defaultActiveKey": "tab1",
            "duty_type": "top",
            "styleClass": "card"
        }
    }
}
