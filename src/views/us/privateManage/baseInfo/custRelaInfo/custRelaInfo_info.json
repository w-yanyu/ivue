{
    "module": "v-inputDataForm-btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "btns": [],
            "controls": [
                {
                    "cust_no": {
                        "control": "hidden",
                        "label": "客户序号",
                        "disabled": true,
                        "width": "10px"
                    },
                    "cust_name": {
                        "control": "hidden",
                        "label": "客户名称",
                        "width": "10px",
                        "disabled": true
                    },
                    "rela_cust_name": {
                        "control": "text",
                        "label": "对方客户名称",
                        "width": "10px",
                        "disabled": true

                    },
                    "rela_cust_no": {
                        "control": "hidden",
                        "width": "10px",
                        "disabled": true,
                        "label": "对方客户号"
                    },
                    "rela_type": {
                        "control": "select",
                        "disabled": true,
                        "label": "关联关系",
                        "width": "10px",
                        "align": "center",
                        "value": "1",
                        "dict": {
                            "dictType": "US_E_RELA_TYPE",
                            "dictKey": [
                                "US_E_RELA_TYPE"
                            ],
                            "format": "label"
                        }
                    },
                    "rela_cv_type": {
                        "control": "select",
                        "label": "逆向关联关系",
                        "width": "10px",
                        "disabled": true,
                        "align": "center",
                        "value": "1",
                        "dict": {
                            "dictType": "US_E_RELA_TYPE",
                            "dictKey": [
                                "US_E_RELA_TYPE"
                            ],
                            "format": "label"
                        }
                    },
                    "real_status": {
                        "control": "select",
                        "disabled": true,
                        "width": "10px",
                        "label": "状态",
                        "align": "center",
                        "dict": {
                            "dictType": "US_E_RELA_STATUS",
                            "dictKey": [
                                "US_E_RELA_STATUS"
                            ],
                            "format": "label"
                        }
                    },
                    "rela_desc": {
                        "label": "关联关系描述",
                        "control": "textarea",
                        "width": "10px",
                        "disabled": true,
                        "span": "24",
                        "autosize": {
                            "minRows": 2
                        }
                    },
                    "remark": {
                        "label": "备注",
                        "control": "textarea",
                        "disabled": true,
                        "width": "10px",
                        "span": "24",
                        "autosize": {
                            "minRows": 4
                        }
                    }
                }
            ]
        }
    }
}
