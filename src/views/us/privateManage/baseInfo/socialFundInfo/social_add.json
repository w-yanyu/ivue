{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "scope": "templateScope",
            "initRequest": "${{params}}",
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "us0159"
                }
            },
            "btns": [
                {
                    "type": "submit",
                    "label": "提交"
                }
            ],
            "controls": [
                {
                    "cust_no": {
                        "control": "text",
                        "label": "客户编号",
                        "width": "130px",
                        "disabled": true
                    },
                    "medical_insurance": {
                        "control": "currency",
                        "label": "医疗保险",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入医疗保险"
                            }
                        ]
                    },
                    "endow_insurance": {
                        "label": "养老保险",
                        "control": "currency",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入养老保险"
                            }
                        ]
                    },
                    "matern_insurance": {
                        "label": "生育保险",
                        "control": "currency",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入生育保险"
                            }
                        ]
                    },
                    "unemploy_insurance": {
                        "control": "currency",
                        "label": "失业保险",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入失业保险"
                            }
                        ]
                    },
                    "injury_insuranc": {
                        "control": "currency",
                        "label": "工伤保险",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入工伤保险"
                            }
                        ]
                    },
                    "insur_paymentdate": {
                        "control": "dateTimePicker",
                        "label": "缴费日期",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择缴费日期"
                            }
                        ]
                    }
                }
            ]
        }
    }
}
