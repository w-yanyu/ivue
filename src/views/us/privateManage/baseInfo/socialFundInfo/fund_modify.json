{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "scope": "templateScope",
            "initRequest": "${{params}}",
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "us0161"
                }
            },
            "btns": [
                {
                    "type": "submit",
                    "label": "提交"
                }
            ],
            "controls": [
                {
                    "fund_no": {
                        "control": "text",
                        "label": "公积金信息主键",
                        "width": "130px",
                        "disabled": true
                    },
                    "cust_no": {
                        "control": "text",
                        "label": "客户编号",
                        "width": "130px",
                        "disabled": true
                    },
                    "fund_company": {
                        "control": "text",
                        "label": "公积金缴存单位名称",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入公积金缴存单位名称"
                            }
                        ]
                    },
                    "fund_base": {
                        "label": "缴存基数",
                        "control": "currency",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入缴存基数"
                            }
                        ]
                    },
                    "fund_paymentamount": {
                        "label": "缴纳金额",
                        "control": "currency",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入缴纳金额"
                            }
                        ]
                    },
                    "fund_paymentdate": {
                        "control": "dateTimePicker",
                        "label": "缴纳日期",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择缴纳日期"
                            }
                        ]
                    }
                }
            ]
        }
    }
}
