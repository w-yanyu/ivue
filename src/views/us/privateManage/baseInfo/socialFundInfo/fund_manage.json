{
  "module": "v_search_btn_datagrid",
  "layout": {

    "toolbar": {
      "btns": [
        {
          "type": "add",
          "target": "dialog",
          "label": "新增",
          "toRequest": {
            "method": "POST",
            "url": "/views/us/privateManage/baseInfo/socialFundInfo/fund_add.json",
            "params": {
              "cust_no": "${{params.cust_no}}"
            }
          },
          "show": {
            "expression": "'${{params.show_flash}}' == '1'"
          },
          "props": {
            "width": "75%",
            "title": "个人社保信息新增"
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": false,
      "scope": "searchTable",
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "us0160",
          "cust_no": "${{params.cust_no}}"
        }
      },
      "thead": [
        {
          "label": "客户编号",
          "align": "center",
          "prop": "cust_no"
        },
        {
          "label": "公积金缴存单位名称",
          "align": "center",
          "prop": "fund_company"
        },
        {
          "type":"currency",
          "label": "基数",
          "prop": "fund_base",
          "align": "center",
          "format": {
            "thousand": ",",
            "decimal": "2",
            "type": "currency"
          }
        },
        {
          "type":"currency",
          "label": "缴纳金额",
          "align": "center",
          "prop": "fund_paymentamount",
          "format": {
            "thousand": ",",
            "decimal": "2",
            "type": "currency"
          }
        },
        {
          "label": "缴纳日期",
          "align": "center",
          "prop": "fund_paymentdate"
        },
        {
          "label": "操作",
          "fixed": "right",
          "width": 160,
          "align": "center",
          "btns": [
            {
              "type": "edit",
              "label": "维护",
              "toRequest": {
                "url": "/views/us/privateManage/baseInfo/socialFundInfo/fund_modify.json",
                "method": "post"
              },
              "show": {
                "expression": "'${{params.show_flash}}' == '1'"
              },
              "props": {
                "width": "75%",
                "title": "个人社保信息维护"
              }
            },
            {
              "type": "delete",
              "label": "删除",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "us0162"
                }
              },
              "show": {
                "expression": "'${{params.show_flash}}' == '1'"
              },
              "confirm": {
                "title": "确定删除",
                "confirmButtonText": "确定",
                "cancelButtonText": "取消"
              }
            }
          ]
        }
      ],
      "pagination": {
        "show": true
      }
    }
  }
}
