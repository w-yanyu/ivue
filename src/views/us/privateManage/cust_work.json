{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "us0170"
                }
            },
            "scope": "formData",
            "btns": [{
                "type": "submit",
                "label": "保存",
                "doRequest": {
                    "url": "/SUMP/icmcall/icmRPCCall",
                    "method": "POST",
                    "params": {
                        "servicecode": "us0171"
                    }
                }
            }
        ],
            "controls": [
                {
                    "cust_no":{
                        "control": "hidden",
                        "label": "客户编号",
                        "width": "25px"
                    }
                },
                {
                "is_my_bankstaff": {
                    "control": "text",
                    "label": "是否我行员工",
                    "width": "25px"
                },
                "department_no": {
                    "control": "text",
                    "label": "部门编号",
                    "width": "25px"              
                },
                "staff_no": {
                    "control": "select",
                    "label": "员工编号",
                    "width": "25px"              
                }
                },
                {
                    "company_name": {
                        "control": "text",
                        "label": "公司名称",
                        "width": "130px"
                    },
                    "firm_address": {
                        "control": "select",
                        "label": "公司地址",
                        "width": "130px"              
                    }
                },
                {
                    "now_entry_date": {
                    "control": "text",
                    "label": "当前公司入职日期",
                    "width": "130px"
                },
                    
                    "entry_date": {
                        "editable": true,
                        "format": "yyyyMMdd",
                        "label": "入职日期",
                        "width": "130px"
                    },
                    "career_type": {
                        "control": "select",
                        "label": "就业类型",
                        "width": "130px"
                    }
                },
                {
                    "post_star_date": {
                        "control": "dateTimePicker",
                        "format": "yyyyMMdd",
                        "label": "当前职位开始时间",
                        "width": "130px"
                    },
                    "first_entry_date": {
                            "control": "dateTimePicker",
                            "editable": true,
                            "format": "yyyyMMdd",
                            "label": "首次参加工作时间",
                            "width": "130px"
                        },
                        "post1": {
                            "control": "text",
                            "label": "职位",
                            "width": "130px"
                        }
                 },
                 {
                    "position": {
                        "control": "text",
                        "label": "公司电话",
                        "width": "130px"
                    },
                    "monthly_income": {
                            "control": "dateTimePicker",
                            "editable": true,
                            "format": "yyyyMMdd",
                            "label": "月收入 (港元) ",
                            "width": "130px"
                    }
                 }
            ]
        }
    }
}