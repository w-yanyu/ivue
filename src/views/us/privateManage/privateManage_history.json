{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "scope": "searchTable",
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "us0014",
          "cust_no": "${{params.cust_no}}"
        }
      },
      "thead": [
        {
          "type": "index",
          "label": "序号"
        },
        {
          "label": "客户号",
          "prop": "cust_no",
          "align": "center"
        },
        {
          "label": "客户名称",
          "prop": "cust_name",
          "align": "center"
        },
        {
          "label": "证件类型",
          "prop": "cert_type",
          "align": "center",
          "dict": {
            "dictType": "MS_E_CRET_TYPE",
            "dictKey": [
              "MS_E_CRET_TYPE"
            ],
            "format": "label"
          }
        },
        {
          "label": "证件号码",
          "prop": "cert_no",
          "align": "center"
        },
        {
          "label": "证件生效日",
          "prop": "cert_day",
          "align": "center"
        },
        {
          "label": "证件到期日",
          "prop": "card_end_day",
          "align": "center"
        },
        {
          "label": "性别",
          "prop": "sex_tp",
          "align": "center",
          "dict": {
            "dictType": "MS_E_GENDER",
            "dictKey": [
              "MS_E_GENDER"
            ],
            "format": "label"
          }
        },
        {
          "label": "出生日期",
          "prop": "birth_date",
          "align": "center"
        },
        {
          "label": "变更日期",
          "prop": "modify_date",
          "align": "center"
        }
      ]
    }
  }
}
