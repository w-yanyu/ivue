{
    "module": "v-inputDataForm-btn",
    "layout": {
      "form": {
        "name": "info",
        "initDataOnce": true,
        "initRequest": {
          "url": "/SUMP/icmcall/icmRPCCall",
          "method": "POST",
          "params": {
            "servicecode": "us0007",
            "cust_no": "${{params.cust_no}}"
          }
        },
        "btns": [
          {
            "type": "submit",
            "label": "提交",
            "icon": "",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "us0002",
                "cust_no": "${{params.cust_no}}"
              }
            }
          }
        ],
        "controlsGroup": [
          {
            "title": "用户基本信息",
            "tooltip": "",
            "titleIcon": "",
            "showHint": "",
            "accordion": false,
            "expand": true,
            "controls": [
              {
                "cert_type": {
                    "control": "select",
                    "label": "开户证件类型",
                    "span": "8",
                    "width": "25px",
                    "dict": {
                      "dictType": "MS_E_CRET_TYPE",
                      "dictKey": [
                        "MS_E_CRET_TYPE"
                      ],
                      "format": "label"
                    },
                  "rules": [
                    {
                      "required": true,
                      "message": "请选择证件类型"
                    }
                  ],
                  "disabled": true
                },
                   
                "cert_no": {
                    "control": "text",
                    "label": "开户证件号码",
                    "span": "8",
                    "width": "25px",
                    "rules": [
                      {
                        "required": true,
                        "message": "请输入证件号码"
                      },
                      {
                        "max": 30,
                        "message": "长度不能超过30个字符"
                      }
                    ],
                    "disabled": true
                  },
                  "country_code": {
                    "control": "select",
                    "label": "国籍",
                    "span": "8",
                    "width": "25px",
                    "dict": {
                      "dictType": "US_E_INCTRY",
                      "dictKey": [
                        "US_E_INCTRY"
                      ],
                      "labelField": "dictName",
                      "valueField": "dictId"
                    },
                    "rules": [
                      {
                        "message": "请选择国籍",
                        "required": true
                      }
                    ]
                  },
                  "surname": {
                    "control": "string",
                    "label": "姓",
                    "span": "8",
                    "width": "25px",
                    "rules": [
                      {
                        "message": "请输入客户姓名",
                        "required": true
                      }
                    ]
                  },
                  "name": {
                    "control": "string",
                    "label": "名",
                    "span": "8",
                    "width": "25px",
                    "rules": [
                      {
                        "message": "请输入客户姓名",
                        "required": true
                      }
                    ]
                  },
                  "en_name": {
                    "control": "string",
                    "label": "英文姓名",
                    "span": "8",
                    "width": "25px",
                    "rules": [
                      {
                        "message": "请输入客户姓名",
                        "required": true
                      }
                    ]
                  },   
                   "cn_name": {
                    "control": "string",
                    "label": "中文姓名",
                    "span": "8",
                    "width": "25px",
                    "rules": [
                      {
                        "message": "请输入客户姓名",
                        "required": true
                      }
                    ]
                  },
                  "birthday": {
                    "label": "出生日期",
                    "width": "25px",
                    "span": "8",
                    "control": "dateTimePicker",
                    "editable": true,
                    "format": "yyyyMMdd"
                },
                "chengnwen": {
                    "control": "string",
                    "label": "称谓",
                    "span": "8",
                    "width": "25px",
                    "rules": [
                      {
                        "message": "请输入客户姓名",
                        "required": true
                      }
                    ]
                  },
                  "sex_tp": {
                    "control": "select",
                    "label": "性别（征）",
                    "span": "8",
                    "width": "25px",
                    "dict": {
                      "dictType": "MS_E_GENDER",
                      "dictKey": [
                        "MS_E_GENDER"
                      ],
                      "labelField": "dictName",
                      "valueField": "dictId"
                    },
                    "rules": [
                      {
                        "message": "请输入性别（征）",
                        "required": true
                      }
                    ]
                  },
                  "date_due": {
                    "label": "到期日期",
                    "width": "25px",
                    "span": "8",
                    "control": "dateTimePicker",
                    "editable": true,
                    "format": "yyyyMMdd"
                },
                "education": {
                    "control": "select",
                    "span": "8",
                    "width": "25px",
                    "label": "教育水平",
                    "dict": {
                      "dictType": "MS_E_MAXEDU",
                      "dictKey": [
                        "DICT_NAME"
                      ],
                      "labelField": "dictName",
                      "valueField": "dictId"
                    },
                    "rules": [
                      {
                        "message": "请选择教育水平",
                        "required": true
                      }
                    ]
                  },
                  "marr_state": {
                    "control": "select",
                    "label": "婚姻状态",
                    "span": "8",
                    "width": "25px",
                    "dict": {
                      "dictType": "MS_E_MARITA",
                      "dictKey": [
                        "MS_E_MARITA"
                      ],
                      "labelField": "dictName",
                      "valueField": "dictId"
                    },
                    "rules": [
                      {
                        "message": "请选择婚姻状态",
                        "required": true
                      }
                    ],
                    "showItem": {
                      "1": [],
                      "4": [],
                      "3": [],
                      "Z": [],
                      "2": [
                          "is_myCust",
                        "spouse_name",
                        "spouse_cust_id",
                        "spouse_cert_type",
                        "spouse_cert_no",
                        "country_code",
                        "dateDue",
                        "spouse_income"
                      ]
                    },
                    "events": {
                      "condition": {
                        "2": {
                          "control": {
                            "is_myCust": {
                                "rules": [
                                  {
                                    "required": true,
                                    "message": "请选择是否是我行客户"
                                  }
                                ]
                              },
                            "spouse_name": {
                              "rules": [
                                {
                                  "required": true,
                                  "message": "配偶姓名不能为空"
                                }
                              ]
                            },
                            "spouse_cust_id": {
                                "rules": [
                                  {
                                    "required": true,
                                    "message": "配偶客户号不能为空"
                                  }
                                ]
                              },
                            "spouse_cert_type": {
                              "rules": [
                                {
                                  "required": true,
                                  "message": "配偶证件类型不能为空"
                                }
                              ]
                            },
                            "spouse_cert_no": {
                              "rules": [
                                {
                                  "required": true,
                                  "message": "配偶证件号码不能为空"
                                }
                              ]
                            },
                            "dateDue": {
                              "rules": [
                                {
                                  "required": true,
                                  "message": "到期日期不能为空"
                                }
                              ]
                            },
                            "spouse_income": {
                              "rules": [
                                {
                                  "required": true,
                                  "message": "配偶收入不能为空"
                                }
                              ]
                            }
                          }
                        }
                      }
                    }
                  },
                  "family_name": {
                    "control": "text",
                    "label": "家庭成员人数",
                    "span": "8",
                    "width": "25px",
                    "rules": [
                      {
                        "message": "请输入家庭成员人数",
                        "required": true
                      }
                    ]
                  },
                  "live_type": {
                    "control": "select",
                    "label": "居住类型",
                    "span": "8",
                    "width": "25px",
                    "dict": {
                      "dictType": "US_E_LIVIE_STATUS",
                      "dictKey": [
                        "US_E_LIVIE_STATUS"
                      ],
                      "labelField": "dictName",
                      "valueField": "dictId"
                    },
                    "rules": [
                      {
                        "message": "请选择居住类型",
                        "required": true
                      }
                    ]
                  },
                  "live_life": {
                    "control": "text",
                    "label": "居住年限",
                    "span": "8",
                    "width": "25px",
                    "rules": [
                      {
                        "message": "请输入家庭成员人数",
                        "required": true
                      }
                    ]
                  },
                  "is_residence": {
                    "control": "select",
                    "label": "是否有住宅",
                    "span": "8",
                    "width": "25px",
                    "dict": {
                      "dictType": "MS_E_YESORNO",
                      "dictKey": [
                        "MS_E_YESORNO"
                      ],
                      "labelField": "dictName",
                      "valueField": "dictId"
                    },
                    "rules": [
                      {
                        "message": "请选择是否外地有房产",
                        "required": true
                      }
                    ]
                  },
                  "is_affiliatedPerson": {
                    "control": "select",
                    "label": "是否我行关联人",
                    "span": "8",
                    "width": "25px",
                    "dict": {
                      "dictType": "MS_E_YESORNO",
                      "dictKey": [
                        "MS_E_YESORNO"
                      ],
                      "labelField": "dictName",
                      "valueField": "dictId"
                    },
                    "showItem": {
                        "0": [],
                        "1": [
                          "is_affiliatedPerson"
                        ]
                      },
                      "events": {
                        "condition": {
                          "1": {
                            "control": {
                              "affiliatedPersonType": {
                                "label": "关联人类别",
                                "control": "select",
                                "rules": [
                                  {
                                    "message": "请输入住宅地址",
                                    "required": true
                                  }
                                ]
                              }
                            }
                          }
                        }
                      }
                },
                
                "custType": {
                    "control": "select",
                    "label": "对私客户类型",
                    "span": "8",
                    "width": "25px",
                    "dict": {
                      "dictType": "MS_E_NATION",
                      "dictKey": [
                        "MS_E_NATION"
                      ],
                      "labelField": "dictName",
                      "valueField": "dictId"
                    },
                    "rules": [
                      {
                        "message": "请选择客户类型",
                        "required": true
                      }
                    ]
                  }
              },
              {
                "cust_manger": {
                    "label": "选择客户经理",
                    "control": "lookup",
                    "edit": true,
                    "width": "130px",
                    "span":"10",
                    "keyField": {
                    },
                    "toRequest": {
                      "url": "/views/us/customerHandoverManage/handover_list.json",
                      "method": "post",
                      "params":{
                        "branch_id":"${{formData.branch_id}}"
                      }
                    },
                    "props": {
                      "title": "选择客户经理",
                      "width": "65%"
                    },
                    "format": "label"
        
                }
              }
            ]
          },
          {
            "title": "用户配偶信息",
            "tooltip": "",
            "titleIcon": "",
            "showHint": "",
            "expand": true,
            "controls": [
              {
                  
                "is_myCust": {
                    "control": "select",
                    "label": "是否是我行客户",
                    "span": "8",
                    "width": "25px",
                    "dict": {
                      "dictType": "MS_E_YESORNO",
                      "dictKey": [
                        "MS_E_YESORNO"
                      ],
                      "labelField": "dictName",
                      "valueField": "dictId"
                    },
                    "rules": [
                      {
                        "message": "请选择是否是我行客户",
                        "required": true
                      }
                    ]
                  },
                "spouse_name": {
                  "control": "text",
                  "label": "配偶姓名",
                  "span": "8",
                  "width": "25px"
                },
                "spouse_cust_id": {
                    "control": "text",
                    "label": "配偶客户号",
                    "span": "8",
                    "width": "25px"
                  },
                "spouse_cert_type": {
                  "control": "select",
                  "label": "配偶证件类型",
                  "span": "8",
                  "width": "25px",
                  "dict": {
                    "dictType": "MS_E_CRET_TYPE",
                    "dictKey": [
                      "MS_E_CRET_TYPE"
                    ],
                    "labelField": "label",
                    "valueField": "value"
                  }
                },
                "spouse_cert_no": {
                  "control": "text",
                  "label": "配偶证件号码",
                  "span": "8",
                  "width": "25px"
                },
                "country_code": {
                    "control": "select",
                    "label": "国籍",
                    "span": "8",
                    "width": "25px",
                    "dict": {
                      "dictType": "US_E_INCTRY",
                      "dictKey": [
                        "US_E_INCTRY"
                      ],
                      "labelField": "dictName",
                      "valueField": "dictId"
                    },
                    "rules": [
                      {
                        "message": "请选择国籍",
                        "required": true
                      }
                    ]
                  },
                  "date_due": {
                    "label": "到期日期",
                    "width": "25px",
                    "span": "8",
                    "control": "dateTimePicker",
                    "editable": true,
                    "format": "yyyyMMdd"
                },
                "spouse_income": {
                    "control": "text",
                    "label": "配偶收入（年）",
                    "span": "8",
                    "width": "25px"
                  }
              }
            ]
          }
        ]
      }
    }
  }
  