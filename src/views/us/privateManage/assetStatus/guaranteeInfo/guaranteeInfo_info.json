{
  "module": "v-realonlyForm-btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "controls": [
        {
          "ins_no": {
            "control": "text",
            "label": "保险单号",
            "width": "130px",
            "disabled": true
          },
          "ins_type": {
            "control": "select",
            "label": "保险险种",
            "width": "130px",
            "dict": {
              "dictType": "US_E_INS_TYPE",
              "dictKey": [
                "US_E_INS_TYPE"
              ],
              "format": "label"
            },
            "disabled": true
          },
          "ins_amt": {
            "control": "currency",
            "thousand": ",",
            "rules": [
              {
                "pattern": "^[0-9]+(\\.[0-9]{1,2})?$"
              }
            ],
            "width": "130px",
            "label": "投保金额",
            "suffix": "元",
            "disabled": true
          },
          "ins_recoup_amt": {
            "control": "currency",
            "thousand": ",",
            "rules": [
              {
                "pattern": "^[0-9]+(\\.[0-9]{1,2})?$"
              }
            ],
            "width": "130px",
            "label": "保险金额",
            "suffix": "元",
            "disabled": true
          },
          "ins_invalid_dt": {
            "control": "dateTimePicker",
            "label": "保险失效日期",
            "width": "130px",
            "format": "yyyyMMdd",
            "disabled": true
          },
          "is_valid": {
            "control": "radio",
            "label": "是否有效",
            "width": "130px",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            },
            "disabled": true
          },
          "ins_bnf_name": {
            "control": "text",
            "label": "保险受益人",
            "width": "130px",
            "disabled": true
          },
          "ins_via_name": {
            "control": "text",
            "label": "经办人",
            "width": "130px",
            "disabled": true
          },
          "ins_corp_name": {
            "control": "text",
            "label": "保险公司名称",
            "width": "130px",
            "disabled": true
          },
          "ins_corp_phone": {
            "control": "text",
            "label": "保险公司联系电话",
            "width": "130px",
            "disabled": true
          },
          "ins_desc": {
            "control": "text",
            "label": "保险单内容",
            "width": "130px",
            "disabled": true
          },
          "remark": {
            "label": "备注",
            "control": "textarea",
            "width": "130px",
            "span": "24",
            "autosize": {
              "minRows": 4
            },
            "disabled": true
          }
        }
      ]
    }
  }
}
