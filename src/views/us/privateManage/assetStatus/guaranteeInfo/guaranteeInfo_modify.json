{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "us0101",
              "insurance_serial_no": "${{params.insurance_serial_no}}",
              "cust_no": "${{params.cust_no}}"
            }
          }
        }
      ],
      "controls": [
        {
          "ins_no": {
            "control": "text",
            "label": "保险单号",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入保险单号"
              },
              {
                "max": 30,
                "message": "长度不能超过30个字符"
              }
            ]
          },
          "ins_type": {
            "control": "select",
            "label": "保险险种",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请选择保险险种"
              }
            ],
            "dict": {
              "dictType": "US_E_INS_TYPE",
              "dictKey": [
                "US_E_INS_TYPE"
              ],
              "format": "label"
            }
          },
          "ins_amt": {
            "control": "currency",
            "thousand": ",",
            "width": "130px",
            "label": "投保金额（元）",
            "rules": [
              {
                "required": true,
                "message": "请输入保险单号"
              },
              {
                "pattern": "^[0-9]+(\\.[0-9]{1,2})?$"
              }
            ]
          },
          "ins_recoup_amt": {
            "control": "currency",
            "thousand": ",",
            "width": "130px",
            "label": "保险金额（元）",
            "rules": [
              {
                "required": true,
                "pattern": "^[0-9]+(\\.[0-9]{1,2})?$",
                "message": "请输入保险金额"
              }
            ]
          },
          "ins_invalid_dt": {
            "control": "dateTimePicker",
            "label": "保险失效日期",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请选择保险失效日期"
              }
            ],
            "format": "yyyyMMdd"
          },
          "is_valid": {
            "control": "radio",
            "label": "是否有效",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请选择"
              }
            ],
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "ins_bnf_name": {
            "control": "text",
            "label": "保险受益人",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入保险受益人"
              },
              {
                "max": 40,
                "message": "长度不能超过40个字符"
              }
            ]
          },
          "ins_via_name": {
            "control": "text",
            "label": "经办人",
            "width": "130px",
            "rules": [
              {
                "max": 40,
                "message": "长度不能超过40个字符"
              }
            ]
          },
          "ins_corp_name": {
            "control": "text",
            "label": "保险公司名称",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入保险公司名称"
              },
              {
                "max": 80,
                "message": "长度不能超过80个字符"
              }
            ]
          },
          "ins_corp_phone": {
            "control": "text",
            "label": "保险公司联系电话",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入保险公司联系电话"
              },
              {
                "max": 80,
                "message": "长度不能超过80个字符"
              }
            ]
          },
          "ins_desc": {
            "control": "text",
            "label": "保险单内容",
            "width": "130px",
            "rules": [
              {
                "max": 100,
                "message": "长度不能超过100个字符"
              }
            ]
          },
          "remark": {
            "label": "备注",
            "control": "textarea",
            "width": "130px",
            "span": "24",
            "autosize": {
              "minRows": 4
            },
            "rules": [
              {
                "max": 100,
                "message": "100字以内"
              }
            ]
          }
        }
      ]
    }
  }
}
