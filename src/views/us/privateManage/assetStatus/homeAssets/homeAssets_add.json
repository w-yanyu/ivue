{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "scope": "templateScope",
            "initRequest": "${{params}}",
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "us0139"
                }
            },
            "btns": [
                {
                    "type": "submit",
                    "label": "提交"
                }
            ],
            "controls": [
                {
                    "cust_no": {
                        "control": "hidden",
                        "label": "客户编号",
                        "width": "130px",
                        "disabled": true
                    },
                    "asset_name": {
                        "control": "text",
                        "label": "资产名称",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入资产名称"
                            }
                        ]
                    },
                    "asset_type": {
                        "label": "资产种类",
                        "control": "select",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择资产种类"
                            }
                        ],
                        "dict": {
                            "dictType": "US_E_ASSET_TYPE",
                            "dictKey": [
                                "US_E_ASSET_TYPE"
                            ],
                            "format": "label"
                        }
                    },
                    "asset_ccy": {
                        "label": "资产评估币种",
                        "control": "select",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择币种"
                            }
                        ],
                        "dict": {
                            "dictType": "MS_CURRENCY",
                            "dictKey": [
                                "MS_CURRENCY"
                            ],
                            "format": "label"
                        }
                    },
                    "asset_value": {
                        "control": "currency",
                        "label": "资产评估金额(元)",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入资产评估金额"
                            }
                        ]
                    },
                    "asset_desc": {
                        "label": "资产描述",
                        "control": "textarea",
                        "width": "130px",
                        "span": "24",
                        "autosize": {
                            "minRows": 4
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入资产描述"
                            },
                            {
                                "max": 100,
                                "message": "字段在100字以内"
                            }
                        ]
                    }
                }
            ]
        }
    }
}
