{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initDataOnce": true,
            "initRequest": "${{params}}",
            "btns": [],
            "controls": [
                {
                    "cust_no": {
                        "control": "hidden",
                        "label": "客户编号",
                        "width": "130px",
                        "disabled": true
                    },
                    "asset_name": {
                        "control": "text",
                        "label": "资产名称",
                        "width": "130px",
                        "disabled": true
                    },
                    "asset_type": {
                        "label": "资产种类",
                        "control": "select",
                        "width": "130px",
                        "disabled": true,

                        "dict": {
                            "dictType": "US_E_ASSET_TYPE",
                            "dictKey": [
                                "US_E_ASSET_TYPE"
                            ],
                            "format": "label"
                        }
                    },
                    "asset_ccy": {
                        "label": "资产评估币种",
                        "control": "select",
                        "disabled": true,
                        "width": "130px",
                        "dict": {
                            "dictType": "MS_CURRENCY",
                            "dictKey": [
                                "MS_CURRENCY"
                            ],
                            "format": "label"
                        }
                    },
                    "asset_value": {
                        "control": "text",
                        "label": "资产评估金额(元)",
                        "width": "130px",
                        "disabled": true,
                        "format": {
                            "thousand": ",",
                            "decimal": "2",
                            "type": "currency"
                        }
                    },
                    "asset_desc": {
                        "label": "资产描述",
                        "control": "textarea",
                        "width": "130px",
                        "disabled": true,
                        "span": "24",
                        "autosize": {
                            "minRows": 4
                        }
                    }
                }
            ]
        }
    }
}
