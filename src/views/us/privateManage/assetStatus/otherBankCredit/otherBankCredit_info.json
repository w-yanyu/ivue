{
    "module": "v-inputDataForm-btn",
    "layout": {
        "form": {
            "btns": [],
            "scope": "myForm",
            "initDataOnce": true,
            "initRequest": "${{params}}",
            "controls": [{
                    "cust_no": {
                        "control": "string",
                        "width": "10px",
                        "label": "客户编号",
                        "disabled": true
                    },
                    "bank_name": {
                        "control": "string",
                        "width": "10px",
                        "label": "授信行名称"
                    },
                    "loan_type": {
                        "control": "string",
                        "width": "10px",
                        "label": "授信业务种类"
                    },
                    "contract_amt": {
                        "control": "string",
                        "width": "10px",
                        "label": "合同金额(元)"
                    },
                    "ccy_code": {
                        "control": "select",
                        "width": "10px",
                        "label": "授信币种",
                        "disabled": "true",
"selectFirstValue":true,
                        "dict": {
                            "dictType": "MS_CURRENCY",
                            "dictKey": [
                                "MS_CURRENCY"
                            ],
                            "format": "label"
                        }
                    },
                    "credit_balance": {
                        "control": "string",
                        "width": "10px",
                        "label": "授信余额(元)"
                    },
                    "guar_type": {
                        "control": "string",
                        "label": "担保方式",
                        "width": "10px",
                        "align": "center",
                        "dict": {
                            "dictType": "MS_E_GUAR_TYPE",
                            "dictKey": [
                                "MS_E_GUAR_TYPE"
                            ],
                            "format": "label"
                        }
                    },
                    "start_date": {
                        "control": "string",
                        "width": "10px",
                        "label": "授信开始日期"
                    },
                    "credit_end_date": {
                        "control": "string",
                        "width": "10px",
                        "label": "授信到期日期",
                        "disabledDate": {
                            "startRange": "${{myForm.start_date}}",
                            "endRange": "3000-01-01"
                        }
                    },
                    "overdue_count": {
                        "control": "string",
                        "width": "10px",
                        "label": "逾期期数"
                    },
                    "overdue_amt": {
                        "control": "string",
                        "width": "10px",
                        "label": "本期逾期总额(元)"
                    },
                    "max_overdue_count": {
                        "control": "string",
                        "width": "10px",
                        "label": "最高累计逾期期数"
                    }
                },
                {
                    "credit_purpose": {
                        "control": "textarea",
                        "disabled": "true",
                        "label": "授信用途",
                        "width": "10px",
                        "span": 24,
                        "autosize": {
                            "minRows": 4
                        }
                    },
                    "reputation_status": {
                        "control": "textarea",
                        "disabled": "true",
                        "label": "履约分析及信誉状况",
                        "width": "10px",
                        "span": 24,
                        "autosize": {
                            "minRows": 4
                        }
                    }
                }
            ]
        }
    }
}
