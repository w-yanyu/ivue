{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "btns": [],
            "initRequest": "${{params}}",
            "controls": [
                {
                    "cust_no": {
                        "control": "hidden",
                        "label": "客户编号",
                        "width": "130px",
                        "disabled": true
                    },
                    "guar_person": {
                        "control": "text",
                        "label": "被担保人",
                        "width": "130px",
                        "disabled": true
                    },
                    "guar_type": {
                        "control": "select",
                        "label": "担保方式:",
                        "width": "130px",
                        "disabled": true,
                        "dict": {
                            "dictType": "MS_E_GUAR_TYPE",
                            "dictKey": [
                                "MS_E_GUAR_TYPE"
                            ],
                            "format": "label"
                        }
                    },
                    "cert_type": {
                        "control": "select",
                        "label": "证件类型(征)",
                        "width": "130px",
                        "disabled": true,
                        "dict": {
                            "dictType": "MS_E_CRET_TYPE",
                            "dictKey": [
                                "MS_E_CRET_TYPE"
                            ],
                            "format": "label"
                        }
                    },
                    "cert_no": {
                        "control": "text",
                        "label": "证件号码(征)",
                        "width": "130px",
                        "disabled": true
                    },
                    "guar_amt": {
                        "control": "currency",
                        "width": "130px",
                        "thousand": ",",
                        "label": "担保金额(元)",
                        "disabled": true
                    },
                    "guaranty_deadline": {
                        "control": "text",
                        "label": "担保期限(月)",
                        "width": "130px",
                        "disabled": true
                    },
                    "effect_date": {
                        "control": "dateTimePicker",
                        "width": "130px",
                        "label": "担保起始日期",
                        "disabled": true
                    },
                    "expiry_date": {
                        "control": "dateTimePicker",
                        "label": "担保到期日期",
                        "width": "130px",
                        "disabled": true,
                        "disabledDate": {
                            "startRange": "${{myForm.effect_date}}",
                            "endRange": "3000-01-01"
                        }
                    },
                    "guar_person_bank": {
                        "control": "text",
                        "width": "130px",
                        "label": "被担保人开户行",
                        "disabled": true
                    },
                    "guar_person_acct": {
                        "control": "text",
                        "width": "130px",
                        "label": "被担保人开户帐号",
                        "disabled": true
                    },
                    "relationship": {
                        "control": "select",
                        "label": "和被担保人关系",
                        "disabled": true,
                        "width": "130px",
                        "dict": {
                            "dictType": "US_E_RELATIONSHIP",
                            "dictKey": [
                                "US_E_RELATIONSHIP"
                            ],
                            "format": "label"
                        }
                    },
                    "contract_no": {
                        "control": "text",
                        "label": "保证合同号",
                        "width": "130px",
                        "disabled": true
                    },
                    "guaranty_right": {
                        "control": "text",
                        "width": "130px",
                        "label": "担保债权",
                        "disabled": true
                    },
                    "reason": {
                        "control": "textarea",
                        "width": "130px",
                        "label": "担保理由",
                        "disabled": true
                    },
                    "is_guaranty_duty": {
                        "control": "select",
                        "label": "是否已履行担保责任",
                        "width": "130px",
                        "disabled": true,
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        }
                    },
                    "perform_amt": {
                        "control": "currency",
                        "thousand": ",",
                        "width": "130px",
                        "label": "履行金额(元)",
                        "disabled": true
                    },
                    "is_risk": {
                        "control": "select",
                        "label": "是否有潜在风险",
                        "width": "130px",
                        "disabled": true,
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        }
                    },
                    "risk_desc": {
                        "control": "textarea",
                        "label": "风险情况分析",
                        "disabled": true,
                        "width": "130px"
                    },
                    "is_litigation": {
                        "control": "select",
                        "label": "是否发生诉讼",
                        "disabled": true,
                        "width": "130px",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        }
                    },
                    "litigation_status": {
                        "control": "textarea",
                        "label": "诉讼情况",
                        "width": "130px",
                        "disabled": true
                    },
                    "status": {
                        "control": "select",
                        "label": "是否有效",
                        "width": "130px",
                        "disabled": true,
                        "dict": {
                            "dictType": "US_E_STATE",
                            "dictKey": [
                                "US_E_STATE"
                            ],
                            "format": "label"
                        }
                    },
                    "remark": {
                        "control": "textarea",
                        "width": "130px",
                        "label": "备注信息",
                        "disabled": true
                    }
                }
            ]
        }
    }
}
