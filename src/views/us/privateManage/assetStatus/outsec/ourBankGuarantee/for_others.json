{
    "module": "v_search_btn_datagrid",
    "layout": {
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": true,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "ar0725",
                    "cust_no": "${{params.cust_no}}",
                    "applicant_type": "GUAR"
                }
            },
            "thead": [{
                    "label": "被担保客户名称",
                    "prop": "cust_name",
                    "align": "center"
                },
                {
                    "label": "担保金额",
                    "prop": "guar_amt",
                    "type": "string",
                    "align": "center",
                    "format": {
                        "thousand": ",",
                        "decimal": "2",
                        "type": "currency"
                    }
                },
                {
                    "label": "保证期间",
                    "prop": "guar_section",
                    "align": "center",
                    "dict": {
                        "dictType": "AR_E_GUAR_SECTION",
                        "dictKey": [
                            "AR_E_GUAR_SECTION"
                        ],
                        "labelField": "dictName",
                        "valueField": "dictId"
                    }
                },
                {
                    "label": "担保比例",
                  "prop": "guar_ratio",
                    "align": "center"
                }
            ]
        }
    }
}
