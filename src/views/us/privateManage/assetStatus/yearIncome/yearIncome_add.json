{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "us0087"
        }
      },
      "btns": [
        {
          "type": "submit",
          "label": "提交"
        }
      ],
      "controls": [
        {
          "cust_no": {
            "control": "hidden",
            "label": "客户编号",
            "width": "130px",
            "disabled": true
          },
          "income_year": {
            "control": "dateTimePicker",
            "type": "year",
            "label": "收入年度(年)",
            "width": "130px",
            "format": "yyyy",
            "rules": [
              {
                "required": true,
                "message": "请输入收入年度"
              }
            ]           
          },
          "yrin_manage": {
            "label": "经营性收入(元)",
            "control": "currency",
            "align": "left",
            "width": "130px",
            "format":{
              "thousand":","
            }
          },
          "yrin_invest": {
            "label": "投资收入(元)",
            "control": "currency",
            "align": "left",
            "width": "130px",
            "format":{
              "thousand":","
            }
          },
          "yrin_salary": {
            "label": "工资性收入(元)",
            "control": "currency",
            "align": "left",
            "width": "130px",
            "format":{
              "thousand":","
            }
          },
          "other_income": {
            "label": "其他收入(元)",
            "control": "currency",
            "align": "left",
            "width": "130px",
            "format":{
              "thousand":","
            }
          },
          "yrin_payout": {
            "label": "货币支出(元)",
            "control": "currency",
            "align": "left",
            "width": "130px",
            "format":{
              "thousand":","
            }
          },
          "yrin_remain": {
            "label": "结存货币",
            "control": "select",
            "width": "130px",
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": [
                "MS_CURRENCY"
              ],
              "format": "label"
            }
          },
          "status": {
            "label": "状态",
            "control": "hidden",
            "width": "130px",
            "rules": [
              {
                "message": "请选择状态"
              }
            ],
            "dict": {
              "dictType": "US_E_STATE",
              "dictKey": [
                "US_E_STATE"
              ],
              "format": "label"
            }
          },
          "job_name": {
            "label": "职务名称",
            "control": "select",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请选择职务名称"
              }
            ],
            "dict": {
              "dictType": "US_E_EXECUTIVE_TYPE",
              "dictKey": [
                "US_E_EXECUTIVE_TYPE"
              ],
              "format": "label"
            }
          },
          "spouse_job_name": {
            "label": "配偶职务名称",
            "control": "select",
            "width": "130px",
            "dict": {
              "dictType": "US_E_EXECUTIVE_TYPE",
              "dictKey": [
                "US_E_EXECUTIVE_TYPE"
              ],
              "format": "label"
            }
          },
          "spouse_income": {
            "label": "配偶月收入(元)",
            "control": "currency",
            "align": "left",
            "width": "130px",
            "format":{
              "thousand":","
            }
          },
          "debt_payout": {
            "label": "债务性支出(元)",
            "control": "currency",
            "align": "left",
            "width": "130px",
            "format":{
              "thousand":","
            }
          },
          "daily_payout": {
            "label": "日常性支出(元)",
            "control": "currency",
            "align": "left",
            "width": "130px",
            "format":{
              "thousand":","
            }
          },
          "other_payout": {
            "label": "其他支出(元)",
            "control": "currency",
            "align": "left",
            "width": "130px",
            "format":{
              "thousand":","
            }
          },
          "remark": {
            "label": "备注",
            "control": "textarea",
            "width": "130px",
            "span": "24",
            "autosize": {
              "minRows": 4
            },
            "rules": [
              {
                "max": 100,
                "message": "100字以内"
              }
            ]
          }
        }
      ]
    }
  }
}
