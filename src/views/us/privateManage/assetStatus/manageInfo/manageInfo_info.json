{
    "module": "v-realonlyForm-btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "controls": [
                {
                    "busi_unit_name": {
                        "control": "text",
                        "label": "经营单位名称",
                        "width": "130px",
                        "disabled":true
                    },
                    "busi_license_no": {
                        "control": "text",
                        "label": "营业执照号码",
                        "width": "130px",
                        "disabled":true
                    },
                    "busi_unit_address": {
                        "control": "text",
                        "label": "经营单位地址",
                        "disabled":true,
                        "width": "130px"
                    },
                    "busi_unit_scale": {
                        "control": "text",
                        "label": "实体规模",
                        "width": "130px",
                        "disabled":true,
                        "min": 1,
                        "suffix": "人"
                    },
                    "busi_unit_type": {
                        "control": "select",
                        "label": "经营单位类型",
                        "disabled":true,
                        "width": "130px",
                        "dict": {
                            "dictType": "US_E_BUSI_UNIT_TYPE",
                            "dictKey": [
                                "US_E_BUSI_UNIT_TYPE"
                            ],
                            "format": "label"
                        }
                    },
                    "busi_unit_industry": {
                        "control": "select",
                        "disabled":true,
                        "label": "经营单位所属行业",
                        "width": "130px",
                        "dict": {
                            "dictType": "MS_E_WORKTP",
                            "dictKey": [
                                "MS_E_WORKTP"
                            ],
                            "format": "label"
                        }
                    },
                    "busi_unit_opening_bank": {
                        "control": "text",
                        "disabled":true,
                        "label": "单位开户行",
                        "width": "130px"
                    },
                    "busi_unit_main_business": {
                        "control": "text",
                        "label": "主营业务",
                        "disabled":true,
                        "width": "130px"
                    },
                    "busi_unit_operation_period": {
                        "control": "text",
                        "label": "从事经营年限",
                        "disabled":true,
                        "width": "130px",
                        "suffix": "年"
                    },
                    "busi_unit_income_ccy": {
                        "control": "select",
                        "disabled":true,
                        "width": "130px",
                        "label": "收入币种",
                        "dict": {
                            "dictType": "MS_CURRENCY",
                            "dictKey": [
                                "MS_CURRENCY"
                            ],
                            "format": "label"
                        }
                    },
                    "busi_unit_annual_income": {
                        "control": "text",
                        "width": "130px",
                        "disabled":true,
                        "label": "近三年年均经营收入",
                        "suffix": "万元"
                    },
                    "busi_curr_start_date": {
                        "control": "dateTimePicker",
                        "label": "现址经营开始时间",
                        "disabled":true,
                        "width": "130px",
                        "format": "yyyyMMdd"
                    },
                    "busi_monthtur": {
                        "control": "text",
                        "width": "130px",
                        "disabled":true,
                        "label": "月营业额",
                        "suffix": "元"
                    },
                    "busi_monthpro": {
                        "control": "text",
                        "width": "130px",
                        "disabled":true,
                        "label": "月营业利润",
                        "suffix": "元"
                    },
                    "owned_area": {
                        "control": "text",
                        "label": "所属片区",
                        "disabled":true,
                        "width": "130px"
                    },
                    "is_special_market": {
                        "control": "select",
                        "label": "是否所属专业市场",
                        "disabled":true,
                        "align": "center",
                        "width": "130px",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        },
                        "showItem": {
                            "1": [
                                "special_market_name"
                            ]
                        }
                    },
                    "special_market_name": {
                        "control": "text",
                        "label": "专业市场名称",
                        "disabled":true,
                        "width": "130px"
                    },
                    "join_commerce_flag": {
                        "control": "select",
                        "label": "是否加入商会",
                        "width": "130px",
                        "disabled":true,
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        },
                        "showItem": {
                            "1": [
                                "commerce_prop",
                                "commerce_name"
                            ]
                        }
                    },
                    "commerce_prop": {
                        "control": "select",
                        "label": "商会性质",
                        "disabled":true,
                        "width": "130px",
                        "dict": {
                            "dictType": "US_E_COMMERCE_PROP",
                            "dictKey": [
                                "US_E_COMMERCE_PROP"
                            ],
                            "format": "label"
                        }
                    },
                    "commerce_name": {
                        "control": "text",
                        "label": "商会名称",
                        "disabled":true,
                        "width": "130px"
                    },
                    "is_have_partner": {
                        "control": "select",
                        "label": "是否与人合伙",
                        "disabled":true,
                        "width": "130px",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        }
                    },
                    "stock_scale": {
                        "control": "text",
                        "label": "所占股份比例(%)",
                        "disabled":true,
                        "width": "130px"
                    },
                    "own_busi_period": {
                        "control": "text",
                        "label": "自主经营年限",
                        "disabled":true,
                        "width": "130px",
                        "suffix": "年"
                    },
                    "curr_busi_period": {
                        "control": "text",
                        "label": "现经营行业年限",
                        "disabled":true,
                        "width": "130px",
                        "suffix": "年"
                    },
                    "is_busilicen": {
                      "control": "select",
                      "label": "是否有营业执照",
                      "disabled":true,
                      "width": "130px",
                      "dict": {
                        "dictType": "MS_E_YESORNO",
                        "dictKey": [
                          "MS_E_YESORNO"
                        ],
                        "format": "label"
                      }
                    },
                    "busi_premi": {
                      "control": "text",
                      "label": "营业场地",
                      "disabled":true,
                      "width": "130px"
                    },
                    "is_keymember": {
                      "control": "select",
                      "label": "是否本人或者家庭主要成员经营",
                      "disabled":true,
                      "width": "130px",
                      "dict": {
                        "dictType": "MS_E_YESORNO",
                        "dictKey": [
                          "MS_E_YESORNO"
                        ],
                        "format": "label"
                      }
                    }
                }
            ]
        }
    }
}

