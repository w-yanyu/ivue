{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "us0153",
              "manage_id": "${{params.manage_id}}",
              "cust_no": "${{params.cust_no}}"
            }
          }
        }
      ],
      "controls": [
        {
          "busi_unit_name": {
            "control": "text",
            "label": "经营单位名称",
            "width": "130px",
            "rules": [
              {
                "max": 100,
                "message": "长度不能超过100个字符"
              },
              {
                "required": true,
                "message": "请输入"
              }
            ]
          },
          "busi_license_no": {
            "control": "text",
            "label": "营业执照号码",
            "width": "130px",
            "rules": [
              {
                "max": 100,
                "message": "长度不能超过100个字符"
              },
              {
                "required": true,
                "message": "请输入"
              }
            ]
          },
          "busi_unit_address": {
            "control": "text",
            "label": "经营单位地址",
            "width": "130px",
            "rules": [
              {
                "max": 100,
                "message": "长度不能超过100个字符"
              },
              {
                "required": true,
                "message": "请输入"
              }
            ]
          },
          "busi_unit_scale": {
            "control": "text",
            "label": "实体规模",
            "width": "130px",
            "min": 1,
            "rules": [
              {
                "required": true,
                "message": "请输入"
              },
              {
                "type": "number",
                "message": "填写整型数据"
              }
            ],
            "suffix": "人"
          },
          "busi_unit_type": {
            "control": "select",
            "label": "经营单位类型",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请选择"
              }
            ],
            "dict": {
              "dictType": "US_E_BUSI_UNIT_TYPE",
              "dictKey": [
                "US_E_BUSI_UNIT_TYPE"
              ],
              "format": "label"
            }
          },
          "busi_unit_industry": {
            "control": "select",
            "label": "经营单位所属行业",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请选择"
              }
            ],
            "dict": {
              "dictType": "MS_E_WORKTP",
              "dictKey": [
                "MS_E_WORKTP"
              ],
              "format": "label"
            }
          },
          "busi_unit_opening_bank": {
            "control": "text",
            "label": "单位开户行",
            "width": "130px",
            "rules": [
              {
                "max": 60,
                "message": "长度不能超过60个字符"
              },
              {
                "required": true,
                "message": "请输入"
              }
            ]
          },
          "busi_unit_main_business": {
            "control": "text",
            "label": "主营业务",
            "width": "130px",
            "rules": [
              {
                "max": 200,
                "message": "长度不能超过200个字符"
              },
              {
                "required": true,
                "message": "请输入"
              }
            ]
          },
          "busi_unit_operation_period": {
            "control": "text",
            "label": "从事经营年限",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入"
              },
              {
                "type": "number",
                "message": "填写整型数据"
              }
            ],
            "suffix": "年"
          },
          "busi_unit_income_ccy": {
            "control": "select",
            "width": "130px",
            "label": "收入币种",
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": [
                "MS_CURRENCY"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          },
          "busi_unit_annual_income": {
            "control": "text",
            "align": "left",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "近三年年均经营收入不为空"
              }
            ],
            "suffix": "万元",
            "label": "近三年年均经营收入"
          },
          "busi_curr_start_date": {
            "control": "dateTimePicker",
            "label": "现址经营开始时间",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请选择保险失效日期"
              }
            ],
            "format": "yyyyMMdd"
          },
          "busi_monthtur": {
            "control": "text",
            "label": "月营业额",
            "align": "left",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入月营业额"
              }
            ],
            "suffix": "元"
          },
          "busi_monthpro": {
            "control": "text",
            "align": "left",
            "label": "月营业利润",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入月营业利润"
              }
            ],
            "suffix": "元"
          },
          "owned_area": {
            "control": "text",
            "label": "所属片区",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "所属片区不为空"
              },
              {
                "max": 32,
                "message": "长度不能超过32个字符"
              }
            ]
          },
          "is_special_market": {
            "control": "select",
            "label": "是否所属专业市场",
            "align": "center",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请选择"
              }
            ],
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            },
            "showItem": {
              "1": [
                "special_market_name"
              ]
            }
          },
          "special_market_name": {
            "control": "text",
            "label": "专业市场名称",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入"
              },
              {
                "max": 100,
                "message": "长度不能超过100个字符"
              }
            ]
          },
          "join_commerce_flag": {
            "control": "select",
            "label": "是否加入商会",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请选择"
              }
            ],
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            },
            "showItem": {
              "1": [
                "commerce_prop",
                "commerce_name"
              ]
            }
          },
          "commerce_prop": {
            "control": "select",
            "label": "商会性质",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请选择"
              }
            ],
            "dict": {
              "dictType": "US_E_COMMERCE_PROP",
              "dictKey": [
                "US_E_COMMERCE_PROP"
              ],
              "format": "label"
            }
          },
          "commerce_name": {
            "control": "text",
            "label": "商会名称",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请选择"
              },
              {
                "max": 100,
                "message": "长度不能超过100个字符"
              }
            ]
          },
          "is_have_partner": {
            "control": "select",
            "label": "是否与人合伙",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请选择"
              }
            ],
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            },
            "showItem": {
              "1": [
                "stock_scale"
              ]
            }
          },
          "stock_scale": {
            "control": "inputNumber",
            "label": "所占股份比例(%)",
            "width": "130px",
            "min": 0,
            "max": 100,
            "controls": false,
            "rules": [
              {
                "required": true,
                "message": "请输入"
              },
              {
                "type": "number",
                "message": "填写整型数据"
              }
            ]
          },
          "own_busi_period": {
            "control": "text",
            "label": "自主经营年限",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入"
              },
              {
                "type": "number",
                "message": "填写整型数据"
              }
            ],
            "suffix": "年"
          },
          "curr_busi_period": {
            "control": "text",
            "label": "现经营行业年限",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入"
              },
              {
                "type": "number",
                "message": "填写整型数据"
              }
            ],
            "suffix": "年"
          },
          "is_busilicen": {
            "control": "select",
            "label": "是否有营业执照",
            "width": "130px",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择是否有营业执照"
              }
            ]
          },
          "busi_premi": {
            "control": "text",
            "label": "营业场地",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入营业场地"
              },
              {
                "max": 100,
                "message": "长度不能超过100个字符"
              }
            ]
          },
          "is_keymember": {
            "control": "select",
            "label": "是否本人或者家庭主要成员经营",
            "width": "130px",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择是否本人或者家庭主要成员经营"
              }
            ]
          }
        }
      ]
    }
  }
}

