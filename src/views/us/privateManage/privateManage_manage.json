{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "cust_no": {
            "control": "text",
            "label": "客户编号",
            "rules": [
              {
                "max": 30,
                "message": "长度不能超过30个字符"
              }
            ]
          },
          "cust_name": {
            "control": "text",
            "label": "客户名称",
            "rules": [
              {
                "max": 80,
                "message": "长度不能超过80个字符"
              }
            ]
          },
          "cert_type": {
            "control": "select",
            "label": "证件类型",
            "dict": {
              "dictType": "US_E_PRI_CRET_TYPE",
              "dictKey": [
                "US_E_PRI_CRET_TYPE"
              ],
              "labelField": "dictName",
              "valueField": "dictId"
            }
          },
          "cert_no": {
            "control": "text",
            "label": "证件号码"
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "target": "dialog",
          "label": "新增",
          "toRequest": {
            "method": "GET",
            "url": "/views/us/privateManage/privateManage_add.json"
          },
          "props": {
            "width": "90%",
            "title": "对私客户建立"
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "belong_flash":"${{params.belong_flash}}",
          "servicecode": "us0001"
        }
      },
      "thead": [
        {
          "type": "index",
          "label": "序号"
        },
        {
          "label": "客户号",
          "prop": "cust_no",
          "align": "center"
        },
        {
          "label": "客户名称",
          "prop": "cust_name",
          "align": "center"
        },
        {
          "label": "证件类型",
          "prop": "cert_type",
          "align": "center",
          "dict": {
            "dictType": "MS_E_CRET_TYPE",
            "dictKey": [
              "MS_E_CRET_TYPE"
            ],
            "labelField": "dictName",
            "valueField": "dictId"
          }
        },
        {
          "label": "证件号码",
          "prop": "cert_no",
          "align": "center"
        },
        {
          "label": "有效状态",
          "prop": "valid_state",
          "align": "center",
          "dict": {
            "dictType": "MS_E_YESORNO",
            "dictKey": [
              "MS_E_YESORNO"
            ],
            "labelField": "dictName",
            "valueField": "dictId"
          }
        },
        {
          "label": "所属法人",
          "prop": "org_id",
          "align": "center"
        },
        {
          "label": "所属用户",
          "prop": "user_id",
          "align": "center"
        },
        {
          "label": "所属机构",
          "prop": "branch_id",
          "align": "center"
        },
        {
          "label": "操作",
          "width": "210px",
          "align": "center",
          "btns": [
            {
              "type": "custom",
              "label": "详情",
              "toRequest": {
                "url": "/views/us/privateManage/privateManage_info.json",
                "method": "POST",
                "params": {
                  "show_flash": 1
                }
              },
              "props": {
                "width": "95%",
                "height": "90%",
                "title": "详情"
              }
            },
            {
              "type": "look",
              "label": "变更历史",
              "toRequest": {
                "url": "/views/us/privateManage/privateManage_history.json",
                "method": "POST"
              },
              "props": {
                "width": "90%",
                "title": "变更历史"
              }
            },
            {
              "type": "edit",
              "show": {
                "prop": "valid_state",
                "is": [
                  "0"
                ]
              },
              "label": "状态审核",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "us0004"
                }
              }
            },
            {
              "type": "look",
              "label": "客户360视图",
              "toRequest": {
                "url": "@CustView",
                "method": "POST",
                "params": {
                  "cust_id":"${{current.cust_no}}"
                }
              },
              "props": {
                "width": "70%",
                "height": "95%",
                "title": "查看客户360视图"
              }
            }
          ]
        }
      ]
    }
  }
}
