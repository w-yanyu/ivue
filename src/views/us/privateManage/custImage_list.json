{
    "module": "v_search_btn_datagrid",
    "pageId":"####",
    "layout": {
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch":true ,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "us0201"

                }
            },
            "thead": [{
                "type": "index",
                "label": "序号"
            },
                {
                    "label": "影像名称",
                    "prop": "qxlx",
                    "align": "center"
                },
                {
                    "label": "上传时间",
                    "prop": "kehmingc",
                    "align": "center"
                },
                {
                    "label": "上传人",
                    "prop": "kehuhaoo",
                    "align": "center",
                    "dict": {
                        "dictType": "US_E_LEGAL_CUST_PROPERTY",
                        "dictKey": [
                            "US_E_LEGAL_CUST_PROPERTY"
                        ],
                        "labelField": "dictName",
                        "valueField": "dictId"
                    }
                },
                {
                    "label": "操作",
                    "width": "210px",
                    "align": "center",
                    "btns": [{
                            "type": "custom",
                            "label": "上传",
                            "toRequest": {
                                "url": "/views/us/entManager/ent_cust_tree.json",
                                "params": {
                                    "show_flash":"1"
                                },
                                "props": {
                                    "width": "95%",
                                    "height": "590px",
                                    "title": "对公客户信息"
                                }
                            }
                        },
                        {
                            "type": "edit",
                            "label": "查看",
                            "toRequest": {
                                "url": "/views/us/entManager/sel_credit_template.json",
                                "props": {
                                    "width": "80%",
                                    "title": "信评模板选择"
                                }
                            }
                        }
                    ]
                }
            ]
        }
    }
}
