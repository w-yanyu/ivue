{
    "module": "v-realonlyForm-btn",
    "layout": {
        "form": {
            "initDataOnce": true,
            "initRequest": "${{params}}",
            "btns": [],
            "controls": [
                {
                "bal_sheet_type": {
                    "control": "text",
                    "label": "负债类型",
                    "disabled": true,
                    "width": "130px"
                    
                },
                "associat_institution": {
                    "control": "text",
                    "label": "关联金融机构",
                    "disabled": true,
                    "width": "130px"              
                }
                },
                {
                    "credit_ccy": {
                        "control": "text",
                        "label": "授信或贷款金额币种",
                        "disabled": true,
                        "width": "130px"              
                    },
                    "credit_bal": {
                        "control": "text",
                        "disabled": true,
                        "label": "授信或贷款金额",
                        "width": "130px"
                    }   
                },
                {
                    "loan_bal": {
                        "control": "text",
                        "label": "贷款余额",
                        "disabled": true,
                        "width": "130px"
                    },
                    "closing_date": {
                        "control": "dateTimePicker",
                        "format":"yyyyMMdd",
                        "disabled": true,
                        "label": "截止日期",
                        "width": "130px"
                    }
                },
                {
                    "monthly_payment": {
                        "control": "text",
                        "disabled": true,
                        "label": "月还款额",
                        "width": "130px"
                    },
                    "matu_date": {
                        "control": "dateTimePicker",
                        "format":"yyyyMMdd",
                        "disabled": true,
                        "label": "到期日期",
                        "width": "130px"
                    }
                 },
                 {
                    "accounts_number": {
                        "control": "text",
                        "disabled": true,
                        "label": "账户数",
                        "width": "130px"
                    }

                 }
            ]
        }
    }
}