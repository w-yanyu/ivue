{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "us0167"
                }
            },
            "controls": [
                {
                    "cust_no": {
                        "control": "text",
                        "label": "客户编号",
                        "value": "${{params.cust_no}}",
                        "disabled": true
                    },
                "bal_sheet_type": {
                    "control": "text",
                    "label": "负债类型",
                    "width": "130px"
                },
                "associat_institution": {
                    "control": "text",
                    "label": "关联金融机构",
                    "width": "130px"              
                }
                },
                {
                    "credit_ccy": {
                        "control": "text",
                        "label": "授信或贷款金额币种",
                        "width": "130px"              
                    },
                    "credit_bal": {
                        "control": "text",
                        "label": "授信或贷款金额",
                        "width": "130px"
                    }   
                },
                {
                    "loan_bal": {
                        "control": "text",
                        "label": "贷款余额",
                        "width": "130px"
                    },
                    "closing_date": {
                        "control": "dateTimePicker",
                        "format":"yyyyMMdd",
                        "label": "截止日期",
                        "width": "130px"
                    }
                },
                {
                    "monthly_payment": {
                        "control": "text",
                        "label": "月还款额",
                        "width": "130px"
                    },
                    "matu_date": {
                        "control": "dateTimePicker",
                        "format":"yyyyMMdd",
                        "disabled": true,
                        "label": "到期日期",
                        "width": "130px"
                    }
                 },
                 {
                    "accounts_number": {
                        "control": "text",
                        "label": "账户数",
                        "width": "130px"
                    }

                 }
            ]
        }
    }
}