{
    "module": "v_search_btn_datagrid",
    "pageId":"####",
    "layout": {
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch":true ,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "us1007"

                }
            },
            "thead": [{
                "type": "index",
                "label": "序号"
                },
                {
                    "label": "主信息标识",
                    "prop": "master_information_mark",
                    "align": "center"
                },
                {
                    "label": "联系人名称",
                    "prop": "contact",
                    "align": "center"
                },
                {
                    "label": "联系人性别",
                    "prop": "contact_gender",
                    "align": "center"
                },
                {
                    "label": "联系人职务",
                    "prop": "contact_position",
                    "align": "center"
                },
                {
                    "label": "联系人常驻国家",
                    "prop": "resident_country_of_contact",
                    "align": "center"
                },
                {
                    "label": "联系人移动电话",
                    "prop": "contact_mobile_phone",
                    "align": "center"
                },
                {
                    "label": "联系人办公电话",
                    "prop": "contact_office_phone",
                    "align": "center"
                },
                {
                    "label": "操作",
                    "width": "210px",
                    "align": "center",
                    "btns": [{
                            "type": "edit",
                            "label": "查看",
                            "toRequest": {
                                "url": "/views/us/entManager/ent_cust_tree.json",
                                "params": {
                                    "show_flash":"1"
                                },
                                "props": {
                                    "width": "95%",
                                    "height": "590px",
                                    "title": "联系人信息"
                                }
                            }
                        },
                        
                        {
                            "type": "edit",
                            "label": "修改",
                            "toRequest": {
                                "url": "/views/us/entManager/sel_credit_template.json",
                                "props": {
                                    "width": "80%",
                                    "title": "修改联系人信息"
                                }
                            }
                        }
                    ]
                }
            ]
        }
    }
}
