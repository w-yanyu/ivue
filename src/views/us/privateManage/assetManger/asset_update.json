{
    "module": "v-inputDataForm-btn",
    "layout": {
        "form": {
            "scope": "templateScope",
            "initDataOnce": true,
            "initRequest": "${{params}}",
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "post",
                "params": {
                    "servicecode": "us0162"
                    
                }
            },
            
            "controls": [
                {
                    "asset_serial_no": {
                        "control": "hidden",
                        "label": "流水号",
                        "width": "130px"
                    },
                   
                "asset_type1": {
                    "control": "text",
                    "label": "资产类型",
                    "width": "130px"
                },
                "associat_institution": {
                    "control": "text",
                    "label": "关联金融机构",
                    "width": "130px"              
                }
                },
                {
                    "asset_desc": {
                        "control": "text",
                        "label": "资产描述",
                        "width": "130px"              
                    },
                    "asset_ccy": {
                        "control": "text",
                        "label": "资产评估币种",
                        "width": "130px"
                    }   
                },
                {
                    "asset_value": {
                        "control": "text",
                        "label": "资产评估金额",
                        "width": "130px"
                    },
                    "valuation_date": {
                        "control": "dateTimePicker",
                        "format":"yyyyMMdd",
                        "label": "评估日期",
                        "width": "130px"
                    }
                },
                {
                    "month_income": {
                        "control": "text",
                        "label": "此资产月收入",
                        "width": "130px"
                    }
                 }
            ]
        }
    }
}