{
    "module": "v-realonlyForm-btn",
    "layout": {
        "form": {
            "initDataOnce": true,
            "initRequest": "${{params}}",
            "btns": [],
            "controls": [
                {
                "asset_type1": {
                    "control": "text",
                    "label": "资产类型",
                    "disabled": true,
                    "width": "130px"
                    
                },
                "associat_institution": {
                    "control": "text",
                    "label": "关联金融机构",
                    "disabled": true,
                    "width": "130px"              
                }
                },
                {
                    "asset_desc": {
                        "control": "text",
                        "label": "资产描述",
                        "disabled": true,
                        "width": "130px"              
                    },
                    "asset_ccy": {
                        "control": "text",
                        "disabled": true,
                        "label": "资产评估币种",
                        "width": "130px"
                    }   
                },
                {
                    "asset_value": {
                        "control": "text",
                        "label": "资产评估金额",
                        "disabled": true,
                        "width": "130px"
                    },
                    "valuation_date": {
                        "control": "dateTimePicker",
                        "format":"yyyyMMdd",
                        "disabled": true,
                        "label": "评估日期",
                        "width": "130px"
                    }
                },
                {
                    "month_income": {
                        "control": "text",
