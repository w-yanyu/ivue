{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initDataOnce": true,
            "initRequest": "${{params}}",
            "btns": [],
            "controls": [
                {
                    "cust_no": {
                        "label": "客户序号",
                        "control": "hidden",
                        "width": "130px"
                    },
                    "event_type": {
                        "label": "事件类型",
                        "control": "string",
                        "width": "130px",
                        "dict": {
                            "dictType": "US_E_EVENT_TYPE",
                            "dictKey": [
                                "US_E_EVENT_TYPE"
                            ],
                            "format": "label"
                        }
                    },
                    "event_name": {
                        "label": "事件名称",
                        "control": "string",
                        "width": "130px"
                    },
                    "start_date": {
                        "control": "string",
                        "label": "发生日期",
                        "format": "yyyyMMdd",
                        "width": "130px"
                    },
                    "end_date": {
                        "control": "string",
                        "label": "结束日期",
                        "format": "yyyyMMdd",
                        "width": "130px",
                        "disabledDate": {
                            "startRange": "${{form.start_date}}",
                            "endRange": "3000-12-30"
                        }
                    },
                    "event_content": {
                        "label": "事件内容",
                        "control": "string",
                        "width": "130px"
                    },
                    "remark": {
                        "label": "备注",
                        "control": "textarea",
                        "width": "130px",
                        "span": "24",
                        "autosize": {
                            "minRows": 4
                        },
                        "disabled": true
                    }
                }
            ]
        }
    }
}
