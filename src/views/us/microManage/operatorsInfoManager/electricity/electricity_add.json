{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "btns": [
        {
          "type": "submit",
          "label": "保存",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "us0841",
              "cust_no": "${{params.cust_no}}",
              "register_branch_id":"$g{{user_info.branch_id}}",
              "hydropower_type": "2",
              "unit": "2"
            }
          }
        }
      ],
      "controls": [
        {
          "years": {
            "control": "dateTimePicker",
            "label": "年份",
            "span": "24",
            "width": "25px",
            "type": "year",
            "format": "yyyy",
            "rules": [
              {
                "required": true,
                "message": "请选择年份"
              }
            ]
          },
          "january": {
            "control": "text",
            "label": "一月",
            "span": "24",
            "width": "25px",
            "suffix": "度"
          },
          "february": {
            "control": "text",
            "span": "24",
            "width": "25px",
            "suffix": "度",
            "label": "二月"
          },
          "march": {
            "control": "text",
            "span": "24",
            "width": "25px",
            "suffix": "度",
            "label": "三月"
          },
          "april": {
            "control": "text",
            "span": "24",
            "width": "25px",
            "suffix": "度",
            "label": "四月"
          },
          "may": {
            "control": "text",
            "span": "24",
            "width": "25px",
            "suffix": "度",
            "label": "五月"
          },
          "june": {
            "control": "text",
            "span": "24",
            "width": "25px",
            "suffix": "度",
            "label": "六月"
          },
          "july": {
            "control": "text",
            "span": "24",
            "width": "25px",
            "suffix": "度",
            "label": "七月"
          }
        },
        {
          "august": {
            "control": "text",
            "span": "24",
            "width": "25px",
            "suffix": "度",
            "align": "left",
            "label": "八月"
          }
        },
        {
          "september": {
            "control": "text",
            "span": "24",
            "width": "25px",
            "suffix": "度",
            "align": "left",
            "label": "九月"
          }
        },
        {
          "october": {
            "control": "text",
            "span": "24",
            "width": "25px",
            "suffix": "度",
            "align": "left",
            "label": "十月"
          }
        },
        {
          "november": {
            "control": "text",
            "span": "24",
            "width": "25px",
            "suffix": "度",
            "label": "十一月"
          },
          "december": {
            "control": "text",
            "span": "24",
            "width": "25px",
            "suffix": "度",
            "label": "十二月"
          },
          "remark": {
            "control": "textarea",
            "span": "24",
            "rows": "4",
            "width": "25px",
            "suffix": "度",
            "label": "备注"
          }
        }
      ]
    }
  }
}
