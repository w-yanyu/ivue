{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "years": {
            "control": "dateTimePicker",
            "label": "年份",
            "span": "8",
            "width": "25px",
            "type": "year",
            "format": "yyyy"
          }
        },
        {
          "pay_tax_type": {
            "control": "select",
            "label": "纳税类型",
            "span": "8",
            "width": "25px",
            "value": "1",
            "dict": {
              "dictType": "E_PAY_TAX_TYPE",
              "dictKey": [
                "E_PAY_TAX_TYPE"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择"
              }
            ]
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "target": "dialog",
          "label": "新增",
          "toRequest": {
            "method": "GET",
            "url": "/views/us/microManage/operatorsInfoManager/payTax/payTax_add.json",
            "params": {
              "cust_no": "${{params.cust_no}}"
            }
          },
          "props": {
            "width": "40%",
            "title": "纳税信息新增"
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "us0866",
          "cust_no": "${{params.cust_no}}"
        }
      },
      "thead": [
        {
          "type": "index",
          "label": "序号"
        },
        {
          "label": "年份",
          "prop": "years",
          "align": "center"
        },
        {
          "label": "纳税类型",
          "prop": "pay_tax_type",
          "align": "center",
          "dict": {
            "dictType": "E_PAY_TAX_TYPE",
            "dictKey": [
              "E_PAY_TAX_TYPE"
            ],
            "format": "label"
          }
        },
        {
          "label": "币种",
          "prop": "ccy_code",
          "align": "center",
          "dict": {
            "dictType": "MS_CURRENCY",
            "dictKey": [
              "MS_CURRENCY"
            ],
            "format": "label"
          }
        },
        {
          "label": "一月",
          "prop": "january",
          "align": "center",
          "format":{
            "thousand": ",",
            "decimal": 2
          }
        },
        {
          "label": "二月",
          "prop": "february",
          "align": "center",
          "format":{
            "thousand": ",",
            "decimal": 2
          }
        },
        {
          "label": "三月",
          "prop": "march",
          "align": "center",
          "format":{
            "thousand": ",",
            "decimal": 2
          }
        },
        {
          "label": "四月",
          "prop": "april",
          "align": "center",
          "format":{
            "thousand": ",",
            "decimal": 2
          }
        },
        {
          "label": "五月",
          "prop": "may",
          "align": "center",
          "format":{
            "thousand": ",",
            "decimal": 2
          }
        },
        {
          "label": "六月",
          "prop": "june",
          "align": "center",
          "format":{
            "thousand": ",",
            "decimal": 2
          }
        },
        {
          "label": "七月",
          "prop": "july",
          "align": "center",
          "format":{
            "thousand": ",",
            "decimal": 2
          }
        },
        {
          "label": "八月",
          "prop": "august",
          "align": "center",
          "format":{
            "thousand": ",",
            "decimal": 2
          }
        },
        {
          "label": "九月",
          "prop": "september",
          "align": "center",
          "format":{
            "thousand": ",",
            "decimal": 2
          }
        },
        {
          "label": "十月",
          "prop": "october",
          "align": "center",
          "format":{
            "thousand": ",",
            "decimal": 2
          }
        },
        {
          "label": "十一月",
          "prop": "november",
          "align": "center",
          "format":{
            "thousand": ",",
            "decimal": 2
          }
        },
        {
          "label": "十二月",
          "prop": "december",
          "align": "center",
          "format":{
            "thousand": ",",
            "decimal": 2
          }
        },
        {
          "label": "操作",
          "width": "180px",
          "fixed": "right",
          "align": "center",
          "btns": [
            {
              "type": "custom",
              "label": "详请",
              "toRequest": {
                "url": "/views/us/microManage/operatorsInfoManager/payTax/payTax_info.json",
                "params": {
                  "cust_no": "${{params.cust_no}}"
                },
                "props": {
                  "width": "40%",
                  "height": "590px",
                  "title": "纳税信息"
                }
              }
            },
            {
              "type": "edit",
              "label": "维护",
              "toRequest": {
                "url": "/views/us/microManage/operatorsInfoManager/payTax/payTax_modify.json",
                "params": {
                  "cust_no": "${{params.cust_no}}"
                },
                "props": {
                  "width": "40%",
                  "height": "590px",
                  "title": "纳税信息维护"
                }
              }
            },
            {
              "type": "custom",
              "label": "删除",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "us0869",
                  "cust_no": "${{params.cust_no}}"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
