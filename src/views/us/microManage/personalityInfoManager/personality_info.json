{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "btns": [],
      "controlsGroup": [
        {
          "title": "人品信息",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "accordion": false,
          "expand": true,
          "controls": [
            {
              "evaluator": {
                "control": "string",
                "label": "评价人",
                "span": "8",
                "width": "30px"
              },
              "is_negative_com": {
                "control": "select",
                "span": "8",
                "width": "30px",
                "disabled": true,
                "label": "是否有负面评价",
                "dict": {
                  "dictType": "MS_E_YESORNO",
                  "dictKey": [
                    "MS_E_YESORNO"
                  ],
                  "format": "label"
                }
              },
              "is_illegal_fundraising": {
                "control": "select",
                "span": "8",
                "width": "30px",
                "disabled": true,
                "label": "有无参与非法集资",
                "dict": {
                  "dictType": "MS_E_YESORNO",
                  "dictKey": [
                    "MS_E_YESORNO"
                  ],
                  "format": "label"
                }
              },
              "is_criminal_record": {
                "control": "select",
                "span": "8",
                "width": "30px",
                "disabled": true,
                "label": "有无犯罪记录",
                "dict": {
                  "dictType": "MS_E_YESORNO",
                  "dictKey": [
                    "MS_E_YESORNO"
                  ],
                  "format": "label"
                }
              },
              "is_drug": {
                "control": "select",
                "span": "8",
                "width": "30px",
                "disabled": true,
                "label": "是否吸毒",
                "dict": {
                  "dictType": "MS_E_YESORNO",
                  "dictKey": [
                    "MS_E_YESORNO"
                  ],
                  "format": "label"
                }
              },
              "is_illegal": {
                "control": "select",
                "span": "8",
                "width": "30px",
                "disabled": true,
                "label": "是否涉赌、涉黄",
                "dict": {
                  "dictType": "MS_E_YESORNO",
                  "dictKey": [
                    "MS_E_YESORNO"
                  ],
                  "format": "label"
                }
              },
              "social_reputation": {
                "control": "select",
                "span": "8",
                "width": "30px",
                "disabled": true,
                "label": "社会声誉",
                "dict": {
                  "dictType": "E_SOCIAL_REPUTATION",
                  "dictKey": [
                    "E_SOCIAL_REPUTATION"
                  ],
                  "format": "label"
                }
              }
            },
            {
              "evaluate": {
                "control": "checkbox",
                "span": "24",
                "width": "30px",
                "disabled": true,
                "align": "left",
                "label": "风评",
                "dict": {
                  "dictType": "US_E_EVALUATE",
                  "dictKey": [
                    "US_E_EVALUATE"
                  ],
                  "format": "label"
                }
              }
            },
            {
              "bad_habit": {
                "control": "checkbox",
                "span": "24",
                "width": "30px",
                "disabled": true,
                "align": "left",
                "label": "不良嗜好",
                "dict": {
                  "dictType": "US_E_BAD_HABIT",
                  "dictKey": [
                    "US_E_BAD_HABIT"
                  ],
                  "format": "label"
                }
              }
            },
            {
              "major_accident": {
                "control": "checkbox",
                "span": "24",
                "width": "30px",
                "disabled": true,
                "align": "left",
                "label": "重大变故",
                "dict": {
                  "dictType": "US_E_MAJOR_ACCIDENT",
                  "dictKey": [
                    "US_E_MAJOR_ACCIDENT"
                  ],
                  "format": "label"
                }
              }
            },
            {
              "is_will_work": {
                "control": "select",
                "span": "8",
                "width": "30px",
                "disabled": true,
                "label": "是否有劳动意愿",
                "dict": {
                  "dictType": "MS_E_YESORNO",
                  "dictKey": [
                    "MS_E_YESORNO"
                  ],
                  "format": "label"
                }
              },
              "is_can_work": {
                "control": "select",
                "span": "8",
                "width": "30px",
                "disabled": true,
                "label": "是否有劳动能力",
                "dict": {
                  "dictType": "MS_E_YESORNO",
                  "dictKey": [
                    "MS_E_YESORNO"
                  ],
                  "format": "label"
                }
              },
              "negative_description": {
                "control": "textarea",
                "span": "24",
                "rows": "4",
                "width": "30px",
                "disabled": true,
                "label": "负面评价说明"
              },
              "other_negative_info": {
                "control": "textarea",
                "span": "24",
                "rows": "4",
                "width": "30px",
                "disabled": true,
                "label": "其他负面信息"
              },
              "remark": {
                "control": "textarea",
                "span": "24",
                "rows": "4",
                "width": "30px",
                "disabled": true,
                "label": "备注"
              }
            }
          ]
        },
        {
          "title": "操作人信息",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "accordion": false,
          "expand": true,
          "controls": [
            {
              "register_branch_id": {
                "control": "string",
                "label": "登记机构",
                "span": "8",
                "width": "30px"
              },
              "update_branch_id": {
                "control": "string",
                "label": "更新机构",
                "span": "8",
                "width": "30px"
              },
              "data_create_user": {
                "control": "string",
                "label": "登记人",
                "span": "8",
                "width": "30px"
              },
              "data_update_user": {
                "control": "string",
                "label": "更新人",
                "span": "8",
                "width": "30px"
              },
              "data_create_time": {
                "control": "dateTimePicker",
                "label": "登记日期",
                "span": "8",
                "width": "30px",
                "format":"yyyyMMdd",
                "disabled": true
              },
              "data_update_time": {
                "control": "dateTimePicker",
                "label": "更新日期",
                "span": "8",
                "width": "30px",
                "format":"yyyyMMdd",
                "disabled": true
              }
            }
          ]
        }
      ]
    }
  }
}
