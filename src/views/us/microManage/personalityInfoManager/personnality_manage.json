{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "evaluator": {
            "control": "text",
            "span": "8",
            "align": "left",
            "width": "25px",
            "label": "评价人",
            "rules": [
              {
                "max": 30,
                "message": "长度不能超过30个字符"
              }
            ]
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "target": "dialog",
          "label": "新增",
          "toRequest": {
            "method": "GET",
            "url": "/views/us/microManage/operatorsInfoManager//personality_add.json",
            "params": {
              "cust_no": "${{params.cust_no}}"
            }
          },
          "props": {
            "width": "90%",
            "title": "评价信息新增"
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "us0510",
          "cust_no": "${{params.cust_no}}"
        }
      },
      "thead": [
        {
          "type": "index",
          "label": "序号"
        },
        {
          "label": "评价人",
          "prop": "evaluator",
          "align": "center"
        },
        {
          "label": "是否有负面评价",
          "prop": "is_negative_com",
          "align": "center",
          "dict": {
            "dictType": "MS_E_YESORNO",
            "dictKey": [
              "MS_E_YESORNO"
            ],
            "labelField": "dictName",
            "valueField": "dictId"
          }
        },
        {
          "label": "有误参与非法集资",
          "prop": "is_illegal_fundraising",
          "align": "center",
          "dict": {
            "dictType": "MS_E_YESORNO",
            "dictKey": [
              "MS_E_YESORNO"
            ],
            "labelField": "dictName",
            "valueField": "dictId"
          }
        },
        {
          "label": "有无犯罪记录",
          "prop": "is_criminal_record",
          "align": "center",
          "dict": {
            "dictType": "MS_E_YESORNO",
            "dictKey": [
              "MS_E_YESORNO"
            ],
            "labelField": "dictName",
            "valueField": "dictId"
          }
        },
        {
          "label": "是否吸毒",
          "prop": "is_drug",
          "align": "center",
          "dict": {
            "dictType": "MS_E_YESORNO",
            "dictKey": [
              "MS_E_YESORNO"
            ],
            "labelField": "dictName",
            "valueField": "dictId"
          }
        },
        {
          "label": "是否涉赌、涉黄",
          "prop": "is_illegal",
          "align": "center",
          "dict": {
            "dictType": "MS_E_YESORNO",
            "dictKey": [
              "MS_E_YESORNO"
            ],
            "labelField": "dictName",
            "valueField": "dictId"
          }
        },
        {
          "label": "操作",
          "width": "210px",
          "fixed": "right",
          "align": "center",
          "btns": [
            {
              "type": "custom",
              "label": "详请",
              "toRequest": {
                "url": "/views/us/microManage/operatorsInfoManager//personality_info.json",
                "params": {
                  "cust_no": "${{params.cust_no}}"
                },
                "props": {
                  "width": "95%",
                  "height": "590px",
                  "title": "评价信息"
                }
              }
            },
            {
              "type": "edit",
              "label": "维护",
              "toRequest": {
                "url": "/views/us/microManage/operatorsInfoManager//personality_modify.json",
                "params": {
                  "cust_no": "${{params.cust_no}}"
                },
                "props": {
                  "width": "95%",
                  "height": "590px",
                  "title": "评价信息"
                }
              }
            },
            {
              "type": "custom",
              "label": "删除",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "us0513"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
