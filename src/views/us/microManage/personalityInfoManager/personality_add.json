{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "btns": [
        {
          "type": "submit",
          "label": "保存",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "us0511",
              "cust_no": "${{params.cust_no}}",
              "register_branch_id":"$g{{user_info.branch_id}}"
            }
          }
        }
      ],
      "controlsGroup": [
        {
          "title": "人品信息",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "accordion": false,
          "expand": true,
          "controls": [
            {
              "evaluator": {
                "control": "text",
                "label": "评价人",
                "span": "8",
                "width": "30px",
                "rules": [
                  {
                    "required": true,
                    "message": "请填写"
                  }
                ]
              },
              "is_negative_com": {
                "control": "select",
                "span": "8",
                "width": "30px",
                "label": "是否有负面评价",
                "dict": {
                  "dictType": "MS_E_YESORNO",
                  "dictKey": [
                    "MS_E_YESORNO"
                  ],
                  "format": "label"
                }
              },
              "is_illegal_fundraising": {
                "control": "select",
                "span": "8",
                "width": "30px",
                "label": "有无参与非法集资",
                "dict": {
                  "dictType": "MS_E_YESORNO",
                  "dictKey": [
                    "MS_E_YESORNO"
                  ],
                  "format": "label"
                }
              },
              "is_criminal_record": {
                "control": "select",
                "span": "8",
                "width": "30px",
                "label": "有无犯罪记录",
                "dict": {
                  "dictType": "MS_E_YESORNO",
                  "dictKey": [
                    "MS_E_YESORNO"
                  ],
                  "format": "label"
                }
              },
              "is_drug": {
                "control": "select",
                "span": "8",
                "width": "30px",
                "label": "是否吸毒",
                "dict": {
                  "dictType": "MS_E_YESORNO",
                  "dictKey": [
                    "MS_E_YESORNO"
                  ],
                  "format": "label"
                }
              },
              "is_illegal": {
                "control": "select",
                "span": "8",
                "width": "30px",
                "label": "是否涉赌、涉黄",
                "dict": {
                  "dictType": "MS_E_YESORNO",
                  "dictKey": [
                    "MS_E_YESORNO"
                  ],
                  "format": "label"
                }
              },
              "social_reputation": {
                "control": "select",
                "span": "8",
                "width": "30px",
                "label": "社会声誉",
                "dict": {
                  "dictType": "E_SOCIAL_REPUTATION",
                  "dictKey": [
                    "E_SOCIAL_REPUTATION"
                  ],
                  "format": "label"
                }
              }
            },
            {
              "evaluate": {
                "control": "checkbox",
                "span": "24",
                "width": "30px",
                "align": "left",
                "label": "风评",
                "dict": {
                  "dictType": "US_E_EVALUATE",
                  "dictKey": [
                    "US_E_EVALUATE"
                  ],
                  "format": "label"
                }
              }
            },
            {
              "bad_habit": {
                "control": "checkbox",
                "span": "24",
                "width": "30px",
                "align": "left",
                "label": "不良嗜好",
                "dict": {
                  "dictType": "US_E_BAD_HABIT",
                  "dictKey": [
                    "US_E_BAD_HABIT"
                  ],
                  "format": "label"
                },
                "rules": [
                  {
                    "required": true,
                    "message": "请勾选"
                  }
                ]
              }
            },
            {
              "major_accident": {
                "control": "checkbox",
                "span": "24",
                "width": "30px",
                "align": "left",
                "label": "重大变故",
                "dict": {
                  "dictType": "US_E_MAJOR_ACCIDENT",
                  "dictKey": [
                    "US_E_MAJOR_ACCIDENT"
                  ],
                  "format": "label"
                },
                "rules": [
                  {
                    "required": true,
                    "message": "请勾选"
                  }
                ]
              }
            },
            {
              "is_will_work": {
                "control": "select",
                "span": "8",
                "width": "30px",
                "label": "是否有劳动意愿",
                "dict": {
                  "dictType": "MS_E_YESORNO",
                  "dictKey": [
                    "MS_E_YESORNO"
                  ],
                  "format": "label"
                },
                "rules": [
                  {
                    "required": true,
                    "message": "请选择"
                  }
                ]
              },
              "is_can_work": {
                "control": "select",
                "span": "8",
                "width": "30px",
                "label": "是否有劳动能力",
                "dict": {
                  "dictType": "MS_E_YESORNO",
                  "dictKey": [
                    "MS_E_YESORNO"
                  ],
                  "format": "label"
                },
                "rules": [
                  {
                    "required": true,
                    "message": "请选择"
                  }
                ]
              },
              "negative_description": {
                "control": "textarea",
                "span": "24",
                "rows": "4",
                "width": "30px",
                "label": "负面评价说明"
              },
              "other_negative_info": {
                "control": "textarea",
                "span": "24",
                "rows": "4",
                "width": "30px",
                "label": "其他负面信息"
              },
              "remark": {
                "control": "textarea",
                "span": "24",
                "rows": "4",
                "width": "30px",
                "label": "备注"
              }
            }
          ]
        }
      ]
    }
  }
}
