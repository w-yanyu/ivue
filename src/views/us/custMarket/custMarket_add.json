{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "us0815"
        }
      },
      "controls": [
        {
          "cust_no": {
            "control": "hidden",
            "label": "客户序号",
            "disabled": true
          },
          "cust_name": {
            "label": "客户名称",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入客户名称"
              }
            ]
          },
          "phone_no": {
            "label": "联系电话",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入联系电话"
              }
            ]
          },
          "sex_tp": {
            "label": "性别",
            "control": "select",
            "dict": {
              "dictType": "MS_E_GENDER",
              "dictKey": "MS_E_GENDER"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择性别"
              }
            ]
          },
          "reg_date": {
            "label": "登记日期",
            "control": "dateTimePicker",
            "editable": false,
            "format": "yyyyMMdd",
            "valueFormat": "yyyyMMdd",
            "value": "today",
            "rules": [
              {
                "required": true,
                "message": "请输入登记日期"
              }
            ]
          },
          "cert_type": {
            "label": "证件类型",
            "control": "select",
            "dict": {
              "dictType": "MS_E_CRET_TYPE",
              "dictKey": "MS_E_CRET_TYPE"
            },
            "rules": [
              {
                "required": true,
                "message": "请输入证件号码"
              }
            ]
            
          },
          "cert_no": {
            "label": "证件号码",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入证件号码"
              }
            ]
          },
          "contact_type": {
            "label": "咨询方式",
            "control": "select",
            "dict": {
              "dictType": "US_E_CONTACT_TYPE",
              "dictKey": "US_E_CONTACT_TYPE"
            }
          },
          "is_corp_legalperson": {
            "label": "是否企业法人",
            "control": "select",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": "MS_E_YESORNO"
            },
            "showItem": {
              "Y": [
                "busi_name",
                "corp_type"
              ]
            }
          },
          "busi_name": {
            "label": "企业名称",
            "initShow": false,
            "control": "text",
            "rules": [
              {
                "required": false,
                "message": "请输入企业名称"
              }
            ]
          },
          "corp_type": {
            "label": "企业组织类型",
            "initShow": false,
            "control": "select",
            "dict": {
              "dictType": "US_E_CORP_TYPE",
              "dictKey": "US_E_CORP_TYPE"
            }
          }
        },
        {
          "msg_source": {
            "control": "checkbox",
            "span": "24",
            "label": "信息来源",
            "rules": [
              {
                "required": true,
                "message": "信息来源不能为空"
              }
            ],
            "showItem": {
              "10": [
                "msg_other_source"
              ]
            },
            "dict": {
              "dictType": "US_E_DATA_SOURCE",
              "dictKey": "US_E_DATA_SOURCE"
            }
          }
        },
        {
          "msg_other_source": {
            "label": "信息其他来源",
            "control": "text",
            "initShow": false,
            "rules": [
              {
                "required": false,
                "message": "请输入信息其他来源"
              }
            ]
          }
        },
        {
          "industry": {
            "label": "客户从事行业",
            "control": "select",
            "dict": {
              "dictType": "US_E_INDSTRY",
              "dictKey": "US_E_INDSTRY"
            }
          }
        },
        {
          "is_operation": {
            "label": "是否有经营场所",
            "control": "select",
            "showItem": {
              "1": [
                "operation_ctiy"
              ]
            },
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": "MS_E_YESORNO"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择是否有经营场所"
              }
            ]
          },
          "operation_ctiy": {
            "control": "areaCascade",
            "label": "经营所在区域",
            "initShow": false,
            "editable": false,
            "rules": [
              {
                "required": false,
                "message": "新行政区划(征)不能为空"
              },
              {
                "max": 80,
                "message": "长度不能超过80个字符"
              }
            ]
          }
        },
        {
          "apply_amt": {
            "label": "申请贷款金额",
            "control": "text",
            "rules": [
              {
                "required": false,
                "message": "请输入申请贷款金额"
              }
            ]
          },
          "apply_deadline": {
            "label": "申请贷款期限(月)",
            "control": "text",
            "rules": [
              {
                "required": false,
                "message": "请输入申请贷款期限(月)："
              }
            ]
          },
          "loan_purpose_type": {
            "label": "贷款用途类型",
            "control": "select",
            "dict": {
              "dictType": "US_E_LOAN_PURPOSE_TYPE",
              "dictKey": "US_E_LOAN_PURPOSE_TYPE"
            }
          },
          "loan_purpose": {
            "label": "具体用途",
            "control": "text",
            "rules": [
              {
                "required": false,
                "message": "请输入具体用途"
              }
            ]
          }
        },
        {
          "is_reject": {
            "label": "是否被拒绝",
            "control": "select",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": "MS_E_YESORNO"
            },
            "showItem": {
              "Y": [
                "reject_reason"
              ]
            },
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          },
          "reject_reason": {
            "label": "被拒绝的原因",
            "control": "text",
            "initShow": false,
            "rules": [
              {
                "required": false,
                "message": "请输入被拒绝的原因"
              }
            ]
          }
        },
        {
          "status": {
            "label": "状态",
            "control": "select",
            "dict": {
              "dictType": "US_E_STATE",
              "dictKey": "US_E_STATE"
            },
            "rules": [
              {
                "required": true,
                "message": "请输入状态"
              }
            ]
          },
          "remark": {
            "control": "text",
            "label": "备注",
            "span": "24",
            "rules": [
              {
                "max": 100,
                "message": "长度不能超过100个字符"
              }
            ]
          }
        }
      ]
    }
  }
}
