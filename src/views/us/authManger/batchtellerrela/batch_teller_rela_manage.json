{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "batch_no": {
            "label": "批次号",
            "control": "text",
            "value": "${{params.batch_no}}"
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "target": "dialog",
          "label": "新增选择用户",
          "toRequest": {
            "method": "GET",
            "url": "/views/us/authManger/batchtellerrela/batch_teller_rela_select.json",
            "params": {
              "batch_no": "${{params.batch_no}}"
            }
          },
          "props": {
            "width": "60%",
            "title": "选择用户"
          }
        },
        {
          "type": "delete",
          "target": "dialog",
          "label": "删除所选用户",
          "doRequest": {
            "url": "/SUMP/call/RPCCall",
            "method": "POST",
            "params": {
              "servicecode": "us1311",
              "selectName": "batchTellerRelaList"
            }
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true ,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "us1312",
          "batch_no": "${{params.batch_no}}"
        }
      },
      "thead": [
        {
          "type": "selection"
        },
        {
          "label": "分配批次号",
          "prop": "batch_no",
          "align": "center"
        },
        {
          "label": "分配用户名称",
          "prop": "user_name",
          "align": "center"
        },
        {
          "label": "分配用户编号",
          "prop": "trxn_teller",
          "align": "center"
        },
        {
          "label": "机构号",
          "prop": "branch_id",
          "align": "center"
        }
      ]
    }
  }
}