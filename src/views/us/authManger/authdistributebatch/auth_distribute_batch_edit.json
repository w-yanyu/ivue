{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "us1300",
          "batch_no": "${{params.batch_no}}",
          "appt": " $g{{user_info.userName}}"
         
        }
      },
      "controls": [
        {
          "batch_no": {
            "label": "批次号",
            "control": "text",
            "disabled": true
          },
          "appt": {
            "label": "申请用户",
            "control": "text",
            "disabled": true,
            "value": "$g{{user_info.userName}}"

          }, 

          "trxn_teller": {
            "label": "用户编号",
            "control": "text",
            "disabled": true,
            "value": "$g{{user_info.userCode}}"

          },
          "branch_id": {
            "label": "机构号",
            "control": "text",
            "disabled": true,
            "value": "$g{{user_info.branch_id}}"

          }
        }
      ]
    }
  }
}