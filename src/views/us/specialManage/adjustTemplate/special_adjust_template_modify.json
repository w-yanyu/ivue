{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "us0431"
        }
      },
      "controls": [
        {
          "temp_no": {
            "label": "组合编号",
            "control": "hidden",
            "value": "${{params.temp_no}}",
            "rules": [{
              "required": true,
              "message": "组合编号为空"
            }]
          },
          "temp_name": {
            "label": "组合名称",
            "control": "text",
            "rules": [{
              "required": true,
              "message": "请输入组合名称"
            }]
          },
          "adj_type": {
            "label": "调整类型",
            "control": "select",
            "dict": {
              "dictType": "US_E_ADJ_TYPE",
              "dictKey": "US_E_ADJ_TYPE"
            },
            "rules": [{
              "required": true,
              "message": "请输入调整类型"
            }]
          },
          "apply_cust_type": {
            "label": "适用客户类型",
            "control": "select",
            "dict": {
              "dictType": "US_E_APPL_CUST_TYPE",
              "dictKey": "US_E_APPL_CUST_TYPE"
            },
            "rules": [{
              "required": true,
              "message": "请输入适用客户类型"
            }]
          },
          "appr_auth": {
            "label": "审批权限",
            "control": "select",
            "dict": {
              "dictType": "US_E_APPR_AUTH",
              "dictKey": "US_E_APPR_AUTH"
            },
            "rules": [{
              "required": true,
              "message": "请输入审批权限"
            }]
          },
          "table_name": {
            "label": "表名",
            "control": "select",
            "dict": {
              "dictType": "US_E_TABLE_NAME",
              "dictKey": "US_E_TABLE_NAME"
            },
            "rules": [
              {
                "required": true,
                "message": "请输入表名"
              }
            ]
          }
        }
      ]
    }
  }
}
