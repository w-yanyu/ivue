{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "btns": [],
      "controls": [
        {
          "temp_no": {
            "label": "组合编号",
            "control": "string",
            "rules": [
              {
                "required": false,
                "message": "请输入组合编号"
              }
            ]
          },
          "temp_name": {
            "label": "组合名称",
            "control": "string",
            "rules": [
              {
                "required": false,
                "message": "请输入组合名称"
              }
            ]
          },
          "adj_type": {
            "label": "调整类型",
            "control": "select",
            "disabled": true,
            "dict": {
              "dictType": "US_E_ADJ_TYPE",
              "dictKey": "US_E_ADJ_TYPE"
            }
          },
          "apply_cust_type": {
            "label": "适用客户类型",
            "control": "select",
            "disabled": true,
            "dict": {
              "dictType": "US_E_APPL_CUST_TYPE",
              "dictKey": "US_E_APPL_CUST_TYPE"
            }
          },
          "appr_auth": {
            "label": "审批权限",
            "control": "select",
            "disabled": true,
            "dict": {
              "dictType": "US_E_APPR_AUTH",
              "dictKey": "US_E_APPR_AUTH"
            }
          },
          "table_name": {
            "label": "表名",
            "control": "select",
            "disabled": true,
            "dict": {
              "dictType": "US_E_TABLE_NAME",
              "dictKey": "US_E_TABLE_NAME"
            }
          }
        }
      ]
    }
