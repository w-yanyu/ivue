{
  "module": "v-search-btn-datagrid",
  "layout": {
    "form": {
      "scope": "formData",
      "initRequest": "${{params}}",
      "controls": [
        {
          "temp_no": {
            "label": "组合编号",
            "control": "text",
            "rules": [
              {
                "required": false,
                "message": "请输入组合编号"
              }
            ]
          },
          "temp_name": {
            "label": "组合名称",
            "control": "text",
            "rules": [
              {
                "required": false,
                "message": "请输入组合名称"
              }
            ]
          },
          "adj_type": {
            "label": "调整类型",
            "control": "select",
            "dict": {
              "dictType": "US_E_ADJ_TYPE",
              "dictKey": "US_E_ADJ_TYPE"
            }
          },
          "apply_cust_type": {
            "label": "适用客户类型",
            "control": "select",
            "dict": {
              "dictType": "US_E_APPL_CUST_TYPE",
              "dictKey": "US_E_APPL_CUST_TYPE"
            }
          },
          "appr_auth": {
            "label": "审批权限",
            "control": "select",
            "dict": {
              "dictType": "US_E_APPR_AUTH",
              "dictKey": "US_E_APPR_AUTH"
            }
          },
          "table_name": {
            "label": "表名",
            "control": "select",
            "rules": [
              {
                "required": false,
                "message": "请输入表名"
              }
            ],
            "dict": {
              "dictType": "US_E_TABLE_NAME",
              "dictKey": [
                "US_E_TABLE_NAME"
              ]
            }
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "target": "dialog",
          "label": "新增",
          "toRequest": {
            "method": "GET",
            "url": "/views/us/specialManage/adjustTemplate/adjustTemplateDetail/special_adjust_template_detail_add.json",
            "params": {
              "temp_no": "${{params.temp_no}}"
            }
          },
          "props": {
            "title": "特殊项调整组合明细新增"
          }
        }
      ]
    },
    "datagrid": {
      "module": "composer",
      "isInitSearch": true,
      "scope": "searchTable",
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "us0413",
          "temp_no": "${{params.temp_no}}"
        }
      },
      "thead": [
        {
          "label": "序号",
          "type": "index",
          "align": "center",
          "prop": "sortno"
        },
        {
          "label": "组合编号",
          "prop": "temp_no",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "顺序号",
          "prop": "order_seq",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "字段编号",
          "prop": "field_no",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "字段",
          "prop": "field_name",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "字段描述",
          "prop": "field_desc",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "主键标识",
          "prop": "key_flash",
          "align": "center",
          "type": "leaf",
          "dict": {
            "dictType": "MS_E_YESORNO",
            "dictKey": [
              "MS_E_YESORNO"
            ],
            "format": "value-label"
          }
        },
        {
          "label": "字段提示",
          "prop": "field_msg",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "表单类型",
          "prop": "form_type",
          "align": "center",
          "type": "leaf",
          "dict": {
            "dictType": "US_E_FORM_TYPE",
            "dictKey": [
              "US_E_FORM_TYPE"
            ],
            "format": "value-label"
          }
        },
        {
          "label": "枚举类型",
          "prop": "enum_type",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "数据类型",
          "prop": "data_type",
          "align": "center",
          "type": "leaf",
          "dict": {
            "dictType": "US_E_DATA_TYPE",
            "dictKey": [
              "US_E_DATA_TYPE"
            ],
            "format": "value-label"
          }
        },
        {
          "label": "操作",
          "btns": [
            {
              "type": "edit",
              "label": "维护",
              "toRequest": {
                "url": "/views/us/specialManage/adjustTemplate/adjustTemplateDetail/special_adjust_template_detail_modify.json",
                "method": "GET",
                "params": {}
              },
              "props": {
                "width": "65%",
                "title": "特殊项组合明细维护"
              }
            },
            {
              "type": "custom",
              "label": "删除",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "us0412",
                  "trantp": "DELETE"
                }
              },
              "confirm": {
                "title": "确定删除",
                "confirmButtonText": "确定",
                "cancelButtonText": "取消"
              }
            }
          ],
          "align": "center"
        }
      ]
    }
  }
}
