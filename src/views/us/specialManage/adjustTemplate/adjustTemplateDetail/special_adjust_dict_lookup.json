{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "controls": [{
                "field_no": {
                    "label": "字段编号",
                    "control": "text",
                    "rules": [{
                        "required": false,
                        "message": "请输入字段编号"
                    }]
                },
                "field_name": {
                    "label": "字段",
                    "control": "text",
                    "rules": [{
                        "required": false,
                        "message": "请输入字段"
                    }]
                },
                "field_desc": {
                    "label": "字段描述",
                    "control": "text",
                    "rules": [{
                        "required": false,
                        "message": "请输入字段描述"
                    }]
                }

            }]
        },
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": true,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "us0404"
                }
            },
            "thead": [
                {
                    "type": "index",
                    "label": "序号"
                },
                {
                    "label": "字段编号",
                    "prop": "field_no",
                    "align": "center",
                    "type": "leaf"
                }, {
                    "label": "字段",
                    "prop": "field_name",
                    "align": "center",
                    "type": "leaf"
                }, {
                    "label": "字段描述",
                    "prop": "field_desc",
                    "align": "center",
                    "type": "leaf"
                }, {
                    "label": "字段提示",
                    "prop": "field_msg",
                    "align": "center",
                    "type": "leaf"
                }, {
                    "label": "表单字段类型",
                    "prop": "form_type",
                    "align": "center",
                    "type": "leaf",
                    "dict": {
                        "dictType": "US_E_FORM_TYPE",
                        "dictKey": ["US_E_FORM_TYPE"],
                        "format": "value-label"
                    }
                }, {
                    "label": "枚举类型",
                    "prop": "enum_type",
                    "align": "center",
                    "type": "leaf"
                }, {
                    "label": "数据类型",
                    "prop": "data_type",
                    "align": "center",
                    "type": "leaf",
                    "dict": {
                        "dictType": "US_E_DATA_TYPE",
                        "dictKey": ["US_E_DATA_TYPE"],
                        "format": "value-label"
                    }
                },
                {
                    "label": "操作",
                    "align": "center",
                    "fixed": "right",
                    "btns": [{
                        "type": "bringback",
                        "label": "选择"
                    }]
                }
            ]
        }
    }
}
