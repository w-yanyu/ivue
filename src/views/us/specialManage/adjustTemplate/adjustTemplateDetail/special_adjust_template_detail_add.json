{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "us0411"
        }
      },
      "controls": [
        {
          "temp_no": {
            "label": "组合编号",
            "control": "text",
            "disabled": true,
            "value": "${{params.temp_no}}",
            "rules": [
              {
                "required": true,
                "message": "请输入组合编号"
              }
            ]
          },
          "order_seq": {
            "label": "顺序号",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入顺序号"
              }
            ]
          },
          "field_no": {
            "label": "字段编号",
            "control": "lookup",
            "edit": true,
            "keyField": {
              "field_no": "field_no",
              "field_name": "field_name",
              "field_desc": "field_desc",
              "field_msg": "field_msg",
              "form_type": "form_type",
              "enum_type": "enum_type",
              "data_type": "data_type"
            },
            "toRequest": {
              "url": "/views/us/specialManage/adjustTemplate/adjustTemplateDetail/special_adjust_dict_lookup.json",
              "method": "post",
              "params": {
              }
            },
            "props": {
              "title": "查询字段字典"
            },
            "format": "label",
            "rules": [
              {
                "required": true,
                "message": "请输入字段编号"
              }
            ]
          },
          "field_name": {
            "label": "字段",
            "control": "text",
            "disabled": true,
            "rules": [
              {
                "required": true,
                "message": "请输入字段"
              }
            ]
          },
          "key_flash": {
            "label": "主键标识",
            "control": "select",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": "MS_E_YESORNO"
            },
            "rules": [
              {
                "required": true,
                "message": "请输入主键标识"
              }
            ]
          },
          "field_desc": {
            "label": "字段描述",
            "control": "text",
            "disabled": true,
            "rules": [
              {
                "required": true,
                "message": "请输入字段描述"
              }
            ]
          },
          "field_msg": {
            "label": "字段提示",
            "control": "text",
            "disabled": true,
            "rules": [
              {
                "required": true,
                "message": "请输入字段提示"
              }
            ]
          },
          "form_type": {
            "label": "表单字段类型",
            "disabled": true,
            "control": "select",
            "dict": {
              "dictType": "US_E_FORM_TYPE",
              "dictKey": "US_E_FORM_TYPE"
            },
            "rules": [
              {
                "required": true,
                "message": "请输入字段提示"
              }
            ]
          },
          "enum_type": {
            "label": "枚举类型",
            "control": "text",
            "disabled": true,
            "rules": [
              {
                "required": false,
                "message": "请输入枚举类型"
              }
            ]
          },
          "data_type": {
            "label": "数据类型",
            "disabled": true,
            "control": "select",
            "dict": {
              "dictType": "US_E_DATA_TYPE",
              "dictKey": "US_E_DATA_TYPE"
            },
            "rules": [
              {
                "required": true,
                "message": "请输入字段提示"
              }
            ]
          }
        }
      ]
    }
  }
}
