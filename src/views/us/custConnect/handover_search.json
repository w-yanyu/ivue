{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "scope": "formData",
      "initRequest": "${{params}}",
      "btns": [],
      "controls": [
        {
          "new_usr_nm": {
            "label": "接收用户名称",
            "control": "lookup",
            "edit": true,
            "width": "130px",
            "keyField": {
              "new_usr_nm": "user_name",
              "new_usr_num": "user_id",
              "new_org_num": "branch_id"
            },
            "toRequest": {
              "url": "/views/us/customerHandoverManage/handover_list.json",
              "method": "post",
              "params":{
                "branch_id":"${{formData.branch_id}}"
              }
            },
            "props": {
              "title": "选择接受客户经理",
              "width": "65%"
            },
            "format": "label"
          },
          "new_org_num": {
            "control": "text",
            "label": "机构编号",
            "width": "130px",
            "disabled": true
          } 
        },
        {
          "new_usr_num": {
            "control": "hidden",
            "label": "接受用户名称",
            "width": "130px"
          },
          "branch_id": {
            "control": "hidden",
            "label": "当前登录者所属机构",
            "width": "130px"
          }
        },
        {
          "cust_no": {
            "control": "hidden",
            "label": "当前登录用户",
            "width": "130px"
          }
        }
      ]
    }
  }
}

