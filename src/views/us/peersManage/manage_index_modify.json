                        {
                            "years": {
                                "control": "dateTimePicker",
                                "disabled":true,
                                "format": "yyyy",
                                "type":"year",
                                "label": "年份",
                                "span": 8,
                                "rules": [
                                    {
                                      "required": true,
                                      "message": "请选择年份"
                                    }
                                ]
                            }
                        }
                    ]
                },
