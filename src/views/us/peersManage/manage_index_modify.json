    "layout": {
        "form": {
            "initRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "us0954",
                  "cust_no": "${{params.cust_no}}",
                  "years":"${{params.years}}"
                }
              },
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "us0955",
                  "cust_no": "${{params.cust_no}}"
