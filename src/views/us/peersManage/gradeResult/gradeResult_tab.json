{
  "module": "dynamicTabs",
  "layout": {
    "tabs": {
      "tabHeader": [
        {
          "id": "tab1",
          "label": "外部评级信息",
          "closable": false,
          "disabled": false,
          "icon": "el-icon-date",
          "toRequest": {
            "url": "/views/us/peersManage/gradeResult/gradeResult_manage.json",
            "params": {
              "show_flash":"${{params.show_flash}}",
              "cust_no": "${{params.cust_no}}"
             },
            "method": "POST"
          }
        },
        {
          "id": "tab2",
          "label": "监管评级信息",
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "toRequest": {
            "url": "/views/us/peersManage/gradeResult/supervisResult_manage.json",
            "params": {
              "show_flash":"${{params.show_flash}}",
              "cust_no": "${{params.cust_no}}"
             },
            "method": "POST"
          }
        }
      ],
      "defaultActiveKey": "tab1",
      "duty_type": "top",
      "styleClass": "card"
    }
  }
}
