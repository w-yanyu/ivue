{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "btns": [
        {
          "type": "submit",
          "label": "保存",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "us1001",
              "cust_no": "${{params.cust_no}}"
            }
          }
        }
      ],
      "controls": [
        {
          "grade_date": {
            "control": "dateTimePicker",
            "label": "评级日期",
            "span": "12",
            "width": "25px",
            "format": "yyyyMMdd",
            "rules": [
              {
                "required": true,
                "message": "评级日期不能为空"
              }
            ]
          },
          "grade_branch_no": {
            "control": "select",
            "span": "12",
            "width": "25px",
            "label": "评级机构",
            "options": [
              {
                "label": "中诚信",
                "value": "中诚信"
              },
              {
                "label": "标准普尔",
                "value": "标准普尔"
              },
              {
                "label": "穆迪投资",
                "value": "穆迪投资"
              },
              {
                "label": "惠誉国际",
                "value": "惠誉国际"
              },{
                "label":"内部测算打分",
                "value":"内部测算打分"
              }
            ],
            "rules": [
              {
                "required": true,
                "message": "评级机构不能为空"
              }
            ]
          },
          "grade_rank": {
            "control": "text",
            "span": "12",
            "width": "25px",
            "label": "评级等级",
            "rules": [
              {
                "required": true,
                "message": "评级等级不能为空"
              }
            ]
          },
          "grade_remark": {
            "control": "textarea",
            "span": "12",
            "width": "20px",
            "label": "评级详细说明",
            "rules": [
              {
                "required": true,
                "message": "发行日期不能为空"
              }
            ]
          }
        }
      ]
    }
  }
}
