{
    "module": "v_inputDataForm_btn",
    "layout": {
      "form": {
        "btns": [
          {
            "type": "submit",
            "label": "保存",
            "icon": "",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "us1004",
                "cust_no": "${{params.cust_no}}"
              }
            }
          }
        ],
        "controls": [
          {
            "input_date": {
              "control": "dateTimePicker",
              "label": "录入日期",
              "span": "12",
              "width": "25px",
              "format": "yyyyMMdd",
              "rules": [
                {
                  "required": true,
                  "message": "录入日期不能为空"
                }
              ]
            },
            "input_person": {
              "control": "text",
              "disabled":true,
              "span": "12",
              "width": "25px",
              "label": "录入人",
              "value":"$g{{user_info.userName}}"
            },
            "input_org": {
                "control": "text",
                "disabled":true,
                "span": "12",
                "width": "25px",
                "label": "录入机构",
                "value":"$g{{user_info.branch_name}}"
              },
              "supervis_approve_org": {
                "control": "text",
                "span": "12",
                "width": "25px",
                "label": "监管认证机构",
                "rules": [
                  {
                    "required": true,
                    "message": "监管认证机构不能为空"
                  }
                ]
              },
              "supervis_grade_result": {
                "control": "text",
                "span": "12",
                "width": "25px",
                "label": "监管评级结果",
                "rules": [
                  {
                    "required": true,
                    "message": "评监管评级结果不能为空"
                  }
                ]
              },
            "supervis_grade_remark": {
              "control": "textarea",
              "span": "12",
              "width": "20px",
              "label": "监管评级详细说明"
            }
          }
        ]
      }
    }
  }
  