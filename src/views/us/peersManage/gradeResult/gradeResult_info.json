{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "btns": [],
      "controls": [
        {
          "grade_date": {
            "control": "dateTimePicker",
            "disabled": true,
            "label": "评级日期",
            "span": "12",
            "width": "25px",
            "format": "yyyyMMdd",
            "rules": [
              {
                "required": true,
                "message": "评级日期不能为空"
              }
            ]
          },
          "grade_branch_no": {
            "control": "text",
            "span": "12",
            "disabled": true,
            "width": "25px",
            "label": "评级机构",
            "rules": [
              {
                "required": true,
                "message": "评级机构不能为空"
              }
            ]
          },
          "grade_rank": {
            "control": "text",
            "span": "12",
            "width": "25px",
            "disabled": true,
            "label": "评级等级",
            "rules": [
              {
                "required": true,
                "message": "评级等级不能为空"
              }
            ]
          },
          "grade_remark": {
            "control": "textarea",
            "span": "12",
            "width": "20px",
            "disabled": true,
            "label": "评级详细说明",
            "rules": [
              {
                "required": true,
                "message": "发行日期不能为空"
              }
            ]
          }
        }
      ]
    }
  }
}
