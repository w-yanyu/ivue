{
    "module": "v_inputDataForm_btn",
    "layout": {
      "form": {
        "initRequest": "${{params}}",
        "btns": [],
        "controls": [
          {
            "input_date": {
              "control": "dateTimePicker",
              "disabled":true,
              "label": "录入日期",
              "span": "12",
              "width": "25px",
              "format": "yyyyMMdd"
            },
            "input_person": {
              "control": "text",
              "disabled":true,
              "span": "12",
              "width": "25px",
              "label": "录入人",
              "value":"$g{{user_info.userName}}"
            },
            "input_org": {
                "control": "text",
                "disabled":true,
                "span": "12",
                "width": "25px",
                "label": "录入机构",
                "value":"$g{{user_info.branch_name}}"
              },
              "supervis_approve_org": {
                "control": "text",
                "disabled":true,
                "span": "12",
                "width": "25px",
                "label": "监管认证机构"
              },
              "supervis_grade_result": {
                "control": "text",
                "disabled":true,
                "span": "12",
                "width": "25px",
                "label": "监管评级结果"
              },
            "supervis_grade_remark": {
              "disabled":true,
              "control": "textarea",
              "span": "12",
              "width": "20px",
              "label": "监管评级详细说明"
            }
          }
        ]
      }
    }
  }
  