{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "btns": [],
      "controls": [
        {
          "issue_date": {
            "control": "dateTimePicker",
            "label": "发行日期",
            "span": "8",
            "width": "25px",
            "disabled": true,
            "format": "yyyyMMdd",
            "rules": [
              {
                "required": true,
                "message": "发行日期不能为空"
              }
            ]
          },
          "bond_type": {
            "control": "select",
            "label": "债券类型",
            "span": "8",
            "width": "25px",
            "disabled": true,
            "dict": {
              "dictType": "E_BOND_TYPE",
              "dictKey": [
                "E_BOND_TYPE"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "发行日期不能为空"
              }
            ]
          },
          "ccy_code": {
            "control": "select",
            "span": "8",
            "width": "25px",
            "label": "发行币种",
            "disabled": true,
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": [
                "MS_CURRENCY"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "发行币种不能为空"
              }
            ]
          },
          "issue_amount": {
            "control": "currency",
            "span": "8",
            "width": "25px",
            "label": "发行金额",
            "disabled": true,
            "decimal": "2",
            "thousand": ",",
            "rules": [
              {
                "required": true,
                "message": "发行金额不能为空"
              }
            ]
          },
          "interest_rate": {
            "control": "currency",
            "span": "8",
            "width": "25px",
            "decimal": "2",
            "disabled": true,
            "thousand": ",",
            "label": "利率",
            "rules": [
              {
                "required": true,
                "message": "发行日期不能为空"
              }
            ]
          },
          "bond_term": {
            "control": "text",
            "span": "8",
            "width": "25px",
            "suffix": "年",
            "label": "债券期限",
            "disabled": true,
            "rules": [
              {
                "required": true,
                "message": "债券期限不能为空"
              },
              {
                "type": "number",
                "message": "请输入正确整数"
              }
            ]
          },
          "is_listed": {
            "control": "select",
            "span": "8",
            "width": "25px",
            "disabled": true,
            "label": "是否上市",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "发行日期不能为空"
              }
            ]
          },
          "remark": {
            "control": "textarea",
            "span": "8",
            "disabled": true,
            "width": "25px",
            "label": "备注"
          }
        }
      ]
    }
  }
}
