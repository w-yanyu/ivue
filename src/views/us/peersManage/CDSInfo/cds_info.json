{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "btns": [],
      "controls": [
        {
          "cds_belong_month": {
            "control": "dateTimePicker",
            "label": "CDS归属月",
            "disabled":true,
            "type":"month",
            "span": "12",
            "width": "25px",
            "format": "yyyyMM"
          },
          "cds_value": {
            "control": "currency",
            "span": "12",
            "width": "25px",
            "label": "CDS值(万元)",
            "disabled":true
          },
          "cds_compare_value": {
            "control": "currency",
            "span": "12",
            "width": "25px",
            "label": "较上月变化值(万元)",
            "disabled":true
          },"input_date": {
            "control": "dateTimePicker",
            "label": "录入日期",
            "span": "12",
            "width": "25px",
            "format": "yyyyMMdd",
            "disabled":true
          },
          "input_person": {
            "control": "text",
            "disabled":true,
            "span": "12",
            "width": "25px",
            "label": "录入人",
            "value":"$g{{user_info.userName}}"
          },
          "input_org": {
              "control": "text",
              "disabled":true,
              "span": "12",
              "width": "25px",
              "label": "录入机构",
              "value":"$g{{user_info.branch_name}}"
            }

        }
      ]
    }
  }
}
