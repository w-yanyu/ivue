{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "btns": [
        {
          "type": "submit",
          "label": "保存",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "us1006",
              "cust_no": "${{params.cust_no}}"
            }
          }
        }
      ],
      "controls": [
        {
          "cds_belong_month": {
            "control": "dateTimePicker",
            "label": "CDS归属月",
            "type":"month",
            "span": "12",
            "width": "25px",
            "format": "yyyyMM",
            "rules": [
              {
                "required": true,
                "message": "CDS归属月不能为空"
              }
            ]
          },
          "cds_value": {
            "control": "currency",
            "span": "12",
            "width": "25px",
            "label": "CDS值(万元)",
            "rules": [
              {
                "required": true,
                "message": "CDS值(万元)不能为空"
              }
            ]
          },
          "cds_compare_value": {
            "control": "currency",
            "span": "12",
            "disabled":true,
            "width": "25px",
            "label": "较上月变化值(万元)"
          },"input_date": {
            "control": "dateTimePicker",
            "label": "录入日期",
            "span": "12",
            "width": "25px",
            "format": "yyyyMMdd",
            "rules": [
              {
                "required": true,
                "message": "录入日期不能为空"
              }
            ]
          },
          "input_person": {
            "control": "text",
            "disabled":true,
            "span": "12",
            "width": "25px",
            "label": "录入人",
            "value":"$g{{user_info.userName}}"
          },
          "input_org": {
              "control": "text",
              "disabled":true,
              "span": "12",
              "width": "25px",
              "label": "录入机构",
              "value":"$g{{user_info.branch_name}}"
            }

        }
      ]
    }
  }
}
