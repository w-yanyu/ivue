{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "us1112"
        }
      },
      "controls": [
        {
          "doc_no": {
            "control": "string",
            "label": "影像资料编号",
            "width": "130px"
          },
          "doc_name": {
            "control": "text",
            "label": "影像资料名称",
            "width": "130px",
            "rules": [
              {
                "min": 0,
                "max": 20,
                "message": "长度不能超过20个字符"
              },
              {
                "required": true
              }
            ]
          },
          "doc_path": {
            "control": "string",
            "label": "客户影像资料路径",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请上传客户影像资料"
              }
            ]
          },
          "fileList": {
            "control": "dynamic",
            "isShowChildForm": true,
            "span": 12,
            "toRequest": {
              "url": "@minioUploadButton",
              "method": "get",
              "params": {
                "file_path": "doc_path",
                "formKey": "fileList",
                "formLabel": "附件上传"
              }
            }
          }
        }
      ]
    }
  }
}
