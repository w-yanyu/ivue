{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "controls": [{
                "project_name": {
                    "label": "项目名称",
                    "control": "text",
                    "rules": [{
                        "max": 80,
                        "message": "长度不能超过80个字符"
                    }]
                }
            }],
            "foldLineNumber": 3
        },
        "toolbar": {
            "btns": [{
                "type": "add",
                "target": "dialog",
                "label": "新增",
                "toRequest": {
                    "method": "POST",
                    "url": "/views/us/entManager/EntCustEventManager/CustEventManager/corpProjectManager/corpProject_add.json",
                    "params": {
                        "cust_no": "${{params.cust_no}}"
                    }
                },
                "show": {
                    "expression": "'${{params.show_flash}}' == '1'"
                },
                "props": {
                    "width": "80%",
                    "title": "企业项目信息新增"
                }
            }]
        },
        "datagrid": {
            "isInitSearch": true,
            "scope": "searchTable",
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "us0340",
                    "cust_no": "${{params.cust_no}}"
                }
            },
            "thead": [{
                    "label": "序号",
                    "type": "index",
                    "align": "center"
                },
                {
                    "label": "客户编号",
                    "prop": "cust_no",
                    "align": "center"
                },
                {
                    "label": "项目名称",
                    "prop": "project_name",
                    "align": "center"
                },
                {
                    "label": "项目进度",
                    "prop": "proj_progress",
                    "align": "center",
                    "dict": {
                        "dictType": "US_E_PROGRESS",
                        "dictKey": [
                            "US_E_PROGRESS"
                        ],
                        "labelField": "dictName",
                        "valueField": "dictId"
                    }
                },
                {
                    "label": "项目总投资额（万元）",
                    "prop": "proj_amt",
                    "align": "center"
                },
                {
                    "label": "操作",
                    "fixed": "right",
                    "width": 160,
                    "align": "center",
                    "btns": [{
                            "type": "look",
                            "label": "详情",
                            "toRequest": {
                                "url": "/views/us/entManager/EntCustEventManager/CustEventManager/corpProjectManager/corpProject_info.json",
                                "method": "POST"
                            },
                            "props": {
                                "width": "80%",
                                "title": "企业项目信息详情"
                            }
                        },
                        {
                            "type": "edit",
                            "label": "维护",
                            "toRequest": {
                                "url": "/views/us/entManager/EntCustEventManager/CustEventManager/corpProjectManager/corpProject_update.json",
                                "method": "POST"
                            },
                            "show": {
                                "expression": "'${{params.show_flash}}' == '1'"
                            },
                            "props": {
                                "width": "80%",
                                "title": "企业项目信息维护"
                            }
                        },
                        {
                            "type": "delete",
                            "label": "删除",
                            "doRequest": {
                                "url": "/SUMP/icmcall/icmRPCCall",
                                "method": "POST",
                                "params": {
                                    "servicecode": "us0343"

                                }
                            },
                            "show": {
                                "expression": "'${{params.show_flash}}' == '1'"
                            },
                            "confirm": {
                                "title": "确定删除",
                                "confirmButtonText": "确定",
                                "cancelButtonText": "取消"
                            }
                        }
                    ]
                }
            ],
            "pagination": {
                "show": true
            }
        }
    }
}
