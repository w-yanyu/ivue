{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "scope": "templateScope",
            "initDataOnce": true,
            "initRequest": "${{params}}",
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "us0342"

                }
            },
            "btns": [{
                "type": "submit",
                "label": "提交"
            }],
            "controls": [{
                "cust_no": {
                    "control": "hidden",
                    "label": "客户编号",
                    "width": "10px",
                    "disabled": true
                },
                "serial_no": {
                    "label": "企业项目信息流水号",
                    "control": "hidden",
                    "width": "10px",
                    "rules": [{
                        "message": "请输入企业项目信息流水号"
                    }]
                },
                "project_name": {
                    "label": "项目名称",
                    "control": "text",
                    "width": "10px",
                    "rules": [{
                        "required": true,
                        "message": "请输入项目名称"
                    }]
                },
                "proj_property": {
                    "label": "项目性质",
                    "control": "select",
                    "width": "10px",
                    "rules": [{
                        "required": true,
                        "message": "请选择项目性质"
                    }],
                    "dict": {
                        "dictType": "US_E_PROJ_NATURE",
                        "dictKey": [
                            "US_E_PROJ_NATURE"
                        ],
                        "format": "label"
                    }
                },
                "proj_type": {
                    "label": "项目种类",
                    "control": "select",
                    "width": "10px",
                    "rules": [{
                        "required": true,
                        "message": "请选择项目种类"
                    }],
                    "dict": {
                        "dictType": "US_E_PROJ_TYPE",
                        "dictKey": [
                            "US_E_PROJ_TYPE"
                        ],
                        "format": "label"
                    }
                },
                "project_no": {
                    "label": "项目编号",
                    "control": "text",
                    "width": "10px",
                    "rules": [{
                        "required": true,
                        "message": "请输入项目编号"
                    }]
                },
                "proj_progress": {
                    "label": "项目进度",
                    "control": "select",
                    "width": "10px",
                    "rules": [{
                        "required": true,
                        "message": "请选择项目进度"
                    }],
                    "dict": {
                        "dictType": "US_E_PROGRESS",
                        "dictKey": [
                            "US_E_PROGRESS"
                        ],
                        "format": "label"
                    }
                },
                "proj_amt": {
                    "label": "项目总投资额(万元)",
                    "control": "text",
                    "width": "10px",
                    "rules": [{
                        "required": true,
                        "pattern": "^[0-9]+(\\.[0-9]{1,2})?$",
                        "message": "请输入项目总投资额"
                    }]
                },
                "own_amt": {
                    "label": "自筹资金(万元)",
                    "control": "text",
                    "width": "10px",
                    "rules": [{
                        "required": true,
                        "pattern": "^[0-9]+(\\.[0-9]{1,2})?$",
                        "message": "请输入自筹资金"
                    }]
                },
                "own_loan_amt": {
                    "label": "本行贷款金额(万元)",
                    "control": "text",
                    "width": "10px",
                    "rules": [{
                        "required": true,
                        "pattern": "^[0-9]+(\\.[0-9]{1,2})?$",
                        "message": "请输入本行贷款金额"
                    }]
                },
                "other_loan_amt": {
                    "label": "他行贷款金额(万元)",
                    "control": "text",
                    "width": "10px",
                    "rules": [{
                        "required": true,
                        "pattern": "^[0-9]+(\\.[0-9]{1,2})?$",
                        "message": "请输入他行贷款金额"
                    }]
                },
                "project_liabilities": {
                    "label": "项目总负债(万元)",
                    "control": "text",
                    "width": "10px",
                    "rules": [{
                        "required": true,
                        "pattern": "^[0-9]+(\\.[0-9]{1,2})?$",
                        "message": "请输入项目总负债"
                    }]
                },
                "invest_payback_period": {
                    "label": "投资回收期(月)",
                    "control": "text",
                    "width": "10px",
                    "rules": [{
                        "required": true,
                        "max": 3,
                        "pattern": "^[0-9]*$",
                        "message": "请输入投资回收期"
                    }]
                },
                "invest_profit_rate": {
                    "label": "投资利润率",
                    "control": "text",
                    "width": "10px",
                    "suffix": "%",
                    "rules": [{
                        "required": true,
                        "max": 250,
                        "pattern": "^(100.00|[1-9]?\\d(\\.\\d\\d\\d?)?)$",
                        "message": "请输入(0-100)之间的比例"
                    }]
                },
                "loan_payback_period": {
                    "label": "贷款回收期(月)",
                    "control": "text",
                    "width": "10px",
                    "rules": [{
                        "max": 3,
                        "pattern": "^[0-9]*$",
                        "required": true,
                        "message": "请输入贷款回收期"
                    }]
                },
                "financial_internal_rate": {
                    "label": "财务内部收益率",
                    "control": "text",
                    "width": "10px",
                    "suffix": "%",
                    "rules": [{
                        "required": true,
                        "max": 250,
                        "pattern": "^(100.00|[1-9]?\\d(\\.\\d\\d\\d?)?)$",
                        "message": "请输入(0-100)之间的比例"
                    }]
                },
                "financial_net_present_value": {
                    "label": "财务净现值(万元)",
                    "control": "text",
                    "width": "10px",
                    "rules": [{
                        "pattern": "^[0-9]+(\\.[0-9]{1,2})?$",
                        "message": "请输入财务净现值"
                    }]
                },
                "repayment_amt": {
                    "label": "每年可还贷款金额(万元)",
                    "control": "text",
                    "width": "10px",
                    "rules": [{
                        "pattern": "^[0-9]+(\\.[0-9]{1,2})?$",
                        "message": "请输入每年可还贷款金额"
                    }]
                },
                "new_sales_revenue": {
                    "label": "新增销售收入(万元)",
                    "control": "text",
                    "width": "10px",
                    "rules": [{
                        "pattern": "^[0-9]+(\\.[0-9]{1,2})?$",
                        "message": "请输入新增销售收入"
                    }]
                },
                "new_after_tax_profits": {
                    "label": "新增税后利润(万元)",
                    "control": "text",
                    "width": "10px",
                    "rules": [{
                        "pattern": "^[0-9]+(\\.[0-9]{1,2})?$",
                        "message": "请输入新增税后利润"
                    }]
                },
                "new_profits_amt": {
                    "label": "新增利润总额(万元)",
                    "control": "text",
                    "width": "10px",
                    "rules": [{
                        "pattern": "^[0-9]+(\\.[0-9]{1,2})?$",
                        "message": "请输入新增利润总额"
                    }]
                },
                "new_depreciation": {
                    "label": "新增折旧",
                    "control": "text",
                    "suffix": "%",
                    "width": "10px",
                    "rules": [{
                        "required": true,
                        "pattern": "^(100.00|[1-9]?\\d(\\.\\d\\d\\d?)?)$",
                        "message": "请输入(0-100)之间的比例"
                    }]
                },
                "valuation_date": {
                    "control": "dateTimePicker",
                    "label": "评估日期",
                    "format": "yyyyMMdd",
                    "width": "10px",
                    "rules": [{
                        "message": "请选择评估日期"
                    }]
                },
                "pro_addr": {
                    "label": "项目地址",
                    "control": "textarea",
                    "width": "10px",
                    "rules": [{
                        "required": true,
                        "max": 250,
                        "message": "250字以内"
                    }]
                },
                "proj_leading_product": {
                    "label": "项目主导产品",
                    "control": "textarea",
                    "width": "10px",
                    "rules": [{
                        "required": true,
                        "max": 250,
                        "message": "250字以内"
                    }]
                },
                "proj_construction": {
                    "label": "项目在建规模",
                    "control": "textarea",
                    "width": "10px",
                    "rules": [{
                        "required": true,
                        "max": 250,
                        "message": "250字以内"
                    }]
                },
                "report": {
                    "label": "评估报告",
                    "control": "textarea",
                    "width": "10px",
                    "span": "24",
                    "autosize": {
                        "minRows": 4
                    },
                    "rules": [
                      {
                        "required": true,
                        "message": "请填写评估报告"
                      },
                      {
                        "max": "500",
                        "message": "长度不能超过500个字符"
                    }]
                },
                "remark": {
                    "label": "备注",
                    "control": "textarea",
                    "width": "10px",
                    "span": "24",
                    "autosize": {
                        "minRows": 4
                    },
                    "rules": [{
                        "max": "100",
                        "message": "长度不能超过100个字符"
                    }]
                }
            }]
        }
    }
}
