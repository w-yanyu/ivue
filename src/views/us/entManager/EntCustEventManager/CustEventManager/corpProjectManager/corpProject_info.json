{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initDataOnce": true,
            "initRequest": "${{params}}",
            "btns": [],
            "controls": [
                {
                    "cust_no": {
                        "label": "客户编号",
                        "control": "string",
                        "width": "10px"
                    },
                    "project_name": {
                        "label": "项目名称",
                        "control": "string",
                        "width": "10px"
                    },
                    "proj_property": {
                        "label": "项目性质",
                        "control": "string",
                        "width": "10px",
                        "dict": {
                            "dictType": "US_E_PROJ_NATURE",
                            "dictKey": [
                                "US_E_PROJ_NATURE"
                            ],
                            "format": "label"
                        }
                    },
                    "proj_type": {
                        "label": "项目种类",
                        "control": "string",
                        "width": "10px",
                        "dict": {
                            "dictType": "US_E_PROJ_TYPE",
                            "dictKey": [
                                "US_E_PROJ_TYPE"
                            ],
                            "format": "label"
                        }
                    },
                    "project_no": {
                        "label": "项目编号",
                        "control": "string",
                        "width": "10px"
                    },
                    "proj_progress": {
                        "label": "项目进度",
                        "control": "string",
                        "width": "10px",
                        "dict": {
                            "dictType": "US_E_PROGRESS",
                            "dictKey": [
                                "US_E_PROGRESS"
                            ],
                            "format": "label"
                        }
                    },
                    "proj_amt": {
                        "label": "项目总投资额(万元)",
                        "control": "string",
                        "width": "10px"
                    },
                    "own_amt": {
                        "label": "自筹资金(万元)",
                        "control": "string",
                        "width": "10px"
                    },
                    "own_loan_amt": {
                        "label": "本行贷款金额(万元)",
                        "control": "string",
                        "width": "10px"
                    },
                    "other_loan_amt": {
                        "label": "他行贷款金额(万元)",
                        "control": "string",
                        "width": "10px"
                    },
                    "project_liabilities": {
                        "label": "项目总负债(万元)",
                        "control": "string",
                        "width": "10px"
                    },
                    "invest_payback_period": {
                        "label": "投资回收期(月)",
                        "control": "string",
                        "width": "10px"
                    },
                    "invest_profit_rate": {
                        "label": "投资利润率",
                        "control": "text",
                        "suffix": "%",
                        "disabled": "true",
                        "width": "10px"
                    },
                    "loan_payback_period": {
                        "label": "贷款回收期(月)",
                        "control": "string",
                        "width": "10px"
                    },
                    "financial_internal_rate": {
                        "label": "财务内部收益率",
                        "control": "text",
                        "suffix": "%",
                        "disabled": "true",
                        "width": "10px"
                    },
                    "financial_net_present_value": {
                        "label": "财务净现值(万元)",
                        "control": "string",
                        "width": "10px"
                    },
                    "repayment_amt": {
                        "label": "每年可还贷款金额(万元)",
                        "control": "string",
                        "width": "10px"
                    },
                    "new_sales_revenue": {
                        "label": "新增销售收入(万元)",
                        "control": "string",
                        "width": "10px"
                    },
                    "new_after_tax_profits": {
                        "label": "新增税后利润(万元)",
                        "control": "string",
                        "width": "10px"
                    },
                    "new_profits_amt": {
                        "label": "新增利润总额(万元)",
                        "control": "string",
                        "width": "10px"
                    },
                    "new_depreciation": {
                        "label": "新增折旧",
                        "control": "text",
                        "suffix": "%",
                        "disabled": "true",
                        "width": "10px"
                    },
                    "valuation_date": {
                        "control": "string",
                        "label": "评估日期",
                        "format": "yyyyMMdd",
                        "width": "10px"
                    },
                    "pro_addr": {
                        "label": "项目地址",
                        "control": "string",
                        "width": "10px"
                    },
                    "proj_leading_product": {
                        "label": "项目主导产品",
                        "control": "string",
                        "width": "10px"
                    },
                    "proj_construction": {
                        "label": "项目在建规模",
                        "control": "string",
                        "width": "10px"
                    },
                    "report": {
                        "label": "评估报告",
                        "control": "textarea",
                        "width": "10px",
                        "span": "24",
                        "autosize": {
                            "minRows": 4
                        },
                        "disabled": "true"
                    },
                    "remark": {
                        "label": "备注",
                        "control": "textarea",
                        "width": "10px",
                        "span": "24",
                        "autosize": {
                            "minRows": 4
                        },
                        "disabled": "true"
                    }
                }
            ]
        }
    }
}
