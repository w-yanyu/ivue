{
  "module": "dynamicTabs",
  "layout": {
    "tabs": {
      "tabHeader": [
        {
          "id": "tab1",
          "label": "重大事件",
          "closable": false,
          "disabled": false,
          "icon": "el-icon-date",
          "toRequest": {
            "url": "/views/us/privateManage/majorEvent/custMajorEvent/custMajorEvent_manage.json",
            "params": {
              "show_flash":"${{params.show_flash}}"
             },
            "method": "POST"
          }
        },
        {
          "id": "tab2",
          "label": "在建重大项目信息",
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "toRequest": {
            "url": "/views/us/entManager/EntCustEventManager/CustEventManager/corpProjectManager/corpProject_list.json",
            "params": {
              "show_flash":"${{params.show_flash}}"
             },
            "method": "POST"
          }
        }
      ],
      "defaultActiveKey": "tab1",
      "duty_type": "top",
      "styleClass": "card"
    }
  }
}
