{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "btns": [{
                "type": "submit",
                "label": "提交",
                "icon": "",
                "doRequest": {
                    "url": "/SUMP/icmcall/icmRPCCall",
                    "method": "POST",
                    "params": {
                        "servicecode": "us0352"
                    }
                }
            }],
            "controls": [{
                "serial_no": {
                    "control": "string",
                    "width": "10px",
                    "label": "客户违规序号",
                    "rules": [{
                            "max": 32,
                            "message": "长度不能超过32个字符"
                        },
                        {
                            "required": true,
                            "message": "客户违规序号不能为空"
                        }
                    ]
                },
                "cust_no": {
                    "control": "string",
                    "width": "10px",
                    "label": "客户编号",
                    "rules": [{
                            "max": 30,
                            "message": "长度不能超过30个字符"
                        },
                        {
                            "required": true,
                            "message": "客户编号不能为空"
                        }
                    ]
                },
                "violtp": {
                    "control": "select",
                    "width": "10px",
                    "label": "违规种类:",
                    "align": "center",
                    "dict": {
                        "dictType": "US_E_VIOLATION_TYPE",
                        "dictKey": [
                            "US_E_VIOLATION_TYPE"
                        ],
                        "format": "label"
                    },
                    "rules": [{
                        "required": true,
                        "message": "请选择"
                    }]
                },
                "violation_date": {
                    "control": "dateTimePicker",
                    "width": "10px",
                    "label": "违规日期",
                    "rules": [{
                        "required": true,
                        "message": "请选择违规日期"
                    }]
                },
                "violation_declare": {
                    "control": "text",
                    "label": "违规说明",
                    "width": "10px",
                    "rules": [{
                            "max": 200,
                            "message": "长度不能超过200个字符"
                        },
                        {
                            "required": true,
                            "message": "请输入违规说明"
                        }
                    ]
                },
                "ccy_code": {
                    "control": "select",
                    "label": "币种",
                    "width": "10px",
                    "dict": {
                        "dictType": "MS_CURRENCY",
                        "dictKey": [
                            "MS_CURRENCY"
                        ],
                        "format": "label"
                    },
                    "rules": [{
                            "max": 3,
                            "message": "长度不能超过3个字符"
                        },
                        {
                            "required": true,
                            "message": "请选择相关币种"
                        }
                    ]
                },
                "violation_amt": {
                    "control": "text",
                    "width": "10px",
                    "label": "讨债金额（元）",
                    "rules": [{
                            "max": 16,
                            "message": "长度不能超过16个字符"
                        },
                        {
                            "required": true,
                            "message": "请输入讨债金额（元）"
                        }
                    ],
                    "format":{
                        "thousand":","
                      }
                },
                "remark": {
                    "label": "备注",
                    "control": "textarea",
                    "width": "10px",
                    "span": "24",
                    "autosize": {
                        "minRows": 4
                    },
                    "rules": [{
                        "max": "100",
                        "message": "长度不能超过100个字符"
                    }]
                }
            }]
        }
    }
}
