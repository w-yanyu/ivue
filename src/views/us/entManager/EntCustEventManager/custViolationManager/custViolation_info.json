{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "btns": [],
            "controls": [
                {
                    "serial_no": {
                        "control": "string",
                        "label": "客户违规序号",
                        "width": "10px"
                    },
                    "cust_no": {
                        "control": "string",
                        "label": "客户编号",
                        "width": "10px"
                    },
                    "violtp": {
                        "control": "string",
                        "label": "违规种类:",
                        "width": "10px",
                        "align": "center",
                        "dict": {
                            "dictType": "US_E_VIOLATION_TYPE",
                            "dictKey": [
                                "US_E_VIOLATION_TYPE"
                            ],
                            "format": "label"
                        }
                    },
                    "violation_date": {
                        "control": "string",
                        "width": "10px",
                        "label": "违规日期"
                    },
                    "violation_declare": {
                        "control": "string",
                        "width": "10px",
                        "label": "违规说明"
                    },
                    "ccy_code": {
                        "control": "string",
                        "width": "10px",
                        "label": "币种",
                        "dict": {
                            "dictType": "MS_CURRENCY",
                            "dictKey": [
                                "MS_CURRENCY"
                            ],
                            "format": "label"
                        }
                    },
                    "violation_amt": {
                        "control": "string",
                        "width": "10px",
                        "label": "讨债金额（元）",
                        "format":{
                            "thousand":","
                          }
                    },
                    "remark": {
                        "label": "备注",
                        "control": "textarea",
                        "width": "10px",
                        "disabled": "true",
                        "span": "24",
                        "autosize": {
                            "minRows": 4
                        }
                    }
                }
            ]
        }
    }
}
