{
    "module": "dynamicTabs",
    "layout": {
        "tabs": {
            "tabHeader": [{
                    "id": "tab1",
                    "label": "手动关联关系",
                    "closable": false,
                    "disabled": false,
                    "icon": "el-icon-date",
                    "toRequest": {
                        "url": "/views/us/entManager/corpRelaManager/cust_relationship_index_tree.json",
                        "method": "POST",
                        "cust_no": "${{params.cust_no}}",
                        "show_flash":"${{params.show_flash}}"

                    }
                },
                {
                    "id": "tab2",
                    "label": "关联关系检索",
                    "icon": "el-icon-date",
                    "closable": false,
                    "disabled": false,
                    "toRequest": {
                        "url": "/views/us/entManager/corpRelaManager/correlation_search_left.json",
                        "method": "POST",
                        "cust_no": "${{params.cust_no}}",
                        "show_flash":"${{params.show_flash}}"
                    }
                }
            ],
            "defaultActiveKey": "tab1",
            "duty_type": "top",
            "styleClass": "card"
        }
    }
}
