{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "scope": "searchTable",
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "us0234",
          "cust_no": "${{params.cust_no}}",
          "ent_relation": "${{params.ent_relation}}",
          "rela_classify": "${{params.rela_classify}}"
        }
      },
      "thead": [
        {
          "label": "序号",
          "type": "index",
          "align": "center"
        },
        {
          "label": "客户类别",
          "prop": "cust_type",
          "align": "center",
          "dict": {
            "format": "label",
            "dictKey": [
              "US_E_CUST_TYPE"
            ],
            "dictType": "US_E_CUST_TYPE"
          }
        },
        {
          "label": "客户名称",
          "prop": "cust_name",
          "align": "center"
        },
        {
          "label": "客户英文名称",
          "prop": "cust_english_name",
          "align": "center"
        },
        {
          "label": "客户曾用名",
          "prop": "cust_befor_name",
          "align": "center"
        },
        {
          "label": "客户性质",
          "prop": "cust_nature",
          "align": "center",
          "dict": {
            "format": "label",
            "dictKey": [
              "US_E_LEGAL_CUST_PROPERTY"
            ],
            "dictType": "US_E_LEGAL_CUST_PROPERTY"
          }
        },
        {
          "label": "证件类型",
          "prop": "cert_type",
          "align": "center",
          "dict": {
            "dictType": "MS_E_CRET_TYPE",
            "dictKey": [
              "MS_E_CRET_TYPE"
            ],
            "labelField": "dictName",
            "valueField": "dictId"
          }
        },
        {
          "label": "证件号码",
          "prop": "cert_no",
          "align": "center"
        }

      ]
    }
  }
}
