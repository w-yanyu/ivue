{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "btns": [],
            "controls": [
                {
                    "serial_no": {
                        "control": "hidden",
                        "label": "企业关联企业流水号",
                        "width": "10px",
                        "rules": [
                            {
                                "max": 19,
                                "message": "长度不能超过19个字符"
                            }
                        ]
                    },
                    "cust_no": {
                        "control": "hidden",
                        "width": "10px",
                        "label": "客户编号",
                        "disabled": true
                    },
                    "cust_name": {
                        "control": "string",
                        "width": "10px",
                        "label": "联系人姓名",
                        "rules": [
                            {
                                "max": 80,
                                "message": "长度不能超过80个字符"
                            }
                        ]
                    },
                    "recust_no": {
                        "control": "string",
                        "width": "10px",
                        "label": "关联企业编号",
                        "rules": [
                            {
                                "max": 19,
                                "message": "长度不能超过19个字符"
                            }
                        ]
                    },
                    "is_our_cust": {
                        "control": "string",
                        "label": "是否我行客户:",
                        "width": "10px",
                        "align": "center",
                        "value": "1",
                        "disabled": true,
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": true,
                            "message": "是否我行客户不能为空"
                        }]
                    },
                    "ent_relation": {
                        "control": "string",
                        "label": "关联关系:",
                        "width": "10px",
                        "align": "center",
                        "dict": {
                            "dictType": "US_E_ENT_RELATION",
                            "dictKey": [
                                "US_E_ENT_RELATION"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": true,
                            "message": "关联关系不能为空"
                        }]
                    },
                    "relate_level": {
                        "control": "string",
                        "label": "关联级别:",
                        "width": "10px",
                        "align": "center",
                        "dict": {
                            "dictType": "US_E_RELATION_LEVEL",
                            "dictKey": [
                                "US_E_RELATION_LEVEL"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": true,
                            "message": "关联级别不能为空"
                        }]
                    },
                    "association_type": {
                        "control": "string",
                        "width": "10px",
                        "label": "关联类型:",
                        "align": "center",
                        "dict": {
                            "dictType": "US_E_RELATION_TYPE",
                            "dictKey": [
                                "US_E_RELATION_TYPE"
                            ],
                            "format": "label"
                        }
                    },
                    "is_relate": {
                        "control": "string",
                        "width": "10px",
                        "label": "是否关联关系定义人:",
                        "align": "center",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        }
                    },
                    "remark": {
                        "label": "备注",
                        "control": "string",
                        "width": "10px",
                        "span": "24",
                        "autosize": {
                            "minRows": 4
                        },
                        "rules": [{
                            "max": "100",
                            "message": "长度不能超过100个字符"
                        }]
                    }
                }
            ]
        }
    }
}
