{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "btns": [{
                "type": "submit",
                "label": "提交",
                "icon": "",
                "doRequest": {
                    "url": "/SUMP/icmcall/icmRPCCall",
                    "method": "POST",
                    "params": {
                        "servicecode": "us0232"

                    }
                }
            }],
            "controls": [{
                "serial_no": {
                    "control": "hidden",
                    "label": "企业关联企业流水号",
                    "rules": [{
                            "max": 32,
                            "message": "长度不能超过32个字符"
                        },
                        {
                            "required": true,
                            "message": "企业关联企业流水号不能为空"
                        }
                    ]
                },
                "cust_no": {
                    "control": "hidden",
                    "label": "客户编号",
                    "width": "10px",
                    "disabled": true
                },
                "cust_name": {
                    "control": "string",
                    "width": "10px",
                    "label": "联系人姓名",
                    "rules": [{
                            "max": 80,
                            "message": "长度不能超过80个字符"
                        },
                        {
                            "required": true,
                            "message": "联系人姓名不能为空"
                        }
                    ]
                },
                "recust_no": {
                    "control": "string",
                    "width": "10px",
                    "label": "关联企业",
                    "rules": [{
                            "max": 19,
                            "message": "长度不能超过20个字符"
                        },
                        {
                            "required": true,
                            "message": "关联企业不能为空"
                        }
                    ]
                },
                "is_our_cust": {
                    "control": "select",
                    "label": "是否我行客户:",
                    "width": "10px",
                    "align": "center",
                    "value": "1",
                    "disabled": true,
                    "dict": {
                        "dictType": "MS_E_YESORNO",
                        "dictKey": [
                            "MS_E_YESORNO"
                        ],
                        "format": "label"
                    },
                    "rules": [{
                        "required": true,
                        "message": "是否我行客户不能为空"
                    }]
                },
                "ent_relation": {
                    "control": "select",
                    "label": "关联关系:",
                    "width": "10px",
                    "align": "center",
                    "dict": {
                        "dictType": "US_E_ENT_RELATION",
                        "dictKey": [
                            "US_E_ENT_RELATION"
                        ],
                        "format": "label"
                    },
                    "rules": [{
                        "required": true,
                        "message": "关联关系不能为空"
                    }]
                },
                "relate_level": {
                    "control": "select",
                    "label": "关联级别:",
                    "width": "10px",
                    "align": "center",
                    "dict": {
                        "dictType": "US_E_RELATION_LEVEL",
                        "dictKey": [
                            "US_E_RELATION_LEVEL"
                        ],
                        "format": "label"
                    },
                    "rules": [{
                        "required": true,
                        "message": "关联级别不能为空"
                    }]
                },
                "association_type": {
                    "control": "select",
                    "width": "10px",
                    "label": "关联类型:",
                    "align": "center",
                    "dict": {
                        "dictType": "US_E_RELATION_TYPE",
                        "dictKey": [
                            "US_E_RELATION_TYPE"
                        ],
                        "format": "label"
                    }
                },
                "is_relate": {
                    "control": "select",
                    "width": "10px",
                    "label": "是否关联关系定义人:",
                    "align": "center",
                    "dict": {
                        "dictType": "MS_E_YESORNO",
                        "dictKey": [
                            "MS_E_YESORNO"
                        ],
                        "format": "label"
                    }
                },
                "remark": {
                    "label": "备注",
                    "control": "textarea",
                    "width": "10px",
                    "span": "24",
                    "autosize": {
                        "minRows": 4
                    },
                    "rules": [{
                        "max": "100",
                        "message": "长度不能超过100个字符"
                    }]
                }
            }]
        }
    }
}
