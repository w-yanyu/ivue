{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "btns": [{
                "type": "submit",
                "label": "提交",
                "icon": "",
                "doRequest": {
                    "url": "/SUMP/icmcall/icmRPCCall",
                    "method": "POST",
                    "params": {
                        "servicecode": "us0321"
                    }
                }
            }],
            "controls": [{
                    "verify_type": {
                        "label": "验资类别",
                        "control": "select",
                        "width": "10px",
                        "rules": [{
                            "required": true,
                            "message": "请选择验资类别"
                        }],
                        "dict": {
                            "dictType": "US_E_VERIFY_TYPE",
                            "dictKey": [
                                "US_E_VERIFY_TYPE"
                            ],
                            "format": "label"
                        }
                    },
                    "verify_report_name": {
                        "control": "text",
                        "width": "10px",
                        "label": "会计验资机构名称",
                        "rules": [{
                                "required": false
                            },
                            {
                                "max": 80,
                                "message": "长度不能超过80个字符"
                            }
                        ]
                    }
                },
                {
                    "verify_code": {
                        "control": "text",
                        "width": "10px",
                        "label": "会计验资报告号码",
                        "rules": [{
                                "required": false
                            },
                            {
                                "max": 30,
                                "message": "长度不能超过30个字符"
                            }
                        ]
                    },
                    "verify_date": {
                        "control": "dateTimePicker",
                        "width": "10px",
                        "label": "会计验资时间",
                        "rules": [{
                            "required": true,
                            "message": "请选择会计验资时间"
                        }]
                    }
                },
                {
                    "register_princi": {
                        "control": "text",
                        "width": "10px",
                        "label": "注册资本金额",
                        "rules": [{
                            "required": true,
                            "max": 16,
                            "pattern": "^[0-9]+(\\.[0-9]{1,2})?$",
                            "message": "请输入两位小数以内数字,长度不能超过16个字符"
                        }]
                    },
                    "register_ccy": {
                        "label": "注册资本币种",
                        "width": "10px",
                        "control": "select",
                        "dict": {
                            "dictType": "MS_CURRENCY",
                            "dictKey": [
                                "MS_CURRENCY"
                            ],
                            "format": "label"
                        }
                    }
                },
                {
                    "actual_fund": {
                        "control": "text",
                        "width": "10px",
                        "label": "实际到位资金",
                        "rules": [{
                            "required": false,
                            "max": 16,
                            "pattern": "^[0-9]+(\\.[0-9]{1,2})?$",
                            "message": "请输入两位小数以内数字,长度不能超过16个字符"
                        }]
                    },
                    "is_listed_company": {
                        "label": "是否公众上市公司",
                        "control": "select",
                        "width": "10px",
                        "rules": [{
                            "required": true,
                            "message": "请选择是否公众上市公司"
                        }],
                        "dict": {
                            "dictType": "MS_E_YESORNO",

                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        }
                    }
                },
                {
                    "state_stock": {
                        "control": "text",
                        "width": "10px",
                        "label": "国有股"
                    },
                    "sponsor_stock": {
                        "control": "text",
                        "width": "10px",
                        "label": "发起人股"
                    }
                },
                {
                    "total_stock": {
                        "control": "text",
                        "width": "10px",
                        "label": "总股本",
                        "rules": [{
                            "required": true,
                            "message": "请输入总股本"
                        }]
                    },
                    "internal_stock": {
                        "control": "text",
                        "width": "10px",
                        "label": "内部职工股"
                    }
                },
                {
                    "social_stock": {
                        "control": "text",
                        "width": "10px",
                        "label": "集体股"
                    },
                    "allotment_stock": {
                        "control": "text",
                        "width": "10px",
                        "label": "转配股",
                        "rules": [{
                            "required": false
                        }]
                    }
                },
                {
                    "foreign_stock": {
                        "control": "text",
                        "width": "10px",
                        "label": "外资持股",
                        "rules": [{
                            "required": false
                        }]
                    },
                    "b_shares": {
                        "control": "text",
                        "width": "10px",
                        "label": "b股",
                        "rules": [{
                            "required": false
                        }]
                    }
                },
                {
                    "preferred_stock": {
                        "control": "text",
                        "width": "10px",
                        "label": "优先股",
                        "rules": [{
                            "required": false
                        }]
                    },
                    "h_stock": {
                        "control": "text",
                        "width": "10px",
                        "label": "h股",
                        "rules": [{
                            "required": false
                        }]
                    }
                },
                {
                    "a_stock": {
                        "control": "text",
                        "width": "10px",
                        "label": "a股",
                        "rules": [{
                            "required": false
                        }]
                    },
                    "other_tradable_stock": {
                        "control": "text",
                        "width": "10px",
                        "label": "其他流通股",
                        "rules": [{
                            "required": false
                        }]
                    }
                },
                {
                    "other_outstanding_stock": {
                        "control": "text",
                        "width": "10px",
                        "label": "其他未流通股份",
                        "rules": [{
                            "required": false
                        }]
                    },
                    "restrict_sales_stock": {
                        "control": "text",
                        "width": "10px",
                        "label": "股权分治限售",
                        "rules": [{
                            "required": false
                        }]
                    }
                },
                {
                    "raise_stock": {
                        "control": "text",
                        "width": "10px",
                        "label": "募集法人股",
                        "rules": [{
                            "required": false
                        }]
                    },
                    "territory_stock": {
                        "control": "text",
                        "width": "10px",
                        "label": "境内法人股",
                        "rules": [{
                            "required": false
                        }]
                    }
                },
                {
                    "release_date": {
                        "control": "dateTimePicker",
                        "label": "发布日期",
                        "width": "10px",
                        "rules": [{
                            "required": true,
                            "message": "请选择发布日期"
                        }]
                    }
                },
                {
                    "remark": {
                        "label": "备注",
                        "control": "textarea",
                        "span": 24,
                        "width": "10px",
                        "autosize": {
                            "minRows": 4
                        },
                        "rules": [{
                            "max": "200",
                            "message": "长度不能超过200个字符"
                        }]
                    }
                },
                {
                    "cust_no": {
                        "control": "hidden",
                        "label": "客户编号",
                        "disabled": true
                    }
                }
            ]
        }
    }
}