{
    "module": "v-inputDataForm-btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "post",
                "params": {
                    "servicecode": "us0331"
                }
            },
            "controls": [{
                "bank_name": {
                    "control": "text",
                    "label": "开户行名称",
                    "width": "10px",
                    "rules": [{
                            "max": 80,
                            "message": "长度不能超过80个字符"
                        },
                        {
                            "required": true,
                            "message": "开户行名称不能为空"
                        }
                    ]
                },
                "open_acct_bank_no": {
                    "control": "text",
                    "label": "开户行号",
                    "width": "10px",
                    "rules": [{
                        "max": 6,
                        "required": true,
                        "message": "开户行号不能为空，且长度不能超过6个字符"
                    }]
                },
                "acct_no": {
                    "control": "text",
                    "width": "10px",
                    "rules": [{
                        "required": true,
                        "max": 40,
                        "message": "长度不能超过40个字符"
                    }],
                    "label": "账号"
                },
                "account_name": {
                    "control": "text",
                    "width": "10px",
                    "rules": [{
                        "required": true,
                        "max": 80,
                        "message": "长度不能超过80个字符"
                    }],
                    "label": "账户名称"
                },
                "ccy_code": {
                    "control": "select",
                    "label": "币种",
                    "width": "10px",
                    "rules": [{
                        "required": true,
                        "message": "请选择币种"
                    }],
                    "dict": {
                        "dictType": "MS_CURRENCY",
                        "dictKey": [
                            "MS_CURRENCY"
                        ],
                        "format": "label"
                    }
                },
                "account_type": {
                    "control": "select",
                    "label": "账户类别",
                    "width": "10px",
                    "rules": [{
                        "required": true,
                        "message": "请选择账户类别"
                    }],
                    "dict": {
                        "dictType": "US_E_ACCOUNT_TYPE",
                        "dictKey": [
                            "US_E_ACCOUNT_TYPE"
                        ],
                        "format": "label"
                    }
                },
                "deposit_type": {
                    "control": "select",
                    "label": "存款种类",
                    "width": "10px",
                    "rules": [{
                        "required": true,
                        "message": "请选择存款种类"
                    }],
                    "dict": {
                        "dictType": "US_E_DEPOSIT_TYPE",
                        "dictKey": [
                            "US_E_DEPOSIT_TYPE"
                        ],
                        "format": "label"
                    }
                },
                "credit_balance": {
                    "control": "text",
                    "label": "余额（元）",
                    "width": "10px",
                    "rules": [{
                        "required": true,
                        "pattern": "^[0-9]+(\\.[0-9]{1,2})?$",
                        "message": "请输入两位小数以内的数字!"
                    }]
                },
                "acct_state": {
                    "control": "select",
                    "label": "账户状态",
                    "width": "10px",
                    "rules": [{
                        "required": true,
                        "message": "请选择账户状态"
                    }],
                    "dict": {
                        "dictType": "US_E_STATE",
                        "dictKey": [
                            "US_E_STATE"
                        ],
                        "format": "label"
                    }
                },
                "remark": {
                    "label": "备注",
                    "control": "textarea",
                    "width": "10px",
                    "span": "24",
                    "autosize": {
                        "minRows": 4
                    },
                    "rules": [{
                        "max": "100",
                        "message": "长度不能超过100个字符"
                    }]
                },
                "cust_no": {
                    "control": "hidden",
                    "label": "客户编号",
                    "width": "10px",
                    "disabled": true
                }
            }]
        }
    }
}
