{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "btns": [],
            "controls": [
                {
                    "bank_name": {
                        "control": "string",
                        "label": "开户行名称",
                        "width": "10px"
                    },
                    "open_acct_bank_no": {
                        "control": "string",
                        "label": "开户行号",
                        "width": "10px"
                    },
                    "acct_no": {
                        "control": "string",
                        "width": "10px",
                        "label": "账号"
                    },
                    "account_name": {
                        "control": "string",
                        "width": "10px",
                        "label": "账户名称"
                    },
                    "ccy_code": {
                        "control": "string",
                        "label": "币种",
                        "width": "10px",
                        "dict": {
                            "dictType": "MS_CURRENCY",
                            "dictKey": [
                                "MS_CURRENCY"
                            ],
                            "format": "label"
                        }
                    },
                    "account_type": {
                        "control": "string",
                        "label": "账户类别",
                        "width": "10px",
                        "dict": {
                            "dictType": "US_E_ACCOUNT_TYPE",
                            "dictKey": [
                                "US_E_ACCOUNT_TYPE"
                            ],
                            "format": "label"
                        }
                    },
                    "deposit_type": {
                        "control": "string",
                        "label": "存款种类",
                        "width": "10px",
                        "dict": {
                            "dictType": "US_E_DEPOSIT_TYPE",
                            "dictKey": [
                                "US_E_DEPOSIT_TYPE"
                            ],
                            "format": "label"
                        }
                    },
                    "credit_balance": {
                        "control": "string",
                        "label": "余额（元）",
                        "width": "10px"
                    },
                    "acct_state": {
                        "control": "string",
                        "label": "账户状态",
                        "width": "10px",
                        "dict": {
                            "dictType": "US_E_STATE",
                            "dictKey": [
                                "US_E_STATE"
                            ],
                            "format": "label"
                        }
                    },
                    "remark": {
                        "label": "备注",
                        "control": "textarea",
                        "disabled": true,
                        "width": "10px",
                        "span": "24",
                        "autosize": {
                            "minRows": 4
                        }
                    },
                    "serial_no": {
                        "control": "hidden"
                    },
                    "cust_no": {
                        "control": "hidden",
                        "label": "客户编号",
                        "width": "10px",
                        "disabled": true
                    }
                }
            ]
        }
    }
}
