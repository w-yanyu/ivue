{
    "module": "v-inputDataForm-btn",
    "layout": {
        "form": {
            "scope": "templateScope",
            "initDataOnce": true,
            "initRequest": "${{params}}",
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "post",
                "params": {
                    "servicecode": "us0312"
                }
            },
            "controls": [{
                    "security_name": {
                        "control": "text",
                        "label": "证券简称",
                        "width": "10px",
                        "rules": [{
                                "max": 30,
                                "message": "长度不能超过30个字符"
                            },
                            {
                                "message": "证券简称不能为空",
                                "required": true
                            }
                        ]
                    },
                    "security_type": {
                        "control": "text",
                        "label": "证券类型",
                        "width": "10px",
                        "rules": [{
                                "max": 10,
                                "message": "长度不能超过10个字符"
                            },
                            {
                                "message": "证券类别不能为空",
                                "required": true
                            }
                        ]
                    }
                },
                {
                    "security_code": {
                        "control": "text",
                        "label": "证券代码",
                        "width": "10px",
                        "rules": [{
                                "max": 30,
                                "message": "长度不能超过30个字符"
                            },
                            {
                                "message": "证券代码不能为空",
                                "required": true
                            }
                        ]
                    },
                    "list_date": {
                        "control": "dateTimePicker",
                        "label": "上市日期",
                        "width": "10px",
                        "rules": [{
                            "required": true,
                            "message": "上市日期不能为空"
                        }]
                    }
                },
                {
                    "list_desc": {
                        "control": "text",
                        "label": "上市批文",
                        "width": "10px",
                        "rules": [{
                            "required": true,
                            "message": "上市批文不能为空"
                        }]
                    },
                    "authority": {
                        "control": "text",
                        "label": "批准单位",
                        "width": "10px",
                        "rules": [{
                            "required": true,
                            "message": "批准单位不能为空"
                        }]
                    }
                },
                {
                    "recommended_unit": {
                        "control": "text",
                        "label": "推荐上市单位",
                        "width": "10px",
                        "rules": [{
                                "max": 80,
                                "message": "长度不能超过80个字符"
                            },
                            {
                                "required": true,
                                "message": "推荐上市单位不能为空"
                            }
                        ]
                    },
                    "accounting_firm": {
                        "control": "text",
                        "label": "会计师事务所",
                        "width": "10px",
                        "rules": [{
                                "max": 80,
                                "message": "长度不能超过80个字符"
                            },
                            {
                                "required": true,
                                "message": "会计师事务所不能为空"
                            }
                        ]
                    }
                },
                {
                    "issue_price": {
                        "control": "currency",
                        "label": "发行价（元）",
                        "width": "10px",
                        "rules": [{
                            "required": true,
                            "message": "发行价不能为空"
                        }]
                    },
                    "issue_amt": {
                        "control": "currency",
                        "label": "发行量",
                        "width": "10px",
                        "rules": [{
                            "required": true,
                            "message": "发行量不能为空"
                        }]
                    }
                },
                {
                    "stock_code": {
                        "control": "text",
                        "width": "10px",
                        "label": "股票代码（征）",
                        "rules": [{
                            "required": true,
                            "message": "股票代码（征）不能为空"
                        }]
                    },
                    "return_investment": {
                        "control": "text",
                        "suffix": "%",
                        "width": "10px",
                        "label": "预期投资收益率",
                        "rules": [{
                                "required": true,
                                "message": "预期投资收益率不能为空"
                            },
                            {
                                "pattern": "^(100.00|[1-9]?\\d(\\.\\d\\d\\d?)?)$",
                                "message": "请输入(0-100)之间的比例"
                            }
                        ]
                    }
                },
                {
                    "per_share_net": {
                        "control": "text",
                        "label": "每股净收益",
                        "width": "10px",
                        "rules": [{
                                "max": 16,
                                "message": "长度不能超过16个字符"
                            },
                            {
                                "required": true,
                                "message": "每股净收益不能为空"
                            }
                        ]
                    },
                    "site": {
                        "control": "select",
                        "label": "上市地",
                        "width": "10px",
                        "align": "center",
                        "dict": {
                            "dictType": "US_E_SITE",
                            "dictKey": [
                                "US_E_SITE"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": true,
                            "message": "请选择相关上市地"
                        }]
                    }
                },
                {
                    "investment_orient": {
                        "control": "text",
                        "label": "投资方向",
                        "width": "10px",
                        "rules": [{
                            "max": 100,
                            "message": "长度不能超过100个字符"
                        }]
                    },
                    "remark": {
                        "label": "备注",
                        "control": "textarea",
                        "width": "10px",
                        "span": "24",
                        "autosize": {
                            "minRows": 4
                        },
                        "rules": [{
                            "max": "100",
                            "message": "长度不能超过100个字符"
                        }]
                    }
                },
                {
                    "cust_no": {
                        "control": "hidden",
                        "label": "客户编号",
                        "value": "${{params.cust_no}}"
                    },
                    "serial_no": {
                        "control": "hidden",
                        "label": "企业证券信息流水号",
                        "value": "${{params.serial_no}}"
                    }
                }
            ]
        }
    }
}
