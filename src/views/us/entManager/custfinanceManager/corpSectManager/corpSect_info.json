{
    "module": "v-inputDataForm-btn",
    "layout": {
        "form": {
            "btns": [],
            "scope": "templateScope",
            "initDataOnce": true,
            "initRequest": "${{params}}",
            "controls": [{
                    "security_name": {
                        "control": "string",
                        "label": "证券简称",
                        "width": "10px"
                    },
                    "security_type": {
                        "control": "string",
                        "label": "证券类型",
                        "width": "10px"
                    }
                },
                {
                    "security_code": {
                        "control": "string",
                        "label": "证券代码",
                        "width": "10px"
                    },
                    "list_date": {
                        "control": "string",
                        "label": "上市日期",
                        "width": "10px"
                    }
                },
                {
                    "list_desc": {
                        "control": "string",
                        "label": "上市批文",
                        "width": "10px"
                    },
                    "authority": {
                        "control": "string",
                        "label": "批准单位",
                        "width": "10px"
                    }
                },
                {
                    "recommended_unit": {
                        "control": "string",
                        "label": "推荐上市单位",
                        "width": "10px"
                    },
                    "accounting_firm": {
                        "control": "string",
                        "label": "会计师事务所",
                        "width": "10px"
                    }
                },
                {
                    "issue_price": {
                        "control": "currency",
                        "label": "发行价（元）",
                        "disabled": true,
                        "width": "10px"
                    },
                    "issue_amt": {
                        "control": "string",
                        "label": "发行量",
                        "width": "10px"
                    }
                },
                {
                    "stock_code": {
                        "control": "string",
                        "width": "10px",
                        "label": "股票代码（征）"
                    },
                    "return_investment": {
                        "control": "text",
                        "label": "预期投资收益率",
                        "suffix": "%",
                        "disabled": true,
                        "width": "10px"
                    }
                },
                {
                    "per_share_net": {
                        "control": "string",
                        "label": "每股净收益",
                        "width": "10px"
                    },
                    "site": {
                        "control": "string",
                        "label": "上市地",
                        "width": "10px",
                        "align": "center",
                        "dict": {
                            "dictType": "US_E_SITE",
                            "dictKey": [
                                "US_E_SITE"
                            ],
                            "format": "label"
                        }
                    }
                },
                {
                    "investment_orient": {
                        "control": "string",
                        "label": "投资方向",
                        "width": "10px"
                    },
                    "remark": {
                        "label": "备注",
                        "control": "textarea",
                        "width": "10px",
                        "disabled": true,
                        "span": "24",
                        "autosize": {
                            "minRows": 4
                        },
                        "rules": [{
                            "max": "100",
                            "message": "长度不能超过100个字符"
                        }]
                    }
                },
                {
                    "cust_no": {
                        "control": "hidden",
                        "label": "客户编号",
                        "value": "${{params.cust_no}}"
                    },
                    "serial_no": {
                        "control": "hidden",
                        "label": "企业证券信息流水号",
                        "value": "${{params.serial_no}}"
                    }
                }
            ]
        }
    }
}
