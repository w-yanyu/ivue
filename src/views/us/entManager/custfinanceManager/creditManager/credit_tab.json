{
    "module": "dynamicTabs",
    "layout": {
        "tabs": {
            "tabHeader": [{
                    "id": "tab1",
                    "label": "他行授信",
                    "closable": false,
                    "disabled": false,
                    "icon": "el-icon-date",
                    "toRequest": {
                        "url": "/views/us/privateManage/assetStatus/otherBankCredit/otherBankCredit_manage.json",
                        "method": "POST",
                        "cust_no": "${{params.cust_no}}",
                        "show_flash":"${{params.show_flash}}",
                        "cust_name": "${{params.cust_name}}"

                    }
                },
                {
                    "id": "tab2",
                    "label": "本行授信",
                    "icon": "el-icon-date",
                    "closable": false,
                    "disabled": false,
                    "toRequest": {
                        "url": "/views/us/entManager/entCustBusiManager/myBankCreditManager/mybankCreditManager_manage.json",
                        "method": "POST",
                        "show_flash":"${{params.show_flash}}",
                        "cust_no": "${{params.cust_no}}"
                    }
                }
            ],
            "defaultActiveKey": "tab1",
            "duty_type": "top",
            "styleClass": "card"
        }
    }
}
