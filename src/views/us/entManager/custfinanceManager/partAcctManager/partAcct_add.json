{
    "module": "v-inputDataForm-btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "post",
                "params": {
                    "servicecode": "us0501"
                }
            },
            "controls": [
                {
                    "charge":{
                        "control": "hidden",
                        "label": "账户类型",
                        "value": "${{params.cust_type}}",
                        "events": {
                            "condition": {
                                "10": {
                                    "control": {
                                        "acct_type1": {
                                            "initShow":true
                                        }
                                    }
                                },
                                "20": {
                                    "control": {
                                        "acct_type2": {
                                            "initShow":true
                                        }
                                    }
                                }
                            }
                        } 
                    },
                    "cust_no": {
                        "control": "text",
                        "label": "客户编号",
                        "width": "10px",
                        "disabled": true,
                        "value": "${{params.cust_no}}",
                        "rules": [
                            {
                                "required": true,
                                "message": "账户类型不能为空"
                            }
                        ]
                    },
                    "acct_type1": {
                        "control": "select",
                        "label": "账户类型",
                        "width": "10px",
                        "dict": {
                            "dictType": "US_E_ACCT_TYPE",
                            "dictKey": [
                                "US_E_ACCT_TYPE"
                            ],
                            "include": [
                                "1",
                                "2"
                            ],
                            "format": "label"
                        },
                        "initShow": false,
                        "rules": [
                            {
                                "required": true,
                                "message": "账户类型不能为空"
                            }
                        ],
                        "events": {
                            "condition": {
                                "$default": {
                                    "control": {
                                        "acct_type": {
                                            "control": "hidden",
                                            "label": "账户类型",
                                            "value":"${{form.acct_type1}}"
                                        }
                                    }
                                }
                            }
                        } 
                    },
                    "acct_type2": {
                        "control": "select",
                        "label": "账户类型",
                        "width": "10px",
                        "dict": {
                            "dictType": "US_E_ACCT_TYPE",
                            "dictKey": [
                                "US_E_ACCT_TYPE"
                            ],
                            "format": "label"
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "账户类型不能为空"
                            }
                        ],
                        "initShow": false,
                        "events": {
                            "condition": {
                                "$default": {
                                    "control": {
                                        "acct_type": {
                                            "control": "hidden",
                                            "label": "账户类型",
                                            "value":"${{form.acct_type2}}"
                                        }
                                    }
                                }
                            }
                        } 
                    },
                    "acct_type": {
                        "control": "hidden",
                        "label": "账户类型",
                        "width": "10px"
                    },
                    "acct_no": {
                        "control": "text",
                        "width": "10px",
                        "rules": [
                            {
                                "required": true,
                                "max": 40,
                                "message": "长度不能超过40个字符"
                            }
                        ],
                        "label": "账号"
                    },
                    "account_name": {
                        "control": "text",
                        "width": "10px",
                        "label": "账户名称"
                    },
                    "bank_no": {
                        "control": "text",
                        "label": "联行号",
                        "width": "10px"
                    },
                    "open_acct_bank_no": {
                        "control": "text",
                        "label": "开户行号",
                        "width": "10px",
                        "rules": [
                            {
                                "required": true,
                                "message": "开户行号不能为空"
                            }
                        ]
                    },
                    "open_acct_bank": {
                        "control": "text",
                        "label": "开户行",
                        "width": "10px",
                        "rules": [
                            {
                                "required": true,
                                "message": "开户行不能为空"
                            }
                        ]
                    },
                    "open_bank_province": {
                        "control": "text",
                        "label": "开户行省份",
                        "width": "10px",
                        "rules": [
                            {
                                "required": true,
                                "message": "开户行省份不能为空"
                            }
                        ]
                    },
                    "open_bank_city": {
                        "control": "text",
                        "label": "开户行城市",
                        "width": "10px",
                        "rules": [
                            {
                                "required": true,
                                "message": "开户行城市不能为空"
                            }
                        ]
                    }
                }
            ]
        }
    }
}