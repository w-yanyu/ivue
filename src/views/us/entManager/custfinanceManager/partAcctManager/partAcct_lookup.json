{
    "module": "v_search_btn_datagrid",
    "layout": {
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": true,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "us0500",
                    "cust_no": "${{params.cust_no}}"


                }
            },
            "thead": [
                {
                    "type": "index",
                    "align": "center",
                    "label": "序号"
                },
                {
                    "label": "客户编号",
                    "prop": "cust_no",
                    "align": "center"
                },
                {
                    "label": "账户类型",
                    "prop": "acct_type",
                    "align": "center",
                    "dict": {
                        "dictType": "US_E_ACCT_TYPE",
                        "dictKey": [
                            "US_E_ACCT_TYPE"
                        ],
                        "labelField": "dictName",
                        "valueField": "dictId"
                    }
                },
                {
                    "label": "账号",
                    "prop": "acct_no",
                    "align": "center"

                },
                {
                    "label": "账户名称",
                    "prop": "account_name",
                    "align": "center"

                },
                {
                    "label": "联行号",
                    "prop": "bank_no",
                    "align": "center"
                },
                {
                    "label": "开户行号",
                    "prop": "open_acct_bank_no",
                    "align": "center"

                },
                {
                    "label": "开户行",
                    "prop": "open_acct_bank",
                    "align": "center"

                },
                {
                    "label": "开户行省份",
                    "prop": "open_bank_province",
                    "align": "center"

                },
                {
                    "label": "开户行城市",
                    "prop": "open_bank_city",
                    "align": "center"

                },

                {
                    "label": "操作",
                    "align": "center",
                    "fixed": "right",
                    "btns": [
                        {
                            "type": "bringback",
                            "label": "选择"
                        }
                    ]
                }
            ]
        }
    }
}
