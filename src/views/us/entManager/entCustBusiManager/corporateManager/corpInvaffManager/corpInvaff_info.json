{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "btns": [],
            "controls": [{
                "cust_no": {
                    "control": "hidden",
                    "label": "客户编号",
                    "width": "10px",
                    "disabled": true
                },
                "serial_no": {
                    "control": "hidden",
                    "label": "企业对外投资及联营信息流水号",
                    "edit": false
                },
                "cust_name": {
                    "label": "被投资单位客户名称",
                    "control": "string",
                    "width": "10px",
                    "disabled": true
                },
                "is_our_cust": {
                    "control": "string",
                    "label": "是否我行客户",
                    "width": "10px",
                    "value": "1",
                    "disabled": true,
                    "dict": {
                        "dictType": "MS_E_YESORNO",
                        "dictKey": [
                            "MS_E_YESORNO"
                        ],
                        "format": "label"
                    }
                },
                "custna2": {
                    "control": "hidden",
                    "edit": true,
                    "width": "10px",
                    "rules": [],
                    "label": "关联企业名称",
                    "keyField": {
                        "cust_name": "cust_name",
                        "incuid": "cust_no"
                    },
                    "toRequest": {
                        "url": "/views/us/usManager/corporatePropertyRights/customer_lookup.json",
                        "method": "post"
                    },
                    "props": {
                        "title": "查询关联企业客户名称"
                    },
                    "format": "label"
                },
                "in_cust_no": {
                    "control": "hidden",
                    "label": "被投资单位客户ID",
                    "width": "10px",
                    "disabled": true
                },

                "inves_type": {
                    "control": "string",
                    "label": "投资类型描述",
                    "width": "10px",
                    "dict": {
                        "dictType": "US_E_INVES_TYPE",
                        "dictKey": [
                            "US_E_INVES_TYPE"
                        ],
                        "format": "label"
                    }
                },
                "is_controller": {
                    "control": "string",
                    "label": "是否实际控制人",
                    "width": "10px",
                    "dict": {
                        "dictType": "MS_E_YESORNO",
                        "dictKey": [
                            "MS_E_YESORNO"
                        ],
                        "format": "label"
                    }
                },
                "retpyj": {
                    "control": "string",
                    "label": "关联企业性质(银监)",
                    "width": "10px",
                    "dict": {
                        "dictType": "US_E_RETYYJ",
                        "dictKey": [
                            "US_E_RETYYJ"
                        ],
                        "format": "label"
                    }
                },
                "nature_type": {
                    "control": "string",
                    "label": "关联企业性质",
                    "width": "10px",
                    "dict": {
                        "dictType": "US_E_NATURE_TYPE",
                        "dictKey": [
                            "US_E_NATURE_TYPE"
                        ],
                        "format": "label"
                    }
                },
                "relate_type": {
                    "control": "string",
                    "label": "关联类型",
                    "width": "10px",
                    "dict": {
                        "dictType": "US_E_RELATION_TYPE",
                        "dictKey": [
                            "US_E_RELATION_TYPE"
                        ],
                        "format": "label"
                    }
                },
                "busi_addr": {
                    "label": "经营地址",
                    "control": "textarea",
                    "disabled": true,
                    "width": "10px",
                    "placeholder": "50字以内",
                    "autosize": {
                        "minRows": 4
                    },
                    "span": "24"
                },
                "busi_scope": {
                    "label": "经营范围",
                    "span": "24",
                    "control": "textarea",
                    "disabled": true,
                    "width": "10px",
                    "placeholder": "100字以内",
                    "autosize": {
                        "minRows": 4
                    }
                },
                "remark": {
                    "label": "备注",
                    "control": "textarea",
                    "disabled": true,
                    "width": "10px",
                    "span": "24",
                    "placeholder": "100字以内",
                    "autosize": {
                        "minRows": 4
                    }
                }
            }]
        }
    }
}
