{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "btns": [],
      "controls": [
        {
          "cust_no": {
            "control": "hidden",
            "label": "客户编号",
            "width": "10px",
            "disabled": true
          },
          "serial_no": {
            "control": "hidden",
            "width": "10px",
            "label": "产权构成序号",
            "edit": false
          },
          "cust_name": {
            "control": "string",
            "width": "10px",
            "disabled": true,
            "label": "股东名称"
          },
          "shareholder_date": {
            "label": "成为股东的日期",
            "control": "string",
            "width": "10px"
          },
          "is_max_holder": {
            "control": "string",
            "label": "是否最大股东",
            "width": "10px",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "ccy_code": {
            "control": "string",
            "label": "币种",
            "width": "10px",
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": [
                "MS_CURRENCY"
              ],
              "format": "label"
            }
          },
          "contributive_amt": {
            "label": "出资金额/价值（万元）",
            "control": "currency",
            "disabled": true,
            "width": "10px",
            "max": 16
          },
          "ownership_ratio": {
            "label": "股权比例",
            "control": "text",
            "disabled": true,
            "width": "10px",
            "suffix": "%"
          },
          "is_controller": {
            "control": "string",
            "label": "是否实际控制人",
            "width": "10px",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "is_valid": {
            "control": "string",
            "label": "是否有效",
            "width": "10px",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "contributive_type": {
            "control": "string",
            "label": "出资类别",
            "width": "10px",
            "dict": {
              "dictType": "US_E_INVEST_TYPE",
              "dictKey": [
                "US_E_INVEST_TYPE"
              ],
              "format": "label"
            }
          },
          "shareholder_change_date": {
            "control": "string",
            "label": "股东结构变更日期",
            "width": "10px"
          }
        }
      ]
    }
  }
}
