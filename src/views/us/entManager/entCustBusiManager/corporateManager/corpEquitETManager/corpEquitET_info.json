{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "btns": [],
      "controls": [
        {
          "cust_no": {
            "control": "hidden",
            "label": "客户编号",
            "width": "10px",
            "disabled": true
          },
          "serial_no": {
            "control": "hidden",
            "label": "产权构成序号",
            "width": "10px",
            "disabled": true
          },
          "cust_name": {
            "control": "string",
            "label": "被控股企业名称",
            "disabled": true,
            "width": "10px"
          },
          "is_our_cust": {
            "control": "string",
            "label": "是否我行客户",
            "width": "10px",
            "value": "1",
            "disabled": true,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "registration_no": {
            "label": "登记注册号",
            "control": "string",
            "width": "10px"
          },
          "is_max_holder": {
            "control": "string",
            "label": "是否最大股东",
            "width": "10px",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "holder_type": {
            "control": "string",
            "label": "法人客户性质",
            "width": "10px",
            "dict": {
              "dictType": "US_E_HOLDER_TYPE",
              "dictKey": [
                "US_E_HOLDER_TYPE"
              ],
              "format": "label"
            }
          },
          "contributive_type": {
            "control": "string",
            "label": "出资类别",
            "width": "10px",
            "dict": {
              "dictType": "US_E_INVEST_TYPE",
              "dictKey": [
                "US_E_INVEST_TYPE"
              ],
              "format": "label"
            }
          },
          "ccy_code": {
            "control": "string",
            "label": "币种",
            "width": "10px",
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": [
                "MS_CURRENCY"
              ],
              "format": "label"
            }
          },
          "contributive_amt": {
            "label": "出资金额/价值（万元）",
            "control": "string",
            "width": "10px",
            "max": 16
          },
          "ownership_ratio": {
            "label": "股权比例",
            "control": "text",
            "disabled": true,
            "width": "10px",
            "suffix": "%"
          },
          "is_controller": {
            "control": "string",
            "label": "是否实际控制人",
            "width": "10px",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "is_valid": {
            "control": "string",
            "label": "是否有效",
            "width": "10px",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "shareholder_date": {
            "label": "成为股东的日期",
            "control": "string",
            "width": "10px"
          }
        }
      ]
    }
  }
}
