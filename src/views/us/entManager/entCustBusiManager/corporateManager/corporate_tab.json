{
    "module": "dynamicTabs",
    "layout": {
      "tabs": {
        "tabHeader": [
          {
            "id": "tab1",
            "label": "股东构成",
            "closable": false,
            "disabled": false,
            "icon": "el-icon-date",
            "toRequest": {
              "url": "/views/us/entManager/entCustBusiManager/corporateManager/corpEquitManager/corpEquit_list.json",
              "params": {
                "show_flash":"${{params.show_flash}}"
            },
              "method": "POST"
            }
          },
          {
            "id": "tab2",
            "label": "主要控股企业",
            "icon": "el-icon-date",
            "closable": false,
            "disabled": false,
            "toRequest": {
              "url": "/views/us/entManager/entCustBusiManager/corporateManager/corpEquitETManager/corpEquitET_list.json",
              "params": {
                "show_flash":"${{params.show_flash}}"
            },
              "method": "POST"
            }
          },
          {
            "id": "tab3",
            "label": "其他关联情况",
            "icon": "el-icon-date",
            "closable": false,
            "disabled": false,
            "toRequest": {
              "url": "/views/us/entManager/entCustBusiManager/corporateManager/corpInvaffManager/corpInvaff_list.json",
              "params": {
                "show_flash":"${{params.show_flash}}"
            },
              "method": "POST"
            }
          }
        ],
        "defaultActiveKey": "tab1",
        "duty_type": "top",
        "styleClass": "card"
      }
    }
  }
