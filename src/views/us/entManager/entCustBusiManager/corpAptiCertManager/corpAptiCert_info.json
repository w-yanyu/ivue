{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "btns": [],
            "controls": [
                {
                    "cust_no": {
                        "control": "hidden",
                        "label": "客户编号",
                        "width": "10px",
                        "value": "${{params.cust_no}}",
                        "disabled": true
                    },
                    "serial_no": {
                        "control": "hidden",
                        "label": "企业资质证书流水号",
                        "width": "10px",
                        "value": "${{params.serial_no}}",
                        "disabled": true
                    },
                    "apcr_code": {
                        "control": "string",
                        "width": "10px",
                        "label": "证书编号",
                        "rules": [
                            {
                                "max": 30,
                                "message": "长度不能超过30个字符"
                            }
                        ]
                    },
                    "apcr_name": {
                        "control": "string",
                        "width": "10px",
                        "label": "证书名称",
                        "rules": [
                            {
                                "max": 30,
                                "message": "长度不能超过30个字符"
                            }
                        ]
                    },
                    "apcr_level": {
                        "control": "string",
                        "label": "资质等级:",
                        "width": "10px",
                        "align": "center",
                        "dict": {
                            "dictType": "US_E_QUAL_LEVEL",
                            "dictKey": [
                                "US_E_QUAL_LEVEL"
                            ],
                            "format": "label"
                        }
                    },
                    "base_start_date": {
                        "control": "string",
                        "width": "10px",
                        "label": "证件生效日期"
                    },
                    "end_date": {
                        "control": "string",
                        "width": "10px",
                        "label": "证件失效日期",
                        "disabledDate": {
                            "startRange": "${{form.apcsdt}}",
                            "endRange": "3000-12-30"
                        }
                    },
                    "apcr_unit": {
                        "control": "string",
                        "width": "10px",
                        "label": "颁发单位",
                        "rules": [
                            {
                                "max": 40,
                                "message": "长度不能超过40个字符"
                            }
                        ]
                    },
                    "apcr_content": {
                        "control": "string",
                        "width": "10px",
                        "label": "证书内容",
                        "rules": [
                            {
                                "max": 100,
                                "message": "长度不能超过100个字符"
                            }
                        ]
                    },
                    "remark": {
                        "label": "备注",
                        "control": "textarea",
                        "disabled": true,
                        "width": "10px",
                        "span": "24",
                        "autosize": {
                            "minRows": 4
                        },
                        "rules": [
                            {
                                "max": "100",
                                "message": "长度不能超过100个字符"
                            }
                        ]
                    }
                }
            ]
        }
    }
}
