{
    "module": "v-inputDataForm-btn",
    "layout": {
        "form": {
            "btns": [],
            "initRequest": "${{params}}",
            "controls": [{
                "apply_no": {
                    "label": "申请号",
                    "control": "text",
                    "disabled": true
                },
                "prod_name": {
                    "label": "贷款产品",
                    "control": "select",
                    "disabled": true
                },
                "apply_amt": {
                    "label": "申请金额",
                    "control": "text",
                    "disabled": true,
                    "format":{
                        "thousand":","
                      }
                },
                "ccy_code": {
                    "label": "币种",
                    "control": "select",
                    "disabled": true,
                    "dict": {
                        "dictType": "MS_CURRENCY",
                        "dictKey": [
                            "MS_CURRENCY"
                        ],
                        "labelField": "dictName",
                        "valueField": "dictId"
                    }
                },
                "effective_date": {
                    "label": "生效日期",
                    "control": "dateTimePicker",
                    "disabled": true
                },
                "matu_date": {
                    "label": "失效日期",
                    "control": "dateTimePicker",
                    "align": "center",
                    "disabled": true
                }
            }]
        }
    }
}
