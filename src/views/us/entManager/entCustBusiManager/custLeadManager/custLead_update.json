{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": "",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "us0272"
                           
                        }
                    }
                }
            ],
            "controls": [
                {
                    "cust_name": {
                        "control": "text",
                        "label": "实际控制人客户名称",
                        "disabled": true,
                        "width": "160px"
                    },
                    "serial_no": {
                        "control": "hidden",
                        "label": "流水号",
                        "width": "160px",
                        "disabled": true
                    },
                    "let_type": {
                        "control": "select",
                        "label": "实际控制人类型",
                        "disabled": true,
                        "width": "160px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择实际控制人类型"
                            }
                        ],
                        "dict": {
                            "dictType": "US_E_LET_TYPE",
                            "dictKey": [
                                "US_E_LET_TYPE"
                            ],
                            "format": "label"
                        },
                        "showItem": {
                            "2": [
                                "corgcd"
                            ],
                            "9": [
                                "corgcd"
                            ]
                        }
                    }
                },
                {
                    "actual_controller_id": {
                        "control": "hidden",
                        "label": "实际控制人ID",
                        "width": "160px"
                    }
                },
                {
                    "cert_type": {
                        "control": "select",
                        "label": "证件类型",
                        "width": "160px",
                        "disabled": true,
                        "dict": {
                            "dictType": "MS_E_CRET_TYPE",
                            "dictKey": [
                                "MS_E_CRET_TYPE"
                            ],
                            "format": "label"
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择证件类型"
                            }
                        ]
                    },
                    "cert_no": {
                        "control": "text",
                        "label": "证件号码",
                        "width": "160px",
                        "disabled": true,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入证件号码"
                            }
                        ]
                    }
                },
                {
                    "cust_no": {
                        "control": "hidden",
                        "label": "客户id",
                        "disabled": true
                    },
                    "org_code": {
                        "control": "hidden",
                        "label": "组织机构代码",
                        "disabled": true,
                        "width": "160px",
                        "#rules": [
                            {
                                "max": 30,
                                "pattern": "^[0-9]+$"
                            },
                            {
                                "required": true,
                                "message": "请输入组织机构代码"
                            }
                        ]
                    },
                    "status": {
                        "control": "select",
                        "label": "是否有效",
                        "width": "160px",
                        "dict": {
                            "dictType": "US_E_STATE",
                            "dictKey": [
                                "US_E_STATE"
                            ],
                            "format": "label"
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择是否有效"
                            }
                        ]
                    }
                }
            ]
        }
    }
}