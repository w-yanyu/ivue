                {
                    "cert_type": {
                        "control": "select",
                        "label": "证件类型",
                        "width": "160px",
                        "disabled": true,
                        "dict": {
                            "dictType": "MS_E_CRET_TYPE",
                            "dictKey": [
                                "MS_E_CRET_TYPE"
