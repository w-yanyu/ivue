{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "btns": [],
            "controls": [
                {
                    "cust_name": {
                        "control": "text",
                        "label": "实际控制人客户名称",
