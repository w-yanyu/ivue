                        "width": "160px"
                    },
                    "serial_no": {
                        "control": "hidden",
                        "label": "流水号",
                        "width": "160px",
                        "disabled": true
                    },
                    "let_type": {
                        "control": "select",
                        "label": "实际控制人类型",
                        "disabled": true,
                        "width": "160px",
                        "dict": {
                            "dictType": "US_E_LET_TYPE",
                            "dictKey": [
                                "US_E_LET_TYPE"
                            ],
                            "format": "label"
                        },
                        "showItem": {
                            "2": [
                                "corgcd"
                            ],
                            "9": [
                                "corgcd"
                            ]
                        }
                    }
                },
                {
                    "actual_controller_id": {
                        "control": "hidden",
