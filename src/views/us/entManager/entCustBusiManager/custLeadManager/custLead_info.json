                    },
                    "status": {
                        "control": "string",
                        "label": "是否有效",
                        "width": "160px",
                        "dict": {
                            "dictType": "US_E_STATE",
                            "dictKey": [
                                "US_E_STATE"
                            ],
                            "format": "label"
                        }
                    },
                    "cust_no": {
                        "control": "hidden",
                        "label": "客户id",
                        "disabled": true
