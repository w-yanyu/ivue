{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "scope": "formData",
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "us0271"
                           
                        }
                    }
                }
            ],
            "controls": [
                {
                    "cust_name": {
                        "control": "lookup",
                        "edit": true,
                        "width": "10px",
                        "label": "控制人名称",
                        "keyField": {
                            "actual_controller_id": "cust_no",
                            "cust_name": "cust_name",
                            "org_code": "org_code",
                            "cert_type": "cert_type",
                            "cert_no": "cert_no"
                        },
                        "toRequest": {
                            "url": "/views/us/cust_lookup.json",
                            "method": "get",
                            "params": {
                                
                                
                            }
                        },
                        "props": {
                            "title": "选择实际控制人",
                            "width": "75%"
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择控制人信息!"
                            }
                        ]
                    },
                    "cert_type": {
                        "control": "select",
                        "label": "证件类型",
                        "width": "10px",
                        "disabled": true,
                        "dict": {
                            "dictType": "MS_E_CRET_TYPE",
                            "dictKey": [
                                "MS_E_CRET_TYPE"
                            ],
                            "format": "label"
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择证件类型"
                            }
                        ]
                    },
                    "cert_no": {
                        "control": "text",
                        "label": "证件号码",
                        "disabled": true,
                        "width": "10px",
                        "rules": [
                            {
                                "max": 50,
                                "required": true,
                                "message": "请输入证件号码"
                            }
                        ]
                    },
                    "let_type": {
                        "control": "select",
                        "label": "控制人类型",
                        "width": "10px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择控制人类型"
                            }
                        ],
                        "dict": {
                            "dictType": "US_E_LET_TYPE",
                            "dictKey": [
                                "US_E_LET_TYPE"
                            ],
                            "format": "label"
                        },
                        "showItem": {
                            "2": [
                                "corgcd"
                            ],
                            "9": [
                                "corgcd"
                            ]
                        }
                    }
                },
                {
                    "status": {
                        "control": "select",
                        "label": "是否有效",
                        "width": "10px",
                        "dict": {
                            "dictType": "US_E_STATE",
                            "dictKey": [
                                "US_E_STATE"
                            ],
                            "format": "label"
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择是否有效"
                            }
                        ]
                    },
                    "org_code": {
                        "control": "hidden",
                        "label": "组织机构代码",
                        "disabled": true,
                        "width": "10px",
                        "#rules": [
                            {
                                "max": 30,
                                "pattern": "^[0-9]+$"
                            },
                            {
                                "required": true,
                                "message": "请输入组织机构代码"
                            }
                        ]
                    },
                    "actual_controller_id": {
                        "control": "hidden",
                        "label": "控制人编号",
                        "width": "10px",
                        "disabled": true
                    },
                    "cust_no": {
                        "control": "hidden",
                        "label": "客户编号",
                        "value": "${{params.cust_no}}",
                        "width": "10px",
                        "disabled": true
                    }
                }
            ]
        }
    }
}