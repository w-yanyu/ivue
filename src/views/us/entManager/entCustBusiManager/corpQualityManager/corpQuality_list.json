{
    "module": "v_search_btn_datagrid",
    "layout": {
        "toolbar": {
            "btns": [{
                "type": "add",
                "target": "dialog",
                "label": "新增",
                "toRequest": {
                    "method": "POST",
                    "url": "/views/us/entManager/entCustBusiManager/corpQualityManager/corpQuality_add.json",
                    "params": {
                        "cust_no": "${{params.cust_no}}"
                    }
                },
                "show": {
                    "expression": "'${{params.show_flash}}' == '1'"
                },
                "props": {
                    "width": "80%",
                    "title": "素质和管理水平信息新增"
                }
            }]
        },
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": true,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "us0250",
                    "cust_no": "${{params.cust_no}}"
                }
            },
            "thead": [{
                    "type": "index",
                    "label": "序号",
                    "align": "center"
                },
                {
                    "label": "客户编号",
                    "prop": "cust_no",
                    "align": "center"
                },
                {
                    "label": "实际控制人",
                    "prop": "actual_controller",
                    "align": "center"
                },
                {
                    "label": "实际控制人代码(贷款卡号/身份证号)",
                    "prop": "actual_controller_code",
                    "align": "center"
                },
                {
                    "label": "员工人数",
                    "prop": "staff_count",
                    "align": "center"
                },
                {
                    "label": "在职人数",
                    "prop": "on_job_count",
                    "align": "center"
                },
                {
                    "label": "退休人数",
                    "prop": "retire_count",
                    "align": "center"
                },
                {
                    "label": "硕士人数",
                    "prop": "master_count",
                    "align": "center"
                },
                {
                    "label": "本科生人数",
                    "prop": "undergrad_count",
                    "align": "center"
                },
                {
                    "label": "专科生人数",
                    "prop": "junior_count",
                    "align": "center"
                },
                {
                    "label": "高师人数",
                    "prop": "lfye_count",
                    "align": "center"
                },
                {
                    "label": "中师人数",
                    "prop": "mode_count",
                    "align": "center"
                },
                {
                    "label": "助师人数",
                    "prop": "base_count",
                    "align": "center"
                },
                {
                    "label": "科技人员人数",
                    "prop": "technology_person_count",
                    "align": "center"
                },
                {
                    "label": "技术人员占比（%）",
                    "prop": "technology_person",
                    "align": "center"
                },
                {
                    "label": "科研经费占销售收入比（%）",
                    "prop": "scientific_funds_ratio",
                    "align": "center"
                },
                {
                    "label": "专利号",
                    "prop": "patent_code",
                    "align": "center"
                },
                {
                    "label": "专利名称",
                    "prop": "patent_name",
                    "align": "center"
                },
                {
                    "label": "企业所获认证资格",
                    "prop": "busi_qualification",
                    "align": "center"
                },
                {
                    "label": "企业是否掌握核心技术",
                    "prop": "is_busi_core_technology",
                    "align": "center",
                    "dict": {
                        "dictType": "MS_E_YESORNO",
                        "dictKey": [
                            "MS_E_YESORNO"
                        ],
                        "labelField": "dictName",
                        "valueField": "dictId"
                    }
                },
                {
                    "label": "实际控制人类型",
                    "prop": "actual_controller_type",
                    "align": "center",
                    "dict": {
                        "dictType": "US_E_CONTROLLER_TYPE",
                        "dictKey": [
                            "US_E_CONTROLLER_TYPE"
                        ],
                        "labelField": "dictName",
                        "valueField": "dictId"
                    }
                },
                {
                    "label": "操作",
                    "fixed": "right",
                    "width": 150,
                    "align": "center",
                    "btns": [{
                            "type": "look",
                            "label": "详情",
                            "toRequest": {
                                "url": "/views/us/entManager/entCustBusiManager/corpQualityManager/corpQuality_info.json",
                                "method": "POST"
                            },
                            "props": {
                                "width": "80%",
                                "title": "素质和管理水平详情"
                            }
                        },
                        {
                            "type": "edit",
                            "label": "维护",
                            "toRequest": {
                                "url": "/views/us/entManager/entCustBusiManager/corpQualityManager/corpQuality_update.json",
                                "method": "GET"
                            },
                            "show": {
                                "expression": "'${{params.show_flash}}' == '1'"
                            },
                            "props": {
                                "width": "80%",
                                "title": "素质和管理水平维护"
                            }
                        },
                        {
                            "type": "custom",
                            "label": "删除",
                            "doRequest": {
                                "url": "/SUMP/icmcall/icmRPCCall",
                                "method": "POST",
                                "params": {
                                    "servicecode": "us0253"
                                   
                                }
                            },
                            "show": {
                                "expression": "'${{params.show_flash}}' == '1'"
                            },
                            "confirm": {
                                "title": "确定删除",
                                "confirmButtonText": "确定",
                                "cancelButtonText": "取消"
                            }
                        }
                    ]
                }
            ]
        }
    }
}
