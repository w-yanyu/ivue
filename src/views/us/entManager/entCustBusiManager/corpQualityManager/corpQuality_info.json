{
    "module": "v-inputDataForm-btn",
    "layout": {
        "form": {
            "scope": "templateScope",
            "initDataOnce": true,
            "btns": [],
            "initRequest": "${{params}}",
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "post",
                "params": {

                }
            },
            "controls": [{
                "cust_no": {
                    "control": "hidden",
                    "width": "10px",
                    "label": "客户编号",
                    "disabled": true
                },
                "actual_controller": {
                    "control": "text",
                    "disabled": true,
                    "width": "10px",
                    "label": "实际控制人"
                },
                "actual_controller_code": {
                    "control": "text",
                    "disabled": true,
                    "width": "10px",
                    "label": "实际控制人代码(贷款卡号/身份证号)"
                },
                "actual_controller_type": {
                    "control": "select",
                    "disabled": true,
                    "label": "实际控制人类型:",
                    "align": "center",
                    "width": "10px",
                    "dict": {
                        "dictType": "US_E_CONTROLLER_TYPE",
                        "dictKey": [
                            "US_E_CONTROLLER_TYPE"
                        ],
                        "format": "label"
                    }
                },
                "staff_count": {
                    "control": "text",
                    "width": "10px",
                    "disabled": true,
                    "label": "员工人数"
                },
                "on_job_count": {
                    "control": "text",
                    "label": "在职人数",
                    "disabled": true,
                    "width": "10px"
                },
                "retire_count": {
                    "control": "text",
                    "width": "10px",
                    "disabled": true,
                    "label": "退休人数"
                },
                "master_count": {
                    "control": "text",
                    "width": "10px",
                    "disabled": true,
                    "label": "硕士人数"
                },
                "undergrad_count": {
                    "control": "text",
                    "disabled": true,
                    "width": "10px",
                    "label": "本科生人数"
                },
                "junior_count": {
                    "control": "text",
                    "width": "10px",
                    "disabled": true,
                    "label": "专科生人数"
                },
                "lfye_count": {
                    "control": "text",
                    "disabled": true,
                    "width": "10px",
                    "label": "高师人数"
                },
                "mode_count": {
                    "control": "text",
                    "width": "10px",
                    "disabled": true,
                    "label": "中师人数"
                },
                "base_count": {
                    "control": "text",
                    "width": "10px",
                    "disabled": true,
                    "label": "助师人数"
                },
                "technology_person_count": {
                    "control": "text",
                    "width": "10px",
                    "disabled": true,
                    "label": "科技人员人数"
                },
                "technology_person": {
                    "control": "text",
                    "width": "10px",
                    "disabled": true,
                    "label": "技术人员占比",
                    "suffix": "%"
                },
                "scientific_funds_ratio": {
                    "control": "text",
                    "width": "10px",
                    "disabled": true,
                    "suffix": "%",
                    "label": "科研经费占销售收入比"
                },
                "patent_code": {
                    "control": "text",
                    "width": "10px",
                    "disabled": true,
                    "label": "专利号"
                },
                "patent_name": {
                    "control": "text",
                    "width": "10px",
                    "disabled": true,
                    "label": "专利名称"
                },
                "busi_qualification": {
                    "control": "text",
                    "label": "企业所获认证资格",
                    "width": "10px",
                    "disabled": true
                },
                "is_busi_core_technology": {
                    "control": "select",
                    "label": "企业是否掌握核心技术和专利",
                    "align": "center",
                    "width": "10px",
                    "disabled": true,
                    "dict": {
                        "dictType": "MS_E_YESORNO",
                        "dictKey": [
                            "MS_E_YESORNO"
                        ],
                        "format": "label"
                    }
                }
            }]
        }
    }
}
