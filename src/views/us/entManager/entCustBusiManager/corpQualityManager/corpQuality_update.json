{
    "module": "v-inputDataForm-btn",
    "layout": {
        "form": {
            "scope": "templateScope",
            "initDataOnce": true,
            "initRequest": "${{params}}",
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "post",
                "params": {
                    "servicecode": "us0252"
                    
                }
            },
            "controls": [{
                "cust_no": {
                    "control": "hidden",
                    "label": "客户编号",
                    "width": "10px",
                    "disabled": true
                },
                "actual_controller": {
                    "control": "text",
                    "width": "10px",
                    "label": "实际控制人",
                    "rules": [{
                            "max": 60,
                            "message": "长度不能超过60个字符"
                        },
                        {
                            "required": true,
                            "message": "实际控制人不能为空"
                        }
                    ]
                },
                "actual_controller_code": {
                    "control": "text",
                    "width": "10px",
                    "label": "实际控制人代码(贷款卡号/身份证号)",
                    "rules": [{
                            "max": 40,
                            "message": "长度不能超过40个字符"
                        },
                        {
                            "required": true,
                            "message": "实际控制人代码不能为空"
                        }
                    ]
                },
                "actual_controller_type": {
                    "control": "select",
                    "width": "10px",
                    "label": "实际控制人类型:",
                    "align": "center",
                    "dict": {
                        "dictType": "US_E_CONTROLLER_TYPE",
                        "dictKey": [
                            "US_E_CONTROLLER_TYPE"
                        ],
                        "format": "label"
                    },
                    "rules": [{
                        "required": true,
                        "message": "实际控制人类型不能为空"
                    }]
                },
                "staff_count": {
                    "control": "text",
                    "width": "10px",
                    "label": "员工人数",
                    "rules": [{
                            "max": 10,
                            "message": "长度不能超过10个字符"
                        },
                        {
                            "required": true,
                            "message": "员工人数不能为空"
                        }
                    ]
                },
                "on_job_count": {
                    "control": "text",
                    "width": "10px",
                    "label": "在职人数",
                    "rules": [{
                            "max": 10,
                            "message": "长度不能超过10个字符"
                        },
                        {
                            "required": true,
                            "message": "在职人数不能为空"
                        }
                    ]
                },
                "retire_count": {
                    "control": "text",
                    "width": "10px",
                    "label": "退休人数",
                    "rules": [{
                            "max": 10,
                            "message": "长度不能超过10个字符"
                        },
                        {
                            "required": true,
                            "message": "退休人数不能为空"
                        }
                    ]
                },
                "master_count": {
                    "control": "text",
                    "width": "10px",
                    "label": "硕士人数",
                    "rules": [{
                            "max": 10,
                            "message": "长度不能超过10个字符"
                        },
                        {
                            "required": true,
                            "message": "硕士人数不能为空"
                        }
                    ]
                },
                "undergrad_count": {
                    "control": "text",
                    "width": "10px",
                    "label": "本科生人数",
                    "rules": [{
                            "max": 10,
                            "message": "长度不能超过10个字符"
                        },
                        {
                            "required": true,
                            "message": "本科生人数不能为空"
                        }
                    ]
                },
                "junior_count": {
                    "control": "text",
                    "width": "10px",
                    "label": "专科生人数",
                    "rules": [{
                            "max": 10,
                            "message": "长度不能超过10个字符"
                        },
                        {
                            "required": true,
                            "message": "专科生人数不能为空"
                        }
                    ]
                },
                "lfye_count": {
                    "control": "text",
                    "width": "10px",
                    "label": "高师人数",
                    "rules": [{
                            "max": 10,
                            "message": "长度不能超过10个字符"
                        },
                        {
                            "required": true,
                            "message": "高师人数不能为空"
                        }
                    ]
                },
                "mode_count": {
                    "control": "text",
                    "label": "中师人数",
                    "width": "10px",
                    "rules": [{
                            "max": 10,
                            "message": "长度不能超过10个字符"
                        },
                        {
                            "required": true,
                            "message": "中师人数不能为空"
                        }
                    ]
                },
                "base_count": {
                    "control": "text",
                    "width": "10px",
                    "label": "助师人数",
                    "rules": [{
                            "max": 10,
                            "message": "长度不能超过10个字符"
                        },
                        {
                            "required": true,
                            "message": "助师人数不能为空"
                        }
                    ]
                },
                "technology_person_count": {
                    "control": "text",
                    "width": "10px",
                    "label": "科技人员人数",
                    "rules": [{
                            "max": 10,
                            "message": "长度不能超过10个字符"
                        },
                        {
                            "required": true,
                            "message": "科技人员人数不能为空"
                        }
                    ]
                },
                "technology_person": {
                    "control": "text",
                    "width": "10px",
                    "label": "技术人员占比",
                    "suffix": "%",
                    "rules": [{
                            "max": 10,
                            "message": "长度不能超过10个字符"
                        },
                        {
                            "required": true,
                            "message": "技术人员占比不能为空"
                        },
                        {
                            "pattern": "^(100.00|[1-9]?\\d(\\.\\d\\d\\d?)?)$",
                            "message": "请输入(0-100)之间的比例"
                        }
                    ]
                },
                "scientific_funds_ratio": {
                    "control": "text",
                    "width": "10px",
                    "suffix": "%",
                    "label": "科研经费占销售收入比",
                    "rules": [{
                            "max": 10,
                            "message": "长度不能超过10个字符"
                        },
                        {
                            "required": true,
                            "message": "科研经费占销售收入比不能为空"
                        },
                        {
                            "pattern": "^(100.00|[1-9]?\\d(\\.\\d\\d\\d?)?)$",
                            "message": "请输入(0-100)之间的比例"
                        }
                    ]
                },
                "patent_code": {
                    "control": "text",
                    "label": "专利号",
                    "width": "10px",
                    "rules": [{
                            "max": 10,
                            "message": "长度不能超过10个字符"
                        },
                        {
                            "required": true,
                            "message": "专利号不能为空"
                        }
                    ]
                },
                "patent_name": {
                    "control": "text",
                    "width": "10px",
                    "label": "专利名称",
                    "rules": [{
                            "max": 10,
                            "message": "长度不能超过10个字符"
                        },
                        {
                            "required": true,
                            "message": "专利名称不能为空"
                        }
                    ]
                },
                "busi_qualification": {
                    "control": "text",
                    "width": "10px",
                    "label": "企业所获认证资格",
                    "rules": [{
                            "max": 10,
                            "message": "长度不能超过10个字符"
                        },
                        {
                            "required": true,
                            "message": "企业所获认证资格不能为空"
                        }
                    ]
                },
                "is_busi_core_technology": {
                    "control": "select",
                    "width": "10px",
                    "label": "企业是否掌握核心技术:",
                    "align": "center",
                    "dict": {
                        "dictType": "MS_E_YESORNO",
                        "dictKey": [
                            "MS_E_YESORNO"
                        ],
                        "format": "label"
                    },
                    "rules": [{
                        "required": true,
                        "message": "企业是否掌握核心技术不能为空"
                    }]
                }
            }]
        }
    }
}
