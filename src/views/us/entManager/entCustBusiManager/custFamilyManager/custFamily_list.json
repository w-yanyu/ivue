{
    "module": "v_search_btn_datagrid",
    "layout": {
        "toolbar": {
            "btns": [{
                "type": "add",
                "target": "dialog",
                "label": "新增",
                "toRequest": {
                    "method": "GET", 
                    "url": "/views/us/entManager/entCustBusiManager/custFamilyManager/custFamily_add.json",
                    "params": {
                        "cust_no": "${{params.cust_no}}",
                        "org_code": "${{params.org_code}}"
                    }
                },
                "show": {
                    "expression": "'${{params.show_flash}}' == '1'"
                },
                "props": {
                    "width": "75%",
                    "title": "家族企业成员信息新增"
                }
            }]
        },
        "datagrid": {
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "post",
                "params": {
                    "servicecode": "us0280",
                    "cust_no": "${{params.cust_no}}"
                }
            },
            "thead": [{
                    "type": "index",
                    "label": "序号",
                    "align": "center"
                },
                {
                    "label": "关系",
                    "prop": "faly_type",
                    "align": "center",
                    "dict": {
                        "dictType": "US_E_FALY_TYPE",
                        "dictKey": [
                            "US_E_FALY_TYPE"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "家族成员姓名",
                    "prop": "cust_name",
                    "align": "center"
                },
                {
                    "label": "证件类型",
                    "prop": "cert_type",
                    "align": "center",
                    "dict": {
                        "dictType": "MS_E_CRET_TYPE",
                        "dictKey": [
                            "MS_E_CRET_TYPE"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "证件号码",
                    "prop": "cert_no",
                    "align": "center"
                },
                {
                    "label": "家族关系",
                    "prop": "family_relation",
                    "align": "center",
                    "dict": {
                        "dictType": "US_E_FAMILY_TIES",
                        "dictKey": [
                            "US_E_FAMILY_TIES"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "有效状态",
                    "prop": "status",
                    "align": "center",
                    "dict": {
                        "dictType": "US_E_STATE",
                        "dictKey": [
                            "US_E_STATE"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "操作",
                    "fixed": "right",
                    "align": "center",
                    "width": 160,
                    "btns": [{
                            "type": "look",
                            "label": "详情",
                            "toRequest": {
                                "url": "/views/us/entManager/entCustBusiManager/custFamilyManager/custFamily_info.json",
                                "method": "POST"
                            },
                            "props": {
                                "width": "75%",
                                "title": "家族企业成员信息详情"
                            }
                        },
                        {
                            "type": "edit",
                            "label": "维护",
                            "toRequest": {
                                "url": "/views/us/entManager/entCustBusiManager/custFamilyManager/custFamily_update.json",
                                "method": "GET"
                            },
                            "show": {
                                "expression": "'${{params.show_flash}}' == '1'"
                            },
                            "props": {
                                "width": "75%",
                                "title": "家族企业成员信息维护"
                            }
                        },
                        {
                            "type": "delete",
                            "label": "删除",
                            "doRequest": {
                                "url": "/SUMP/icmcall/icmRPCCall",
                                "method": "POST",
                                "params": {
                                    "servicecode": "us0283"
                                }
                            },
                            "show": {
                                "expression": "'${{params.show_flash}}' == '1'"
                            },
                            "confirm": {
                                "title": "确定删除",
                                "confirmButtonText": "确定",
                                "cancelButtonText": "取消"
                            }
                        }
                    ]
                }
            ]
        }
    }
}