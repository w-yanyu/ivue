{
    "module": "v_search_btn_datagrid",
    "layout": {
        "toolbar": {
            "btns": [{
                "type": "add",
                "target": "dialog",
                "label": "新增",
                "toRequest": {
                    "method": "POST",
                    "params": {
                        "cust_no": "${{params.cust_no}}"
                    },
                    "url": "/views/us/entManager/entCustBusiManager/corporateHandlerManager/corpHandlerManager/corpHandler_add.json"
                },
                "show": {
                    "expression": "'${{params.show_flash}}' == '1'"
                },
                "props": {
                    "width": "75%",
                    "title": "企业高管信息新增"
                }
            }]
        },
        "datagrid": {
            "isInitSearch": true,
            "scope": "searchTable",
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "us0260",
                    "cust_no": "${{params.cust_no}}"
                }
            },
            "thead": [{
                    "label": "序号",
                    "type": "index",
                    "align": "center"
                },
                {
                    "label": "高管客户编号",
                    "prop": "hdct_no",
                    "align": "center"
                },
                {
                    "label": "姓名",
                    "prop": "cust_name",
                    "align": "center"
                },
                {
                    "label": "性别",
                    "prop": "sex_tp",
                    "align": "center",
                    "dict": {
                        "dictType": "MS_E_GENDER",
                        "dictKey": [
                            "MS_E_GENDER"
                        ],
                        "labelField": "dictName",
                        "valueField": "dictId"
                    }
                },
                {
                    "label": "高管类型",
                    "prop": "executive_type",
                    "align": "center",
                    "dict": {
                        "dictType": "US_E_EXECUTIVE_TYPE",
                        "dictKey": [
                            "US_E_EXECUTIVE_TYPE"
                        ],
                        "labelField": "dictName",
                        "valueField": "dictId"
                    }
                },
                {
                    "label": "是否总经理",
                    "prop": "is_general_manager",
                    "align": "center",
                    "dict": {
                        "dictType": "MS_E_YESORNO",
                        "dictKey": [
                            "MS_E_YESORNO"
                        ],
                        "labelField": "dictName",
                        "valueField": "dictId"
                    }
                },
                {
                    "label": "证件类型",
                    "prop": "cert_type",
                    "align": "center",
                    "dict": {
                        "dictType": "MS_E_CRET_TYPE",
                        "dictKey": [
                            "MS_E_CRET_TYPE"
                        ],
                        "labelField": "dictName",
                        "valueField": "dictId"
                    }
                },
                {
                    "label": "证件号码",
                    "prop": "cert_no",
                    "align": "center"
                },
                {
                    "label": "高管社会关系",
                    "fixed": "right",
                    "width": 200,
                    "align": "center",
                    "btns": [{
                        "type": "edit",
                        "label": "详情",
                        "toRequest": {
                            "url": "/views/us/entManager/entCustBusiManager/corporateHandlerManager/corpHandlerRelaManager/corpHandlerRela_list.json",
                            "params": {
                                "serial_no": "${{current.serial_no}}",
                                "cust_no": "${{current.hdct_no}}",
                               "show_flash" :"${{params.show_flash}}"
                            },
                            "method": "post"
                        },
                        "props": {
                            "width": "75%",
                            "title": "企业高管社会关系详情"
                        }
                    }]
                },
                {
                    "label": "操作",
                    "fixed": "right",
                    "width": 160,
                    "align": "center",
                    "btns": [{
                            "type": "look",
                            "label": "详情",
                            "toRequest": {
                                "url": "/views/us/entManager/entCustBusiManager/corporateHandlerManager/corpHandlerManager/corpHandler_info.json",
                                "method": "POST"
                            },
                            "props": {
                                "width": "75%",
                                "title": "企业高管信息详情"
                            }
                        },
                        {
                            "type": "edit",
                            "label": "维护",
                            "toRequest": {
                                "url": "/views/us/entManager/entCustBusiManager/corporateHandlerManager/corpHandlerManager/corpHandler_update.json",
                                "method": "post"
                            },
                            "show": {
                                "expression": "'${{params.show_flash}}' == '1'"
                            },
                            "props": {
                                "width": "75%",
                                "title": "企业高管信息维护"
                            }
                        },
                        {
                            "type": "delete",
                            "label": "删除",
                            "doRequest": {
                                "url": "/SUMP/icmcall/icmRPCCall",
                                "method": "POST",
                                "params": {
                                    "servicecode": "us0263"
                                }
                            },
                            "show": {
                                "expression": "'${{params.show_flash}}' == '1'"
                            },
                            "confirm": {
                                "title": "确定删除",
                                "confirmButtonText": "确定",
                                "cancelButtonText": "取消"
                            }
                        }
                    ]
                }
            ],
            "pagination": {
                "show": true
            }
        }
    }
}
