{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "us0261"
                }
            },
            "controls": [{
                "cust_name": {
                    "label": "姓名",
                    "control": "lookup",
                    "width": "10px",
                    "edit": true,
                    "keyField": {
                        "cust_name": "cust_name",
                        "hdct_no": "cust_no",
                        "cert_type": "cert_type",
                        "cert_no": "cert_no",
                        "sex_tp":"sex_tp",
                        "phone_no":"phone_no",
                        "birth_date":"birth_date"
                    },
                    "toRequest": {
                        "url": "/views/us/privateManage/customer_lookup.json",
                        "method": "post"
                    },
                    "props": {
                        "title": "查询客户名称"
                    },
                    "rules": [{
                        "required": true,
                        "message": "请选择客户名称"
                    }],
                    "format": "label"
                },
                "sex_tp": {
                    "label": "性别",
                    "control": "select",
                    "disabled": true,
                    "width": "10px",
                    "rules": [{
                        "required": true,
                        "message": "请选择性别"
                    }],
                    "dict": {
                        "dictType": "MS_E_GENDER",
                        "dictKey": [
                            "MS_E_GENDER"
                        ],
                        "format": "label"
                    }
                },
                "hdct_no": {
                    "control": "string",
                    "disabled": true,
                    "label": "高管客户编号",
                    "width": "10px"
                },
                "cust_no": {
                    "control": "hidden",
                    "disabled": true,
                    "label": "客户编号",
                    "width": "10px"
                },
                "executive_type": {
                    "label": "高管类别",
                    "control": "select",
                    "width": "10px",
                    "rules": [{
                        "required": true,
                        "message": "请选择高管类别"
                    }],
                    "dict": {
                        "dictType": "US_E_EXECUTIVE_TYPE",
                        "dictKey": [
                            "US_E_EXECUTIVE_TYPE"
                        ],
                        "format": "label"
                    }
                },
                "is_general_manager": {
                    "label": "是否总经理",
                    "control": "select",
                    "width": "10px",
                    "rules": [{
                        "required": true,
                        "message": "请选择是否总经理"
                    }],
                    "dict": {
                        "dictType": "MS_E_YESORNO",
                        "dictKey": [
                            "MS_E_YESORNO"
                        ],
                        "format": "label"
                    }
                },
                "is_our_cust": {
                    "label": "是否我行客户",
                    "control": "select",
                    "width": "10px",
                    "value": "1",
                    "disabled": true,
                    "rules": [{
                        "message": "请选择是否我行客户"
                    }],
                    "dict": {
                        "dictType": "MS_E_YESORNO",
                        "dictKey": [
                            "MS_E_YESORNO"
                        ],
                        "format": "label"
                    },
                    "selectFirstValue": true
                },
                "is_group_executive": {
                    "label": "是否为集团高管",
                    "control": "select",
                    "width": "10px",
                    "rules": [{
                        "required": true,
                        "message": "请选择是否为集团高管"
                    }],
                    "dict": {
                        "dictType": "MS_E_YESORNO",
                        "dictKey": [
                            "MS_E_YESORNO"
                        ],
                        "format": "label"
                    }
                },
                "cert_type": {
                    "label": "证件类型",
                    "control": "select",
                    "disabled": true,
                    "width": "10px",
                    "rules": [{
                        "required": true,
                        "message": "请选择证件类型"
                    }],
                    "dict": {
                        "dictType": "MS_E_CRET_TYPE",
                        "dictKey": [
                            "MS_E_CRET_TYPE"
                        ],
                        "format": "label"
                    },
                    "selectFirstValue": true
                },
                "cert_no": {
                    "label": "证件号码",
                    "control": "text",
                    "disabled": true,
                    "width": "10px",
                    "rules": [{
                        "required": true,
                        "message": "请输入证件号码"
                    }]
                },
                "acct_location": {
                    "label": "户口所在地（50个汉字以内）",
                    "control": "textarea",
                    "width": "10px",
                    "rules": [{
                        "message": "请输入户口所在地"
                    }]
                },
                "addres": {
                    "label": "现住址(50个汉字以内)",
                    "control": "textarea",
                    "width": "10px",
                    "rules": [{
                        "max": 50,
                        "message": "请输入现住址"
                    }]
                },
                "start_date": {
                    "control": "dateTimePicker",
                    "label": "任职起始日期",
                    "format": "yyyyMMdd",
                    "width": "10px",
                    "rules": [{
                        "required": true,
                        "message": "请选择任职起始日期"
                    }]
                },
                "end_date": {
                    "control": "dateTimePicker",
                    "label": "任职到期日期",
                    "format": "yyyyMMdd",
                    "width": "10px",
                    "rules": [{
                        "message": "请选择任职到期日期"
                    }],
                    "disabledDate": {
                        "startRange": "${{form.stardt}}",
                        "endRange": "3000-12-30"
                    }
                },
                "is_controller": {
                    "label": "是否企业实际控制人",
                    "control": "select",
                    "width": "10px",
                    "rules": [{
                        "required": true,
                        "message": "请选择是否企业实际控制人"
                    }],
                    "dict": {
                        "dictType": "MS_E_YESORNO",
                        "dictKey": [
                            "MS_E_YESORNO"
                        ],
                        "format": "label"
                    }
                },
                "birth_date": {
                    "control": "dateTimePicker",
                    "label": "出生日期",
                    "disabled": true,
                    "format": "yyyyMMdd",
                    "width": "10px",
                    "rules": [{
                        "required": true,
                        "message": "请选择出生日期"
                    }]
                },
                "phone_no": {
                    "label": "联系电话",
                    "control": "text",
                    "width": "10px",
                    "rules": [{
                        "required": true,
                        "message": "请输入联系电话"
                    }]
                },
                "max_educat_degree": {
                    "label": "最高学位",
                    "control": "select",
                    "width": "10px",
                    "rules": [{
                        "required": true,
                        "message": "请输入最高学位"
                    }],
                    "dict": {
                        "dictType": "MS_E_MAXDEG",
                        "dictKey": [
                            "MS_E_MAXDEG"
                        ],
                        "format": "label"
                    }
                },
                "is_office": {
                    "label": "是否在任",
                    "control": "select",
                    "width": "10px",
                    "rules": [{
                        "required": true,
                        "message": "请选择是否在任"
                    }],
                    "dict": {
                        "dictType": "MS_E_YESORNO",
                        "dictKey": [
                            "MS_E_YESORNO"
                        ],
                        "format": "label"
                    }
                },
                "email": {
                    "label": "电子邮箱",
                    "control": "text",
                    "width": "10px",
                    "rules": [{
                        "max": 100,
                        "message": "请输入正确的电子邮箱",
                        "type": "email"
                    }]
                },
                "is_passport_foreign": {
                    "label": "是否有护照或外国居住权",
                    "control": "select",
                    "width": "10px",
                    "rules": [{
                        "message": "请选择是否有护照或外国居住权"
                    }],
                    "dict": {
                        "dictType": "MS_E_YESORNO",
                        "dictKey": [
                            "MS_E_YESORNO"
                        ],
                        "format": "label"
                    }
                },
                "is_foreign_relate": {
                    "label": "是否有海外关系",
                    "control": "select",
                    "width": "10px",
                    "rules": [{
                        "message": "请选择是否有海外关系"
                    }],
                    "dict": {
                        "dictType": "MS_E_YESORNO",
                        "dictKey": [
                            "MS_E_YESORNO"
                        ],
                        "format": "label"
                    }
                },
                "is_start_company": {
                    "label": "是否有家人开公司",
                    "control": "select",
                    "width": "10px",
                    "dict": {
                        "dictType": "MS_E_YESORNO",
                        "dictKey": [
                            "MS_E_YESORNO"
                        ],
                        "format": "label"
                    }
                },
                "is_part_time": {
                    "label": "是否兼职",
                    "control": "select",
                    "width": "10px",
                    "dict": {
                        "dictType": "MS_E_YESORNO",
                        "dictKey": [
                            "MS_E_YESORNO"
                        ],
                        "format": "label"
                    }
                },
                "is_board_member": {
                    "label": "是否董事会成员",
                    "control": "select",
                    "width": "10px",
                    "dict": {
                        "dictType": "MS_E_YESORNO",
                        "dictKey": [
                            "MS_E_YESORNO"
                        ],
                        "format": "label"
                    }
                },
                "is_escape_debt": {
                    "label": "是否逃废债",
                    "control": "select",
                    "width": "10px",
                    "dict": {
                        "dictType": "MS_E_YESORNO",
                        "dictKey": [
                            "MS_E_YESORNO"
                        ],
                        "format": "label"
                    }
                },
                "fax_number": {
                    "label": "传真号码",
                    "control": "text",
                    "width": "10px",
                    "rules": [{
                        "max": 60,
                        "message": "不超过60位"
                    }]
                },
                "job_resume": {
                    "label": "工作简历（120汉字以内）",
                    "control": "textarea",
                    "width": "10px",
                    "autosize": {
                        "minRows": 4
                    },
                    "span": 24,
                    "rules": [{
                        "required": true,
                        "max": 100,
                        "message": "120汉字以内"
                    }]
                },
                "bad_record": {
                    "label": "不良记录（100汉字以内）",
                    "control": "textarea",
                    "width": "10px",
                    "autosize": {
                        "minRows": 4
                    },

                    "span": 24,
                    "rules": [{
                        "max": 100,
                        "message": "100汉字以内"
                    }]
                },
                "personal_hobbies": {
                    "label": "个人嗜好（100汉字以内）",
                    "control": "textarea",
                    "width": "10px",
                    "autosize": {
                        "minRows": 4
                    },
                    "span": 24,
                    "rules": [{
                        "max": 100,
                        "message": "100汉字以内"
                    }]
                },
                "remark": {
                    "label": "备注（100汉字以内）",
                    "control": "textarea",
                    "width": "10px",
                    "autosize": {
                        "minRows": 4
                    },
                    "span": 24,
                    "rules": [{
                        "max": 100,
                        "message": "100汉字以内"
                    }]
                }
            }]
        }
    }
}
