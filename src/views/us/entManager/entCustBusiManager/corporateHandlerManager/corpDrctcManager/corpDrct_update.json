{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "scope": "templateScope",
            "initDataOnce": true,
            "initRequest": "${{params}}",
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "us0362"

                }
            },
            "btns": [{
                "type": "submit",
                "label": "提交"
            }],
            "controls": [{
                "serial_no": {
                    "label": "流水号",
                    "control": "hidden",
                    "width": "10px"
                },
                "cust_no": {
                    "control": "hidden",
                    "label": "客户编号",
                    "disabled": true,
                    "align": "center",
                    "width": "10px"
                },
                "member_cust_no": {
                    "label": "董事会成员客户编号",
                    "control": "hidden",
                    "align": "center",
                    "width": "10px"
                },
                "cust_name": {
                    "label": "姓名",
                    "control": "string",
                    "width": "10px",
                    "align": "center"
                },
                "is_our_cust": {
                    "label": "是否我行客户",
                    "control": "select",
                    "width": "10px",
                    "align": "center",
                    "value": "1",
                    "disabled": true,
                    "rules": [{
                        "required": true,
                        "message": "请选择是否我行客户"
                    }],
                    "dict": {
                        "dictType": "MS_E_YESORNO",
                        "dictKey": [
                            "MS_E_YESORNO"
                        ],
                        "format": "label"
                    }
                },
                "is_office": {
                    "label": "是否在任",
                    "control": "select",
                    "align": "center",
                    "width": "10px",
                    "rules": [{
                        "require": true,
                        "message": "请选择是否在任"
                    }],
                    "dict": {
                        "dictType": "MS_E_YESORNO",
                        "dictKey": [
                            "MS_E_YESORNO"
                        ],
                        "format": "label"
                    }
                },
                "board_job": {
                    "label": "董事会职务",
                    "align": "center",
                    "control": "select",
                    "width": "10px",
                    "rules": [{
                        "require": true,
                        "message": "请选择董事会职务"
                    }],
                    "dict": {
                        "dictType": "US_E_BOARD_JOB",
                        "dictKey": [
                            "US_E_BOARD_JOB"
                        ],
                        "format": "label"
                    }
                },
                "post_start": {
                    "control": "dateTimePicker",
                    "label": "职务任职开始",
                    "format": "yyyyMMdd",
                    "width": "10px",
                    "rules": [{
                        "required": true,
                        "message": "请选择职务任职开始"
                    }]
                },
                "post_ent": {
                    "control": "dateTimePicker",
                    "label": "职务任职结束",
                    "format": "yyyyMMdd",
                    "width": "10px",
                    "rules": [{
                        "message": "请选择职务任职结束"
                    }],
                    "disabledDate": {
                        "startRange": "${{form.testdt}}",
                        "endRange": "3000-12-30"
                    }
                },
                "remark": {
                    "label": "备注",
                    "control": "textarea",
                    "width": "10px",
                    "span": "24",
                    "autosize": {
                        "minRows": 4
                    },
                    "rules": [{
                        "max": "100",
                        "message": "长度不能超过100个字符"
                    }]
                }
            }]
        }
    }
}
