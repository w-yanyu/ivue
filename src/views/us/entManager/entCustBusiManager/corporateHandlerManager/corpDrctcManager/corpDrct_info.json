{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initDataOnce": true,
            "initRequest": "${{params}}",
            "btns": [],
            "controls": [
                {
                    "serial_no": {
                        "label": "流水号",
                        "control": "hidden",
                        "width": "10px"
                    },
                    "cust_no": {
                        "label": "客户序号",
                        "control": "hidden",
                        "width": "10px",
                        "rules": [
                            {
                                "message": "请输入客户序号"
                            }
                        ]
                    },
                    "member_cust_no": {
                        "label": "董事会成员客户编号",
                        "control": "hidden",
                        "width": "10px"
                    },
                    "cust_name": {
                        "label": "姓名",
                        "control": "string",
                        "width": "10px"
                    },
                    "is_our_cust": {
                        "label": "是否我行客户",
                        "control": "string",
                        "width": "10px",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        }
                    },
                    "is_office": {
                        "label": "是否在任",
                        "control": "string",
                        "width": "10px",
                        "rules": [
                            {
                                "require": true,
                                "message": "请选择是否在任"
                            }
                        ],
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        }
                    },
                    "board_job": {
                        "label": "董事会职务",
                        "control": "string",
                        "width": "10px",
                        "rules": [
                            {
                                "require": true,
                                "message": "请选择董事会职务"
                            }
                        ],
                        "dict": {
                            "dictType": "US_E_BOARD_JOB",
                            "dictKey": [
                                "US_E_BOARD_JOB"
                            ],
                            "format": "label"
                        }
                    },
                    "post_start": {
                        "control": "string",
                        "label": "职务任职开始",
                        "format": "yyyyMMdd",
                        "width": "10px"
                    },
                    "post_ent": {
                        "control": "string",
                        "label": "职务任职结束",
                        "format": "yyyyMMdd",
                        "width": "10px"
                    },
                    "remark": {
                        "label": "备注",
                        "control": "textarea",
                        "disabled": true,
                        "width": "10px",
                        "span": "24",
                        "autosize": {
                            "minRows": 4
                        },
                        "rules": [
                            {
                                "max": "100",
                                "message": "长度不能超过100个字符"
                            }
                        ]
                    }
                }
            ]
        }
    }
}
