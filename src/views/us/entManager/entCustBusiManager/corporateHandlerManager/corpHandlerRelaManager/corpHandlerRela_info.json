{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initDataOnce": true,
            "initRequest": "${{params}}",
            "btns": [],
            "controls": [
                {
                    "serial_no": {
                        "label": "企业高管信息流水号",
                        "control": "hidden",
                        "width": "10px"
                    },
                    "cust_name": {
                        "label": "家族成员姓名",
                        "control": "string",
                        "width": "10px"
                    },
                    "cert_type": {
                        "label": "证件类型",
                        "control": "string",
                        "width": "10px",
                        "dict": {
                            "dictType": "MS_E_CRET_TYPE",
                            "dictKey": [
                                "MS_E_CRET_TYPE"
                            ],
                            "format": "label"
                        }
                    },
                    "cert_no": {
                        "label": "证件号码",
                        "control": "string",
                        "width": "10px"
                    },
                    "family_relation": {
                        "label": "家族关系",
                        "width": "10px",
                        "control": "string",
                        "dict": {
                            "dictType": "US_E_FAMILY_TIES",
                            "dictKey": [
                                "US_E_FAMILY_TIES"
                            ],
                            "format": "label"
                        }
                    },
                    "fpro_no": {
                        "label": "家族成员所在企业贷款卡编码",
                        "control": "string",
                        "width": "10px"
                    },
                    "fcon_name": {
                        "label": "家族成员所在企业名称",
                        "control": "string",
                        "width": "10px"
                    },
                    "dutymm": {
                        "label": "任企业职务",
                        "control": "string",
                        "width": "10px",
                        "dict": {
                            "dictType": "US_E_DUTYMM",
                            "dictKey": [
                                "US_E_DUTYMM"
                            ],
                            "format": "label"
                        }
                    },
                    "remark": {
                        "label": "备注",
                        "control": "textarea",
                        "width": "10px",
                        "disabled": true,
                        "span": "24",
                        "autosize": {
                            "minRows": 4
                        }
                    }
                }
            ]
        }
    }
}
