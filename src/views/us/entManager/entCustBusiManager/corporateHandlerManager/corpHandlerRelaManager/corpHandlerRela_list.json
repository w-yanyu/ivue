{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "controls": [{
                "serial_no": {
                    "label": "企业高管信息流水号",
                    "control": "hidden",
                    "width": "130px",
                    "value": "${{params.serial_no}}",
                    "rules": [{
                        "message": "请输入企业高管信息流水号"
                    }]
                },
                "cust_name": {
                    "label": "姓名",
                    "control": "text",
                    "rules": [{
                        "max": 30,
                        "message": "长度不能超过30个字符"
                    }]
                },
                "family_relation": {
                    "label": "关系",
                    "control": "select",
                    "dict": {
                        "dictType": "US_E_FAMILY_TIES",
                        "dictKey": [
                            "US_E_FAMILY_TIES"
                        ],
                        "format": "label"
                    }
                }
            }],
            "foldLineNumber": 3
        },
        "toolbar": {
            "btns": [{
                "type": "add",
                "target": "dialog",
                "label": "新增",
                "toRequest": {
                    "method": "POST",
                    "url": "/views/us/entManager/entCustBusiManager/corporateHandlerManager/corpHandlerRelaManager/corpHandlerRela_add.json",
                    "params": {
                        "serial_no": "${{params.serial_no}}",
                        "cust_no": "${{params.cust_no}}"
                    }
                },
                "show": {
                    "expression": "'${{params.show_flash}}' == '1'"
                },
                "props": {
                    "width": "80%",
                    "title": "企业高管社会关系新增"
                }
            }]
        },
        "datagrid": {
            "isInitSearch": false,
            "scope": "searchTable",
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "us0370"
                }
            },
            "thead": [{
                    "label": "序号",
                    "type": "index",
                    "align": "center"
                },
                {
                    "label": "姓名",
                    "prop": "cust_name",
                    "align": "center"
                },
                {
                    "label": "证件类型",
                    "prop": "cert_type",
                    "align": "center",
                    "dict": {
                        "dictType": "MS_E_CRET_TYPE",
                        "dictKey": [
                            "MS_E_CRET_TYPE"
                        ],
                        "labelField": "dictName",
                        "valueField": "dictId"
                    }
                },
                {
                    "label": "证件号码",
                    "prop": "cert_no",
                    "align": "center"
                },
                {
                    "label": "家族关系",
                    "prop": "family_relation",
                    "align": "center",
                    "dict": {
                        "dictType": "US_E_FAMILY_TIES",
                        "dictKey": [
                            "US_E_FAMILY_TIES"
                        ],
                        "labelField": "dictName",
                        "valueField": "dictId"
                    }
                },
                {
                    "label": "家族成员所在企业名称",
                    "prop": "fcon_name",
                    "align": "center"
                },
                {
                    "label": "家族成员所在企业贷款卡编码",
                    "prop": "fpro_no",
                    "align": "center"
                },
                {
                    "label": "备注",
                    "prop": "remark",
                    "align": "center"
                },
                {
                    "label": "操作",
                    "fixed": "right",
                    "width": 160,
                    "align": "center",
                    "btns": [{
                            "type": "look",
                            "label": "详情",
                            "toRequest": {
                                "url": "/views/us/entManager/entCustBusiManager/corporateHandlerManager/corpHandlerRelaManager/corpHandlerRela_info.json",
                                "method": "POST",
                                "params": {
                                    "serial_no": "${{params.serial_no}}",
                                    "cust_no": "${{params.hdct_no}}"
                                }
                            },
                            "props": {
                                "width": "80%",
                                "title": "企业高管社会关系详情"
                            }
                        },
                        {
                            "type": "edit",
                            "label": "维护",
                            "toRequest": {
                                "url": "/views/us/entManager/entCustBusiManager/corporateHandlerManager/corpHandlerRelaManager/corpHandlerRela_update.json",
                                "method": "post",
                                "params": {
                                    "serial_no": "${{params.serial_no}}",
                                    "cust_no": "${{params.hdct_no}}"
                                }
                            },
                            "show": {
                                "expression": "'${{params.show_flash}}' == '1'"
                            },
                            "props": {
                                "width": "80%",
                                "title": "企业高管社会关系维护"
                            }
                        },
                        {
                            "type": "delete",
                            "label": "删除",
                            "doRequest": {
                                "url": "/SUMP/icmcall/icmRPCCall",
                                "method": "POST",
                                "params": {
                                    "servicecode": "us0373"
                                }
                            },
                            "show": {
                                "expression": "'${{params.show_flash}}' == '1'"
                            },
                            "confirm": {
                                "title": "确定删除",
                                "confirmButtonText": "确定",
                                "cancelButtonText": "取消"
                            }
                        }
                    ]
                }
            ],
            "pagination": {
                "show": true
            }
        }
    }
}
