{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "us0371"
                }
            },
            "controls": [{
                    "cust_no": {
                        "control": "text",
                        "label": "客户编号",
                        "width": "10px",
                        "disabled": true
                    },
                    "serial_no": {
                        "label": "企业高管信息流水号",
                        "control": "hidden",
                        "width": "10px",
                        "value": "${{params.serial_no}}"
                    },
                    "cust_name": {
                        "label": "家族成员姓名",
                        "control": "text",
                        "width": "10px",
                        "rules": [{
                            "required": true,
                            "message": "请输入家族成员姓名"
                        }]
                    },
                    "cert_type": {
                        "label": "证件类型",
                        "control": "select",
                        "width": "10px",
                        "rules": [{
                            "required": true,
                            "message": "请选择证件类型"
                        }],
                        "dict": {
                            "dictType": "MS_E_CRET_TYPE",
                            "dictKey": [
                                "MS_E_CRET_TYPE"
                            ],
                            "format": "label"
                        }
                    },
                    "cert_no": {
                        "label": "证件号码",
                        "control": "text",
                        "width": "10px",
                        "rules": [{
                            "required": true,
                            "message": "请输入证件号码"
                        }]
                    },
                    "family_relation": {
                        "label": "家族关系",
                        "control": "select",
                        "width": "10px",
                        "dict": {
                            "dictType": "US_E_FAMILY_TIES",
                            "dictKey": [
                                "US_E_FAMILY_TIES"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": true,
                            "message": "请选择家族关系"
                        }]
                    },
                    "fpro_no": {
                        "label": "家族成员所在企业贷款卡编码",
                        "control": "text",
                        "width": "10px",
                        "rules": [{
                            "required": true,
                            "message": "请选择家族成员所在企业贷款卡编码"
                            
                        }]
                    },
                    "fcon_name": {
                        "label": "家族成员所在企业名称",
                        "control": "text",
                        "width": "10px",
                        "rules": [{
                            "required": true,
                            "message": "请选择家族成员所在企业名称"
                        }]
                    },
                    "dutymm": {
                        "label": "任企业职务",
                        "control": "select",
                        "width": "10px",
                        "rules": [{
                            "required": true,
                            "message": "请选择任企业职务"
                        }],
                        "dict": {
                            "dictType": "US_E_DUTYMM",
                            "dictKey": [
                                "US_E_DUTYMM"
                            ],
                            "format": "label"
                        }
                    }
                },
                {
                    "remark": {
                        "label": "备注",
                        "control": "textarea",
                        "width": "10px",
                        "span": "24",
                        "autosize": {
                            "minRows": 4
                        },
                        "rules": [{
                            "max": "100",
                            "message": "长度不能超过100个字符"
                        }]
                    }
                }
            ]
        }
    }
}
