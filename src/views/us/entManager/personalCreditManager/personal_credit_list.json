{
    "module": "v_search_btn_datagrid",
    "layout": {
        "toolbar": {
        "btns": []
    },
        
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch":true ,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "ar8031",
                    "cust_no":"${{params.cust_no}}"
                }
            },
            "thead": [
                {
                    "label": "申请号",
                    "prop": "serialno",
                    "align": "center"
                },
                {
                    "label": "客户编号",
                    "prop": "cust_no",
                    "align": "center"
                },
                {
                    "label": "征信报告编号",
                    "prop": "reportId",
                    "align": "center"
                },
                {
                    "label": "查询日期",
                    "prop": "quary_date",
                    "align": "center"
                },
                {
                    "label": "查询原因",
                    "prop": "queryReason",
                    "align": "center"
                    
                },
                {
                    "label": "操作",
                    "fixed": "right",
                    "align": "center",
                    "btns": [
                      {
                        "type": "custom",
                        "label": "征信报告",
                        "toRequest": {
                          "url": "@personalCreditReport",
                          "method": "GET",
                          "params": {
                            "serialNo": "${{current.serialno}}",
                            "reportId": "${{current.reportId}}",
                            "sysflg": "${{current.sysflg}}",
                            "fileId": "${{current.fileId}}",
                            "filepath": "${{current.filepath}}",
                            "filename": "${{current.filename}}"
                          }
                        },
                        "props": {
                            "width": "80%",
                            "height": "85%",
                            "title": "征信报告"
                          }
                      }
                    ]
                  }
            ]
        }
    }
}
