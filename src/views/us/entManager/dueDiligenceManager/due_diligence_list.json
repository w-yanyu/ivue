{
    "module": "v_search_btn_datagrid",
    "layout": {
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": true,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "ar1864",
                    "cust_no": "${{params.cust_no}}"
                }
            },
            "thead": [
                {
                    "type": "index",
                    "align": "center",
                    "label": "序号"
                },
                {
                    "label": "申请编号",
                    "prop": "apply_no",
                    "align": "center"
                },
                {
                    "label": "客户编号",
                    "prop": "cust_no",
                    "align": "center"
                },
                {
                    "label": "申请日期",
                    "prop": "apply_date",
                    "align": "center"
                },
                {
                    "label": "可售产品编号",
                    "prop": "sale_prod_cd",
                    "align": "center"
                },
                {
                    "label": "业务类型",
                    "prop": "busi_type",
                    "align": "center"
                },
                {
                    "label": "申请类型",
                    "prop": "apply_type",
                    "align": "center",
                    "dict": {
                        "dictType": "PD_E_APPLY_TYPE",
                        "dictKey": [
                            "PD_E_APPLY_TYPE"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "审批状态",
                    "prop": "approve_status",
                    "align": "center",
                    "dict": {
                      "dictType": "PT_E_APPROVE_STATUS",
                      "dictKey": [
                        "PT_E_APPROVE_STATUS"
                      ],
                      "format": "label"
                    }
                  },
                {
                    "label": "操作",
                    "fixed": "right",
                    "width": 160,
                    "align": "center",
                    "btns": [
                        {
                            "type": "look",
                            "label": "尽职调查报告",
                            "toRequest": {
                                "url": "/views/pd/dueDiliTemplate/infomationCollect/infomationCollect_tree_cust.json",
                                "method": "POST"
                            },
                            "props": {
                                "width": "80%",
                                "height": "90%",
                                "title": "尽职调查报告"
                            }
                        }
                    ]
                }
            ]
        }
    }
}