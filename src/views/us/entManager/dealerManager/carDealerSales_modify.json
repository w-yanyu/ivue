{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "scope": "formData",
      "initRequest": "${{params}}",
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "us0523",
          "cust_no": "${{params.cust_no}}"
        }
      },
      "controls": [
        {
          "cust_no": {
            "control": "hidden",
            "label": "客户编号",
            "span": 8
          },
          "car_purchase_date": {
            "control": "dateTimePicker",
            "label": "提车年月",
            "span": 8,
            "format": "yyyyMM",
            "type": "month",
            "disabled": true,
            "rules": [
              {
                "required": true,
                "message": "请输入提车年月"
              }
            ]
          },
          "car_purchases": {
            "control": "text",
            "label": "提车量(辆)",
            "span": 8,
            "rules": [
              {
                "type": "number",
                "message": "请输入整数"
              },
              {
                "required": true,
                "message": "请输入提车量"
              }
            ]
          },
          "car_model_no": {
            "control": "lookup",
            "label": "车型编号",
            "span": 8,
            "keyField":{
              "car_model_no":"car_model_no",
              "car_model_name":"car_model_name"
            },
            "toRequest": {
              "url": "@CarModelChooser",
              "method": "post"
            },
            "props": {
              "width": "70%",
              "title": "车型选择"
            },
            "eventAfter": {
              "doParentCallBack": true
            },
            "rules": [
              {
                "required": true,
                "message": "请输入车型编号"
              }
            ]
          },
          "car_model_name": {
            "control": "text",
            "label": "车型名称",
            "span": 8,
            "rules": [
              {
                "required": true,
                "message": "请输入车型名称"
              }
            ]
          },
          "car_purchase_amt": {
            "control": "currency",
            "label": "提车金额(万元)",
            "span": 8,
            "format": {
              "thousand": ",",
              "decimal": 2
            },
            "rules": [
              {
                "required": true,
                "message": "请输入提车金额"
              }
            ]
          },
          "actual_sales": {
            "control": "text",
            "label": "实销量(辆)",
            "span": 8,
            "rules": [
              {
                "type": "number",
                "message": "请输入整数"
              },
              {
                "required": true,
                "message": "请输入实销量"
              }
            ]
          },
          "actual_amt": {
            "control": "currency",
            "label": "实销金额(万元)",
            "span": 8,
            "format": {
              "thousand": ",",
              "decimal": 2
            },
            "rules": [
              {
                "required": true,
                "message": "请输入实销金额"
              }
            ]
          },
          "ranking": {
            "control": "text",
            "label": "实销排名",
            "span": 8,
            "rules": [
              {
                "type": "number",
                "message": "请输入整数"
              },
              {
                "required": true,
                "message": "请输入实销排名"
              }
            ]
          },
          "inventory": {
            "control": "text",
            "label": "库存量(辆)",
            "span": 8,
            "rules": [
              {
                "type": "number",
                "message": "请输入整数"
              },
              {
                "required": true,
                "message": "请输入库存量"
              }
            ]
          },
          "inventory_amt": {
            "control": "currency",
            "label": "库存金额(万元)",
            "span": 8,
            "format": {
              "thousand": ",",
              "decimal": 2
            },
            "rules": [
              {
                "required": true,
                "message": "请输入库存金额"
              }
            ]
          },
          "inventory_ratio": {
            "control": "currency",
            "label": "存销比(%)",
            "span": 8,
            "compute": "${{formData.inventory}}/${{formData.actual_sales}}*100",
            "disabled": true,
            "format": {
              "thousand": ",",
              "decimal": 2
            },
            "rules": [
              {
                "required": true,
                "message": "请输入存销比"
              }
            ]
          }
        }
      ]
    }
  }
}
