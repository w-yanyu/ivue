{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "car_purchase_date": {
            "control": "dateTimePicker",
            "label": "提车年月",
            "span": 12,
            "format": "yyyyMM",
            "type": "month"
          },
          "car_model_no": {
            "control": "text",
            "label": "车型编号",
            "span": 12
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "target": "dialog",
          "label": "新增",
          "toRequest": {
            "method": "GET",
            "url": "/views/us/entManager/dealerManager/carDealerSales_add.json",
            "params": {
              "cust_no": "${{params.cust_no}}"
            }
          },
          "show": {
            "expression": "'${{params.show_flash}}' == '1'"
          },
          "props": {
            "width": "60%",
            "title": "新增经销商销售信息"
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "us0521",
          "cust_no": "${{params.cust_no}}"
        }
      },
      "thead": [
        {
          "type": "string",
          "label": "提车年月",
          "prop": "car_purchase_date",
          "align": "center"
        },
        {
          "type": "string",
          "label": "提车量(辆)",
          "prop": "car_purchases",
          "align": "center"
        },
        {
          "type": "string",
          "label": "车型编号",
          "prop": "car_model_no",
          "align": "center"
        },
        {
          "type": "string",
          "label": "车型名称",
          "prop": "car_model_name",
          "align": "center"
        },
        {
          "type": "string",
          "label": "提车金额(万元)",
          "prop": "car_purchase_amt",
          "align": "center",
          "format": {
            "thousand": ",",
            "decimal": 2
          }
        },
        {
          "type": "string",
          "label": "实销量(辆)",
          "prop": "actual_sales",
          "align": "center"
        },
        {
          "type": "string",
          "label": "实销金额(万元)",
          "prop": "actual_amt",
          "align": "center",
          "format": {
            "thousand": ",",
            "decimal": 2
          }
        },
        {
          "type": "string",
          "label": "实销排名",
          "prop": "ranking",
          "align": "center"
        },
        {
          "type": "string",
          "label": "库存量(辆)",
          "prop": "inventory",
          "align": "center"
        },
        {
          "type": "string",
          "label": "库存金额(万元)",
          "prop": "inventory_amt",
          "align": "center",
          "format": {
            "thousand": ",",
            "decimal": 2
          }
        },
        {
          "type": "string",
          "label": "存销比(%)",
          "prop": "inventory_ratio",
          "align": "center",
          "format": {
            "thousand": ",",
            "decimal": 2
          }
        },
        {
          "label": "操作",
          "align": "center",
          "width": "200px",
          "fixed": "right",
          "btns": [
            {
              "type": "look",
              "label": "详情",
              "toRequest": {
                "url": "/views/us/entManager/dealerManager/carDealerSales_info.json",
                "method": "GET",
                "params": {
                }
              },
              "props": {
                "width": "60%",
                "title": "经销商销售信息详情"
              }
            },
            {
              "type": "look",
              "label": "维护",
              "toRequest": {
                "url": "/views/us/entManager/dealerManager/carDealerSales_modify.json",
                "method": "GET",
                "params": {
                }
              },
              "show": {
                "expression": "'${{params.show_flash}}' == '1'"
              },
              "props": {
                "width": "60%",
                "title": "维护经销商销售信息"
              }
            },
            {
              "type": "custom",
              "label": "删除",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "us0524"
                }
              },
              "show": {
                "expression": "'${{params.show_flash}}' == '1'"
              },
              "confirm": {
                "title": "删除",
                "msg": "确定删除吗？",
                "confirmButtonText": "确定",
                "cancelButtonText": "取消"
              }
            }
          ]
        }
      ]
    }
  }
}
