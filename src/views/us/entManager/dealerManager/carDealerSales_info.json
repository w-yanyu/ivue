{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "btns": [],
      "controls": [
        {
          "cust_no": {
            "control": "hidden",
            "label": "客户编号",
            "span": 8
          },
          "car_purchase_date": {
            "control": "dateTimePicker",
            "label": "提车年月",
            "span": 8,
            "format": "yyyyMM",
            "type": "month",
            "disabled": true
          },
          "car_purchases": {
            "control": "string",
            "label": "提车量(辆)",
            "span": 8
          },
          "car_model_no": {
            "control": "string",
            "label": "车型编号",
            "span": 8
          },
          "car_model_name": {
            "control": "string",
            "label": "车型名称",
            "span": 8
          },
          "car_purchase_amt": {
            "control": "currency",
            "label": "提车金额(万元)",
            "span": 8,
            "disabled": true,
            "format": {
              "thousand": ",",
              "decimal": 2
            }
          },
          "actual_sales": {
            "control": "string",
            "label": "实销量(辆)",
            "span": 8
          },
          "actual_amt": {
            "control": "currency",
            "label": "实销金额(万元)",
            "span": 8,
            "disabled": true,
            "format": {
              "thousand": ",",
              "decimal": 2
            }
          },
          "ranking": {
            "control": "string",
            "label": "实销排名",
            "span": 8
          },
          "inventory": {
            "control": "string",
            "label": "库存量(辆)",
            "span": 8
          },
          "inventory_amt": {
            "control": "currency",
            "label": "库存金额(万元)",
            "span": 8,
            "disabled": true,
            "format": {
              "thousand": ",",
              "decimal": 2
            }
          },
          "inventory_ratio": {
            "control": "currency",
            "label": "存销比(%)",
            "span": 8,
            "disabled": true,
            "format": {
              "thousand": ",",
              "decimal": 2
            }
          }
        }
      ]
    }
  }
}
