{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "us1125",
          "cust_no": "${{params.cust_no}}"
        }
      },
      "controls": [
        {
          "years": {
            "control": "dateTimePicker",
            "label": "评级年度",
            "span": 12,
            "width": "25px",
            "type": "year",
            "disabled": true
          },
          "taxpayer_identifier_no": {
            "control": "text",
            "span": 12,
            "width": "25px",
            "label": "纳税人识别号"
          },
          "credit_rating_level": {
            "control": "text",
            "span": 12,
            "width": "25px",
            "label": "纳税信用评级"
          },
          "rating_unit": {
            "control": "text",
            "span": 12,
            "width": "25px",
            "label": "评价单位"
          }
        }
      ]
    }
  }
}
