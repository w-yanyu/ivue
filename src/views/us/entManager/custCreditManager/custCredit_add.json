{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "us0529",
          "cust_no": "${{params.cust_no}}"
        }
      },
      "controls": [
        {
          "credit_no": {
            "control": "hidden",
            "label": "信用编号",
            "span": 24
          },
          "cust_no": {
            "control": "hidden",
            "label": "客户编号",
            "span": 24
          },
          "credit_record": {
            "control": "textarea",
            "label": "人行征信记录",
            "span": 24,
            "autosize":{"minRows": 4,"maxRows":6}
          },
          "inner_credit_rating": {
            "control": "textarea",
            "label": "本公司信用评级",
            "span": 24,
            "autosize":{"minRows": 4,"maxRows":6},
            "rules": [
              {
                "required": true,
                "message": "请输入本公司信用评级"
              }
            ]
          },
          "busi_sys_record": {
            "control": "textarea",
            "label": "工商系统记录",
            "span": 24,
            "autosize":{"minRows": 4,"maxRows":6}
          },
          "tax_sys_record": {
            "control": "textarea",
            "label": "水务系统记录",
            "span": 24,
            "autosize":{"minRows": 4,"maxRows":6},
            "rules": [
              {
                "required": true,
                "message": "请输入水务系统记录"
              }
            ]
          },
          "social_security_sys_record": {
            "control": "textarea",
            "label": "社保系统记录",
            "span": 24,
            "autosize":{"minRows": 4,"maxRows":6},
            "rules": [
              {
                "required": true,
                "message": "请输入社保系统记录"
              }
            ]
          }
        }
      ]
    }
  }
}
