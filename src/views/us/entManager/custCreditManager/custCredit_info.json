{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "scope": "formData",
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "us0531",
          "cust_no": "${{params.cust_no}}"
        }
      },
      "btns": [
        {
          "type": "custom",
          "label": "修改",
          "toRequest": {
            "url": "/views/us/entManager/custCreditManager/custCredit_add.json",
            "method": "GET",
            "params": {
              "cust_no": "${{params.cust_no}}",
              "credit_no": "${{formData.credit_no}}",
              "credit_record": "${{formData.credit_record}}",
              "inner_credit_rating": "${{formData.inner_credit_rating}}",
              "busi_sys_record": "${{formData.busi_sys_record}}",
              "tax_sys_record": "${{formData.tax_sys_record}}",
              "social_security_sys_record": "${{formData.social_security_sys_record}}"
            }
          },
          "show": {
            "expression": "'${{params.show_flash}}' == '1'"
          },
          "eventAfter": {
            "callback": "resetCallback"
          },
          "props": {
            "width": "80%",
            "title": "客户信用信息维护"
          }
        }
      ],
      "controls": [
        {
          "credit_no": {
            "control": "hidden",
            "label": "信用编号",
            "span": 24,
            "disabled": true
          },
          "credit_record": {
            "control": "textarea",
            "label": "人行征信记录",
            "span": 24,
            "disabled": true,
            "autosize": {
              "minRows": 4,
              "maxRows": 6
            }
          },
          "inner_credit_rating": {
            "control": "textarea",
            "label": "本公司信用评级",
            "span": 24,
            "disabled": true,
            "autosize": {
              "minRows": 4,
              "maxRows": 6
            }
          },
          "busi_sys_record": {
            "control": "textarea",
            "label": "工商系统记录",
            "span": 24,
            "disabled": true,
            "autosize": {
              "minRows": 4,
              "maxRows": 6
            }
          },
          "tax_sys_record": {
            "control": "textarea",
            "label": "水务系统记录",
            "span": 24,
            "disabled": true,
            "autosize": {
              "minRows": 4,
              "maxRows": 6
            }
          },
          "social_security_sys_record": {
            "control": "textarea",
            "label": "社保系统记录",
            "span": 24,
            "disabled": true,
            "autosize": {
              "minRows": 4,
              "maxRows": 6
            }
          }
        }
      ]
    }
  }
}
