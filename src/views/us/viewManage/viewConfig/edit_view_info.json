{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/call/RPCCall",
        "method": "POST",
        "params": {
          "servicecode": "us1136",
          "info_item_id": "${{params.info_item_id}}",
          "sort_no": "${{params.sort_no}}"
        }
      },
      "doRequest": {
        "url": "/SUMP/call/RPCCall",
        "method": "POST",
        "params": {
          "servicecode": "us1138",
          "parent_node_cd": "${{params.parent_node_cd}}",
          "node_type": "${{params.node_type}}"
        }
      },
      "controls": [
        {
          "info_item_id": {
            "label": "信息项编号",
            "control": "text",
            "span": 12,
            "disabled": true,
            "rules": [
              {
                "required": true,
                "message": "信息项编号不能为空"
              },
              {
                "max": 64,
                "message": "信息项编号长度不能超过64个字符"
              }
            ]
          },
          "info_item_name": {
            "label": "信息项名称",
            "control": "text",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "信息项名称不能为空"
              },
              {
                "max": 500,
                "message": "信息项名称长度不能超过500个字符"
              }
            ]
          }
        },
        {
          "sort_no": {
            "label": "信息项序号",
            "control": "inputNumber",
            "span": 12,
            "value": "${{params.sort_no}}",
            "rules": [
              {
                "required": true,
                "message": "信息项序号不能为空"
              }
            ]
          },
          "icon": {
            "label": "图标",
            "control": "text",
            "span": 12,
            "value": "${{params.icon}}",
            "rules": [
              {
                "required": true,
                "message": "图标不能为空"
              }
            ]
          }
        },
        {
          "info_thumbnail_item_page": {
            "label": "概要信息页面",
            "control": "text",
            "span": 24,
            "rules": [
              {
                "required": true,
                "message": "概要信息页面不能为空"
              },
              {
                "max": 750,
                "message": "概要信息页面长度不能超过750个字符"
              }
            ]
          }
        },
        {
          "info_item_page": {
            "label": "详细信息页面",
            "control": "text",
            "span": 24,
            "rules": [
              {
                "required": true,
                "message": "详细信息页面不能为空"
              },
              {
                "max": 750,
                "message": "详细信息页面长度不能超过750个字符"
              }
            ]
          }
        }
      ]
    }
  }
}
