{
  "module": "v_search_btn_datagrid",
  "layout": {
    "toolbar": {
      "btns": []
    },
    "datagrid": {
      "isInitSearch": false,
      "scope": "searchTable",
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "us0138",
          "cust_no": "${{params.cust_no}}"
        }
      },
      "thead": [
        {
          "label": "资产名称",
          "align": "center",
          "prop": "asset_name"
        },
        {
          "label": "资产种类",
          "prop": "asset_type",
          "align": "center",
          "dict": {
            "dictType": "US_E_ASSET_TYPE",
            "dictKey": [
              "US_E_ASSET_TYPE"
            ],
            "format": "label"
          }
        },
        {
          "type":"currency",
          "label": "资产评估金额",
          "align": "center",
          "prop": "asset_value",
          "format": {
            "thousand": ",",
            "decimal": "2",
            "type": "currency"
        }
        },
        {
          "label": "资产评估币种",
          "prop": "asset_ccy",
          "align": "center",
          "dict": {
            "dictType": "MS_CURRENCY",
            "dictKey": [
              "MS_CURRENCY"
            ],
            "format": "label"
          }
        },
        {
          "label": "资产描述",
          "align": "center",
          "prop": "asset_desc"
        }
      ],
      "pagination": {
        "show": true
      }
    }
  }
}
