{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "name": "info",
      "initDataOnce": true,
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "us0164",
          "cust_no": "${{params.cust_no}}"
        }
      },
      "controls": [
        {
          "fund_company": {
            "control": "text",
            "label": "公积金缴存单位名称",
            "span": "8",
            "width": "25px",
            "disabled": true
          },
          "fund_base": {
            "control": "select",
            "label": "基数（元）",
            "span": "8",
            "width": "25px",
            "disabled": true
          },
          "fund_paymentamount": {
            "control": "text",
            "label": "缴纳金额（元）",
            "span": "8",
            "width": "25px",
            "disabled": true
          },
          "fund_paymentdate": {
            "control": "text",
            "label": "缴纳日期",
            "span": "8",
            "width": "25px",
            "disabled": true
          },
          "medical_insurance": {
            "control": "text",
            "label": "医疗保险（元）",
            "span": "8",
            "width": "25px",
            "disabled": true
          },
          "endow_insurance": {
            "control": "text",
            "label": "养老保险（元）",
            "span": "8",
            "width": "25px",
            "disabled": true
          },
          "matern_insurance": {
            "control": "text",
            "label": "生育保险（元）",
            "span": "8",
            "width": "25px",
            "disabled": true
          },
          "unemploy_insurance": {
            "control": "text",
            "label": "失业保险（元）",
            "span": "8",
            "width": "25px",
            "disabled": true
          },
          "injury_insuranc": {
            "control": "text",
            "label": "工伤保险（元）",
            "span": "8",
            "width": "25px",
            "disabled": true
          },
          "insur_paymentdate": {
            "control": "text",
            "label": "缴费日期",
            "span": "8",
            "width": "25px",
            "disabled": true
          }
        }
      ]
    }
  }
}
