{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "controls": [],
            "btns": []
        },
      "toolbar": {
        "btns": []
      },
        "datagrid": {
            "isInitSearch": true,
            "scope": "searchTable",
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "us0160",
                    "cust_no": "${{params.cust_no}}"
                }
            },
            "thead": [
                {
                    "label": "公积金缴存单位名称",
                    "prop": "fund_company"

                },
                {
                    "type":"currency",
                    "label": "基数",
                    "align": "center",
                    "prop": "fund_base",
                    "format": {
                      "thousand": ",",
                      "decimal": "2",
                      "type": "currency"
                    }
                },
                {
                    "type":"currency",
                    "label": "缴纳金额",
                    "align": "center",
                    "prop": "fund_paymentamount",
                    "format": {
                      "thousand": ",",
                      "decimal": "2",
                      "type": "currency"
                    }
                },
                {
                    "label": "缴纳日期",
                    "prop": "fund_paymentdate"
                }
            ],
            "pagination": {
                "show": true
            }
        }
    }
}
