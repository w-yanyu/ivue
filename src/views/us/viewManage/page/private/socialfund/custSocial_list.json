{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "controls": [],
            "btns": []
        },
      "toolbar": {
        "btns": []
      },
        "datagrid": {
            "isInitSearch": true,
            "scope": "searchTable",
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "us0156",
                    "cust_no": "${{params.cust_no}}"
                }
            },
            "thead": [
                {
                    "type":"currency",
                    "label": "医疗保险",
                    "align": "center",
                    "prop": "medical_insurance",
                    "format": {
                      "thousand": ",",
                      "decimal": "2",
                      "type": "currency"
                    }
                },
                {
                    "type":"currency",
                    "label": "养老保险",
                    "align": "center",
                    "prop": "endow_insurance",
                    "format": {
                      "thousand": ",",
                      "decimal": "2",
                      "type": "currency"
                    }
                },
                {
                    "type":"currency",
                    "label": "生育保险",
                    "align": "center",
                    "prop": "matern_insurance",
                    "format": {
                      "thousand": ",",
                      "decimal": "2",
                      "type": "currency"
                    }
                },
                {
                    "type":"currency",
                    "label": "失业保险",
                    "align": "center",
                    "prop": "unemploy_insurance",
                    "format": {
                      "thousand": ",",
                      "decimal": "2",
                      "type": "currency"
                    }
                },
                {
                    "type":"currency",
                    "label": "工伤保险",
                    "align": "center",
                    "prop": "injury_insuranc",
                    "format": {
                      "thousand": ",",
                      "decimal": "2",
                      "type": "currency"
                    }
                },
                {
                    "label": "缴纳日期",
                    "prop": "insur_paymentdate"
                }
            ],
            "pagination": {
                "show": true
            }
        }
    }
}
