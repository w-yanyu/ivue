{
  "module": "dynamicTabs",
  "layout": {
    "tabs": {
      "scope": "formDate",
      "tabHeader": [
        {
          "id": "violations",
          "label": "社保信息",
          "closable": false,
          "disabled": false,
          "icon": "el-icon-date",
          "toRequest": {
            "url": "/views/us/viewManage/page/private/socialfund/custSocial_list.json",
            "method": "post",
            "params": {
              "cust_id": "${{params.cust_no}}"
            }
          }
        },
        {
          "id": "majorEvents",
          "label": "公积金信息",
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "toRequest": {
            "url": "/views/us/viewManage/page/private/socialfund/custFund_list.json",
            "method": "post",
            "params": {
              "cust_id": "${{params.cust_no}}"
            }
          }
        }
      ],
      "defaultActiveKey": "violations",
      "position": "top",
      "styleClass": "card"
    }
  }
}
