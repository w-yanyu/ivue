{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "name": "info",
      "initDataOnce": true,
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "us0007",
          "cust_no": "${{params.cust_no}}"
        }
      },
      "btns": [
        {
            "type": "close",
            "label": "关闭"
        }
      ],
      "controls": [
        {
          "family_addr": {
            "control": "areaCascade",
            "title": "请选择家庭地址",
            "span": "8",
            "width": "25px",
            "label": "家庭地址",
            "disabled": true
          },
          "family_addr_details": {
            "control": "text",
            "label": "家庭地址详细信息",
            "span": "8",
            "width": "25px",
            "disabled": true
          },
          "family_zip": {
            "control": "text",
            "label": "家庭地址邮编（征）",
            "span": "8",
            "width": "25px",
            "disabled": true
          },
          "family_phone": {
            "control": "text",
            "label": "家庭电话（征）",
            "span": "8",
            "width": "25px",
            "disabled": true
          },
          "fend_persons": {
            "control": "text",
            "label": "家庭供养人数（人）",
            "span": "8",
            "width": "25px",
            "disabled": true
          },
          "is_family_live": {
            "control": "select",
            "label": "家人是否在本地居住",
            "span": "8",
            "width": "25px",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "labelField": "dictName",
              "valueField": "dictId"
            },
            "disabled": true
          },
          "is_own_children": {
            "control": "select",
            "label": "是否有子女",
            "span": "8",
            "width": "25px",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "labelField": "dictName",
              "valueField": "dictId"
            },
            "showItem": {
              "0": [],
              "1": [
                "children_condition"
              ]
            },
            "disabled": true
          },
          "children_condition": {
            "control": "text",
            "label": "子女情况",
            "span": "8",
            "width": "25px",
            "disabled": true
          },
          "housing_desc": {
            "control": "text",
            "label": "住房情况",
            "span": "8",
            "width": "25px",
            "disabled": true
          },
          "vehicle_desc": {
            "control": "text",
            "label": "车辆信息",
            "span": "8",
            "width": "25px",
            "disabled": true
          },
          "family_mth_Income": {
            "control": "currency",
            "label": "家庭月均收入(元）",
            "align": "left",
            "span": "8",
            "width": "25px",
            "disabled": true,
            "format":{
              "thousand":","
            }
          },
          "major_income_source": {
            "control": "text",
            "label": "主要收入来源",
            "span": "8",
            "width": "25px",
            "disabled": true
          },
          "family_tota": {
            "control": "currency",
            "label": "家庭总资产（元）",
            "align": "left",
            "span": "8",
            "width": "25px",
            "disabled": true,
            "format":{
              "thousand":","
            }
          },
          "family_everyday_expend": {
            "control": "currency",
            "label": "现有家庭日常支出（按月计）（元）",
            "align": "left",
            "span": "8",
            "width": "25px",
            "disabled": true,
            "format":{
              "thousand":","
            }
          },
          "family_debt_out": {
            "control": "currency",
            "label": "现有家庭月债务支出（元）",
            "align": "left",
            "span": "8",
            "width": "25px",
            "disabled": true,
            "format":{
              "thousand":","
            }
          },
          "family_mth_expend": {
            "control": "currency",
            "label": "家庭月总支出（元）",
            "align": "left",
            "span": "8",
            "width": "25px",
            "disabled": true,
            "format":{
              "thousand":","
            }
          },
          "native_prop": {
            "control": "select",
            "label": "户籍性质",
            "span": "8",
            "width": "25px",
            "dict": {
              "dictType": "US_E_NATIVE_PROP",
              "dictKey": [
                "US_E_NATIVE_PROP"
              ],
              "labelField": "dictName",
              "valueField": "dictId"
            },
            "disabled": true
          },
          "native_struct_area": {
            "control": "text",
            "label": "目前住宅建筑面积（㎡)",
            "span": "8",
            "width": "25px",
            "disabled": true
          },
          "generally_health": {
            "control": "select",
            "label": "健康状况",
            "span": "8",
            "width": "25px",
            "dict": {
              "dictType": "US_E_GENERALLY_HEALTH",
              "dictKey": [
                "US_E_GENERALLY_HEALTH"
              ],
              "labelField": "dictName",
              "valueField": "dictId"
            },
            "disabled": true
          },
          "job_status": {
            "control": "select",
            "label": "就业状态",
            "span": "8",
            "width": "25px",
            "dict": {
              "dictType": "US_E_JOB_STATUS",
              "dictKey": [
                "US_E_JOB_STATUS"
              ],
              "labelField": "dictName",
              "valueField": "dictId"
            },
            "disabled": true
          },
          "bank_loans_count": {
            "control": "text",
            "label": "银行贷款总笔数",
            "span": "8",
            "width": "25px",
            "disabled": true
          },
          "family_liability": {
            "control": "currency",
            "label": "家庭总负债（元）",
            "align": "left",
            "span": "8",
            "width": "25px",
            "disabled": true,
            "format":{
              "thousand":","
            }
          },
          "outsec_total_amount": {
            "control": "currency",
            "label": "对外担保总额（元）",
            "align": "left",
            "span": "8",
            "width": "25px",
            "disabled": true,
            "format":{
              "thousand":","
            }
          },
          "unit_contact": {
            "control": "text",
            "label": "单位联系人",
            "span": "8",
            "width": "25px",
            "disabled": true
          },
          "employ_start": {
            "control": "text",
            "###type": "year",
            "label": "参加工作年限（年）",
            "span": "8",
            "width": "25px",
            "disabled": true
          },
          "employ_years": {
            "control": "text",
            "label": "目前工作持续年限（年）",
            "span": "8",
            "width": "25px",
            "disabled": true
          },
          "other_income": {
            "control": "text",
            "label": "其他收入来源",
            "span": "8",
            "width": "25px",
            "disabled": true
          }
        }
      ]
    }
  }
}
