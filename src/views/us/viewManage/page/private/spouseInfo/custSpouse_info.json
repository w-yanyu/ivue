{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "name": "info",
      "initDataOnce": true,
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "us0007",
          "cust_no": "${{params.cust_no}}"
        }
      },
      "btns": [
        {
            "type": "close",
            "label": "关闭"
        }
      ],
      "controls": [
        {
          "spouse_name": {
            "control": "text",
            "label": "配偶姓名",
            "span": "8",
            "width": "25px",
            "disabled": true
          },
          "spouse_cert_type": {
            "control": "select",
            "label": "配偶证件类型",
            "span": "8",
            "width": "25px",
            "dict": {
              "dictType": "MS_E_CRET_TYPE",
              "dictKey": [
                "MS_E_CRET_TYPE"
              ],
              "labelField": "label",
              "valueField": "value"
            },
            "disabled": true
          },
          "spouse_cert_no": {
            "control": "text",
            "label": "配偶证件号码",
            "span": "8",
            "width": "25px",
            "disabled": true
          },
          "spouse_phone": {
            "control": "text",
            "label": "配偶电话",
            "span": "8",
            "width": "25px",
            "disabled": true
          },
          "spouse_work_unit_name": {
            "control": "text",
            "label": "配偶工作单位名称",
            "span": "8",
            "width": "25px",
            "disabled": true
          },
          "spouse_work_phone": {
            "control": "text",
            "label": "配偶工作单位电话",
            "span": "8",
            "width": "25px",
            "disabled": true
          }
        }
      ]
    }
  }
}
