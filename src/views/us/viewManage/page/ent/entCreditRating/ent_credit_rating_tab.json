{
  "module": "dynamicTabs",
  "layout": {
    "tabs": {
      "defaultActiveKey": "tab1",
      "position": "top",
      "styleClass": "card",
      "tabHeader": [
        {
          "id": "tab1",
          "label": "行内信评",
          "closable": false,
          "disabled": false,
          "icon": "el-icon-date",
          "toRequest": {
            "url": "/views/us/entManager/ratingManager/rating_list.json",
            "method": "POST",
            "params": {
              "show_flash": "${{params.show_flash}}",
              "cust_no": "${{params.cust_no}}"
            }
          }
        },
        {
          "id": "tab2",
          "label": "税务信评",
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "toRequest": {
            "url": "/views/us/entManager/ratingManager/tax_credit_rating_manage.json",
            "method": "POST",
            "params": {
              "show_flash": "${{params.show_flash}}",
              "cust_no": "${{params.cust_no}}"
            }
          }
        }
      ]
    }
  }
}
