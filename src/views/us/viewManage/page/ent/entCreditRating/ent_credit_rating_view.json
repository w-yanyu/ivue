{
  "module": "v_search_btn_datagrid",
  "layout": {
    "toolbar": {
      "btns": []
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "cr1023",
          "cust_no": "${{params.cust_no}}"
        }
      },
      "thead": [
        {
          "label": "申请编号",
          "prop": "appl_id",
          "align": "center"
        },
        {
          "label": "客户编号",
          "prop": "cust_no",
          "align": "center"
        },
        {
          "label": "评级总分",
          "prop": "eval_sc",
          "align": "center"
        },
        {
          "label": "认定等级",
          "prop": "fl_rank",
          "align": "center"
        },
        {
          "label": "评级状态",
          "prop": "flag",
          "align": "center",
          "dict": {
            "dictType": "PL_E_STATUS",
            "dictKey": [
              "PL_E_STATUS"
            ],
            "format": "label"
          }
        },
        {
          "label": "评级日期",
          "prop": "eval_dt",
          "align": "center"
        }
      ]
    }
  }
}
