{
  "module": "dynamicTabs",
  "layout": {
    "tabs": {
      "defaultActiveKey": "tab1",
      "position": "left",
      "styleClass": "card",
      "tabHeader": [
        {
          "id": "tab1",
          "label": "营业收入",
          "closable": false,
          "disabled": false,
          "icon": "el-icon-date",
          "toRequest": {
            "url": "@financialEchartsView",
            "method": "POST",
            "params": {
              "show_flash":"${{params.show_flash}}",
              "simpleFinancialType": "1",
              "title": "营业收入",
              "cust_no": "${{params.cust_no}}"
            }
          },
          "props": {
            "height": "400px"
          }
        },
        {
          "id": "tab2",
          "label": "净利润",
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "toRequest": {
            "url": "@financialEchartsView",
            "method": "POST",
            "params": {
              "show_flash":"${{params.show_flash}}",
              "simpleFinancialType": "2",
              "title": "净利润",
              "cust_no": "${{params.cust_no}}"
            }
          }
        },
        {
          "id": "tab3",
          "label": "总资产",
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "toRequest": {
            "url": "@financialEchartsView",
            "method": "POST",
            "params": {
              "show_flash":"${{params.show_flash}}",
              "simpleFinancialType": "3",
              "title": "总资产",
              "cust_no": "${{params.cust_no}}"
            }
          }
        },
        {
          "id": "tab4",
          "label": "净资产",
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "toRequest": {
            "url": "@financialEchartsView",
            "method": "POST",
            "params": {
              "show_flash":"${{params.show_flash}}",
              "simpleFinancialType": "4",
              "title": "净资产",
              "cust_no": "${{params.cust_no}}"
            }
          }
        },
        {
          "id": "tab5",
          "label": "经营现金流量净额",
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "toRequest": {
            "url": "@financialEchartsView",
            "method": "POST",
            "params": {
              "show_flash":"${{params.show_flash}}",
              "simpleFinancialType": "5",
              "title": "经营现金流量净额",
              "cust_no": "${{params.cust_no}}"
            }
          }
        }
      ]
    }
  }
}
