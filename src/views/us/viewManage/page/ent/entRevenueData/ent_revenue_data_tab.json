{
  "module": "dynamicTabs",
  "layout": {
    "tabs": {
      "defaultActiveKey": "tab1",
      "position": "top",
      "styleClass": "card",
      "tabHeader": [
        {
          "id": "tab1",
          "label": "营业收入",
          "closable": false,
          "disabled": false,
          "icon": "el-icon-date",
          "toRequest": {
            "url": "@cardLayout",
            "method": "POST",
            "params": {
              "show_flash": "${{params.show_flash}}",
              "cust_no": "${{params.cust_no}}",
              "revenue_data_type": "1"
            }
          }
        },
        {
          "id": "tab2",
          "label": "营业成本",
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "toRequest": {
            "url": "@cardLayout",
            "method": "POST",
            "params": {
              "show_flash": "${{params.show_flash}}",
              "cust_no": "${{params.cust_no}}",
              "revenue_data_type": "2"
            }
          }
        },
        {
          "id": "tab3",
          "label": "利润比例",
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "toRequest": {
            "url": "@cardLayout",
            "method": "POST",
            "params": {
              "show_flash": "${{params.show_flash}}",
              "cust_no": "${{params.cust_no}}",
              "revenue_data_type": "3"
            }
          }
        }
      ]
    }
  }
}
