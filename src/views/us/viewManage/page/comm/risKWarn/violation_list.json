{
    "module": "v_search_btn_datagrid",
    "layout": {
        "toolbar": {
            "btns": []
        },
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": true,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "us0350",
                    "cust_no": "${{params.cust_no}}"
                }
            },
            "thead": [{
                    "type": "index",
                    "align": "center",
                    "label": "序号"
                },
                {
                    "label": "违规种类",
                    "prop": "violtp",
                    "align": "center",
                    "dict": {
                        "dictType": "US_E_VIOLATION_TYPE",
                        "dictKey": [
                            "US_E_VIOLATION_TYPE"
                        ],
                        "labelField": "dictName",
                        "valueField": "dictId"
                    }
                },
                {
                    "label": "违规日期",
                    "prop": "violation_date",
                    "align": "center"
                },
                {
                    "label": "违规说明",
                    "prop": "violation_declare",
                    "align": "center"
                },
                {
                    "label": "讨债金额（元）",
                    "prop": "violation_amt",
                    "align": "center",
                    "format":{
                        "thousand":","
                      }
                }
            ]
        }
    }
}
