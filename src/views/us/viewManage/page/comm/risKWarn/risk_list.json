{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "controls": [],
            "btns": []
        },
      "toolbar": {
        "btns": []
      },
        "datagrid": {
            "isInitSearch": true,
            "scope": "searchTable",
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "us1166",
                    "risk_list_status": "1",
                    "cust_no": "${{params.cust_no}}",
                    "cert_type": "${{params.cert_type}}",
                    "cert_no": "${{params.cert_no}}",
                    "apply_cust_type": "${{params.apply_cust_type}}"
                }
            },
            "thead": [
                {
                    "label": "客户名称",
                    "prop": "cust_name",
                    "width": "100px"

                },
                {
                    "label": "名单类型",
                    "prop": "risk_list_type",
                    "dict": {
                        "dictType": "US_E_RISK_LIST_TYPE",
                        "dictKey": [
                            "US_E_RISK_LIST_TYPE"
                        ]
                    }
                },
                {
                    "label": "入库日期",
                    "prop": "effect_date"
                },
                {
                    "label": "入库原因",
                    "prop": "risk_list_reason"
                },
                {
                    "label": "来源系统",
                    "prop": "risk_list_from",
                    "dict": {
                        "dictType": "US_E_RISK_LIST_FROM",
                        "dictKey": [
                            "US_E_RISK_LIST_FROM"
                        ]
                    }
                }
            ],
            "pagination": {
                "show": true
            }
        }
    }
}
