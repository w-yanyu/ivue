{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [],
      "btns": []
    },
    "toolbar": {
      "btns": []
    },
    "datagrid": {
      "isInitSearch": true,
      "scope": "searchTable",
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "us0073",
          "cust_no": "${{params.cust_no}}"
        }
      },
      "thead": [
        {
          "label": "事件名称",
          "prop": "event_name"
        },
        {
          "label": "事件类型",
          "prop": "event_type",
          "dict": {
            "dictType": "US_E_EVENT_TYPE",
            "dictKey": [
              "US_E_EVENT_TYPE"
            ],
            "labelField": "dictName",
            "valueField": "dictId"
          }
        },
        {
          "label": "发生日期",
          "prop": "start_date"
        },
        {
          "label": "结束日期",
          "prop": "end_date"
        }
      ],
      "pagination": {
        "show": true
      }
    }
  }
}
