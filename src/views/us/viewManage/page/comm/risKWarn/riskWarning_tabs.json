{
  "module": "dynamicTabs",
  "layout": {
    "tabs": {
      "scope": "formDate",
      "tabHeader": [
        {
          "id": "violations",
          "label": "违规情况",
          "closable": false,
          "disabled": false,
          "icon": "el-icon-date",
          "toRequest": {
            "url": "/views/us/viewManage/page/comm/risKWarn/violation_list.json",
            "method": "post",
            "params": {
              
            }
          }
        },
        {
          "id": "majorEvents",
          "label": "重大事件",
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "toRequest": {
            "url": "/views/us/viewManage/page/comm/risKWarn/majorEvent_list.json",
            "method": "post",
            "params": {
              
            }
          }
        },
        {
          "id": "overdueRecords",
          "label": "历史贷款逾期记录",
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "toRequest": {
            "url": "/views/us/viewManage/page/comm/risKWarn/overdueHistory_list.json",
            "method": "post",
            "params": {
              
            }
          }
        },
        {
          "id": "overdueSituation",
          "label": "当前逾期贷款情况",
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "toRequest": {
            "url": "/views/us/viewManage/page/comm/risKWarn/overdue_list.json",
            "method": "post",
            "params": {
              
            }
          }
        },
        {
          "id": "riskList",
          "label": "风险名单",
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "toRequest": {
            "url": "/views/us/viewManage/page/comm/risKWarn/risk_list.json",
            "method": "post",
            "params": {
              "apply_cust_type": "${{params.apply_cust_type}}"
            }
          }
        }
      ],
      "defaultActiveKey": "violations",
      "position": "top",
      "styleClass": "card"
    }
  }
}
