{
  "module": "dynamicTabs",
  "layout": {
    "tabs": {
      "defaultActiveKey": "tab1",
      "position": "top",
      "styleClass": "card",
      "tabHeader": [
        {
          "id": "tab1",
          "label": "本行借款",
          "closable": false,
          "disabled": false,
          "icon": "el-icon-date",
          "toRequest": {
            "url": "/views/us/viewManage/page/comm/debtInfo/loan_of_bank.json",
            "method": "POST",
            "params": {
              "cust_no": "${{params.cust_no}}"
            }
          },
          "props": {
            "height": "400px"
          }
        },
        {
          "id": "tab2",
          "label": "他行借款",
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "toRequest": {
            "url": "/views/us/viewManage/page/comm/debtInfo/repay_duty_list.json",
            "method": "POST",
            "params": {
              "cust_no": "${{params.cust_no}}",
              "cert_type": "${{params.cert_type}}",
              "cert_no": "${{params.cert_no}}",
              "repay_duty_type": "9"
            }
          }
        },
        {
          "id": "tab3",
          "label": "对外担保",
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "toRequest": {
            "url": "/views/us/viewManage/page/comm/debtInfo/repay_duty_list.json",
            "method": "POST",
            "params": {
              "cust_no": "${{params.cust_no}}",
              "cert_type": "${{params.cert_type}}",
              "cert_no": "${{params.cert_no}}",
              "repay_duty_type": "1"
            }
          }
        }
      ]
    }
  }
}
