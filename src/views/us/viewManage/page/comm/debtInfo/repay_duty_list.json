{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ar2101",
          "cert_type": "${{params.cert_type}}",
          "cert_no": "${{params.cert_no}}",
          "repay_duty_type": "${{params.repay_duty_type}}"
        }
      },
      "thead": [
        {
          "label": "序号",
          "width": "50px",
          "type": "index",
          "align": "center"
        },
        {
          "align": "center",
          "label": "借款人身份类别",
          "prop": "borrower_type",
          "dict": {
            "dictType": "AR_E_C_BORROWERTYPE",
            "dictKey": [
              "AR_E_C_BORROWERTYPE"
            ],
            "format": "label"
          }
        },
        {
          "align": "center",
          "label": "账户数",
          "prop": "count"
        },
        {
          "align": "center",
          "label": "担保金额/还款责任限额",
          "prop": "guarantee_amount"
        },
        {
          "align": "center",
          "width": "100px",
          "label": "余额",
          "prop": "balance"
        }
      ]
    }
  }
}
