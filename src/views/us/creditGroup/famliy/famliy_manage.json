{
  "module": "v_search_btn_datagrid",
  "layout": {
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "target": "dialog",
          "label": "新增",
          "toRequest": {
            "method": "GET",
            "url": "/views/us/creditGroup/famliy/famliy_add.json",
            "params": {
              "famliy_cd": "${{params.famliy_cd}}"
            }
          },
          "show": {
            "expression": "'${{params.show_flash}}' == '0'"
          },
          "props": {
            "width": "80%",
            "title": "新增家庭成员"
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "pagination": {
        "show": false
      },
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "us1225",
          "famliy_cd": "${{params.famliy_cd}}"
        }
      },
      "thead": [
        {
          "type": "index",
          "label": "序号"
        },
        {
          "label": "姓名",
          "prop": "cust_name",
          "align": "center"
        },
        {
          "label": "性别",
          "prop": "sex_tp",
          "align": "center",
          "dict": {
            "dictType": "MS_E_GENDER",
            "dictKey": [
              "MS_E_GENDER"
            ],
            "format": "label"
          }
        },
        {
          "label": "证件类型",
          "prop": "cert_type",
          "align": "center",
          "dict": {
            "dictType": "MS_E_CRET_TYPE",
            "dictKey": [
              "MS_E_CRET_TYPE"
            ],
            "format": "label"
          }
        },
        {
          "label": "证件号码",
          "prop": "cert_no",
          "align": "center"
        },
        {
          "label": "是否户主",
          "prop": "is_head_of_famliy",
          "align": "center",
          "dict": {
            "dictType": "MS_E_YESORNO",
            "dictKey": [
              "MS_E_YESORNO"
            ],
            "format": "label"
          }
        },
        {
          "label": "家庭关系",
          "prop": "famliy_relation",
          "align": "center",
          "dict": {
            "dictType": "E_FAMILY_RELATION1",
            "dictKey": [
              "E_FAMILY_RELATION1"
            ],
            "format": "label"
          }
        },
        {
          "label": "操作",
          "width": "210px",
          "align": "center",
          "fixed": "right",
          "btns": [
            {
              "type": "look",
              "label": "详情",
              "fixed": "right",
              "toRequest": {
                "url": "/views/us/creditGroup/famliy/famliy_info.json",
                "method": "POST",
                "params": {
                  "famliy_cd": "${{params.famliy_cd}}"
                }
              },
              "show": {
                "expression": "'${{params.show_flash}}' == '1'"
              },
              "props": {
                "width": "80%",
                "title": "家庭成员详情"
              }
            },
            {
              "type": "edit",
              "label": "维护",
              "toRequest": {
                "url": "/views/us/creditGroup/famliy/famliy_modify.json",
                "method": "POST",
                "params": {
                  "famliy_cd": "${{params.famliy_cd}}"
                }
              },
              "show": {
                "expression": "'${{params.show_flash}}' == '0'"
              },
              "props": {
                "width": "80%",
                "title": "维护家庭成员信息"
              }
            },
            {
              "type": "delete",
              "label": "删除",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "us1229"
                }
              },
              "show": {
                "expression": "'${{params.show_flash}}' == '0'"
              }
            }
          ]
        }
      ]
    }
  }
}
