{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "scope": "formData",
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "us1227",
          "famliy_cd": "${{params.famliy_cd}}"
        }
      },
      "controls": [
        {
          "cust_name": {
            "control": "text",
            "width": "20px",
            "span": 8,
            "label": "姓名",
            "rules": [
              {
                "required": true,
                "message": "请输入姓名"
              }
            ]
          },
          "sex_tp": {
            "control": "select",
            "label": "性别",
            "width": "20px",
            "span": 8,
            "dict": {
              "dictType": "MS_E_GENDER",
              "dictKey": [
                "MS_E_GENDER"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请输入性别"
              }
            ]
          },
          "cert_type": {
            "control": "select",
            "label": "证件类型",
            "width": "20px",
            "span": 8,
            "dict": {
              "dictType": "MS_E_CRET_TYPE",
              "dictKey": [
                "MS_E_CRET_TYPE"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请输入证件类型"
              }
            ]
          },
          "cert_no": {
            "control": "text",
            "label": "证件号码",
            "width": "20px",
            "span": 8,
            "rules": [
              {
                "required": true,
                "message": "请输入证件号码"
              },
              {
                "max": 25,
                "message": "长度不能超过18个字符"
              }
            ]
          },
          "is_head_of_famliy": {
            "control": "select",
            "width": "20px",
            "span": 8,
            "label": "是否户主",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择"
              }
            ]
          },
          "famliy_relation": {
            "control": "select",
            "width": "20px",
            "span": 8,
            "label": "家庭关系",
            "dict": {
              "dictType": "E_FAMILY_RELATION1",
              "dictKey": [
                "E_FAMILY_RELATION1"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择"
              }
            ]
          }
        }
      ]
    }
  }
}

