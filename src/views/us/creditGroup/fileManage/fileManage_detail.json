{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest":"${{params}}",
      "controls": [
        {
          "file_name": {
            "control": "text",
            "label": "文件名称",
            "disabled": true,
            "span": 8,
            "width": "25px"
          },
          "file_type": {
            "control": "select",
            "label": "文件类型",
            "disabled": true,
            "span": 8,
            "width": "25px",
            "dict": {
              "dictType": "US_E_FILE_TYPE",
              "dictKey": [
                "US_E_FILE_TYPE"
              ],
              "format": "label"
            }
          },
          "file_path": {
            "control": "string",
            "disabled": true,
            "label": "文件路径",
            "span": 8,
            "width": "25px"
          }
        },
        {
          "description": {
            "control": "textarea",
            "disabled": true,
            "span": "24",
            "width": "25px",
            "rows": "4",
            "label": "说明"
          }
        }
      ]
    }
  }
}
