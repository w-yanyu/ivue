{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "famliy_cd": {
            "control": "text",
            "label": "户口号",
            "events": {
              "condition": {
                "$default": {
                  "control": {
                    "area_name": {
                      "disabled": true
                    }
                  }
                },
                "": {
                  "control": {
                    "area_name": {
                      "value": "",
                      "disabled": false
                    }
                  }
                }
              }
            }
          },
          "area_name": {
            "control": "lookup",
            "label": "所属区域",
            "keyField": {
              "area_no": "node_cd",
              "area_name": "node_name"
            },
            "format": "label",
            "toRequest": {
              "url": "@creditGroupTree",
              "method": "get",
              "params": {
                "inputName": "area_no",
                "treeTitle": "信用群体",
                "node_group": "0001",
                "is_leaf": false
              }
            },
            "props": {
              "title": "所属村查询"
            }
          },
          "area_no": {
            "control": "hidden",
            "label": "区域号"
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "target": "dialog",
          "label": "新增",
          "toRequest": {
            "method": "GET",
            "url": "/views/us/creditGroup/letterUser/letterUser_add.json"
          },
          "props": {
            "width": "90%",
            "title": "信用户认定"
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "us1212",
          "node_group": "0001"
        }
      },
      "thead": [
        {
          "type": "index",
          "label": "序号"
        },
        {
          "label": "户口号",
          "prop": "famliy_cd",
          "align": "center"
        },
        {
          "label": "信用户代表人客户号",
          "prop": "cust_no",
          "align": "center"
        },
        {
          "label": "信用户代表人",
          "prop": "cust_name",
          "align": "center"
        },
        {
          "label": "是否户主",
          "prop": "is_head_of_famliy",
          "align": "center",
          "dict": {
            "dictType": "MS_E_YESORNO",
            "dictKey": [
              "MS_E_YESORNO"
            ],
            "format": "label"
          }
        },
        {
          "label": "所属村",
          "prop": "area_no",
          "align": "center",
          "dict": {
            "dictType": "usOnl",
            "dictKey": ["usOnl"],
            "labelField": "node_name",
            "valueField": "node_cd",
            "format": "label",
            "doRequest":{
              "url":"/SUMP/icmcall/icmRPCCall",
              "method":"POST",
              "params":{
                "servicecode": "us1100",
                "node_group":"0001"
              }
            }
          }
        },
        {
          "label": "操作",
          "width": "210px",
          "align": "center",
          "btns": [
            {
              "type": "look",
              "label": "基本信息",
              "fixed": "right",
              "toRequest": {
                "url": "/views/us/creditGroup/privateManage_info.json",
                "method": "POST",
                "params": {
                  "show_flash": 1
                }
              },
              "props": {
                "width": "90%",
                "title": "详情"
              }
            },
            {
              "type": "edit",
              "label": "维护",
              "toRequest": {
                "url": "/views/us/creditGroup/letterUser/letterUser_modify.json",
                "method": "POST"
              },
              "props": {
                "width": "90%",
                "title": "维护信用户信息"
              }
            },
            {
              "type": "delete",
              "label": "删除",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "us1215"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
