{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "scope": "formData",
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "us1240"
        }
      },
      "controls": [
        {
          "cust_no": {
            "control": "lookup",
            "width": "20px",
            "span": 8,
            "label": "客户号",
            "keyField": {
              "cust_no": "cust_no",
              "cust_name": "cust_name",
              "cert_type": "cert_type",
              "cert_no": "cert_no",
              "cust_type": "cust_type"
            },
            "format": "label",
            "toRequest": {
              "url": "/views/us/cust_lookup.json",
              "method": "get"
            },
            "props": {
              "title": "查询客户"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择客户"
              }
            ]
          },
          "cust_name": {
            "control": "text",
            "width": "20px",
            "span": 8,
            "label": "客户名称",
            "disabled": true,
            "rules": [
              {
                "required": true,
                "message": "请输入姓名"
              }
            ]
          },
          "cert_type": {
            "control": "select",
            "label": "证件类型",
            "width": "20px",
            "span": 8,
            "disabled": true,
            "dict": {
              "dictType": "MS_E_CRET_TYPE",
              "dictKey": [
                "MS_E_CRET_TYPE"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请输入证件类型"
              }
            ]
          },
          "cert_no": {
            "control": "text",
            "label": "证件号码",
            "width": "20px",
            "span": 8,
            "disabled": true,
            "rules": [
              {
                "required": true,
                "message": "请输入证件号码"
              },
              {
                "max": 25,
                "message": "长度不能超过18个字符"
              }
            ]
          },
          "cust_type": {
            "control": "select",
            "width": "20px",
            "span": 8,
            "label": "客户类型",
            "disabled": true,
            "dict": {
              "dictType": "US_E_CUST_TYPE",
              "dictKey": [
                "US_E_CUST_TYPE"
              ],
              "labelField": "dictName",
              "valueField": "dictId"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择"
              }
            ]
          }
        }
      ]
    }
  }
}

