{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "cust_no": {
            "control": "text",
            "label": "客户编号",
            "rules": [
              {
                "max": 30,
                "message": "长度不能超过30个字符"
              }
            ]
          },
          "cust_name": {
            "control": "text",
            "label": "客户名称",
            "rules": [
              {
                "max": 80,
                "message": "长度不能超过80个字符"
              }
            ]
          },
          "cust_type": {
            "control": "select",
            "label": "客户类型",
            "dict": {
              "dictType": "US_E_CUST_TYPE",
              "dictKey": [
                "US_E_CUST_TYPE"
              ],
              "labelField": "dictName",
              "valueField": "dictId"
            }
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "target": "dialog",
          "label": "新增",
          "toRequest": {
            "method": "GET",
            "url": "/views/us/creditGroup/keyPeople/keyPeople_add.json"
          },
          "props": {
            "width": "90%",
            "title": "新增关键人信息"
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "us1239"
        }
      },
      "thead": [
        {
          "type": "index",
          "label": "序号"
        },
        {
          "label": "客户号",
          "prop": "cust_no",
          "align": "center"
        },
        {
          "label": "客户名称",
          "prop": "cust_name",
          "align": "center"
        },
        {
          "label": "客户类型",
          "prop": "cust_type",
          "align": "center",
          "dict": {
            "dictType": "US_E_CUST_TYPE",
            "dictKey": [
              "US_E_CUST_TYPE"
            ],
            "format": "label"
          }
        },
        {
          "label": "证件类型",
          "prop": "cert_type",
          "align": "center",
          "dict": {
            "dictType": "MS_E_CRET_TYPE",
            "dictKey": [
              "MS_E_CRET_TYPE"
            ],
            "labelField": "dictName",
            "valueField": "dictId"
          }
        },
        {
          "label": "证件号码",
          "prop": "cert_no",
          "align": "center"
        },
        {
          "label": "证件类型",
          "prop": "cert_type",
          "align": "center",
          "dict": {
            "dictType": "MS_E_CRET_TYPE",
            "dictKey": [
              "MS_E_CRET_TYPE"
            ],
            "labelField": "dictName",
            "valueField": "dictId"
          }
        },
        {
          "label": "操作",
          "width": "210px",
          "align": "center",
          "fixed": "right",
          "btns": [
            {
              "type": "look",
              "label": "基本信息",
              "toRequest": {
                "url": "@keyPeopleTransfer",
                "method": "POST",
                "params": {
                  "show_flash": 0
                }
              },
              "props": {
                "width": "90%",
                "title": "客户信息"
              }
            },
            {
              "type": "delete",
              "label": "删除",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "us1241"
                }
              },
              "confirm": {
                "title": "关键人可能被关联，被删除后关联信息失效，确定删除？",
                "confirmButtonText": "确定",
                "cancelButtonText": "取消"
              }
            }
          ]
        }
      ]
    }
  }
}
