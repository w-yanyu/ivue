{
  "module": "v_search_btn_datagrid",
  "layout": {
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "target": "dialog",
          "label": "新增",
          "toRequest": {
            "method": "GET",
            "url": "/views/us/creditGroup/keyPeople/keyPeopleRelation/keyPeopleRelation_add.json",
            "params": {
              "area_no": "${{params.area_no}}"
            }
          },
          "props": {
            "width": "90%",
            "title": "新增关键人信息"
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "us1251",
          "area_no": "${{params.area_no}}"
        }
      },
      "thead": [
        {
          "type": "index",
          "label": "序号"
        },
        {
          "label": "客户号",
          "prop": "cust_no",
          "align": "center"
        },
        {
          "label": "客户名称",
          "prop": "cust_name",
          "align": "center"
        },
        {
          "label": "证件类型",
          "prop": "cert_type",
          "align": "center",
          "dict": {
            "dictType": "MS_E_CRET_TYPE",
            "dictKey": [
              "MS_E_CRET_TYPE"
            ],
            "labelField": "dictName",
            "valueField": "dictId"
          }
        },
        {
          "label": "证件号码",
          "prop": "cert_no",
          "align": "center"
        },
        {
          "label": "客户类型",
          "prop": "cust_type",
          "align": "center",
          "dict": {
            "dictType": "US_E_CUST_TYPE",
            "dictKey": [
              "US_E_CUST_TYPE"
            ],
            "format": "label"
          }
        },
        {
          "label": "有效状态",
          "prop": "status",
          "align": "center",
          "dict": {
            "dictType": "US_E_STATE",
            "dictKey": [
              "US_E_STATE"
            ],
            "format": "label"
          }
        },
        {
          "label": "操作",
          "width": "210px",
          "align": "center",
          "fixed": "right",
          "btns": [
            {
              "type": "look",
              "label": "基本信息",
              "toRequest": {
                "url": "@keyPeopleTransfer",
                "method": "POST",
                "params": {
                  "show_flash": 0
                }
              },
              "props": {
                "width": "90%",
                "title": "客户信息"
              }
            },
            {
              "type": "look",
              "label": "详情",
              "toRequest": {
                "url": "/views/us/creditGroup/keyPeople/keyPeopleRelation/keyPeopleRelation_info.json",
                "method": "POST"
              },
              "props": {
                "width": "90%",
                "title": "作用信息"
              }
            },
            {
              "type": "delete",
              "label": "删除",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "us1253"
                }
              },
              "confirm": {
                "title": "确定删除？",
                "confirmButtonText": "确定",
                "cancelButtonText": "取消"
              }
            }
          ]
        }
      ]
    }
  }
}
