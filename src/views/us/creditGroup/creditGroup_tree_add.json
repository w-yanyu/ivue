{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "us1101",
          "node_group": "0001",
          "node_type": "${{params.node_type}}",
          "parent_node_cd": "${{params.node_cd}}"
        }
      },
      "controls": [
        {
          "node_cd": {
            "width": "180px",
            "control": "text",
            "label": "节点编号",
            "rules": [
              {
                "required": true,
                "message": "必填项"
              }
            ]
          },
          "node_name": {
            "width": "180px",
            "control": "text",
            "label": "节点名称",
            "rules": [
              {
                "required": true,
                "message": "必填项"
              }
            ]
          }
        },
        {
          "sort_no": {
            "width": "180px",
            "control": "text",
            "label": "序号",
            "rules": [
              {
                "type": "number",
                "message": "请填入整数"
              }
            ]
          }
        }
      ]
    }
  }
}
