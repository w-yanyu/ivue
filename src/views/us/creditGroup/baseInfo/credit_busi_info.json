{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "#initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POSt",
        "params": {
          "servicecode": "us1200",
          "area_no": "${{params.area_no}}",
          "area_name": "${{params.area_name}}"
        }
      },
      "btns": [],
      "controls": [
        {
          "credit_total_amt": {
            "width": "20px",
            "control": "currency",
            "span": 8,
            "disabled": true,
            "label": "授信总额",
            "format": {
              "thousand": ",",
              "decimal": 2
            }
          },
          "credit_balance": {
            "width": "20px",
            "control": "currency",
            "span": 8,
            "disabled": true,
            "label": "授信余额",
            "format": {
              "thousand": ",",
              "decimal": 2
            }
          },
          "loan_total_amt": {
            "width": "20px",
            "control": "currency",
            "span": 8,
            "label": "贷款总额",
            "disabled": true,
            "format": {
              "thousand": ",",
              "decimal": 2
            }
          },
          "loan_balance": {
            "width": "20px",
            "control": "currency",
            "span": 8,
            "label": "贷款余额",
            "disabled": true,
            "format": {
              "thousand": ",",
              "decimal": 2
            }
          },
          "loan_people_num": {
            "width": "20px",
            "control": "text",
            "span": 8,
            "label": "贷款人数",
            "disabled": true
          },
          "bad_rate": {
            "width": "20px",
            "control": "text",
            "span": 8,
            "label": "不良率",
            "disabled": true
          }
        }
      ]
    }
  }
}
