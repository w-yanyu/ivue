{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "btns": [],
      "controlsGroup": [
        {
          "title": "基本信息",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "accordion": false,
          "expand": true,
          "controls": [
            {
              "view1": {
                "control": "dynamic",
                "span": 24,
                "isShowChildForm": true,
                "toRequest": {
                  "url": "/views/us/creditGroup/baseInfo/village/village_info.json",
                  "method": "get",
                  "params": {
                    "area_no": "${{params.area_no}}"
                  }
                }
              }
            }
          ]
        },
        {
          "title": "行内业务信息",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "accordion": false,
          "expand": true,
          "controls": [
            {
              "view2": {
                "control": "dynamic",
                "span": 24,
                "isShowChildForm": true,
                "toRequest": {
                  "url": "/views/us/creditGroup/baseInfo/credit_busi_info.json",
                  "method": "get",
                  "params": {
                    "area_no": "${{params.area_no}}"
                  }
                }
              }
            }
          ]
        }
      ]
    }
  }
}
