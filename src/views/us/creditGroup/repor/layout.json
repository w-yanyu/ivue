{
  "module": "dynamicTabs",
  "layout": {
    "tabs": {
      "scope": "formDate",
      "tabHeader": [
        {
          "id": "assets",
          "label": "资产负债表",
          "closable": false,
          "disabled": false,
          "icon": "el-icon-date",
          "toRequest": {
            "url": "@assetsRepor",
            "method": "post",
            "params": {
              "cust_no": "${{params.cust_no}}"
            }
          }
        },
        {
          "id": "income",
          "label": "收入支出表",
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "toRequest": {
            "url": "@incomeAndExRepor",
            "method": "post",
            "params": {
              "cust_no": "${{params.cust_no}}"
            }
          }
        }
      ],
      "defaultActiveKey": "assets",
      "position": "top",
      "styleClass": "card"
    }
  }
}
