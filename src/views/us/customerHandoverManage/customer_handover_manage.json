{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "controls": [{
                "cust_no": {
                    "control": "text",
                    "label": "客户编号",
                    "rules": [{
                        "max": 30,
                        "message": "长度不能超过30个字符"
                    }]
                },
                "cust_name": {
                    "control": "text",
                    "label": "客户名称",
                    "rules": [{
                        "max": 80,
                        "message": "长度不能超过80个字符"
                    }]
                },
                "cert_type": {
                    "control": "select",
                    "label": "证件类型",
                    "dict": {
                        "dictType": "MS_E_CRET_TYPE",
                        "dictKey": [
                            "MS_E_CRET_TYPE"
                        ],
                        "labelField": "dictName",
                        "valueField": "dictId"
                    }
                },
                "cert_no": {
                    "control": "text",
                    "label": "证件号码"
                },
                "valid_state": {
                    "control": "select",
                    "label": "是否有效",
                    "dict": {
                        "dictType": "MS_E_YESORNO",
                        "dictKey": [
                            "MS_E_YESORNO"
                        ],
                        "labelField": "dictName",
                        "valueField": "dictId"
                    }
                }
            }]
        },
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch":false ,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "us0203",
                    "cust_no":"${{form.cust_no}}",
                    "cust_name":"${{form.cust_name}}",
                    "cert_type":"${{form.cert_type}}",
                    "valid_state":"${{form.valid_state}}",
                    "cert_no":"${{form.cert_no}}",
                    "belong_flash": "1"
                }
            },
            "thead": [{
                    "type": "index",
                    "label": "序号"
                },
                {
                    "label": "客户编号",
                    "prop": "cust_no",
                    "align": "center"
                },

                {
                    "label": "客户名称",
                    "prop": "cust_name",
                    "align": "center"
                },
                {
                    "label": "证件类型",
                    "prop": "cert_type",
                    "align": "center",
                    "dict": {
                        "dictType": "MS_E_CRET_TYPE",
                        "dictKey": [
                            "MS_E_CRET_TYPE"
                        ],
                        "labelField": "dictName",
                        "valueField": "dictId"
                    }
                },
                {
                    "label": "证件号码",
                    "prop": "cert_no",
                    "align": "center"
                },

                {
                    "label": "是否有效",
                    "prop": "valid_state",
                    "align": "center",
                    "dict": {
                        "dictType": "MS_E_YESORNO",
                        "dictKey": [
                            "MS_E_YESORNO"
                        ],
                        "labelField": "dictName",
                        "valueField": "dictId"
                    }
                },
                {
                    "label": "操作",
                    "width": "210px",
                    "align": "center",
                    "btns": [
                        {
                            "type": "edit",
                            "label": "移交",
                            "toRequest": {
                                "url": "/views/us/customerHandoverManage/handover_search.json",
                                "props": {
                                    "width": "80%",
                                    "title": "客户移交操作"
                                },
                                "params":{
                                    "cust_no":"${{current.cust_no}}",
                                    "branch_id":"$g{{user_info.branch_id}}"
                                }
                            }
                        }
                    ]
                }
            ]
        }
    }
}
