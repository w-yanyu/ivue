{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "us0803"
                }
            },
            "btns": [{
                "type": "submit",
                "label": "提交"
            }],
            "controls": [
            {

                "list_no": {
                    "control": "hidden",
                    "disabled": true,
                    "label": "流水号",
                    "width": "130px"
                },
                "busi_name": {
                    "control": "string",
                    "disabled": true,
                    "label": "企业名称",
                    "width": "130px",
                    "rules": [{
                        "required": true
                    }]
                },
                "cust_no": {
                    "control": "string",
                    "disabled": true,
                    "label": "客户编号",
                    "width": "130px",
                    "rules": [{
                        "required": true
                    }]
                },
                "cust_branch": {
                    "control": "string",
                    "disabled": true,
                    "label": "客户所属网点",
                    "width": "130px"
                },
                "list_type": {
                    "label": "名单类别",
                    "control": "select",
                    "disabled": "true",
                    "width": "130px",
                    "rules": [{
                        "required": true,
                        "message": "请选择名单类别"
                    }],
                    "dict": {
                        "dictType": "US_E_LIST_TYPE",
                        "dictKey": [
                            "US_E_LIST_TYPE"
                        ],
                        "format": "label"
                    }
                },
                "list_status": {
                    "label": "名单状态",
                    "control": "hidden",
                    "width": "130px",
                    "rules": [{
                        "message": "请选择名单状态"
                    }],
                    "dict": {
                        "dictType": "US_E_LIST_STATUS",
                        "dictKey": [
                            "US_E_LIST_STATUS"
                        ],
                        "format": "label"
                    }
                },

                "cust_apte": {
                    "label": "企业资质",
                    "control": "text",
                    "width": "130px",
                    "rules": [{
                        "required": true,
                        "message": "请选择名单类别"
                    }]
                },
                "is_bad_record": {
                    "label": "有无不良记录",
                    "control": "select",
                    "width": "130px",
                    "value":"0",
                    "rules": [{
                        "message": "请选择有无不良记录"
                    }],
                    "dict": {
                        "dictType": "MS_E_YESORNO",
                        "dictKey": [
                            "MS_E_YESORNO"
                        ],
                        "format": "label"
                    }
                },
                "is_hoard_house": {
                    "label": "有无囤地囤房行为",
                    "control": "select",
                    "width": "130px",
                    "value":"0",
                    "rules": [{
                        "message": "请选择有无囤地囤房行为"
                    }],
                    "dict": {
                        "dictType": "MS_E_YESORNO",
                        "dictKey": [
                            "MS_E_YESORNO"
                        ],
                        "format": "label"
                    }
                },
                "is_blacklist": {
                    "label": "是否被外部机构列为黑名单",
                    "control": "select",
                    "width": "130px",
                    "value":"0",
                    "rules": [{
                        "required": true,
                        "message": "请选择是否被外部机构列为黑名单"
                    }],
                    "dict": {
                        "dictType": "MS_E_YESORNO",
                        "dictKey": [
                            "MS_E_YESORNO"
                        ],
                        "format": "label"
                    },
                    "showItem": {
                        "1": ["org_name","listing_time"],
                        "0": []
                    }
                },
                "org_name": {
                    "label": "外部机构名称",
                    "control": "text",
                    "width": "130px",
                    "rules": [{
                        "required": true,
                        "message": "请输入外部机构名称"
                    }]
                },
                "listing_time": {
                    "control": "dateTimePicker",
                    "label": "列入时间",
                    "format": "yyyyMMdd",
                    "width": "130px",
                    "rules": [{
                        "required": true,
                        "message": "请输入列入时间"
                    }]
                },
                "remark": {
                    "label": "备注",
                    "control": "textarea",
                    "width": "130px",
                    "rows": "10px",
                    "span": 24,
                    "rules": [{
                        "max": 100,
                        "message": "100字以内"
                    }]
                }
            }]
        }
    }
}
