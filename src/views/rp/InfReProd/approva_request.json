{
    "module": "dynamicTabs",
    "layout": {
        "tabs": {
            "scope": "templateScope",
            "tabHeader": [
                {
                    "id": "photo_confirm",
                    "label": "影像信息确认",
                    "closable": false,
                    "disabled": false,
                    "icon": "el-icon-date",
                    "toRequest": {
                        "url":"/views/rp/InfReProd/proof_payment.json",
                        "method": "post"
                    }
                },
                {
                    "id": "approval",
                    "label": "审批提交页面",
                    "closable": false,
                    "disabled": false,
                    "icon": "el-icon-date",
                    "toRequest": {
                        "url": "/views/rp/InfReProd/limit_approval.json",
                        "method": "post"
                    }
                }
            ],
            "defaultActiveKey": "photo_confirm",
            "position": "top",
            "styleClass": "card"
        }
    }
}
