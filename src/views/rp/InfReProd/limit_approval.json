{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "callimitappro"
                }
            },
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": ""
                }
            ],
            "controls": [
                {
                   
                    "mercNo": {
                        "control": "string",
                        "label": "商户号",
                        "value":"${{params.mercNo}}",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入商户号"
                            },
                            {
                                "max": 50,
                                "message": "长度不能超过20个字符"
                            }
                        ]
                    },
                    "approvalStatus": {
                        "control": "select",
                        "label": "审批结果",
                        "dict": {
                            "dictType": "E_APPSTA",
                            "dictKey": [
                              "E_APPSTA"
                            ],
                            "format": "label",
                          "labelField":"dictName",
                          "valueField":"dictId"
                        }
                    }
                   

                },
                {
                    "transCode": {
                        "control":  "hidden",
                        "label": "商户限额记录流水号",
                        "value":"${{params.transCode}}",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入商户号"
                            },
                            {
                                "max": 20,
                                "message": "长度不能超过20个字符"
                            }
                        ]
                    }
                   ,
                    "approvalComment": {
                        "control": "textarea",
                        "label": "处理意见",
                        
                        "rules": [
                          {
                            "max": 200,
                            "message": "长度不能超过200个字符"
                          }
                        ]
                      }
                    
                }
            ]
        }
        
    }
}
