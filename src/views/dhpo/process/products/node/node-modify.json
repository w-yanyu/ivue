{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/call/nodeModify",
            "method": "POST",
            "params": {
              "servicecode": "plms_wftask",
              "wftask_cd": "${{params.wftask_cd}}",
              "trans_type": "UPDATE"
            }
          }
        }
      ],
      "controls": [
        {
          "wfprod_cd": {
            "control": "string",
            "label": "流程编号",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入流程编号"
              }
            ]
          },
          "wfprod_name": {
            "control": "string",
            "label": "流程名称",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入流程名称"
              }
            ]
          },
          "task_def_id": {
            "control": "string",
            "label": "活动id",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入活动id"
              }
            ]
          },
          "task_def_name": {
            "control": "string",
            "label": "活动名称",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入活动名称"
              }
            ]
          },
          "task_type": {
            "control": "string",
            "label": "节点类型",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入节点类型"
              }
            ]
          },
          "task_url1": {
            "control": "text",
            "label": "编辑框架",
            "width": "130px",
            "rules": [
              {
                "max": 100,
                "message": "长度不能超过50个字符"
              }
            ]
          },
          "task_url2": {
            "control": "text",
            "label": "查看框架",
            "width": "130px",
            "rules": [
              {
                "max": 100,
                "message": "长度不能超过50个字符"
              }
            ]
          },
          "ver": {
            "control": "string",
            "label": "版本号",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入版本号"
              }
            ]
          },
          "remark": {
            "control": "textarea",
            "label": "流程备注",
            "width": "130px"
          }
        }
      ]
    }
  }
}
