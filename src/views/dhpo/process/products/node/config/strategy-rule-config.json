{
  "module": "v-form-editableDataGrid-btn",
  "layout": {
    "form": {

    },
    "toolbar": {

    },
    "datagrid": {
      "doRequest": {
        "url":"/SUMP/icmcall/icmRPCCall",
        "method":"POST",
        "params": {
          "wftask_cd": "${{params.wftask_cd}}",
          "servicecode": "plms_nxdfid"
        }
      },
      "pagination": {
        "show": false
      },
      "thead": [
        {
          "type": "route_id",
          "label": "序号"
        },
        {
          "prop": "next_task_def_name",
          "label": "下一节点名称"
        },
        {
          "prop": "gzrule",
          "label": "配置规则"
        },
        {
          "prop": "workType",
          "label": "操作"
        },
        {
          "prop": "is_def_flow",
          "label": "是否默认路线"
        },
        {
          "prop": "check_seq",
          "label": "校验序号"
        }
      ]
    },
    
    "footer": {
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "el-icon-tickets",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "post",
            "param": {
              "wftask_cd": "${{params.wftask_cd}}",
              "servicecode": "plms_comclv"
            }
          }
        }
      ]
    }
  }
}
