{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
    "initRequest": "${{params}}",
      "btns": [
        {
          "type": "submit",
          "label": "保存",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "AS_PLMS_PB_UPWFRL"
            }
          }
        },
        {
          "type": "reset",
          "label": "重置"
        }
      ],
      "controls": [
        {
          "wfrlid": {
            "control": "string",
            "label": "审批角色编号",
            "rules": [
              {
                "required": true,
                "message": "请输入审批角色编号"
              }
            ]
          },
          "wfrlnm": {
            "control": "text",
            "label": "审批角色名称",
            "rules": [
              {
                "required": true,
                "message": "请输入审批角色编号"
              }
            ]
          }
        },
        {
          "wfrlds": {
            "control": "text",
            "label": "审批角色描述"
          },
          "rlauth": {
            "control": "select",
            "label": "角色权限",
            "dict": {
              "dictType": "E_PL_RLAUTH",
              "dictKey": [
                "E_PL_RLAUTH"
              ],
              "labelField": "dictName",
              "valueField": "dictId"
            }
          }
        },
        {
          "verson": {
            "control": "text",
            "label": "版本号"
          },
          "remark": {
            "control": "text",
            "label": "备注"
          }
        }

      ]
    }
  }
}
