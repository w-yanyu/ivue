{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [],
      "btns": []
    },
    "toolbar": {
      "btns": [
        {
          "type": "custom",
          "label": "自动获取",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "plms_upwfin",
              "wfprcd": "${{params.wfprod_cd}}",
              "updway": "PART"
            }
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "plms_qrwfpt",
          "wfprcd": "${{params.wfprod_cd}}"
        }
      },
      "thead": [
        {
          "label": "配置编号",
          "align": "center",
          "prop": "wfpart_cd"
        },
        {
          "label": "配置名称",
          "align": "center",
          "prop": "wfpart_name"
        },
        {
          "label": "流程编号",
          "align": "center",
          "prop": "wfprod_cd"
        },
        {
          "label":"流程名称",
          "align": "center",
          "prop":"wfprod_name"
        },
        {
          "label":"定义ID",
          "align": "center",
          "prop":"task_def_id"
        },
        {
          "label":"定义名称",
          "align": "center",
          "prop":"task_def_name"
        },{
          "label":"处理方式",
          "prop":"select_type",
          "dict": {
            "dictType": "BPM_E_SELECT_TYPE",
            "dictKey": [
              "BPM_E_SELECT_TYPE"
            ],
            "format": "label"
          },
          "align": "center"
        },{
          "label":"参与人类型",
          "prop":"part_type",
          "dict": {
            "dictType": "BPM_E_PART_TYPE",
            "dictKey": [
              "BPM_E_PART_TYPE"
            ],
            "format": "label"
          },
          "align": "center"
        },{
          "label":"版本号",
          "align": "center",
          "prop":"ver"
        },{
          "label":"修改日期",
          "align": "center",
          "prop":"last_upd_dt"
        },{
          "label":"修改人",
          "align": "center",
          "prop":"last_upd_usr"
        },{
          "label":"备注",
          "align": "center",
          "prop":"remark"
        },{
          "label": "操作",
          "align": "center",
          "width": "100px",
          "align":"center",
          "btns": [{
            "type": "edit",
            "label": "查看",
            "toRequest": {
              "url": "/views/dhpo/process/products/participant/view-detail.json",
              "method": "GET"
            },
            "props": {
              "width": "70%",
              "height": "60%",
              "title": "查看参与者详情"
            }
          },{
            "type": "edit",
            "label": "修改",
            "toRequest": {
              "url": "/views/dhpo/process/products/participant/view-modify.json",
              "method": "GET"
            },
            "props": {
              "width": "70%",
              "title": "参与者修改"
            }
          }]
        }
      ]
    }
  }
}
