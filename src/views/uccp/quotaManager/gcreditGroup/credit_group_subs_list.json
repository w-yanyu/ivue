{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "initRequest":"${{params}}",
            "controls": [
                {
                    "group_subs_credit_seq": {
                        "control": "text",
                        "label": "集团子公司额度编号"
                    },
                    "cust_name": {
                        "control": "text",
                        "label": "客户名称"
                    }
                }
            ]
          },
        "toolbar": {
            
        },
        "datagrid": {
            "isInitSearch": true,
            "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "post",
                "params": {
                    "servicecode": "uccp3093",
                    "group_credit_seq": "${{params.group_credit_seq}}",
                    "appId":"uccpOnl"
                }
            },
            "pagination": {
                "show": false,
                "pageSizes": [
                    10,
                    20,
                    50,
                    100
                
                ],
                "pageSize": 10
            },
            "thead": [
                {
                    "label": "集团子公司额度编号",
                    "prop": "group_subs_credit_seq",
                    "align": "center"
                },
                {
                    "label": "客户编号",
                    "prop": "cust_no",
                    "align": "center"
                },
                {
                    "label": "客户名称",
                    "prop": "cust_name",
                    "align": "center"
                },
                {
                    "label": "集团子公司额度",
                    "prop": "group_subs_credit_limit",
                    "format": {
                        "decimal": "2"
                    },
                    "align": "center"
                },
                {
                    "label": "已用集团子公司额度",
                    "prop": "used_group_subs_credit_limit",
                    "format": {
                        "decimal": "2"
                    },
                    "align": "center"
                },
                {
                    "label": "集团子公司敞口额度",
                    "prop": "group_subs_credit_exposure_limit",
                    "format": {
                        "decimal": "2"
                    },
                    "align": "center"
                },
                {
                    "label": "已用集团子公司敞口额度",
                    "prop": "used_group_subs_credit_exposure_limit",
                    "align": "center",
                    "format": {
                        "decimal": "2"
                    }
                },
                {
                    "label": "额度状态",
                    "prop": "quota_status",
                    "align": "center",
                    "dict": {
                        "dictType": "UCCP_E_QUOTA_STATUS",
                        "dictKey": [
                            "UCCP_E_QUOTA_STATUS"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "操作",
                    "fixed": "right",
                    "width": 140,
                    "align": "center",
                    "btns": [
                        {
                            "type": "edit",
                            "label": "详情",
                            "toRequest": {
                                "url": "/views/uccp/quotaManager/gcreditGroup/credit_group_subs_detail.json",
                                "method": "GET",
                                "params": {
                                    
                                }
                            },
                            "props": {
                                "width": "80%",
                                "title": "集团子公司信息详情"
                            }
                        }
                    ]
                } 
            ]
        }
    }
}
