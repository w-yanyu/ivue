{
    "module": "v-inputDataForm-btn",
    "layout": {
        "form": {
            "initRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POSt",
                "params": {
                  "servicecode": "uccp3092",
                  "appId":"uccpOnl",
                  "group_credit_seq": "${{params.group_credit_seq}}"
                }
            },
            "btns":[],
            "controls": [{
                "group_credit_seq": {
                    "control": "string",
                    "span": "8",
                    "width": "25px",
                    "label": "集团额度编号"
                },
                "source_credit_number": {
                    "control": "string",
                    "span": "8",
                    "width": "25px",
                    "label": "源系统授信额度编号"
                },
                "source_system": {
                    "control": "string",
                    "span": "8",
                    "width": "25px",
                    "label": "来源系统",
                    "dict": {
                        "dictType": "UCCP_E_SOURCE_SYSTEM",
                        "dictKey": [
                            "UCCP_E_SOURCE_SYSTEM"
                        ],
                        "format": "label"
                    }
                },
                "group_cust_no": {
                    "control": "string",
                    "span": "8",
                    "width": "25px",
                    "label": "集团客户编号"
                },
                "group_cust_name": {
                    "control": "string",
                    "span": "8",
                    "width": "25px",
                    "label": "集团客户名称"
                },
                "cl_prod_code": {
                    "control": "string",
                    "span": "8",
                    "width": "25px",
                    "label": "额度产品编号"
                },
                "cl_prod_name": {
                    "control": "string",
                    "span": "8",
                    "width": "25px",
                    "label": "额度产品名称"
                },
                "effect_date": {
                    "control": "string",
                    "span": "8",
                    "width": "25px",
                    "label": "生效日期"
                },
                "expiry_date": {
                    "control": "string",
                    "span": "8",
                    "width": "25px",
                    "label": "失效日期"
                },
                "base_ccy_code": {
                    "control": "string",
                    "span": "8",
                    "width": "25px",
                    "label": "基础币种",
                    "dict": {
                        "dictType": "MS_CURRENCY",
                        "dictKey": [
                            "MS_CURRENCY"
                        ],
                        "labelField": "dictName",
                        "valueField": "dictId"
                    }
                },
                "old_ccy_code": {
                    "control": "string",
                    "span": "8",
                    "width": "25px",
                    "label": "原始币种",
                    "dict": {
                        "dictType": "MS_CURRENCY",
                        "dictKey": [
                            "MS_CURRENCY"
                        ],
                        "labelField": "dictName",
                        "valueField": "dictId"
                    }
                },
                "time_point_rate": {
                    "control": "string",
                    "span": "8",
                    "width": "25px",
                    "label": "时点汇率"
                },
                "group_total_limit": {
                    "control": "string",
                    "span": "8",
                    "width": "25px",
                    "label": "集团总额度",
                    "format": {
                        "decimal": "2"
                    },
                    "align": "left"
                },
                "used_group_total_limit": {
                    "control": "string",
                    "span": "8",
                    "width": "25px",
                    "label": "已用集团总额度",
                    "format": {
                        "decimal": "2"
                    },
                    "align": "left"
                },
                "group_total_exposure_limit": {
                    "control": "string",
                    "span": "8",
                    "width": "25px",
                    "label": "集团总敞口额度",
                    "format": {
                        "decimal": "2"
                    },
                    "align": "left"
                },
                "used_group_total_exposure_limit": {
                    "control": "string",
                    "span": "8",
                    "width": "25px",
                    "label": "已用集团总敞口额度",
                    "format": {
                        "decimal": "2"
                    },
                    "align": "left"
                },
                "group_credit_distype": {
                    "control": "string",
                    "span": "8",
                    "width": "25px",
                    "label": "集团额度分配类型",
                    "dict": {
                        "dictType": "UCCP_E_CREDIT_DISTYPE",
                        "dictKey": [
                            "UCCP_E_CREDIT_DISTYPE"
                        ],
                        "labelField": "dictName",
                        "valueField": "dictId"
                    },
                    "align": "left"
                },
                "quota_status": {
                    "control": "string",
                    "label": "额度状态",
                    "span": "8",
                    "width": "25px",
                    "value":"1",
                    "dict": {
                        "dictType": "UCCP_E_QUOTA_STATUS",
                        "dictKey": [
                            "UCCP_E_QUOTA_STATUS"
                        ],
                        "labelField": "dictName",
                        "valueField": "dictId"
                    }
                }}
            ]
        }
    }
}
