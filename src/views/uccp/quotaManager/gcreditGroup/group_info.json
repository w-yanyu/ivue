{
  "module": "v-inputDataForm-btn",
  "layout": {
    "scope": "templateScope",
    "form": {
      "btns":[],
      "controls": [
          {
            "top": {
              "control":"dynamic",
              "span":24,
              "toRequest":{
                "url":"/views/uccp/quotaManager/gcreditGroup/credit_group_detail.json",
                "method": "post",
                "params": {
                  "group_credit_seq": "${{params.group_credit_seq}}"
                }
              }
            },
            "bottom": {
              "control":"dynamic",
              "span":24,
              "toRequest":{
                "url":"/views/uccp/quotaManager/gcreditGroup/credit_group_subs_list.json",
                "method": "post",
                "params": {
                  "group_credit_seq": "${{params.group_credit_seq}}"
                }
              }
            }
          } 
      ]
    }
  }
}
