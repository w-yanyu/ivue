{
    "module": "dynamicTabs",
    "layout": {
        "tabs": {
            "initDataOnce": true,
            "btns": [],
            "tabHeader": [
                {
                    "id": "baseInfo",
                    "label": "基础信息",
                    "closable": false,
                    "disabled": false,
                    "icon": "el-icon-date",
                    "toRequest": {
                        "url": "/views/uccp/quotaManager/gcreditGroup/group_info.json",
                        "method": "get"
                    }
                },
                {
                    "id": "occu",
                    "#label": "用信记录",
                    "label": {
                        "default": "用信记录", 
                        "i18n": "T302.credit-histories"
                     },
                    "icon": "el-icon-date",
                    "closable": false,
                    "disabled": false,
                    "toRequest": {
                        "url": "/views/uccp/quotaManager/gcreditGroup/group_occup_info.json",
                        "method": "get",
                        "params":{
                            "group_cust_no": "${{params.group_cust_no}}"
                        }
                    }
                },
                {
                    "id": "credit",
                    "#label": "客户额度视图",
                    "label": {
                        "default": "客户额度视图", 
                        "i18n": "T302.quota-view"
                     },
                    "icon": "el-icon-date",
                    "closable": false,
                    "disabled": false,
                    "toRequest": {
                        "url": "@groupCustTree",
                        "method": "get",
                        "params":{
                            "cust_no": "${{params.group_cust_no}}"
                        }
                    }
                }
            ],
            "defaultActiveKey": "baseInfo",
            "position": "top",
            "styleClass": "card"
        }
    }
}