{
    "module": "v-inputDataForm-btn",
    "layout": {
        "form": {
            "initRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POSt",
                "params": {
                  "servicecode": "uccp3094",
                  "appId":"uccpOnl",
                  "group_subs_credit_seq": "${{params.group_subs_credit_seq}}"
                }
            },
            "btns":[],
            "controls": [{
                "group_subs_credit_seq": {
                    "control": "string",
                    "span": "8",
                    "width": "25px",
                    "label": "集团子公司额度编号"
                },
                "group_credit_seq": {
                    "control": "string",
                    "span": "8",
                    "width": "25px",
                    "label": "集团额度编号"
                },
                "source_system": {
                    "control": "string",
                    "span": "8",
                    "width": "25px",
                    "label": "来源系统",
                    "dict": {
                        "dictType": "UCCP_E_SOURCE_SYSTEM",
                        "dictKey": [
                            "UCCP_E_SOURCE_SYSTEM"
                        ],
                        "format": "label"
                    }
                },
                "group_cust_no": {
                    "control": "string",
                    "span": "8",
                    "width": "25px",
                    "label": "集团客户编号"
                },
                "cust_no": {
                    "control": "string",
                    "span": "8",
                    "width": "25px",
                    "label": "客户编号"
                },
                "cust_name": {
                    "control": "string",
                    "span": "8",
                    "width": "25px",
                    "label": "客户名称"
                },
                "effect_date": {
                    "control": "string",
                    "span": "8",
                    "width": "25px",
                    "label": "生效日期"
                },
                "expiry_date": {
                    "control": "string",
                    "span": "8",
                    "width": "25px",
                    "label": "失效日期"
                },
                "base_ccy_code": {
                    "control": "string",
                    "span": "8",
                    "width": "25px",
                    "label": "基础币种",
                    "dict": {
                        "dictType": "MS_CURRENCY",
                        "dictKey": [
                            "MS_CURRENCY"
                        ],
                        "labelField": "dictName",
                        "valueField": "dictId"
                    }
                },
                "old_ccy_code": {
                    "control": "string",
                    "span": "8",
                    "width": "25px",
                    "label": "原始币种",
                    "dict": {
                        "dictType": "MS_CURRENCY",
                        "dictKey": [
                            "MS_CURRENCY"
                        ],
                        "labelField": "dictName",
                        "valueField": "dictId"
                    }
                },
                "time_point_rate": {
                    "control": "string",
                    "span": "8",
                    "width": "25px",
                    "label": "时点汇率"
                },
                "group_subs_credit_limit": {
                    "control": "string",
                    "span": "8",
                    "width": "25px",
                    "label": "集团子公司额度",
                    "format": {
                        "decimal": "2"
                    },
                    "align": "left"
                },
                "used_group_subs_credit_limit": {
                    "control": "string",
                    "span": "8",
                    "width": "25px",
                    "label": "已用集团子公司额度",
                    "format": {
                        "decimal": "2"
                    },
                    "align": "left"
                },
                "group_subs_credit_exposure_limit": {
                    "control": "string",
                    "span": "8",
                    "width": "25px",
                    "label": "集团子公司敞口额度",
                    "format": {
                        "decimal": "2"
                    },
                    "align": "left"
                },
                "used_group_subs_credit_exposure_limit": {
                    "control": "string",
                    "span": "8",
                    "width": "25px",
                    "label": "已用集团子公司敞口额度",
                    "format": {
                        "decimal": "2"
                    },
                    "align": "left"
                },
                "quota_status": {
                    "control": "string",
                    "label": "额度状态",
                    "span": "8",
                    "width": "25px",
                    "value":"1",
                    "dict": {
                        "dictType": "UCCP_E_QUOTA_STATUS",
                        "dictKey": [
                            "UCCP_E_QUOTA_STATUS"
                        ],
                        "labelField": "dictName",
                        "valueField": "dictId"
                    }
                }}
            ]
        }
    }
}
