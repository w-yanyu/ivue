{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "uccp3046",
          "record_no": "${{params.record_no}}"
        }
      },
      "btns":[
        {
          "type":"submit",
          "label":"提交"
        }
      ],
      "controls": [
        {
          "appv_status": {
            "control": "select",
            "label": "审核",
            "filterable": false,
            "options": [
              {
                "label": "同意",
                "value": "1"
              }
            ]
          }
        }
      ]
    }
  }
}
