{
  "module": "v-inputDataForm-btn",
  "layout": {
    "scope": "templateScope",
    "form": {
      "btns":[],
      "controls": [
          {
            "top": {
              "control":"dynamic",
              "span":24,
              "toRequest":{
                "url":"/views/uccp/quotaManager/creditTree/credit_user_info.json",
                "method": "post",
                "params": {
                  "cust_no": "${{params.cust_no}}",
                  "cust_name": "${{params.cust_name}}"
                }
              }
            },
            "bottom": {
              "control":"dynamic",
              "span":24,
              "toRequest":{
                "url":"/views/uccp/quotaManager/creditTree/credit_tree_manager.json",
                "method": "post",
                "params": {
                  "cust_no": "${{params.cust_no}}",
                  "servicecode": "${{params.servicecode}}"
                }
              }
            }
          } 
      ]
    }
  }
}
