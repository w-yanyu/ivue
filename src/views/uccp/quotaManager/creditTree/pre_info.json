{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "controls": [],
            "btns": []
          },
        "toolbar": {},
        "datagrid": {
            "isInitSearch": true,
            "scope": "searchTable",
            "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "post",
                "params": {
                    "cust_no": "${{params.cust_no}}",
                    "servicecode": "uccp3019",
                    "appId":"uccpOnl"
                }
            },
            "pagination": {
                "show": false
              },
            "thead": [
                {
                    "label": "合同/协议/业务编号",
                    "prop": "contract_no",
                    "align": "center"
                },
                {
                    "label": "产品编号",
                    "prop": "prod_code",
                    "align": "center"
                },
                {
                    "label": "产品名称",
                    "prop": "prod_name",
                    "align": "center"
                },
                {
                    "label": "产品组编号",
                    "prop": "prod_group_code",
                    "align": "center"
                },
                {
                    "label": "产品组名称",
                    "prop": "prod_group_name",
                    "align": "center"
                },
                {
                    "label": "预占用金额",
                    "prop": "preccup_reality_limit",
                    "align": "center",
                    "format": {
                        "thousand": ","
                      }
                },
                {
                    "label": "预占用敞口金额",
                    "prop": "preccup_reality_exposure_limit",
                    "align": "center",
                    "format": {
                        "thousand": ","
                      }
                },
                {
                    "label": "预占生效日期",
                    "prop": "preccup_effective_date",
                    "align": "center"
                },
                {
                    "label": "预占失效日期",
                    "prop": "preccup_expiry_date",
                    "align": "center"
                },
                {
                    "label": "占用状态",
                    "prop": "preccup_status",
                    "align": "center",
                    "dict": {
                        "dictType": "UCCP_E_PRECCUP_STATUS",
                        "dictKey": [
                            "UCCP_E_PRECCUP_STATUS"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "业务币种",
                    "prop": "base_ccy_code",
                    "align": "center",
                    "dict": {
                        "dictType": "MS_CURRENCY",
                        "dictKey": [
                            "MS_CURRENCY"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "来源系统",
                    "prop": "source_system",
                    "align": "center",
                    "dict": {
                        "dictType": "UCCP_E_SOURCE_SYSTEM",
                        "dictKey": [
                            "UCCP_E_SOURCE_SYSTEM"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "预占用流水",
                    "prop": "src_preccup_seq",
                    "align": "center"
                }
            ]
        }
    }
}
