{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [],
      "btns": []
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "pagination": {
        "show": false
      },
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "uccp3082",
          "call_seq": "${{params.call_seq}}",
          "source_credit_number": "${{params.limit_no}}"
        }
      },
      "thead": [
        {
          "label": {
            "default": "授信额度类型",
            "i18n": "T302.Type-of-credit-limit"
          },
          "prop": "quota_type_name",
          "align": "left",
          "width": "200px"
        },
        {
          "label": {
            "default": "额度名称",
            "i18n": "T302.Quota-Name"
          },
          "prop": "quota_name",
          "align": "left",
          "width": "200px"
        },
        {
          "label": "额度编号",
          "prop": "limit_no",
          "align": "center",
          "width": "200px"
        },
        {
          "label": "调整时间",
          "prop": "create_time",
          "align": "center"
      },
        {
          "label": "生效日期",
          "prop": "effect_date",
          "align": "center"
        },
        {
          "label": "失效日期",
          "prop": "expiry_date",
          "align": "center"
        },
        {
          "label": "额度状态",
          "prop": "quota_status",
          "align": "center",
          "dict": {
            "dictType": "UCCP_E_QUOTA_STATUS",
            "dictKey": [
              "UCCP_E_QUOTA_STATUS"
            ],
            "format": "label"
          }
        },
        {
          "label": {
            "default": "渠道来源",
            "i18n": "T302.Channel-Source"
          },
          "prop": "source_system",
          "align": "center",
          "dict": {
            "dictType": "UCCP_E_SOURCE_SYSTEM",
            "dictKey": [
              "UCCP_E_SOURCE_SYSTEM"
            ],
            "format": "label"
          }
        },
        {
          "label": {
            "default": "授信总额",
            "i18n": "T302.Total-credit-amount"
          },
          "prop": "credit_total_amt",
          "align": "center",
          "format": {
            "thousand": ","
          }
        },
        {
          "label": {
            "default": "授信可用额度",
            "i18n": "T302.Available-credit-limit"
          },
          "prop": "available_use_limit",
          "align": "center",
          "format": {
            "thousand": ","
          }
        },
        {
          "label": {
            "default": "授信已用额度",
            "i18n": "T302.Used-credit-limit"
          },
          "prop": "total_limit_used",
          "align": "center",
          "format": {
            "thousand": ","
          }
        },
        {
          "label": {
            "default": "授信敞口",
            "i18n": "T302.Credit-exposure"
          },
          "prop": "credit_exposure_amount",
          "align": "center",
          "format": {
            "thousand": ","
          }
        },
        {
          "label": {
            "default": "敞口可用额度",
            "i18n": "T302.Available-limit-of-exposure"
          },
          "prop": "used_exposure_limit",
          "align": "center",
          "format": {
            "thousand": ","
          }
        },
        {
          "label": {
            "default": "敞口已用额度",
            "i18n": "T302.Used-limit-of-exposure"
          },
          "prop": "available_use_exposure",
          "align": "center",
          "format": {
            "thousand": ","
          }
        }
      ]
    }
  }
}