{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
      ],
      "btns":[]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "pagination": {
        "show": false
      },
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "uccp4010",
          "outlistid": "lnCpstnQryType",
          "ctrct_num":"${{params.contract_no}}",
          "strt_cnt_num": "1",
          "qry_cnt_num": "10"
        }
      },
      "thead": [
        {
          "label": "序号",
          "width": "50px",
          "type": "index",
          "align": "center"
        },
        {
          "align": "center",
          "label": "客户姓名",
          "width": "120px",
          "prop": "cust_nm"
        },
        {
          "align": "center",
          "label": "借据号",
          "prop": "loan_due_bill_num"
        },
        {
          "align": "center",
          "label": "合同编号",
          "prop": "ctrct_num"
        },
        {
          "align": "center",
          "label": "客户号",
          "prop": "cust_num"
        },
        {
          "align": "center",
          "width": "100px",
          "label": "可售产品编号",
          "prop": "slbl_prod_code"
        },
        {
          "align": "center",
          "label": "可售产品名称",
          "width": "120px",
          "prop": "slbl_prod_nm"
        },
        {
          "align": "center",
          "width": "100px",
          "label": "放款日期",
          "prop": "open_acct_dt"
        },
        {
          "align": "center",
          "width": "100px",
          "label": "到期日期",
          "prop": "matu_dt"
        },
        {
          "align": "center",
          "width": "140px",
          "label": "借据金额(元)",
          "prop": "due_bill_amt",
          "format": {
            "thousand": ","
          }
        },
        {
          "align": "center",
          "width": "140px",
          "label": "合同金额(元)",
          "prop": "ctrct_amt",
          "format": {
            "thousand": ","
          }
        },
        {
          "align": "center",
          "width": "80px",
          "label": "贷款形态",
          "prop": "loan_form",
          "dict": {
            "dictType": "LN_E_LOAN_FORM",
            "dictKey": [
              "LN_E_LOAN_FORM"
            ],
            "format": "label"
          }
        },
        {
          "align": "center",
          "width": "120px",
          "label": "贷款账户状态",
          "prop": "loan_acct_ste",
          "dict": {
            "dictType": "LN_E_LOAN_ACCT_STE",
            "dictKey": [
              "LN_E_LOAN_ACCT_STE"
            ],
            "format": "label"
          }
        }
      ]
    }
  }
}
