{
    "module": "v-inputDataForm-btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "doRequest": {
            },
            "btns": [],
            "controls": [{
                    "occup_no": {
                        "control": "string",
                        "disabled":"true",
                        "label": "占用记录编号"
                    },
                    "src_occup_seq": {
                        "control": "string",
                        "label": "源系统占用流水"
                    },
                    "src_preccup_seq": {
                        "control": "string",
                        "label": "源系统预占用流水"
                    },
                    "prod_credit_seq": {
                        "control": "string",
                        "label": "产品授信额度编号"
                    },
                    "source_group_credit_seq": {
                        "control": "string",
                        "label": "源系统授信分组额度编号"
                    },
                    "source_credit_number": {
                        "control": "string",
                        "label": "源系统授信额度编号"
                    },
                    "prod_group_credit_seq": {
                        "control": "string",
                        "label": "产品组授信额度编号"
                    },
                    "cust_credit_seq": {
                        "control": "string",
                        "label": "客户层授信额度编号"
                    },
                    "cust_no": {
                        "control": "string",
                        "label": "客户编号"
                    },
                    "prod_code": {
                        "control": "string",
                        "label": "产品编号"
                    },
                    "prod_group_code": {
                        "control": "string",
                        "label": "产品组编号"
                    },
                    "source_system": {
                        "control": "string",
                        "label": "来源系统",
                        "dict": {
                            "dictType": "UCCP_E_SOURCE_SYSTEM",
                            "dictKey": [
                                "UCCP_E_SOURCE_SYSTEM"
                            ],
                            "format": "label"
                        }
                    },
                    "contract_no": {
                        "control": "string",
                        "label": "合同编号"
                    },
                    "base_ccy_code": {
                        "control": "string",
                        "label": "基准币种",
                        "dict": {
                            "dictType": "MS_CURRENCY",
                            "dictKey": [
                                "MS_CURRENCY"
                            ],
                            "format": "label"
                        }
                    },
                    "old_ccy_code": {
                        "control": "string",
                        "label": "原始币种",
                        "dict": {
                            "dictType": "MS_CURRENCY",
                            "dictKey": [
                                "MS_CURRENCY"
                            ],
                            "format": "label"
                        }
                    },
                    "time_point_rate": {
                        "control": "string",
                        "label": "时点汇率"
                    },
                    "occup_quota_type": {
                        "control": "string",
                        "label": "占用额度类型",
                        "dict": {
                            "dictType": "UCCP_E_PRECCUP_QUOTA_TYPE",
                            "dictKey": [
                                "UCCP_E_PRECCUP_QUOTA_TYPE"
                            ],
                            "format": "label"
                        }
                    },
                    "occup_appl_limit": {
                        "control": "currency",
                        "disabled": "true",
                        "label": "申请占用金额", 
                        "format": {
                            "thousand": ","
                          }
                    },
                    "occup_appl_exposure_limit": {
                        "control": "currency",
                        "disabled": "true",
                        "label": "申请占用敞口金额",
                        "format": {
                            "thousand": ","
                          }
                    },
                    "occup_reality_limit": {
                        "control": "currency",
                        "disabled": "true",
                        "label": "实际占用金额",
                        "format": {
                            "thousand": ","
                          }
                    },
                    "occup_reality_exposure_limit": {
                        "control": "currency",
                        "disabled": "true",
                        "label": "实际占用敞口金额",
                        "format": {
                            "thousand": ","
                          }
                    },
                    "occup_effective_date": {
                        "control": "string",
                        "label": "占用生效日期"
                    },
                    "occup_expiry_date": {
                        "control": "string",
                        "label": "占用失效日期"
                    },
                    "occup_status": {
                        "control": "string",
                        "label": "占用状态",
                        "dict": {
                            "dictType": "UCCP_E_PRECCUP_STATUS",
                            "dictKey": [
                                "UCCP_E_PRECCUP_STATUS"
                            ],
                            "format": "label"
                        }
                    },
                    "credit_split_type": {
                        "control": "string",
                        "label": "额度分项类型",
                        "dict": {
                            "dictType": "UCCP_E_CREDIT_SPLIT_TYPE",
                            "dictKey": [
                                "UCCP_E_CREDIT_SPLIT_TYPE"
                            ],
                            "format": "label"
                        }
                    },
                    "occup_original_record": {
                        "control": "string",
                        "label": "串用原始占用记录编号"
                    },
                    "sort_no": {
                        "control": "string",
                        "label": "串用序号"
                    },
                    "is_revo_loan": {
                        "control": "string",
                        "label": "是否可循环用款",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        }
                    },
                    "is_multi_loan": {
                        "control": "string",
                        "label": "是否允许多次用款",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        }
                    },
                    "grant_letter_sign": {
                        "control": "string",
                        "label": "授用信一体标志",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        }
                    }
                }
            ]
        }
    }
}
