{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [],
      "btns": []
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "pagination": {
        "show": true
      },
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "uccp3083",
          "cust_no":"${{params.cust_no}}"
        }
      },
      "thead": [
        {
          "label": {
            "default": "授信额度类型",
            "i18n": "T302.Type-of-credit-limit"
          },
          "prop": "quota_type_name",
          "align": "left",
          "width": "200px"
        },
        {
          "label": "产品名称",
          "prop": "prod_name",
          "align": "center",
          "width": "200px"
        },
        {
          "label": "额度编号",
          "prop": "limit_no",
          "align": "center",
          "width": "200px"
        },
        {
          "label": "额度状态",
          "prop": "quota_status",
          "align": "center",
          "dict": {
            "dictType": "UCCP_E_QUOTA_STATUS",
            "dictKey": [
              "UCCP_E_QUOTA_STATUS"
            ],
            "format": "label"
          }
        },
        {
          "label": "操作时间",
          "prop": "data_create_time",
          "align": "center"
        },
        {
          "label": "操作人",
          "prop": "data_create_user",
          "align": "center"
        },
        {
          "label": "操作类型",
          "prop": "quota_status",
          "align": "center",
          "dict": {
            "dictType": "UCCP_E_OPERATE_TYPE",
            "dictKey": [
              "UCCP_E_OPERATE_TYPE"
            ],
            "format": "label"
          }
        }
      ]
    }
  }
}