{
    "module": "dynamicTabs",
    "layout": {
        "tabs": {
            "initDataOnce": true,
            "btns": [],
            "tabHeader": [
                {
                    "id": "baseInfo",
                    "label": "额度详细信息",
                    "closable": false,
                    "disabled": false,
                    "icon": "el-icon-date",
                    "toRequest": {
                        "url": "/views/uccp/quotaManager/creditTree/credit_tree_detail.json",
                        "method": "get",
                        "params": {
                            "servicecode": "uccp3070",
                            "cust_no": "${{params.cust_no}}"
                        }
                    }
                },
                {
                    "id": "ftInfo",
                    "#label": "额度冻结解冻终止记录", 
                    "label": {
                        "default": "额度冻结解冻终止记录", 
                        "i18n": "T302.Limit-FT-Record"
                     },
                    "icon": "el-icon-date",
                    "closable": false,
                    "disabled": false,
                    "toRequest": {
                        "url": "/views/uccp/quotaManager/quotaVeiw/components/LimitFTRecord/limit_ft_list.json",
                        "method": "get",
                        "params":{
                            "cust_no": "${{params.cust_no}}"
                        }
                    }
                }
            ],
            "defaultActiveKey": "baseInfo",
            "position": "top",
            "styleClass": "card"
        }
    }
}