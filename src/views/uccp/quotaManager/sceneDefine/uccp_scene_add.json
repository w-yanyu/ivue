{
    "module": "v-inputDataForm-btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "post",
                "params": {
                    "servicecode": "uccp2006",
                   "scene_status":"0"
                }
            },
            "controls": [
                {
                    "scene_number": {
                        "control": "text",
                        "label": "场景定义编号",
                        "rules": [{
                            "required": true,
                            "message": "场景定义编号不能为空!"
                        }]
                    },
                    "scene_type": {
                        "control": "select",
                        "edit": false,
                        "label": "场景定义类型",
                        "dict": {
                            "dictType": "UCCP_E_SCENE_TYPE",
                            "dictKey": [
                                "UCCP_E_SCENE_TYPE"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": true,
                            "message": "场景定义类型不能为空!"
                        }]
                    },
                    "scene_desc": {
                        "control": "text",
                        "label": "场景定义描述"
                    }
                }
            ]
        }
    }
}
