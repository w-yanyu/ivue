{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "#initRequest": "${{params}}",
            "controls": [],
            "btns": []
        },
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": true,
            "#name":"prodlist",
            "fieldPath": "prodlist",
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "uccp3031",
                    "exceed_small_limit_ident":"0",
                    "source_system":"001",
                    "cust_no":"${{params.cust_no}}"
                }
            },
            "thead": [
                {
                    "type": "index",
                    "label": "序号"
                },
                {
                    "label": "产品授信额度编号",
                    "prop": "prod_credit_seq",
                    "align": "center"
                },
                {
                    "label": "源系统授信分组额度编号",
                    "prop": "source_group_credit_seq",
                    "align": "center"
                },
                {
                    "label": "源系统授信额度编号",
                    "prop": "source_credit_number",
                    "align": "center"
                },
                {
                    "label": "产品组授信额度编号",
                    "prop": "prod_group_credit_seq",
                    "align": "center"
                },
                {
                    "label": "基本授信额度编号",
                    "prop": "base_credit_seq",
                    "align": "center"
                },
                {
                    "label": "客户编号",
                    "prop": "cust_no",
                    "align": "center"
                },
                {
                    "label": "产品编号",
                    "prop": "prod_code",
                    "align": "center"
                },
                {
                    "label": "产品组编号",
                    "prop": "prod_group_code",
                    "align": "center"
                },
                {
                    "label": "额度产品编号",
                    "prop": "cl_prod_code",
                    "align": "center"
                },
                {
                    "label": "生效日期",
                    "prop": "effect_date",
                    "align": "center"
                },
                {
                    "label": "失效日期",
                    "prop": "expiry_date",
                    "align": "center"
                },
                {
                    "label": "基准币种",
                    "prop": "base_ccy_code",
                    "align": "center"
                },
                {
                    "label": "原始币种",
                    "prop": "old_ccy_code",
                    "align": "center"
                },
                {
                    "label": "时点汇率",
                    "prop": "time_point_rate",
                    "align": "center"
                },
                {
                    "label": "额度状态",
                    "prop": "quota_status",
                    "align": "center"
                },
                {
                    "label": "来源系统",
                    "prop": "source_system",
                    "align": "center"
                },
                {
                    "label": "产品授信总额度",
                    "prop": "prod_total_credit",
                    "align": "center"
                },
                {
                    "label": "产品授信总敞口额度",
                    "prop": "prod_total_exposure_credit",
                    "align": "center"
                },
                {
                    "label": "最终可用信额度金额",
                    "prop": "available_use_limit",
                    "align": "center"
                },
                {
                    "label": "最终可用信敞口金额",
                    "prop": "available_use_exposure",
                    "align": "center"
                },
                {
                    "label": "操作",
                    "align": "center",
                    "fixed": "right",
                    "btns": [
                        {
                            "type": "bringback",
                            "label": "选择"
                        }
                    ]
                }
            ]
        }
    }
}
