{
    "module": "v-inputDataForm-btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "post",
                "params": {
                    "servicecode": "uccp2014"
                   
                }
            },
            "controls": [{
                    "processing_seq": {
                        "control": "text",
                        "label": "文件处理流水",
                        "disabled":"false",
                        "required": true
                    },
                    "processing_date": {
                        "label": "文件处理日期",
                        "control": "dateTimePicker"
                    },
                    "file_type": {
                        "control": "select",
                        "label": "文件类型",
                        "disabled":"false",
                        "dict": {
                            "dictType": "UCCP_E_FILE_TYPE",
                            "dictKey": [
                                "UCCP_E_FILE_TYPE"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": false,
                            "message": "请选择文件类型"
                        }]
                    },
                    "interactive_system_code": {
                        "control": "select",
                        "label": "交互系统",
                         "disabled":"false",
                        "dict": {
                            "dictType": "UCCP_E_SOURCE_SYSTEM",
                            "dictKey": [
                                "UCCP_E_SOURCE_SYSTEM"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": false,
                            "message": "请选择交互系统"
                        }]
                    },
                    "data_stored_table_name": {
                        "control": "text",
                        "label": "数据存储表名称",
                         "disabled":"false"
                    },
                    "file_name_keywords1": {
                        "control": "text",
                        "label": "文件名关键字1",
                         "disabled":"false"
                    },
                    "file_name_keywords2": {
                        "control": "text",
                        "label": "文件名关键字2",
                         "disabled":"false"
                    },
                    "file_full_name": {
                        "control": "text",
                        "label": "文件全名",
                         "disabled":"false"
                    },
                    "file_full_path": {
                        "control": "text",
                        "label": "文件存放全路径",
                         "disabled":"false"
                    },
                    "file_processing_action": {
                        "control": "select",
                        "label": "交互处理动作",
                         "disabled":"false",
                        "dict": {
                            "dictType": "UCCP_E_FILE_PROCESSING_ACTION",
                            "dictKey": [
                                "UCCP_E_FILE_PROCESSING_ACTION"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": false,
                            "message": "请选择文件处理动作"
                        }]
                    },
                    "processing_status": {
                        "control": "select",
                        "label": "处理状态",
                         "disabled":"false",
                        "dict": {
                            "dictType": "UCCP_E_PROCESSING_STATUS",
                            "dictKey": [
                                "UCCP_E_PROCESSING_STATUS"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": false,
                            "message": "请选择处理状态"
                        }]
                    },
                    "exec_batch_group": {
                        "control": "text",
                        "label": "调起批量交易组号",
                         "disabled":"false"
                    },
                    "remark1": {
                        "control": "text",
                        "label": "备注1",
                         "disabled":"false"
                    },
                    "remark2": {
                        "control": "text",
                        "label": "备注2",
                         "disabled":"false"
                    },
                    "processing_start": {
                        "control": "text",
                        "label": "处理开始时间",
                         "disabled":"false"
                    },
                    "processing_end": {
                        "control": "text",
                        "label": "处理结束时间",
                         "disabled":"false"
                    },
                    "processing_mainseq": {
                        "control": "text",
                        "label": "全局串联流水",
                         "disabled":"false"
                    },
                    "last_processing_seq": {
                        "control": "text",
                        "label": "前驱处理流水",
                         "disabled":"false"
                    },
                    "file_compressed_type": {
                        "control": "select",
                        "label": "文件压缩包类型",
                        "disabled":"false",
                        "align": "center",
                        "dict": {
                            "dictType": "UCCP_E_FILE_COMPRESSED_TYPE",
                            "dictKey": [
                                "UCCP_E_FILE_COMPRESSED_TYPE"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": false,
                            "message": "请选择文件压缩包类型"
                        }]
                    },
                    "file_pretreatment_type": {
                        "control": "text",
                        "label": "文件内容预处理方式",
                         "disabled":"false",
                         "align": "center",
                         "dict": {
                             "dictType": "UCCP_E_FILE_PRETREATMENT_TYPE",
                             "dictKey": [
                                 "UCCP_E_FILE_PRETREATMENT_TYPE"
                             ],
                             "format": "label"
                         }
                    },
                    "file_source_encoding": {
                        "control": "select",
                        "label": "文件原始编码格式",
                         "disabled":"false",
                        "align": "center",
                        "dict": {
                            "dictType": "UCCP_E_FILE_ENCODING",
                            "dictKey": [
                                "UCCP_E_FILE_ENCODING"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": false,
                            "message": "请选择文件原始编码格式"
                        }]
                    },
                    "file_target_encoding": {
                        "control": "select",
                        "label": "文件目标编码格式",
                         "disabled":"false",
                        "align": "center",
                        "dict": {
                            "dictType": "UCCP_E_FILE_ENCODING",
                            "dictKey": [
                                "UCCP_E_FILE_ENCODING"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": false,
                            "message": "请选择文件目标编码格式"
                        }]
                    },
                    "current_task_num": {
                        "control": "text",
                        "label": "当前处理批量流水",
                         "disabled":"false"
                    },
                    "exec_task_num": {
                        "control": "text",
                        "label": "调起批量流水",
                         "disabled":"false"
                    },
                    "data_bathno": {
                        "control": "text",
                        "label": "数据批次号",
                         "disabled":"false"
                    },
                    "source_data_bathno": {
                        "control": "text",
                        "label": "源系统上送数据批次编号",
                         "disabled":"false"
                    }
                }
            ]
        }
    }
}
