{
    "module": "v-inputDataForm-btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "doRequest": {
            },
            "btns": [],
            "controls": [{
                "processing_seq": {
                    "control": "string",
                    "label": "文件处理流水"
                },
                "processing_date": {
                    "label": "文件处理日期",
                    "control": "string"
                },
                "file_type": {
                    "control": "string",
                    "label": "文件类型",
                    "dict": {
                        "dictType": "UCCP_E_FILE_TYPE",
                        "dictKey": [
                            "UCCP_E_FILE_TYPE"
                        ],
                        "format": "label"
                    }
                },
                "interactive_system_code": {
                    "control": "string",
                    "label": "交互系统",
                    "dict": {
                        "dictType": "UCCP_E_SOURCE_SYSTEM",
                        "dictKey": [
                            "UCCP_E_SOURCE_SYSTEM"
                        ],
                        "format": "label"
                    }
                },
                "data_stored_table_name": {
                    "control": "string",
                    "label": "数据存储表名称"
                },
                "file_name_keywords1": {
                    "control": "string",
                    "label": "文件名关键字1"
                },
                "file_name_keywords2": {
                    "control": "string",
                    "label": "文件名关键字2"
                },
                "file_full_name": {
                    "control": "string",
                    "label": "文件全名"
                },
                "file_full_path": {
                    "control": "string",
                    "label": "文件存放全路径"
                },
                "file_processing_action": {
                    "control": "string",
                    "label": "文件处理动作",
                    "disabled":"false",
                    "dict": {
                        "dictType": "UCCP_E_FILE_PROCESSING_ACTION",
                        "dictKey": [
                            "UCCP_E_FILE_PROCESSING_ACTION"
                        ],
                        "format": "label"
                    }
                },
                "processing_status": {
                    "control": "string",
                    "label": "处理状态",
                    "disabled":"false",
                    "dict": {
                        "dictType": "UCCP_E_PROCESSING_STATUS",
                        "dictKey": [
                            "UCCP_E_PROCESSING_STATUS"
                        ],
                        "format": "label"
                    }
                },
                "exec_batch_group": {
                    "control": "string",
                    "label": "调起批量交易组号"
                },
                "remark1": {
                    "control": "string",
                    "label": "备注1"
                },
                "remark2": {
                    "control": "string",
                    "label": "备注2"
                },
                "processing_start": {
                    "control": "string",
                    "label": "处理开始时间"
                },
                "processing_end": {
                    "control": "string",
                    "label": "处理结束时间"
                },
                "processing_mainseq": {
                    "control": "string",
                    "label": "全局串联流水"
                },
                "last_processing_seq": {
                    "control": "string",
                    "label": "前驱处理流水"
                },
                "file_compressed_type": {
                    "control": "string",
                    "label": "文件压缩包类型",
                    "disabled":"false",
                    "align": "center",
                    "dict": {
                        "dictType": "UCCP_E_FILE_COMPRESSED_TYPE",
                        "dictKey": [
                            "UCCP_E_FILE_COMPRESSED_TYPE"
                        ],
                        "format": "label"
                    }
                },
                "file_pretreatment_type": {
                    "control": "string",
                    "label": "文件内容预处理方式",
                    "align": "center",
                    "dict": {
                        "dictType": "UCCP_E_FILE_PRETREATMENT_TYPE",
                        "dictKey": [
                            "UCCP_E_FILE_PRETREATMENT_TYPE"
                        ],
                        "format": "label"
                    }
                },
                "file_source_encoding": {
                    "control": "string",
                    "label": "文件原始编码格式",
                    "disabled":"false",
                    "align": "center",
                    "dict": {
                        "dictType": "UCCP_E_FILE_ENCODING",
                        "dictKey": [
                            "UCCP_E_FILE_ENCODING"
                        ],
                        "format": "label"
                    }
                },
                "file_target_encoding": {
                    "control": "string",
                    "label": "文件目标编码格式",
                    "disabled":"false",
                    "align": "center",
                    "dict": {
                        "dictType": "UCCP_E_FILE_ENCODING",
                        "dictKey": [
                            "UCCP_E_FILE_ENCODING"
                        ],
                        "format": "label"
                    }
                },
                "current_task_num": {
                    "control": "string",
                    "label": "当前处理批量流水"
                },
                "exec_task_num": {
                    "control": "string",
                    "label": "调起批量流水"
                },
                "data_bathno": {
                    "control": "string",
                    "label": "数据批次号"
                },
                "source_data_bathno": {
                    "control": "string",
                    "label": "源系统上送数据批次编号"
                }
            }
        ]
        }
    }
}
