{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "controls": [
                {
                    "processing_seq": {
                        "control": "text",
                        "label": "文件处理流水"
                    },
                    "processing_date": {
                        "label": "文件处理日期",
                        "control": "dateTimePicker"
                    },
                    "file_type": {
                        "control": "select",
                        "label": "文件类型",
                        "align": "center",
                        "dict": {
                            "dictType": "UCCP_E_FILE_TYPE",
                            "dictKey": [
                                "UCCP_E_FILE_TYPE"
                            ],
                            "format": "label"
                        }
                    },
                    "interactive_system_code": {
                        "control": "select",
                        "label": "交互系统",
                        "align": "center",
                        "dict": {
                            "dictType": "UCCP_E_SOURCE_SYSTEM_CODE",
                            "dictKey": [
                                "UCCP_E_SOURCE_SYSTEM_CODE"
                            ],
                            "format": "label"
                        }
                    },
                    "data_stored_table_name": {
                        "control": "text",
                        "label": "数据存储表名称"
                    },
                    "file_name_keywords1": {
                        "control": "text",
                        "label": "文件名关键字1"
                    },
                    "file_name_keywords2": {
                        "control": "text",
                        "label": "文件名关键字2"
                    },
                    "file_full_name": {
                        "control": "text",
                        "label": "文件全名"
                    }
                    
                }
            ]
          },
        "toolbar": {
            "btns": [{
                "type": "add",
                "label": "新增",
                "target": "dialog",
                "toRequest": {
                    "method": "GET",
                    "url": "/views/uccp/batchFile/batchFileQueue/uccp_batchFileQueue_add.json",
                    "params": {
                        
                    }
                },
                "props": {
                    "width": "80%",
                    "title": "文件处理队列信息新增"
                }
            }]
        },
        "datagrid": {
            "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "post",
                "params": {
                    "servicecode": "uccp2017",
                    "appId":"uccpOnl"
                }
            },
            "thead": [
                {
                    "label": "文件处理流水",
                    "prop": "processing_seq",
                    "align": "center"
                },
                {
                    "label": "文件处理日期",
                    "prop": "processing_date",
                    "align": "center"
                },
                {
                    "label": "文件类型",
                    "prop": "file_type",
                    "align": "center",
                    "dict": {
                        "dictType": "UCCP_E_FILE_TYPE",
                        "dictKey": [
                            "UCCP_E_FILE_TYPE"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "交互系统",
                    "prop": "interactive_system_code",
                    "align": "center",
                    "dict": {
                        "dictType": "UCCP_E_SOURCE_SYSTEM_CODE",
                        "dictKey": [
                            "UCCP_E_SOURCE_SYSTEM_CODE"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "数据存储表名称",
                    "prop": "data_stored_table_name",
                    "align": "center"
                },
                {
                    "label": "文件名关键字1",
                    "prop": "file_name_keywords1",
                    "align": "center"
                },
                {
                    "label": "文件名关键字2",
                    "prop": "file_name_keywords2",
                    "align": "center"
                },
                {
                    "label": "文件全名",
                    "prop": "file_full_name",
                    "align": "center"
                },
                {
                    "label": "文件存放全路径",
                    "prop": "file_full_path",
                    "align": "center"
                },
                {
                    "label": "文件处理动作",
                    "prop": "file_processing_action",
                    "align": "center",
                    "dict": {
                        "dictType": "UCCP_E_FILE_PROCESSING_ACTION",
                        "dictKey": [
                            "UCCP_E_FILE_PROCESSING_ACTION"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "处理状态",
                    "prop": "processing_status",
                    "align": "center",
                    "dict": {
                        "dictType": "UCCP_E_PROCESSING_STATUS",
                        "dictKey": [
                            "UCCP_E_PROCESSING_STATUS"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "调起批量交易组号",
                    "prop": "exec_batch_group",
                    "align": "center"
                },
                {
                    "label": "备注1",
                    "prop": "remark1",
                    "align": "center"
                },
                {
                    "label": "备注2",
                    "prop": "remark2",
                    "align": "center"
                },
                {
                    "label": "处理开始时间",
                    "prop": "processing_start",
                    "align": "center"
                },
                {
                    "label": "处理结束时间",
                    "prop": "processing_end",
                    "align": "center"
                },
                {
                    "label": "全局串联流水",
                    "prop": "processing_mainseq",
                    "align": "center"
                },
                {
                    "label": "前驱处理流水",
                    "prop": "last_processing_seq",
                    "align": "center"
                },
                {
                    "label": "文件压缩包类型",
                    "prop": "file_compressed_type",
                    "align": "center",
                    "dict": {
                        "dictType": "UCCP_E_FILE_COMPRESSED_TYPE",
                        "dictKey": [
                            "UCCP_E_FILE_COMPRESSED_TYPE"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "文件内容预处理方式",
                    "prop": "file_pretreatment_type",
                    "align": "center",
                    "dict": {
                        "dictType": "UCCP_E_FILE_PRETREATMENT_TYPE",
                        "dictKey": [
                            "UCCP_E_FILE_PRETREATMENT_TYPE"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "文件原始编码格式",
                    "prop": "file_source_encoding",
                    "align": "center",
                    "dict": {
                        "dictType": "UCCP_E_FILE_ENCODING",
                        "dictKey": [
                            "UCCP_E_FILE_ENCODING"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "文件目标编码格式",
                    "prop": "file_target_encoding",
                    "align": "center",
                    "dict": {
                        "dictType": "UCCP_E_FILE_ENCODING",
                        "dictKey": [
                            "UCCP_E_FILE_ENCODING"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "当前处理批量流水",
                    "prop": "current_task_num",
                    "align": "center"
                },
                {
                    "label": "调起批量流水",
                    "prop": "exec_task_num",
                    "align": "center"
                },
                {
                    "label": "数据批次号",
                    "prop": "data_bathno",
                    "align": "center"
                },
                {
                    "label": "源系统上送数据批次编号",
                    "prop": "source_data_bathno",
                    "align": "center"
                },
                {
                    "label": "操作",
                    "fixed": "right",
                    "width": 230,
                    "align": "center",
                    "btns": [
                        {
                            "type": "edit",
                            "label": "详情",
                            "toRequest": {
                                "url": "/views/uccp/batchFile/batchFileQueue/uccp_batchFileQueue_detail.json",
                                "method": "GET",
                                "params": {
                                    
                                }
                            },

                            "props": {
                                "width": "80%",
                                "title": "文件处理队列信息详情"
                            }
                        },
                        {
                            "type": "edit",
                            "label": "维护",
                            "toRequest": {
                                "url": "/views/uccp/batchFile/batchFileQueue/uccp_batchFileQueue_upd.json",
                                "method": "GET",
                                "params": {
                                    
                                }
                            },

                            "props": {
                                "width": "80%",
                                "title": "文件处理队列信息维护"
                            }
                        },
                        {
                            "type": "custom",
                            "label": "删除",
                            "doRequest": {
                                "url": "/SUMP/call/RPCCall",
                                "method": "POST",
                                "params": {
                                    "servicecode": "uccp2015"
                                }
                            },
                            "confirm": {
                                "title": "确定删除",
                                "confirmButtonText": "确定",
                                "cancelButtonText": "取消"
                            }
                        }
                    ]
                }
            ]
        }
    }
}
