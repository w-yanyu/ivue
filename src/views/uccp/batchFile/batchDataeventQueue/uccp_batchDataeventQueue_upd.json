{
    "module": "v-inputDataForm-btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "post",
                "params": {
                    "servicecode": "uccp2020"
                   
                }
            },
            "controls": [{
                    "prim_key": {
                        "control": "text",
                        "label": "主键",
                        "disabled":"true",
                        "required": true
                    },
                    "event_generation_date": {
                        "label": "事件生成日期",
                        "control": "dateTimePicker"
                    },
                    "event_type": {
                        "control": "select",
                        "label": "事件类型",
                        "disabled":"false",
                        "dict": {
                            "dictType": "UCCP_E_EVENT_TYPE",
                            "dictKey": [
                                "UCCP_E_EVENT_TYPE"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": false,
                            "message": "请选择文件类型"
                        }]
                    },
                    "cust_no": {
                        "control": "text",
                        "label": "客户编号",
                        "disabled":"false"
                    },
                    "event_generation_reason": {
                        "control": "text",
                        "label": "事件生成原因",
                        "disabled":"false"
                    },
                    "event_generation_busiseq": {
                        "control": "text",
                        "label": "事件生成业务流水",
                        "disabled":"false"
                    },
                    "processing_status": {
                        "control": "select",
                        "label": "处理状态",
                        "disabled":"false",
                        "dict": {
                            "dictType": "UCCP_E_PROCESSING_STATUS",
                            "dictKey": [
                                "UCCP_E_PROCESSING_STATUS"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": false,
                            "message": "请选择处理状态"
                        }]
                    },
                    "remark1": {
                        "control": "text",
                        "label": "备注1",
                        "disabled":"false"
                    },
                    "remark2": {
                        "control": "text",
                        "label": "备注2",
                        "disabled":"false"
                    },
                    "processing_start": {
                        "control": "text",
                        "label": "处理开始时间",
                        "disabled":"false"
                    },
                    "processing_end": {
                        "control": "text",
                        "label": "处理结束时间",
                        "disabled":"false"
                    },
                    "current_task_num": {
                        "control": "text",
                        "label": "当前处理批量流水",
                        "disabled":"false"
                    }
                }
            ]
        }
    }
}
