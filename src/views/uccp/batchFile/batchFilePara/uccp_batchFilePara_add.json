{
    "module": "v-inputDataForm-btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "post",
                "params": {
                    "servicecode": "uccp2010"
                   
                }
            },
            "controls": [{
                    "file_type": {
                        "control": "select",
                        "label": "文件类型",
                         "disabled":"false",
                        "dict": {
                            "dictType": "UCCP_E_FILE_TYPE",
                            "dictKey": [
                                "UCCP_E_FILE_TYPE"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": true,
                            "message": "请选择文件类型"
                        }]
                    },
                    "interactive_system_code": {
                        "control": "select",
                        "label": "交互系统",
                         "disabled":"false",
                        "dict": {
                            "dictType": "UCCP_E_SOURCE_SYSTEM",
                            "dictKey": [
                                "UCCP_E_SOURCE_SYSTEM"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": false,
                            "message": "请选择交互系统"
                        }]
                    },
                    "data_stored_table_name": {
                        "control": "text",
                        "label": "数据存储表名称",
                         "disabled":"false"
                    },
                    "file_name_keywords1": {
                        "control": "text",
                        "label": "文件名关键字1",
                         "disabled":"false"
                    },
                    "file_name_keywords2": {
                        "control": "text",
                        "label": "文件名关键字2",
                         "disabled":"false"
                    },
                    "file_proc_type": {
                        "control": "select",
                        "label": "文件处理类型",
                        "disabled":"false",
                        "align": "center",
                        "dict": {
                            "dictType": "UCCP_E_FILE_PROC_TYPE",
                            "dictKey": [
                                "UCCP_E_FILE_PROC_TYPE"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": false,
                            "message": "请选择件处理类型"
                        }]
                    }
                },
                {
                    "file_interactive_mode": {
                        "control": "select",
                        "label": "文件交互模式",
                         "disabled":"false",
                        "align": "center",
                        "dict": {
                            "dictType": "UCCP_E_FILE_INTERACTIVE_MODE",
                            "dictKey": [
                                "UCCP_E_FILE_INTERACTIVE_MODE"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": false,
                            "message": "请选择文件交互模式"
                        }]
                    },
                    "file_path": {
                        "control": "text",
                        "label": "文件存放路径",
                         "disabled":"false"
                    },
                    "file_processing_time_start": {
                        "control": "text",
                        "label": "文件处理区间(起)",
                         "disabled":"false"
                    },
                    "file_processing_time_end": {
                        "control": "text",
                        "label": "文件处理区间(止)",
                         "disabled":"false"
                    },
                    "file_read_batch_group": {
                        "control": "text",
                        "label": "文件读取批量交易组号",
                         "disabled":"false"
                    },
                    "data_processing_batch_group": {
                        "control": "text",
                        "label": "文件业务数据处理交易组号",
                         "disabled":"false"
                    },
                    "file_read_type": {
                        "control": "select",
                        "label": "文件读取方式",
                         "disabled":"false",
                        "align": "center",
                        "dict": {
                            "dictType": "UCCP_E_FILE_READ_TYPE",
                            "dictKey": [
                                "UCCP_E_FILE_READ_TYPE"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": false,
                            "message": "请选择文件读取方式"
                        }]
                    },
                    "file_compressed_flag": {
                        "control": "select",
                        "label": "原始文件是否压缩包",
                        "disabled":"false",
                        "align": "center",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": false,
                            "message": "请选择原始文件是否压缩包"
                        }]
                    },
                    "file_compressed_type": {
                        "control": "select",
                        "label": "文件压缩包类型",
                        "disabled":"false",
                        "align": "center",
                        "dict": {
                            "dictType": "UCCP_E_FILE_COMPRESSED_TYPE",
                            "dictKey": [
                                "UCCP_E_FILE_COMPRESSED_TYPE"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": false,
                            "message": "请选择文件压缩包类型"
                        }]
                    },
                    "file_pretreatment_flag": {
                        "control": "select",
                        "label": "文件内容是否需预处理",
                         "disabled":"false",
                        "align": "center",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": false,
                            "message": "请选择文件内容是否需预处理"
                        }]
                    },
                    "file_pretreatment_type": {
                        "control": "text",
                        "label": "文件内容预处理方式",
                         "disabled":"false",
                         "align": "center",
                         "dict": {
                             "dictType": "UCCP_E_FILE_PRETREATMENT_TYPE",
                             "dictKey": [
                                 "UCCP_E_FILE_PRETREATMENT_TYPE"
                             ],
                             "format": "label"
                         }
                    },
                    "file_source_encoding": {
                        "control": "select",
                        "label": "文件原始编码格式",
                         "disabled":"false",
                        "align": "center",
                        "dict": {
                            "dictType": "UCCP_E_FILE_ENCODING",
                            "dictKey": [
                                "UCCP_E_FILE_ENCODING"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": false,
                            "message": "请选择文件原始编码格式"
                        }]
                    },
                    "file_target_encoding": {
                        "control": "select",
                        "label": "文件目标编码格式",
                         "disabled":"false",
                        "align": "center",
                        "dict": {
                            "dictType": "UCCP_E_FILE_ENCODING",
                            "dictKey": [
                                "UCCP_E_FILE_ENCODING"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": false,
                            "message": "请选择文件目标编码格式"
                        }]
                    },
                    "relyon_file_type": {
                        "control": "select",
                        "label": "依赖文件类型",
                         "disabled":"false",
                        "align": "center",
                        "dict": {
                            "dictType": "UCCP_E_FILE_TYPE",
                            "dictKey": [
                                "UCCP_E_FILE_TYPE"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": false,
                            "message": "请选择依赖文件类型"
                        }]
                    },
                    "field_term_char": {
                        "control": "text",
                        "label": "字段间隔符",
                         "disabled":"false"
                    },
                    "remark": {
                        "control": "text",
                        "label": "备注",
                         "disabled":"false"
                    }
                }
            ]
        }
    }
}
