{
    "module": "v_search_editableDataGrid",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "controls": [
                {
                    "cust_name": {
                        "label": "客户名称",
                        "control": "text"
                    }
                }
            ]
        },
        "toolbar": {
            "btns": [
                {
                    "type": "add",
                    "label": "新增",
                    "icon": "el-icon-plus"
                },
                {
                    "type": "batchDelete",
                    "label": "批量删除",
                    "icon": "el-icon-close"
                },
                {
                    "type": "save",
                    "label": "保存",
                    "icon": "el-icon-tickets",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "uccp1006",
                            "cust_group_seq": "${{params.cust_group_seq}}"
                        }
                    }
                }
            ]
        },
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": true,
            "##fieldPath": "list01",
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "uccp1008",
                    "cust_name": "${{form.cust_name}}",
                    "cust_group_seq": "${{params.cust_group_seq}}"
                }
            },
            "allData": false,
            "pagination": {
                "show": true,
                "pageSizes": [
                    10,
                    20,
                    30,
                    50
                ],
                "pageSize": 10
            },
            "thead": [
                {
                    "type": "selection",
                    "align": "center"
                },
                {
                    "prop": "cust_name",
                    "type": "lookup",
                    "label": "客户名称",
                    "keyField": {
                        "cust_no": "cust_no",
                        "cust_name": "cust_name"
                    },
                    "toRequest": {
                        "url": "/views/us/cust_lookup.json",
                        "method": "get"
                    },
                    "props": {
                        "title": "选择客户",
                        "width": "75%"
                    },
                    "rules": [
                        {
                            "required": true,
                            "message": "请选择客户名称"
                        }
                    ]
                },
                {
                    "label": "客户编号",
                    "prop": "cust_no",
                    "type": "string"
                },
                {
                    "label": "有效状态",
                    "prop": "valid_flag",
                    "type": "select",
                    "dict": {
                        "format": "label",
                        "dictKey": [
                            "UCCP_E_VALID_FLAG"
                        ],
                        "dictType": "UCCP_E_VALID_FLAG"
                    },
                    "rules": [
                        {
                            "required": true,
                            "message": "请选有效状态"
                        }
                    ]
                },
                {
                    "label": "操作",
                    "width": "280px",
                    "align": "center",
                    "btns": [
                        {
                            "type": "delete",
                            "label": "删除",
                            "icon": "el-icon-close"
                        }
                    ]
                }
            ]
        }
    }
}