{
    "module": "v-inputDataForm-btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "post",
                "params": {
                    "servicecode": "uccp1001",
                    "valid_flag": "1"
                }
            },
            "controls": [
                {
                    "cust_group_name": {
                        "control": "text",
                        "label": "客户组名称",
                        "rules": [
                            {
                                "required": true,
                                "message": "客户组名称不能为空!"
                            }
                        ]
                    },
                    "cust_group_type": {
                        "control": "select",
                        "label": "客户组类型",
                        "align": "center",
                        "dict": {
                            "dictType": "UCCP_E_CUST_GROUP_TYPE",
                            "dictKey": [
                                "UCCP_E_CUST_GROUP_TYPE"
                            ],
                            "format": "label"
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择客户组类型"
                            }
                        ]
                    },
                    "cust_group_describe": {
                        "label": "客户组描述",
                        "control": "textarea",
                        "#width": "300px",
                        "span": "12",
                        "#inputWidth": "250px",
                        "align": "center",
                        "autosize": {
                            "minRows": 4,
                            "maxRows": 5
                        },
                        "maxlength": 200,
                        "showWordLimit": true
                    }
                }
            ]
        }
    }
}