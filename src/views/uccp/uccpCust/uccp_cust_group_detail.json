{
    "module": "v-inputDataForm-btn",
    "layout": {
       
        "form": {
            "initRequest": "${{params}}",
            "btns": [],
           
            "controls": [
                {
                    "cust_group_seq": {
                        "control": "text",
                        "label": "客户组编号",
                        "disabled": "true"
                        
                    },
                    "cust_group_name": {
                        "control": "text",
                        "label": "客户组名称",
                        "disabled": "true"
                    },
                    "cust_group_type": {
                        "control": "select",
                        "label": "客户组类型",
                        "align": "center",
                        "dict": {
                            "dictType": "UCCP_E_CUST_GROUP_TYPE",
                            "dictKey": [
                                "UCCP_E_CUST_GROUP_TYPE"
                            ],
                            "format": "label"
                        },
                        "disabled": "true"
                    },
                    "valid_flag": {
                        "control": "select",
                        "label": "有效标识",
                        "align": "center",
                        "disabled": "true",
                        "value": "1",
                        "dict": {
                            "dictType": "UCCP_E_VALID_FLAG",
                            "dictKey": [
                                "UCCP_E_VALID_FLAG"
                            ],
                            "format": "label"
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择有效标识"
                            }
                        ]
                    },
                    "cust_group_describe": {
                        "label": "客户组描述",
                        "disabled": "true",
                        "control": "textarea",
                        "#width": "300px",
                        "span": "12",
                        "#inputWidth": "250px",
                        "align": "center",
                        "autosize": {
                            "minRows": 4,
                            "maxRows": 5
                        },
                        "maxlength": 200,
                        "showWordLimit": true
                    }
                }
            ]
        }
    }
}