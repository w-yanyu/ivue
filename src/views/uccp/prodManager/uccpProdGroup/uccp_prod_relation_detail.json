{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "controls": [
                
            ],
            "btns": []
          },
        "toolbar": {
            "btns": []
        },
        "datagrid": {
            "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "post",
                "params": {
                    "servicecode": "uccp0011",
                    "prod_group_code":"${{params.prod_group_code}}"
                }
            },
            "thead": [
                {
                    "label": "产品组编号",
                    "prop": "prod_group_code",
                    "align": "center"
                },
                {
                    "label": "产品编号",
                    "prop": "prod_code",
                    "align": "center"
                },
                {
                    "label": "产品名称",
                    "prop": "prod_name",
                    "align": "center"
                }
            ]
        }
    }
}
