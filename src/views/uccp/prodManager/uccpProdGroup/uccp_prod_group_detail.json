{
    "module": "v-inputDataForm-btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "doRequest": {},
            "controls": [
                {
                    "prod_group_code": {
                        "control": "string",
                        "label": "产品组编号"
                    },
                    "prod_group_name": {
                        "control": "string",
                        "label": "产品组名称"
                    },
                    "quota_calculation_rule": {
                        "control": "string",
                        "label": "额度默认计算规则",
                        "align": "center",
                        "dict": {
                            "dictType": "UCCP_E_QUOTA_CALCULATION_RULE",
                            "dictKey": [
                                "UCCP_E_QUOTA_CALCULATION_RULE"
                            ],
                            "format": "label"
                        }
                    },
                    "prod_group_limit": {
                        "control": "string",
                        "label": "产品组限额",
                        "align":"left",
                        "format": {
                            "decimal": "2"
                          }
                    },
                    "valid_flag": {
                        "control": "string",
                        "label": "有效标识",
                        "align": "center",
                        "dict": {
                            "dictType": "UCCP_E_VALID_FLAG",
                            "dictKey": [
                                "UCCP_E_VALID_FLAG"
                            ],
                            "format": "label"
                        }
                    },
                    "sup_prod_group_code": {
                        "control": "string",
                        "label": "上级产品组编号",
                        "align":"center"
                    }
                }
            ]
        }
    }
}
