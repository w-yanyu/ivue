{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "controls": [
                {
                    "prod_code": {
                        "control": "text",
                        "label": "产品编号"
                    },
                    "prod_name": {
                        "control": "text",
                        "label": "产品名称"
                    },
                    "prod_type": {
                        "control": "select",
                        "label": "产品类型",
                        "align": "center",
                        "dict": {
                            "dictType": "UCCP_E_BUSI_TYPE",
                            "dictKey": [
                                "UCCP_E_BUSI_TYPE"
                            ],
                            "format": "label"
                        }
                    }
                }
            ]
          },
        "toolbar": {},
        "datagrid": {
            "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "post",
                "params": {
                    "servicecode": "uccp0004",
                    "appId":"uccpOnl",
                    "valid_flag": "1"
                }
            },
            "thead": [
                {
                    "label": "产品编号",
                    "prop": "prod_code",
                    "align": "center"
                },
                {
                    "label": "产品名",
                    "prop": "prod_name",
                    "align": "center"
                },
                {
                    "label": "来源系统",
                    "prop": "source_system",
                    "align": "center",
                    "dict": {
                        "dictType": "UCCP_E_SOURCE_SYSTEM",
                        "dictKey": [
                            "UCCP_E_SOURCE_SYSTEM"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "产品类型",
                    "prop": "prod_type",
                    "align": "center",
                    "dict": {
                        "dictType": "UCCP_E_BUSI_TYPE",
                        "dictKey": [
                            "UCCP_E_BUSI_TYPE"
                        ],
                        "labelField": "dictName",
                        "valueField": "dictId"
                    }
                },
                {
                    "label": "授用信一体标志",
                    "prop": "grant_letter_sign",
                    "align": "center",
                    "dict": {
                        "dictType": "MS_E_YESORNO",
                        "dictKey": [
                            "MS_E_YESORNO"
                        ],
                        "labelField": "dictName",
                        "valueField": "dictId"
                    }
                },
                {
                    "label": "有效标识",
                    "prop": "valid_flag",
                    "align": "center",
                    "dict": {
                        "dictType": "UCCP_E_VALID_FLAG",
                        "dictKey": [
                            "UCCP_E_VALID_FLAG"
                        ],
                        "labelField": "dictName",
                        "valueField": "dictId"
                    }
                },
                {
                    "label": "操作",
                    "fixed": "right",
                    "width": 70,
                    "align": "center",
                    "btns": [{
                        "type": "bringback",
                        "label": "选择"
                      }]
                }
            ]
        }
    }
}
