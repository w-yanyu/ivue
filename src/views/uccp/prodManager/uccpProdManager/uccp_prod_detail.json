{
    "module": "v-inputDataForm-btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "doRequest": {
            },
            "btns": [],
            "controls": [{
                    "prod_code": {
                        "control": "string",
                        "disabled":"true",
                        "edit": false,
                        "label": "产品编号",
                        "keyField": {
                            "prod_code":"sale_prod_cd",
                            "prod_name": "sale_prod_name",
                            "prod_type":"prod_type"
                        },
                        "toRequest": {
                            "url": "/views/pt/partnerProjrctManage/patrs/sellProduct/sell_prod_look_up.json",
                            "method": "get",
                            "params": {
                                "rpt_type": "${{params.rpt_type}}",
                                "is_halt_flag": "0"


                            }
                        },
                        "props": {
                            "title": "请选择产品编号",
                            "width": "80%"
                        }
                    },
                    "prod_name": {
                        "control": "string",
                        "label": "产品名称"
                    },
                    "source_system": {
                        "control": "string",
                        "label": "来源系统",
                        "dict": {
                            "dictType": "UCCP_E_SOURCE_SYSTEM",
                            "dictKey": [
                                "UCCP_E_SOURCE_SYSTEM"
                            ],
                            "format": "label"
                        }
                    },
                    "prod_type": {
                        "control": "string",
                        "disabled":"true",
                        "label": "产品类型",
                        "align": "center",
                        "dict": {
                            "dictType": "UCCP_E_BUSI_TYPE",
                            "dictKey": [
                                "UCCP_E_BUSI_TYPE"
                            ],
                            "format": "label"
                        }
                    }
                },
                {
                    "grant_letter_sign": {
                        "control": "string",
                        "label": "授用信一体标志",
                        "align": "center",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        }
                    },
                    "valid_flag": {
                        "control": "string",
                        "label": "有效标识",
                        "align": "center",
                        "dict": {
                            "dictType": "UCCP_E_VALID_FLAG",
                            "dictKey": [
                                "UCCP_E_VALID_FLAG"
                            ],
                            "format": "label"
                        }
                    }
                }
            ]
        }
    }
}
