{
    "module": "v-inputDataForm-btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "post",
                "params": {
                    "servicecode": "uccp0001"
                   
                }
            },
            "controls": [{
                    "prod_code": {
                        "control": "lookup",
                        "edit": false,
                        "label": "产品编号",
                        "keyField": {
                            "prod_code":"sale_prod_cd",
                            "prod_name": "sale_prod_name",
                            "prod_type":"prod_type"
                        },
                        "toRequest": {
                            "url": "/views/pt/partnerProjrctManage/patrs/sellProduct/sell_prod_look_up.json",
                            "method": "get",
                            "params": {
                                "rpt_type": "${{params.rpt_type}}",
                                "is_halt_flag": "0"


                            }
                        },
                        "props": {
                            "title": "请选择产品编号",
                            "width": "80%"
                        },
                        "rules": [{
                            "required": true,
                            "message": "产品编号不能为空!"
                        }]
                    },
                    "prod_name": {
                        "control": "string",
                        "label": "产品名称"
                    },
                    "source_system": {
                        "control": "select",
                        "label": "来源系统",
                        "dict": {
                            "dictType": "UCCP_E_SOURCE_SYSTEM",
                            "dictKey": [
                                "UCCP_E_SOURCE_SYSTEM"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": true,
                            "message": "请选择来源系统"
                        }]
                    },
                    "prod_type": {
                        "control": "select",
                        "label": "产品类型",
                        "disabled":"true",
                        "align": "center",
                        "dict": {
                            "dictType": "UCCP_E_BUSI_TYPE",
                            "dictKey": [
                                "UCCP_E_BUSI_TYPE"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": true,
                            "message": "请选择产品类型"
                        }]
                    }
                },
                {
                    "grant_letter_sign": {
                        "control": "select",
                        "label": "授用信一体标志",
                        "align": "center",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": true,
                            "message": "请选择授用信一体标志"
                        }]
                    },
                    "valid_flag": {
                        "control": "select",
                        "label": "有效标识",
                        "disabled":"true",
                        "value":"1",
                        "align": "center",
                        "dict": {
                            "dictType": "UCCP_E_VALID_FLAG",
                            "dictKey": [
                                "UCCP_E_VALID_FLAG"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": true,
                            "message": "请选择有效标识"
                        }]
                    }
                }
            ]
        }
    }
}
