{
    "module": "v-inputDataForm-btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "post",
                "params": {
                    "servicecode": "uccp5003"
                   
                }
            },
            "controls": [{
                "limit_dimension_code": {
                    "control": "string",
                    "label": "限额维度编号",
                    "disabled":"true",
                    "rules": [{
                        "required": true,
                        "message": "限额维度编号!"
                    }]
                },
                "limit_dimension_name": {
                    "control": "text",
                    "label": "限额维度名称",
                    
                    "rules": [{
                        "required": true,
                        "message": "限额维度名称!"
                    }]
                },
                "limit_dimension_type": {
                    "control": "select",
                    "label": "限额维度类型",
                    "align": "center",
                    "dict": {
                        "dictType": "UCCP_E_LIMIT_DIMENSION_TYPE",
                        "dictKey": [
                            "UCCP_E_LIMIT_DIMENSION_TYPE"
                        ],
                        "format": "label"
                    },
                    "rules": [{
                        "required": true,
                        "message": "请选择限额维度类型"
                    }]
                },
                "valid_flag": {
                    "control": "select",             
                    "label": "有效标识",
                    "align": "center",                       
                    "value":"1",
                    "dict": {
                        "dictType": "UCCP_E_VALID_FLAG",
                        "dictKey": [
                            "UCCP_E_VALID_FLAG"
                        ],
                        "format": "label"
                    },
                    "rules": [{
                        "required": true,
                        "message": "请选择有效标识"
                    }]
                },
                "limit_achieve_method": {
                    "control": "text",
                    "label": "限额维度可选值获取方法",
                    "rules": [{
                        "required": true,
                        "message": "限额维度可选值获取方法!"
                    }]
                },
                "limit_dimension_desc": {
                    "control": "textarea",
                    
                    "label": "限额维度描述",
                    "align": "center"
                    
                }
            }]
        }
    }
}
