{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "controls": [
                {
                    "limit_component_name": {
                        "label": "限额维度部件名称",
                        "control": "text"
                    },
                    "limit_component_type": {
                        "label": "部件适用限额类型",
                        "control": "select",
                        "align": "center",
                        "dict": {
                            "dictType": "UCCP_E_COMPONENT_TYPE",
                            "dictKey": [
                                "UCCP_E_COMPONENT_TYPE"
                            ],
                            "format": "label"
                        }
                    },
                    "limit_control_way": {
                        "label": "控制方式可选项",
                        "control": "select",
                        "align": "center",
                        "dict": {
                            "dictType": "UCCP_E_LIMIT_CONTORL_WAY",
                            "dictKey": [
                                "UCCP_E_LIMIT_CONTORL_WAY"
                            ],
                            "format": "label"
                        }
                    },
                    "limit_manage_way": {
                        "label": "管控措施可选项",
                        "control": "select",
                        "align": "center",
                        "dict": {
                            "dictType": "UCCP_E_LIMIT_MANAGE_WAY",
                            "dictKey": [
                                "UCCP_E_LIMIT_MANAGE_WAY"
                            ],
                            "format": "label"
                        }
                    },

                    "valid_flag": {
                        "label": "有效标识",
                        "control": "select",
                        "align": "center",
                        "dict": {
                            "dictType": "UCCP_E_VALID_FLAG",
                            "dictKey": [
                                "UCCP_E_VALID_FLAG"
                            ],
                            "format": "label"
                        }
                    }
                }
            ]
        },
        "toolbar": {
            "btns": [
                {
                    "type": "add",
                    "label": "新增",
                    "target": "dialog",
                    "toRequest": {
                        "method": "GET",
                        "url": "/views/uccp/limit/component/uccp_component_define_add_step.json",
                        "params": {}
                    },
                    "props": {
                        "width": "80%",
                        "title": "限额部件新增"
                    }
                }
            ]
        },
        "datagrid": {
            "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "post",
                "params": {
                    "servicecode": "uccp5005"
                }
            },
            "thead": [
                {
                    "label": "限额维度部件名称",
                    "prop": "limit_component_name",
                    "align": "center"
                },
                {
                    "label": "部件适用限额类型",
                    "prop": "limit_component_type",
                    "align": "center",
                
                    "dict": {
                        "dictType": "UCCP_E_COMPONENT_TYPE",
                        "dictKey": [
                            "UCCP_E_COMPONENT_TYPE"
                        ],
                        "labelField": "dictName",
                        "valueField": "dictId"
                    }
                },
                {
                    "label": "控制方式可选项",
                    "prop": "limit_control_way_en",
                    "align": "center"
                   
                },
                {
                    "label": "管控措施可选项",
                    "prop": "limit_manage_way_en",
                    
                    "align": "center"
                    
                },
                {
                    "label": "有效标识",
                    "prop": "valid_flag",
                    "align": "center",
                    "dict": {
                        "dictType": "UCCP_E_VALID_FLAG",
                        "dictKey": [
                            "UCCP_E_VALID_FLAG"
                        ],
                        "labelField": "dictName",
                        "valueField": "dictId"
                    }
                },
                {
                    "label": "是否被引用",
                    "prop": "is_quote",
                    "align": "center",
                    "dict": {
                        "dictType": "MS_E_YESORNO",
                        "dictKey": [
                            "MS_E_YESORNO"
                        ],
                        "labelField": "dictName",
                        "valueField": "dictId"
                    }
                },
                {
                    "label": "操作",
                    "fixed": "right",
                    "width": 230,
                    "align": "center",
                    "btns": [
                        {
                            "type": "edit",
                            "label": "维护",
                            "show":{
                                "expression":"'${{current.is_quote}}' === '0'"
                            },
                            "toRequest": {
                                "url": "/views/uccp/limit/component/uccp_component_define_upd_step.json",
                                "method": "GET",
                                "params": {
                                    "limit_component_type":"${{current.limit_component_type}}",
                                    "limit_component_code":"${{current.limit_component_code}}"
                                }
                            },
                            "props": {
                                "width": "80%",
                                "title": "限额部件维护"
                            }
                        },
                        {
                            "type": "edit",
                            "label": "详情",
                            "toRequest": {
                                "url": "/views/uccp/limit/component/uccp_component_define_detail_step.json",
                                "method": "GET",
                                "params": {
                                    "limit_component_type":"${{current.limit_component_type}}",
                                    "limit_component_code":"${{current.limit_component_code}}"
                                }
                            },
                            "props": {
                                "width": "80%",
                                "title": "限额部件详情"
                            }
                        },
                        {
                            "type": "custom",
                            "label": "删除",
                            "show":{
                                "expression":"'${{current.is_quote}}' === '0'"
                            },
                            "doRequest": {
                                "url": "/SUMP/call/RPCCall",
                                "method": "POST",
                                "params": {
                                    "servicecode": "uccp5008"
                                }
                            },
                            "confirm": {
                                "title": "确定删除",
                                "confirmButtonText": "确定",
                                "cancelButtonText": "取消"
                            }
                        }
                    ]
                }
            ]
        }
    }
}