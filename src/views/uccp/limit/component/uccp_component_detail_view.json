{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "controls": [],
            "btns": []  
        },
        "toolbar": {
            "btns": [
            ]
        },
        "datagrid": {
            "isInitSearch": true,
            "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "post",
                "params": {
                    "servicecode": "uccp5010",
                    "limit_component_code":"${{params.limit_component_code}}"
                }
            },
            "pagination": {
                "show": false
            },
            "thead": [
              
                {
                    "label": "限额维度名称",
                    "prop": "limit_dimension_name",
                    "align": "center"
                },
                {
                    "label": "限额维度展示序号",
                    "prop": "limit_dimension_view_sort",
                    "align": "center"
                },
                {
                    "label": "限额维度是否必选",
                    "prop": "dimension_must_sel",
                    "align": "center",
                    "dict": {
                        "dictType": "MS_E_YESORNO",
                        "dictKey": [
                            "MS_E_YESORNO"
                        ],
                        "format": "label"
                    }
                   
                },
                {
                    "label": "有效标识",
                    "prop": "valid_flag",
                    "align": "center",
                    "dict": {
                        "dictType": "UCCP_E_VALID_FLAG",
                        "dictKey": [
                            "UCCP_E_VALID_FLAG"
                        ],
                        "labelField": "dictName",
                        "valueField": "dictId"
                    }
                }
            ]
        }
    }
}