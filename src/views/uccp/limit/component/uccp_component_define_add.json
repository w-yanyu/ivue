{
    "module": "v-inputDataForm-btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "post",
                "params": {
                    "servicecode": "uccp5006"
                }
            },
            "controls": [
                {
                    "limit_component_name": {
                        "control": "text",
                        "label": "限额维度部件名称",
                        "rules": [
                            {
                                "required": true,
                                "message": "限额维度部件名称!"
                            }
                        ]
                    },
                    "limit_component_type": {
                        "control": "select",
                        "label": "部件适用限额类型",
                        "align": "center",
                        "value": "1",
                        "dict": {
                            "dictType": "UCCP_E_COMPONENT_TYPE",
                            "dictKey": [
                                "UCCP_E_COMPONENT_TYPE"
                            ],
                            "format": "label"
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择部件适用限额类型"
                            }
                        ]
                    },
                    "limit_control_way": {
                        "control": "select",
                        "label": "控制方式可选项",
                        "align": "center",
                        "multiple": true,
                        "selectFirstValue": false,
                        "dict": {
                            "dictType": "UCCP_E_LIMIT_CONTORL_WAY",
                            "dictKey": [
                                "UCCP_E_LIMIT_CONTORL_WAY"
                            ],
                            "format": "label"
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择控制方式可选项"
                            }
                        ]
                    },
                    "limit_manage_way": {
                        "control": "select",
                        "label": "管控措施可选项",
                        "align": "center",
                        "multiple": true,
                        "selectFirstValue": false,
                        "value": "1",
                        "dict": {
                            "dictType": "UCCP_E_LIMIT_MANAGE_WAY",
                            "dictKey": [
                                "UCCP_E_LIMIT_MANAGE_WAY"
                            ],
                            "format": "label"
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择管控措施可选项"
                            }
                        ]
                    },
                    "need_check_flag": {
                        "control": "select",
                        "label": "是否需要复核",
                        "align": "center",
                        "value": "1",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择是否需要复核"
                            }
                        ]
                    },
                    "exposure_flag": {
                        "control": "select",
                        "label": "是否包含敞口",
                        "align": "center",
                        "value": "1",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择是否包含敞口"
                            }
                        ]
                    },
                    "valid_flag": {
                        "control": "select",
                        "label": "有效标识",
                        "align": "center",
                        "value": "1",
                        "dict": {
                            "dictType": "UCCP_E_VALID_FLAG",
                            "dictKey": [
                                "UCCP_E_VALID_FLAG"
                            ],
                            "format": "label"
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择有效标识"
                            }
                        ]
                    },
                    "limit_component_desc": {
                        "control": "textarea",
                        "label": "限额部件描述",
                        "align": "center"
                    }
                }
            ]
        }
    }
}