{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "norm_no": {
            "label": "限额编号",
            "control": "text",
            "rules": [
              {
              }
            ]
          },
          "norm_name": {
            "label": "限额名称",
            "control": "text",
            "rules": [
              {
              }
            ]
          }
        }
      ]
    },
    "toolbar": {
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "uccp5014",
          "limit_component_code": "${{params.limit_component_code}}"
        }
      },
      "thead": [
        {
          "type": "index"
        },
        {
          "prop": "norm_no",
          "label": "限额编号",
          "align": "center"
        },
        {
          "prop": "norm_name",
          "label": "限额名称",
          "align": "center"
        },
        {
          "prop": "start_date",
          "label": "开始日期",
          "align": "center"
        },
        {
          "prop": "limit_manage_way",
          "label": "管控措施",
          "align": "center",
          "dict": {
            "dictType": "UCCP_E_LIMIT_MANAGE_WAY",
            "dictKey": [
              "UCCP_E_LIMIT_MANAGE_WAY"
            ],
            "format": "label"
          }
        },
        {
          "prop": "limit_control_way",
          "label": "控制方式",
          "align": "center",
          "dict": {
            "dictType": "UCCP_E_LIMIT_CONTORL_WAY",
            "dictKey": [
              "UCCP_E_LIMIT_CONTORL_WAY"
            ],
            "format": "label"
          }
        },
        {
          "prop": "limit_total_amt",
          "label": "限额总额",
          "align": "center",
          "format":{
            "thousand":","
          }
        },
        {
          "prop": "limit_pre_used_amt",
          "label": "限额预占用金额",
          "align": "center",
          "format":{
            "thousand":","
          }
        },
        {
          "prop": "limit_used_amt",
          "label": "限额占用金额",
          "align": "center",
          "format":{
            "thousand":","
          }
        },
        {
          "prop": "limit_available_amt",
          "label": "限额可用金额",
          "align": "center",
          "format":{
            "thousand":","
          }
        },
        {
          "prop": "limit_status",
          "label": "限额状态",
          "align": "center",
          "dict": {
            "dictType": "UCCP_E_LIMIT_STATUS",
            "dictKey": [
              "UCCP_E_LIMIT_STATUS"
            ],
            "format": "label"
          }
        }
      ]
    }
  }
}