{
    "module": "v-inputDataForm-btn",
    "layout": {
        "form": {
          "doRequest":{
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "post",
            "params": {
                "servicecode": "uccp5026",
                "norm_no": "${{params.norm_no}}"
            }
            },
            "controls": [
                {
                    "norm_name": {
                        "label": "限额名称",
                        "keyField":{
                            "norm_name": "norm_name"
                          },
                        "control": "text",
                        "rules": [
                            {
                              "required": true,
                              "message": "请输入"
                            },
                            {
                              "expression": "'${{form.priority}}'.length < 51",
                              "message": "最多可以输入50个汉字"
                            }
                          ]
                    }
                }
            ]
        }
    }
}