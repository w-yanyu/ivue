{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "norm_no": {
            "label": "限额编号",
            "control": "text",
            "rules": [
              {
              }
            ]
          },
          "norm_name": {
            "label": "限额名称",
            "control": "text",
            "rules": [
              {
              }
            ]
          }
        }
      ]
    },
    "toolbar": {
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "uccp5014",
          "limit_component_type": "0",
          "limit_status":"2"
        }
      },
      "thead": [
        {
          "type": "index"
        },
        {
          "prop": "norm_no",
          "label": "限额编号",
          "align": "center"
        },
        {
          "prop": "norm_name",
          "label": "限额名称",
          "align": "center"
        },
        {
          "prop": "start_date",
          "label": "开始日期",
          "align": "center"
        },
        {
          "prop": "limit_manage_way",
          "label": "管控措施",
          "align": "center",
          "dict": {
            "dictType": "UCCP_E_LIMIT_MANAGE_WAY",
            "dictKey": [
              "UCCP_E_LIMIT_MANAGE_WAY"
            ],
            "format": "label"
          }
        },
        {
          "prop": "limit_control_way",
          "label": "控制方式",
          "align": "center",
          "dict": {
            "dictType": "UCCP_E_LIMIT_CONTORL_WAY",
            "dictKey": [
              "UCCP_E_LIMIT_CONTORL_WAY"
            ],
            "format": "label"
          }
        },
        {
          "prop": "limit_total_amt",
          "label": "限额总额",
          "align": "center",
          "format":{
            "thousand":","
          }
        },
        {
          "prop": "limit_pre_used_amt",
          "label": "限额预占用金额",
          "align": "center",
          "format":{
            "thousand":","
          }
        },
        {
          "prop": "limit_used_amt",
          "label": "限额占用金额",
          "align": "center",
          "format":{
            "thousand":","
          }
        },
        {
          "prop": "limit_available_amt",
          "label": "限额可用金额",
          "align": "center",
          "format":{
            "thousand":","
          }
        },
        {
          "label": "操作",
          "fixed": "right",
          "width": "210",
          "align": "center",
          "btns": [
            {
              "toRequest": {
                "method": "POST",
                "url": "@limitComOptionUpd",
                "params":{
                  "limit_component_type":"0"
                }
              },
              "label": "维护",
              "type": "edit",
              "props": {
                "title": "限额信息维护"
              }
            },
            {
              "toRequest": {
                "method": "POST",
                "url": "/views/uccp/limit/limitSettBasic/limit_sett_copy.json",
                "params": {
                  "norm_no": "${{current.norm_no}}"
              }
              },
              "label": "复制",
              "type": "edit",
              "props": {
                "title": "限额复制"
              }
            },
            {
              "doRequest": {
                "method": "POST",
                "url": "/SUMP/icmcall/icmRPCCall",
                "params": {
                  "servicecode": "uccp5018",
                  "method": "del"
                }
              },
              "label": "删除",
              "type": "delete"
            }, 
            {
              "toRequest": {
                "method": "POST",
                "target": "dialog",
                "url": "@limitDetail"
              },
              "label": "限额详情",
              "type": "edit",
              "props": {
                "title": "限额信息详情",
                "width": "80%"
              }
            },
            {
              "toRequest": {
                "method": "POST",
                "target": "dialog",
                "url": "/views/uccp/limit/limitSettBasic/limit_use_list.json",
                "params":{
                  "start_date":"",
                  "end_date":""
                }
              },
              "label": "使用情况查看",
              "type": "edit",
              "props": {
                "title": "限额使用情况查看",
                "width": "80%"
              }
            }                    
          ]
        }
      ]
    }
  }
}