{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "post",
                "params": {
                  "servicecode": "bpm0018"
                }
              },
              "btns":[
				{
					"type":"submit",
					"label":"提交"
				}
			],
            "controls": [
                {
                    "metadata_id": {
                        "label": "元数据编号",
                        "control": "text",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入元数据编号"
                            }
                        ]
                    },
                    "obtain_mode": {
                        "control": "select",
                        "label": "获取方式",
                        "dict": {
                            "dictType": "BPM_E_GETTYP",
                            "dictKey": [
                                "BPM_E_GETTYP"
                            ],
                            "format": "label"
                        },
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择获取方式"
                            }
                        ],

                      "showItem": {
                        "0": [
                          "table_name",
                          "field_name",
                          "query_term",
                          "sql_statement",
                          "statis_exec_sql",
                          "metadata_type"
                        ],
                        "1": [
                          "return_field",
                          "method_name"
                        ]
                      },
                      "events": {
                        "condition": {
                          "0": {
                            "control": {
                              "table_name": {
                                "rules": [
                                  {
                                    "required": true,
                                    "message": "请输入表名"
                                  }
                                ]
                              },

                              "field_name": {
                                "rules": [
                                  {
                                    "required": true,
                                    "message": "请输入字段名"
                                  }
                                ]
                              }
                            },
                            "query_term": {
                              "rules": [
                                {
                                  "required": true,
                                  "message": "请输入查询条件"
                                }
                              ]
                            },
                            "statis_exec_sql": {
                              "rules": [
                                {
                                  "required": true,
                                  "message": "请输入统计执行SQL"
                                }
                              ]
                            }

                          },
                          "1": {
                            "control": {
                              "method_name": {
                                "rules": [
                                  {
                                    "required": true,
                                    "message": "请输入方法名"
                                  }
                                ]
                              },

                              "return_field": {
                                "rules": [
                                  {
                                    "required": true,
                                    "message": "请输入方法返回字段"
                                  }
                                ]
                              }
                            }
                          }
                        }
                      }
                    },
                    "metadata_type": {
                        "control": "select",
                        "label": "元数据分类",
                        "dict": {
                            "dictType": "BPM_E_SJFENL",
                            "dictKey": [
                                "BPM_E_SJFENL"
                            ],
                            "format": "label"
                        },
                        "width": "130px",
                        "initShow": false,
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择元数据分类"
                            }
                        ]
                    },
                    "metadata_name": {
                        "label": "元数据名称",
                        "control": "text",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入元数据名称"
                            }
                        ]
                    },
                    "data_type": {
                        "control": "select",
                        "label": "数据类型",
                        "dict": {
                            "dictType": "BPM_E_DATTYP",
                            "dictKey": [
                                "BPM_E_DATTYP"
                            ],
                            "format": "label"
                        },
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择数据类型"
                            }
                        ]
                    },                
                    "method_name": {
                        "label": "方法名",
                        "width": "130px",
                        "control": "text",
                        "initShow": false,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入方法名"
                            }
                        ]
                    },
                    "return_field": {
                        "label": "方法返回字段",
                        "width": "130px",
                        "control": "text",
                        "initShow": false,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入方法返回字段"
                            }
                        ]
                    },
                    "table_name": {
                        "label": "表名",
                        "width": "130px",
                        "control": "text",
                        "initShow": false
                    },
                    "field_name": {
                        "label": "字段名",
                        "width": "130px",
                        "control": "text",
                        "initShow": false
                    },
                    "query_term": {
                        "label": "查询条件",
                        "width": "130px",
                        "control": "text",
                        "initShow": false
                    },
                    "sql_statement": {
                        "label": "SQL预览",
                        "width": "130px",
                        "control": "text",
                        "initShow": false
                    },
                    "statis_exec_sql": {
                        "label": "统计执行SQL",
                        "width": "130px",
                        "control": "text",
                        "initShow": false
                    }
                }
            ]
        }
    }
}
