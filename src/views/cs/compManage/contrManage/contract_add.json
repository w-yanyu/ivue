{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "scope": "form",
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "cb0301"
            }
          }
        }
      ],
      "controlsGroup": [
        {
          "title": "基础信息",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "accordion": false,
          "expand": true,
          "controls": [
            {
              "contract_type":{
                "control": "select",
                "label": "合同类别",
                "dict": {
                  "dictType": "CS_E_CONTRACT_TYPE",
                  "dictKey": ["CS_E_CONTRACT_TYPE"],
                  "format": "value-label"
                },
                "rules": [
                  {
                    "required": true,
                    "message": "请输入"
                  }
                ]
              },
              "ctrno": {
                "control": "string",
                "label": "合同编号"
              },
              "contract_name": {
                "control": "text",
                "label": "合同名称",
                "rules": [
                  {
                    "required": true,
                    "message": "请输入"
                  }
                ]
              },
              "sign_date": {
                "control": "dateTimePicker",
                "label": "合同签署日期",
                "editable": true,
                "format": "yyyyMMdd",
                "rules": [
                  {
                    "required": true,
                    "message": "请输入"
                  }
                ]
              },
              "daterange": {
                "control": "dateTimePicker",
                "label": "合同有效期",
                "type": "daterange",
                "unlinkPanels": true,
                "valueFormat": "yyyyMMdd",
                "startPlaceholder": "请选择开始时间",
                "endPlaceholder": "请选择结束时间",
                "startField": "start_date_c",
                "endField": "end_date_c",
                "rules": [
                  {
                    "required": true,
                    "message": "请输入"
                  }
                ]
              }
            }
          ]
        },
        {
          "title": "企业信息",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "accordion": false,
          "expand": true,
          "controls": [
            {
              "party_a_name": {
                "control": "text",
                "label": "甲方名称"
              },
              "party_a_contact": {
                "control": "text",
                "label": "甲方联系人"
              },
              "party_a_phone": {
                "control": "text",
                "label": "甲方联系方式"
              },
              "party_a_address": {
                "control": "text",
                "label": "甲方地址"
              },
              "party_a_taxe_no": {
                "control": "text",
                "label": "甲方纳税人识别号"
              },
              "party_a_bank_name": {
                "control": "text",
                "label": "甲方开户行名称"
              },
              "party_a_bank_account": {
                "control": "text",
                "label": "甲方开户行账号"
              },
              "party_b_name": {
                "control": "text",
                "label": "乙方名称"
              },
              "party_b_contact": {
                "control": "text",
                "label": "乙方联系人"
              },
              "party_b_phone": {
                "control": "text",
                "label": "乙方联系方式"
              },
              "party_b_address": {
                "control": "text",
                "label": "乙方地址"
              },
              "party_b_taxe_no": {
                "control": "text",
                "label": "乙方纳税人识别号"
              },
              "party_b_bank_name": {
                "control": "text",
                "label": "乙方开户行名称"
              },
              "party_b_bank_account": {
                "control": "text",
                "label": "乙方开户行账号"
              }
            }
          ]
        },
        {
          "title": "配置信息",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "accordion": false,
          "expand": true,
          "controls": [
            {
              "bill_type":{
                "control": "select",
                "label": "发票类型",
                "dict": {
                  "dictType": "CS_E_BILL_TYPE",
                  "dictKey": ["CS_E_BILL_TYPE"],
                  "format": "value-label"
                }
              },
              "stamp_tax_rate":{
                "control": "select",
                "label": "税率",
                "dict": {
                  "dictType": "CS_E_TAX_RATE",
                  "dictKey": ["CS_E_TAX_RATE"],
                  "format": "value-label"
                }
              },
              "settlement_frequency":{
                "control": "select",
                "label": "结算频度",
                "dict": {
                  "dictType": "CS_E_SETTLE_FREQ",
                  "dictKey": ["CS_E_SETTLE_FREQ"],
                  "format": "value-label"
                },
                "rules": [
                  {
                    "required": true,
                    "message": "请输入"
                  }
                ]
              },
              "settlement_day": {
                "control": "dateTimePicker",
                "label": "结算日",
                "editable": true,
                "format": "yyyyMMdd"
              },
              "remarks": {
                "control": "textarea",
                "label": "合同备注",
                "width": "180px",
                "align": "center",
                "span": "24",
                "rows": 6,
                "placeholder": "请输入"
              }
            }
          ]
        },
        {
          "title": "附件信息",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "accordion": false,
          "expand": true,
          "controls": [
            {
              "file": {
                "doRequest": {
                    "url": "/SUMP/minioCall/upload",
                    "method": "post",
                    "params": {
                      "org_id": "$g{{user_info.org_id}}"
                    },
                    "resultSet": {
                      "file_url": "path",
                      "file_name": "oldName",
                      "file_size": "size",
                      "file_type":"CONTRACT"
                    }
                  },
                  "width": "240px",
                  "control": "file",
                  "limit": 1,
                  "limitSize": 10,
                  "label": "上传文件",
                  "btnLable": "请选择文件"
              }
            }
          ]
        }
      ]
    }
  }
}
