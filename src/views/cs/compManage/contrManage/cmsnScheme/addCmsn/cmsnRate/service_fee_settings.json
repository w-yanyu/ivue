{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "btns": [],
            "controls": [
              {
                "service_fee_settings":{
                  "control": "radio",
                  "value":"close",
                  "label": "服务费设置",
                  "options": [
                    {
                      "value": "open",
                      "label": "开启"
                    },
                    {
                      "value": "close",
                      "label": "关闭"
                    }
                  ],
                  "events": {
                    "showItem":{
                      "open":["service_fee_settings_open"],
                      "close":["service_fee_settings_close"]
                    }
                  }
                },
                "service_fee_settings_open": {
                  "span": 24,
                  "allowCreate": true,
                  "control": "dynamic",
                  "toRequest":{
                    "url":"/views/cs/compManage/contrManage/cmsnScheme/addCmsn/cmsnRate/service_fee_settings_open.json",
                    "params": {
                      "servicecode": "xxxxx",
                      "appId": "cbOnl"
                    }
                  }
                },
                "service_fee_settings_close": {
                  "span": 24,
                  "allowCreate": true,
                  "control": "dynamic",
                  "label": "",
                  "toRequest":{
                    "url":"/views/cs/compManage/contrManage/cmsnScheme/addCmsn/cmsnRate/service_fee_settings_close.json",
                    "params": {
                      "servicecode": "xxxxx",
                      "appId": "cbOnl"
                    }
                  }
                }
              }
            ]
        }
    }
}
