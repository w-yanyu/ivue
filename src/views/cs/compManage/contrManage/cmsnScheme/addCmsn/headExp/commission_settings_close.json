{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
          "btns": [
            {
              "type": "submit",
              "label": "提交",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "cb0311",
                  "commission_name": "${{params.commission_name}}",
                  "start_date": "${{params.start_date}}",
                  "end_date":"${{params.end_date}}",
                  "commission_calculation_plan":"${{params.commission_calculation_plan}}"
                }
              }
            }
          ],
            "controls": [
              {
                "head_price": {
                  "control": "text",
                  "label": "人头单价(元/人/月)",
                  "rules": [
                    {
                      "required": true,
                      "message": "请输入"
                    }
                  ]
                },
                "head_count": {
                  "control": "text",
                  "label": "人头数量(个)",
                  "rules": [
                    {
                      "required": true,
                      "message": "请输入"
                    }
                  ]
                }
              }
            ]
        }
    }
}
