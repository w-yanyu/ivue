{
  "module": "v_search_btn_datagrid",
  "layout": {
    "btns": [],
    "initRequest": "${{params}}",
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "target": "dialog",
          "label": "新增",
          "toRequest": {
            "method": "GET",
            "url": "/views/cs/compManage/contrManage/cmsnScheme/addCmsn/commission_scheme_add.json",
            "params": {
            }
          },
          "props": {
            "width": "70%",
            "height": "95%",
            "title": "新增佣金方案"
          }
        }
      ]
    },
    "datagrid": {
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "length": 999,
          "servicecode": "cb0310"
        }
      },
      "pagination": {
        "show": true,
        "pageSize": 10
      },
      "thead": [
        {
          "type": "index"
        },
        {
          "label": "佣金方案名称",
          "prop": "commission_name"
        },
        {
          "label": "佣金计算方案",
          "prop": "calculation_type"
        },
        {
          "label": "生效开始时间",
          "prop": "start_date"
        },
        {
          "label": "生效结束时间",
          "prop": "end_date"
        },
        {
          "label": "关联状态",
          "prop": "relation_status"
        },
        {
          "label": "操作",
          "fixed": "right",
          "align": "center",
          "btns": [
            {
              "type": "look",
              "label": "详情",
              "toRequest": {
                "url": "/views/cs/compManage/contrManage/cmsnScheme/viewCmsn/commission_scheme_info.json",
                "method": "POST",
                "params": {
                  "servicecode": "xxxxx",
                  "appId": "cbOnl",
                  "commission_calculation_plan": "001",
                  "commission_settings": "open",
                  "percentage_calculation_plan": "ind_results",
                  "indicator_results": "001",
                  "default_or_custom": "default"
                }
              },
              "props": {
                "width": "70%",
                "height": "95%",
                "title": "查看佣金方案"
              }
            }
          ]
        }
      ]
    }
  }
}
