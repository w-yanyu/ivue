{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
          "btns": [
          ],
            "controls": [
              {
                "batch_no":{
                  "control": "select",
                  "label": "批次号",
                  "options": [
                    {
                      "value": "001",
                      "label": "001"
                    },
                    {
                      "value": "002",
                      "label": "002"
                    },
                    {
                      "value": "003",
                      "label": "003"
                    }
                  ]
                }
              },
              {
                "default_or_custom":{
                  "control": "radio",
                  "value":"default",
                  "label": "逾期阶段设置",
                  "options": [
                    {
                      "value": "default",
                      "label": "默认逾期阶段"
                    },
                    {
                      "value": "custom",
                      "label": "自定义逾期阶段"
                    }
                  ],
                  "events": {
                    "showItem":{
                      "default":["default_or_custom_default"],
                      "custom":["default_or_custom_custom"]
                    }
                  }
                },
                "default_or_custom_default": {
                  "span": 24,
                  "allowCreate": true,
                  "control": "dynamic",
                  "toRequest":{
                    "url":"/views/cs/compManage/contrManage/cmsnScheme/viewCmsn/cmsnRate/default_or_custom_default.json",
                    "params": {
                      "servicecode": "xxxxx",
                      "appId": "cbOnl"
                    }
                  }
                },
                "default_or_custom_custom": {
                  "span": 24,
                  "allowCreate": true,
                  "control": "dynamic",
                  "toRequest":{
                    "url":"/views/cs/compManage/contrManage/cmsnScheme/viewCmsn/cmsnRate/default_or_custom_custom.json",
                    "params": {
                      "servicecode": "xxxxx",
                      "appId": "cbOnl"
                    }
                  }
                }
              }
            ]
        }
    }
}
