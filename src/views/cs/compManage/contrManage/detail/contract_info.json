{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "cb0302",
          "id": "${{params.id}}",
          "appId": "cbOnl"
          
        }
      },
      "controlsGroup": [
        {
          "title": "基础信息",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "accordion": false,
          "expand": true,
          "controls": [
            {
              "contract_type":{
                "control": "string",
                "label": "合同类别",
                "dict": {
                  "dictType": "CS_E_CONTRACT_TYPE",
                  "dictKey": ["CS_E_CONTRACT_TYPE"],
                  "format": "value-label"
                }
              },
              "ctrno": {
                "control": "string",
                "label": "合同编号"
              },
              "contract_name": {
                "control": "string",
                "label": "合同名称"
              },
              "sign_date": {
                "control": "dateTimePicker",
                "label": "合同签署日期",
                "editable": true,
                "format": "yyyyMMdd",
                "disabled":true
              }
            }
          ]
        },
        {
          "title": "企业信息",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "accordion": false,
          "expand": true,
          "controls": [
            {
              "party_a_name": {
                "control": "text",
                "label": "甲方名称",
                "disabled":true
              },
              "party_a_contact": {
                "control": "text",
                "label": "甲方联系人",
                "disabled":true
              },
              "party_a_phone": {
                "control": "text",
                "label": "甲方联系方式",
                "disabled":true
              },
              "party_a_address": {
                "control": "text",
                "label": "甲方地址",
                "disabled":true
              },
              "party_a_taxe_no": {
                "control": "text",
                "label": "甲方纳税人识别号",
                "disabled":true
              },
              "party_a_bank_name": {
                "control": "text",
                "label": "甲方开户行名称",
                "disabled":true
              },
              "party_a_bank_account": {
                "control": "text",
                "label": "甲方开户行账号",
                "disabled":true
              },
              "party_b_name": {
                "control": "text",
                "label": "乙方名称",
                "disabled":true
              },
              "party_b_contact": {
                "control": "text",
                "label": "乙方联系人",
                "disabled":true
              },
              "party_b_phone": {
                "control": "text",
                "label": "乙方联系方式",
                "disabled":true
              },
              "party_b_address": {
                "control": "text",
                "label": "乙方地址",
                "disabled":true
              },
              "party_b_taxe_no": {
                "control": "text",
                "label": "乙方纳税人识别号",
                "disabled":true
              },
              "party_b_bank_name": {
                "control": "text",
                "label": "乙方开户行名称",
                "disabled":true
              },
              "party_b_bank_account": {
                "control": "text",
                "label": "乙方开户行账号",
                "disabled":true
              }
            }
          ]
        },
        {
          "title": "配置信息",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "accordion": false,
          "expand": true,
          "controls": [
            {
              "remarks": {
                "control": "textarea",
                "label": "合同备注",
                "width": "180px",
                "align": "center",
                "span": "24",
                "rows": 6,
                "placeholder": "请输入",
                "disabled":true
              }
            }
          ]
        },
        {
          "title": "附件信息",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "accordion": false,
          "expand": true,
          "disabled":true,
          "controls": [
            {
              "file_name": {
                "control": "dynamic",
                "isShowChildForm": true,
                "span": 12,
                "toRequest": {
                  "url": "@SingleUpload",
                  "method": "get",
                  "params": {
                    "path": "collRec",
                    "uploadType": "sftp",
                    "formKey": "file_name",
                    "formLabel": "合同附件",
                    "accept": ".xlsx,.xls",
                    "required": "true",
                    "tipText": "只能上传xls/xlsx，且不超过10MB"
                  }
                }
              }
            }
          ]
        }
      ]
    }
  }
}
