{
  "module": "dynamicTabs",
  "layout": {
    "tabs": {
      "tabHeader": [
        {
          "id": "01",
          "label": "合同详情",
          "closable": false,
          "disabled": false,
          "icon": "el-icon-date",
          "toRequest": {
            "url": "/views/cs/compManage/contrManage/detail/contract_info.json",
            "method": "post",
            "params": {
              "id": "${{params.id}}"
            },
            "props": {
              "title": "合同详情"
            }
          }
        },
        {
          "id": "02",
          "label": "收付款",
          "closable": false,
          "disabled": false,
          "icon": "el-icon-date",
          "toRequest": {
            "url": "/views/cs/compManage/contrManage/detail/collection_and_payment.json",
            "method": "post",
            "params": {
              "id": "${{params.id}}"
            },
            "props": {
              "title": "收付款"
            }
          }
        }
      ],
      "defaultActiveKey": "01",
      "position": "top",
      "styleClass": "card"
    }
  }
}
