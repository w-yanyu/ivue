{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "cb2100",
          "apply_no": "${{params.apply_no}}"
        }
      },
      "btns": [],
      "controls": [
        {
          "work_order_id": {
            "control": "string",
            "label": "工单号",
            "span": 8,
            "width": "120px"
          },
          "work_order_type": {
            "control": "string",
            "label": "工单模板类型",
            "span": 8,
            "width": "120px"
          },
          "template_id": {
            "control": "string",
            "label": "模板编号",
            "span": 8,
            "width": "120px"
          },
          "template_name": {
            "control": "string",
            "label": "模板名称",
            "span": 8,
            "width": "120px"
          },
          "branch_id": {
            "control": "string",
            "label": "分支机构",
            "span": 8,
            "width": "120px"
          },
          "priority": {
            "control": "string",
            "label": "优先级",
            "span": 8,
            "width": "120px"
          },
          "create_user": {
            "control": "string",
            "label": "提单人",
            "span": 8,
            "width": "120px"
          },
          "copy_user": {
            "control": "string",
            "label": "抄送人",
            "span": 8,
            "width": "120px"
          },
          "case_id": {
            "control": "string",
            "label": "案件编号",
            "span": 8,
            "width": "120px"
          },
          "reminder_type": {
            "control": "string",
            "label": "提醒方式",
            "span": 8,
            "width": "120px"
          },
          "reminder_time": {
            "control": "string",
            "label": "提醒时间",
            "span": 8,
            "width": "120px"
          },
          "complaint_content": {
            "control": "textarea",
            "label": "投诉内容",
            "span": 24,
            "width": "120px",
            "rows":"4",
            "maxlength": 200,
            "showWordLimit": true,
            "disabled": true
          },
          "oa_org_id": {
            "control": "string",
            "label": "委外公司代码",
            "span": 8,
            "width": "120px"
          },
          "case_no": {
            "control": "string",
            "label": "主案件编号",
            "span": 8,
            "width": "120px"
          },
          "cust_id": {
            "control": "string",
            "label": "客户号",
            "span": 8,
            "width": "120px"
          },
          "cust_name": {
            "control": "string",
            "label": "客户姓名",
            "span": 8,
            "width": "120px"
          },
          "handle_user": {
            "control": "string",
            "label": "处理人",
            "span": 8,
            "width": "120px"
          },
          "end_date": {
            "control": "string",
            "label": "截止时间",
            "span": 8,
            "width": "120px"
          }
        }
      ]
    }
  }
}
