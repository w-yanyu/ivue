{
    "module": "v_search_btn_datagrid",
    "layout": {
      "form": {
        "scope": "formData",
        "controls": [
          {
            "batch_no": {
              "control": "text",
              "label": "分案批次号"
            },
            "case_id": {
              "control": "text",
              "label": "案件编号"
            }
          },
          {
            "cust_name": {
              "control": "text",
              "label": "客户姓名"
            },
            "outsourcing_branch_id": {
              "hidden": true,
              "label": "委外公司代码",
              "control": "remoteSelect",
              "labelField": "outsourcing_org_name",
              "valueField": "outsourcing_org_id",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "cb0052",
                  "org_id": "$g{{user_info.org_id}}"
                }
              },
              "events": {
                "emit": [
                  "operation"
                ]
              },
              "format": "value-label"
            }
          },
          {
            "start_time": {
              "label": "起始时间",
              "control": "dateTimePicker",
              "span": 12,
              "format": "yyyy-MM-dd HH:mm:ss"
            },
            "end_time": {
              "label": "结束时间",
              "control": "dateTimePicker",
              "span": 12,
              "format": "yyyy-MM-dd HH:mm:ss"
            }
          },
          {
            "target_collector": {
              "hidden": true,
              "label": "催收员姓名",
              "control": "remoteSelect",
              "labelField": "collector_name",
              "valueField": "collector",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode":"cb0043",
                  "coll_team":"${{current.coll_team}}"
                }
              },
              "events": {
                "emit": [
                  "operation"
                ]
              },
              "format": "value-label"
            }
          }
        ]
      },
      "datagrid": {
        "isInitSearch": false,
        "doRequest": {
          "url": "/SUMP/icmcall/icmRPCCall",
          "method": "POST",
          "params": {
            "servicecode": "qc0001",
            "appId": "qcOnl"
          }
        },
        "thead": [
          {
            "type": "string",
            "align": "center",
            "label": "质检内容序号",
            "prop": "quality_serial_no"
          },
          {
            "type": "string",
            "align": "center",
            "label": "案件编号",
            "prop": "case_id"
          },
          {
            "type": "string",
            "align": "center",
            "label": "分案批次号",
            "prop": "batch_no"
          },
          {
            "type": "string",
            "align": "center",
            "label": "客户姓名",
            "prop": "cust_name"
          },
          {
            "type": "string",
            "align": "center",
            "label": "证件号码",
            "prop": "cert_no"
          },
          {
            "type": "string",
            "align": "center",
            "label": "委外公司",
            "prop": "outsourcing_org_id"
          },
          {
            "type": "string",
            "align": "center",
            "label": "催收员",
            "prop": "collector"
          },
          {
            "align": "center",
            "width": "180px",
            "label": "起始时间",
            "format": "yyyy-MM-dd HH:mm:ss",
            "prop": "start_time"
          },
          {
            "type": "string",
            "align": "center",
            "width": "180px",
            "label": "结束时间",
            "format": "yyyy-MM-dd HH:mm:ss",
            "prop": "end_time"
          },
          {
            "type": "string",
            "align": "center",
            "width": "180px",
            "label": "逾期天数",
            "prop": "overdue_days"
          },
          {
            "type": "string",
            "align": "center",
            "width": "180px",
            "label": "催员方号码",
            "prop": "collector_number"
          },
          {
            "type": "string",
            "align": "center",
            "width": "180px",
            "label": "催员坐席号",
            "prop": "collector_seats_number"
          },
          {
            "type": "string",
            "align": "center",
            "width": "180px",
            "label": "电话状态",
            "prop": "phone_status",
            "dict": {
              "dictType": "CS_E_PHONE_STATUS",
              "dictKey": [
                "CS_E_PHONE_STATUS"
              ],
              "format": "value-label"
            }
          },
          {
            "type": "string",
            "align": "center",
            "width": "180px",
            "label": "案件状态",
            "prop": "case_status",
            "dict": {
              "dictType": "CS_E_STATUS_CD",
              "dictKey": [
                "CS_E_STATUS_CD"
              ],
              "format": "value-label"
            }
          },
          {
            "type": "string",
            "align": "center",
            "width": "180px",
            "label": "催收结果",
            "prop": "coll_result",
            "dict":{
              "dictKey": [
                "coll_result"
              ],
              "format": "value-label",
              "labelField": "coll_result_name",
              "valueField": "coll_result_code",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode":"cb0087",
                  "is_valid": "Y",
                  "appId": "cbOnl"
                }
              }
            }
          },
          {
            "label": "操作",
            "align": "center",
            "fixed": "right",
            "width": "250%",
            "btns": [
              {
                "type": "look",
                "label": "详情",
                "toRequest": {
                  "url": "/views/cs/qualityManage/qualityDataSource/qualityTestDataSource_info.json"
                },
                "props": {
                  "title": "质检详情"
                }
              },
              {
                "type": "look",
                "label": "质检明细数据信息",
                "toRequest": {
                  "url": "/views/cs/qualityManage/qualityDataSource/qualityTestDataSourceDetail_info.json",
                  "method": "POST",
                  "params": {

                  }

                },
                "props": {
                  "title": "质检明细数据信息"
                }
              }

            ]
          }
        ]
      }
    }
  }
