{
    "module": "v_search_btn_datagrid",
    "layout": {
      "datagrid": {
        "initRequest": "${{params}}",
        "doRequest": {
          "url": "/SUMP/icmcall/icmRPCCall",
          "method": "POST",
          "params": {
            "servicecode": "qc0003",
            "appId": "qcOnl",
            "quality_serial_no": "${{params.quality_serial_no}}"
          }
        },
        "thead": [
          {
            "type": "string",
            "align": "center",
            "label": "质检内容明细序号",
            "prop": "detail_serial_no"
          },
          {
            "type": "string",
            "align": "center",
            "label": "质检内容序号",
            "prop": "quality_serial_no"
          },
          {
            "type":"integer",
            "label": "通话方",
            "align":"center",
            "prop": "calling_party",
            "width": "130px",
            "dict": {
                "dictType": "CS_CALLING_PARTY",
                "dictKey": [
                    "CS_CALLING_PARTY"
                ],
                "format": "value-label"
            }
          },
          {
            "align": "center",
            "width": "180px",
            "label": "本句话起始时间",
            "format": "yyyy-MM-dd HH:mm:ss",
            "prop": "sentence_begin_time"
          },
          {
            "type": "string",
            "align": "center",
            "width": "180px",
            "label": "本句话结束时间",
            "format": "yyyy-MM-dd HH:mm:ss",
            "prop": "sentence_end_time"
          },
          {
            "type": "string",
            "align": "center",
            "label": "本句话时长",
            "prop": "sentence_duration"
          },
          {
            "type": "string",
            "align": "center",
            "label": "本句话文本",
            "prop": "sentence_content"
          },
          {
            "type": "string",
            "align": "center",
            "label": "本句话语速",
            "prop": "sentence_speech_speed"
          },
          {
            "type": "string",
            "align": "center",
            "label": "本句话语义标签",
            "prop": "sentence_semantic_label"
          },
          {
            "type": "string",
            "align": "center",
            "label": "本句话情绪标签",
            "prop": "sentence_affective_label"
          },
          {
            "type": "string",
            "align": "center",
            "label": "本句话话术流程",
            "prop": "sentence_speech_process"
          },
          {
            "type":"integer",
            "label": "上一句通话方",
            "align":"center",
            "prop": "before_calling_party",
            "width": "130px",
            "dict": {
                "dictType": "CS_CALLING_PARTY",
                "dictKey": [
                    "CS_CALLING_PARTY"
                ],
                "format": "value-label"
            }
          },
          {
            "type": "string",
            "align": "center",
            "label": "上句话起始时间",
            "prop": "before_sentence_begin_time"
          },
         
          {
            "type": "string",
            "align": "center",
            "width": "180px",
            "label": "上句话结束时间",
            "format": "yyyy-MM-dd HH:mm:ss",
            "prop": "before_sentence_end_time"
          },
          {
            "type": "string",
            "align": "center",
            "label": "上句话时长",
            "prop": "before_sentence_duration"
          }
        ]
      }
    }
  }