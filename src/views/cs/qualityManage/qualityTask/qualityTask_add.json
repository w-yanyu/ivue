{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "scope": "formdata",
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "qc1001",
                            "appId": "qcOnl",
                            "page_id": "/views/cs/qualityManage/qualityTask/qualityTask_add.json"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "quality_task_name": {
                        "control": "text",
                        "label": "质检任务名称",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            },
                            {
                                "max": 64,
                                "message": "最多可以输入 64 个字节"
                            }
                        ]
                    },
                    "quality_plan_no": {
                        "hidden": true,
                        "label": "质检方案",
                        "required": true,
                        "control": "remoteSelect",
                        "labelField": "quality_plan_name",
                        "valueField": "quality_plan_no",
                        "doRequest": {
                            "url": "/SUMP/icmcall/icmRPCCall",
                            "method": "POST",
                            "params": {
                                "servicecode": "qc0005",
                                "org_id": "$g{{user_info.org_id}}"
                            }
                        },
                        "events": {
                            "emit": [
                                "operation"
                            ]
                        },
                        "format": "value-label"
                    }
                },
                {
                    "task_effective_mode": {
                        "control": "select",
                        "label": "任务生效方式",
                        "dict": {
                            "dictType": "CS_TASK_EFFECTIVE_MODE",
                            "dictKey": [
                                "CS_TASK_EFFECTIVE_MODE"
                            ],
                            "format": "value-label"
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择任务生效方式"
                            }
                        ],
                        "events": {
                            "condition": {
                                "now": {
                                    "control": {
                                        "task_effective_time": {
                                            "value": "",
                                            "disabled": true,
                                            "rules": [
                                                {
                                                    "required": false
                                                }
                                            ]
                                        }
                                    }
                                },
                                "reserve": {
                                    "control": {
                                        "task_effective_time": {
                                            "disabled": false,
                                            "rules": [
                                                {
                                                    "required": true,
                                                    "message": "请选择任务生效时间"
                                                }
                                            ]
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "task_effective_time": {
                        "label": "任务生效时间",
                        "control": "dateTimePicker",
                        "disabledDate": "dateBefore",
                        "span": 12,
                        "type": "datetime",
                        "format": "yyyy-MM-dd HH:mm:ss",
                        "rules": [
                            {
                                "required": false
                            }
                        ]
                    }
                },
                {
                    "task_trigger_type": {
                        "control": "select",
                        "label": "任务触发类型",
                        "required": true,
                        "selectFirstValue": false,
                        "dict": {
                            "dictType": "CS_TASK_TRIGGER_TYPE",
                            "dictKey": [
                                "CS_TASK_TRIGGER_TYPE"
                            ],
                            "format": "value-label"
                        },
                        "events": {
                            "condition": {
                                "single": {
                                    "control": {
                                        "task_trigger_frequency_num": {
                                            "value": "",
                                            "disabled": true,
                                            "rules": [
                                                {
                                                    "required": false
                                                }
                                            ]
                                        },
                                        "task_trigger_frequency": {
                                            "value": "",
                                            "disabled": true,
                                            "rules": [
                                                {
                                                    "required": false
                                                }
                                            ]
                                        }
                                    }
                                },
                                "multi": {
                                    "control": {
                                        "task_trigger_frequency": {
                                            "value": "",
                                            "disabled": false,
                                            "rules": [
                                                {
                                                    "required": true
                                                }
                                            ]
                                        },
                                        "task_trigger_frequency_num": {
                                            "value": "",
                                            "disabled": true,
                                            "rules": [
                                                {
                                                    "required": false
                                                }
                                            ]
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "task_trigger_frequency": {
                        "control": "select",
                        "label": "任务触发频率",
                        "selectFirstValue": false,
                        "required": false,
                        "dict": {
                            "dictType": "CS_TASK_TRIGGER_FREQUENCY",
                            "dictKey": [
                                "CS_TASK_TRIGGER_FREQUENCY"
                            ],
                            "format": "value-label"
                        },
                        "events": {
                            "condition": {
                                "seconds": {
                                    "control": {
                                        "task_trigger_frequency_num": {
                                            "value": "",
                                            "disabled": false,
                                            "control": "inputNumber",
                                            "controls": false,
                                            "min": 1,
                                            "max": 60,
                                            "align": "left",
                                            "rules": [
                                                {
                                                    "required": true,
                                                    "message": "请输入[1-60]之间的整数"
                                                }
                                            ]
                                        }
                                    }
                                },
                                "minutes": {
                                    "control": {
                                        "task_trigger_frequency_num": {
                                            "value": "",
                                            "disabled": false,
                                            "control": "inputNumber",
                                            "controls": false,
                                            "min": 1,
                                            "max": 60,
                                            "align": "left",
                                            "rules": [
                                                {
                                                    "required": true,
                                                    "message": "请输入[1-60]之间的整数"
                                                }
                                            ]
                                        }
                                    }
                                },
                                "hours": {
                                    "control": {
                                        "task_trigger_frequency_num": {
                                            "value": "",
                                            "disabled": false,
                                            "control": "inputNumber",
                                            "controls": false,
                                            "min": 1,
                                            "max": 24,
                                            "align": "left",
                                            "rules": [
                                                {
                                                    "required": true,
                                                    "message": "请输入[1-24]之间的整数"
                                                }
                                            ]
                                        }
                                    }
                                },
                                "days": {
                                    "control": {
                                        "task_trigger_frequency_num": {
                                            "value": "",
                                            "disabled": false,
                                            "control": "inputNumber",
                                            "controls": false,
                                            "min": 1,
                                            "max": 31,
                                            "align": "left",
                                            "rules": [
                                                {
                                                    "required": true,
                                                    "message": "请输入[1-31]之间的整数"
                                                }
                                            ]
                                        }
                                    }
                                },
                                "months": {
                                    "control": {
                                        "task_trigger_frequency_num": {
                                            "value": "",
                                            "disabled": false,
                                            "control": "inputNumber",
                                            "controls": false,
                                            "min": 1,
                                            "max": 12,
                                            "align": "left",
                                            "rules": [
                                                {
                                                    "required": true,
                                                    "message": "请输入[1-12]之间的整数"
                                                }
                                            ]
                                        }
                                    }
                                },
                                "years": {
                                    "control": {
                                        "task_trigger_frequency_num": {
                                            "value": "",
                                            "disabled": false,
                                            "control": "inputNumber",
                                            "controls": false,
                                            "min": 1,
                                            "max": 2,
                                            "align": "left",
                                            "rules": [
                                                {
                                                    "required": true,
                                                    "message": "请输入[1-2]之间的整数"
                                                }
                                            ]
                                        }
                                    }
                                },
                                "$default": {
                                    "control": {
                                        "task_trigger_frequency_num": {
                                            "value": "",
                                            "controls": false,
                                            "disabled": true,
                                            "min": 1,
                                            "max": 60,
                                            "align": "left",
                                            "rules": [
                                                {
                                                    "required": false
                                                }
                                            ]
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "task_trigger_frequency_num": {
                        "control": "inputNumber",
                        "label": "任务触发频率数值",
                        "width": "130",
                        "min": 1,
                        "max": 60,
                        "controls": false,
                        "disabled": true,
                        "align": "left",
                        "rules": [
                            {
                                "required": false
                            }
                        ]
                    }
                }
            ]
        }
    }
}