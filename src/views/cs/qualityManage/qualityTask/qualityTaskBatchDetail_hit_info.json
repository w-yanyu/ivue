{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "isInitSearch": true,
      "initRequest": "${{params}}",
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "qc1010",
          "appId": "qcOnl",
          "quality_batch_no": "${{params.quality_batch_no}}",
          "quality_rule_type": "${{params.quality_rule_type}}"
        }
      },
      "thead": [
        {
          "type": "string",
          "align": "center",
          "label": "质检内容序号",
          "prop": "quality_serial_no"
        },
        {
          "type": "string",
          "align": "center",
          "label": "质检批次号",
          "prop": "quality_batch_no"
        },
        {
          "type": "string",
          "align": "center",
          "label": "分案批次号",
          "prop": "batch_no"
        },
        {
          "type": "string",
          "align": "center",
          "label": "案件编号",
          "prop": "case_id"
        },
        {
          "type": "string",
          "align": "center",
          "label": "客户姓名",
          "prop": "cust_name"
        },
        {
          "type": "string",
          "align": "center",
          "label": "委外公司",
          "prop": "outsourcing_org_id"
        },
        {
          "type": "string",
          "align": "center",
          "label": "贷款机构",
          "prop": "branch_id"
        },
        {
          "type": "string",
          "align": "center",
          "label": "贷款产品",
          "prop": "loan_prod_cd"
        },
        {
          "type": "string",
          "align": "center",
          "label": "催收员",
          "prop": "collector"
        },
        {
          "type": "string",
          "align": "center",
          "label": "催收组",
          "prop": "coll_team"
        },
        {
          "type": "string",
          "align": "center",
          "label": "开始时间",
          "prop": "start_time"
        },
        {
          "type": "string",
          "align": "center",
          "label": "结束时间",
          "prop": "end_time"
        },
        {
          "type": "string",
          "align": "center",
          "label": "质检内容类型",
          "prop": "quality_content_type",
          "dict": {
            "dictType": "CS_QUALITY_CONTENT_TYPE",
            "dictKey": [
                "CS_QUALITY_CONTENT_TYPE"
            ],
            "format": "value-label"
           }
        },
        {
          "type": "string",
          "align": "center",
          "label": "质检员",
          "prop": "quality_inspector"
        },
        {
          "type": "string",
          "align": "center",
          "label": "质检状态",
          "prop": "quality_status",
          "dict": {
            "dictType": "CS_QUALITY_STATUS",
            "dictKey": [
                "CS_QUALITY_STATUS"
            ],
            "format": "value-label"
           }
        },
        {
          "type": "string",
          "align": "center",
          "label": "质检结论",
          "prop": "quality_conclusion",
          "dict": {
            "dictType": "CS_QUALITY_CONCLUSION",
            "dictKey": [
                "CS_QUALITY_CONCLUSION"
            ],
            "format": "value-label"
           }

        },
        {
          "type": "string",
          "align": "center",
          "label": "质检备注",
          "prop": "quality_remark"
        },
        {
          "label": "操作",
          "align": "center",
          "fixed": "right",
          "width": "100%",
          "btns": [
            {
              "type": "look",
              "label": "质检详情",
              "toRequest": {
                "url": "@qualityCheck",
                "method": "POST",
                "params": {

                }
              },
              "props": {
                "width": "65%",
                "title": "质检详情"
              }
            }
          ]
        }
      ]
    }
  }
}
