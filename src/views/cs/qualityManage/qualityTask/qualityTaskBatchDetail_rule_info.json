{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "isInitSearch": true,
      "initRequest": "${{params}}",
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "qc1007",
          "appId": "qcOnl",
          "quality_task_no": "${{params.quality_task_no}}"
        }
      },
      "thead": [
        {
          "type": "string",
          "label": "规则id",
          "prop": "rule_id",
          "align": "center"
        },
        {
          "type": "string",
          "label": "规则代码",
          "align": "center",
          "prop": "rule_code"
        },
        {
          "type": "string",
          "label": "规则名称",
          "prop": "rule_name",
          "align": "center"
        },
        {
          "label": "规则类型",
          "prop": "rule_type",
          "align": "center",
          "type": "string",
          "dict": {
            "dictType": "PD_E_RULE_TYPE",
            "dictKey": [
              "PD_E_RULE_TYPE"
            ],
            "format": "value-label"
          }
        }
      ]
    }
  }
}