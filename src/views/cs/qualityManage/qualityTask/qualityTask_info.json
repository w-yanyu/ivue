{
    "module": "v-realonlyForm-btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "controls": [
                {
                    "quality_task_no": {
                        "control": "string",
                        "label": "质检任务编号",
                        "width": "130px"
                    },
                    "quality_task_name": {
                        "control": "string",
                        "label": "质检任务名称",
                        "width": "130px"
                    }
                },
                {
                    "quality_plan_no": {
                        "control": "string",
                        "label": "质检方案编号",
                        "width": "130px"
                    },
                    "task_effective_mode": {
                        "control": "string",
                        "label": "任务生效方式",
                        "width": "130px",
                        "dict": {
                            "dictType": "CS_TASK_EFFECTIVE_MODE",
                            "dictKey": [
                                "CS_TASK_EFFECTIVE_MODE"
                            ],
                            "format": "value-label"
                        }
                    }
                },
                {
                    "task_effective_time": {
                        "control": "string",
                        "label": "任务生效时间",
                        "width": "130px"
                    },
                    "task_trigger_type": {
                        "control": "string",
                        "label": "任务触发类型",
                        "width": "130px",
                        "dict": {
                            "dictType": "CS_TASK_TRIGGER_TYPE",
                            "dictKey": [
                                "CS_TASK_TRIGGER_TYPE"
                            ],
                            "format": "value-label"
                        }
                    }
                },
                {
                    "task_trigger_frequency_num": {
                        "control": "string",
                        "label": "任务触发频率数值",
                        "width": "130px"
                    },
                    "task_trigger_frequency": {
                        "control": "string",
                        "label": "任务触发频率",
                        "width": "130px",
                        "dict": {
                            "dictType": "CS_TASK_TRIGGER_FREQUENCY",
                            "dictKey": [
                                "CS_TASK_TRIGGER_FREQUENCY"
                            ],
                            "format": "value-label"
                        }
                    }
                },
                {
                    "task_status": {
                        "control": "string",
                        "label": "任务状态",
                        "width": "130px",
                        "dict": {
                            "dictType": "CS_TASK_STATUS",
                            "dictKey": [
                                "CS_TASK_STATUS"
                            ],
                            "format": "value-label"
                        }
                    },
                    "branch_id": {
                        "control": "string",
                        "label": "创建机构",
                        "width": "130px"
                    }
                },
                {
                    "data_create_user": {
                        "control": "string",
                        "label": "创建人",
                        "width": "130px"
                    },
                    "data_create_time": {
                        "control": "string",
                        "label": "创建时间",
                        "width": "130px"
                    },
                    "task_execution_time": {
                        "control": "string",
                        "label": "预计执行时间",
                        "width": "130px"
                    }
                }
            ]
        }
    }
}