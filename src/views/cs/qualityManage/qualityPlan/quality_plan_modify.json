{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "qc0013"
            }
          }
        }
      ],
      "controls": [
        {
          "quality_plan_no": {
            "width": "130px",
            "control": "text",
            "disabled": true,
            "label": "质检方案编号"
          },
          "is_enable": {
            "control": "select",
            "label": "是否启用",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ],
            "dict": {
              "dictType": "CS_QUALITY_PLAN_STATE",
              "dictKey": [
                "CS_QUALITY_PLAN_STATE"
              ],
              "format": "value-label"
            }
          }
        },
        {
          "quality_plan_type": {
            "control": "select",
            "label": "方案类型",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ],
            "dict": {
              "dictType": "CS_QUALITY_PLAN_TYPE",
              "dictKey": [
                "CS_QUALITY_PLAN_TYPE"
              ],
              "format": "value-label"
            }
          },
          "quality_plan_name": {
            "width": "130px",
            "control": "text",
            "disabled": false,
            "label": "方案名称"
          }
        }
      ]
    }
  }
}