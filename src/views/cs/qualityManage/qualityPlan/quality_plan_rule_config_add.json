{
    "module": "v_inputDataForm_btn",
    "layout": {
      "form": {
        "initRequest": "${{params}}",
        "pagination": {
          "show": false
        },
        "btns": [
          {
            "type": "submit",
            "label": "提交",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": { 
                "servicecode": "qc0008",
                "appId": "qcOnl",
                "quality_plan_no": "${{params.quality_plan_no}}"
               }
            }
          }
        ],
        "controls": [
          {
            "quality_plan_no": {
              "control": "text",
              "disabled":true,
              "label": "质检内容序号",
              "width": "130px",
              "value":"${{params.quality_plan_no}}"
            },
            "quality_rule_type": {
              "control": "select",
              "label": "规则类型",
              "width": "130px",
              "selectFirstValue": false,
              "dict": {
                "dictType": "CS_QUALITY_PLAN_RULE_TYPE",
                "dictKey": [
                  "CS_QUALITY_PLAN_RULE_TYPE"
                ],
                "format": "value-label"
              }
            }
          }
        ]
      }
    }
  }