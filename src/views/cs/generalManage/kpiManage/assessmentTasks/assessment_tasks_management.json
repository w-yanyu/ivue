{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "scope": "formData",
      "controls": [
        {
          "oa_org_id": {
            "label": "委外公司",
            "control": "remoteSelect",
            "labelField": "outsourcing_org_name",
            "valueField": "outsourcing_org_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0052",
                "org_id": "$g{{user_info.org_id}}"
              }
            },
            "format": "value-label"
          },
          "coll_team": {
            "control": "inputTree",
            "label": "催收组",
            "idField": "coll_team",
            "nodeTypeField": "type",
            "pIdField": "parent_coll_team",
            "labelField": "coll_team_name",
            "events": {
              "emit": [
                "collector"
              ]
            },
            "nodeTypes": {
              "collTeam": {
                "icon": "el-icon-news",
                "children": [
                  "collTeam"
                ]
              }
            },
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0041"
              }
            },
            "format": "value-label"
          }
        },
        {
          "collector": {
            "control": "remoteSelect",
            "label": "催收员",
            "labelField": "user_name",
            "valueField": "user_id",
            "lazy": true,
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "appId": "cbOnl",
                "servicecode": "cb0084"
              }
            },
            "format": "value-label"
          },
          "batch_no": {
            "control": "text",
            "label": "批次号"
          }
        },
        {
          "stage_code": {
            "control": "remoteSelect",
            "label": "逾期阶段",
            "labelField": "stage_name",
            "valueField": "stage_code",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "length": 999,
                "servicecode": "cb0083"
              }
            },
            "format": "value-label"
          },
          "indicator_no": {
            "control": "remoteSelect",
            "label": "考核指标",
            "labelField": "indicator_alias",
            "valueField": "indicator_no",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "length": 999,
                "servicecode": "cb0313"
              }
            },
            "format": "value-label"
          }
        },
        {
          "frequency": {
            "control": "select",
            "label": "频度",
            "dict": {
              "dictType": "CS_E_INDICATOR_FREQUENCY",
              "dictKey": [
                "CS_E_INDICATOR_FREQUENCY"
              ],
              "format": "value-label"
            }
          },
          "daterange": {
            "control": "dateTimePicker",
            "label": "日期",
            "type": "daterange",
            "unlinkPanels": true,
            "valueFormat": "yyyyMMdd",
            "startPlaceholder": "请选择开始日期",
            "endPlaceholder": "请选择结束日期",
            "startField": "startDate",
            "endField": "endDate"
          }
        },
        {
          "org_id": {
            "label": "创建法人",
            "control": "remoteSelect",
            "labelField": "org_name",
            "valueField": "org_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2311"
              }
            },
            "format": "value-label"
          },
          "branch_id": {
            "label": "创建机构",
            "control": "remoteSelect",
            "labelField": "branch_name",
            "valueField": "branch_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2142",
                "user_id": "$g{{user_info.userCode}}",
                "branch_id": "$g{{user_info.branch_id}}",
                "brch_relation_code": "01"
              }
            },
            "format": "value-label"
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "icon": "el-icon-circle-plus",
          "target": "dialog",
          "label": "新增考核任务",
          "toRequest": {
            "method": "GET",
            "url": "/views/cs/generalManage/kpiManage/assessmentTasks/assessment_tasks_add.json"
          },
          "props": {
            "width": "60%",
            "title": "新增考核任务"
          }
        },
        {
          "type": "export",
          "label": "批量导入考核任务",
          "toRequest": {
            "method": "GET",
            "url": "/views/cs/generalManage/kpiManage/assessmentDmension/assessment_tasks_export.json"
          },
          "props": {
            "title": "批量导入考核任务"
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "cb0319"
        }
      },
      "thead": [
        {
          "type": "string",
          "label": "考核任务编号",
          "align": "center",
          "prop": "id"
        },
        {
          "type": "string",
          "label": "指标名称",
          "align": "center",
          "prop": "indicator_no",
          "dict": {
            "dictKey": [
              "indicator_no"
            ],
            "labelField": "indicator_alias",
            "valueField": "indicator_no",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0313",
                "indicator_no": "${{current.indicator_no}}"
              }
            },
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "label": "频度",
          "align": "center",
          "prop": "frequency",
          "dict": {
            "dictType": "CS_E_INDICATOR_FREQUENCY",
            "dictKey": [
              "CS_E_INDICATOR_FREQUENCY"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "label": "日期",
          "align": "center",
          "prop": "date"
        },
        {
          "type": "string",
          "align": "center",
          "label": "币种",
          "prop": "ccy_code",
          "dict": {
            "dictType": "MS_CURRENCY",
            "dictKey": [
              "MS_CURRENCY"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "委外公司",
          "dict": {
            "dictKey": [
              "oa_org_id"
            ],
            "labelField": "outsourcing_org_name",
            "valueField": "outsourcing_org_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0052",
                "outsourcing_org_id": "${{current.oa_org_id}}"
              }
            },
            "format": "value-label"
          },
          "prop": "oa_org_id"
        },
        {
          "type": "string",
          "label": "批次",
          "align": "center",
          "prop": "batch_no"
        },
        {
          "type": "string",
          "label": "逾期阶段",
          "align": "center",
          "prop": "stage_code",
          "dict": {
            "dictKey": [
              "stage_code"
            ],
            "labelField": "stage_name",
            "valueField": "stage_code",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0083",
                "stage_code": "${{current.stage_code}}"
              }
            },
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "label": "催收组",
          "align": "center",
          "prop": "coll_team"
        },
        {
          "type": "string",
          "label": "催收员",
          "align": "center",
          "prop": "collector"
        },
        {
          "type": "string",
          "label": "贷款产品",
          "align": "center",
          "prop": "loan_prod_cd"
        },
        {
          "type": "string",
          "label": "指标任务值",
          "align": "center",
          "prop": "indicator_task"
        },
        {
          "type": "string",
          "align": "center",
          "label": "创建法人",
          "dict": {
            "dictKey": [
              "org_id"
            ],
            "labelField": "org_name",
            "valueField": "org_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2311",
                "org_id": "${{current.org_id}}"
              }
            },
            "format": "value-label"
          },
          "prop": "org_id"
        },
        {
          "type": "string",
          "align": "center",
          "label": "创建机构",
          "dict": {
            "dictKey": [
              "branch_id"
            ],
            "labelField": "branch_name",
            "valueField": "branch_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2142",
                "branch_id": "${{current.branch_id}}"
              }
            },
            "format": "value-label"
          },
          "prop": "branch_id"
        },
        {
          "label": "操作",
          "fixed": "right",
          "align": "center",
          "btns": [
            {
              "type": "edit",
              "label": "维护",
              "toRequest": {
                "url": "/views/cs/generalManage/kpiManage/assessmentTasks/assessment_tasks_modify.json",
                "method": "POST",
                "params": {
                  "id": "${{current.id}}",
                  "oa_org_id": "${{current.oa_org_id}}",
                  "overdue_status": "${{current.overdue_status}}",
                  "batch_no": "${{current.batch_no}}",
                  "coll_org_id": "${{current.coll_org_id}}",
                  "indicator_task": "${{current.indicator_task}}",
                  "frequency": "${{current.frequency}}",
                  "collector": "${{current.collector}}"
                }
              },
              "props": {
                "width": "60%",
                "title": "考核任务维护"
              }
            }
          ]
        }
      ]
    }
  }
}