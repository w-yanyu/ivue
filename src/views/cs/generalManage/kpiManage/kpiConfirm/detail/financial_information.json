{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "cb0107",
          "appId": "cbOnl",
          "case_id": "${{params.case_id}}",
          "case_no": "${{params.case_no}}",
          "cust_id": "${{params.cust_id}}",
          "org_id": "${{params.org_id}}",
          "oa_org_id": "${{params.oa_org_id}}"
        }
      },
      "thead": [
        {
          "type": "string",
          "label": "联系人姓名",
          "align": "center",
          "prop": "relation_cust_name"
        },
        {
          "type": "string",
          "label": "联系人关系",
          "align": "center",
          "prop": "relationship",
          "dict": {
            "dictType": "CS_E_RELATIONSHIP",
            "dictKey": [
              "CS_E_RELATIONSHIP"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "label": "电话类型",
          "align": "center",
          "prop": "phone_type",
          "dict": {
            "dictType": "CS_E_PHONE_TYPE",
            "dictKey": [
              "CS_E_PHONE_TYPE"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "label": "电话号码",
          "align": "center",
          "prop": "phone_no"
        },
        {
          "type": "string",
          "label": "是否有效",
          "align": "center",
          "prop": "is_valid",
          "dict": {
            "dictType": "E_YESORNO",
            "dictKey": [
              "E_YESORNO"
            ],
            "format": "value-label"
          }
        }
      ]
    }
  }
}
