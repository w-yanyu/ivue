{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "cb0107",
          "appId": "cbOnl",
          "case_id": "${{params.cust_id}}",
          "case_no": "${{params.case_no}}",
          "cust_id": "${{params.cust_id}}",
          "org_id": "${{params.org_id}}",
          "oa_org_id": "${{params.oa_org_id}}"
        }
      },
      "thead": [
        {
          "type": "string",
          "label": "流转时间",
          "align": "center",
          "value": "2023-02-10",
          "prop": "transfer_time"
        },
        {
          "type": "string",
          "label": "流转类型",
          "align": "center",
          "prop": "transfer_type",
          "dict": {
            "dictType": "CS_E_RELATIONSHIP",
            "dictKey": [
              "CS_E_RELATIONSHIP"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "label": "案件流转至公司",
          "align": "center",
          "value": "xx催收公司",
          "prop": "case_transfer_company"
        },
        {
          "type": "string",
          "label": "案件流转至人员",
          "align": "center",
          "value": "zhangsan",
          "prop": "case_transfer_user"
        }
      ]
    }
  }
}
