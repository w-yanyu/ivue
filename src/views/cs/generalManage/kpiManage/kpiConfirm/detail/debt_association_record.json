{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "cb0107",
          "appId": "cbOnl",
          "case_id": "${{params.cust_id}}",
          "case_no": "${{params.case_no}}",
          "cust_id": "${{params.cust_id}}",
          "org_id": "${{params.org_id}}",
          "oa_org_id": "${{params.oa_org_id}}"
        }
      },
      "thead": [
        {
          "type": "string",
          "label": "案件编号",
          "align": "center",
          "value": "000000000002222",
          "prop": "case_id"
        },
        {
          "type": "string",
          "label": "分案批次号",
          "align": "center",
          "value": "000000000002222",
          "prop": "batch_case_no"
        },
        {
          "type": "string",
          "label": "债务人姓名",
          "align": "center",
          "value": "乐圣钞",
          "prop": "debtor_name"
        },
        {
          "type": "string",
          "label": "债协方案",
          "align": "center",
          "value": "债协方案00001",
          "prop": "debt_association_scheme"
        },
        {
          "type": "string",
          "label": "还款方式",
          "align": "center",
          "prop": "repayment_method",
          "dict": {
            "dictType": "CS_E_PHONE_TYPE",
            "dictKey": [
              "CS_E_PHONE_TYPE"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "label": "还款明细及附件",
          "align": "center",
          "value": "附件001",
          "prop": "repayment_details_attachments"
        },
        {
          "type": "string",
          "label": "债协原因",
          "align": "center",
          "value": "协商还款",
          "prop": "debt_association_reason"
        },
        {
          "type": "string",
          "label": "协商后还款金额",
          "align": "center",
          "value": "18500",
          "prop": "repayment_amount_after_negotiation"
        },
        {
          "type": "string",
          "label": "申请说明",
          "align": "center",
          "value": "协商还款1",
          "prop": "application_reason"
        },
        {
          "type": "string",
          "label": "审核状态",
          "align": "center",
          "value": "已审批",
          "prop": "approve_status",
          "dict": {
            "dictType": "E_YESORNO",
            "dictKey": [
              "E_YESORNO"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "label": "履约状态",
          "align": "center",
          "value": "已履约",
          "prop": "performance_status",
          "dict": {
            "dictType": "E_YESORNO",
            "dictKey": [
              "E_YESORNO"
            ],
            "format": "value-label"
          }
        }
      ],
      "pagination": {
        "show": true
      }
    }
  }
}
