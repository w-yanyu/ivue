{
  "module": "dynamicTabs",
  "layout": {
    "tabs": {
      "tabHeader": [
        {
          "id": "01",
          "label": "基础详情",
          "closable": false,
          "disabled": false,
          "icon": "el-icon-date",
          "toRequest": {
            "url": "/views/cs/generalManage/kpiManage/kpiConfirm/detail/base_info.json",
            "method": "post",
            "params": {
              "case_id": "${{params.case_id}}"
            },
            "props": {
              "title": "基础信息"
            }
          }
        },
        {
          "id": "02",
          "label": "贷款信息",
          "closable": false,
          "disabled": false,
          "icon": "el-icon-date",
          "toRequest": {
            "url": "/views/cs/generalManage/kpiManage/kpiConfirm/detail/loan_info.json",
            "method": "post",
            "params": {
              "case_id": "${{params.case_id}}"
            },
            "props": {
              "title": "贷款信息"
            }
          }
        },
        {
          "id": "03",
          "label": "计息信息",
          "closable": false,
          "disabled": false,
          "icon": "el-icon-date",
          "toRequest": {
            "url": "/views/cs/generalManage/kpiManage/kpiConfirm/detail/interest_accrual_records.json",
            "method": "post",
            "params": {
              "case_id": "${{params.case_id}}"
            },
            "props": {
              "title": "计息信息"
            }
          }
        },
        {
          "id": "04",
          "label": "财务事件",
          "closable": false,
          "disabled": false,
          "icon": "el-icon-date",
          "toRequest": {
            "url": "@financialEvents",
            "method": "post",
            "params": {
              "case_id": "${{params.case_id}}"
            }
          }
        },
        {
          "id": "05",
          "label": "债协记录",
          "closable": false,
          "disabled": false,
          "icon": "el-icon-date",
          "toRequest": {
            "url": "/views/cs/generalManage/kpiManage/kpiConfirm/detail/debt_association_record.json",
            "method": "post",
            "params": {
              "case_id": "${{params.case_id}}"
            },
            "props": {
              "title": "财务事件"
            }
          }
        },
        {
          "id": "06",
          "label": "流转记录",
          "closable": false,
          "disabled": false,
          "icon": "el-icon-date",
          "toRequest": {
            "url": "/views/cs/generalManage/kpiManage/kpiConfirm/detail/transfer_records.json",
            "method": "post",
            "params": {
              "case_id": "${{params.case_id}}"
            },
            "props": {
              "title": "财务事件"
            }
          }
        },
        {
          "id": "07",
          "label": "资产附件",
          "closable": false,
          "disabled": false,
          "icon": "el-icon-date",
          "toRequest": {
            "url": "/views/cs/generalManage/kpiManage/kpiConfirm/detail/asset_attachment.json",
            "method": "post",
            "params": {
              "case_id": "${{params.case_id}}"
            },
            "props": {
              "title": "财务事件"
            }
          }
        }
      ],
      "defaultActiveKey": "01",
      "position": "top",
      "styleClass": "card"
    }
  }
}
