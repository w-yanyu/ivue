{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "scope": "formData",
      "initRequest": "${{params}}",
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "claim_status": "1005",
              "servicecode": "cb0326"
            }
          }
        },
        {
          "type": "close",
          "label": "取消"
        }
      ],
      "controls": [
        {
          "serial_no": {
            "control": "text",
            "label": "还款流水号",
            "disabled": true,
            "hidden": true
          },
          "repay_amt": {
            "control": "text",
            "label": "总还款金额",
            "disabled": true,
            "value": ""
          }
        },
        {
          "coll_contribution_rate": {
            "control": "inputNumber",
            "label": "催收公司贡献比例(%)",
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          },
          "law_contribution_rate": {
            "control": "inputNumber",
            "label": "律师所贡献比例(%)",
            "min": 0,
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          }
        },
        {
          "coll_contribution_amount": {
            "control": "inputNumber",
            "label": "催收公司催收金额",
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          },
          "law_contribution_amount": {
            "control": "inputNumber",
            "label": "律师所催收金额",
            "min": 0,
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          }
        }
      ]
    }
  }
}