{
  "module": "v-form-editableDataGrid-btn",
  "layout": {
    "form": {
      "scope": "formData",
      "initRequest": "${{params}}",
      "controls": [
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "label": "新增",
          "icon": "el-icon-plus"
        },
        {
          "type": "batchDelete",
          "label": "删除",
          "icon": "el-icon-close"
        }
      ]
    },
    "datagrid": {
      "name": "model_customization",
       "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "py0004",
          "strategy_type": "OT",
          "appId": "pyOnl"
        }
      },
      "pagination": {
        "show": false
      },
      "allData":true,
      "thead": [
        {
          "type": "selection",
          "show": {
            "prop": "changkuan",
            "is": [
              "0"
            ]
          }
        },
        {
          "type": "text",
          "prop": "sort_num",
          "label": "逻辑顺序"
        },
        {
          "type": "text",
          "prop": "model_condition",
          "label": "计算条件"
        },
        {
          "type": "text",
          "prop": "model_formula",
          "label": "计算公式"
        },
        {
          "type": "text",
          "prop": "detail",
          "label": "说明"
        }
      ]
    },
    "footer": {
      "btns": []
    }
  }
}
