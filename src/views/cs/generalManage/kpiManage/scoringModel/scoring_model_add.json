{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "scope": "formData",
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "cb0324"
            }
          }
        },
        {
          "type": "close",
          "label": "取消"
        }
      ],
      "controlsGroup": [
        {
          "title": "模型配置",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "accordion": false,
          "expand": true,
          "controls": [
            {
              "model_no": {
                "control": "text",
                "label": "模型编码",
                "disabled":true
              },
              "model_name": {
                "control": "text",
                "label": "模型名称",
                "rules": [
                  {
                    "required": true,
                    "message": "请输入"
                  }
                ]
              }
            },
            {
              "model_template": {
                "control": "textarea",
                "label": "模型模板",
                "width": "180px",
                "align": "center",
                "span": "24",
                "rows": 6,
                "placeholder": "请输入",
                "rules": [
                  {
                    "required": true,
                    "message": "请输入"
                  }
                ]
              }
            },
            {
              "detail": {
                "control": "textarea",
                "label": "描述",
                "width": "180px",
                "align": "center",
                "span": "24",
                "rows": 6,
                "placeholder": "请输入"
              }
            }
          ]
        },
        {
          "title": "模型自定义变量配置",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "accordion": false,
          "expand": true,
          "controls": [
            {
              "model_customization": {
                "span": 24,
                "allowCreate": true,
                "control": "dynamic",
                "label": "",
                "toRequest":{
                  "url":"/views/cs/generalManage/kpiManage/scoringModel/model_customization_add.json"
                }
              }
            }
          ]
        },
        {
          "title": "模型条件公式配置",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "accordion": false,
          "expand": true,
          "controls": [
            {
              "model_condition_formula": {
                "span": 24,
                "allowCreate": true,
                "control": "dynamic",
                "label": "",
                "toRequest":{
                  "url":"/views/cs/generalManage/kpiManage/scoringModel/model_condition_formula_add.json"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
