{
  "module": "v-form-editableDataGrid-btn",
  "layout": {
    "form": {
      "scope": "formData",
      "initRequest": "${{params}}",
      "controls": [
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "label": "新增",
          "icon": "el-icon-plus"
        },
        {
          "type": "batchDelete",
          "label": "删除",
          "icon": "el-icon-close"
        }
      ]
    },
    "datagrid": {
      "name": "model_condition_formula",
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "cb0348",
          "model_no": "${{params.model_no}}"
        }
      },
      "pagination": {
        "show": false
      },
      "allData":true,
      "thead": [
        {
          "type": "selection",
          "show": {
            "prop": "changkuan",
            "is": [
              "0"
            ]
          }
        },
        {
          "type": "text",
          "prop": "sort_num",
          "label": "逻辑顺序"
        },
        {
          "type": "text",
          "prop": "variable_code",
          "label": "自定义变量"
        },
        {
          "type": "text",
          "prop": "variable_name",
          "label": "变量名称"
        },
        {
          "type": "text",
          "prop": "variable_value",
          "label": "变量值"
        },
        {
          "type": "text",
          "prop": "detail",
          "label": "说明"
        }
      ]
    },
    "footer": {
      "btns": []
    }
  }
}
