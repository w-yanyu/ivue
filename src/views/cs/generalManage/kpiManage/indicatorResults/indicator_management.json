{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "scope": "formData",
      "controls": [
        {
          "oa_org_id": {
            "hidden": true,
            "label": "委外公司",
            "control": "remoteSelect",
            "labelField": "outsourcing_org_name",
            "valueField": "outsourcing_org_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0052",
                "org_id": "$g{{user_info.org_id}}"
              }
            },
            "events": {
              "emit": [
                "operation"
              ]
            },
            "format": "value-label"
          },
          "batch_no": {
            "control": "text",
            "label": "批次号"
          }
        },
        {
          "stage_code": {
            "control": "remoteSelect",
            "label": "逾期阶段",
            "labelField": "stage_name",
            "valueField": "stage_code",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0083"
              }
            },
            "format": "value-label"
          },
          "indicator_no": {
            "label": "指标",
            "control": "remoteSelect",
            "format": "value-label",
            "labelField": "indicator_alias",
            "valueField": "indicator_no",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "length": 999,
                "servicecode": "cb0313"
              }
            }
          }
        },
        {
          "frequency": {
            "control": "select",
            "label": "频度",
            "dict": {
              "dictType": "CS_E_INDICATOR_FREQUENCY",
              "dictKey": [
                "CS_E_INDICATOR_FREQUENCY"
              ],
              "format": "value-label"
            }
          },
          "daterange": {
            "control": "dateTimePicker",
            "label": "日期",
            "type": "daterange",
            "unlinkPanels": true,
            "valueFormat": "yyyyMMdd",
            "startPlaceholder": "开始时间",
            "endPlaceholder": "结束时间",
            "startField": "start_date",
            "endField": "end_date"
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "cb0342"
        }
      },
      "thead": [
        {
          "type": "string",
          "label": "考核指标",
          "align": "center",
          "prop": "indicator_no",
          "dict": {
            "dictKey": [
              "indicator_no"
            ],
            "labelField": "indicator_alias",
            "valueField": "indicator_no",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0313",
                "indicator_no": "${{current.indicator_no}}"
              }
            },
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "频度",
          "prop": "frequency",
          "dict": {
            "dictType": "CS_E_INDICATOR_FREQUENCY",
            "dictKey": [
              "CS_E_INDICATOR_FREQUENCY"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "日期",
          "prop": "date"
        },
        {
          "type": "string",
          "align": "center",
          "label": "币种",
          "prop": "currency_type",
          "dict": {
            "dictType": "MS_CURRENCY",
            "dictKey": [
              "MS_CURRENCY"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "align": "center",
          "width": "150",
          "label": "委外公司",
          "prop": "oa_org_id",
          "dict": {
              "dictKey": [
                "oa_org_id"
              ],
              "format": "value-label",
              "labelField": "outsourcing_org_name",
              "valueField": "outsourcing_org_id",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "cb0052",
                  "org_id": "$g{{user_info.org_id}}"
                }
              }
            }
      },
        {
          "type": "string",
          "align": "center",
          "label": "批次",
          "prop": "batch_no"
        },
        {
          "type": "string",
          "label": "逾期阶段",
          "align": "center",
          "prop": "stage_code",
          "dict": {
            "dictKey": [
              "stage_code"
            ],
            "labelField": "stage_name",
            "valueField": "stage_code",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0083",
                "stage_code": "${{current.stage_code}}"
              }
            },
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "催收机构",
          "prop": "coll_org_name"
        },
        {
          "type": "string",
          "label": "催收员",
          "align": "center",
          "prop": "collector",
          "dict": {
            "dictKey": [
              "collector"
            ],
            "format": "value-label",
            "labelField": "user_name",
            "valueField": "user_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0084",
                "appId": "cbOnl"
              }
            }
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "放贷机构",
          "prop": "loan_org_na"
        },
        {
          "type": "string",
          "align": "center",
          "label": "贷款产品",
          "prop": "loan_product"
        },
        {
          "type": "string",
          "align": "center",
          "label": "委托方",
          "prop": "loan_org_na"
        },
        {
          "type": "string",
          "align": "center",
          "label": "放贷机构",
          "prop": "entrust_name"
        },
        {
          "type": "string",
          "align": "center",
          "label": "指标任务值",
          "prop": "indicator_task"
        },
        {
          "type": "string",
          "align": "center",
          "label": "指标值",
          "prop": "indicator_value"
        }
      ]
    }
  }
}