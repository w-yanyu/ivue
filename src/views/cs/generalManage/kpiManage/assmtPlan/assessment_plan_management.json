{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "scope": "formData",
      "controls": [
        {
          "solution_no": {
            "control": "text",
            "label": "方案编码"
          },
          "solution_name": {
            "control": "text",
            "label": "方案名称"
          }
        },
        {
          "daterange": {
            "control": "dateTimePicker",
            "label": "生效时间",
            "type": "daterange",
            "unlinkPanels": true,
            "valueFormat": "yyyyMMdd",
            "startPlaceholder": "请选择开始时间",
            "endPlaceholder": "请选择结束时间",
            "startField": "start_effect_time",
            "endField": "end_effect_time"
          },
          "is_enable": {
            "control": "select",
            "label": "是否启用",
            "dict": {
              "dictType": "CS_E_YESORNO",
              "dictKey": [
                "CS_E_YESORNO"
              ],
              "format": "value-label"
            }
          }
        },
        {
          "org_id": {
            "label": "创建法人",
            "control": "remoteSelect",
            "labelField": "org_name",
            "valueField": "org_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2311"
              }
            },
            "format": "value-label"
          },
          "branch_id": {
            "label": "创建机构",
            "control": "remoteSelect",
            "labelField": "branch_name",
            "valueField": "branch_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2142",
                "user_id": "$g{{user_info.userCode}}",
                "branch_id": "$g{{user_info.branch_id}}",
                "brch_relation_code": "01"
              }
            },
            "format": "value-label"
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "icon": "el-icon-circle-plus",
          "target": "dialog",
          "label": "新增",
          "toRequest": {
            "method": "GET",
            "url": "/views/cs/generalManage/kpiManage/assmtPlan/assessment_plan_add.json"
          },
          "props": {
            "width": "70%",
            "title": "新增方案"
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "cb0338"
        }
      },
      "thead": [
        {
          "type": "string",
          "label": "方案编码",
          "align": "center",
          "prop": "solution_no"
        },
        {
          "type": "string",
          "label": "方案名称",
          "align": "center",
          "prop": "solution_name"
        },
        {
          "type": "string",
          "label": "生效开始时间",
          "align": "center",
          "prop": "start_effect_time"
        },
        {
          "type": "string",
          "label": "生效结束时间",
          "align": "center",
          "prop": "end_effect_time"
        },
        {
          "type": "string",
          "align": "center",
          "label": "是否启用",
          "prop": "is_enable",
          "dict": {
            "dictType": "CS_E_YESORNO",
            "dictKey": [
              "CS_E_YESORNO"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "label": "描述",
          "align": "center",
          "prop": "solution_desc"
        },
        {
          "type": "string",
          "align": "center",
          "label": "创建法人",
          "dict": {
            "dictKey": [
              "org_id"
            ],
            "labelField": "org_name",
            "valueField": "org_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2311",
                "org_id": "${{current.org_id}}"
              }
            },
            "format": "value-label"
          },
          "prop": "org_id"
        },
        {
          "type": "string",
          "align": "center",
          "label": "创建机构",
          "dict": {
            "dictKey": [
              "branch_id"
            ],
            "labelField": "branch_name",
            "valueField": "branch_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2142",
                "branch_id": "${{current.branch_id}}"
              }
            },
            "format": "value-label"
          },
          "prop": "branch_id"
        },
        {
          "label": "操作",
          "fixed": "right",
          "align": "center",
          "width": "180px",
          "btns": [
            {
              "type": "edit",
              "label": "维护",
              "toRequest": {
                "url": "/views/cs/generalManage/kpiManage/assmtPlan/assessment_plan_modify.json",
                "method": "POST"
              },
              "props": {
                "width": "70%",
                "title": "方案维护"
              }
            },
            {
              "type": "edit",
              "label": "方案明细设置",
              "toRequest": {
                "url": "/views/cs/generalManage/kpiManage/assmtPlan/assessment_plan_details_settings.json",
                "method": "POST"
              },
              "props": {
                "width": "70%",
                "title": "方案明细设置"
              }
            }
          ]
        }
      ]
    }
  }
}