{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "foldLineNumber": 3,
      "scope": "formData",
      "controls": [
        {
          "frequency": {
            "control": "select",
            "label": "频度",
            "dict": {
              "dictType": "CS_E_INDICATOR_FREQUENCY",
              "dictKey": [
                "CS_E_INDICATOR_FREQUENCY"
              ],
              "format": "value-label"
            }
          },
          "daterange": {
            "control": "dateTimePicker",
            "label": "日期",
            "type": "daterange",
            "unlinkPanels": true,
            "valueFormat": "yyyyMMdd",
            "startPlaceholder": "请选择开始时间",
            "endPlaceholder": "请选择结束时间",
            "startField": "startDate",
            "endField": "endDate"
          }
        },
        {
          "oa_org_id": {
            "hidden": true,
            "label": "委外公司",
            "control": "remoteSelect",
            "labelField": "outsourcing_org_name",
            "valueField": "outsourcing_org_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0052",
                "org_id": "$g{{user_info.org_id}}"
              }
            },
            "events": {
              "emit": [
                "operation"
              ]
            },
            "format": "value-label"
          },
          "batch_no": {
            "control": "text",
            "label": "分案批次号"
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "export",
          "icon": "iconfont icon-daochu",
          "label": "导出",
          "doRequest": {
            "method": "POST",
            "url": "/SUMP/rest/file/commExport",
            "params": {
              "length": 999,
              "servicecode": "cb0291"
            }
          },
          "confirm": {
            "title": "提示信息",
            "msg": "确认导出案件吗？",
            "confirmButtonText": "确定",
            "cancelButtonText": "取消"
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "cb0291"
        }
      },
      "thead": [
        {
          "type": "string",
          "align": "center",
          "label": "日期",
          "prop": "reg_date"
        },
        {
          "type": "string",
          "align": "center",
          "label": "委外公司",
          "width": 150,
          "dict": {
            "dictKey": [
              "outsourcing_org_id"
            ],
            "labelField": "outsourcing_org_name",
            "valueField": "outsourcing_org_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0052",
                "outsourcing_org_id": "${{current.oa_org_id}}"
              }
            },
            "format": "value-label"
          },
          "prop": "oa_org_id"
        },
        {
          "type": "string",
          "align": "center",
          "label": "分案批次号",
          "prop": "batch_no"
        },
        {
          "type": "string",
          "align": "center",
          "label": "佣金方案",
          "prop": "commission_no"
        },
        {
          "type": "string",
          "label": "生效时间",
          "align": "center",
          "prop": "effect_date"
        },
        {
          "type": "string",
          "align": "center",
          "label": "佣金总额",
          "format": {
            "decimal": 2,
            "decimalPoint": ".",
            "thousandsPoint": ","
          },
          "prop": "commission_total_amt"
        },
        {
          "type": "string",
          "align": "center",
          "label": "应付总额",
          "format": {
            "decimal": 2,
            "decimalPoint": ".",
            "thousandsPoint": ","
          },
          "prop": "payable_amt"
        },
        {
          "type": "string",
          "align": "center",
          "label": "实付总额",
          "format": {
            "decimal": 2,
            "decimalPoint": ".",
            "thousandsPoint": ","
          },
          "prop": "realpay_amt"
        },
        {
          "type": "string",
          "align": "center",
          "label": "频度",
          "prop": "frequency",
          "dict": {
            "dictType": "CS_E_INDICATOR_FREQUENCY",
            "dictKey": [
              "CS_E_INDICATOR_FREQUENCY"
            ],
            "format": "value-label"
          }
        },
        {
          "label": "佣金计算明细",
          "align": "center",
          "width": "130px",
          "fixed": "right",
          "btns": [
            {
              "type": "edit",
              "label": "详情",
              "toRequest": {
                "url": "/views/cs/generalManage/checkAccountManage/commission_calculation_details.json",
                "method": "POST",
                "params": {
                  "serial_no": "${{current.serial_no}}"
                }
              },
              "props": {
                "width": "65%",
                "title": "佣金计算明细"
              }
            }
          ]
        }
      ]
    }
  }
}