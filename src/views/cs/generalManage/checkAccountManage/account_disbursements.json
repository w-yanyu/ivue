{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "foldLineNumber": 3,
      "controls": [

        {
          "payment_date": {
            "control": "dateTimePicker",
            "label": "付款日期",
            "type": "daterange",
            "unlinkPanels": true,
            "valueFormat": "yyyyMMdd",
            "startPlaceholder": "请选择开始时间",
            "endPlaceholder": "请选择结束时间",
            "startField": "start_date",
            "endField": "end_date"
          },
          "oa_org_id": {
            "hidden": true,
            "label": "委外公司",
            "control": "remoteSelect",
            "labelField": "outsourcing_org_name",
            "valueField": "outsourcing_org_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0052",
                "org_id": "$g{{user_info.org_id}}"
              }
            },
            "events": {
              "emit": [
                "operation"
              ]
            },
            "format": "value-label"
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "export",
          "label": "导出",
          "target": "dialog",
          "icon": "iconfont icon-daochu",
          "toRequest": {
            "method": "post",
            "url": "@exportOaFile",
            "params": {
              "servicecode": "RLMSPLTS1070",
              "regional_center":"${{formData.regional_center}}",
              "prodcd":"${{formData.prodcd}}",
              "function_cd":"${{formData.function_cd}}",
              "lncf_no":"${{formData.lncf_no}}",
              "cust_nm":"${{formData.cust_nm}}",
              "cert_id":"${{formData.cert_id}}"
            }
          },
          "props": {
            "width": "30%",
            "title": "导出"
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "cb0159",
          "appId": "cbOnl"
        }
      },
      "thead": [
        {
          "type": "selection",
          "align": "center",
          "width": "55",
          "fixed": "left"
        },
        {
          "type": "string",
          "align": "center",
          "label": "日期",
          "prop": "payment_date"
        },
        {
          "type": "string",
          "align": "center",
          "label": "委外公司",
          "prop": "outsourcing_org_name"
        },
        {
          "type": "string",
          "align": "center",
          "label": "实付佣金费用",
          "prop": "loan_amount"
        }

      ]
    }
  }
}
