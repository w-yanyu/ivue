{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "height": "80%",
      "scope": "formData",
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "cb0XXX",
              "appId": "cbOnl"
            }
          }
        },
        {
          "type": "close",
          "label": "取消"
        }
      ],
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "cb0159",
          "case_id": "${{params.case_id}}",
          "appId": "cbOnl"
        }
      },
      "controlsGroup": [
        {
          "title": "提示",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "accordion": false,
          "expand": true,
          "controls": [
            {
              "template_type": {
                "control": "select",
                "span": 6,
                "label": "模板类型",
                "value": "1-业务咨询",
                "options": [
                  {
                    "label": "客户投诉",
                    "value": "0"
                  },
                  {
                    "label": "业务咨询",
                    "value": "1"
                  },
                  {
                    "label": "结清证明",
                    "value": "2"
                  }
                ]
              },
              "template_name": {
                "control": "remoteSelect",
                "span": 8,
                "label": "模板名称",
                "labelField": "queue_name",
                "valueField": "queue_code",
                "value": "1-业务咨询模板",
                "options": [
                  {
                    "label": "客户投诉模板",
                    "value": "0"
                  },
                  {
                    "label": "业务咨询模板",
                    "value": "1"
                  },
                  {
                    "label": "结清证明模板",
                    "value": "2"
                  }
                ]
              },
              "order_name": {
                "control": "remoteSelect",
                "span": 8,
                "label": "工单名称",
                "labelField": "queue_name",
                "valueField": "queue_code",
                "###doRequest": {
                  "url": "/SUMP/call/RPCCall",
                  "method": "POST",
                  "params": {
                    "servicecode": "ce0402"
                  }
                },
                "format": "value-label"
              },
              "order_no": {
                "control": "text",
                "disabled": true,
                "span": 6,
                "value": "7070003316729581568",
                "label": "工单号"
              },
              "commit_user": {
                "control": "select",
                "span": 8,
                "label": "提单人",
                "value": "1-长亮科技管理员",
                "options": [
                  {
                    "label": "长亮科技管理员",
                    "value": "1"
                  },
                  {
                    "label": "系统管理员",
                    "value": "0"
                  }
                ]
              },
              "priority": {
                "control": "select",
                "span": 8,
                "label": "优先级",
                "value": "1-紧急",
                "options": [
                  {
                    "label": "紧急",
                    "value": "1"
                  },
                  {
                    "label": "一般",
                    "value": "0"
                  }
                ]
              },
              "deadline_time": {
                "control": "dateTimePicker",
                "span": 6,
                "label": "截止日期",
                "rules": [
                  {
                    "required": true,
                    "message": "请选择"
                  }
                ]
              },
              "cc_to": {
                "control": "select",
                "span": 8,
                "label": "抄送人",
                "options": [
                  {
                    "label": "长亮科技管理员",
                    "value": "1"
                  },
                  {
                    "label": "系统管理员",
                    "value": "0"
                  }
                ]
              },
              "related_cases": {
                "control": "text",
                "span": 8,
                "label": "关联案件"
              }
            }
          ]
        },
        {
          "title": "提醒信息",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "accordion": false,
          "expand": true,
          "controls": [
            {
              "reminder_time": {
                "control": "dateTimePicker",
                "span": 6,
                "label": "提醒时间",
                "rules": [
                  {
                    "required": true,
                    "message": "请选择"
                  }
                ]
              },
              "reminder_type": {
                "control": "select",
                "span": 8,
                "label": "提醒方式",
                "options": [
                  {
                    "label": "系统",
                    "value": "1"
                  },
                  {
                    "label": "邮件",
                    "value": "0"
                  }
                ]
              }
            }
          ]
        },
        {
          "title": "案件信息",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "accordion": false,
          "expand": true,
          "controls": [
          ]
        }
      ]
    }
  }
}
