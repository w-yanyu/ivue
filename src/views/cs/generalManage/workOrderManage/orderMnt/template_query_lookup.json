{
  "module": "v_search_btn_datagrid",
  "pageId": "page002",
  "layout": {
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "appId": "cbOnl",
          "servicecode": "cb0065",
          "work_order_type": "${{current.work_order_type}}"
        }
      },
      "thead": [
        {
          "type": "string",
          "label": "模板编号",
          "align": "center",
          "prop": "template_id"
        },
        {
          "type": "string",
          "label": "模板名称",
          "align": "center",
          "prop": "template_name"
        },
        {
          "type": "string",
          "align": "center",
          "label": "模板类型",
          "prop": "work_order_type",
          "dict": {
            "dictType": "CS_ORDER_TYPE",
            "dictKey": [
              "CS_ORDER_TYPE"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "label": "模板描述",
          "align": "center",
          "prop": "template_desc"
        },
        {
          "type": "string",
          "label": "模板状态",
          "align": "center",
          "prop": "template_status",
          "dict": {
            "dictType": "CS_TEMPLATE_STATUS",
            "dictKey": [
              "CS_TEMPLATE_STATUS"
            ],
            "format": "value-label"
          }
        },
        {
          "label": "操作",
          "align": "center",
          "fixed": "right",
          "btns": [
            {
              "type": "bringback",
              "label": "选择"
            }
          ]
        }
      ]
    }
  }
}