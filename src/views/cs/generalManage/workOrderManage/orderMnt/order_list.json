{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "foldLineNumber": 3,
      "scope": "formData",
      "controls": [
        {
          "work_order_type": {
            "control": "select",
            "label": "工单类别",
            "dict": {
              "dictType": "CS_ORDER_TYPE",
              "dictKey": [
                "CS_ORDER_TYPE"
              ],
              "format": "value-label"
            }
          },
          "template_name": {
            "control": "remoteSelect",
            "label": "模板名称",
            "labelField": "template_name",
            "valueField": "template_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0065"
              }
            },
            "format": "value-label"
          }
        },
        {
          "order_no": {
            "control": "text",
            "label": "工单编号"
          },
          "work_order_id": {
            "control": "remoteSelect",
            "label": "工单号",
            "labelField": "template_name",
            "valueField": "work_order_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0170"
              }
            },
            "format": "value-label"
          },
          "create_user": {
            "hidden": true,
            "label": "提单人",
            "control": "remoteSelect",
            "labelField": "user_name",
            "valueField": "user_id",
            "doRequest": {
              "url": "/SUMP/rest/auth/userManager",
              "method": "POST",
              "params": {
                "servicecode": "ct2002",
                "brch_relation_code": "01"
              }
            },
            "format": "value-label"
          },
          "handle_status": {
            "control": "select",
            "label": "处理状态",
            "dict": {
              "dictType": "CS_E_DEALSTATUS",
              "dictKey": [
                "CS_E_DEALSTATUS"
              ],
              "format": "value-label"
            }
          }
        },
        {
          "work_order_priority": {
            "control": "select",
            "label": "优先级",
            "dict": {
              "dictType": "CS_ORDER_PRIORITY",
              "dictKey": [
                "CS_ORDER_PRIORITY"
              ],
              "format": "value-label"
            }
          },
          "daterange": {
            "control": "dateTimePicker",
            "label": "截止日期",
            "type": "daterange",
            "valueFormat": "yyyyMMdd",
            "startPlaceholder": "选择开始时间",
            "endPlaceholder": "选择结束时间",
            "unlinkPanels": true,
            "startField": "start_time",
            "endField": "end_time"
          }
        },
        {
          "copy_user": {
            "hidden": true,
            "label": "抄送人",
            "control": "remoteSelect",
            "labelField": "user_name",
            "valueField": "user_id",
            "doRequest": {
              "url": "/SUMP/rest/auth/userManager",
              "method": "POST",
              "params": {
                "servicecode": "ct2002",
                "brch_relation_code": "01"
              }
            },
            "format": "value-label"
          },
          "case_id": {
            "label": "关联案件",
            "hidden": true,
            "control": "remoteSelect",
            "valueField": "case_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0136"
              }
            },
            "format": "value"
          }
        },
        {
          "org_id": {
            "label": "创建法人",
            "control": "remoteSelect",
            "labelField": "org_name",
            "valueField": "org_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2311",
                "org_id": "$g{{user_info.org_id}}"
              }
            },
            "format": "value-label"
          },
          "branch_id": {
            "label": "创建机构",
            "control": "remoteSelect",
            "labelField": "branch_name",
            "valueField": "branch_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2142",
                "user_id": "$g{{user_info.userCode}}",
                "branch_id": "$g{{user_info.branch_id}}",
                "brch_relation_code": "01"
              }
            },
            "format": "value-label"
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "icon": "el-icon-circle-plus",
          "target": "dialog",
          "###checkCountMin": 1,
          "label": "新增",
          "toRequest": {
            "method": "GET",
            "url": "/views/cs/generalManage/workOrderManage/orderMnt/order_add.json"
          },
          "props": {
            "width": "70%"
          }
        },
        {
          "type": "custom",
          "icon": "el-icon-edit",
          "target": "dialog",
          "checkCountMin": 1,
          "label": "发起流程",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "cb2000"
            }
          },
          "confirm": {
            "title": "提示信息",
            "msg": "确认发起流程吗？",
            "confirmButtonText": "确定",
            "cancelButtonText": "取消"
          }
        },
        {
          "type": "delete",
          "icon": "el-icon-delete",
          "checkCountMin": 1,
          "label": "撤回",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "cb0173"
            }
          },
          "confirm": {
            "title": "提示信息",
            "msg": "确认撤回吗？",
            "confirmButtonText": "确定",
            "cancelButtonText": "取消"
          }
        },
        {
          "type": "delete",
          "icon": "el-icon-delete",
          "target": "dialog",
          "checkCountMin": 1,
          "label": "关闭",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "cb0174"
            }
          },
          "confirm": {
            "title": "提示信息",
            "msg": "确认关闭吗？",
            "confirmButtonText": "确定",
            "cancelButtonText": "取消"
          }
        },
        {
          "type": "export",
          "icon": "el-icon-download",
          "target": "dialog",
          "checkCountMin": 1,
          "label": "导出",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "###"
            }
          },
          "confirm": {
            "title": "提示信息",
            "msg": "确认导出吗？",
            "confirmButtonText": "确定",
            "cancelButtonText": "取消"
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "cb0170",
          "appId": "cbOnl"
        }
      },
      "thead": [
        {
          "type": "selection",
          "align": "center",
          "width": "55",
          "fixed": "left"
        },
        {
          "type": "string",
          "align": "center",
          "label": "工单类别",
          "prop": "work_order_type",
          "dict": {
            "dictType": "CS_ORDER_TYPE",
            "dictKey": [
              "CS_ORDER_TYPE"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "工单号",
          "prop": "work_order_id"
        },
        {
          "type": "string",
          "align": "center",
          "label": "工单状态",
          "prop": "work_order_status",
          "dict": {
            "dictType": "CS_E_WORK_ORDER_TYPE",
            "dictKey": [
              "CS_E_WORK_ORDER_TYPE"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "提单人",
          "prop": "create_user"
        },
        {
          "type": "string",
          "align": "center",
          "label": "关联案件",
          "prop": "case_id"
        },
        {
          "type": "string",
          "align": "center",
          "label": "投诉内容",
          "prop": "complaint_content"
        },
        {
          "type": "string",
          "align": "center",
          "label": "优先级",
          "prop": "work_order_priority",
          "dict": {
            "dictType": "CS_ORDER_PRIORITY",
            "dictKey": [
              "CS_ORDER_PRIORITY"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "prop": "handle_status",
          "align": "center",
          "label": "处理状态",
          "dict": {
            "dictType": "CS_E_DEALSTATUS",
            "dictKey": [
              "CS_E_DEALSTATUS"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "截止日期",
          "prop": "end_date"
        },
        {
          "type": "string",
          "align": "center",
          "label": "创建时间",
          "prop": "data_create_time"
        },
        {
          "type": "string",
          "align": "center",
          "label": "抄送人",
          "prop": "copy_user"
        },
        {
          "type": "string",
          "align": "center",
          "label": "创建法人",
          "prop": "org_id"
        },
        {
          "type": "string",
          "align": "center",
          "label": "创建机构",
          "prop": "branch_id"
        },
        {
          "label": "操作",
          "align": "center",
          "fixed": "right",
          "width": "90",
          "btns": [
            {
              "type": "custom",
              "label": "编辑",
              "toRequest": {
                "url": "/views/cs/generalManage/workOrderManage/orderMnt/order_modify.json",
                "method": "POST",
                "params": {
                  "case_id": "${{current.case_id}}"
                }
              },
              "props": {
                "width": "70%",
                "title": "编辑"
              },
              "##show":{
                "expression":"'${{work_order_type}}' === '1'"
              }
            }
          ]
        }
      ]
    }
  }
}
