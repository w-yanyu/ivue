{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "scope": "formData",
      "controls": [
        {
          "payer_p": {
            "control": "text",
            "label": "付款方"
          },
          "payment_account": {
            "control": "text",
            "label": "付款账号"
          }
        },
        {
          "payment_time": {
            "format": "yyyyMMdd",
            "control": "dateTimePicker",
            "label": "付款日期",
            "valueFormat": "yyyyMMdd"
          },
          "payee": {
            "control": "text",
            "label": "收款方"
          }
        },
        {
          "payee_account": {
            "control": "text",
            "label": "收款账号"
          },
          "data_create_user": {
            "control": "text",
            "label": "登记人"
          }
        },
        {
          "reg_date": {
            "format": "yyyyMMdd",
            "control": "dateTimePicker",
            "label": "登记日期",
            "unlinkPanels": true,
            "valueFormat": "yyyyMMdd"
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "icon": "el-icon-circle-plus",
          "target": "dialog",
          "label": "付款登记",
          "toRequest": {
            "method": "GET",
            "url": "/views/cs/generalManage/paymentManage/paymentRegistration.json"
          },
          "props": {
            "width": "65%",
            "title": "新增付款登记"
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "cb0293"
        }
      },
      "thead": [
        {
          "type": "string",
          "align": "center",
          "label": "付款登记流水号",
          "prop": "serial_no"
        },
        {
          "type": "string",
          "align": "center",
          "label": "付款类型",
          "prop": "payment_type",
          "dict": {
            "dictType": "CS_PAYMENT_TYPE",
            "dictKey": [
              "CS_PAYMENT_TYPE"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "关联合同编号",
          "dict": {
            "dictKey": [
              "contract_no"
            ],
            "labelField": "contract_name",
            "valueField": "id_",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0300",
                "id_": "${{current.contract_no}}"
              }
            },
            "format": "value-label"
          },
          "prop": "contract_no"
        },
        {
          "type": "string",
          "align": "center",
          "label": "付款方",
          "prop": "payer_p"
        },
        {
          "type": "string",
          "align": "center",
          "label": "付款账号",
          "prop": "payment_account"
        },
        {
          "type": "string",
          "align": "center",
          "label": "付款方式",
          "prop": "payment_way",
          "dict": {
            "dictType": "CS_PAYMENT_WAY",
            "dictKey": [
              "CS_PAYMENT_WAY"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "付款日期",
          "prop": "payment_time"
        },
        {
          "type": "string",
          "align": "center",
          "label": "付款金额",
          "prop": "payment_amount"
        },
        {
          "type": "string",
          "align": "center",
          "label": "收款方",
          "prop": "payee"
        },
        {
          "type": "string",
          "align": "center",
          "label": "收款账号",
          "prop": "payee_account"
        },
        {
          "type": "string",
          "align": "center",
          "label": "附件名",
          "prop": "file_name"
        },
        {
          "type": "string",
          "label": "登记人",
          "align": "center",
          "width": "180px",
          "prop": "data_create_user",
          "dict": {
            "dictKey": [
              "data_create_user"
            ],
            "format": "value-label",
            "labelField": "user_name",
            "valueField": "user_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0084",
                "appId": "cbOnl"
              }
            }
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "登记日期",
          "prop": "reg_date"
        },
        {
          "type": "string",
          "align": "center",
          "label": "创建法人",
          "dict": {
            "dictKey": [
              "org_id"
            ],
            "labelField": "org_name",
            "valueField": "org_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2311",
                "org_id": "${{current.org_id}}"
              }
            },
            "format": "value-label"
          },
          "prop": "org_id"
        },
        {
          "type": "string",
          "align": "center",
          "label": "创建机构",
          "dict": {
            "dictKey": [
              "branch_id"
            ],
            "labelField": "branch_name",
            "valueField": "branch_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2142",
                "branch_id": "${{current.branch_id}}"
              }
            },
            "format": "value-label"
          },
          "prop": "branch_id"
        },
        {
          "label": "操作",
          "align": "center",
          "fixed": "right",
          "width": "150%",
          "btns": [
            {
              "type": "export",
              "label": "下载附件",
              "disabled": "'${{current.file_path}}' === ''",
              "doRequest": {
                "url": "/SUMP/minioCall/download",
                "method": "GET",
                "params": {
                  "path": "${{current.file_path}}",
                  "fileName": "${{current.file_name}}"
                }
              },
              "eventAfter": {
                "doParentCallback": true
              },
              "confirm": {
                "title": "提醒",
                "msg": "确定下载附件吗？",
                "confirmButtonText": "确定",
                "cancelButtonText": "取消"
              }
            },
            {
              "type": "delete",
              "label": "删除",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "cb0295",
                  "serial_no": "${{current.serial_no}}"
                }
              },
              "confirm": {
                "title": "提醒",
                "msg": "确定删除吗？",
                "confirmButtonText": "确定",
                "cancelButtonText": "取消"
              }
            }
          ]
        }
      ]
    }
  }
}