{
    "module": "v_search_btn_datagrid",
    "layout": {
        "datagrid": {
            "isInitSearch": false,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "cb0115",
                    "case_id": "${{params.case_id}}"
                }
            },
            "thead": [
                {
                    "type": "string",
                    "label": "流转时间",
                    "align": "center",
                    "prop": "data_create_time"
                },
                {
                    "type": "string",
                    "label": "流转类型",
                    "align": "center",
                    "prop": "trans_type",
                    "dict": {
                        "dictType": "CS_TRANS_TYPE",
                        "dictKey": [
                            "CS_TRANS_TYPE"
                        ],
                        "format": "value-label"
                    }
                },
                {
                    "type": "string",
                    "label": "来源法人",
                    "align": "center",
                    "prop": "source_org_id",
                    "dict": {
                        "dictKey": [
                            "source_org_id"
                        ],
                        "labelField": "org_name",
                        "valueField": "org_id",
                        "doRequest": {
                            "url": "/SUMP/icmcall/icmRPCCall",
                            "method": "POST",
                            "params": {
                                "servicecode": "ct2311",
                                "org_id": "${{current.source_org_id}}"
