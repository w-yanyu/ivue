                    "align": "center",
                    "prop": "target_collector",
                    "dict": {
                        "dictKey": [
                            "target_collector"
                        ],
                        "format": "value-label",
                        "labelField": "user_name",
                        "valueField": "user_id",
                        "doRequest": {
                            "url": "/SUMP/icmcall/icmRPCCall",
