                        "dictKey": [
                            "source_branch_id"
                        ],
                        "labelField": "branch_name",
                        "valueField": "branch_id",
                        "doRequest": {
                            "url": "/SUMP/icmcall/icmRPCCall",
                            "method": "POST",
                            "params": {
                                "servicecode": "ct2142",
                                "branch_id": "${{current.source_branch_id}}"
                            }
                        },
                        "format": "value-label"
                    }
                },
                {
                    "type": "string",
                    "label": "来源人员",
                    "align": "center",
                    "prop": "source_collector",
                    "dict": {
                        "dictKey": [
                            "source_collector"
                        ],
                        "format": "value-label",
                        "labelField": "user_name",
                        "valueField": "user_id",
                        "doRequest": {
                            "url": "/SUMP/icmcall/icmRPCCall",
                            "method": "POST",
                            "params": {
                                "servicecode": "cb0084",
                                "appId": "cbOnl"
                            }
                        }
                    }
                },
                {
                    "type": "string",
                    "label": "目的法人",
                    "align": "center",
                    "prop": "org_id",
                    "dict": {
                        "dictKey": [
                            "org_id"
                        ],
                        "labelField": "org_name",
                        "valueField": "org_id",
                        "doRequest": {
                            "url": "/SUMP/icmcall/icmRPCCall",
                            "method": "POST",
                            "params": {
                                "servicecode": "ct2311",
                                "org_id": "${{current.org_id}}"
                            }
                        },
                        "format": "value-label"
                    }
                },
                {
                    "type": "string",
                    "label": "目的机构",
                    "align": "center",
                    "prop": "branch_id",
                    "dict": {
                        "dictKey": [
                            "branch_id"
                        ],
                        "labelField": "branch_name",
                        "valueField": "branch_id",
                        "doRequest": {
                            "url": "/SUMP/icmcall/icmRPCCall",
