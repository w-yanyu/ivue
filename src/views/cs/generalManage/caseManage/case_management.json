{
  "module": "v_search_btn_datagrid",
  "pageId": "page001",
  "layout": {
    "form": {
      "foldLineNumber": 3,
      "scope": "formData",
      "controls": [
        {
          "branch_id": {
            "control": "inputTree",
            "label": "机构",
            "idField": "coll_team",
            "nodeTypeField": "type",
            "pIdField": "parent_coll_team",
            "labelField": "coll_team_name",
            "nodeTypes": {
              "branchTeam": {
                "icon": "el-icon-news",
                "children": [
                  "branchTeam"
                ]
              }
            },
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0041",
                "query_type": "branchTeam"
              }
            },
            "format": "value-label"
          },
          "cust_name": {
            "control": "text",
            "label": "客户姓名"
          }
        },
        {
          "cust_id": {
            "control": "text",
            "label": "客户号"
          },
          "cert_no": {
            "control": "text",
            "label": "证件号码"
          }
        },
        {
          "case_status": {
            "control": "select",
            "label": "案件状态",
            "dict": {
              "dictType": "CS_E_STATUS_CD",
              "dictKey": [
                "CS_E_STATUS_CD"
              ],
              "format": "value-label"
            }
          },
          "coll_type": {
            "control": "select",
            "label": "催收类型",
            "dict": {
              "dictType": "CS_COLL_TYPE",
              "dictKey": [
                "CS_COLL_TYPE"
              ],
              "format": "value-label"
            }
          }
        },
        {
          "stage_code": {
            "control": "remoteSelect",
            "label": "逾期阶段",
            "labelField": "stage_name",
            "valueField": "stage_code",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0083"
              }
            },
            "format": "value-label"
          },
          "classify_code": {
            "control": "select",
            "label": "案件类型",
            "dict": {
              "dictType": "CS_E_LOCATION_CD",
              "dictKey": [
                "CS_E_LOCATION_CD"
              ],
              "format": "value-label"
            }
          }
        },
        {
          "start_day": {
            "control": "inputNumber",
            "label": "逾期天数开始值"
          },
          "end_day": {
            "control": "inputNumber",
            "label": "逾期天数结束值"
          }
        },
        {
          "actualmaturity": {
            "control": "dateTimePicker",
            "label": "到期日",
            "format": "yyyyMMdd"
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "export",
          "icon": "iconfont icon-daochu",
          "label": "导出",
          "doRequest": {
            "method": "POST",
            "url": "/SUMP/rest/file/commExport",
            "params": {
              "length": 999,
              "servicecode": "cb0278"
            }
          },
          "confirm": {
            "title": "提示信息",
            "msg": "确认导出吗？",
            "confirmButtonText": "确定",
            "cancelButtonText": "取消"
          }
        },
        {
          "type": "custom",
          "icon": "el-icon-edit",
          "target": "dialog",
          "checkCountMin": 1,
          "label": "停催",
          "doRequest": {
            "method": "POST",
            "url": "/SUMP/icmcall/icmRPCCall",
            "params": {
              "servicecode": "cb0280",
              "case_no": "${{current.selectedData}}"
            }
          },
          "confirm": {
            "title": "提示信息",
            "msg": "确认停催吗？",
            "confirmButtonText": "确定",
            "cancelButtonText": "取消"
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "appId": "cbOnl",
          "servicecode": "cb0278"
        }
      },
      "thead": [
        {
          "type": "selection",
          "align": "center",
          "width": "55",
          "fixed": "left"
        },
        {
          "type": "string",
          "align": "center",
          "label": "机构",
          "prop": "branch_id",
          "dict": {
            "dictKey": [
              "branch_id"
            ],
            "labelField": "branch_name",
            "valueField": "branch_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2142",
                "branch_id": "${{current.branch_id}}"
              }
            },
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "label": "客户号",
          "align": "center",
          "prop": "cust_id"
        },
        {
          "type": "string",
          "label": "客户姓名",
          "align": "center",
          "prop": "cust_name"
        },
        {
          "type": "string",
          "align": "center",
          "label": "证件类型",
          "prop": "cert_type",
          "dict": {
            "dictType": "CS_E_CERT_TYPE",
            "dictKey": [
              "CS_E_CERT_TYPE"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "label": "证件号码",
          "align": "center",
          "prop": "cert_no"
        },
        {
          "type": "string",
          "align": "center",
          "label": "案件类型",
          "dict": {
            "dictKey": [
              "classify_code"
            ],
            "labelField": "classify_name",
            "valueField": "classify_code",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0005",
                "classify_code": "${{current.classify_code}}"
              }
            },
            "format": "value-label"
          },
          "prop": "classify_code"
        },
        {
          "type": "string",
          "align": "center",
          "label": "案件状态",
          "prop": "case_status",
          "dict": {
            "dictType": "CS_E_STATUS_CD",
            "dictKey": [
              "CS_E_STATUS_CD"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "队列名称",
          "prop": "queue_code",
          "dict": {
            "dictKey": [
              "queue_code"
            ],
            "format": "value-label",
            "labelField": "queue_name",
            "valueField": "queue_code",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0082"
              }
            }
          }
        },
        {
          "type": "string",
          "label": "到期日",
          "align": "center",
          "prop": "depute_end_date"
        },
        {
          "type": "string",
          "align": "center",
          "label": "逾期天数",
          "prop": "overdue_days"
        },
        {
          "type": "currency",
          "align": "center",
          "label": "逾期金额",
          "prop": "overdue_amount",
          "format": {
            "decimal": "2"
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "催收类型",
          "prop": "coll_type",
          "dict": {
            "dictType": "CS_COLL_TYPE",
            "dictKey": [
              "CS_COLL_TYPE"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "委外公司",
          "dict": {
            "dictKey": [
              "oa_org_id"
            ],
            "labelField": "outsourcing_org_name",
            "valueField": "outsourcing_org_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0052",
                "outsourcing_org_id": "${{current.oa_org_id}}"
              }
            },
            "format": "value-label"
          },
          "prop": "oa_org_id"
        },
        {
          "type": "string",
          "label": "最后操作时间",
          "align": "center",
          "prop": "data_update_time"
        },
        {
          "label": "操作",
          "align": "center",
          "fixed": "right",
          "width": "240%",
          "btns": [
            {
              "type": "custom",
              "label": "案件详情",
              "toRequest": {
                "url": "/views/cs/assistManage/caseDetail/caseDetail_2/case_detail_main.json",
                "method": "POST",
                "params": {
                  "case_id": "${{current.case_id}}",
                  "case_no": "${{current.case_no}}",
                  "cust_id": "${{current.cust_id}}",
                  "org_id": "${{current.org_id}}",
                  "oa_org_id": "${{current.oa_org_id}}"
                }
              },
              "props": {
                "width": "80%",
                "title": "案件详情"
              }
            },
            {
              "type": "edit",
              "label": "分案历史",
              "toRequest": {
                "url": "/views/cs/generalManage/caseManage/assignHis.json",
                "method": "POST",
                "params": {
                  "case_id": "${{current.case_id}}"
                }
              },
              "props": {
                "width": "80%",
                "title": "分案历史"
              }
            },
            {
              "type": "edit",
              "label": "队列历史",
              "toRequest": {
                "url": "/views/cs/generalManage/caseManage/queueHis.json",
                "method": "POST",
                "params": {
                  "case_no": "${{current.case_no}}"
                }
              },
              "props": {
                "width": "80%",
                "title": "队列历史"
              }
            }
          ]
        }
      ]
    }
  }
}