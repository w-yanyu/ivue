{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "foldLineNumber": 3,
      "controls": [
        {
          "cust_name": {
            "control": "text",
            "label": "客户姓名"
          },
          "id_no": {
            "control": "text",
            "label": "证件号码"
          }
        },
        {
          "daterange": {
            "control": "dateTimePicker",
            "label": "申请时间",
            "type": "daterange",
            "unlinkPanels": true,
            "valueFormat": "yyyyMMdd",
            "startPlaceholder": "请选择开始时间",
            "endPlaceholder": "请选择结束时间",
            "startField": "start_date",
            "endField": "end_date",
            "disabledDate": "dateAfter"
          },
          "apply_type": {
            "control": "select",
            "label": "申请类型",
            "dict": {
              "dictType": "E_ENTRYTYPE",
              "dictKey": ["E_ENTRYTYPE"],
              "format": "value-label"
            }
          }
        },
        {
          "approval_status": {
            "control": "select",
            "label": "审核状态",
            "default": "NC",
            "dict": {
              "dictType": "CS_E_CHECK_STATUS",
              "dictKey": ["CS_E_CHECK_STATUS"],
              "format": "value-label"
            }
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "cb0257",
          "appId": "cbOnl"
        }
      },
      "thead": [
        {
          "type": "selection",
          "align": "center",
          "width": "55",
          "fixed": "left"
        },
        {
          "type": "string",
          "align": "center",
          "label": "客户号",
          "prop": "cust_id"
        },
        {
          "type": "string",
          "align": "center",
          "label": "客户姓名",
          "prop": "cust_name"
        },
        {
          "type": "string",
          "align": "center",
          "label": "证件号码",
          "prop": "cert_no"
        },
        {
          "type": "string",
          "align": "center",
          "label": "手机号码",
          "prop": "phone_no"
        },
        {
          "type": "string",
          "align": "center",
          "label": "逾期天数",
          "prop": "overdue_days"
        },
        {
          "type": "string",
          "align": "center",
          "label": "逾期金额",
          "prop": "overdue_amount"
        },
        {
          "type": "string",
          "align": "center",
          "label": "贷款金额",
          "prop": "loan_amt"
        },
        {
          "type": "string",
          "align": "center",
          "label": "执行利率",
          "prop": "execution_Interest_rate"
        },
        {
          "type": "string",
          "align": "center",
          "label": "结算方式",
          "prop": "seltte_type"
        },
        {
          "type": "string",
          "label": "指定还款日期",
          "align": "center",
          "prop": "repayment_method"
        },
        {
          "type": "string",
          "align": "center",
          "label": "还款方式",
          "prop": "repayment_method"
        },
        {
          "type": "string",
          "label": "起始日期",
          "align": "center",
          "prop": "start_date"
        },
        {
          "type": "string",
          "label": "终止日期",
          "align": "center",
          "prop": "end_date"
        },
        {
          "type": "string",
          "align": "center",
          "label": "申请人",
          "prop": "apply_user"
        },
        {
          "type": "string",
          "align": "center",
          "label": "申请时间",
          "prop": "apply_time"
        },
        {
          "type": "string",
          "align": "center",
          "label": "申请说明",
          "prop": "apply_reason"
        },
        {
          "type": "string",
          "align": "center",
          "label": "申请类型",
          "prop": "apply_type"
        }
      ]
    }
  }
}
