{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "scope": "formData",
      "controls": [
        {
          "branch_id": {
            "control": "inputTree",
            "label": "机构",
            "idField": "coll_team",
            "nodeTypeField": "type",
            "pIdField": "parent_coll_team",
            "labelField": "coll_team_name",
            "nodeTypes": {
              "branchTeam": {
                "icon": "el-icon-news",
                "children": [
                  "branchTeam"
                ]
              }
            },
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0041",
                "query_type": "branchTeam"
              }
            },
            "format": "value-label"
          },
          "cust_name": {
            "control": "text",
            "label": "客户姓名"
          }
        },
        {
          "cert_no": {
            "control": "text",
            "label": "证件号码"
          },
          "phone_no": {
            "control": "text",
            "label": "手机号码"
          }
        },
        {
          "depute_end_date": {
            "control": "dateTimePicker",
            "label": "到期日"
          }
        },
        {
          "classify_code": {
            "control": "select",
            "label": "案件类型",
            "dict": {
              "dictType": "CS_E_LOCATION_CD",
              "dictKey": [
                "CS_E_LOCATION_CD"
              ],
              "format": "value-label"
            }
          },
          "coll_type": {
            "control": "select",
            "label": "催收类型",
            "dict": {
              "dictType": "CS_COLL_TYPE",
              "dictKey": [
                "CS_COLL_TYPE"
              ],
              "format": "value-label"
            }
          }
        },
        {
          "start_day": {
            "control": "inputNumber",
            "label": "逾期天数开始值"
          },
          "end_day": {
            "control": "inputNumber",
            "label": "逾期天数结束值"
          }
        },
        {
          "daterange": {
            "control": "dateTimePicker",
            "label": "交易日期",
            "type": "daterange",
            "startPlaceholder": "请选择开始时间",
            "endPlaceholder": "请选择结束时间",
            "unlinkPanels": true,
            "startField": "start_date",
            "endField": "end_date"
          },
          "queue_code": {
            "control": "remoteSelect",
            "label": "队列名称",
            "labelField": "queue_name",
            "valueField": "queue_code",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0082"
              }
            },
            "format": "value-label"
          }
        },
        {
          "case_status": {
            "control": "select",
            "label": "案件状态",
            "dict": {
              "dictType": "CS_E_STATUS_CD",
              "dictKey": [
                "CS_E_STATUS_CD"
              ],
              "format": "value-label"
            }
          },
          "stage_code": {
            "control": "remoteSelect",
            "label": "逾期阶段",
            "labelField": "stage_name",
            "valueField": "stage_code",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0083"
              }
            },
            "format": "value-label"
          }
        },
        {
          "ovd_amt_total_1": {
            "control": "inputNumber",
            "label": "欠款金额区间开始值"
          },
          "ovd_amt_total_2": {
            "control": "inputNumber",
            "label": "欠款金额区间结束值"
          }
        },
        {
          "collect_date_1": {
            "control": "inputNumber",
            "label": "入催天数区间开始值"
          },
          "collect_date_2": {
            "control": "inputNumber",
            "label": "入催天数区间结束值"
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "export",
          "icon": "iconfont icon-daochu",
          "label": "导出",
          "doRequest": {
            "method": "POST",
            "url": "/SUMP/rest/file/commExport",
            "params": {
              "length": 999,
              "servicecode": "cb0271"
            }
          },
          "confirm":{
            "title":"提示信息",
            "msg":"确认导出吗？",
            "confirmButtonText":"确定",
            "cancelButtonText":"取消"
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "appId": "cbOnl",
          "servicecode": "cb0271"
        }
      },
      "thead": [
        {
          "type": "string",
          "label": "机构",
          "align": "center",
          "prop": "branch_id",
          "dict": {
            "dictKey": [
              "branch_id"
            ],
            "labelField": "branch_name",
            "valueField": "branch_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2142",
                "branch_id": "${{current.branch_id}}"
              }
            },
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "客户编号",
          "prop": "cust_id"
        },
        {
          "type": "string",
          "align": "center",
          "label": "客户姓名",
          "prop": "cust_name"
        },
        {
          "type": "string",
          "align": "center",
          "label": "证件类型",
          "prop": "cert_type",
          "dict": {
            "dictType": "CS_E_CERT_TYPE",
            "dictKey": [
              "CS_E_CERT_TYPE"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "证件号码",
          "prop": "cert_no"
        },
        {
          "type": "string",
          "align": "center",
          "label": "还款总额",
          "format": {
            "thousand": ","
          },
          "prop": "total_repayment_amount"
        },
        {
          "type": "string",
          "align": "center",
          "label": "交易码",
          "prop": "trxn_code"
        },
        {
          "type": "string",
          "label": "交易币种",
          "align": "center",
          "prop": "currency_code",
          "dict": {
            "dictType": "MS_CURRENCY",
            "dictKey": [
              "MS_CURRENCY"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "交易日期",
          "prop": "trxn_date"
        },
        {
          "type": "string",
          "align": "center",
          "label": "交易时间",
          "prop": "tran_timestamp"
        },
        {
          "type": "string",
          "align": "center",
          "label": "案件类型",
          "prop": "classify_code",
          "dict": {
            "dictKey": [
              "classify_code"
            ],
            "labelField": "classify_name",
            "valueField": "classify_code",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0005",
                "classify_code": "${{current.classify_code}}"
              }
            },
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "催收类型",
          "dict": {
            "dictType": "CS_COLL_TYPE",
            "dictKey": [
              "CS_COLL_TYPE"
            ],
            "format": "value-label"
          },
          "prop": "coll_type"
        },
        {
          "type": "string",
          "align": "center",
          "label": "到期日",
          "prop": "depute_end_date"
        },
        {
          "type": "string",
          "align": "center",
          "label": "逾期天数",
          "prop": "overdue_days"
        }
      ]
    }
  }
}