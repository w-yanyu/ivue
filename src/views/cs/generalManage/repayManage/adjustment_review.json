{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "scope": "formData",
      "foldLineNumber": 3,
      "controls": [
        {
          "cust_name": {
            "control": "text",
            "label": "客户姓名"
          },
          
          "id_no": {
            "control": "text",
            "label": "证件号码"
          }
        },
        {
          "daterange": {
            "control": "dateTimePicker",
            "label": "申请时间",
            "type": "daterange",
            "unlinkPanels": true,
            "valueFormat": "yyyyMMdd",
            "startPlaceholder": "请选择开始时间",
            "endPlaceholder": "请选择结束时间",
            "startField": "start_date",
            "endField": "end_date",
            "disabledDate": "dateAfter"
          },
          "apply_type": {
            "control": "select",
            "label": "申请类型",
            "dict": {
              "dictType": "E_ENTRYTYPE",
              "dictKey": ["E_ENTRYTYPE"],
              "format": "value-label"
            }
          }
        },
        {
          
          "approval_status": {
            "control": "select",
            "label": "审核状态",
            "default": "NC",
            "dict": {
              "dictType": "CS_E_CHECK_STATUS",
              "dictKey": ["CS_E_CHECK_STATUS"],
              "format": "value-label"
            }
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "icon": "el-icon-edit",
          "target": "dialog",
          "checkCountMin": 1,
          "label": "批量审核",
          "toRequest": {
            "method": "GET",
            "url": "/views/cs/generalManage/repayManage/adjust_batapprove.json"
          }
        },
        {
          "type": "add",
          "icon": "el-icon-circle-check",
          "target": "dialog",
          "label": "全部核准",
          "disabled": "'${{formData.approval_status}}' === 'P' || '${{formData.approval_status}}' === 'R' || '${{formData.approval_status}}' === ''",
          "doRequest": {
            "method": "POST",
            "url": "/SUMP/icmcall/icmRPCCall",
            "params": {
              "appId": "cbOnl",
              "servicecode": "cb0263",
              "submit_type": "P",
              "approval_desc": "批量审核-同意",
              "approval_status": "NC",
              "cust_name": "${{formData.cust_name}}",
              "start_date": "${{formData.start_date}}",
              "end_date": "${{formData.end_date}}",
              "id_no": "${{formData.id_no}}",
              "apply_type": "${{formData.apply_type}}"
            }
          }
        },
        {
          "type": "add",
          "icon": "el-icon-circle-close",
          "target": "dialog",
          "label": "全部拒绝",
          "disabled": "'${{formData.approval_status}}' === 'P' || '${{formData.approval_status}}' === 'R' || '${{formData.approval_status}}' === ''",
          "####doRequest": {
            "method": "POST",
            "url": "/SUMP/icmcall/icmRPCCall",
            "params": {
              "appId": "cbOnl",
              "servicecode": "cb0263",
              "submit_type": "R",
              "approval_desc": "批量审核-拒绝",
              "approval_status": "NC",
              "cust_name": "${{formData.cust_name}}",
              "start_date": "${{formData.start_date}}",
              "end_date": "${{formData.end_date}}",
              "id_no": "${{formData.id_no}}",
              "apply_type": "${{formData.apply_type}}"
            }
          }
          
        }
      ]
    },
    "datagrid": {
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "cb0257",
          "appId": "cbOnl"
        }
      },
      "thead": [
        {
          "type": "selection",
          "align": "center",
          "width": "55",
          "fixed": "left"
        },
        {
          "type": "string",
          "align": "center",
          "label": "客户号",
          "prop": "cust_id"
        },
        {
          "type": "string",
          "align": "center",
          "label": "客户姓名",
          "prop": "cust_name"
        },
        {
          "type": "string",
          "align": "center",
          "label": "证件号码",
          "prop": "cert_no"
        },
        {
          "type": "string",
          "align": "center",
          "label": "手机号码",
          "prop": "phone_no"
        },
        {
          "type": "string",
          "align": "center",
          "label": "逾期天数",
          "prop": "overdue_days"
        },
        {
          "type": "string",
          "align": "center",
          "label": "逾期金额",
          "prop": "overdue_amount"
        },
        {
          "type": "string",
          "align": "center",
          "label": "贷款金额",
          "prop": "loan_amt"
        },
        {
          "type": "string",
          "align": "center",
          "label": "执行利率",
          "prop": "execution_Interest_rate"
        },
        {
          "type": "string",
          "align": "center",
          "label": "结算方式",
          "prop": "seltte_type"
        },
        {
          "type": "string",
          "label": "指定还款日期",
          "align": "center",
          "prop": "seltte_type"
        },
        {
          "type": "string",
          "align": "center",
          "label": "还款方式",
          "prop": "repayment_method"
        },
        {
          "type": "string",
          "label": "起始日期",
          "align": "center",
          "prop": "start_date"
        },
        {
          "type": "string",
          "label": "终止日期",
          "align": "center",
          "prop": "end_date"
        },
        {
          "type": "string",
          "align": "center",
          "label": "申请人",
          "prop": "apply_user"
        },
        {
          "type": "string",
          "align": "center",
          "label": "申请时间",
          "prop": "apply_time"
        },
        {
          "type": "string",
          "align": "center",
          "label": "申请说明",
          "prop": "apply_reason"
        },
        {
          "type": "string",
          "align": "center",
          "label": "申请类型",
          "prop": "apply_type",
          "dict": {
            "dictType": "CS_E_REGISTER_TYPE",
            "dictKey": ["CS_E_REGISTER_TYPE"],
            "format": "value-label"
          }
        },
        {
          "label": "操作",
          "align": "center",
          "fixed": "right",
          "width": "130",
          "btns": [
            {
              "type": "custom",
              "label": "案件详情",
              "toRequest": {
                "url": "/views/cs/assistManage/caseDetail/caseDetail_1/case_detail_main.json",
                "method": "POST",
                "params": {
                  "case_id": "${{current.case_id}}",
                  "case_no": "${{current.case_no}}",
                  "cust_id": "${{current.cust_id}}",
                  "org_id": "${{current.org_id}}",
                  "oa_org_id": "${{current.org_id}}"
                }
              },
              "props": {
                "width": "80%",
                "title": "案件详情"
              }
            }
          ]
        }
      ]
    }
  }
}
