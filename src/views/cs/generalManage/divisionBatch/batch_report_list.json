{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "isInitSearch": true,
      "initRequest": "${{params}}",
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "appId": "cbOnl",
          "servicecode": "cb0277",
          "batch_no": "${{params.batch_no}}"
        }
      },
      "thead": [
        {
          "type": "string",
          "prop": "collector",
          "align": "center",
          "label": "催收员"
        },
        {
          "type": "string",
          "label": "总案件",
          "align": "center",
          "prop": "case_cnt"
        },
        {
          "type": "string",
          "label": "还款成功数",
          "align": "center",
          "prop": "succ_repay_cnt"
        },
        {
          "type": "string",
          "label": "三天以上未跟进",
          "align": "center",
          "prop": "three_day_cnt"
        },
        {
          "type": "string",
          "label": "总金额",
          "align": "center",
          "prop": "total_amt"
        },
        {
          "type": "string",
          "label": "回收率",
          "align": "center",
          "prop": "recycle_rate"
        }
      ]
    }
  }
}