{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "cust_name": {
            "control": "text",
            "label": "客户姓名"
          },
          "cert_no": {
            "control": "text",
            "label": "证件号码"
          }
        }
      ]
    },
    "#toolbar": {
      "btns": [
        {
          "type": "custom",
          "icon": "el-icon-edit",
          "target": "dialog",
          "checkCountMin": 1,
          "label": "批量审核",
          "toRequest": {
            "method": "GET",
            "url": "/views/cs/assistManage/payDeduct/payDeductCheck/payDeductCheck_auditing.json"
          }
        },
        {
          "type": "custom",
          "icon": "el-icon-circle-check",
          "target": "dialog",
          "label": "全部核准",
          "disabled": "'${{current.check_status}}' === 'P' || '${{current.check_status}}' === 'R' || '${{current.check_status}}' === ''",
          "doRequest": {
            "method": "POST",
            "url": "/SUMP/icmcall/icmRPCCall",
            "params": {
              "servicecode": "pl1578",
              "submit_type": "P",
              "check_desc": "批量审核-同意",
              "check_status": "NC",
              "cust_name": "${{selection.cust_name}}",
              "id_no": "${{form.id_no}}",
              "apply_id": "${{form.apply_id}}"
            }
          }
        },
        {
          "type": "custom",
          "icon": "el-icon-circle-close",
          "target": "dialog",
          "label": "全部拒绝",
          "disabled": "'${{current.check_status}}' === 'P' || '${{current.check_status}}' === 'R' || '${{current.check_status}}' === ''",
          "doRequest": {
            "method": "POST",
            "url": "/SUMP/icmcall/icmRPCCall",
            "params": {
              "servicecode": "pl1578",
              "submit_type": "R",
              "check_desc": "批量审核-拒绝",
              "check_status": "NC",
              "start_date": "${{form.start_date}}",
              "id_no": "${{form.id_no}}",
              "apply_id": "${{form.apply_id}}"
            }
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "cb0261"
        }
      },
      "thead": [
        {
          "type": "string",
          "align": "center",
          "label": "客户姓名",
          "prop": "cust_name"
        },
        {
          "type": "string",
          "align": "center",
          "label": "证件号码",
          "prop": "cert_no"
        },
        {
          "type": "string",
          "align": "center",
          "label": "可划扣账户",
          "prop": "account_no"
        },
        {
          "type": "string",
          "align": "center",
          "label": "账户余额",
          "prop": "account_balance"
        },
        {
          "type": "string",
          "align": "center",
          "label": "逾期金额",
          "prop": "overdue_amount"
        },
        {
          "type": "string",
          "align": "center",
          "label": "申请人",
          "prop": "apply_user",
          "dict": {
            "dictKey": [
              "apply_user"
            ],
            "format": "value-label",
            "labelField": "user_name",
            "valueField": "user_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "pl0403"
              }
            }
          }
        },
        {
          "type": "currency",
          "align": "center",
          "label": "申请时间",
          "prop": "apply_time"
        },
        {
          "type": "string",
          "align": "center",
          "label": "申请说明",
          "prop": "apply_reason"
        },
        {
          "type": "string",
          "align": "center",
          "label": "审核人",
          "prop": "check_user",
          "dict": {
            "dictKey": [
              "check_user"
            ],
            "format": "value-label",
            "labelField": "user_name",
            "valueField": "user_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "pl0403"
              }
            }
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "审核时间",
          "prop": "approval_time"
        },
        {
          "type": "string",
          "align": "center",
          "label": "审核说明",
          "prop": "approval_desc"
        },
        {
          "type": "string",
          "align": "center",
          "label": "审核状态",
          "prop": "approval_status",
          "dict": {
            "dictType": "CS_E_CHECK_STATUS",
            "dictKey": [
              "CS_E_CHECK_STATUS"
            ],
            "format": "value-label"
          }
        },
        {
          "label": "操作",
          "align": "center",
          "fixed": "right",
          "width": "130px",
          "btns": [
            {
              "type": "custom",
              "label": "案件详情",
              "toRequest": {
                "url": "/views/cs/assistManage/caseDetail/caseDetail_2/case_detail_main.json",
                "method": "POST",
                "params": {
                  "case_id": "${{current.case_id}}"
                }
              },
              "props": {
                "width": "80%",
                "title": "案件详情"
              }
            }
          ]
        }
      ]
    }
  }
}