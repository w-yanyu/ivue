{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "foldLineNumber": 3,
      "scope": "formData",
      "controls": [
        {
          "cust_name": {
            "control": "text",
            "label": "客户姓名"
          },
          "cert_no": {
            "control": "text",
            "label": "证件号码"
          }
        },
        {
          "approval_status": {
            "control": "select",
            "label": "审核状态",
            "default": "NC",
            "dict": {
              "dictType": "CS_E_CHECK_STATUS",
              "dictKey": [
                "CS_E_CHECK_STATUS"
              ],
              "format": "value-label"
            }
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "custom",
          "icon": "el-icon-edit",
          "target": "dialog",
          "checkCountMin": 1,
          "label": "批量审核",
          "toRequest": {
            "method": "GET",
            "url": "/views/cs/assistManage/payDeduct/payDeductCheck/payDeductCheck_auditing.json"
          }
        },
        {
          "type": "custom",
          "icon": "el-icon-circle-check",
          "target": "dialog",
          "label": "全部核准",
          "disabled": "'${{formData.approval_status}}' === 'P' || '${{formData.approval_status}}' === 'R' || '${{formData.approval_status}}' === ''",
          "doRequest": {
            "method": "POST",
            "url": "/SUMP/icmcall/icmRPCCall",
            "params": {
              "servicecode": "cb0266",
              "submit_type": "P",
              
              "approval_desc": "批量审核-同意",
              "approval_status": "NC",
              "cust_name": "${{selection.cust_name}}",
              "id_no": "${{form.id_no}}",
              "apply_id": "${{form.apply_id}}"
            }
          },
          "confirm": {
            "title": "提醒",
            "msg": "确定将案件全部核准？",
            "confirmButtonText": "确定",
            "cancelButtonText": "取消"
          }
        },
        {
          "type": "custom",
          "icon": "el-icon-circle-close",
          "target": "dialog",
          "label": "全部拒绝",
          "disabled": "'${{formData.approval_status}}' === 'P' || '${{formData.approval_status}}' === 'R' || '${{formData.approval_status}}' === ''",
          "doRequest": {
            "method": "POST",
            "url": "/SUMP/icmcall/icmRPCCall",
            "params": {
              "servicecode": "cb0266",
              "submit_type": "R",
              "approval_desc": "批量审核-拒绝",
              "cust_name": "${{selection.cust_name}}",
              "approval_status": "NC",
              "id_no": "${{form.id_no}}",
              "apply_id": "${{form.apply_id}}"
            }
          },
          "confirm": {
            "title": "提醒",
            "msg": "确定将案件全部拒绝？",
            "confirmButtonText": "确定",
            "cancelButtonText": "取消"
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "cb0256",
          "approval_status": "${{formData.approval_status}}"
        }
      },
      "thead": [
        {
          "type": "selection",
          "align": "center",
          "width": "55",
          "fixed": "left",
          "show": {
            "expression": "'${{approval_status}}' === 'NC'"
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "客户姓名",
          "prop": "cust_name"
        },
        {
          "type": "string",
          "align": "center",
          "label": "证件号码",
          "prop": "cert_no"
        },
        {
          "type": "string",
          "align": "center",
          "label": "可划扣账户",
          "prop": "account_no"
        },
        {
          "type": "string",
          "align": "center",
          "label": "账户余额",
          "prop": "account_balance"
        },
        {
          "type": "string",
          "align": "center",
          "label": "逾期金额",
          "prop": "overdue_amount"
        },
        {
          "type": "string",
          "align": "center",
          "label": "申请人",
          "prop": "apply_user",
          "dict": {
            "dictType": "deciOnl",
            "dictKey": [
              ""
            ],
            "labelField": "user_name",
            "valueField": "user_id",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "length": 1000,
                  "start": 1,
                  "servicecode": "ct2502"
              }
            },
            "format": "value-label"
          }
        },
        {
          "type": "currency",
          "align": "center",
          "label": "申请时间",
          "prop": "apply_time"
        },
        {
          "type": "string",
          "align": "center",
          "label": "申请说明",
          "prop": "apply_reason"
        },
        {
          "label": "操作",
          "align": "center",
          "fixed": "right",
          "width": "130px",
          "btns": [
            {
              "type": "custom",
              "label": "案件详情",
              "toRequest": {
                "url": "/views/cs/assistManage/caseDetail/caseDetail_2/case_detail_main.json",
                "method": "POST",
                "params": {
                  "case_id": "${{current.case_id}}"
                }
              },
              "props": {
                "width": "80%",
                "title": "案件详情"
              }
            }
          ]
        }
      ]
    }
  }
}
