{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "appId": "cbOnl",
          "servicecode": "cb0086",
          "case_id": "${{params.case_id}}"
        }
      },
      "thead": [
        {
          "type": "string",
          "label": "催收时间",
          "align": "center",
          "prop": "coll_time"
        },
        {
          "type": "string",
          "label": "催收员",
          "align": "center",
          "prop": "collector",
          "dict": {
            "dictType": "deciOnl",
            "dictKey": [
              "collector"
            ],
            "labelField": "user_name",
            "valueField": "user_id",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "length": 1000,
                  "start": 1,
                  "servicecode": "ct2502"
              }
            },
            "format": "value-label"
          }
        },
        {
          "label": "客户关系",
          "prop": "relationship",
          "align": "center",
          "dict": {
            "dictType": "CS_E_RELATIONSHIP",
            "dictKey": [
              "CS_E_RELATIONSHIP"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "label": "联系姓名",
          "align": "center",
          "prop": "relation_cust_name"
        },
        {
          "type": "string",
          "label": "电话号码",
          "align": "center",
          "prop": "phone_no"
        },
        {
          "type": "string",
          "label": "是否接通",
          "align": "center",
          "prop": "action_code",
          "dict": {
            "dictType": "CS_E_ACTION_CODE",
            "dictKey": [
              "CS_E_ACTION_CODE"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "label": "内容类型",
          "align": "center",
          "prop": "message_type",
          "dict": {
            "dictType": "CS_E_MESSAGE_TYPE",
            "dictKey": [
              "CS_E_MESSAGE_TYPE"
            ],
            "format": "value-label"
          },
          "btns": [
            {
              "type": "look",
              "show": {
                "expression": "'${{current.message_type}}' === 'VOICE'"
              },
              "toRequest": {
                "url": "@voiceAudio",
                "method": "get",
                "params": {
                  "case_id": "$g{{params.case_id}}"
                }
              },
              "props": {
                "width": "45%",
                "title": "语音详情"
              }
            },
            {
              "type": "look",
              "show": {
                "expression": "'${{current.message_type}}' === 'IVR'"
              },
              "toRequest": {
                "url":"/views/cs/assistManage/caseDetail/messageDetail/ivr_info.json",
                "method": "POST",
                "params": {
                  "case_id": "$g{{params.case_id}}"
                }
              },
              "props": {
                "width": "45%",
                "title": "IVR语音详情"
              }
            },
            {
              "type": "look",
              "show": {
                "expression": "'${{current.message_type}}' === 'SMS'"
              },
              "toRequest": {
                "url":"/views/cs/assistManage/caseDetail/messageDetail/sms_info.json",
                "method": "POST",
                "params": {
                  "case_id": "$g{{params.case_id}}"
                }
              },
              "props": {
                "width": "45%",
                "title": "短信详情"
              }
            },
            {
              "type": "look",
              "show": {
                "expression": "'${{current.message_type}}' === 'EMAIL'"
              },
              "toRequest": {
                "url":"/views/cs/assistManage/caseDetail/messageDetail/email_info.json",
                "method": "POST",
                "params": {
                  "case_id": "$g{{params.case_id}}"
                }
              },
              "props": {
                "width": "45%",
                "title": "邮件详情"
              }
            }
          ]
        },
        {
          "type": "string",
          "label": "催收结果",
          "align": "center",
          "prop": "coll_result",
          "dict": {
            "dictKey": [
              "coll_result"
            ],
            "format": "value-label",
            "labelField": "coll_result_name",
            "valueField": "coll_result_code",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "appId": "cbOnl",
                "servicecode": "cb0087"
              }
            }
          }
        },
        {
          "type": "string",
          "label": "备注",
          "align": "center",
          "prop": "remark"
        },
        {
          "type": "string",
          "label": "是否有效结果",
          "align": "center",
          "prop": "is_valid_result",
          "dict": {
            "dictType": "CS_E_YESORNO",
            "dictKey": [
              "CS_E_YESORNO"
            ],
            "format": "value-label"
          }
        }
      ]
    }
  }
}