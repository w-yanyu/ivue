{
  "module": "v_realonlyForm_btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "cb0096",
          "appId": "cbOnl",
          "case_id": "${{params.case_id}}",
          "case_no": "${{params.case_no}}",
          "cust_id": "${{params.cust_id}}",
          "org_id": "${{params.org_id}}",
          "oa_org_id": "${{params.oa_org_id}}"
        }
      },
      "controls": [
        {
          "cust_id": {
            "control": "string",
            "label": "客户编号",
            "span": "6"
          },
          "cust_name": {
            "control": "string",
            "label": "客户姓名",
            "span": "8"
          },
          "cert_no": {
            "control": "string",
            "label": "证件号码",
            "span": "8"
          },
          "loan_amount": {
            "control": "string",
            "label": "贷款金额",
            "span": "6",
            "format": {
              "decimal": "2"
            }
          },
          "current_balance": {
            "control": "string",
            "label": "当前余额",
            "span": "8",
            "format": {
              "decimal": "2"
            }
          },
          "normal_balance": {
            "control": "string",
            "label": "正常余额",
            "span": "8",
            "format": {
              "decimal": "2"
            }
          },
          "overdue_principal": {
            "control": "string",
            "label": "逾期本金",
            "span": "6",
            "format": {
              "decimal": "2"
            }
          },
          "overdue_balance": {
            "control": "string",
            "label": "逾期余额",
            "span": "8",
            "format": {
              "decimal": "2"
            }
          },
          "overdue_interest": {
            "control": "string",
            "label": "逾期利息",
            "span": "8",
            "format": {
              "decimal": "2"
            }
          },
          "principal_penalty_interest": {
            "control": "string",
            "label": "本金罚息",
            "span": "6",
            "format": {
              "decimal": "2"
            }
          },
          "interest_penalty": {
            "control": "string",
            "label": "利息罚息",
            "span": "8",
            "format": {
              "decimal": "2"
            }
          },
          "overdue_amount": {
            "control": "string",
            "label": "逾期金额",
            "span": "8",
            "format": {
              "decimal": "2"
            }
          },
          "interest_calculation_date": {
            "control": "string",
            "label": "贷款发放日",
            "span": "6"
          },
          "overdue_days": {
            "control": "string",
            "label": "逾期天数",
            "span": "8"
          },
          "total_debt_period": {
            "control": "string",
            "label": "当前欠供期数",
            "span": "8"
          },
          "loan_no": {
            "control": "string",
            "label": "借据号",
            "span": "6"
          }
        }
      ]
    }
  }
}
