{
  "module": "v-inputDataForm-btn",
  "layout": {
    "scope": "templateScope",
    "form": {
      "btns":[],
      "controls": [
          {
            "top": {
              "control":"dynamic",
              "span":24,
              "toRequest":{
                "url":"/views/cs/assistManage/caseDetail/caseDetail_2/case_detail_top.json",
                "method": "post",
                "params": {
                  "case_id":"${{params.case_id}}",
                  "case_no": "${{params.case_no}}",
                  "cust_id": "${{params.cust_id}}",
                  "org_id": "${{params.org_id}}",
                  "oa_org_id": "${{params.cs_org_id}}"
                }
              }
            },
            "bottom": {
              "control":"dynamic",
              "span":24,
              "toRequest":{
                "url":"/views/cs/assistManage/caseDetail/case_detail_bottom.json",
                "method": "post",
                "params": {
                  "case_id":"${{params.case_id}}",
                  "case_no": "${{params.case_no}}",
                  "cust_id": "${{params.cust_id}}",
                  "org_id": "${{params.org_id}}",
                  "oa_org_id": "${{params.cs_org_id}}"
                }
              }
            }
          }
      ]
    }
  }
}
