{
  "module": "v_realonlyForm_btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "appId": "cbOnl",
          "servicecode": "cb0090",
          "case_id": "${{params.case_id}}"
        }
      },
      "controls": [
        {
          "cust_name": {
            "control": "string",
            "label": "客户姓名",
            "span": "6"
          },
          "sex": {
            "control": "string",
            "label": "性别",
            "span": "8",
            "dict": {
              "dictType": "CS_E_SEX",
              "dictKey": [
                "CS_E_SEX"
              ],
              "format": "value-label"
            }
          },
          "birth_date": {
            "control": "string",
            "label": "出生日期",
            "span": "8"
          },
          "marital_status": {
            "control": "string",
            "label": "婚姻状况",
            "span": "6",
            "dict": {
              "dictType": "CS_E_MARITAL_STATUS",
              "dictKey": [
                "CS_E_MARITAL_STATUS"
              ],
              "format": "value-label"
            }
          },
          "cert_type": {
            "control": "string",
            "label": "证件类型",
            "span": "8",
            "dict": {
              "dictType": "CS_E_CERT_TYPE",
              "dictKey": [
                "CS_E_CERT_TYPE"
              ],
              "format": "value-label"
            }
          },
          "cert_no": {
            "control": "string",
            "label": "证件号码",
            "span": "8"
          },
          "phone_no": {
            "control": "string",
            "label": "手机号码",
            "span": "6"
          },
          "email": {
            "control": "string",
            "label": "电子邮箱",
            "span": "8"
          },
          "company_name": {
            "control": "string",
            "label": "工作单位",
            "span": "8"
          },
          "unit_nature": {
            "control": "string",
            "label": "单位类别",
            "span": "6",
            "dict": {
              "dictType": "CS_E_NUIT_NATURE",
              "dictKey": [
                "CS_E_NUIT_NATURE"
              ],
              "format": "value-label"
            }
          },
          "monthly_income": {
            "type": "currency",
            "control": "string",
            "label": "月收入",
            "span": "8",
            "prop": "current_balance",
            "format": {
              "decimal": "2"
            }
          },
          "emp_years": {
            "control": "string",
            "label": "现工龄",
            "span": "8"
          },
          "postal_address": {
            "control": "string",
            "label": "通讯地址",
            "span": "6"
          },
          "company_address_detail": {
            "control": "string",
            "label": "单位地址",
            "span": "8"
          },
          "company_phone": {
            "control": "string",
            "label": "单位电话",
            "span": "8"
          },
          "occupation": {
            "control": "string",
            "label": "职业",
            "span": "6",
            "dict": {
              "dictType": "MS_E_WORKTP",
              "dictKey": [
                "MS_E_WORKTP"
              ],
              "format": "value-label"
            }
          }
        }
      ]
    }
  }
}
