{
  "module": "dynamicTabs",
  "layout": {
    "tabs": {
      "scope": "formDate",
      "tabHeader": [
        {
          "id": "baseInfo",
          "label": "基本信息",
          "closable": false,
          "disabled": false,
          "icon": "el-icon-date",
          "toRequest": {
            "url": "/views/cs/assistManage/caseDetail/base_info.json",
            "method": "post",
            "params": {
              "case_id": "${{params.case_id}}"
            }
          }
        },
        {
          "id": "personalInfo",
          "label": "个人信息",
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "toRequest": {
            "url": "/views/cs/assistManage/caseDetail/caseDetail_2/personal_info.json",
            "method": "post",
            "params": {
              "case_id": "${{params.case_id}}"
            }
          }
        },
        {
          "id": "iousinfo",
          "label": "借据信息",
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "toRequest": {
            "url": "/views/cs/assistManage/caseDetail/ious_info.json",
            "method": "post",
            "params": {
              "case_id": "${{params.case_id}}"
            }
          }
        }
      ],
      "defaultActiveKey": "baseInfo",
      "position": "top",
      "styleClass": "card"
    }
  }
}
