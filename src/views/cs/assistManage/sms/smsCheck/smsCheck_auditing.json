{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "btns": [
        {
          "type": "submit",
          "label": "核准",
          "doRequest": {
            "method": "POST",
            "appId": "cbOnl",
            "url": "/SUMP/icmcall/icmRPCCall",
            "page_id": "/views/cs/assistManage/sms/smsCheck/smsCheck_auditing.json",
            "params": {
              "servicecode": "cb0242",
              "list01": "${{params.selectedData}}",
              "approval_status": "P"
            }
          }
        },
        {
          "type": "submit",
          "label": "拒绝",
          "doRequest": {
            "method": "POST",
            "appId": "cbOnl",
            "url": "/SUMP/icmcall/icmRPCCall",
            "page_id": "/views/cs/assistManage/sms/smsCheck/smsCheck_auditing.json",
            "params": {
              "servicecode": "cb0243",
              "list01": "${{params.selectedData}}",
              "approval_status": "R"
            }
          }
        }
      ],
      "controls": [
        {
          "approval_desc": {
            "control": "textarea",
            "label": "审核说明",
            "width": "180px",
            "align": "center",
            "span": "20",
            "rows": 6,
            "placeholder": "请输入",
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          }
        }
      ]
    }
  }
}
