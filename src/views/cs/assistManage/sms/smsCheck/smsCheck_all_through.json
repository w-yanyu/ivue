{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "btns": [
        {
          "type": "submit",
          "label": "确认",
          "doRequest": {
            "method": "POST",
            "url": "/SUMP/icmcall/icmRPCCall",
            "params": {
              "appId": "cbOnl",
              "servicecode": "cb0244",
              "page_id": "/views/cs/assistManage/sms/smsCheck/smsCheck_all_through.json",
