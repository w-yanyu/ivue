{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "foldLineNumber": 3,
      "controls": [
        {
          "branch_id": {
            "control": "inputTree",
            "label": "机构",
            "idField": "coll_team",
            "nodeTypeField": "type",
            "pIdField": "parent_coll_team",
            "labelField": "coll_team_name",
            "nodeTypes": {
              "branchTeam": {
                "icon": "el-icon-news",
                "children": [
                  "branchTeam"
                ]
              }
            },
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0041",
                "query_type": "branchTeam"
              }
            },
            "format": "value-label"
          },
          "cust_name": {
            "control": "text",
            "label": "客户姓名"
          }
        },
        {
          "daterange": {
            "control": "dateTimePicker",
            "label": "发送时间",
            "type": "daterange",
            "unlinkPanels": true,
            "valueFormat": "yyyyMMdd",
            "startPlaceholder": "请选择开始时间",
            "endPlaceholder": "请选择结束时间",
            "startField": "start_date",
            "endField": "end_date",
            "disabledDate": "dateAfter"
          },
          "cert_no": {
            "control": "text",
            "label": "证件号码"
          }
        },
        {
          "phone_no": {
            "control": "text",
            "label": "手机号码"
          },
          "cust_id": {
            "control": "text",
            "label": "客户号"
          }
        },
        {
          "loan_no": {
            "control": "text",
            "label": "借据号"
          },
          "businessname": {
            "control": "text",
            "label": "业务品种"
          }
        },
        {
          "template_code": {
            "control": "lookup",
            "label": "短信模板",
            "keyField": {
              "template_code": "template_id",
              "template_type": "template_type",
              "sms_content": "template_content"
            },
            "toRequest": {
              "url": "/views/cs/collManage/tcManage/collWorkbench/functionMenu/smsApply/prodMailSet_lookup.json",
              "method": "post",
              "params": {
                "org_id": "$g{{user_info.org_id}}"
              }
            }
          },
          "send_status": {
            "control": "select",
            "label": "发送状态",
            "dict": {
              "dictType": "CS_E_SEND_STATUS",
              "dictKey": [
                "CS_E_SEND_STATUS"
              ],
              "format": "value-label"
            }
          }
        },
        {
          "case_cust_type": {
            "control": "select",
            "label": "客户类型",
            "dict": {
              "dictType": "CS_E_CUST_TYPE",
              "dictKey": [
                "CS_E_CUST_TYPE"
              ],
              "format": "value-label"
            }
          },
          "classifyresult": {
            "control": "select",
            "label": "本期五级分类",
            "dict": {
              "dictType": "CS_E_CLASSIFYRESULT",
              "dictKey": [
                "CS_E_CLASSIFYRESULT"
              ],
              "format": "value-label"
            }
          }
        },
        {
          "collector": {
            "control": "text",
            "label": "催收员"
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "export",
          "icon": "iconfont icon-daochu",
          "label": "导出",
          "doRequest": {
            "method": "POST",
            "url": "/SUMP/rest/file/commExport",
            "params": {
              "length": 999,
              "servicecode": "cb0246"
            }
          },
          "confirm": {
            "title": "提示信息",
            "msg": "确认导出吗？",
            "confirmButtonText": "确定",
            "cancelButtonText": "取消"
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "appId": "cbOnl",
          "servicecode": "cb0246",
          "page_id": "/views/cs/assistManage/sms/smsHistory/smsHistory_management.json"
        }
      },
      "thead": [
        {
          "type": "string",
          "align": "center",
          "label": "机构",
          "width": "180px",
          "prop": "branch_id",
          "dict": {
            "dictKey": [
              "branch_id"
            ],
            "labelField": "branch_name",
            "valueField": "branch_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2142",
                "branch_id": "${{current.branch_id}}"
              }
            },
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "align": "center",
          "width": "180px",
          "label": "客户姓名",
          "prop": "cust_name"
        },
        {
          "type": "string",
          "align": "center",
          "width": "180px",
          "label": "发送时间",
          "prop": "send_date"
        },
        {
          "type": "string",
          "align": "center",
          "width": "180px",
          "label": "短信模板",
          "prop": "template_code"
        },
        {
          "type": "string",
          "align": "center",
          "width": "180px",
          "label": "短信内容",
          "prop": "sms_content"
        },
        {
          "type": "string",
          "align": "center",
          "label": "证件类型",
          "width": "180px",
          "prop": "cert_type",
          "dict": {
            "dictType": "CS_E_CERT_TYPE",
            "dictKey": [
              "CS_E_CERT_TYPE"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "align": "center",
          "width": "180px",
          "label": "证件号码",
          "prop": "cert_no"
        },
        {
          "type": "string",
          "align": "center",
          "width": "180px",
          "label": "手机号码",
          "prop": "phone_no"
        },
        {
          "type": "string",
          "align": "center",
          "width": "180px",
          "label": "发送类型",
          "prop": "send_type",
          "dict":{
            "dictType": "MS_E_SEND_TYPE",
            "dictKey": [
              "MS_E_SEND_TYPE"
            ],
            "format": "value-label"

          }
        },
        {
          "type": "string",
          "align": "center",
          "width": "180px",
          "label": "发送状态",
          "prop": "send_status",
          "dict": {
            "dictType": "CS_E_SEND_STATUS",
            "dictKey": [
              "CS_E_SEND_STATUS"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "align": "center",
          "width": "180px",
          "label": "客户号",
          "prop": "cust_id"
        },
        {
          "type": "string",
          "align": "center",
          "width": "180px",
          "label": "客户类型",
          "prop": "case_cust_type",
          "dict": {
            "dictType": "CS_E_CUST_TYPE",
            "dictKey": [
              "CS_E_CUST_TYPE"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "prop": "collector",
          "align": "center",
          "width": "180px",
          "label": "催收员",
          "dict": {
            "dictType": "deciOnl",
            "dictKey": [
              "collector"
            ],
            "labelField": "user_name",
            "valueField": "user_id",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "length": 1000,
                  "start": 1,
                  "servicecode": "ct2502"
              }
            },
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "align": "center",
          "width": "180px",
          "label": "审核人",
          "prop": "approval_user",
          "dict": {
            "dictType": "deciOnl",
            "dictKey": [
              "approval_user"
            ],
            "labelField": "user_name",
            "valueField": "user_id",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "length": 1000,
                  "start": 1,
                  "servicecode": "ct2502"
              }
            },
            "format": "value-label"
          }
        }
      ]
    }
  }
}