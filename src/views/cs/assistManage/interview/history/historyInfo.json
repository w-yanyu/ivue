{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "foldLineNumber": 3,
            "controls": [
                {
                    "cust_name": {
                        "control": "text",
                        "label": "客户姓名"
                    },
                    "cert_no": {
                        "control": "text",
                        "label": "证件号码"
                    }
                },
                {
                    "cust_id": {
                        "control": "text",
                        "label": "客户号"
                    }
                }
            ]
        },
        "toolbar": {},
        "datagrid": {
            "isInitSearch": false,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "cb0164"
                }
            },
            "thead": [
              {
                "type": "string",
                "label": "案件编号",
                "align": "center",
                "prop": "case_id"
              },
              {
                "type": "string",
                "align": "center",
                "label": "客户姓名",
                "prop": "cust_name"
              },
              {
                "type": "string",
                "align": "center",
                "label": "客户号",
                "prop": "cust_id"
              },
              {
                "type": "string",
                "align": "center",
                "label": "证件号码",
                "prop": "cert_no"
              },
              {
                "type": "string",
                "align": "center",
                "label": "被访人姓名",
                "prop": "interview_name"
              },
              {
                "type": "string",
                "align": "center",
                "label": "客户关系",
                "prop": "relationship",
                "dict": {
                  "dictType": "CS_E_RELATIONSHIP",
                  "dictKey": [
                    "CS_E_RELATIONSHIP"
                  ],
                  "format": "value-label"
                }
              },
              {
                "type": "string",
                "align": "center",
                "label": "外访日期",
                "prop": "interview_time"
              },
              {
                "type": "string",
                "align": "center",
                "label": "外访结果",
                "prop": "interview_result_details"
              },
              {
                "type": "string",
                "align": "center",
                "label": "备注",
                "prop": "remark"
              },
              {
                "label": "操作",
                "align": "center",
                "fixed": "right",
                "btns": [
                  {
                    "type": "custom",
                    "label": "案件详情",
                    "toRequest": {
                      "url": "/views/cs/assistManage/caseDetail/caseDetail_2/case_detail_main.json",
                      "method": "POST",
                      "params": {
                        "case_id": "${{current.case_id}}",
                        "case_no": "${{current.case_no}}",
                        "cust_id": "${{current.cust_id}}",
                        "org_id": "${{current.org_id}}",
                        "oa_org_id": "${{current.oa_org_id}}"
                      }
                    },
                    "props": {
                      "width": "80%",
                      "title": "案件详情"
                    }
                  }
                ]
              }
            ]
        }
    }
}
