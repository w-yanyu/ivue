{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "foldLineNumber": 3,
      "controls": [
        {
          "branch_id": {
            "control": "inputTree",
            "label": "机构",
            "idField": "coll_team",
            "nodeTypeField": "type",
            "pIdField": "parent_coll_team",
            "labelField": "coll_team_name",
            "nodeTypes": {
              "branchTeam": {
                "icon": "el-icon-news",
                "children": [
                  "branchTeam"
                ]
              }
            },
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0041",
                "query_type": "branchTeam"
              }
            },
            "format": "value-label"
          },
          "cust_name": {
            "control": "text",
            "label": "客户姓名"
          }
        },
        {
          "case_id": {
            "control": "text",
            "label": "案件编号"
          },
          "cust_id": {
            "control": "text",
            "label": "客户编号"
          }
        },
        {
          "cert_no": {
            "control": "text",
            "label": "证件号码"
          },
          "phone_no": {
            "control": "text",
            "label": "手机号码"
          }
        },
        {
          "case_status": {
            "control": "select",
            "label": "案件状态",
            "dict": {
              "dictType": "CS_E_STATUS_CD",
              "dictKey": [
                "CS_E_STATUS_CD"
              ],
              "format": "value-label"
            }
          },
          "handle_status": {
            "control": "select",
            "label": "处理状态",
            "dict": {
              "dictType": "CS_E_DEAL_STATE",
              "dictKey": [
                "CS_E_DEAL_STATE"
              ],
              "format": "value-label"
            }
          }
        },
        {
          "currencyRange": {
            "control": "currencyRange",
            "label": "逾期金额",
            "min": "1",
            "max": "16",
            "decimal": "2",
            "symbol": "￥",
            "valueFormat": "number",
            "startField": "min_amount",
            "endField": "max_amount"
          },
          "phone_status": {
            "control": "select",
            "label": "最近电话状态",
            "dict": {
              "dictType": "CS_E_PHONE_STATUS",
              "dictKey": [
                "CS_E_PHONE_STATUS"
              ],
              "format": "value-label"
            }
          }
        },
        {
          "last_coll_result": {
            "control": "remoteSelect",
            "label": "最近催收结果",
            "labelField": "coll_result_name",
            "valueField": "coll_result_code",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0087",
                "is_valid": "Y",
                "appId": "cbOnl"
              }
            },
            "format": "value-label"
          },
          "daterange": {
            "control": "dateTimePicker",
            "label": "最近催收时间",
            "type": "daterange",
            "unlinkPanels": true,
            "valueFormat": "yyyyMMdd",
            "startPlaceholder": "请选择开始时间",
            "endPlaceholder": "请选择结束时间",
            "startField": "start_date",
            "endField": "end_date"
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "custom",
          "icon": "el-icon-edit",
          "target": "dialog",
          "checkCountMin": 1,
          "label": "手工调整",
          "closeCurrent": true,
          "toRequest": {
            "method": "GET",
            "url": "/views/cs/assistManage/interview/assign/dispatch_scheme.json"
          }
        },
        {
          "type": "custom",
          "icon": "el-icon-upload",
          "target": "dialog",
          "label": "导入Excel分配",
          "toRequest": {
            "method": "GET",
            "url": "/views/cs/collManage/tcManage/manualAssign/manualAssign_Import.json"
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "cb0160",
          "appId": "cbOnl"
        }
      },
      "thead": [
        {
          "type": "selection",
          "align": "center",
          "width": "55",
          "fixed": "left"
        },
        {
          "type": "string",
          "align": "center",
          "label": "案件编号",
          "prop": "case_id"
        },
        {
          "type": "string",
          "align": "center",
          "label": "机构",
          "prop": "branch_id"
        },
        {
          "type": "string",
          "align": "center",
          "label": "客户号",
          "prop": "cust_id"
        },
        {
          "type": "string",
          "align": "center",
          "label": "客户姓名",
          "prop": "cust_name"
        },
        {
          "type": "string",
          "align": "center",
          "label": "证件类型",
          "prop": "cert_type",
          "dict": {
            "dictType": "CS_E_CERT_TYPE",
            "dictKey": [
              "CS_E_CERT_TYPE"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "证件号码",
          "prop": "cert_no"
        },
        {
          "type": "string",
          "align": "center",
          "label": "队列",
          "prop": "queue_code",
          "dict": {
            "dictKey": [
              "queue_code"
            ],
            "format": "value-label",
            "labelField": "queue_name",
            "valueField": "queue_code",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0082"
              }
            }
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "案件类型",
          "prop": "classify_code",
          "dict": {
            "dictType": "CS_E_LOCATION_CD",
            "dictKey": [
              "CS_E_LOCATION_CD"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "案件状态",
          "prop": "case_status",
          "dict": {
            "dictType": "CS_E_STATUS_CD",
            "dictKey": [
              "CS_E_STATUS_CD"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "入催日期",
          "prop": "collect_date"
        },
        {
          "type": "string",
          "align": "center",
          "label": "逾期天数",
          "prop": "overdue_days"
        },
        {
          "type": "currency",
          "align": "center",
          "label": "逾期金额",
          "prop": "overdue_amount",
          "format": {
            "decimal": "2"
          }
        },
        {
          "type": "currency",
          "align": "center",
          "label": "逾期本金",
          "prop": "ovdue_principal",
          "format": {
            "decimal": "2"
          }
        },
        {
          "type": "currency",
          "align": "center",
          "label": "贷款余额",
          "prop": "current_balance",
          "format": {
            "decimal": "2"
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "是否结案",
          "prop": "is_close",
          "dict": {
            "dictType": "CS_E_YESORNO",
            "dictKey": [
              "CS_E_YESORNO"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "本期五级分类",
          "prop": "five_level",
          "dict": {
            "dictType": "CS_E_CLASSIFYRESULT",
            "dictKey": [
              "CS_E_CLASSIFYRESULT"
            ],
            "format": "value-label"
          }
        },
        {
          "label": "操作",
          "align": "center",
          "fixed": "right",
          "width": "130px",
          "btns": [
            {
              "type": "custom",
              "label": "案件详情",
              "toRequest": {
                "url": "/views/cs/assistManage/caseDetail/caseDetail_2/case_detail_main.json",
                "method": "POST",
                "params": {
                  "case_id": "${{current.case_id}}",
                  "case_no": "${{current.case_no}}",
                  "cust_id": "${{current.cust_id}}",
                  "org_id": "${{current.org_id}}",
                  "oa_org_id": "${{current.oa_org_id}}"
                }
              },
              "props": {
                "width": "80%",
                "title": "案件详情"
              }
            }
          ]
        }
      ]
    }
  }
}