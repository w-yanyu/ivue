{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "btns": []
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "cb0168",
          "work_serial_no": "${{params.work_serial_no}}"
        }
      },
      "thead": [
        {
          "type": "string",
          "align": "center",
          "label": "案件编号",
          "prop": "case_id"
        },
        {
          "type": "string",
          "align": "center",
          "label": "客户号",
          "prop": "cust_id"
        },
        {
          "type": "string",
          "align": "center",
          "label": "文件名称",
          "prop": "file_name"
        },
        {
          "type": "string",
          "align": "center",
          "label": "文件类型",
          "prop": "file_type",
          "dict": {
            "dictType": "CS_E_FILE_TYPE",
            "dictKey": [
              "CS_E_FILE_TYPE"
            ],
            "format": "value-label"
          }
        },
        {
          "label": "操作",
          "align": "center",
          "fixed": "right",
          "btns": [
            {
              "type": "custom",
              "label": "查看",
              "toRequest": {
                "url": "@lookInterviewImageBtn",
                "params": {
                  "file_url": "${{current.file_url}}"
                }
              },
              "props": {
                "width": "75%",
                "title": "查看"
              }
            }
          ]
        }
      ]
    }
  }
}
