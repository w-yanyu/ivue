{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "btns": []
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "cb0167",
          "work_serial_no": "${{params.work_serial_no}}"
        }
      },
      "thead": [
        {
          "type": "string",
          "align": "center",
          "label": "被访人姓名",
          "prop": "interview_name"
        },
        {
          "type": "string",
          "align": "center",
          "label": "外访日期",
          "prop": "interview_time"
        },
        {
          "type": "string",
          "align": "center",
          "label": "外访结果",
          "prop": "interview_result_details"
        },
        {
          "type": "string",
          "align": "center",
          "label": "外访对象",
          "prop": "relationship",
          "dict": {
            "dictType": "CS_E_RELATIONSHIP",
            "dictKey": [
              "CS_E_RELATIONSHIP"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "外访原因",
          "prop": "interview_apply_reason"
        },
        {
          "type": "string",
          "align": "center",
          "label": "外访时间",
          "prop": "interview_time"
        },
        {
          "type": "string",
          "align": "center",
          "label": "外访地址类型",
          "prop": "interview_address_type",
          "dict": {
            "dictType": "CS_E_ADDRESS_TYPE",
            "dictKey": [
              "CS_E_ADDRESS_TYPE"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "外访地址是否存在",
          "prop": "interview_address_exist",
          "dict": {
            "dictType": "CS_E_YESORNO",
            "dictKey": [
              "CS_E_YESORNO"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "催收结果",
          "prop": "coll_result",
          "dict":{
            "dictKey": [
              "coll_result"
            ],
            "format": "value-label",
            "labelField": "coll_result_name",
            "valueField": "coll_result_code",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode":"cb0087",
                "is_valid": "Y",
                "appId": "cbOnl"
              }
            }
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "外访地址",
          "prop": "address"
        },
        {
          "type": "string",
          "align": "center",
          "label": "是否有人应门",
          "prop": "answer_the_door",
          "dict": {
            "dictType": "CS_E_YESORNO",
            "dictKey": [
              "CS_E_YESORNO"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "催收函交付对象",
          "prop": "post_who",
          "dict": {
            "dictType": "CS_E_YESORNO",
            "dictKey": [
              "CS_E_YESORNO"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "排查对象",
          "prop": "look_into_who"
        },
        {
          "type": "string",
          "align": "center",
          "label": "备注",
          "prop": "remark"
        }
