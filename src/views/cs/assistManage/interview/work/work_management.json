{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "foldLineNumber": 3,
      "controls": [
        {
          "cust_name": {
            "control": "text",
            "label": "客户姓名"
          },
          "cert_no": {
            "control": "text",
            "label": "证件号码"
          }
        },
        {
          "cust_id": {
            "control": "text",
            "label": "客户号"
          },
          "pc_interview_status": {
            "control": "select",
            "label": "外访状态",
            "dict": {
              "dictType": "CS_E_PC_INTERVIEW_STATUS",
              "dictKey": [
                "CS_E_PC_INTERVIEW_STATUS"
              ],
              "format": "value-label"
            }
          }
        },
        {
          "pc_is_complete": {
            "control": "select",
            "label": "是否完成",
            "dict": {
              "dictType": "CS_E_YESORNO",
              "dictKey": [
                "CS_E_YESORNO"
              ],
              "format": "value-label"
            }
          },
          "daterange": {
            "control": "dateTimePicker",
            "label": "申请时间",
            "type": "daterange",
            "unlinkPanels": true,
            "valueFormat": "yyyy-MM-dd",
            "startPlaceholder": "请选择开始时间",
            "endPlaceholder": "请选择结束时间",
            "startField": "start_date",
            "endField": "end_date"
          }
        }
      ]
    },
    "toolbar": {},
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "cb0165"
        }
      },
      "thead": [
        {
          "type": "string",
          "label": "案件编号",
          "align": "center",
          "prop": "case_id"
        },
        {
          "type": "string",
          "align": "center",
          "label": "客户姓名",
          "prop": "cust_name"
        },
        {
          "type": "string",
          "align": "center",
          "label": "客户号",
          "prop": "cust_id"
        },
        {
          "type": "string",
          "align": "center",
          "label": "证件号码",
          "prop": "cert_no"
        },
        {
          "type": "string",
          "align": "center",
          "label": "借据号",
          "prop": "loan_no"
        },
        {
          "type": "string",
          "align": "center",
          "label": "合同号",
          "prop": "contract_no"
        },
        {
          "type": "string",
          "align": "center",
          "label": "催收员",
          "prop": "collector",
          "dict": {
            "dictType": "deciOnl",
            "dictKey": [
              ""
            ],
            "labelField": "user_name",
            "valueField": "user_id",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "length": 1000,
                  "start": 1,
                  "servicecode": "ct2502"
              }
            },
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "申请人",
          "prop": "apply_user",
          "dict": {
            "dictType": "deciOnl",
            "dictKey": [
              ""
            ],
            "labelField": "user_name",
            "valueField": "user_id",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "length": 1000,
                  "start": 1,
                  "servicecode": "ct2502"
              }
            },
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "审核人",
          "prop": "approval_user",
          "dict": {
            "dictType": "deciOnl",
            "dictKey": [
              ""
            ],
            "labelField": "user_name",
            "valueField": "user_id",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "length": 1000,
                  "start": 1,
                  "servicecode": "ct2502"
              }
            },
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "是否完成",
          "prop": "pc_is_complete",
          "dict": {
            "dictType": "CS_E_YESORNO",
            "dictKey": [
              "CS_E_YESORNO"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "外访状态",
          "prop": "pc_interview_status",
          "dict": {
            "dictType": "CS_E_PC_INTERVIEW_STATUS",
            "dictKey": [
              "CS_E_PC_INTERVIEW_STATUS"
            ],
            "format": "value-label"
          }
        },
        {
          "label": "操作",
          "align": "center",
          "width": "200px",
          "fixed": "right",
          "btns": [
            {
              "type": "custom",
              "label": "标记完成",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "cb0166",
                  "work_serial_no": "${{current.work_serial_no}}"
                }
              }
            },
            {
              "type": "custom",
              "label": "外访记录",
              "toRequest": {
                "url": "/views/cs/assistManage/interview/work/query_record.json",
                "method": "POST",
                "params": {
                  "work_serial_no": "${{current.work_serial_no}}"
                }
              },
              "props": {
                "title": "查看外访记录",
                "width": "80%"
              }
            },
            {
              "type": "custom",
              "label": "外访影像",
              "toRequest": {
                "url": "/views/cs/assistManage/interview/work/query_video.json",
                "method": "POST",
                "params": {
                  "work_serial_no": "${{current.work_serial_no}}"
                }
              },
              "props": {
                "title": "外访影像",
                "width": "80%"
              }
            },
            {
              "type": "custom",
              "label": "打印外访单",
              "toRequest": {
                "url": "/views/cs/assistManage/interview/work/work_print.json",
                "params": {
                  "work_serial_no": "${{current.work_serial_no}}"
                }
              },
              "props": {
                "title": "打印外访单",
                "width": "60%"
              }
            }
          ]
        }
      ]
    }
  }
}