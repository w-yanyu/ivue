{
  "module": "v_realonlyForm_btn",
  "layout": {
    "form": {
      "scope": "printHtml",
      "btns": [
        {
          "type": "print",
          "label": "打印外访单",
          "icon": "el-icon-printer",
          "printable": "printHtml",
          "css": "",
          "printType": "html",
          "documentTitle": "外访打印表",
          "header": "<h3 class='custom-h3'>外访打印表</h3>",
          "maxWidth": 1400,
          "headerStyle": "font-weight：600;",
          "style": ".custom-h3 { color: red; }",
          "scanStyles": true
        }
      ],
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "cb0169",
          "work_serial_no": "${{params.work_serial_no}}"
        }
      },
      "controls": [
        {
          "cust_name": {
            "control": "string",
            "label": "客户姓名："
          }
        },
        {
          "cert_no": {
            "control": "string",
            "label": "身份证号："
          }
        },
        {
          "overdue_amount": {
            "control": "string",
            "label": "逾期金额："
          }
        },
        {
          "contact_no": {
            "control": "string",
            "label": "联系方式："
          }
        },
        {
          "overdue_days": {
            "control": "string",
            "label": "逾期天数："
          }
        },
        {
          "address": {
            "address": "string",
            "label": "外访地址：",
            "inputWidth": "500px"
          }
        },
        {
          "fill_in_value1": {
            "control": "textarea",
            "label": "外访反馈：",
            "inputWidth": "500px",
            "rows": 8
          }
        },
        {
          "fill_in_value2": {
            "control": "text",
            "label": "取得新电话：",
            "inputWidth": "500px"
          }
        },
        {
          "fill_in_value3": {
            "control": "text",
            "label": "取得新地址：",
            "inputWidth": "500px"
          }
        },
        {
          "fill_in_value4": {
            "control": "textarea",
            "label": "后续电催建议：",
            "inputWidth": "500px",
            "rows": 8
          }
        },
        {
          "fill_in_value5": {
            "control": "text",
            "label": "主管：",
            "inputWidth": "300px"
          }
        },
        {
          "fill_in_value6": {
            "control": "text",
            "label": "催收员：",
            "inputWidth": "300px"
          }
        },
        {
          "print_date": {
            "control": "string",
            "label": "打印日期："
          }
        }
      ]
    }
  }
}
