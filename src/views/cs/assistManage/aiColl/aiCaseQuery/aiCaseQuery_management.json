{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "batch_no": {
            "control": "text",
            "label": "委托批次"
          },
          "cust_name": {
            "control": "text",
            "label": "客户姓名"
          },
          "cert_no": {
            "control": "text",
            "label": "证件号码"
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "export",
          "label": "导出",
          "doRequest": {
            "method": "POST",
            "url": "/SUMP/rest/file/commExport",
            "params": {
              "length": 999,
              "servicecode": "cb0176"
            }
          },
          "confirm":{
            "title":"提示信息",
            "msg":"确认导出吗？",
            "confirmButtonText":"确定",
            "cancelButtonText":"取消"
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "cb0176"
        }
      },
      "thead": [
        {
          "type": "string",
          "align": "center",
          "label": "委托批次",
          "prop": "batch_no"
        },
        {
          "type": "string",
          "align": "center",
          "label": "案件编号",
          "prop": "case_id"
        },
        {
          "type": "string",
          "align": "center",
          "label": "客户姓名",
          "prop": "cust_name"
        },
        {
          "type": "string",
          "align": "center",
          "label": "证件号码",
          "prop": "cert_no"
        },
        {
          "type": "currency",
          "align": "center",
          "label": "逾期金额",
          "prop": "overdue_amount",
          "format": {
            "decimal": "2"
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "逾期天数",
          "prop": "overdue_days"
        },
        {
          "type": "string",
          "align": "center",
          "label": "委托日期",
          "prop": "depute_start_date"
        },
        {
          "type": "string",
          "align": "center",
          "label": "委托天数",
          "prop": "depute_days"
        },
        {
          "label": "操作",
          "align": "center",
          "fixed": "right",
          "width": "150",
          "btns": [
            {
              "type": "look",
              "label": "详情",
              "toRequest": {
                "url": "/views/cs/assistManage/caseDetail/caseDetail_2/case_detail_main.json",
                "method": "POST",
                "params": {
                  "case_id": "${{current.case_id}}",
                  "case_no": "${{current.case_no}}",
                  "cust_id": "${{current.cust_id}}",
                  "org_id": "${{current.org_id}}",
                  "oa_org_id": "${{current.oa_org_id}}"
                },
                "props": {
                  "title": "催记信息",
                  "width": "80%"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
