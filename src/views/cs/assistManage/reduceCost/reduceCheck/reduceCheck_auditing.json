{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "btns": [
                {
                    "type": "submit",
                    "icon": "el-icon-check",
                    "label": "核准",
                    "doRequest": {
                        "method": "POST",
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "page_id": "/views/cs/assistManage/reduceCost/reduceCheck/reduceCheck_auditing.json",
                        "params": {
                            "servicecode":"cb0259",
                            "appId": "cbOnl",
                            "list01": "${{params.selectedData}}",
                            "approval_status": "P"
                        }
                    }
                },
                {
                    "type": "submit",
                    "icon": "el-icon-close",
                    "label": "拒绝",
                    "doRequest": {
                        "method": "POST",
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "page_id": "/views/cs/assistManage/reduceCost/reduceCheck/reduceCheck_auditing.json",
                        "params": {
                            "servicecode":"cb0259",
                            "appId": "cbOnl",
                            "list01": "${{params.selectedData}}",
                            "approval_status": "R"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "approval_desc": {
                        "control": "textarea",
                        "label": "审核说明",
                        "width": "180px",
                        "align": "center",
                        "span": "20",
                        "rows": 6,
                        "placeholder":"请输入",
                        "rules": [
                            {
                                "required": true,
                                "message":"请输入"
                            }
                        ]
                    }
                }
            ]
        }
    }
}
