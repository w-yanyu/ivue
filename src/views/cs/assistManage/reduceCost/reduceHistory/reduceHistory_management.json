{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "foldLineNumber": 3,
      "controls": [
        {
          "cust_name": {
            "control": "text",
            "label": "客户姓名"
          },
          "daterange": {
            "control": "dateTimePicker",
            "label": "申请时间",
            "type": "daterange",
            "unlinkPanels": true,
            "valueFormat": "yyyyMMdd",
            "startPlaceholder": "请选择开始时间",
            "endPlaceholder": "请选择结束时间",
            "startField": "start_date",
            "endField": "end_date"
          }
        },
        {
          "cert_no": {
            "control": "text",
            "label": "证件号码"
          },
          "reduce_mode": {
            "control": "select",
            "label": "减免方式",
            "labelField": "drop_list_desc",
            "valueField": "drop_list_value",
            "dict": {
              "dictType": "CS_REDUCE_MODE",
              "dictKey": [
                "CS_REDUCE_MODE"
              ]
            },
            "format": "value-label"
          }
        },
        {
          "reduce_type": {
            "control": "select",
            "label": "减免类型",
            "labelField": "drop_list_desc",
            "valueField": "drop_list_value",
            "dict": {
              "dictType": "CS_REDUCE_TYPE",
              "dictKey": [
                "CS_REDUCE_TYPE"
              ]
            },
            "format": "value-label"
          },
          "applt_reason": {
            "control": "select",
            "label": "调整申请原因",
            "dict": {
              "dictType": "CS_APPLT_REASON",
              "dictKey": [
                "CS_APPLT_REASON"
              ],
              "format": "value-label"
            }
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "cb0258",
          "appId": "cbOnl"
        }
      },
      "thead": [
        {
          "type": "string",
          "align": "center",
          "label": "案件编号",
          "prop": "case_id"
        },
        {
          "type": "string",
          "align": "center",
          "label": "客户姓名",
          "prop": "cust_name"
        },
        {
          "type": "string",
          "align": "center",
          "label": "证件号码",
          "prop": "cert_no"
        },
        {
          "type": "string",
          "align": "center",
          "label": "逾期天数",
          "prop": "overdue_days"
        },
        {
          "type": "currency",
          "align": "center",
          "label": "逾期金额",
          "prop": "overdue_amount",
          "format": {
            "decimal": "2"
          }
        },
        {
          "type": "currency",
          "align": "center",
          "label": "贷款金额",
          "prop": "loan_amt",
          "format": {
            "decimal": "2"
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "减免方式",
          "prop": "reduce_mode",
          "dict": {
            "format": "value-label",
            "dictType": "CS_REDUCE_MODE",
            "dictKey": [
                "CS_REDUCE_MODE"
            ]
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "减免类型",
          "prop": "reduce_type",
          "dict": {
            "format": "value-label",
            "dictType": "CS_REDUCE_TYPE",
            "dictKey": [
                "CS_REDUCE_TYPE"
            ]
          }
        },
        {
          "type": "string",
          "label": "调整申请原因",
          "align": "center",
          "prop": "applt_reason",
          "dict": {
            "format": "value-label",
            "dictType": "CS_APPLT_REASON",
            "dictKey": [
                "CS_APPLT_REASON"
            ]
          }
        },
        {
          "type": "string",
          "label": "客户申请减免时间",
          "align": "center",
          "prop": "cust_apply_time"
        },
        {
          "type": "string",
          "label": "减免本金",
          "align": "center",
          "prop": "reduce_principal"
        },
        {
          "type": "string",
          "label": "减免利息",
          "align": "center",
          "prop": "reduce_interest"
        },
        {
          "type": "string",
          "label": "减免罚息",
          "align": "center",
          "prop": "reduce_default_interest"
        },
        {
          "type": "string",
          "label": "减免费用",
          "align": "center",
          "prop": "reduce_cost"
        },
        {
          "type": "string",
          "label": "减免总金额",
          "align": "center",
          "prop": "reduce_total_amount"
        },
        {
          "type": "string",
          "label": "审核状态",
          "align": "center",
          "prop": "approval_status",
          "dict": {
            "format": "value-label",
            "dictType": "CS_E_CHECK_STATUS",
            "dictKey": [
                "CS_E_CHECK_STATUS"
            ]
          }
        },
        {
          "type": "string",
          "label": "申请人",
          "align": "center",
          "prop": "apply_user"
        },
        {
          "type": "string",
          "label": "申请时间",
          "align": "center",
          "prop": "apply_time"
        },
        {
          "type": "string",
          "label": "备注",
          "align": "center",
          "prop": "remark"
        },
        {
          "type": "string",
          "label": "审核人",
          "align": "center",
          "prop": "approval_user"
        },
        {
          "type": "string",
          "label": "审核时间",
          "align": "center",
          "prop": "approval_time"
        },
        {
          "type": "string",
          "label": "审核说明",
          "align": "center",
          "prop": "approval_desc"
        },
        {
          "label": "操作",
          "align": "center",
          "fixed": "right",
          "width": "130px",
          "btns": [
              {
                  "type": "custom",
                  "label": "案件详情",
                  "toRequest": {
                      "url": "/views/cs/assistManage/caseDetail/caseDetail_2/case_detail_main.json",
                      "method": "POST",
                      "params": {
                          "case_id": "${{current.case_id}}",
                          "case_no": "${{current.case_no}}",
                          "cust_id": "${{current.cust_id}}",
                          "org_id": "${{current.org_id}}",
                          "oa_org_id": "${{current.oa_org_id}}"
                        }
                  },
                  "props": {
                      "width": "80%",
                      "title": "案件详情"
                  }
              }
          ]
      }
      ]
    }
  }
}