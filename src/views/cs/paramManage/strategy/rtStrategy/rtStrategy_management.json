{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "scope": "formData",
      "controls": [
        {
          "strategy_code": {
            "control": "text",
            "label": "策略代码"
          },
          "strategy_name": {
            "control": "text",
            "label": "策略名称"
          }
        },
        {
          "start_time": {
            "label": "起始时间",
            "control": "dateTimePicker",
            "span": 12,
            "format": "yyyy-MM-dd HH:mm:ss"
          },
          "end_time": {
            "label": "结束时间",
            "control": "dateTimePicker",
            "span": 12,
            "format": "yyyy-MM-dd HH:mm:ss"
          }
        },
        {
          "is_valid": {
            "control": "select",
            "label": "是否启用",
            "selectFirstValue": false,
            "dict": {
              "dictType": "CS_E_YESORNO",
              "dictKey": [
                "CS_E_YESORNO"
              ],
              "format": "value-label"
            }
          },
          "org_id": {
            "hidden": true,
            "label": "创建法人",
            "control": "remoteSelect",
            "labelField": "org_name",
            "valueField": "org_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2311",
                "org_id": "$g{{user_info.org_id}}"
              }
            },
            "events": {
              "emit": [
                "operation"
              ]
            },
            "initShow": {
              "expression": "'${{form.org_id}}' === ''"
            },
            "format": "value-label"
          }
          },
        {
          "branch_id": {
            "hidden": true,
            "label": "创建机构",
            "control": "remoteSelect",
            "labelField": "branch_name",
            "valueField": "branch_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2142",
                "user_id": "$g{{user_info.userCode}}",
                "branch_id": "$g{{user_info.branch_id}}",
                "brch_relation_code": "01"
              }
            },
            "events": {
              "emit": [
                "operation"
              ]
            },
            "initShow": {
              "expression": "'${{form.branch_id}}' === ''"
            },
            "format": "value-label"
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "icon": "el-icon-circle-plus",
          "target": "dialog",
          "label": "新增",
          "toRequest": {
            "method": "GET",
            "url": "/views/cs/paramManage/strategy/rtStrategy/rtStrategy_add.json"
          },
          "props": {
            "width": "65%",
            "title": "新增策略"
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "py0004",
          "appId": "pyOnl",
          "strategy_type": "RT"
        }
      },
      "thead": [
        {
          "type": "string",
          "align": "center",
          "label": "策略代码",
          "prop": "strategy_code"
        },
        {
          "type": "string",
          "align": "center",
          "label": "策略名称",
          "prop": "strategy_name"
        },
        {
          "type": "string",
          "align": "center",
          "label": "策略描述",
          "prop": "strategy_desc"
        },
        {
          "align": "center",
          "width": "180px",
          "label": "起始时间",
          "format": "yyyy-MM-dd HH:mm:ss",
          "prop": "start_time"
        },
        {
          "type": "string",
          "align": "center",
          "width": "180px",
          "label": "结束时间",
          "format": "yyyy-MM-dd HH:mm:ss",
          "prop": "end_time"
        },
        {
          "type": "string",
          "align": "center",
          "label": "优先级",
          "prop": "priority"
        },
        {
          "type": "string",
          "align": "center",
          "label": "是否启用",
          "prop": "is_valid",
          "dict": {
            "dictType": "CS_E_YESORNO",
            "dictKey": [
              "CS_E_YESORNO"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "创建法人",
          "dict": {
            "dictKey": [
              "org_id"
            ],
            "labelField": "org_name",
            "valueField": "org_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2311",
                "org_id": "${{current.org_id}}"
              }
            },
            "format": "value-label"
          },
          "prop": "org_id"
        },
        {
          "type": "string",
          "align": "center",
          "label": "创建机构",
          "dict": {
            "dictKey": [
              "branch_id"
            ],
            "labelField": "branch_name",
            "valueField": "branch_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2142",
                "branch_id": "${{current.branch_id}}"
              }
            },
            "format": "value-label"
          },
          "prop": "branch_id"
        },
        {
          "label": "操作",
          "align": "center",
          "fixed": "right",
          "width": "250%",
          "btns": [
            {
              "type": "look",
              "label": "详情",
              "toRequest": {
                "url": "/views/cs/paramManage/strategy/rtStrategy/rtStrategy_info.json",
                "method": "POST",
                "params": {
                  "strategy_type": "$g{{strategy_type}}"
                }
              },
              "props": {
                "title": "策略详情"
              }
            },
            {
              "type": "edit",
              "label": "维护",
              "toRequest": {
                "url": "/views/cs/paramManage/strategy/rtStrategy/rtStrategy_modify.json",
                "method": "POST",
                "params": {
                  "strategy_type": "$g{{strategy_type}}"
                }
              },
              "props": {
                "title": "策略维护"
              }
            },
            {
              "type": "edit",
              "label": "配置规则",
              "toRequest": {
                "url": "/views/cs/paramManage/strategy/strategy_rule_config.json",
                "method": "POST"
              },
              "props": {
                "width": "80%",
                "title": "配置规则"
              }
            },
            {
              "type": "delete",
              "label": "删除",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "py0002",
                  "appId": "pyOnl"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
