{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "scope": "formData",
      "initRequest": "${{params}}",
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "py0003",
          "appId": "pyOnl"
        }
      },
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "el-icon-tickets"
        }
      ],
      "controls": [
        {
          "strategy_type": {
            "control": "select",
            "disabled": true,
            "label": "策略类型",
            "width": "130px",
            "selectFirstValue": false,
            "dict": {
              "dictType": "CS_E_STRATEGY_TYPE",
              "dictKey": [
                "CS_E_STRATEGY_TYPE"
              ],
              "format": "value-label"
            }
          },
          "strategy_code": {
            "control": "string",
            "disabled": true,
            "label": "策略代码",
            "width": "130px"
          },
          "queue_code": {
            "control": "string",
            "label": "队列代码",
            "width": "130px",
            "initShow": {
              "expression": "'${{params.strategy_type}}' === 'QT'"
            }
          },
          "queue_name": {
            "control": "string",
            "label": "队列名称",
            "width": "130px",
            "initShow": {
              "expression": "'${{params.strategy_type}}' === 'QT'"
            }
          }
        },
        {
          "strategy_name": {
            "control": "string",
            "disabled": true,
            "label": "策略名称",
            "width": "130px"
          },
          "strategy_desc": {
            "control": "text",
            "label": "策略描述",
            "width": "130px",
            "rules": [
              {
                "max": 100,
                "message": "最多可以输入 100 个字节"
              }
            ]
          }
        },
        {
          "start_time": {
            "label": "起始时间",
            "control": "dateTimePicker",
            "type": "dateTimePicker",
            "span": 12,
            "width": "130px",
            "format": "yyyy-MM-dd HH:mm:ss",
            "disabledDate": {
              "endRange": "today"
            },
            "rules": [
              {
                "required": true,
                "message": "起始时间不能为空"
              }
            ]
          },
          "end_time": {
            "label": "结束时间",
            "control": "dateTimePicker",
            "type": "dateTimePicker",
            "span": 12,
            "width": "130px",
            "format": "yyyy-MM-dd HH:mm:ss",
            "disabledDate": {
              "endRange": "today"
            },
            "rules": [
              {
                "required": true,
                "message": "结束时间不能为空"
              }
            ]
          }
        },
        {
          "priority": {
            "control": "inputNumber",
            "label": "优先级",
            "width": "130px",
            "min": 1,
            "rules": [
              {
                "required": true,
                "pattern": "^[0-9]",
                "message": "请输入 0 到 11 之间的数字"
              },
              {
                "expression": "'${{form.priority}}'.length < 3",
                "message": "最多可以输入 3 个字节"
              }
            ]
          },
          "is_valid": {
            "control": "select",
            "label": "是否启用",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ],
            "dict": {
              "dictType": "CS_E_YESORNO",
              "dictKey": [
                "CS_E_YESORNO"
              ],
              "format": "value-label"
            }
          }
        }
      ]
    }
  }
}