{
  "module": "v-realonlyForm-btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "controls": [
        {
          "coll_result_code": {
            "hidden": true,
            "label": "最新沟通结果（语义理解标签）",
            "disabled": true,
            "width": "130px",
            "control": "remoteSelect",
            "labelField": "coll_result_name",
            "valueField": "coll_result_code",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0087",
                "appId": "cbOnl"
              }
            },
            "events": {
              "emit": [
                "operation"
              ]
            },
            "format": "value-label"
          },
          "mobile_status": {
            "control": "select",
            "label": "最新沟通电话状态",
            "disabled": true,
            "width": "130px",
            "selectFirstValue": false,
            "dict": {
              "dictType": "CS_E_PHONE_STATUS",
              "dictKey": [
                "CS_E_PHONE_STATUS"
              ],
              "format": "value-label"
            }
          }
        },
        {
          "reply_person": {
            "control": "select",
            "label": "回复对象",
            "disabled": true,
            "width": "130px",
            "selectFirstValue": false,
            "dict": {
              "dictType": "CS_E_RELATIONSHIP",
              "dictKey": [
                "CS_E_RELATIONSHIP"
              ],
              "format": "value-label"
            }
          },
          "cust_label": {
            "control": "string",
            "label": "客户标签",
            "width": "130px"
          }
        },
        {
          "work_type": {
            "control": "select",
            "label": "作业类型",
            "disabled": true,
            "width": "130px",
            "selectFirstValue": false,
            "dict": {
              "dictType": "CS_WORK_TYPE",
              "dictKey": [
                "CS_WORK_TYPE"
              ],
              "format": "value-label"
            }
          },
          "work_template_id": {
            "control": "string",
            "label": "作业模板编号",
            "width": "130px"
          }
        },
        {
          "send_person": {
            "control": "select",
            "label": "发送对象",
            "disabled": true,
            "width": "130px",
            "selectFirstValue": false,
            "dict": {
              "dictType": "CS_E_RELATIONSHIP",
              "dictKey": [
                "CS_E_RELATIONSHIP"
              ],
              "format": "value-label"
            }
          },
          "work_interval_day": {
            "control": "string",
            "label": "作业间隔天数",
            "width": "130px"
          }
        },
        {
          "work_interval_hour": {
            "control": "string",
            "label": "作业间隔小时",
            "width": "130px"
          },
          "channel": {
            "control": "select",
            "label": "呼叫中心通道",
            "disabled": true,
            "width": "130px",
            "selectFirstValue": false,
            "dict": {
              "dictType": "CS_CHANNEL",
              "dictKey": [
                "CS_CHANNEL"
              ],
              "format": "value-label"
            }
          }
        }
      ]
    }
  }
}