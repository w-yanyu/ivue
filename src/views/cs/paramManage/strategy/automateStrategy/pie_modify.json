{
  "module": "v-form-editableDataGrid-btn",
  "layout": {
    "form": {
      "scope": "formData",
      "initRequest": "${{params}}",
      "controls": [
        {
          "strategy_group_code": {
            "control": "string",
            "disabled": true,
            "label": "策略组代码",
            "width": "130px"
          },
          "strategy_group_name": {
            "control": "string",
            "disabled": true,
            "label": "策略组名称",
            "width": "130px"
          },
          "group_assign_ratio": {
            "control": "inputNumber",
            "label": "组分配比重",
            "width": "130px",
            "min": 1,
            "max": 100,
            "rules": [
              {
                "required": true,
                "pattern": "^[0-9]",
                "message": "请输入 1 到 100 之间的数字"
              },
              {
                "expression": "'${{form.group_assign_ratio}}'.length < 4",
                "message": "最多可以输入 4 个字节"
              }
            ]
          },
          "assign_algorithm": {
            "control": "select",
            "label": "分配算法",
            "width": "130px",
            "format": "value-label",
            "selectFirstValue": false,
            "dict": {
              "dictType": "CS_ASSIGN_ALG",
              "dictKey": [
                "CS_ASSIGN_ALG"
              ],
              "format": "value-label"
            }
          },
          "assign_ratio_mode": {
            "control": "select",
            "label": "分配比重模式",
            "width": "130px",
            "format": "value-label",
            "selectFirstValue": false,
            "dict": {
              "dictType": "CS_ASSIGN_RATIO_MODE",
              "dictKey": [
                "CS_ASSIGN_RATIO_MODE"
              ],
              "format": "value-label"
            }
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "icon": "el-icon-circle-plus",
          "target": "dialog",
          "label": "新增策略明细",
          "toRequest": {
            "method": "GET",
            "url": "/views/cs/paramManage/strategy/automateStrategy/pie_add.json",
            "params": {
              "strategy_group_code": "${{params.strategy_group_code}}"
            }
          },
          "props": {
            "width": "65%",
            "title": "新增策略明细"
          }
        }
      ]
    },
    "datagrid": {
      "name": "scheme",
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "py0016",
          "appId": "pyOnl"
        }
      },
      "thead": [
        {
          "type": "selection",
          "align": "center",
          "width": "55",
          "fixed": "left",
          "show": {
            "prop": "changkuan",
            "is": [
              "0"
            ]
          }
        },
        {
          "prop": "coll_team",
          "type": "string",
          "align": "center",
          "label": "催收组"
        },
        {
          "prop": "collector",
          "type": "string",
          "align": "center",
          "label": "催收员"
        },
        {
          "prop": "detail_assign_ratio",
          "type": "string",
          "align": "center",
          "label": "明细分配比重"
        },
        {
          "label": "操作",
          "align": "center",
          "fixed": "right",
          "width": "180px",
          "btns": [
            {
              "type": "look",
              "label": "详情",
              "toRequest": {
                "url": "/views/cs/paramManage/strategy/automateStrategy/pie_info.json",
                "method": "POST"
              },
              "props": {
                "title": "详情"
              }
            },
            {
              "type": "edit",
              "label": "维护",
              "toRequest": {
                "url": "/views/cs/paramManage/strategy/automateStrategy/pie_detail_modify.json",
                "method": "POST"
              },
              "props": {
                "width": "70%",
                "title": "维护"
              }
            },
            {
              "type": "custom",
              "label": "删除",
              "align": "center",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "py0014",
                  "appId": "pyOnl"
                }
              }
            }
          ]
        }
      ]
    },
    "footer": {
      "btns": [
        {
          "type": "submit",
          "label": "保存",
          "icon": "el-icon-tickets",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "py0007",
              "appId": "pyOnl"
            }
          }
        }
      ]
    }
  }
}