{
  "module": "v-realonlyForm-btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "controls": [
        {
          "strategy_group_code": {
            "control": "string",
            "label": "策略组代码",
            "width": "130px"
          },
          "strategy_group_name": {
            "control": "string",
            "label": "策略组名称",
            "width": "130px"
          }
        },
        {
          "group_assign_ratio": {
            "control": "string",
            "label": "组分配比重",
            "width": "130px"
          },
          "assign_algorithm": {
            "control": "string",
            "label": "分配算法",
            "width": "130px",
            "dict": {
              "dictType": "CS_ASSIGN_ALG",
              "dictKey": [
                "CS_ASSIGN_ALG"
              ],
              "format": "value-label"
            }
          }
        },
        {
          "assign_ratio_mode": {
            "control": "string",
            "label": "分配比重模式",
            "width": "130px",
            "dict": {
              "dictType": "CS_ASSIGN_RATIO_MODE",
              "dictKey": [
                "CS_ASSIGN_RATIO_MODE"
              ],
              "format": "value-label"
            }
          }
        }
      ]
    }
  }
}