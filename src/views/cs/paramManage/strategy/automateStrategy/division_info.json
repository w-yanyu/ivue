{
  "module": "v-realonlyForm-btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "controls": [
        {
          "oa_org_id": {
            "hidden": true,
            "label": "委外公司代码",
            "disabled": true,
            "width": "130px",
            "control": "remoteSelect",
            "labelField": "outsourcing_org_name",
            "valueField": "outsourcing_org_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0052",
                "org_id": "$g{{user_info.org_id}}"
              }
            },
            "events": {
              "emit": [
                "operation"
              ]
            },
            "format": "value-label"
          },
          "commission_plan_id": {
            "control": "string",
            "label": "佣金方案编号",
            "width": "130px"
          }
        },
        {
          "oa_cycle": {
            "control": "string",
            "label": "委外周期",
            "width": "130px"
          },
          "oa_cycle_unit": {
            "control": "string",
            "label": "委外周期单位",
            "width": "130px",
            "dict": {
              "dictType": "CS_OA_CYCLE_UNIT",
              "dictKey": [
                "CS_OA_CYCLE_UNIT"
              ],
              "format": "value-label"
            }
          }
        },
        {
          "detail_assign_ratio": {
            "control": "string",
            "label": "明细分配比重",
            "width": "130px"
          }
        }
      ]
    }
  }
}