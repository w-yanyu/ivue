{
    "module": "v-realonlyForm-btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "controls": [
                {
                    "status_code": {
                        "control": "string",
                        "disabled":true,
                        "label": "案件状态代码",
                        "width": "130px"

                    },
                    "status_name": {
                        "control": "string",
                        "label": "案件状态名称",
                        "width": "130px"

                    }
                },
                {
                    "status_desc": {
                        "control": "string",
                        "label": "案件状态描述",
                        "width": "130px"
                    },
                    "status_type": {
                        "control": "string",
                        "label": "案件状态类型",
                        "selectFirstValue": false,
                        "width": "130px",
                        "dict": {
                            "dictType": "CS_STATUS_TYPE",
                            "dictKey": [
                                 "CS_STATUS_TYPE"
                             ],
                        "format": "value-label"
                      }
                    }
                },
                {
                    "is_default": {
                        "control": "string",
                        "label": "是否默认",
                        "width": "130px",
                        "dict": {
                            "dictType": "CS_E_YESORNO",
                            "dictKey": [
                                "CS_E_YESORNO"
                            ],
                            "format": "value-label"
                        }
                    }
                }
            ]
        }
    }
}
