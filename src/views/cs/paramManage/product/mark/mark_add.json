{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "cb0022",
                            "page_id": "/views/pl/paramManage/product/mark/mark_add.json",
                            "appId": "cbOnl"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "mark_code": {
                        "control": "text",
                        "label": "案件标签代码",
                        "width": "130",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            },
                            {
                                "min": 1,
                                "max": 10,
                                "message": "请输入长度在 1 到 10 之间的字符串"
                            }
                        ]
                    },
                    "mark_name": {
                        "control": "text",
                        "label": "案件标签名称",
                        "width": "130",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            },
                            {
                                "max": 30,
                                "message": "最多可以输入 30 个字节"
                            }
                        ]
                    }
                },
                {
                    "mark_desc": {
                        "control": "text",
                        "label": "案件标签描述",
                        "width": "130",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            },
                            {
                                "max": 100,
                                "message": "最多可以输入 100 个字节"
                            }
                        ]
                    }
                }
            ]
        }
    }
}
