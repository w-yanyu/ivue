{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "scope":"formData",
            "initRequest": "${{params}}",
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "cb0003",
                    "appId": "cbOnl"
                }
            },
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": "el-icon-tickets"
                }
            ],
            "controls": [
                {
                    "classify_code": {
                        "control": "text",
                        "disabled":true,
                        "label": "案件分类代码",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            },
                            {
                                "min": 1,
                                "max": 10,
                                "message": "请输入长度在 1 到 10 之间的字符串"
                            }
                        ]
                    },
                    "classify_name": {
                        "control": "text",
                        "label": "案件分类名称",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            },
                            {
                                "min": 1,
                                "max": 30,
                                "message": "最多可以输入 30 个字节"
                            }
                        ]
                    }
                },
                {
                    "classify_desc": {
                        "control": "text",
                        "label": "案件分类描述",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            }
                        ]
                    },
                    "is_default": {
                        "control": "select",
                        "label": "是否默认",
                        "width": "130px",
                        "dict": {
                            "dictType": "CS_E_YESORNO",
                            "dictKey": [
                                "CS_E_YESORNO"
                            ],
                            "format": "value-label"
                        },
                        "rules": [
                          {
                            "required":true,
                            "message": "请输入"
                          }
                        ]
                    }
                }
            ]
        }
    }
}
