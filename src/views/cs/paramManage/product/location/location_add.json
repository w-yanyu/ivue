{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "cb0001",
                            "appId": "cbOnl",
                            "page_id": "/views/cs/paramManage/product/location/location_add.json"
                        }
                    }
                }
            ],

            "controls": [
                {
                    "classify_code": {
                        "control": "text",
                        "label": "案件分类代码",
                        "width": "130",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            },
                            {
                                "min": 1,
                                "max": 10,
                                "message": "请输入长度在 1 到 10 之间的字符串"
                            }
                        ]
                    },
                    "classify_name": {
                        "control": "text",
                        "label": "案件分类名称",
                        "width": "130",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            },
                            {
                                "min": 1,
                                "max": 30,
                                "message": "最多可以输入 30 个字节"
                            }
                        ]
                    }
                },
                {
                    "classify_desc": {
                        "control": "text",
                        "label": "案件分类描述",
                        "width": "130",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            }
                        ]
                    },
                    "is_default": {
                        "control": "select",
                        "label": "是否默认",
                        "default":"N",
                        "width": "130",
                        "dict": {
                            "dictType": "CS_E_YESORNO",
                            "dictKey": [
                                "CS_E_YESORNO"
                            ],
                            "format": "value-label"
                        },
                        "rules": [
                          {
                            "required":true,
                            "message": "请输入"
                          }
                        ]
                    }
                }
            ]
        }
    }
}
