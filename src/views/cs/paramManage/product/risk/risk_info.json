{
    "module": "v-realonlyForm-btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "controls": [
                {
                    "risk_code": {
                        "control": "string",
                        "disabled":true,
                        "label": "风险等级代码",
                        "width": "130px"
                    },
                    "risk_name": {
                        "control": "string",
                        "label": "风险等级名称",
                        "width": "130px"
                    }
                },
                {
                    "risk_desc": {
                        "control": "string",
                        "label": "风险等级描述",
                        "width": "130px"
                    },
                    "is_default": {
                        "control": "string",
                        "label": "是否默认",
                        "width": "130px",
                        "dict": {
                            "dictType": "CS_E_YESORNO",
                            "dictKey": [
                                "CS_E_YESORNO"
                            ],
                            "format": "value-label"
                        }
                    }
                }
            ]
        }
    }
}
