{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "cb0012",
                            "appId": "cbOnl",
                            "page_id": "/views/cs/paramManage/product/risk/risk_add.json"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "risk_code": {
                        "control": "text",
                        "label": "风险等级代码",
                        "width": "130",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            },
                            {
                                "max": 10,
                                "message": "最多可以输入 10 个字节"
                            }
                        ]
                    },
                    "risk_name": {
                        "control": "text",
                        "label": "风险等级名称",
                        "width": "130",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            },
                            {
                                "max": 30,
                                "message": "最多可以输入 30 个字节"
                            }
                        ]
                    }
                },
                {
                    "risk_desc": {
                        "control": "text",
                        "label": "风险等级描述",
                        "width": "130"
                    },
                    "is_default": {
                        "control": "select",
                        "selectFirstValue":"true",
                        "label": "是否默认",
                        "default":"N",
                        "width": "130",
                        "dict": {
                            "dictType": "CS_E_YESORNO",
                            "dictKey": [
                                "CS_E_YESORNO"
                            ],
                            "format": "value-label"
                        },
                        "rules": [
                          {
                            "required":true,
                            "message": "请输入"
                          }
                        ]
                    }
                }
            ]
        }
    }
}
