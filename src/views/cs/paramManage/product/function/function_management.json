{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "scope": "formData",
      "controls": [
        {
          "stage_code": {
            "control": "text",
            "label": "逾期阶段代码"
          },
          "stage_name": {
            "control": "text",
            "label": "逾期阶段名称"
          }
        },
        {
          "stage_type": {
            "control": "select",
            "label": "逾期阶段类型",
            "selectFirstValue":false,
            "dict": {
              "dictType": "CS_FUNCTION_TYPE",
              "dictKey": [
                  "CS_FUNCTION_TYPE"
              ],
              "format": "value-label"
            }
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "icon": "el-icon-circle-plus",
          "target": "dialog",
          "label": "新增",
          "toRequest": {
            "method": "GET",
            "url": "/views/cs/paramManage/product/function/function_add.json"
          },
          "props": {
            "width": "65%",
            "title": "新增逾期阶段定义"
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "cb0010",
          "appId": "cbOnl"
        }
      },
      "thead": [
        {
          "type": "string",
          "align": "center",
          "label": "逾期阶段代码",
          "prop": "stage_code"
        },
        {
          "type": "string",
          "align": "center",
          "label": "逾期阶段名称",
          "prop": "stage_name"
        },
        {
          "align": "center",
          "label": "逾期阶段类型",
          "prop": "stage_type",
          "dict": {
            "dictType": "CS_FUNCTION_TYPE",
            "dictKey": [
                "CS_FUNCTION_TYPE"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "逾期阶段描述",
          "prop": "stage_desc"
        },
        {
          "type": "string",
          "align": "center",
          "label": "承诺付款比例(%)",
          "prop": "promise_payment_ratio"
        },
        {
          "type": "string",
          "align": "center",
          "label": "最大承诺天数",
          "prop": "max_promise_days"
        },
        {
          "type": "string",
          "align": "center",
          "label": "最大跟催天数",
          "prop": "max_coll_days"
        },
        {
          "type": "string",
          "label": "是否默认",
          "align": "center",
          "prop": "is_default",
          "dict": {
            "dictType": "CS_E_YESORNO",
            "dictKey": [
              "CS_E_YESORNO"
            ],
            "format": "value-label"
          }
        },
        {
          "label": "操作",
          "align": "center",
          "fixed": "right",
          "width": "150%",
          "btns": [
            {
              "type": "look",
              "label": "详情",
              "toRequest": {
                "url": "/views/cs/paramManage/product/function/function_info.json",
                "method": "POST"
              },
              "props": {
                "title": "逾期阶段定义详情"
              }
            },
            {
              "type": "edit",
              "label": "维护",
              "toRequest": {
                "url": "/views/cs/paramManage/product/function/function_modify.json",
                "method": "POST"
              },
              "props": {
                "title": "逾期阶段定义维护"
              }
            },
            {
              "type": "delete",
              "label": "删除",
              "doRequest": {
                "###url": "/SUMP/iccsCall/iccsEurekaCall",
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "cb0007",
                  "appId": "cbOnl",
                  "coll_kind": "02"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
