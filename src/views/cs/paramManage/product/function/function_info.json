{
    "module": "v-realonlyForm-btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "controls": [
                {
                    "stage_code": {
                        "control": "string",
                        "label": "逾期阶段代码",
                        "width": "130px"

                    },
                    "stage_name": {
                        "control": "string",
                        "label": "逾期阶段名称",
                        "width": "130px"
                    }
                },
                {
                    "stage_type": {
                        "control": "string",
                        "label": "逾期阶段类型",
                        "width": "130px",
                        "dict": {
                            "dictType": "CS_FUNCTION_TYPE",
                            "dictKey": [
                                "CS_FUNCTION_TYPE"
                            ],
                            "format": "value-label"
                          }
                    },
                    "stage_desc": {
                        "control": "string",
                        "label": "逾期阶段描述",
                        "width": "130px"
                    }
                },
                {
                    "promise_payment_ratio": {
                        "control": "string",
                        "label": "承诺付款比例(%)",
                        "width": "130px"
                    },
                    "max_promise_days": {
                        "control": "string",
                        "label": "最大承诺天数",
                        "width": "130px"
                    }
                },
                {
                    "max_coll_days": {
                        "control": "string",
                        "label": "最大跟催天数",
                        "width": "130px"
                    },
                    "is_default": {
                        "control": "string",
                        "label": "是否默认",
                        "width": "130px",
                        "dict": {
                            "dictType": "CS_E_YESORNO",
                            "dictKey": [
                                "CS_E_YESORNO"
                            ],
                            "format": "value-label"
                        }
                    }
                }
            ]
        }
    }
}
