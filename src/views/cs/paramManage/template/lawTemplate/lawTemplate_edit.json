{
  "module": "v_inputDataForm_btn",
  "layout": {
      "form": {
          "btns": [
              {
                  "type": "submit",
                  "label": "提交",
                  "icon": "",
                  "doRequest": {
                      "url": "/SUMP/icmcall/icmRPCCall",
                      "method": "POST",
                      "params": {
                          "servicecode": "cb0072",
                          "page_id": "/views/cs/paramManage/template/collTemplate/collTemplate_edit.json",
                          "appId": "cbOnl",
                          "coll_kind": "02",
                          "litigation_parent_stage_code":"${{params.litigation_parent_stage_code}}",
                          "litigation_stage_code":"${{params.litigation_stage_code}}",
                          "stage_type":"${{params.stage_type}}"
                      }
                  }
              }
          ],
          "controls": [
              {
                  "stage_name": {
                      "control": "text",
                      "label": "法诉阶段名称",
                      "width": "130px",
                      "disabled":true,
                      "value":"${{params.stage_name}}",
                      "rules": [
                          {
                              "required": true,
                              "message":"请输入"
                          }
                      ]
                  }
              },
              {
                  "data_sort": {
                      "control": "text",
                      "label": "序号",
                      "width": "130px",
                      "disabled":true,
                      "value":"${{params.data_sort}}",
                      "rules": [
                          {
                              "required": true,
                              "message":"请输入"
                          }
                      ]
                  }
              },
              {
                  "reminder_time": {
                      "control": "dateTimePicker",
                      "label": "提醒时间",
                      "disabled": true,
                      "type":"datetime",
                      "valueFormat": "yyyy-MM-dd HH:mm:ss",
                      "value":"${{params.reminder_time}}",
                      "width": "130px",
                      "rules": [
                          {
                              "required": true,
                              "message":"请输入"
                          }
                      ]
                  }
              },
              {
                  "reminder_method": {
                    "control": "select",
                    "label": "提醒方式",
                    "disabled": true,
                    "width": "130px",
                    "value":"${{params.reminder_method}}",
                    "options":[
                      {
                        "label": "系统",
                        "value": "S"
                      },
                      {
                        "label": "邮箱",
                        "value": "  M"
                      }
                    ],
                    "rules": [
                        {
                            "required": true,
                            "message":"请输入"
                        }
                    ]
                }
              },
              {
                  "reminder": {
                      "control": "text",
                      "label": "被提醒人",
                      "disabled": true,
                      "width": "130px",
                      "value":"${{params.reminder}}",
                      "rules": [
                          {
                              "required": true,
                              "message":"请输入"
                          }
                      ]
                  }
              },
              {
                  "reminder_content": {
                      "control": "text",
                      "label": "提醒内容",
                      "disabled": true,
                      "width": "130px",
                      "value":"${{params.reminder_content}}",
                      "rules": [
                          {
                              "required": true,
                              "message":"请输入"
                          }
                      ]
                  }
              }
          ]
      }
  }
}
