{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": "",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "cb0076",
                            "page_id": "/views/cs/paramManage/template/lawTemplate/lawTemplate_add_item_number.json",
                            "appId": "cbOnl",
                            "coll_kind": "02",
                            "litigation_stage_code":"${{params.litigation_stage_code}}",
                            "field_code":"${{params.field_code}}",
                            "field_type":"N"
                        }
                    }
                }
            ],
  
            "controlsGroup": [
              {
                  "title": "详细设置",
                  "tooltip": "",
                  "titleIcon": "",
                  "showHint": "",
                  "accordion": false,
                  "expand": true,
  
                  "controls": [
                      {
                          "field_name": {
                              "control": "text",
                              "label": "字段名称",
                              "span": 16,
                              "width": "130px",
                              "value":"${{params.field_name}}",
                              "rules": [
                                  {
                                      "required": true,
                                      "message":"请输入"
                                  }
                              ]
                          },
                          "default_value": {
                              "control": "currency",
                              "label": "默认值",
                              "span": 16,
                              "width": "130px",
                              "value":"${{params.default_value}}",
                              "decimal":2,
                              "max":11,
                              "rules": [
                                  {
                                  "required": true,
                                  "message": "请选择模板类型"
                                  }
                              ]
                          },
                          "field_group": {
                            "control": "select",
                            "label": "分组",
                            "span": 16,
                            "value":"${{params.field_group}}",
                            "width": "130px",
                            "options":[
                                {
                                  "label": "法诉信息",
                                  "value": "M"
                                },
                                {
                                  "label": "法诉费用",
                                  "value": "F"
                                },
                                {
                                  "label": "提醒信息",
                                  "value": "R"
                                },
                                {
                                  "label": "其他",
                                  "value": "O"
                                }
                              ],
                            "rules": [
                                {
                                "required": true,
                                "message": "请选择分组"
                                }
                            ]
                        },
                        "is_required": {
                            "control": "radio",
                            "label": "是否必填",
                            "span": 16,
                            "width": "130px",
                            "value":"${{params.is_required}}",
                            "options":[
                              {
                                  "label":"是",
                                  "value":"Y"
                              },
                              {
                                  "label":"否",
                                  "value":"N"
                              }],
                            "rules": [
                                {
                                "required": true,
                                "message": "请选择是否必填"
                                }
                            ]
                        }
                          }
                  ]
              }, {
                  "title": "效果展示",
                  "tooltip": "",
                  "titleIcon": "",
                  "showHint": "",
                  "accordion": false,
                  "expand": true,
                  "doControls": 
                  {        
                      "url":"/SUMP/cscall/icmRPCCall03",
                      "method": "POST",
                      "params": {
                        "servicecode": "cs0180",
                        "appId": "iccsOnl",
                        "coll_kind": "02",
                        "param_type":"select_type"
                      }
                  }
                  }
            ]
      }
    }
  }
  