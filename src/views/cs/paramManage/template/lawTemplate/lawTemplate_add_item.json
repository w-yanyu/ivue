{
    "module": "lr_tree_dynamic",
    "layout": {
        "refJs": "/refJs/treeMenu.js",
      "tree": {
        "scope": "treeTemplateParams",
        "title": "法诉模板定义",
        "lazy": false,
        "width": "300px",
        "hideDynamicArea": false,
        "draggable": true,
        "showToolbar": false,
        "stopAddNodeField": "stopAddNode",
        "stopDeleteNodeField": "isStopDeleteNode",
        "draggableField": "isStopDraggable",
        "isSupportHandleNode": false,
        "idField": "deptId",
        "nodeTypeField": "type",
        "pIdField": "parentPageCode",
        "labelField": "title",
        "isLeafField": "isLeaf",
        "stopClickNodeField": "stopNodeClick",
        "disabledField": "disabled",
        "initRequest": {
            "url": "/views/cs/paramManage/template/lawTemplate/lawTemplate_add_item_menu.json",
            "method": "POST"
          },
          "nodeTypes": {
            "template_date": {
              "icon": "el-icon-news",
              "children": [
                "type"
              ],
              "noClose": true,
              "eventAfter": {
                      "doParentCallback": true
              },
              "toRequest": {
              "url": "/views/cs/paramManage/template/lawTemplate/lawTemplate_add_item_date.json",
              "method": "get",
              "params": {
                "props": "${{current}}",
                "litigation_stage_code":"${{params.litigation_stage_code}}"
              }
            }
            },
            "template_text": {
              "icon": "el-icon-news",
              "children": [
                "type"
              ],
              "noClose": true,
              "eventAfter": {
                      "doParentCallback": true
              },
              "toRequest": {
              "url": "/views/cs/paramManage/template/lawTemplate/lawTemplate_add_item_text.json",
              "method": "get",
              "params": {
                "props": "${{current}}",
                "litigation_stage_code":"${{params.litigation_stage_code}}"
              }
            }
            },
            "template_select": {
              "icon": "el-icon-news",
              "children": [
                "type"
              ],
              "noClose": true,
              "eventAfter": {
                      "doParentCallback": true
              },
              "toRequest": {
              "url": "/views/cs/paramManage/template/lawTemplate/lawTemplate_add_item_select.json",
              "method": "get",
              "params": {
                "props": "${{current}}",
                "litigation_stage_code":"${{params.litigation_stage_code}}"
              }
            }
            },
            "template_number": {
              "icon": "el-icon-news",
              "children": [
                "type"
              ],
              "noClose": true,
              "eventAfter": {
                      "doParentCallback": true
              },
              "toRequest": {
              "url": "/views/cs/paramManage/template/lawTemplate/lawTemplate_add_item_number.json",
              "method": "get",
              "params": {
                "props": "${{current}}",
                "litigation_stage_code":"${{params.litigation_stage_code}}"
              }
            }
            },
            "template_file": {
              "icon": "el-icon-news",
              "children": [
                "type"
              ],
              "noClose": true,
              "eventAfter": {
                      "doParentCallback": true
              },
              "toRequest": {
              "url": "/views/cs/paramManage/template/lawTemplate/lawTemplate_add_item_file.json",
              "method": "get",
              "params": {
                "props": "${{current}}",
                "litigation_stage_code":"${{params.litigation_stage_code}}"
              }
            }
            },
            "template_contact": {
              "icon": "el-icon-news",
              "children": [
                "type"
              ],
              "noClose": true,
              "eventAfter": {
                      "doParentCallback": true
              },
              "toRequest": {
              "url": "/views/cs/paramManage/template/lawTemplate/lawTemplate_add_item_contact.json",
              "method": "get",
              "params": {
                "props": "${{current}}",
                "litigation_stage_code":"${{params.litigation_stage_code}}"
              }
            }
            },
            "template_radio": {
              "icon": "el-icon-news",
              "children": [
                "type"
              ],
              "noClose": true,
              "eventAfter": {
                      "doParentCallback": true
              },
              "toRequest": {
              "url": "/views/cs/paramManage/template/lawTemplate/lawTemplate_add_item_radio.json",
              "method": "get",
              "params": {
                "props": "${{current}}",
                "litigation_stage_code":"${{params.litigation_stage_code}}"
              }
            }
            }
            }
        }
    }
  }
  