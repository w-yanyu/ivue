{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "scope": "formData",
            "controls": [
                {
                    "template_name": {
                        "control": "text",
                        "label": "模板名称"
                    },
                    "work_order_type": {
                        "control": "select",
                        "label": "模板类型",
                        "selectFirstValue": false,
                        "dict": {
                            "dictType": "CS_ORDER_TYPE",
                            "dictKey": [
                                "CS_ORDER_TYPE"
                            ],
                            "format": "value-label"
                        }
                    }
                }
            ]
        },
        "toolbar": {
            "btns": [
                {
                    "type": "add",
                    "icon": "el-icon-circle-plus",
                    "target": "dialog",
                    "label": "新增",
                    "toRequest": {
                        "method": "GET",
                        "url": "/views/cs/paramManage/template/workerOrderTemplate/workerOrderTemplate_add.json"
                    },
                    "props": {
                        "width": "65%",
                        "title": "新增工单模板定义"
                    }
                }
            ]
        },
        "datagrid": {
            "isInitSearch": false,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "cb0065",
                    "appId": "cbOnl"
                }
            },
            "thead": [
                {
                    "type": "string",
                    "label": "模板编号",
                    "align": "center",
                    "prop": "template_id"
                    },
                {
                    "type": "string",
                    "label": "模板名称",
                    "align": "center",
                    "prop": "template_name"
                },
                {
                    "type": "string",
                    "align": "center",
                    "label": "模板类型",
                    "prop": "work_order_type",
                    "dict": {
                        "dictType": "CS_ORDER_TYPE",
                        "dictKey": [
                            "CS_ORDER_TYPE"
                        ],
                        "format": "value-label"
                    }
                },
                {
                    "type": "string",
                    "label": "模板描述",
                    "align": "center",
                    "prop": "template_desc"
                },
                {
                    "type": "string",
                    "label": "模板状态",
                    "align": "center",
                    "prop": "template_status",
                    "dict": {
                        "dictType": "CS_TEMPLATE_STATUS",
                        "dictKey": [
                            "CS_TEMPLATE_STATUS"
                        ],
                        "format": "value-label"
                    }
                },
                {
                    "label": "操作",
                    "width": "200%",
                    "fixed": "right",
                    "align": "center",
                    "btns": [
                        {
                            "type": "look",
                            "label": "详情",
                            "toRequest": {
                                "url": "/views/cs/paramManage/template/workerOrderTemplate/workerOrderTemplate_info.json",
                                "method": "POST"
                            },
                            "props": {
                                "title": "工单模板定义详情"
                            }
                        },
                        {
                            "type": "edit",
                            "label": "维护",
                            "toRequest": {
                                "url": "/views/cs/paramManage/template/workerOrderTemplate/workerOrderTemplate_modify.json",
                                "method": "POST"
                            },
                            "props": {
                                "title": "工单模板定义维护"
                            }
                        },
                        {   
                            "type": "edit",
                            "label": "扩展信息",
                            "toRequest": {
                                "url": "/views/cs/paramManage/template/workerOrderTemplate/field_details.json",
                                "method": "POST",
                                "params": {
                                    "props": "${{current}}"
                                  }
                            },
                            "props": {
                                "title": "扩展信息"
                            }
                        },
                        {
                            "type": "delete",
                            "label": "删除",
                            "doRequest": {
                                "url": "/SUMP/icmcall/icmRPCCall",
                                "method": "POST",
                                "params": {
                                    "servicecode": "cb0062",
                                    "appId": "cbOnl"
                                }
                            }
                        }
                    ]
                }
            ]
        }
    }
}
