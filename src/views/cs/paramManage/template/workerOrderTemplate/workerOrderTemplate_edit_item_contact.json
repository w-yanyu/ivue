{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": "",
                    "doRequest": {
                      "url": "/SUMP/icmcall/icmRPCCall",
                      "method": "POST",
                      "params": {
                          "servicecode": "cs0067",
                          "page_id": "/views/cs/paramManage/template/workerOrderTemplate/workerOrderTemplate_add_item_date.json",
                          "appId": "iccsOnl",
                          "coll_kind": "02",
                          "template_id":"${{params.template_id}}",
                          "filed_cd":"${{params.filed_cd}}",
                          "filed_type":"D"
                        }
                    }
                }
            ],

            "controlsGroup": [
              {
                  "title": "详细设置",
                  "tooltip": "",
                  "titleIcon": "",
                  "showHint": "",
                  "accordion": false,
                  "expand": true,

                  "controls": [
                      {
                          "filed_name": {
                              "control": "text",
                              "label": "字段名称",
                              "span": 16,
                              "width": "130px",
                              "rules": [
                                  {
                                      "required": true,
                                      "message":"请输入"
                                  }
                              ]
                          },
                          "default_value": {
                              "control": "text",
                              "label": "默认值",
                              "span": 16,
                              "width": "130px",
                              "rules": [
                                {
                                    "required": true,
                                    "message": "请输入"
                                },
                                {
                                    "max": 11,
                                    "message": "最多可以输入 11 个字节"
                                },
                                {
                                  "pattern": "^1(3[0-9]|5[189]|8[6789])[0-9]{8}$",
                                  "message": "联系电话格式错误"
                                }
                            ]
                          },
                          "filed_group": {
                            "control": "select",
                            "label": "分组",
                            "span": 16,
                            "width": "130px",
                            "options":[
                                {
                                  "label": "法诉信息",
                                  "value": "M"
                                },
                                {
                                  "label": "法诉费用",
                                  "value": "F"
                                },
                                {
                                  "label": "提醒信息",
                                  "value": "R"
                                },
                                {
                                  "label": "其他",
                                  "value": "O"
                                }
                              ],
                            "rules": [
                                {
                                "required": true,
                                "message": "请选择分组"
                                }
                            ]
                        },
                        "is_required": {
                            "control": "radio",
                            "label": "是否必填",
                            "span": 16,
                            "width": "130px",
                            "options":[
                              {
                                  "label":"是",
                                  "value":"Y"
                              },
                              {
                                  "label":"否",
                                  "value":"N"
                              }],
                            "rules": [
                                {
                                "required": true,
                                "message": "请选择是否必填"
                                }
                            ]
                        }
                          }
                  ]
              }, {
                  "title": "效果展示",
                  "tooltip": "",
                  "titleIcon": "",
                  "showHint": "",
                  "accordion": false,
                  "expand": true,

                  "controls": [
                      {
                          "template_result": {
                            "control": "text",
                            "label": "联系方式",
                            "span": 16,
                            "width": "130px",
                            "rules": [
                              {
                                  "required": true,
                                  "message": "请输入"
                              },
                              {
                                  "max": 11,
                                  "message": "最多可以输入 11 个字节"
                              },
                              {
                                "pattern": "^1(3[0-9]|5[189]|8[6789])[0-9]{8}$",
                                "message": "联系电话格式错误"
                              }
                          ]
                          }
                      }
                      ]
                  }
            ]
      }
    }
  }
