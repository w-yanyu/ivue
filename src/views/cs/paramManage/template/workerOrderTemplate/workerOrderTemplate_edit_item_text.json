{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": "",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "cs0067",
                            "page_id": "/views/cs/paramManage/template/workerOrderTemplate/workerOrderTemplate_add_item_text.json",
                            "appId": "iccsOnl",
                            "coll_kind": "02",
                            "template_id":"${{params.template_id}}",
                            "filed_cd":"${{params.filed_cd}}",
                            "filed_type":"T"
                        }
                    }
                }
            ],

            "controlsGroup": [
              {
                  "title": "详细设置",
                  "tooltip": "",
                  "titleIcon": "",
                  "showHint": "",
                  "accordion": false,
                  "expand": true,

                  "controls": [
                      {
                          "filed_name": {
                              "control": "text",
                              "label": "字段名称",
                              "span": 16,
                              "width": "130px",
                              "rules": [
                                  {
                                      "required": true,
                                      "message":"请输入"
                                  }
                              ]
                          },
                          "default_value": {
                            "label": "默认值",
                            "control": "textarea",
                            "span": 16,
                            "width": "130px",
                            "placeholder": "150字以内",
                            "autosize": {
                                "minRows": 4
                            },"rules": [{
                                "required": false,
                                "max": 150
                            }]
                          },
                          "filed_group": {
                            "control": "select",
                            "label": "分组",
                            "span": 16,
                            "width": "130px",
                            "options":[
                                {
                                  "label": "法诉信息",
                                  "value": "M"
                                },
                                {
                                  "label": "法诉费用",
                                  "value": "F"
                                },
                                {
                                  "label": "提醒信息",
                                  "value": "R"
                                },
                                {
                                  "label": "其他",
                                  "value": "O"
                                }
                              ],
                            "rules": [
                                {
                                "required": true,
                                "message": "请选择分组"
                                }
                            ]
                        },
                        "is_required": {
                            "control": "radio",
                            "label": "是否必填",
                            "span": 16,
                            "width": "130px",
                            "options":[
                              {
                                  "label":"是",
                                  "value":"Y"
                              },
                              {
                                  "label":"否",
                                  "value":"N"
                              }],
                            "rules": [
                                {
                                "required": true,
                                "message": "请选择是否必填"
                                }
                            ]
                        }
                       }
                  ]
              }, {
                  "title": "效果展示",
                  "tooltip": "",
                  "titleIcon": "",
                  "showHint": "",
                  "accordion": false,
                  "expand": true,

                  "controls": [
                      {
                          "template_result": {
                            "label": "字段名称xxx",
                            "control": "textarea",
                            "span": 16,
                            "width": "130px",
                            "placeholder": "150字以内",
                            "autosize": {
                                "minRows": 4
                            },"rules": [{
                                "required": false,
                                "max": 150
                            }]
                          }
                      }
                      ]
                  }
            ]
      }
    }
  }
