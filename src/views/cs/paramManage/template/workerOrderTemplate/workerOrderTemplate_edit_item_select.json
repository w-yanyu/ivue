{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": "",
                    "doRequest": {
                      "url": "/SUMP/icmcall/icmRPCCall",
                      "method": "POST",
                      "params": {
                          "servicecode": "cs0067",
                          "page_id": "/views/cs/paramManage/template/workerOrderTemplate/workerOrderTemplate_edit_item_date.json",
                          "appId": "iccsOnl",
                          "coll_kind": "02",
                          "template_id":"${{params.template_id}}",
                          "filed_cd":"${{params.filed_cd}}",
                          "filed_type":"S"
                      }
                    }
                }
            ],

            "controlsGroup": [
              {
                  "title": "详细设置",
                  "tooltip": "",
                  "titleIcon": "",
                  "showHint": "",
                  "accordion": false,
                  "expand": true,

                  "controls": [
                      {
                          "filed_name": {
                              "control": "text",
                              "label": "字段名称",
                              "span": 16,
                              "width": "130px",
                              "rules": [
                                  {
                                      "required": true,
                                      "message":"请输入"
                                  }
                              ]
                          },
                          "filed_group": {
                            "control": "select",
                            "label": "分组",
                            "span": 16,
                            "width": "130px",
                            "options":[
                                {
                                  "label": "法诉信息",
                                  "value": "M"
                                },
                                {
                                  "label": "法诉费用",
                                  "value": "F"
                                },
                                {
                                  "label": "提醒信息",
                                  "value": "R"
                                },
                                {
                                  "label": "其他",
                                  "value": "O"
                                }
                              ],
                            "rules": [
                                {
                                "required": true,
                                "message": "请选择分组"
                                }
                            ]
                        },
                        "is_required": {
                            "control": "radio",
                            "label": "是否必填",
                            "span": 16,
                            "width": "130px",
                            "options":[
                              {
                                  "label":"是",
                                  "value":"Y"
                              },
                              {
                                  "label":"否",
                                  "value":"N"
                              }],
                            "rules": [
                                {
                                "required": true,
                                "message": "请选择是否必填"
                                }
                            ]
                        }
                      }
                  ]
              }, {
                  "title": "下拉框选项",
                  "tooltip": "",
                  "titleIcon": "",
                  "showHint": "",
                  "accordion": false,
                  "expand": true,
                  "controls": [
                      {
                        "filed_select_data": {
                            "label": "选项（输入添加）",
                            "control": "select",
                            "span": 12,
                           "allowCreate":true,
                            "multiple":true,
                            "format": "value",
                            "options":[
                            ]
                          }
                      }
                      ]
                  }, {
                  "title": "效果展示",
                  "tooltip": "",
                  "titleIcon": "",
                  "showHint": "",
                  "accordion": false,
                  "expand": true,
                  "doControls":
                  {
                      "url":"/SUMP/cscall/icmRPCCall03",
                      "method": "POST",
                      "params": {
                        "servicecode": "cs0180",
                        "appId": "iccsOnl",
                        "coll_kind": "02",
                        "param_type":"select_type"
                      }
                  }
                  }
            ]
      }
    }
  }
