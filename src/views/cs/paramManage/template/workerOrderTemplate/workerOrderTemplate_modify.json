{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "scope": "formData",
            "initRequest": "${{params}}",
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "cb0063",
                    "appId": "cbOnl"
                }
            },
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": "el-icon-tickets"
                },
                {
                    "type": "close",
                    "label": "取消"
                }
            ],
            "controlsGroup": [
                {
                    "title": "模板基本信息",
                    "tooltip": "",
                    "titleIcon": "el-icon-info",
                    "showHint": "",
                    "accordion": false,
                    "expand": true,
                    "controls": [
                        {
                            "template_id": {
                                "control": "text",
                                "label": "模板编号",
                                "disabled": true,
                                "rules": [
                                    {
                                        "message": "请输入模板编号"
                                    }
                                ]
                            },
                            "template_name": {
                                "control": "text",
                                "label": "模板名称",
                                "disabled": true,
                                "rules": [
                                    {
                                        "message": "请输入模板名称"
                                    }
                                ]
                            },
                            "work_order_type": {
                                "control": "select",
                                "label": "模板类型",
                                "selectFirstValue": false,
                                "dict": {
                                    "dictType": "CS_ORDER_TYPE",
                                    "dictKey": [
                                        "CS_ORDER_TYPE"
                                    ],
                                    "format": "value-label"
                                }
                            },
                            "template_desc": {
                                "control": "textarea",
                                "label": "模板描述",
                                "width": "130",
                                "min": 1
                            },
                            "wf_prod_no": {
                                "control": "text",
                                "label": "流程产品编号",
                                "width": "130",
                                "rules": [
                                  {
                                    "required": true,
                                    "message": "请输入流程产品编号"
                                  },
                                  {
                                    "max": 64,
                                    "message": "长度不能超过64个字符"
                                  }
                                ]
                              },
                              "wf_prod_name": {
                                "control": "text",
                                "label": "流程产品名称",
                                "width": "130",
                                "rules": [
                                  {
                                    "required": true,
                                    "message": "请输入流程产品名称"
                                  },
                                  {
                                    "max": 64,
                                    "message": "长度不能超过64个字符"
                                  }
                                ]
                              },
                            "template_status": {
                                "control": "select",
                                "label": "模板状态",
                                "selectFirstValue": false,
                                "dict": {
                                    "dictType": "CS_TEMPLATE_STATUS",
                                    "dictKey": [
                                        "CS_TEMPLATE_STATUS"
                                    ],
                                    "format": "value-label"
                                }
                            }
                        }
                    ]
                },
                {
                    "title": "固定信息",
                    "tooltip": "",
                    "titleIcon": "el-icon-info",
                    "showHint": "",
                    "accordion": false,
                    "expand": true,
                    "controls": [
                        {
                            "content": {
                                "control": "text",
                                "disabled": true,
                                "label": "工单号",
                                "message": "请输入",
                                "rules": [
                                    {
                                        "message": "请输入"
                                    }
                                ]
                            },
                            "yjdate": {
                                "control": "text",
                                "disabled": true,
                                "label": "提单人",
                                "rules": [
                                    {
                                        "message": "请输入"
                                    }
                                ]
                            },
                            "category": {
                                "control": "text",
                                "disabled": true,
                                "label": "优先级"
                            },
                            "yjcssj": {
                                "control": "text",
                                "disabled": true,
                                "label": "截止日期"
                            },
                            "cmanager": {
                                "control": "text",
                                "disabled": true,
                                "label": "抄送人"
                            },
                            "cmanager111": {
                                "control": "text",
                                "disabled": true,
                                "label": "关联案件"
                            }
                        }
                    ]
                }
            ]
        }
    }
}