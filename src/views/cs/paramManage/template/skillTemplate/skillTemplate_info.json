{
    "module": "v-realonlyForm-btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "controls": [
                {
                    "call_reference_id": {
                        "control": "string",
                        "disabled":true,
                        "label": "话术代码",
                        "width": "130px"
                    },
                    "call_reference_name": {
                        "control": "string",
                        "label": "话术名称",
                        "width": "130px"
                    }
                },
                {
                    "call_reference_order": {
                        "control": "inputNumber",
                        "disabled":true,
                        "label": "话术排序",
                        "width": "130px"
                    },
                    "is_valid": {
                        "control": "string",
                        "label": "是否启用",
                        "width": "130px",
                        "default":"Y",
                        "dict": {
                            "dictType": "CS_E_YESORNO",
                            "dictKey": [
                                "CS_E_YESORNO"
                            ],
                            "format": "value-label"
                        }
                    }
                },
                {
                    "call_reference_desc": {
                        "control": "string",
                        "label": "话术描述",
                        "width": "130px",
                        "span":"24",
                        "autosize":"{ minRows: 2, maxRows: 4}"
                    }
                }
            ]
        }
    }
}
