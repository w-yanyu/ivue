{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "scope":"formData",
            "initRequest": "${{params}}",
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "cb0068",
                    "appId": "cbOnl",
                    "coll_kind": "02"
                }
            },
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": "el-icon-tickets"
                }
            ],
            "controls": [
                {
                    "call_reference_id": {
                        "control": "text",
                        "label": "话术代码",
                        "disabled":true,
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message":"请输入"
                            }
                        ]
                    },
                    "call_reference_name": {
                        "control": "text",
                        "label": "话术名称",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message":"请输入"
                            }
                        ]
                    }
                },
                {
                    "call_reference_order": {
                        "control": "inputNumber",
                        "label": "话术排序",
                        "width": "130px",
                        "min": 1,
                        "rules": [
                            {
                                "required": true,
                                "message":"请输入"
                            }
                        ]
                    },
                    "is_valid": {
                        "control": "select",
                        "label": "是否启用",
                        "width": "130px",
                        "default":"Y",
                        "rules": [
                            {
                                "required": true,
                                "message":"请输入"
                            }
                        ],
                        "dict": {
                            "dictType": "CS_E_YESORNO",
                            "dictKey": [
                                "CS_E_YESORNO"
                            ],
                            "format": "value-label"
                        }
                    }
                },
                {
                    "call_reference_desc": {
                        "control": "textarea",
                        "label": "话术描述",
                        "width": "130px",
                        "span":"24",
                        "autosize":"{ minRows: 2, maxRows: 4}",
                        "rules": [
                            {
                                "required": true,
                                "message":"请输入"
                            }
                        ]
                    }
                }
            ]
        }
    }
}
