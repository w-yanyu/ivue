{
    "module": "v_search_btn_datagrid",

    "layout": {
        "form": {
            "controls": [
                {
                    "template_type": {
                        "control": "select",
                        "label": "模板类型",
                        "dict": {
                          "dictType": "CS_E_TEMPLATE_TYPE",
                          "dictKey": [
                            "CS_E_TEMPLATE_TYPE"
                          ],
                          "format": "value-label"
                        },
                        "rules": [
                          {
                            "message": "请选择模板类型"
                          }
                        ]
                    },
                    "template_name": {
                        "control": "text",
                        "label": "模板名称"
                    }
                },
                {
                    "template_desc": {
                        "control": "text",
                        "label": "模板用途"
                    }
                }
            ]
        },
        "toolbar": {
            "btns": [
                {
                    "type": "add",
                    "icon": "el-icon-circle-plus",
                    "target": "dialog",
                    "label": "新增",
                    "toRequest": {
                        "method": "GET",
                        "url": "/views/cs/paramManage/template/collTemplate/collTemplate_add.json"
                    },
                    "props": {
                        "width": "50%",
                        "title": "新增导入模板"
                    }
                }
            ]
        },
        "datagrid": {
            "isInitSearch": false,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "cb0058",
                    "appId": "cbOnl",
                    "coll_kind": "02"
                }
            },

            "thead": [
                {
                    "type":"string",
                    "label": "模板类型",
                    "align":"center",
                    "prop": "template_type",
                    "dict": {
                        "dictType": "CS_E_TEMPLATE_TYPE",
                        "dictKey": [
                          "CS_E_TEMPLATE_TYPE"
                        ],
                        "format": "value-label"
                      }
                },
                {
                    "type":"string",
                    "label": "模板代码",
                    "align":"center",
                    "prop": "template_code"
                },
                {
                    "type":"string",
                    "label": "模板名称",
                    "align":"center",
                    "prop": "template_name"
                },
                {
                    "type":"string",
                    "label": "模板用途",
                    "align":"center",
                    "prop": "template_desc"
                },
                {
                    "type":"string",
                    "label": "文件名称",
                    "align":"center",
                    "prop": "file_name"
                },
                {
                    "label": "操作",
                    "fixed": "right",
                    "align":"center",
                    "width":"150%",
                    "btns": [
                        {
                        "type": "export",
                        "label": "下载",
                        "doRequest": {
                            "url": "/SUMP/minioCall/download",
                            "method": "GET",
                            "params": {
                            "path": "${{current.template_url}}",
                            "fileName": "${{current.file_name}}"
                            }
                        },
                        "eventAfter": {
                            "doParentCallback": true
                        }
                        },
                        {
                            "type": "delete",
                            "label": "删除",
                            "doRequest": {
                                "url": "/SUMP/icmcall/icmRPCCall",
                                "method": "POST",
                                "params": {
                                    "servicecode": "cb0060",
                                    "appId": "cbOnl",
                                    "coll_kind": "02"
                                }
                            }
                        }
                    ]
                }
            ]
        }
    }
}
