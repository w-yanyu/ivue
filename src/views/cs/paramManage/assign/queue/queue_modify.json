{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "scope": "formData",
      "initRequest": "${{params}}",
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "cb0080",
          "appId": "cbOnl"
        }
      },
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "el-icon-tickets"
        }
      ],
      "controls": [
        {
          "queue_code": {
            "control": "text",
            "disabled": true,
            "label": "队列代码",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入"
              },
              {
                "max": 10,
                "message": "最多可以输入 10 个字节"
              }
            ]
          },
          "queue_name": {
            "control": "text",
            "label": "队列名称",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入"
              },
              {
                "max": 30,
                "message": "最多可以输入 30 个字节"
              }
            ]
          }
        },
        {
          "queue_desc": {
            "control": "text",
            "label": "队列描述",
            "width": "130px",
            "rules": [
              {
                "max": 100,
                "message": "最多可以输入 100 个字节"
              }
            ]
          },
          "coll_type": {
            "control": "select",
            "label": "催收方式",
            "width": "130px",
            "selectFirstValue": false,
            "dict": {
              "dictType": "CS_COLL_TYPE",
              "dictKey": [
                "CS_COLL_TYPE"
              ],
              "format": "value-label",
              "rules": [
                {
                  "required": true,
                  "message": "请输入"
                }
              ]
            }
          }
        },
        {
          "queue_type": {
            "control": "select",
            "label": "队列类型",
            "width": "130px",
            "selectFirstValue": false,
            "dict": {
              "dictType": "CS_QUEUE_TYPE",
              "dictKey": [
                "CS_QUEUE_TYPE"
              ],
              "format": "value-label",
              "rules": [
                {
                  "required": true,
                  "message": "请输入"
                }
              ]
            }
          },
          "priority": {
            "control": "inputNumber",
            "label": "优先级",
            "width": "130px",
            "min": 1,
            "rules": [
              {
                "required": true,
                "message": "请输入"
              },
              {
                "expression": "'${{form.priority}}'.length < 20",
                "message": "最多可以输入 19 个字节"
              }
            ]
          }
        },
        {
          "is_valid": {
            "control": "select",
            "label": "是否启用",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ],
            "dict": {
              "dictType": "CS_E_YESORNO",
              "dictKey": [
                "CS_E_YESORNO"
              ],
              "format": "value-label"
            }
          }
        }
      ]
    }
  }
}