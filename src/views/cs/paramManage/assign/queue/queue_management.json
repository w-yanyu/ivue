{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "scope": "formData",
      "controls": [
        {
          "queue_code": {
            "control": "text",
            "label": "队列代码"
          },
          "queue_name": {
            "control": "text",
            "label": "队列名称"
          }
        },
        {
          "queue_desc": {
            "control": "text",
            "label": "队列描述"
          },
          "coll_type": {
            "control": "select",
            "label": "催收方式",
            "selectFirstValue": false,
            "dict": {
              "dictType": "CS_COLL_TYPE",
              "dictKey": [
                "CS_COLL_TYPE"
              ],
              "format": "value-label"
            }
          }
        },
        {
          "queue_type": {
            "control": "select",
            "label": "队列类型",
            "selectFirstValue": false,
            "dict": {
              "dictType": "CS_QUEUE_TYPE",
              "dictKey": [
                "CS_QUEUE_TYPE"
              ],
              "format": "value-label"
            }
          },
          "org_id": {
            "hidden": true,
            "label": "创建法人",
            "control": "remoteSelect",
            "labelField": "org_name",
            "valueField": "org_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2311",
                "org_id": "$g{{user_info.org_id}}"
              }
            },
            "events": {
              "emit": [
                "operation"
              ]
            },
            "initShow": {
              "expression": "'${{form.org_id}}' === ''"
            },
            "format": "value-label"
          }
        },
        {
          "branch_id": {
            "hidden": true,
            "label": "创建机构",
            "control": "remoteSelect",
            "labelField": "branch_name",
            "valueField": "branch_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2142",
                "user_id": "$g{{user_info.userCode}}",
                "branch_id": "$g{{user_info.branch_id}}",
                "brch_relation_code": "01"
              }
            },
            "events": {
              "emit": [
                "operation"
              ]
            },
            "initShow": {
              "expression": "'${{form.branch_id}}' === ''"
            },
            "format": "value-label"
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "icon": "el-icon-circle-plus",
          "target": "dialog",
          "label": "新增",
          "toRequest": {
            "method": "GET",
            "url": "/views/cs/paramManage/assign/queue/queue_add.json"
          },
          "props": {
            "width": "65%",
            "title": "新增催收队列定义"
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "cb0081",
          "appId": "cbOnl"
        }
      },
      "thead": [
        {
          "type": "string",
          "align": "center",
          "label": "队列代码",
          "prop": "queue_code"
        },
        {
          "type": "string",
          "align": "center",
          "label": "队列名称",
          "prop": "queue_name"
        },
        {
          "type": "string",
          "align": "center",
          "width": "270px",
          "label": "队列描述",
          "prop": "queue_desc"
        },
        {
          "type": "string",
          "align": "center",
          "label": "催收方式",
          "prop": "coll_type",
          "dict": {
            "dictType": "CS_COLL_TYPE",
            "dictKey": [
              "CS_COLL_TYPE"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "队列类型",
          "prop": "queue_type",
          "dict": {
            "dictType": "CS_QUEUE_TYPE",
            "dictKey": [
              "CS_QUEUE_TYPE"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "优先级",
          "prop": "priority"
        },
        {
          "type": "string",
          "align": "center",
          "label": "创建法人",
          "dict": {
            "dictKey": [
              "org_id"
            ],
            "labelField": "org_name",
            "valueField": "org_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2311",
                "org_id": "${{current.org_id}}"
              }
            },
            "format": "value-label"
          },
          "prop": "org_id"
        },
        {
          "type": "string",
          "align": "center",
          "label": "创建机构",
          "dict": {
            "dictKey": [
              "branch_id"
            ],
            "labelField": "branch_name",
            "valueField": "branch_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2155",
                "org_id": "${{current.branch_id}}"
              }
            },
            "format": "value-label"
          },
          "prop": "branch_id"
        },
        {
          "type": "string",
          "align": "center",
          "label": "是否启用",
          "prop": "is_valid",
          "dict": {
            "dictType": "CS_E_YESORNO",
            "dictKey": [
              "CS_E_YESORNO"
            ],
            "format": "value-label"
          }
        },
        {
          "label": "操作",
          "align": "center",
          "fixed": "right",
          "width": "150%",
          "btns": [
            {
              "type": "look",
              "label": "详情",
              "toRequest": {
                "url": "/views/cs/paramManage/assign/queue/queue_info.json",
                "method": "POST"
              },
              "props": {
                "title": "催收队列定义详情"
              }
            },
            {
              "type": "edit",
              "label": "维护",
              "toRequest": {
                "url": "/views/cs/paramManage/assign/queue/queue_modify.json",
                "method": "POST"
              },
              "props": {
                "title": "催收队列定义维护"
              }
            },
            {
              "type": "delete",
              "label": "删除",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "cb0079",
                  "appId": "cbOnl"
                }
              }
            }
          ]
        }
      ]
    }
  }
}