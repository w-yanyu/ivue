{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "scope": "formData",
      "initRequest": "${{params}}",
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "cb0051",
          "server_range": "cbOnl"
        }
      },
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "el-icon-tickets"
        }
      ],
      "controls": [
        {       
          "outsourcing_org_id": {
            "control": "remoteSelect",
            "label": "委外公司法人",
            "labelField": "branch_id",
            "valueField": "org_id",
            "disabled":true,
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2311"            
              }
            },  
            "events": {
              "emit": [
                "outsourcing_org_name",
                "outsourcing_branch_id"
              ]
            },
            "format": "value",
            "rules": [
              {
                "message": "委外公司法人不能为空",
                "required": true
              }
            ]
          },"outsourcing_org_name": {
            "control": "lookup",
            "label": "委外公司名称",
            "disabled":true,
            "keyField":{
              "outsourcing_branch_id": "branch_id",
              "outsourcing_org_name": "branch_name"
            },
            "toRequest": {
              "url": "/views/cs/paramManage/assign/collOrg/branch-info-manager_lookup.json",
              "method": "post",
              "params": {
                "org_id":"${{form.outsourcing_org_id}}"         
              }
            },
            "rules": [
              {
                "required": true,
                "message": "委外公司不得为空"
              }
            ]
        
          }
        },
        { 
          "outsourcing_branch_id": {
          "control": "text",
          "label": "委外公司机构号",
          "disabled":true,
          "format": "value",
          "rules": [
            {
              "message": "委外公司机构号不能为空",
              "required": true
            }
          ]
        },        
          "outsourcing_org_type": {
            "control": "select",
            "label": "委外公司类型",
            "selectFirstValue": false,
            "dict": {
              "dictType": "CS_ORGAN_TYPE",
              "dictKey": [
                "CS_ORGAN_TYPE"
              ],
              "labelField": "dictName",
              "valueField": "dictId",
              "format": "value-label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择委外公司类型"
              }
            ]
          }
        },
        {
          "postal_code": {
            "control": "text",
            "label": "邮编"
          },
          "address": {
            "control": "text",
            "label": "地址"
          }
        },
        {
          "phone_no": {
            "control": "text",
            "label": "电话号码",
            "rules": [
              {
                "max": 30,
                "message": "最多可以输入 30 个字节"
              },
              {
                "pattern": "^1(3[0-9]|5[189]|8[6789])[0-9]{8}$",
                "message": "电话号码格式错误"
              }
            ]
          },
          "fox": {
            "control": "text",
            "label": "传真",
            "rules": [
              {
                "max": 20,
                "message": "最多可以输入 20 个字节"
              },
              {
                "pattern": "^[+]{0,1}(\\d){1,3}[ ]?([-]?((\\d)|[ ]){1,12})+$",
                "message": "传真格式错误"
              }
            ]
          }
        },
        {
          "relation_cust_name": {
            "control": "text",
            "label": "联系人",
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          },
          "relation_phone_no": {
            "control": "text",
            "label": "联系电话",
            "rules": [
              {
                "required": true,
                "message": "请输入"
              },
              {
                "max": 11,
                "message": "最多可以输入 11 个字节"
              },
              {
                "pattern": "^1(3[0-9]|5[189]|8[6789])[0-9]{8}$",
                "message": "联系电话格式错误"
              }
            ]
          }
        },
        {
          "is_valid": {
            "control": "select",
            "label": "是否启用",
            "default": "Y",
            "dict": {
              "dictType": "CS_E_YESORNO",
              "dictKey": [
                "CS_E_YESORNO"
              ],
              "format": "value-label"
            },
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          }
        }
      ]
    }
  }
}
