{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/call/RPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ct2154",
          "ccy_code":"156",
          "brch_relation_code":"01",
          "org_id":"${{params.org_id}}",
          "brch_level":"1"

        }
      },
      "thead": [       
        {
          "label": "机构号",
          "width": 120,
          "align": "center",
          "prop": "branch_id"
        },
        {
          "label": "机构名称",
          "align": "center",
          "prop": "branch_name"
        },
        {
          "label": "实体机构标志",
          "align": "center",
          "prop": "real_branch_ind",
          "dict": {
            "dictType": "MS_E_YESORNO",
            "dictKey": [
              "MS_E_YESORNO"
            ],
            "format": "value-label"
          }
        },
        {
          "label": "法人代码",
          "align": "center",
          "prop": "org_id"
        },
        {
          "label": "操作",
          "width": 120,
          "fixed": "right",
          "align": "center",
          "btns": [
            {
              "type": "bringback",
              "label": "选择"
            }
          ]
        }
      ]
    }
  }
}
