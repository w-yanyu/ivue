{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "btns": [
                {
                    "type": "submit",
                    "label": "确认",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                          "servicecode": "cb0044",
                          "appId": "cbOnl",
                          "branch_id": "${{params.branch_id}}"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "collector": {
                        "control": "text",
                        "label": "催收员编号",
                        "width": "130px",
                        "value": "${{params.user_id}}",
                        "disabled": true
                    },
                    "collector_name": {
                        "control": "text",
                        "label": "催收员姓名",
                        "width": "130px",
                        "value": "${{params.user_name}}",
                        "disabled": true
                    }
                },
                {
                    "coll_team": {
                        "control": "text",
                        "label": "所属催收组",
                        "width": "130px",
                        "disabled": true,
                        "value": "${{params.coll_team}}"
                    }
                }
            ]
        }
    }
}
