{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "scope":"formData",
            "initRequest": "${{params}}",
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "cb0046",
                  "appId": "cbOnl"
                }
            },
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": "el-icon-tickets"
                }
            ],
            "controls": [
                {
                    "collector": {
                        "control": "text",
                        "label": "催收员编号",
                        "disabled": true,
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            },
                            {
                                "max": 20,
                                "message": "最多可以输入 20 个字节"
                            }
                        ]
                    },
                    "collector_name": {
                        "control": "text",
                        "label": "催收员姓名",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            },
                            {
                                "max": 30,
                                "message": "最多可以输入 30 个字节"
                            }
                        ]
                    }
                },
                {
                    "coll_team": {
                        "control": "text",
                        "label": "所属催收组",
                        "width": "130px",
                        "disabled": true,
                        "value": "${{params.props.coll_team}}"
                    },
                    "coll_position": {
                        "control": "select",
                        "default":"1",
                        "label": "催收岗位",
                        "width": "130px",
                        "dict": {
                            "dictType": "CS_E_COLL_POST",
                            "dictKey": [
                                "CS_E_COLL_POST"
                            ],
                            "format": "value-label"
                        },
                        "rules": [
                            {
                                "required":true,
                                "message": "请输入"
                            }
                        ]
                    }
                },
                {
                    "work_number": {
                        "control": "text",
                        "label": "坐席工号",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            }
                        ]
                    },
                    "work_password": {
                        "control": "text",
                        "label": "坐席密码",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            },
                            {
                                "max": 20,
                                "message": "最多可以输入 20 个字节"
                            }
                        ]
                    }
                },
                {
                    "sub_phone_no": {
                        "control": "text",
                        "label": "分机号",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            },
                            {
                              "pattern": "^1[3-9]\\d{9}|0\\d{2,3}-\\d{7,8}(-\\d{1,6})?$",
                              "message": "分机号格式错误"
                            }
                        ]
                    }
                }
            ]
        }
    }
}
