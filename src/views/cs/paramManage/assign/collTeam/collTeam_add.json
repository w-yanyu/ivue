{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                          "servicecode": "cb0020",
                          "appId": "cbOnl"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "coll_team": {
                        "control": "text",
                        "label": "催收组编号",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            },
                            {
                                "max": 20,
                                "message": "最多可以输入 20 个字节"
                            }
                        ]
                    },
                    "coll_team_name": {
                        "control": "text",
                        "label": "催收组名称",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            },
                            {
                                "max": 30,
                                "message": "最多可以输入 30 个字节"
                            }
                        ]
                    }
                },
                {
                    "parent_coll_team": {
                        "control": "text",
                        "label": "上级催收组",
                        "width": "130px",
                        "disabled": true,
                        "value": "${{params.props.coll_team}}"
                    },
                    "sort": {
                        "control": "text",
                        "label": "催收组序号",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            },
                            {
                                "max": 30,
                                "message": "最多可以输入 30 个字节"
                            }
                        ]
                    }
                },
                {
                  "source_branch_id": {
                    "control": "text",
                    "label": "所属机构信息",
                    "width": "130px",
                    "rules": [
                      {
                        "required": true,
                        "message": "请输入"
                      },
                      {
                        "max": 30,
                        "message": "最多可以输入 30 个字节"
                      }
                    ]
                  },
                  "org_id": {
                    "control": "text",
                    "label": "创建法人",
                    "width": "130px",
                    "disabled": true,
                    "value": "$g{{user_info.org_id}}"
                  }
                },
                {
                  "branch_id": {
                    "control": "text",
                    "label": "创建机构",
                    "width": "130px",
                    "disabled": true,
                    "value": "$g{{user_info.branch_id}}"
                  }
                }
            ]
        }
    }
}
