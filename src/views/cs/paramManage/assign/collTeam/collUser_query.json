{
  "module": "v_search_btn_datagrid",
  "layout": {
    "toolbar": {
      "btns": [
      ]
    },
    "datagrid": {
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "cb0048",
          "appId": "cbOnl",
          "org_id": "$g{{user_info.org_id}}"
        }
      },
      "thead": [
        {
          "label": "用户ID",
          "align": "center",
          "prop": "user_id"
        },
        {
          "label": "用户姓名",
          "align": "center",
          "prop": "user_name"
        },
        {
          "label": "操作",
          "align": "center",
          "fixed": "right",
          "btns": [
            {
              "type": "custom",
              "label": "选择",
              "toRequest": {
                "url": "/views/cs/paramManage/assign/collTeam/collUser_add.json",
                "method": "GET",
                "params": {
                  "props": "${{current}}",
                  "coll_team": "${{params.props.coll_team}}"
                }
              },
              "props": {
                "title": "催收员新增"
              }
            }
          ]
        }
      ]
    }
  }
}
