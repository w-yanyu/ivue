{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "scope": "formData",
            "controls": [
                {
                    "scheme_type": {
                      "control": "select",
                      "label": "分配方案类型",
                      "selectFirstValue":false,
                      "dict": {
                        "dictType": "CS_COLL_TYPE",
                        "dictKey": [
                          "CS_COLL_TYPE"
                        ],
                        "format": "value-label"
                      }
                    },
                    "scheme_pattern": {
                      "control": "select",
                      "label": "分配方式",
                      "selectFirstValue":false,
                      "dict": {
                        "dictType": "CS_SCHEME_PATTERN",
                        "dictKey": [
                          "CS_SCHEME_PATTERN"
                        ],
                        "format": "value-label"
                      }
                    },
                    "scheme_name": {
                        "control": "text",
                        "label": "分配方案名称"
                    }
                }
            ]
        },
        "toolbar": {
            "btns": [
                {
                    "type": "add",
                    "icon": "el-icon-circle-plus",
                    "target": "dialog",
                    "label": "新增",
                    "toRequest": {
                        "method": "GET",
                        "url": "/views/pl/paramManage/assign/scheme/scheme_add.json"
                    },
                    "props": {
                        "width": "65%",
                        "title": "新增分配方案定义"
                    }
                }
            ]
        },
        "datagrid": {
            "isInitSearch": false,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "cs0064",
                    "appId": "iccsOnl",
                    "coll_kind": "02"
                }
            },
            "thead": [
                {
                    "type":"string",
                    "align":"center",
                    "label": "分配方案编号",
                    "prop": "scheme_cd"

                },
                {
                    "type":"string",
                    "align":"center",
                    "label": "分配方案名称",
                    "prop": "scheme_name"
                },
                {
                    "type":"string",
                    "align":"center",
                    "label": "分配方案类型",
                     "prop": "scheme_type",
                    "dict": {
                      "dictType": "CS_COLL_TYPE",
                      "dictKey": [
                        "CS_COLL_TYPE"
                      ],
                      "format": "value-label"
                    }
                },
                {
                    "type":"string",
                    "align":"center",
                    "label": "分配方式",
                    "prop": "scheme_pattern",
                    "dict": {
                      "dictType": "CS_SCHEME_PATTERN",
                      "dictKey": [
                        "CS_SCHEME_PATTERN"
                      ],
                      "format": "value-label"
                   }
                },
                {
                    "type":"string",
                    "align":"center",
                    "label": "优先级",
                    "prop": "priority"
                },

                {
                    "type":"string",
                    "align":"center",
                    "label": "是否启用",
                    "prop": "is_valid",
                    "dict": {
                        "dictType": "CS_E_YESORNO",
                        "dictKey": [
                            "CS_E_YESORNO"
                        ],
                        "format": "value-label"
                    }
                },

                {
                    "label": "操作",
                    "align":"center",
                    "fixed": "right",
                    "width": "150%",
                    "btns": [
                        {
                            "type":"look",
                            "label":"详情",
                            "show": {
                              "expression": "'${{current.scheme_type}}' === 'TC01'"
                            },
                            "toRequest":{
                                "url":"/views/cs/paramManage/assign/scheme/tc_scheme_info.json",
                                "method":"POST"
                            },
                            "props":{
                                "width":"80%",
                                "title":"分配方案定义详情"
                            }
                        },
                        {
                          "type":"look",
                          "label":"详情",
                          "show": {
                            "expression": "'${{current.scheme_type}}' !== 'TC01'"
                          },
                          "toRequest":{
                            "url":"/views/cs/paramManage/assign/scheme/scheme_info.json",
                            "method":"POST"
                          },
                          "props":{
                            "width":"80%",
                            "title":"分配方案定义详情"
                          }
                        },
                        {
                            "type": "edit",
                            "label": "维护",
                            "show": {
                              "expression": "'${{current.scheme_type}}' === 'TC01'"
                            },
                            "toRequest": {
                                "url": "/views/cs/paramManage/assign/scheme/tc_scheme_modify.json",
                                "method": "POST"
                            },
                            "props": {
                                "width":"80%",
                                "title": "分配方案定义维护"
                            }
                        },
                      {
                        "type": "edit",
                        "label": "维护",
                        "show": {
                          "expression": "'${{current.scheme_type}}' !== 'TC01'"
                        },
                        "toRequest": {
                          "url": "/views/cs/paramManage/assign/scheme/scheme_modify.json",
                          "method": "POST"
                        },
                        "props": {
                          "width":"80%",
                          "title": "分配方案定义维护"
                         }
                      },
                        {
                            "type": "delete",
                            "label": "删除",
                            "doRequest": {
                                "url": "/SUMP/iccsCall/iccsEurekaCall",
                                "method": "POST",
                                "params": {
                                    "servicecode": "cs0061"
                                }
                            }
                        }
                    ]
                }
            ]
        }
    }
}
