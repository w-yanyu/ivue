{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "doRequest": {
            "url": "/SUMP/iccsCall/iccsEurekaCall",
            "method": "POST",
            "params": {
              "servicecode": "cs0060",
              "page_id": "/views/pl/paramManage/assign/scheme/scheme_add.json"
            }
          }
        }
      ],
      "controls": [
        {
          "scheme_cd": {
            "control": "text",
            "label": "分配方案编号",
            "width": "130",
            "rules": [
              {
                "required": true,
                "message": "请输入"
              },
              {
                "max": 10,
                "message": "最多可以输入 10 个字节"
              }
            ]
          },
          "scheme_name": {
            "control": "text",
            "label": "分配方案名称",
            "width": "130",
            "rules": [
              {
                "required": true,
                "message": "请输入"
              },
              {
                "max": 30,
                "message": "最多可以输入 30 个字节"
              }
            ]
          }
        },
        {
          "scheme_type": {
            "control": "remoteSelect",
            "label": "分配方案类型",
            "default": "TC01",
            "width": "130",
            "labelField": "drop_list_desc",
            "valueField": "drop_list_value",
            "doRequest": {
              "url": "/SUMP/iccsCall/iccsEurekaCall",
              "method": "POST",
              "params": {
                "servicecode": "ap1071",
                "drop_list_type": "CS_COLL_TYPE"
              }
            },
            "format": "value-label",
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          },
          "scheme_pattern": {
            "control": "remoteSelect",
            "label": "分配方式",
            "default": "A",
            "width": "130",
            "labelField": "drop_list_desc",
            "valueField": "drop_list_value",
            "doRequest": {
              "url": "/SUMP/iccsCall/iccsEurekaCall",
              "method": "POST",
              "params": {
                "servicecode": "ap1071",
                "drop_list_type": "scheme_pattern"
              }
            },
            "format": "value-label",
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          }
        },
        {
          "priority": {
            "control": "inputNumber",
            "label": "优先级",
            "width": "130",
            "min": 1,
            "rules": [
              {
                "required": true,
                "message": "请输入"
              },
              {
                "expression": "'${{form.priority}}'.length < 20",
                "message": "最多可以输入 19 个字节"
              }
            ]
          },
          "is_valid": {
            "control": "select",
            "label": "是否启用",
            "default": "Y",
            "width": "130",
            "dict": {
              "dictType": "CS_E_YESORNO",
              "dictKey": [
                "CS_E_YESORNO"
              ],
              "format": "value-label"
            },
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          }
        }
      ]
    }
  }
}
