{
  "module": "lr_transferTable",
  "layout": {
    "transfer": {
      "height":"600px",
      "left": {
        "filterable":false,
        "title": "催收结果列表",
        "placeholder": "请输入催收结果代码",
        "query":["coll_result_code","coll_result_name"],
        "btn":{
          "label": "授权",
          "doRequest": {
            "url":"/SUMP/icmcall/icmRPCCall",
            "method":"post",
            "params":{
              "servicecode": "cb0039",
              "appId": "cbOnl",
              "coll_result_code": "${{params.coll_result_code}}",
              "coll_result_name": "${{params.coll_result_name}}",
              "org_id": "${{params.org_id}}"
            }
          }
        },
        "datagrid": {
          "scope": "left",
          "doRequest": {
            "url":"/SUMP/icmcall/icmRPCCall",
            "method":"POST",
            "params": {
              "servicecode": "cb0038",
              "appId": "cbOnl"
            }
          },
          "checked":"checked",
          "thead": [
            {
              "prop": "coll_result_code",
              "label": "催收结果代码"
            },
            {
              "prop": "coll_result_name",
              "label": "催收结果名称"
            },
            {
              "prop": "coll_result_type",
              "label": "是否可联",
              "dict": {
                "dictType": "CS_E_YESORNO",
                "dictKey": [
                  "CS_E_YESORNO"
                ],
                "format": "value-label"
              }
            },
            {
              "prop": "is_valid_result",
              "label": "是否有效",
              "dict": {
                "dictType": "CS_E_YESORNO",
                "dictKey": [
                  "CS_E_YESORNO"
                ],
                "format": "value-label"
              }
            }
          ]
        }
      },
      "right": {
        "filterable":false,
        "title":"已选催收结果",
        "placeholder": "请输入催收结果名称",
        "btn":{
          "label":"取消授权",
          "doRequest": {
            "url":"/SUMP/icmcall/icmRPCCall",
            "method":"post",
            "params":{
              "servicecode": "cb0040",
              "appId": "cbOnl",
              "coll_result_code": "${{params.coll_result_code}}",
              "coll_result_name": "${{params.coll_result_name}}",
              "org_id": "${{params.org_id}}"
            }
          }
        },
        "datagrid": {
          "scope": "right",
          "doRequest": {
            "url":"/SUMP/icmcall/icmRPCCall",
            "method":"POST",
            "params": {
              "servicecode": "cb0037",
              "appId": "cbOnl",
              "coll_result_code": "${{params.coll_result_code}}",
              "org_id": "${{params.org_id}}"
            }
          },
          "thead": [
            {
              "prop": "org_id",
              "label": "法人代码"
            },
            {
              "prop": "coll_result_code",
              "label": "催收结果代码"
            },
            {
              "prop": "coll_result_name",
              "label": "催收结果名称"
            }
          ]
        }
      }
    }
  }
}
