{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "scope": "formData",
      "controls": [
        {
          "fee_cd": {
            "control": "text",
            "label": "费率代码"
          },
          "fee_type": {
            "control": "remoteSelect",
            "label": "费率类型",
            "labelField": "drop_list_desc",
            "valueField": "drop_list_value",
            "doRequest": {
              "url": "/SUMP/iccsCall/iccsEurekaCall",
              "method": "POST",
              "params": {
                "servicecode": "ap1071",
                "drop_list_type": "fee_type_pl"
              }
            },
            "format": "value-label"
          },
          "handstr": {
            "control": "inputNumber",
            "label": "手别",
            "min": 0,
            "max": 10
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "icon": "el-icon-circle-plus",
          "target": "dialog",
          "label": "新增",
          "toRequest": {
            "method": "GET",
            "url": "/views/pl/paramManage/business/fee/fee_add.json"
          },
          "props": {
            "width": "65%",
            "title": "新增佣金参数"
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/iccsCall/iccsEurekaCall",
        "method": "POST",
        "params": {
          "servicecode": "cs0084"
        }
      },
      "thead": [
        {
          "type": "string",
          "align": "center",
          "label": "费率代码",
          "prop": "fee_cd"
        },
        {
          "type": "string",
          "align": "center",
          "label": "费率类型",
          "prop": "fee_type",
          "dict": {
            "dictKey": [
              "fee_aaa"
            ],
            "format": "value-label",
            "labelField": "drop_list_desc",
            "valueField": "drop_list_value",
            "doRequest": {
              "url": "/SUMP/iccsCall/iccsEurekaCall",
              "method": "POST",
              "params": {
                "servicecode": "ap1071",
                "drop_list_type": "fee_type_pl"
              }
            }
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "费率类别",
          "prop": "rate_type",
          "dict": {
            "dictKey": [
              "fee_bbb"
            ],
            "format": "value-label",
            "labelField": "drop_list_desc",
            "valueField": "drop_list_value",
            "doRequest": {
              "url": "/SUMP/iccsCall/iccsEurekaCall",
              "method": "POST",
              "params": {
                "servicecode": "ap1071",
                "drop_list_type": "RATE_TYPE_LOAN"
              }
            }
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "阶段释义",
          "prop": "stage",
          "dict": {
            "dictKey": [
              "fee_ccc"
            ],
            "format": "value-label",
            "labelField": "drop_list_desc",
            "valueField": "drop_list_value",
            "doRequest": {
              "url": "/SUMP/iccsCall/iccsEurekaCall",
              "method": "POST",
              "params": {
                "servicecode": "ap1071",
                "drop_list_type": "STAGE_LOAN"
              }
            }
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "本金费率(%)",
          "prop": "principal_rate",
          "format": {
            "decimal": 2,
            "decimalPoint": ".",
            "thousandsPoint": ","
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "利费费率(%)",
          "prop": "lev_rate",
          "format": {
            "decimal": 2,
            "decimalPoint": ".",
            "thousandsPoint": ","
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "手别",
          "prop": "hands"
        },
        {
          "label": "操作",
          "align": "center",
          "btns": [
            {
              "type": "look",
              "label": "详情",
              "toRequest": {
                "url": "/views/pl/paramManage/business/fee/fee_info.json",
                "method": "POST"
              },
              "props": {
                "title": "佣金参数详情"
              }
            },
            {
              "type": "edit",
              "label": "维护",
              "toRequest": {
                "url": "/views/pl/paramManage/business/fee/fee_modify.json",
                "method": "POST"
              },
              "props": {
                "title": "佣金参数维护"
              }
            },
            {
              "type": "delete",
              "label": "删除",
              "doRequest": {
                "url": "/SUMP/iccsCall/iccsEurekaCall",
                "method": "POST",
                "params": {
                  "servicecode": "cs0081"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
