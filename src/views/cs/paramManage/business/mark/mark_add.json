{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "cb0027",
                            "page_id": "/views/pl/paramManage/business/mark/mark_add.json",
                            "appId": "cbOnl"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "mark_code": {
                        "control": "text",
                        "label": "催收标识代码",
                        "width": "130",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            },
                            {
                                "max": 20,
                                "message": "最多可以输入 20 个字节"
                            }
                        ]
                    },
                    "mark_name": {
                        "control": "text",
                        "label": "催收标识名称",
                        "width": "130",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            }
                        ]
                    }
                },
                {
                    "is_valid": {
                        "control": "select",
                        "label": "是否启用",
                        "width": "130",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            }
                        ],
                        "dict": {
                            "dictType": "CS_E_YESORNO",
                            "dictKey": [
                                "CS_E_YESORNO"
                            ],
                            "format": "value-label"
                        }
                    }
                }
            ]
        }
    }
}
