{
    "module": "dynamicTabs",
    "layout": {
        "tabs": {
            "scope": "finaStat",
            "defaultActiveKey": "messageItem",
            "duty_type": "top",
            "styleClass": "card",
            "tabHeader": [{
                    "id": "messageItem",
                    "label": "短信",
                    "closable": false,
                    "disabled": false,
                    "toRequest": {
                        "url": "/views/cs/paramManage/callCenterCfg/shortMessage/short_message_list.json",
                        "params": {}
                    }
                },
                {
                    "id": "ivrItem",
                    "label": "IVR语音",
                    "closable": false,
                    "disabled": false,
                    "toRequest": {
                        "url": "/views/cs/paramManage/callCenterCfg/ivrVoice/ivr_voice_list.json",
                        "params": {}
                    }
                },
              {
                "id": "aiItem",
                "label": "智能语音",
                "icon": "el-icon-date",
                "closable": false,
                "disabled": false,
                "toRequest": {
                  "url": "/views/cs/paramManage/callCenterCfg/aiVoice/ai_voice_list.json",
                  "method": "post",
                  "params": {
                    "case_id": "${{params.case_id}}"
                  }
                }
              }
            ]
        }
    }
}
