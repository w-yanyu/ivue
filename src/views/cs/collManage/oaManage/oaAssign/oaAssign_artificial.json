{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "btns": [
                {
                    "type": "submit",
                    "icon": "el-icon-check",
                    "label": "确认",
                    "doRequest": {
                        "method": "POST",
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "params": {
                            "servicecode": "cb0213",
                            "case_id": "${{params.selectedData}}"
                        }
                    }
                },
                {
                    "type": "close",
                    "label": "取消"
                }
            ],
            "controls": [
                {
                    "oa_org_id": {
                        "hidden": true,
                        "label": "委外公司代码",
                        "control": "remoteSelect",
                        "labelField": "outsourcing_org_name",
                        "valueField": "outsourcing_org_id",
                        "doRequest": {
                            "url": "/SUMP/icmcall/icmRPCCall",
                            "method": "POST",
                            "params": {
                                "servicecode": "cb0052",
                                "length": 100,
                                "outsourcing_org_type": "1",
                                "org_id": "$g{{user_info.org_id}}"
                            }
                        },
                        "events": {
                            "emit": [
                                "operation"
                            ]
                        },
                        "format": "value-label"
                    },
                    "oa_cycle": {
                        "control": "inputNumber",
                        "label": "委外周期",
                        "width": "130",
                        "min": 1,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            },
                            {
                                "expression": "'${{form.oa_cycle}}'.length < 6",
                                "message": "最多可以输入 5 个字节"
                            }
                        ]
                    },
                    "oa_cycle_unit": {
                        "control": "select",
                        "label": "委外周期单位",
                        "selectFirstValue": false,
                        "dict": {
                            "dictType": "CS_OA_CYCLE_UNIT",
                            "dictKey": [
                                "CS_OA_CYCLE_UNIT"
                            ],
                            "format": "value-label"
                        }
                    },
                    "commission_plan_id": {
                        "control": "text",
                        "label": "佣金方案编号",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            },
                            {
                                "max": 30,
                                "message": "最多可以输入 30 个字节"
                            }
                        ]
                    }
                }
            ]
        }
    }
}