{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
          "foldLineNumber": 3,
            "scope": "formData",
            "controls": [
                {
                    "branch_id": {
                        "control": "remoteSelect",
                        "label": "机构",
                        "labelField": "branch_name",
                        "valueField": "branch_id",
                        "doRequest": {
                          "url": "/SUMP/icmcall/icmRPCCall",
                          "method": "POST",
                          "params": {
                            "servicecode": "ct2142",
                            "user_id": "$g{{user_info.userCode}}",
                            "brch_relation_code": "01"
                          }
                        },
                        "format": "value-label"
                      },
                    "cust_name": {
                        "control": "text",
                        "label": "客户姓名"
                    }
                },
                {
                    "id_no": {
                        "control": "text",
                        "label": "证件号码"
                    },
                    "cust_id": {
                        "control": "text",
                        "label": "客户编号"
                    }
                },
                {
                    "function_cd": {
                        "control": "remoteSelect",
                        "label": "逾期阶段",
                        "labelField": "stage_name",
                        "valueField": "stage_code",
                        "doRequest": {
                          "url": "/SUMP/icmcall/icmRPCCall",
                          "method": "POST",
                          "params": {
                            "servicecode": "cb0083"
                          }
                        },
                        "format": "value-label"
                      }
                },
                {
                    "start_date": {
                        "control": "inputNumber",
                        "label": "逾期天数开始值"
                    },
                    "end_date": {
                        "control": "inputNumber",
                        "label": "逾期天数结束值"
                    }
                },
                {
                    "queue_cd": {
                        "control": "remoteSelect",
                        "label": "队列名称",
                        "labelField": "queue_name",
                        "valueField": "queue_code",
                        "doRequest": {
                          "url": "/SUMP/icmcall/icmRPCCall",
                          "method": "POST",
                          "params": {
                            "servicecode": "cb0082"
                          }
                        },
                        "format": "value-label"
                      },
                    "status_cd": {
                        "control": "select",
                        "label": "案件状态",
                        "dict": {
                          "dictType": "CS_E_STATUS_CD",
                          "dictKey": [
                            "CS_E_STATUS_CD"
                          ],
                          "format": "value-label"
                        }
                      }
                },
                {
                    "oa_hands": {
                        "control": "inputNumber",
                        "label": "手別"
                    },
                    "location_cd": {
                        "control": "select",
                        "label": "案件类型",
                        "dict": {
                            "dictType": "CS_E_LOCATION_CD",
                            "dictKey": [
                              "CS_E_LOCATION_CD"
                            ],
                            "format": "value-label"
                          }
                    }
                },
                {
                    "ovd_amt_total_1": {
                        "control": "inputNumber",
                        "label": "欠款金额区间开始值"
                    },
                    "ovd_amt_total_2": {
                        "control": "inputNumber",
                        "label": "欠款金额区间结束值"
                    }
                },
                {
                    "collect_date_1": {
                        "control": "inputNumber",
                        "label": "入催天数区间开始值"
                    },
                    "collect_date_2": {
                        "control": "inputNumber",
                        "label": "入催天数区间结束值"
                    }
                }
            ]
        },
        "toolbar": {
            "btns": [
                {
                    "type": "custom",
                    "icon": "el-icon-edit",
                    "target": "dialog",
                    "checkCountMin":1,
                    "label": "人工分配",
                    "toRequest": {
                        "method": "GET",
                        "url": "/views/cs/collManage/oaManage/oaAssign/oaAssign_artificial.json"
                    }
                },
                {
                    "type": "custom",
                    "icon": "el-icon-bell",
                    "target": "dialog",
                    "checkCountMin":1,
                    "label": "自动分配",
                    "toRequest": {
                        "method": "GET",
                        "url": "/views/cs/collManage/oaManage/oaAssign/oaAssign_auto.json"
                    }
                },
                {
                    "type": "delete",
                    "icon": "el-icon-back",
                    "checkCountMin":1,
                    "label": "委外退案",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "cb0216",
                            "case_id": ""
                        }
                    },
                    "confirm": {
                        "title": "委外退案提醒",
                        "msg": "确定将案件退案，退出委外队列？",
                        "confirmButtonText": "确定",
                        "cancelButtonText": "取消"
                    }
                }
            ]
        },

        "datagrid": {
            "isInitSearch": false,
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                  "servicecode": "cb0212",
                  "strategy_type": "OT",
                  "appId": "cbOnl"
              }
            },
            "thead": [
                {
                    "type": "selection",
                    "align": "center",
                    "width": "55px",
                    "fixed": "left"
                },
                {
                    "type": "string",
                    "align": "center",
                    "label": "机构",
                    "prop": "branch_id"
                  },
                {
                    "type": "string",
                    "align": "center",
                    "width": "180px",
                    "label": "客户编号",
                    "prop": "cust_id"

                },
                {
                    "type": "string",
                    "align": "center",
                    "width": "180px",
                    "label": "客户姓名",
                    "prop": "cust_name"
                },
                {
                    "type": "string",
                    "align": "center",
                    "label": "证件类型",
                    "value": "身份证",
                    "prop": "cert_type",
                    "dict": {
                        "dictType": "CS_E_CERT_TYPE",
                        "dictKey": [
                          "CS_E_CERT_TYPE"
                        ],
                        "format": "value-label"
                      }
                  },
                {
                    "type": "string",
                    "align": "center",
                    "width": "180px",
                    "label": "证件号码",
                    "prop": "cert_no"
                },
                {
                    "type": "string",
                    "align": "center",
                    "label": "逾期阶段",
                    "prop": "stage_code"
                },
                {
                    "type": "string",
                    "align": "center",
                    "label": "逾期金额",
                    "prop": "overdue_amount"
                },
                {
                    "type": "string",
                    "align": "center",
                    "label": "逾期天数",
                    "prop": "overdue_days"
                },
                {
                    "type": "string",
                    "align": "center",
                    "label": "手别",
                    "prop": "case_hands"
                },
                {
                    "type": "string",
                    "align": "center",
                    "label": "案件分类",
                    "prop": "classify_code",
                    "dict": {
                        "dictType": "CS_E_LOCATION_CD",
                        "dictKey": [
                          "CS_E_LOCATION_CD"
                        ],
                        "format": "value-label"
                      }
                },
                {
                    "type": "string",
                    "align": "center",
                    "label": "案件状态",
                    "prop": "case_status",
                    "dict": {
                        "dictType": "CS_E_STATUS_CD",
                        "dictKey": [
                          "CS_E_STATUS_CD"
                        ],
                        "format": "value-label"
                      }
                  },
                  {
                    "type": "string",
                    "align": "center",
                    "width": "180px",
                    "label": "队列名称",
                    "prop": "queue_code"
                  },
                {
                  "type": "string",
                  "align": " center",
                  "label": "入催日期",
                  "prop": "collect_date"
                },
                {
                    "type": "string",
                    "align": " center",
                    "label": "案件编号",
                    "prop": "case_id"
                  }
            ]
        }
    }
}
