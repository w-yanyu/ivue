{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "foldLineNumber": 3,
            "scope": "formData",
            "controls": [
                {
                    "branch_id": {
                        "control": "remoteSelect",
                        "label": "机构",
                        "labelField": "branch_name",
                        "valueField": "branch_id",
                        "doRequest": {
                          "url": "/SUMP/icmcall/icmRPCCall",
                          "method": "POST",
                          "params": {
                            "servicecode": "ct2142",
                            "user_id": "$g{{user_info.userCode}}",
                            "brch_relation_code": "01"
                          }
                        },
                        "format": "value-label"
                      },
                    "cust_name": {
                        "control": "text",
                        "label": "客户姓名"
                    }
                },
                {
                    "cert_no": {
                        "control": "text",
                        "label": "证件号码"
                    },
                    "oa_org_id": {
                        "hidden": true,
                        "label": "委外公司代码",
                        "disabled": true,
                        "width": "130px",
                        "control": "remoteSelect",
                        "labelField": "outsourcing_org_name",
                        "valueField": "outsourcing_org_id",
                        "doRequest": {
                          "url": "/SUMP/icmcall/icmRPCCall",
                          "method": "POST",
                          "params": {
                            "servicecode": "cb0052",
                            "org_id": "$g{{user_info.org_id}}"
                          }
                        },
                        "events": {
                          "emit": [
                            "operation"
                          ]
                        },
                        "format": "value-label"
                      }
                },
                {
                    "daterange": {
                        "control": "dateTimePicker",
                        "label": "委外日期",
                        "type": "daterange",
                        "unlinkPanels": true,
                        "valueFormat": "yyyyMMdd",
                        "startPlaceholder": "请选择开始时间",
                        "endPlaceholder": "请选择结束时间",
                        "startField":"oa_begin_date",
                        "endField":"oa_end_date"
                    },
                    "oa_hands": {
                        "control": "inputNumber",
                        "label": "手别"
                    }
                },
                {
                    "start_day": {
                        "control": "inputNumber",
                        "label": "逾期天数开始值"
                    },
                    "end_day": {
                        "control": "inputNumber",
                        "label": "逾期天数结束值"
                    }
                },
                {
                    "stage_code": {
                        "control": "remoteSelect",
                        "label": "逾期阶段",
                        "labelField": "stage_name",
                        "valueField": "stage_code",
                        "doRequest": {
                          "url": "/SUMP/icmcall/icmRPCCall",
                          "method": "POST",
                          "params": {
                            "servicecode": "cb0083"
                          }
                        },
                        "format": "value-label"
                      }
                }
            ]
        },
        "toolbar": {
            "btns": [
                {
                    "type": "export",
                    "icon": "iconfont icon-daochu",
                    "target": "dialog",
                    "label": "导出",
                    "doRequest": {
                        "method": "POST",
                        "url": "/SUMP/rest/file/export",
                        "params": {
                            "servicecode":"###"
                        }
                    },
                    "confirm":{
                      "title":"提示信息",
                      "msg":"确认导出吗？",
                      "confirmButtonText":"确定",
                      "cancelButtonText":"取消"
                    }
                }
            ]
        },
        "datagrid": {
            "isInitSearch": false,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "cb0042"
                }
            },
            "thead": [
                {
                    "type": "string",
                    "align": "center",
                    "label": "机构",
                    "width": "180px",
                    "prop": "branch_id"
                  },
                {
                    "type":"string",
                    "align":"center",
                     "width": "150",
                    "label": "交易流水",
                    "prop": "jioyls"
                },
                {
                    "type":"string",
                    "align":"center",
                     "width": "150",
                    "label": "案件编号",
                    "prop": "case_id"
                },
                {
                    "type":"string",
                    "align":"center",
                     "width": "150",
                    "label": "客户姓名",
                    "prop": "cust_name"
                },
                {
                    "type":"string",
                    "align":"center",
                     "width": "150",
                    "label": "借据号",
                    "prop": "loan_no"
                },
                {
                    "type": "string",
                    "align": "center",
                    "label": "证件类型",
                    "width": "150",
                    "prop": "cert_type",
                    "dict": {
                      "dictType": "CS_E_CERT_TYPE",
                      "dictKey": [
                        "CS_E_CERT_TYPE"
                      ],
                      "format": "value-label"
                    }
                  },
                {
                    "type":"string",
                    "align":"center",
                     "width": "150",
                    "label": "证件号码",
                    "prop": "cert_no"
                },
                {
                    "type": "string",
                    "align": "center",
                    "width": "150",
                    "label": "委外公司",
                    "prop": "oa_org_id",
                    "dict": {
                      "dictKey": [
                        "oa_org_id"
                      ],
                      "format": "value-label",
                      "labelField": "outsourcing_org_name",
                      "valueField": "outsourcing_org_id",
                      "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                          "servicecode": "cb0052",
                          "org_id": "$g{{user_info.org_id}}"
                        }
                      }
                    }
                  },
                {
                    "type":"string",
                    "align":"center",
                     "width": "150",
                    "label": "委外开始日期",
                    "prop": "oa_begin_date"
                },
                {
                    "type":"string",
                    "align":"center",
                     "width": "150",
                    "label": "委外结束日期",
                    "prop": "oa_end_date"
                },
                {
                    "type":"string",
                    "align":"center",
                     "width": "150",
                    "label": "实际结束日期",
                    "prop": "real_end_date"
                },
                {
                    "type":"string",
                    "align":"center",
                     "width": "150",
                    "label": "委外金额",
                    "prop": "oa_amt"
                },
                {
                    "type":"string",
                    "align":"center",
                     "width": "150",
                    "label": "交易日期",
                    "prop": "jioyrq"
                },
                {
                    "type":"string",
                    "align":"center",
                     "width": "150",
                    "label": "有效委外回收金额",
                    "prop": "pmt_amt_without_exce"
                },
                {
                    "type":"string",
                    "align":"center",
                     "width": "150",
                    "label": "主要担保类型",
                    "prop": "vouchtype"
                },
                {
                    "type":"string",
                    "align":"center",
                     "width": "150",
                    "label": "逾期天数",
                    "prop": "over_due_day"
                },
                {
                    "type":"string",
                    "align":"center",
                     "width": "150",
                    "label": "费率(%)",
                    "prop": "principal_rate"
                },
                {
                    "type":"string",
                    "align":"center",
                     "width": "150",
                    "label": "佣金",
                    "prop": "real_comm"
                }
            ]
        }
    }
}
