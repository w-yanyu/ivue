{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "scope":"formData",
            "initRequest": "${{params}}",
            "btns": [
                {
                    "type": "submit",
                    "label": "确认",
                    "doRequest": {
                        "method": "POST",
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "params": {
                            "servicecode":"###",
                            "branch_id":"${{params.branch_id}}",
                            "cust_name":"${{params.cust_name}}",
                            "cert_no":"${{params.cert_no}}",
                            "cust_id":"${{params.cust_id}}",
                            "loan_no":"${{params.loan_no}}",
                            "check_status":"${{params.check_status}}",
                            "daterange":"${{params.daterange}}",
                            "oa_status":"${{params.oa_status}}",
                            "queue_code":"${{params.queue_code}}",
                            "location_cd":"${{params.location_cd}}",
                            "case_status":"${{params.case_status}}",
                            "stage_code":"${{params.stage_code}}",
                            "ovd_amt_total_1":"${{params.ovd_amt_total_1}}",
                            "ovd_amt_total_2":"${{params.ovd_amt_total_2}}",
                            "collect_date_1":"${{params.collect_date_1}}",
                            "collect_date_2":"${{params.collect_date_2}}"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "check_desc": {
                        "control": "textarea",
                        "label": "审核说明",
                        "width": "180px",
                        "align": "center",
                        "span": "20",
                        "rows": 6,
                        "placeholder":"请输入",
                        "rules": [
                            {
                                "required": true,
                                "message":"请输入"
                            }
                        ]
                    }
                }
            ]
        }
    }
}
