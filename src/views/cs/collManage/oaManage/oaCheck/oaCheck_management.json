{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "foldLineNumber": 3,
            "scope": "formData",
            "controls": [
                {
                    "branch_id": {
                        "control": "remoteSelect",
                        "label": "机构",
                        "labelField": "branch_name",
                        "valueField": "branch_id",
                        "doRequest": {
                          "url": "/SUMP/icmcall/icmRPCCall",
                          "method": "POST",
                          "params": {
                            "servicecode": "ct2142",
                            "user_id": "$g{{user_info.userCode}}",
                            "brch_relation_code": "01"
                          }
                        },
                        "format": "value-label"
                      },
                    "cust_name": {
                        "control": "text",
                        "label": "客户姓名"
                    }
                },
                {
                    "id_no": {
                        "control": "text",
                        "label": "证件号码"
                    },
                    "cust_id": {
                        "control": "text",
                        "label": "客户编号"
                    }
                },
                {
                    "check_status": {
                        "control": "select",
                        "label": "审核状态",
                        "default": "NC",
                        "dict": {
                            "dictType": "CS_E_CHECK_STATUS",
                            "dictKey": [
                              "CS_E_CHECK_STATUS"
                            ],
                            "format": "value-label"
                          }
                    },
                    "oa_status": {
                        "control": "select",
                        "label": "委外状态",
                        "dict": {
                          "dictType": "CS_E_OA_STATUS",
                          "dictKey": [
                            "CS_E_OA_STATUS"
                          ],
                          "format": "value-label"
                        }
                      }
                },
                {
                    "queue_cd": {
                        "control": "remoteSelect",
                        "label": "队列名称",
                        "labelField": "queue_name",
                        "valueField": "queue_code",
                        "doRequest": {
                          "url": "/SUMP/icmcall/icmRPCCall",
                          "method": "POST",
                          "params": {
                            "servicecode": "cb0082"
                          }
                        },
                        "format": "value-label"
                      },
                    "location_cd": {
                        "control": "select",
                        "label": "案件类型",
                        "dict": {
                          "dictType": "CS_E_LOCATION_CD",
                          "dictKey": [
                            "CS_E_LOCATION_CD"
                          ],
                          "format": "value-label"
                        }
                      }
                },
                {
                    "status_cd": {
                        "control": "select",
                        "label": "案件状态",
                        "dict": {
                          "dictType": "CS_E_STATUS_CD",
                          "dictKey": [
                            "CS_E_STATUS_CD"
                          ],
                          "format": "value-label"
                        }
                      },
                      "function_cd": {
                        "control": "remoteSelect",
                        "label": "逾期阶段",
                        "labelField": "stage_name",
                        "valueField": "stage_code",
                        "doRequest": {
                          "url": "/SUMP/icmcall/icmRPCCall",
                          "method": "POST",
                          "params": {
                            "servicecode": "cb0083"
                          }
                        },
                        "format": "value-label"
                      }
                },
                {
                    "ovd_amt_total_1": {
                        "control": "currency",
                        "label": "欠款金额区间开始值"
                    },
                    "ovd_amt_total_2": {
                        "control": "currency",
                        "label": "欠款金额区间结束值"
                    }
                },
                {
                    "collect_date_1": {
                        "control": "inputNumber",
                        "label": "入催天数区间开始值"
                    },
                    "collect_date_2": {
                        "control": "inputNumber",
                        "label": "入催天数区间结束值"
                    }
                },
                {
                    "daterange": {
                        "control": "dateTimePicker",
                        "label": "申请时间",
                        "type": "daterange",
                        "unlinkPanels": true,
                        "valueFormat": "yyyyMMdd",
                        "startPlaceholder": "请选择开始时间",
                        "endPlaceholder": "请选择结束时间",
                        "startField": "start_date",
                        "endField": "end_date"
                    }
                }
            ]
        },
        "toolbar": {
            "btns": [
                {
                    "type": "add",
                    "icon": "el-icon-edit",
                    "target": "dialog",
                    "checkCountMin": 1,
                    "label": "批量审核",
                    "toRequest": {
                        "method": "GET",
                        "url": "/views/cs/collManage/oaManage/oaCheck/oaCheck_auditing.json"
                    }
                },
                {
                    "type": "add",
                    "icon": "el-icon-circle-check",
                    "target": "dialog",
                    "label": "全部核准",
                    "toRequest": {
                        "method": "GET",
                        "url": "/views/cs/collManage/oaManage/oaCheck/oaCheck_all_through.json",
                        "params": {
                            "branch_id":"${{form.branch_id}}",
                            "cust_name":"${{form.cust_name}}",
                            "id_no":"${{form.id_no}}",
                            "cust_id":"${{form.cust_id}}",
                            "duebill_no":"${{form.duebill_no}}",
                            "check_status":"NC",
                            "daterange":"${{form.daterange}}",
                            "oa_status":"${{form.oa_status}}",
                            "queue_cd":"${{form.queue_cd}}",
                            "location_cd":"${{form.location_cd}}",
                            "status_cd":"${{form.status_cd}}",
                            "function_cd":"${{form.function_cd}}",
                            "ovd_amt_total_1":"${{form.ovd_amt_total_1}}",
                            "ovd_amt_total_2":"${{form.ovd_amt_total_2}}",
                            "collect_date_1":"${{form.collect_date_1}}",
                            "collect_date_2":"${{form.collect_date_2}}"
                        }
                    }
                },
                {
                    "type": "add",
                    "icon": "el-icon-circle-close",
                    "target": "dialog",
                    "label": "全部拒绝",
                    "toRequest": {
                        "method": "GET",
                        "url": "/views/cs/collManage/oaManage/oaCheck/oaCheck_all_rejected.json",
                        "params": {
                            "branch_id":"${{form.branch_id}}",
                            "cust_name":"${{form.cust_name}}",
                            "id_no":"${{form.id_no}}",
                            "cust_id":"${{form.cust_id}}",
                            "duebill_no":"${{form.duebill_no}}",
                            "check_status":"NC",
                            "daterange":"${{form.daterange}}",
                            "oa_status":"${{form.oa_status}}",
                            "queue_cd":"${{form.queue_cd}}",
                            "location_cd":"${{form.location_cd}}",
                            "status_cd":"${{form.status_cd}}",
                            "function_cd":"${{form.function_cd}}",
                            "ovd_amt_total_1":"${{form.ovd_amt_total_1}}",
                            "ovd_amt_total_2":"${{form.ovd_amt_total_2}}",
                            "collect_date_1":"${{form.collect_date_1}}",
                            "collect_date_2":"${{form.collect_date_2}}"
                        }
                    }
                }
            ]
        },
        "datagrid": {
            "isInitSearch": false,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "cb0208",
                    "strategy_type": "OT",
                    "appId": "cbOnl"
                }
            },
            "thead": [
                {
                    "type": "selection",
                    "align": "center",
                    "width": "55",
                    "fixed": "left",
                    "show": {
                        "expression": "'${{check_status}}' === 'NC'"
                    }
                },
                {
                    "type": "string",
                    "align": "center",
                    "label": "机构",
                    "prop": "branch_id"
                },
                {
                    "type": "string",
                    "align": "center",
                    "width": "160px",
                    "label": "案件编号",
                    "prop": "case_id"
                },
                {
                    "type": "string",
                    "align": "center",
                    "width": "160px",
                    "label": "客户编号",
                    "prop": "cust_id"
                },
                {
                    "type": "string",
                    "align": "center",
                    "width": "130px",
                    "label": "客户姓名",
                    "prop": "cust_name"
                },
                {
                    "type": "string",
                    "align": "center",
                    "width": "130px",
                    "label": "证件类型",
                    "prop": "cert_type",
                    "dict": {
                        "dictType": "CS_E_CERT_TYPE",
                        "dictKey": [
                          "CS_E_CERT_TYPE"
                        ],
                        "format": "value-label"
                      }
                  },
                {
                    "type": "string",
                    "align": "center",
                    "width": "180px",
                    "label": "证件号码",
                    "prop": "cert_no"
                },
                {
                    "type": "string",
                    "align": "center",
                    "width": "100px",
                    "label": "委外状态",
                    "prop": "oa_status",
                    "dict": {
                        "dictType": "CS_E_OA_STATUS",
                        "dictKey": [
                          "CS_E_OA_STATUS"
                        ],
                        "format": "value-label"
                      }
                },
                {
                    "type": "string",
                    "align": "center",
                    "label": "手别",
                    "prop": "oa_hands"
                },
                {
                    "type": "string",
                    "align": "center",
                    "label": "展期次数",
                    "prop": "extend_times"
                },
                {
                    "type": "string",
                    "align": "center",
                    "label": "申请人",
                    "prop": "apply_user",
                    "dict": {
                        "dictKey": [
                            "apply_user"
                        ],
                        "format": "value-label",
                        "labelField": "user_name",
                        "valueField": "user_name",
                        "doRequest": {
                            "url": "/SUMP/iccsCall/iccsEurekaCall",
                            "method": "POST",
                            "params": {
                                "servicecode": "pl0403"
                            }
                        }
                    }
                },
                {
                    "type": "string",
                    "align": "center",
                    "label": "申请日期",
                    "prop": "apply_time"
                },
                {
                    "type": "string",
                    "align": "center",
                    "label": "申请说明",
                    "prop": "apply_reason"
                },
                {
                    "type": "string",
                    "align": "center",
                    "label": "审核状态",
                    "prop": "check_status",
                    "dict": {
                        "dictType": "CS_E_CHECK_STATUS",
                        "dictKey": [
                          "CS_E_CHECK_STATUS"
                        ],
                        "format": "value-label"
                      }
                },
                {
                    "type": "string",
                    "align": "center",
                    "label": "审核人",
                    "prop": "check_user"
                },
                {
                    "type": "string",
                    "align": "center",
                    "label": "审核日期",
                    "prop": "check_time"
                },
                {
                    "type": "string",
                    "align": "center",
                    "label": "审核说明",
                    "prop": "check_desc"
                },
                {
                    "label": "操作",
                    "align": "center",
                    "fixed": "right",
                    "width": "130px",
                    "btns": [
                        {
                            "type": "custom",
                            "label": "案件详情",
                            "toRequest": {
                                "url": "/views/cs/assistManage/caseDetail/caseDetail_1/case_detail_main.json",
                                "method": "POST",
                                "params": {
                                    "case_id": "${{current.case_id}}"
                                }
                            },
                            "props": {
                                "width": "80%",
                                "title": "案件详情"
                            }
                        }
                    ]
                }
            ]
        }
    }
}