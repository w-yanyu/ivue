{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "scope": "formData",
            "controls": [
                {
                    "cust_name": {
                        "control": "text",
                        "label": "客户姓名"
                    },
                    "oa_org_id": {
                        "hidden": true,
                        "label": "委外公司代码",
                        "disabled": false,
                        "width": "130px",
                        "control": "remoteSelect",
                        "labelField": "outsourcing_org_name",
                        "valueField": "outsourcing_org_id",
                        "doRequest": {
                          "url": "/SUMP/icmcall/icmRPCCall",
                          "method": "POST",
                          "params": {
                            "servicecode": "cb0052",
                            "org_id": "$g{{user_info.org_id}}"
                          }
                        },
                        "events": {
                          "emit": [
                            "operation"
                          ]
                        },
                        "format": "value-label"
                      }
                }
            ]
        },
        "toolbar": {
            "btns": [
                {
                    "type": "custom",
                    "icon": "el-icon-back",
                    "checkCountMin":1,
                    "label": "退件",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "cb0219",
                            "case_id": ""
                        }
                    },
                    "confirm": {
                        "title": "委外退件",
                        "msg": "确定将案件进行退件，重新分配吗？",
                        "confirmButtonText": "确定",
                        "cancelButtonText": "取消"
                    }
                },
                {
                    "type": "custom",
                    "icon": "el-icon-edit",
                    "target": "dialog",
                    "checkCountMin":1,
                    "label": "修改",
                    "toRequest": {
                        "method": "GET",
                        "url": "/views/cs/collManage/oaManage/oaConfirm/oaConfirm_modify.json"
                    }
                },
                {
                    "type": "custom",
                    "icon": "el-icon-check",
                    "label": "确认分配",
                    "checkCountMin":1,
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "post",
                        "params": {
                            "servicecode": "cb0221",
                            "case_id": ""
                        }
                    },
                  "confirm": {
                    "title": "提示信息",
                    "msg": "确定分配吗？",
                    "confirmButtonText": "确定",
                    "cancelButtonText": "取消"
                  }
                }
            ]
        },

        "datagrid": {
            "isInitSearch": false,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "cb0218",
                    "strategy_type": "OT",
                    "appId": "cbOnl"
                }
            },
            "thead": [
                {
                    "type": "selection",
                    "align":"center",
                    "width": "55",
                    "fixed": "left"
                },
                {
                    "type":"string",
                    "align":"center",
                    "width": "100",
                    "label": "客户姓名",
                    "prop": "cust_name"
                },
                {
                    "type":"string",
                    "align":"center",
                    "width": "180",
                    "label": "客户编号",
                    "prop": "cust_id"

                },
                {
                    "type":"string",
                    "align":"center",
                    "width": "180",
                    "label": "证件号码",
                    "prop": "cert_no"
                },
                {
                    "type":"string",
                    "align":"center",
                    "width": "100",
                    "label": "逾期天数",
                    "prop": "over_due_day"
                },
                {
                    "type":"string",
                    "align":"center",
                    "width": "100",
                    "label": "逾期金额",
                    "prop": "over_due_amt"
                },
                {
                    "type": "string",
                    "align": "center",
                    "width": "100",
                    "label": "委外公司",
                    "prop": "oa_org_id"
                  },
                {
                    "type":"string",
                    "align":"center",
                    "width": "100",
                    "label": "委外金额",
                    "prop": "oa_amt"

                },
                {
                    "type":"string",
                    "align":"center",
                    "width": "150",
                    "label": "派案日期",
                    "prop": "oa_begin_date"

                },
                {
                    "type":"string",
                    "align":"center",
                    "width": "150",
                    "label": "回收日期",
                    "prop": "oa_end_date"

                },
                {
                    "type":"string",
                    "align":"center",
                    "width": "150",
                    "label": "上一次公司名称",
                    "prop": "last_oa_org_id"

                },
                {
                    "type":"string",
                    "align":"center",
                    "width": "150",
                    "label": "上一次派案日期",
                    "prop": "last_oa_begin_date"

                },
                {
                    "type":"string",
                    "align":"center",
                    "width": "150",
                    "label": "上一次回收日期",
                    "prop": "last_real_end_date"

                }
            ]
        }
    }
}
