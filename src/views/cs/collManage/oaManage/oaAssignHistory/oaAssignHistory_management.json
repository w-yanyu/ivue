{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "foldLineNumber": 3,
            "scope": "formData",
            "controls": [
                {
                    "branch_id": {
                        "control": "remoteSelect",
                        "label": "机构",
                        "labelField": "branch_name",
                        "valueField": "branch_id",
                        "doRequest": {
                          "url": "/SUMP/icmcall/icmRPCCall",
                          "method": "POST",
                          "params": {
                            "servicecode": "ct2142",
                            "user_id": "$g{{user_info.userCode}}",
                            "brch_relation_code": "01"
                          }
                        },
                        "format": "value-label"
                      },
                    "cust_name": {
                        "control": "text",
                        "label": "客户姓名"
                    }
                },
                {
                    "cert_no": {
                        "control": "text",
                        "label": "证件号码"
                    },
                    "oa_status": {
                        "control": "select",
                        "label": "委外状态",
                        "dict": {
                          "dictType": "CS_E_OA_STATUS",
                          "dictKey": [
                            "CS_E_OA_STATUS"
                          ],
                          "format": "value-label"
                        }
                      }
                },
                {
                    "oa_begin_date": {
                        "control": "dateTimePicker",
                        "label": "派案日期",
                        "unlinkPanels": false,
                        "valueFormat": "yyyyMMdd"
                    },
                    "oa_end_date": {
                        "control": "dateTimePicker",
                        "label": "回收日期",
                        "unlinkPanels": false,
                        "valueFormat": "yyyyMMdd"
                    }
                },
                {
                    "oa_hands": {
                        "control": "inputNumber",
                        "label": "手别"
                    },
                    "stage_code": {
                        "control": "remoteSelect",
                        "label": "逾期阶段",
                        "labelField": "stage_name",
                        "valueField": "stage_code",
                        "doRequest": {
                          "url": "/SUMP/icmcall/icmRPCCall",
                          "method": "POST",
                          "params": {
                            "servicecode": "cb0083"
                          }
                        },
                        "format": "value-label"
                      }
                },
                {
                    "oa_org_id": {
                        "hidden": true,
                        "label": "委外公司代码",
                        "disabled": true,
                        "width": "130px",
                        "control": "remoteSelect",
                        "labelField": "outsourcing_org_name",
                        "valueField": "outsourcing_org_id",
                        "doRequest": {
                          "url": "/SUMP/icmcall/icmRPCCall",
                          "method": "POST",
                          "params": {
                            "servicecode": "cb0052",
                            "org_id": "$g{{user_info.org_id}}"
                          }
                        },
                        "events": {
                          "emit": [
                            "operation"
                          ]
                        },
                        "format": "value-label"
                      }
                }
            ]
        },
        "datagrid": {
            "isInitSearch": false,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "cb0042"
                }
            },
            "thead": [
                {
                    "type": "string",
                    "align": "center",
                    "label": "机构",
                    "width": "180px",
                    "prop": "branch_id"
                },
                {
                    "type": "string",
                    "align": "center",
                    "width": "150",
                    "label": "案件编号",
                    "prop": "case_id"
                },
                {
                    "type": "string",
                    "align": "center",
                    "width": "150",
                    "label": "客户姓名",
                    "prop": "cust_name"
                },
                {
                    "type": "string",
                    "align": "center",
                    "width": "180",
                    "label": "证件号码",
                    "prop": "cert_no"
                },
                {
                    "type": "string",
                    "align": "center",
                    "width": "150",
                    "label": "批次",
                    "prop": "series_num"
                },
                {
                    "type": "string",
                    "align": "center",
                    "width": "150",
                    "label": "手别",
                    "prop": "oa_hands"
                },
                {
                    "type": "string",
                    "align": "center",
                    "width": "150",
                    "label": "委外公司",
                    "prop": "oa_org_id",
                    "dict": {
                        "dictKey": [
                          "oa_org_id"
                        ],
                        "format": "value-label",
                        "labelField": "outsourcing_org_name",
                        "valueField": "outsourcing_org_id",
                        "doRequest": {
                          "url": "/SUMP/icmcall/icmRPCCall",
                          "method": "POST",
                          "params": {
                            "servicecode": "cb0052",
                            "org_id": "$g{{user_info.org_id}}"
                          }
                        }
                      }
                },
                {
                    "type": "string",
                    "align": "center",
                    "width": "150",
                    "label": "委外金额",
                    "prop": "oa_amt"
                },
                {
                    "type": "string",
                    "align": "center",
                    "width": "150",
                    "label": "派案日期",
                    "prop": "oa_begin_date"
                },
                {
                    "type": "string",
                    "align": "center",
                    "width": "150",
                    "label": "回收日期",
                    "prop": "oa_end_date"
                },
                {
                    "type": "string",
                    "align": "center",
                    "width": "150",
                    "label": "实际结束时间",
                    "prop": "real_end_date"
                },
                {
                    "type": "string",
                    "align": "center",
                    "width": "150",
                    "label": "委外状态",
                    "prop": "oa_status",
                    "dict": {
                        "dictType": "CS_E_OA_STATUS",
                        "dictKey": [
                          "CS_E_OA_STATUS"
                        ],
                        "format": "value-label"
                      }
                },
                {
                    "type": "string",
                    "align": "center",
                    "width": "150",
                    "label": "案件状态",
                    "prop": "case_status",
                    "dict": {
                      "dictType": "CS_E_STATUS_CD",
                      "dictKey": [
                        "CS_E_STATUS_CD"
                      ],
                      "format": "value-label"
                    }
                  },
                {
                    "label": "操作",
                    "align": "center",
                    "fixed": "right",
                    "width": "130px",
                    "btns": [
                        {
                            "type": "edit",
                            "label": "案件详情",
                            "toRequest": {
                                "url": "/views/cs/assistManage/caseDetail/caseDetail_2/case_detail_main.json",
                                "method": "POST",
                                "params": {
                                    "case_id": "${{current.case_id}}"
                                }
                            },
                            "props": {
                                "width": "80%",
                                "title": "案件详情"
                            }
                        }
                    ]
                }
            ]
        }
    }
}