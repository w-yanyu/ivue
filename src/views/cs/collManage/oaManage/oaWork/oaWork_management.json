{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "foldLineNumber": 3,
      "scope": "formData",
      "controls": [
        {
          "branch_id": {
            "control": "remoteSelect",
            "label": "机构",
            "labelField": "branch_name",
            "valueField": "branch_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2142",
                "user_id": "$g{{user_info.userCode}}",
                "brch_relation_code": "01"
              }
            },
            "format": "value-label"
          },
          "cust_name": {
            "control": "text",
            "label": "客户姓名"
          }
        },
        {
          "cert_no": {
            "control": "text",
            "label": "证件号码"
          },
          "cust_id": {
            "control": "text",
            "label": "客户编号"
          }
        },
        {
          "oa_hands": {
            "control": "inputNumber",
            "label": "手别"
          }
        },
        {
          "daterange": {
            "control": "dateTimePicker",
            "label": "委外日期",
            "type": "daterange",
            "unlinkPanels": true,
            "valueFormat": "yyyyMMdd",
            "startPlaceholder": "请选择开始时间",
            "endPlaceholder": "请选择结束时间",
            "startField": "oa_begin_date",
            "endField": "oa_end_date"
          },
          "oa_org_id": {
            "hidden": true,
            "label": "委外公司代码",
            "disabled": false,
            "control": "remoteSelect",
            "labelField": "outsourcing_org_name",
            "valueField": "outsourcing_org_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0052",
                "org_id": "$g{{user_info.org_id}}"
              }
            },
            "events": {
              "emit": [
                "operation"
              ]
            },
            "format": "value-label"
          }
        },
        {
          "start_day": {
            "control": "inputNumber",
            "label": "逾期天数开始值"
          },
          "end_day": {
            "control": "inputNumber",
            "label": "逾期天数结束值"
          }
        },
        {
          "oa_status": {
            "control": "select",
            "label": "委外状态",
            "dict": {
              "dictType": "CS_E_OA_STATUS",
              "dictKey": [
                "CS_E_OA_STATUS"
              ],
              "format": "value-label"
            }
          },
          "queue_code": {
            "control": "remoteSelect",
            "label": "队列名称",
            "labelField": "queue_name",
            "valueField": "queue_code",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0082"
              }
            },
            "format": "value-label"
          }
        },
        {
          "case_status": {
            "control": "select",
            "label": "案件状态",
            "dict": {
              "dictType": "CS_E_STATUS_CD",
              "dictKey": [
                "CS_E_STATUS_CD"
              ],
              "format": "value-label"
            }
          },
          "location_cd": {
            "control": "select",
            "label": "案件类型",
            "dict": {
              "dictType": "CS_E_LOCATION_CD",
              "dictKey": [
                "CS_E_LOCATION_CD"
              ],
              "format": "value-label"
            }
          }
        },
        {
          "collect_date_1": {
            "control": "inputNumber",
            "label": "入催天数区间开始值"
          },
          "collect_date_2": {
            "control": "inputNumber",
            "label": "入催天数区间结束值"
          }
        },
        {
          "ovd_amt_total_1": {
            "control": "inputNumber",
            "label": "欠款金额区间开始值"
          },
          "ovd_amt_total_2": {
            "control": "inputNumber",
            "label": "欠款金额区间结束值"
          }
        },
        {
          "stage_code": {
            "control": "remoteSelect",
            "label": "逾期阶段",
            "labelField": "stage_name",
            "valueField": "stage_code",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0083"
              }
            },
            "format": "value-label"
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "custom",
          "icon": "el-icon-date",
          "target": "dialog",
          "checkCountMin": 1,
          "label": "展期",
          "toRequest": {
            "method": "GET",
            "url": "/views/cs/collManage/oaManage/oaWork/oaWork_extension.json"
          }
        },
        {
          "type": "custom",
          "icon": "el-icon-back",
          "target": "dialog",
          "checkCountMin": 1,
          "label": "退件",
          "confirm": {
            "title": "提示信息",
            "msg": "确定退件吗？",
            "confirmButtonText": "确定",
            "cancelButtonText": "取消"
          },
          "doRequest": {
            "method": "POST",
            "url": "/SUMP/icmcall/icmRPCCall",
            "params": {
              "servicecode": "cb0224"
            }
          }
        },
        {
          "type": "custom",
          "icon": "el-icon-picture",
          "target": "dialog",
          "checkCountMin": 1,
          "checkCountMax": 1,
          "label": "影像资料",
          "toRequest": {
            "method": "GET",
            "url": "/views/cs/collManage/oaManage/oaWork/oaWork_plVideo.json"
          }
        },
        {
          "type": "custom",
          "target": "dialog",
          "icon": "el-icon-upload",
          "label": "导入催记",
          "toRequest": {
            "method": "GET",
            "url": "/views/cs/collManage/oaManage/oaWork/oaWork_Import.json"
          },
          "props": {
            "width": "50%",
            "title": "导入催记"
          }
        },
        {
          "type": "export",
          "target": "dialog",
          "icon": "el-icon-download",
          "label": "委外报表下载",
          "doRequest": {
            "method": "POST",
            "url": "/SUMP/rest/file/export",
            "params": {
              "servicecode": "cb0227"
            }
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "cb0222",
          "strategy_type": "OT",
          "appId": "cbOnl"
      }
      },
      "thead": [
        {
          "type": "selection",
          "align": "center",
          "width": "55",
          "fixed": "left"
        },
        {
          "type": "string",
          "align": "center",
          "label": "机构",
          "prop": "branch_id"
        },
        {
          "type": "string",
          "align": "center",
          "width": "160",
          "label": "案件编号",
          "prop": "case_id"
        },
        {
          "type": "string",
          "align": "center",
          "width": "160",
          "label": "客户编号",
          "prop": "cust_id"
        },
        {
          "type": "string",
          "align": "center",
          "width": "150",
          "label": "客户姓名",
          "prop": "cust_name"
        },
        {
          "type": "string",
          "align": "center",
          "label": "证件类型",
          "width": "150",
          "prop": "cert_type",
          "dict": {
            "dictType": "CS_E_CERT_TYPE",
            "dictKey": [
              "CS_E_CERT_TYPE"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "align": "center",
          "width": "180",
          "label": "证件号码",
          "prop": "cert_no"
        },
        {
          "type": "string",
          "align": "center",
          "label": "逾期金额",
          "prop": "over_due_amt"
        },
        {
          "type": "string",
          "align": "center",
          "label": "逾期天数",
          "prop": "over_due_day"
        },
        {
          "type": "string",
          "align": "center",
          "label": "余额",
          "prop": "balance"
        },
        {
          "type": "string",
          "align": "center",
          "label": "入催日期",
          "prop": "collect_date"
        },
        {
          "type": "string",
          "align": "center",
          "label": "手别",
          "prop": "oa_hands"
        },
        {
          "type": "string",
          "align": "center",
          "label": "上次委外公司",
          "prop": "last_oa_org_id"
        },
        {
          "type": "string",
          "align": "center",
          "width": "180",
          "label": "委外公司",
          "prop": "oa_org_id"
        },
        {
          "type": "string",
          "align": "center",
          "label": "委外开始日期",
          "prop": "oa_begin_date"
        },
        {
          "type": "string",
          "align": "center",
          "label": "委外结束日期",
          "prop": "oa_end_date"
        },
        {
          "type": "string",
          "align": "center",
          "label": "委外状态",
          "prop": "oa_status",
          "dict": {
            "dictType": "CS_E_OA_STATUS",
            "dictKey": [
              "CS_E_OA_STATUS"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "风险级别",
          "prop": "risk_cd"
        },
        {
          "type": "string",
          "align": "center",
          "label": "案件状态",
          "prop": "case_status",
          "dict": {
            "dictType": "CS_E_STATUS_CD",
            "dictKey": [
              "CS_E_STATUS_CD"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "案件分类",
          "prop": "classify_code",
          "dict": {
            "dictType": "CS_E_LOCATION_CD",
            "dictKey": [
              "CS_E_LOCATION_CD"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "逾期阶段",
          "prop": "stage_code"
        },
        {
          "type": "string",
          "align": "center",
          "label": "主案件编号",
          "prop": "case_no"
        },
        {
          "label": "操作",
          "align": "center",
          "fixed": "right",
          "width": "180%",
          "btns": [
            {
              "type": "look",
              "label": "案件详情",
              "toRequest": {
                "url": "/views/cs/assistManage/caseDetail/caseDetail_2/case_detail_main.json",
                "method": "POST",
                "params": {
                  "case_id": "${{current.case_id}}"
                }
              },
              "props": {
                "width": "80%",
                "title": "案件详情"
              }
            },
            {
              "type": "look",
              "label": "派单记录",
              "toRequest": {
                "url": "/views/cs/collManage/oaManage/oaWork/orders_record.json",
                "method": "POST",
                "params": {
                  "current": "${{current}}"
                }
              },
              "props": {
                "width": "80%",
                "title": "派单记录"
              }
            }
          ]
        }
      ]
    }
  }
}
