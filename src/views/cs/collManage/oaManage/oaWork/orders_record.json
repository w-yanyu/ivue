{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "scope": "formData",
            "controls": [
                {
                    "cust_name": {
                        "control": "text",
                        "default":"${{params.current.cust_name}}",
                        "disabled":true,
                        "label": "客户姓名"
                    },
                    "cert_no": {
                        "control": "text",
                        "default":"${{params.current.cert_no}}",
                        "disabled":true,
                        "label": "证件号码"
                    }
                }
            ],
            "btns": []
        },

        "datagrid": {
            "isInitSearch": true,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "cb0228"
                }
            },
            "thead": [
                {
                    "type":"string",
                    "label": "记录日期",
                    "align":"center",
                    "width": "180",
                    "prop": "data_create_time"
                },
                {
                    "type":"string",
                    "label": "批次",
                    "align":"center",
                    "width": "150",
                    "prop": "series_num"
                },
                {
                    "type":"string",
                    "label": "委外公司",
                    "align":"center",
                    "width": "150",
                    "prop": "oa_org_id"
                },
                {
                    "type":"string",
                    "label": "委外金额",
                    "align":"center",
                    "width": "150",
                    "prop": "oa_amt"
                },
                {
                    "type":"string",
                    "label": "派案日期",
                    "align":"center",
                    "width": "150",
                    "prop": "oa_begin_date"
                },
                {
                    "type":"string",
                    "label": "回收日期",
                    "align":"center",
                    "width": "150",
                    "prop": "oa_end_date"
                },
                {
                    "type":"string",
                    "label": "实际结束时间",
                    "align":"center",
                    "width": "150",
                    "prop": "real_end_date"
                },
                {
                    "type": "string",
                    "align": "center",
                    "width": "150",
                    "label": "案件状态",
                    "prop": "case_status",
                    "dict": {
                        "dictType": "CS_E_STATUS_CD",
                        "dictKey": [
                          "CS_E_STATUS_CD"
                      ],
                      "format": "value-label"
                    }
                  },
                {
                    "type":"string",
                    "label": "派单状态",
                    "align":"center",
                    "width": "150",
                    "prop": "oa_status"
                }
            ]
        }
    }
}