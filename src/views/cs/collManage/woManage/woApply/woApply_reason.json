{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "scope": "formData",
      "initRequest": "${{params}}",
      "btns": [
        {
          "type": "submit",
          "icon": "el-icon-check",
          "label": "提交",
          "doRequest": {
            "method": "POST",
            "url": "/SUMP/icmcall/icmRPCCall",
            "params": {
              "appId": "cbOnl",
              "servicecode": "cb0231",
              "list01": "${{params.selectedData}}",
              "pageId": "/views/cs/collManage/woManage/woApply/woApply_reason.json"
            }
          }
        },
        {
          "type": "close",
          "label": "取消"
        }
      ],
      "controls": [
        {
          "wo_type": {
            "control": "select",
            "label": "核销类型",
            "align": "left",
            "dict": {
              "dictType": "CB_E_WO_TYPE",
              "dictKey": [
                "CB_E_WO_TYPE"
              ],
              "format": "value-label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择核销类型"
              }
            ]
          }
        },
        {
          "apply_reason": {
            "control": "textarea",
            "label": "申请说明",
            "align": "left",
            "rows": 6,
            "span": "20",
            "placeholder": "请输入",
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          }
        }
      ]
    }
  }
}
