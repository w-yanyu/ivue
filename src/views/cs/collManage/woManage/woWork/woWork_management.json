{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "branch_id": {
            "control": "inputTree",
            "label": "机构",
            "idField": "coll_team",
            "nodeTypeField": "type",
            "pIdField": "parent_coll_team",
            "labelField": "coll_team_name",
            "nodeTypes": {
              "branchTeam": {
                "icon": "el-icon-news",
                "children": [
                  "branchTeam"
                ]
              }
            },
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0041",
                "query_type": "branchTeam"
              }
            },
            "format": "value-label"
          },
          "cust_name": {
            "control": "text",
            "label": "客户姓名"
          }
        },
        {
          "cert_no": {
            "control": "text",
            "label": "证件号码"
          },
          "cust_id": {
            "control": "text",
            "label": "客户编号"
          }
        },
        {
          "daterange": {
            "control": "dateTimePicker",
            "label": "申请时间",
            "type": "daterange",
            "unlinkPanels": true,
            "valueFormat": "yyyyMMdd",
            "startPlaceholder": "请选择开始时间",
            "endPlaceholder": "请选择结束时间",
            "startField": "start_date",
            "endField": "end_date"
          },
          "queue_code": {
            "control": "select",
            "label": "队列名称",
            "options": [
              {
                "value": "HX_QE_DL",
                "label": "核销队列"
              }
            ]
          },
          "location_cd": {
            "control": "select",
            "label": "案件类型",
            "dict": {
              "dictType": "CS_E_LOCATION_CD",
              "dictKey": [
                "CS_E_LOCATION_CD"
              ],
              "format": "value-label"
            }
          },
          "case_status": {
            "control": "select",
            "label": "案件状态",
            "dict": {
              "dictType": "CS_E_STATUS_CD",
              "dictKey": [
                "CS_E_STATUS_CD"
              ],
              "format": "value-label"
            }
          },
          "stage_code": {
            "control": "remoteSelect",
            "label": "逾期阶段",
            "labelField": "stage_name",
            "valueField": "stage_code",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0083"
              }
            },
            "format": "value-label"
          },
          "ovd_amt_total_1": {
            "control": "currency",
            "label": "欠款金额区间开始值"
          },
          "ovd_amt_total_2": {
            "control": "currency",
            "label": "欠款金额区间结束值"
          },
          "collect_date_1": {
            "control": "inputNumber",
            "label": "入催天数区间开始值"
          },
          "collect_date_2": {
            "control": "inputNumber",
            "label": "入催天数区间结束值"
          },
          "wo_type": {
            "control": "select",
            "label": "核销类型",
            "dict": {
              "dictType": "CB_E_WO_TYPE",
              "dictKey": [
                "CB_E_WO_TYPE"
              ],
              "format": "value-label"
            }
          },
          "wo_status": {
            "control": "select",
            "label": "核销状态",
            "dict": {
              "dictType": "CB_E_WO_STATUS",
              "dictKey": [
                "CB_E_WO_STATUS"
              ],
              "format": "value-label"
            }
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "authId": "btn001",
          "type": "export",
          "icon": "iconfont icon-daochu",
          "label": "生成核销材料",
          "doRequest": {
            "method": "POST",
            "url": "/SUMP/rest/file/export",
            "params": {
              "appId": "cbOnl",
              "servicecode": "pl3506"
            }
          }
        },
        {
          "type": "export",
          "label": "导出核销材料",
          "icon": "el-icon-download",
          "doRequest": {
            "url": "/SUMP/rest/file/export",
            "params": {
              "servicecode": "pl4707"
            }
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "method": "POST",
        "appId": "cbOnl",
        "url": "/SUMP/icmcall/icmRPCCall",
        "pageId": "/views/cs/collManage/woManage/woWork/woWork_management.json",
        "params": {
          "servicecode": "cb0236"
        }
      },
      "thead": [
        {
          "type": "string",
          "align": "center",
          "label": "机构",
          "prop": "branch_id"
        },
        {
          "type": "string",
          "align": "center",
          "label": "案件编号",
          "prop": "case_id"
        },
        {
          "type": "string",
          "align": "center",
          "label": "客户编号",
          "prop": "cust_id"
        },
        {
          "type": "string",
          "align": "center",
          "label": "客户姓名",
          "prop": "cust_name"
        },
        {
          "type": "string",
          "align": "center",
          "label": "证件类型",
          "width": "160px",
          "prop": "cert_type",
          "dict": {
            "dictType": "CS_E_CERT_TYPE",
            "dictKey": [
              "CS_E_CERT_TYPE"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "证件号码",
          "prop": "cert_no"
        },
        {
          "type": "currency",
          "align": "center",
          "label": "本金余额",
          "prop": "current_balance",
          "format": {
            "decimal": "2"
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "核销类型",
          "prop": "wo_type",
          "dict": {
            "dictType": "CB_E_WO_TYPE",
            "dictKey": [
              "CB_E_WO_TYPE"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "申请说明",
          "prop": "apply_desc"
        },
        {
          "type": "string",
          "align": "center",
          "label": "申请时间",
          "prop": "apply_time"
        },
        {
          "type": "string",
          "align": "center",
          "label": "审核时间",
          "prop": "approval_time"
        },
        {
          "type": "string",
          "align": "center",
          "label": "核销状态",
          "prop": "wo_status",
          "dict": {
            "dictType": "CB_E_WO_STATUS",
            "dictKey": [
              "CB_E_WO_STATUS"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "是否已完成",
          "prop": "is_finish"
        },
        {
          "label": "操作",
          "align": "center",
          "fixed": "right",
          "width": "180px",
          "btns": [
            {
              "type": "custom",
              "label": "案件详情",
              "toRequest": {
                "url": "/views/cs/assistManage/caseDetail/caseDetail_2/case_detail_main.json",
                "method": "POST",
                "params": {
                  "case_id": "${{current.case_id}}",
                  "case_no": "${{current.case_no}}",
                  "cust_id": "${{current.cust_id}}",
                  "org_id": "${{current.org_id}}",
                  "oa_org_id": "${{current.oa_org_id}}"
                }
              },
              "props": {
                "width": "80%",
                "title": "案件详情"
              }
            },
            {
              "type": "export",
              "label": "下载核销材料",
              "doRequest": {
                "url": "/SUMP/rest/file/exportNullExcel",
                "method": "get"
              },
              "confirm": {
                "title": "提示信息",
                "msg": "确认要下载核销材料吗？",
                "confirmButtonText": "确定",
                "cancelButtonText": "取消"
              }
            }
          ]
        }
      ]
    }
  }
}
