{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "btns": [
                {
                    "type": "submit",
                    "icon": "el-icon-check",
                    "label": "打标签",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "cb0138",
                            "list01": "${{params.selectedData}}"
                        }
                    }
                },
                {
                    "type": "close",
                    "label": "取消"
                }
            ],
            "controls": [
                {
                    "mark_code": {
                        "control": "remoteSelect",
                        "label": "标签",
                        "placeholder": "请选择",
                        "width": "180px",
                        "align": "center",
                        "span": "16",
                        "labelField": "mark_name",
                        "valueField": "mark_code",
                        "lazy": true,
                        "doRequest": {
                            "url": "/SUMP/icmcall/icmRPCCall",
                            "method": "POST",
                            "params": {
                                "servicecode": "cb0026"
                            }
                        },
                        "format": "value-label",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            }
                        ]
                    }
                }
            ]
        }
    }
}