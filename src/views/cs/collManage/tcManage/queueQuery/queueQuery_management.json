{
  "module": "v_search_btn_datagrid",
  "pageId": "page001",
  "layout": {
    "form": {
      "foldLineNumber": 3,
      "scope": "formData",
      "controls": [
        {
          "cust_name": {
            "control": "text",
            "label": "客户姓名"
          },
          "cust_id": {
            "control": "text",
            "label": "客户编号"
          }
        },
        {
          "cert_no": {
            "control": "text",
            "label": "证件号码"
          },
          "phone_no": {
            "control": "text",
            "label": "手机号码"
          }
        },
        {
          "case_id": {
            "control": "text",
            "label": "案件编号"
          },
          "batch_no": {
            "control": "text",
            "label": "案件批次号"
          }
        },
        {
          "case_status": {
            "control": "select",
            "label": "案件状态",
            "dict": {
              "dictType": "CS_E_STATUS_CD",
              "dictKey": [
                "CS_E_STATUS_CD"
              ],
              "format": "value-label"
            }
          },
          "handle_status": {
            "control": "select",
            "label": "处理状态",
            "dict": {
              "dictType": "CS_E_DEAL_STATE",
              "dictKey": [
                "CS_E_DEAL_STATE"
              ],
              "format": "value-label"
            }
          }
        },
        {
          "currencyRange": {
            "control": "currencyRange",
            "label": "逾期金额",
            "min": "1",
            "max": "16",
            "decimal": "2",
            "symbol": "￥",
            "valueFormat": "number",
            "startField": "min_amount",
            "endField": "max_amount"
          },
          "phone_status": {
            "control": "select",
            "label": "最近电话状态",
            "dict": {
              "dictType": "CS_E_PHONE_STATUS",
              "dictKey": [
                "CS_E_PHONE_STATUS"
              ],
              "format": "value-label"
            }
          }
        },
        {
          "last_coll_result": {
            "control": "remoteSelect",
            "label": "最近催收结果",
            "labelField": "coll_result_name",
            "valueField": "coll_result_code",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0087",
                "is_valid": "Y",
                "appId": "cbOnl"
              }
            },
            "format": "value-label"
          },
          "daterange": {
            "control": "dateTimePicker",
            "label": "最近催收时间",
            "type": "daterange",
            "unlinkPanels": true,
            "valueFormat": "yyyyMMdd",
            "startPlaceholder": "请选择开始时间",
            "endPlaceholder": "请选择结束时间",
            "startField": "start_date",
            "endField": "end_date"
          }
        },
        {
          "stage_code": {
            "control": "remoteSelect",
            "label": "逾期阶段",
            "labelField": "stage_name",
            "valueField": "stage_code",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0083"
              }
            },
            "format": "value-label"
          },
          "queue_code": {
            "control": "remoteSelect",
            "label": "队列名称",
            "labelField": "queue_name",
            "valueField": "queue_code",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0082"
              }
            },
            "format": "value-label"
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "export",
          "icon": "iconfont icon-daochu",
          "label": "导出",
          "doRequest": {
            "method": "POST",
            "url": "/SUMP/rest/file/commExport",
            "params": {
              "length": 999,
              "servicecode": "cb0042"
            }
          },
          "confirm":{
            "title":"提示信息",
            "msg":"确认导出吗？",
            "confirmButtonText":"确定",
            "cancelButtonText":"取消"
          }
        },
        {
          "authId": "btn001",
          "type": "custom",
          "icon": "el-icon-edit",
          "checkCountMin": 1,
          "label": "勾选生成法律文书",
          "toRequest": {
            "method": "GET",
            "url": "/views/cs/collManage/tcManage/queueQuery/generate_legal_documents.json"
          }
        },
        {
          "authId": "btn001",
          "type": "export",
          "icon": "iconfont icon-daochu",
          "label": "更多功能",
          "doRequest": {
            "method": "POST",
            "url": "/SUMP/rest/file/export",
            "params": {
              "appId": "",
              "servicecode": ""
            }
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "appId": "cbOnl",
          "servicecode": "cb0042"
        }
      },
      "sortDb": true,
      "thead": [
        {
          "type": "selection",
          "align": "center",
          "width": "55",
          "fixed": "left"
        },
        {
          "label": "操作",
          "align": "center",
          "fixed": "left",
          "width": "130px",
          "btns": [
            {
              "type": "openVerify",
              "label": "进入工作台",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "appId": "cbOnl",
                  "servicecode": "cb0047"
                }
              },
              "toRequest": {
                "target": "navTab",
                "url": "/views/cs/collManage/tcManage/collWorkbench/collWorkbench.json",
                "method": "POST",
                "params": {
                  "case_id": "${{current.case_id}}",
                  "case_no": "${{current.case_no}}",
                  "cust_id": "${{current.cust_id}}",
                  "org_id": "${{current.org_id}}",
                  "oa_org_id": "${{current.oa_org_id}}",
                  "cert_no": "${{current.cert_no}}",
                  "cert_type": "${{current.cert_type}}",
                  "queue_code": "${{current.queue_code}}",
                  "product_type": "${{current.product_type}}"
                }
              },
              "props": {
                "title": "催收工作台"
              }
            }
          ]
        },
        {
          "type": "tag",
          "align": "center",
          "width": "100px",
          "label": "紧急程度",
          "prop": "urgency_degree",
          "switch": {
            "warning": "warning",
            "danger": "danger",
            "normal": "info"
          }
        },
        {
          "type": "string",
          "align": "center",
          "width": "180px",
          "label": "客户编号",
          "prop": "cust_id"
        },
        {
          "type": "string",
          "align": "center",
          "width": "180px",
          "label": "客户姓名",
          "prop": "cust_name"
        },
        {
          "type": "string",
          "align": "center",
          "width": "180px",
          "label": "案件批次号",
          "prop": "batch_no"
        },
        {
          "type": "string",
          "align": "center",
          "width": "180px",
          "label": "案件编号",
          "prop": "case_id"
        },
        {
          "type": "string",
          "align": "center",
          "width": "180px",
          "label": "证件类型",
          "prop": "cert_type",
          "dict": {
            "dictType": "CS_E_CERT_TYPE",
            "dictKey": [
              "CS_E_CERT_TYPE"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "align": "center",
          "width": "180px",
          "label": "证件号码",
          "prop": "cert_no"
        },
        {
          "type": "string",
          "align": "center",
          "width": "180px",
          "label": "手机号码",
          "prop": "phone_no"
        },
        {
          "type": "string",
          "align": "center",
          "width": "180px",
          "label": "处理状态",
          "prop": "handle_status",
          "dict": {
            "dictType": "CS_E_DEAL_STATE",
            "dictKey": [
              "CS_E_DEAL_STATE"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "align": "center",
          "width": "180px",
          "label": "案件类型",
          "prop": "classify_code",
          "dict": {
            "dictType": "CS_E_LOCATION_CD",
            "dictKey": [
              "CS_E_LOCATION_CD"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "案件状态",
          "prop": "case_status",
          "dict": {
            "dictType": "CS_E_STATUS_CD",
            "dictKey": [
              "CS_E_STATUS_CD"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "align": "center",
          "width": "180px",
          "label": "队列名称",
          "prop": "queue_code",
          "dict": {
            "dictKey": [
              "queue_code"
            ],
            "format": "value-label",
            "labelField": "queue_name",
            "valueField": "queue_code",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0082"
              }
            }
          }
        },
        {
          "type": "currency",
          "align": "center",
          "label": "逾期阶段",
          "prop": "stage_code"
        },
        {
          "type": "currency",
          "align": "center",
          "label": "当前余额",
          "prop": "current_balance",
          "format": {
            "decimal": "2"
          }
        },
        {
          "type": "currency",
          "align": "center",
          "label": "逾期金额",
          "sortable": true,
          "prop": "overdue_amount",
          "format": {
            "decimal": "2"
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "逾期天数",
          "prop": "overdue_days"
        },
        {
          "type": "string",
          "align": "center",
          "label": "产品类型",
          "prop": "product_type",
          "dict": {
            "dictType": "CS_E_PRODUCT_TYPE",
            "dictKey": [
              "CS_E_PRODUCT_TYPE"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "催收组",
          "prop": "coll_team",
          "dict": {
            "dictKey": [
              "coll_team"
            ],
            "format": "value-label",
            "labelField": "coll_team_name",
            "valueField": "coll_team",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0041",
                "appId": "cbOnl"
              }
            }
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "催收员",
          "prop": "collector",
          "dict": {
            "dictKey": [
              "collector"
            ],
            "format": "value-label",
            "labelField": "user_name",
            "valueField": "user_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0084",
                "appId": "cbOnl"
              }
            }
          }
        },
        {
          "type": "string",
          "align": "center",
          "width": "180px",
          "label": "跟催时间",
          "prop": "handle_time"
        },
        {
          "type": "string",
          "align": "center",
          "width": "180px",
          "label": "最近催收结果",
          "prop": "last_coll_result",
          "dict": {
            "dictKey": [
              "last_coll_result"
            ],
            "format": "value-label",
            "labelField": "coll_result_name",
            "valueField": "coll_result_code",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0087",
                "appId": "cbOnl"
              }
            }
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "排序名称",
          "prop": "sortName",
          "isShow": false
        },
        {
          "type": "string",
          "align": "center",
          "label": "排序顺序",
          "prop": "sortDesc",
          "isShow": false
        }
      ]
    }
  }
}