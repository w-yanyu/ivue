{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "btns": [
        {
          "type": "submit",
          "target": "dialog",
          "label": "申请",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "cb0128",
              "case_id":"${{params.case_id}}",
              "appId": "cbOnl"
            }
          }
        },
        {
          "type": "close",
          "label": "返回"
        }
      ],
      "controls": [
        {
          "wo_type": {
            "control": "select",
            "span": 8,
            "labelWidth": "0",
            "inputWidth": "250px",
            "label": "核销类型",
            "dict": {
              "dictType": "CB_E_WO_TYPE",
              "dictKey": [
                "CB_E_WO_TYPE"
              ]
            },
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          },
          "apply_reason": {
            "control": "textarea",
            "span": 22,
            "labelWidth": "0",
            "rows":"6",
            "label": "申请原因",
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          }
        }
      ]
    }
  }
}
