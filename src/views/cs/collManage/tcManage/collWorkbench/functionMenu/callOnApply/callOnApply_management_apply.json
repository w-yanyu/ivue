{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "btns": [
        {
          "type": "submit",
          "target": "dialog",
          "label": "申请",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "cb0126",
              "case_id":"${{params.case_id}}",
              "appId": "cbOnl"
            }
          }
        }
      ],
      "controls": [
        {
          "interview_name": {
            "control": "string",
            "span": 11,
            "labelWidth": "0",
            "inputWidth": "250px",
            "disabled": false,
            "label": "被访人姓名"
          },
          "relationship": {
            "control": "string",
            "width": "300px",
            "span": 10,
            "inputWidth": "245px",
            "disable": false,
            "label": "与持卡人关系",
            "dict": {
              "dictType": "CS_E_RELATIONSHIP",
              "dictKey": [
                "CS_E_RELATIONSHIP"
              ],
              "format": "value-label"
            }
          },
          "address": {
            "control": "string",
            "span": 24,
            "labelWidth": "0",
            "disable": false,
            "label": "被访人地址"
          },
          "apply_reason": {
            "control": "textarea",
            "span": 24,
            "labelWidth": "0",
            "rows": "6",
            "label": "申请原因",
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          }
        }
      ]
    }
  }
}
