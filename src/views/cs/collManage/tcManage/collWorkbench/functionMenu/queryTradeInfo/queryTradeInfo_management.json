{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "cb0146",
          "case_id": "${{params.case_id}}",
          "case_no": "${{params.case_no}}",
          "cust_id": "${{params.cust_id}}",
          "org_id": "${{params.org_id}}",
          "oa_org_id": "${{params.oa_org_id}}",
          "appId": "cbOnl"
        }
      },
      "thead": [
        {
          "type": "string",
          "label": "明细编号",
          "align": "center",
          "width": "180px",
          "prop": "detl_num"
        },
        {
          "type": "string",
          "label": "借据号",
          "align": "center",
          "width": "180px",
          "prop": "loan_no"
        },
        {
          "type": "string",
          "label": "币种",
          "align": "center",
          "width": "180px",
          "prop": "currency_code",
          "dict": {
            "dictType": "E_CRCYCD",
            "dictKey": [
              "E_CRCYCD"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "label": "交易日期",
          "align": "center",
          "width": "180px",
          "prop": "trxn_date"
        },
        {
          "type": "currency",
          "label": "正常本金",
          "align": "center",
          "width": "180px",
          "prop": "normal_principal",
          "format": {
            "decimal": "2"
          }
        },
        {
          "type": "string",
          "label": "还款账号",
          "align": "center",
          "width": "180px",
          "prop": "repayment_account"
        },
        {
          "type": "currency",
          "label": "还款总额",
          "align": "center",
          "width": "180px",
          "prop": "total_repayment_amount",
          "format": {
            "decimal": "2"
          }
        },
        {
          "type": "string",
          "label": "还款状态",
          "align": "center",
          "width": "180px",
          "prop": "repayment_status"
        },
        {
          "type": "currency",
          "label": "归还本金",
          "align": "center",
          "width": "180px",
          "prop": "repayment_principal",
          "format": {
            "decimal": "2"
          }
        },
        {
          "type": "currency",
          "label": "归还应收应计利息",
          "align": "center",
          "width": "180px",
          "prop": "repayment_interest",
          "format": {
            "decimal": "2"
          }
        },
        {
          "type": "currency",
          "label": "归还应收欠息",
          "align": "center",
          "width": "180px",
          "prop": "repayment_due_interest",
          "format": {
            "decimal": "2"
          }
        },
        {
          "type": "currency",
          "label": "归还应收罚息",
          "align": "center",
          "width": "180px",
          "prop": "repayment_due_penalty_interest",
          "format": {
            "decimal": "2"
          }
        },
        {
          "type": "currency",
          "label": "归还复息",
          "align": "center",
          "width": "180px",
          "prop": "repayment_compound_interest",
          "format": {
            "decimal": "2"
          }
        },
        {
          "type": "currency",
          "label": "归还费用",
          "align": "center",
          "width": "180px",
          "prop": "repayment_fee",
          "format": {
            "decimal": "2"
          }
        }
      ]
    }
  }
}
