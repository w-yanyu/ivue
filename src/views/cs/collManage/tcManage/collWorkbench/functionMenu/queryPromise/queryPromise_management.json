{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "btns": []
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "cb0088",
          "case_id": "${{params.case_id}}",
          "case_no": "${{params.case_no}}",
          "cust_id": "${{params.cust_id}}",
          "org_id": "${{params.org_id}}",
          "oa_org_id": "${{params.oa_org_id}}",
          "appId": "cbOnl"
        }
      },
      "thead": [
        {
          "type": "string",
          "label": "催收组",
          "align": "center",
          "width": "180px",
          "prop": "coll_team",
          "dict": {
            "dictKey": [
              "coll_team"
            ],
            "format": "value-label",
            "labelField": "coll_team_name",
            "valueField": "coll_team",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0041",
                "appId": "cbOnl"
              }
            }
          }
        },
        {
          "type": "string",
          "label": "催收员",
          "align": "center",
          "width": "180px",
          "prop": "collector",
          "dict": {
            "dictKey": [
              "collector"
            ],
            "format": "value-label",
            "labelField": "user_name",
            "valueField": "user_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0084",
                "appId": "cbOnl"
              }
            }
          }
        },
        {
          "type": "string",
          "label": "逾期金额",
          "align": "center",
          "width": "180px",
          "prop": "overdue_amount"
        },
        {
          "type": "string",
          "label": "承诺金额",
          "align": "center",
          "width": "180px",
          "prop": "promise_amount"
        },
        {
          "type": "string",
          "label": "承诺剩余金额",
          "align": "center",
          "width": "180px",
          "prop": "promise_remaining_amount"
        },
        {
          "type": "string",
          "label": "承诺日期",
          "align": "center",
          "width": "180px",
          "prop": "promise_date"
        },
        {
          "type": "string",
          "label": "累计还款金额",
          "align": "center",
          "width": "180px",
          "prop": "repayment_amount"
        },
        {
          "type": "string",
          "label": "承诺状态",
          "align": "center",
          "width": "180px",
          "prop": "promise_status",
          "dict": {
            "dictType": "CS_E_PROMISE_STATUS",
            "dictKey": [
              "CS_E_PROMISE_STATUS"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "label": "创建人",
          "align": "center",
          "width": "180px",
          "prop": "data_create_user",
          "dict": {
            "dictKey": [
              "data_create_user"
            ],
            "format": "value-label",
            "labelField": "user_name",
            "valueField": "user_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0084",
                "appId": "cbOnl"
              }
            }
          }
        },
        {
          "type": "string",
          "label": "创建时间",
          "align": "center",
          "width": "180px",
          "prop": "data_create_time"
        },
        {
          "type": "string",
          "label": "是否有效",
          "align": "center",
          "width": "180px",
          "prop": "is_valid",
          "dict": {
            "dictType": "CS_E_YESORNO",
            "dictKey": [
              "CS_E_YESORNO"
            ],
            "format": "value-label"
          }
        }
      ]
    }
  }
}
