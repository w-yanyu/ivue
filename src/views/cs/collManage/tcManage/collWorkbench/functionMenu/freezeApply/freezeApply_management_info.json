{
  "module": "v_search_btn_datagrid",
  "layout": {
    "toolbar": {
      "btns": []
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/iccsCall/iccsEurekaCall",
        "method": "POST",
        "params": {
          "servicecode": "pl3763",
          "case_id": "${{params.case_id}}",
          "apply_id": ""
        }
      },
      "thead": [
        {
          "type": "string",
          "label": "冻结类型",
          "align": "center",
          "prop": "frozen_type",
          "dict": {
            "dictKey": [
              "frozen_type"
            ],
            "format": "value-label",
            "labelField": "drop_list_desc",
            "valueField": "drop_list_value",
            "doRequest": {
              "url": "/SUMP/iccsCall/iccsEurekaCall",
              "method": "POST",
              "params": {
                "servicecode": "ap1071",
                "drop_list_type": "freezeType"
              }
            }
          }
        },
        {
          "type": "string",
          "label": "部分冻结额度",
          "align": "center",
          "prop": "frozen_quota"
        },
        {
          "type": "string",
          "label": "申请人",
          "align": "center",
          "width": "180px",
          "prop": "apply_user",
          "dict": {
            "dictKey": [
              "apply_user"
            ],
            "format": "value-label",
            "labelField": "user_name",
            "valueField": "user_id",
            "doRequest": {
              "url": "/SUMP/iccsCall/iccsEurekaCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0084"
              }
            }
          }
        },
        {
          "type": "string",
          "label": "申请时间",
          "align": "center",
          "width": "180px",
          "prop": "apply_time"
        },
        {
          "type": "string",
          "label": "申请原因",
          "align": "center",
          "prop": "apply_reason"
        },
        {
          "type": "string",
          "label": "审核状态",
          "align": "center",
          "width": "180px",
          "prop": "check_status",
          "dict": {
            "dictKey": [
              "check_status"
            ],
            "format": "value-label",
            "labelField": "drop_list_desc",
            "valueField": "drop_list_value",
            "doRequest": {
              "url": "/SUMP/iccsCall/iccsEurekaCall",
              "method": "POST",
              "params": {
                "servicecode": "ap1071",
                "drop_list_type": "check_status"
              }
            }
          }
        },
        {
          "type": "string",
          "label": "审核人",
          "align": "center",
          "width": "180px",
          "prop": "check_user",
          "dict": {
            "dictKey": [
              "check_user"
            ],
            "format": "value-label",
            "labelField": "user_name",
            "valueField": "user_id",
            "doRequest": {
              "url": "/SUMP/iccsCall/iccsEurekaCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0084"
              }
            }
          }
        },
        {
          "type": "string",
          "label": "审核时间",
          "align": "center",
          "width": "180px",
          "prop": "check_time"
        },
        {
          "type": "string",
          "label": "审核说明",
          "align": "center",
          "width": "180px",
          "prop": "check_desc"
        },
        {
          "label": "操作",
          "fixed": "right",
          "align": "center",
          "btns": [
            {
              "type": "delete",
              "label": "撤回",
              "doRequest": {
                "url": "/SUMP/iccsCall/iccsEurekaCall",
                "params": {
                  "servicecode": "pl1639",
                  "apply_id": "${{current.apply_id}}"
                }
              },
              "show": {
                "prop": "check_status",
                "is": [
                  "NC"
                ]
              },
              "confirm":{
                "title":"提示信息",
                "msg":"确定撤回减免申请吗？",
                "confirmButtonText":"确定",
                "cancelButtonText":"取消"
              }
            }
          ]
        }
      ]
    }
  }
}
