{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "btns": []
        },
        "datagrid": {
            "isInitSearch": false,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "cb0086",
                    "case_id":"${{params.case_id}}",
                     "appId": "cbOnl"
                }
            },
            "thead": [
                {
                    "type":"string",
                    "label": "催记类型",
                    "align":"center",
                    "width":"180px",
                    "prop": "coll_type",
                    "dict": {
                      "dictType": "CS_E_COLL_TYPE",
                      "dictKey": [
                        "CS_E_COLL_TYPE"
                      ],
                      "format": "value-label"
                    }
                },
                {
                    "type":"string",
                    "label": "客户关系",
                    "align":"center",
                    "width":"180px",
                    "prop": "relationship",
                    "dict": {
                      "dictType": "CS_E_RELATIONSHIP",
                      "dictKey": [
                        "CS_E_RELATIONSHIP"
                      ],
                      "format": "value-label"
                    }
                },
                {
                    "type":"string",
                    "label": "联系姓名",
                    "align":"center",
                    "width":"180px",
                    "prop": "relation_cust_name"
                },
                {
                    "type":"string",
                    "label": "电话号码",
                    "align":"center",
                    "width":"180px",
                    "prop": "phone_no"
                },
                {
                    "type":"string",
                    "label": "是否接通",
                    "align":"center",
                    "width":"180px",
                    "prop": "action_code",
                    "dict": {
                      "dictType": "CS_E_ACTION_CODE",
                      "dictKey": [
                        "CS_E_ACTION_CODE"
                      ],
                      "format": "value-label"
                    }
                },
                {
                    "type":"string",
                    "label": "催收结果",
                    "align":"center",
                    "width":"180px",
                    "prop": "coll_result",
                    "dict":{
                      "dictKey": [
                        "coll_result"
                      ],
                      "format": "value-label",
                      "labelField": "coll_result_name",
                      "valueField": "coll_result_code",
                      "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                          "servicecode":"cb0087",
                          "appId": "cbOnl"
                        }
                      }
                    }
                },
                {
                    "type":"string",
                    "label": "是否有效结果",
                    "align":"center",
                    "width":"180px",
                    "prop": "is_valid_result",
                    "dict": {
                        "dictType": "CS_E_YESORNO",
                        "dictKey": [
                            "CS_E_YESORNO"
                        ],
                        "format": "value-label"
                    }
                },
                {
                    "type":"string",
                    "label": "备注",
                    "align":"center",
                    "width":"180px",
                    "prop": "remark"
                },
                {
                    "type":"string",
                    "label": "催收员",
                    "align":"center",
                    "width":"180px",
                    "prop": "collector",
                    "dict":{
                        "dictKey": [
                            "collector"
                        ],
                        "format": "value-label",
                        "labelField": "user_name",
                        "valueField": "user_id",
                        "doRequest": {
                            "url": "/SUMP/icmcall/icmRPCCall",
                            "method": "POST",
                            "params": {
                                "servicecode": "cb0084",
                                "appId": "cbOnl"
                            }
                        }
                    }
                },
                {
                    "type":"string",
                    "label": "催收时间",
                    "align":"center",
                    "width":"180px",
                    "prop": "coll_time"
                },
                {
                    "type":"currency",
                    "label": "承诺金额",
                    "align":"center",
                    "width":"180px",
                    "prop": "promise_amount",
                    "format": {
                      "decimal": "2"
                    }
                },
                {
                    "type":"string",
                    "label": "承诺日期",
                    "align":"center",
                    "width":"180px",
                    "prop": "promise_date"
                },
                {
                    "type":"currency",
                    "label": "逾期金额",
                    "align":"center",
                    "width":"180px",
                    "prop": "overdue_amount",
                    "format": {
                      "decimal": "2"
                    }
                },
                {
                    "type":"string",
                    "label": "所属催收组",
                    "align":"center",
                    "width":"180px",
                    "prop": "owner_team",
                    "dict":{
                        "dictKey": [
                            "owner_team"
                        ],
                        "format": "value-label",
                        "labelField": "coll_team_name",
                        "valueField": "coll_team",
                        "doRequest": {
                            "url": "/SUMP/icmcall/icmRPCCall",
                            "method": "POST",
                            "params": {
                                "servicecode": "cb0041",
                                "appId": "cbOnl"
                            }
                        }
                    }
                },
                {
                    "type":"string",
                    "label": "所属催收员",
                    "align":"center",
                    "width":"180px",
                    "prop": "owner_collector",
                    "dict":{
                        "dictKey": [
                            "owner_collector"
                        ],
                        "format": "value-label",
                        "labelField": "user_name",
                        "valueField": "user_id",
                        "doRequest": {
                            "url": "/SUMP/icmcall/icmRPCCall",
                            "method": "POST",
                            "params": {
                              "servicecode": "cb0084",
                              "appId": "cbOnl"
                            }
                        }
                    }
                }
            ]
        }
    }
}
