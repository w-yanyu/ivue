{
  "module": "v_search_btn_datagrid",
  "layout": {
    "toolbar": {
      "btns": []
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "cb0122",
          "case_id":"${{params.case_id}}"
        }
      },
      "thead": [
        {
          "type": "string",
          "label": "申请人",
          "align": "center",
          "prop": "apply_user"
        },
        {
          "type": "string",
          "label": "申请时间",
          "align": "center",
          "prop": "apply_time"
        },
        {
          "type": "string",
          "label": "申请原因",
          "align": "center",
          "prop": "apply_reason"
        },
        {
          "type": "string",
          "label": "审核状态",
          "align": "center",
          "prop": "approval_status",
          "dict": {
            "dictType": "CS_E_CHECK_STATUS",
            "dictKey": [
              "CS_E_CHECK_STATUS"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "label": "审核人",
          "align": "center",
          "prop": "approval_user"
        },
        {
          "type": "string",
          "label": "审核日期",
          "align": "center",
          "prop": "approval_time"
        },
        {
          "type": "string",
          "label": "审核说明",
          "align": "center",
          "prop": "approval_desc"
        }
      ]
    }
  }
}
