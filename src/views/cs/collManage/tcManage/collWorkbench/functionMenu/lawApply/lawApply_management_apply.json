{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "btns": [
        {
          "type": "submit",
          "####target": "dialog",
          "label": "申请",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "cb0123",
              "case_id":"${{params.case_id}}",
              "appId": "cbOnl"
            }
          }
        },
        {
          "type": "close",
          "label": "返回"
        }
      ],
      "controls": [
        {
          "apply_reason": {
            "control": "textarea",
            "span": 22,
            "labelWidth": "0",
            "rows":"6",
            "label": "申请原因",
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          }
        }
      ]
    }
  }
}
