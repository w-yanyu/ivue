{
    "module": "v_search_btn_datagrid",
    "layout": {
      "datagrid": {
        "isInitSearch": true,
        "doRequest": {
          "url": "/SUMP/icmcall/icmRPCCall",
          "method": "POST",
          "params": {
            "servicecode": "cb0256",
            "case_id":"${{params.case_id}}"
          }
        },
        "thead": [
          {
            "type":"string",
            "label": "客户姓名",
            "align":"center",
            "width": "180px",
            "prop": "cust_name"
          },
          {
            "type":"string",
            "label": "可用余额",
            "align":"center",
            "width": "180px",
            "prop": "account_balance"
          },
          {
            "type":"string",
            "label": "申请人",
            "align":"center",
            "width": "180px",
            "prop": "apply_user",
            "dict": {
              "dictKey": [
                "apply_user"
              ],
              "format": "value-label",
              "labelField": "user_name",
              "valueField": "user_id",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "cb0084"
                }
              }
            }
          },
          {
            "type":"string",
            "label": "申请时间",
            "align":"center",
            "width": "180px",
            "prop": "apply_time"
          },
          {
            "type":"string",
            "label": "申请原因",
            "align":"center",
            "width": "180px",
            "prop": "apply_reason"
          },
          {
            "type":"string",
            "label": "审核状态",
            "align":"center",
            "width": "180px",
            "prop": "check_status",
            "dict":{
              "dictKey": [
                "check_status"
              ],
              "format": "value-label",
              "labelField": "drop_list_desc",
              "valueField": "drop_list_value",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode":"ap1071",
                  "drop_list_type": "check_status"
                }
              }
            }
          },
          {
            "type":"string",
            "label": "审核人",
            "align":"center",
            "width": "180px",
            "prop": "check_user",
            "dict":{
              "dictKey": [
                "check_user"
              ],
              "format": "value-label",
              "labelField": "user_name",
              "valueField": "user_id",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "cb0084"
                }
              }
            }
          },
          {
            "type":"string",
            "label": "审核时间",
            "align":"center",
            "width": "180px",
            "prop": "check_time"
          },
          {
            "type":"string",
            "label": "审核说明",
            "align":"center",
            "width": "180px",
            "prop": "check_desc"
          },
          {
            "label": "操作",
            "#fixed": "right",
            "align":"center",
            "width": "55px",
            "btns": [
              {
                "type": "delete",
                "label": "撤回",
                "doRequest": {
                  "url": "/SUMP/icmcall/icmRPCCall",
                  "method": "POST",
                  "params": {
                    "servicecode": "pl1576"
                  }
                },
                "confirm": {
                  "title": "提示信息",
                  "msg": "确认撤回吗?"
                },
                "show": {
                  "prop": "check_status",
                  "is": [
                    "NC"
                  ]
                }
              }
            ]
          }
        ]
      }
    }
}
