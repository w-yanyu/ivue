{
    "module": "v-inputDataForm-btn",
    "layout": {
        "scope": "templateScope",
        "form": {
            "btns": [],
            "controls": [
                {
                    "top": {
                        "control": "dynamic",
                        "span": 24,
                        "toRequest": {
                            "url": "/views/cs/collManage/tcManage/collWorkbench/functionMenu/adjustApply/adjustApply_top.json",
                            "method": "post",
                            "params": {
                              "case_id": "${{params.case_id}}"
                            }
                        }
                    },
                    "bottom": {
                        "control": "dynamic",
                        "span": 24,
                        "toRequest": {
                            "url": "/views/cs/collManage/tcManage/collWorkbench/functionMenu/adjustApply/adjustApply_bottom.json",
                            "method": "post",
                            "params": {
                              "case_id": "${{params.case_id}}"
                            }
                        }
                    }
                }
            ]
        }
    }
}
