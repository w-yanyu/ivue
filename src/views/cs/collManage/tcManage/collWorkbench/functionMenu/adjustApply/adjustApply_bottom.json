{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "btns": []
        },
        "datagrid": {
            "isInitSearch": true,
            "doRequest": {
                "url": "/SUMP/iccsCall/iccsEurekaCall",
                "method": "POST",
                "params": {
                    "servicecode": "pl2740",
                    "case_id": "${{params.case_id}}"
                }
            },
            "thead": [
              {
                "label": "操作",
                "fixed": "right",
                "align": "center",
                "width": "55px",
                "btns": [
                  {
                    "type": "delete",
                    "label": "撤回",
                    "doRequest": {
                      "url": "/SUMP/iccsCall/iccsEurekaCall",
                      "params": {
                        "servicecode": "pl2741"
                      }
                    },
                    "show": {
                      "prop": "check_status",
                      "is": [
                        "NC"
                      ]
                    },
                    "confirm":{
                      "title":"提示信息",
                      "msg":"确定撤回申请吗？",
                      "confirmButtonText":"确定",
                      "cancelButtonText":"取消"
                    }
                  }
                ]
              },
                {
                    "type": "string",
                    "label": "客户姓名",
                    "align": "center",
                    "width": "180px",
                    "prop": "cust_name"
                },
                {
                    "type": "currency",
                    "label": "金额",
                    "align": "center",
                    "width": "180px",
                    "prop": "plan_amt",
                    "decimal": "2"
                },
                {
                    "type": "currency",
                    "label": "执行利率",
                    "align": "center",
                    "width": "180px",
                    "prop": "plan_rate",
                    "decimal": "2"

                },
                {
                    "type": "string",
                    "label": "结算方式",
                    "align": "center",
                    "width": "180px",
                    "prop": "settlement_type",
                    "dict": {
                      "dictKey": [
                        "settlement_type"
                      ],
                      "format": "value-label",
                      "labelField": "drop_list_desc",
                      "valueField": "drop_list_value",
                      "doRequest": {
                        "url": "/SUMP/iccsCall/iccsEurekaCall",
                        "method": "POST",
                        "params": {
                          "servicecode": "ap1071",
                          "drop_list_type": "settlement_type"
                        }
                      }
                    }
                },
                {
                    "type": "string",
                    "label": "指定还款日期",
                    "align": "center",
                    "width": "180px",
                    "prop": "specified_date"
                },
                {
                    "type": "string",
                    "label": "还款方式",
                    "align": "center",
                    "width": "180px",
                    "prop": "pay_type",
                    "dict": {
                      "dictType": "E_PAYTYPE",
                      "dictKey": [
                        "E_PAYTYPE"
                      ],
                      "format": "value-label"
                    }
                },
                {
                    "type": "string",
                    "label": "起始日期",
                    "align": "center",
                    "width": "180px",
                    "prop": "start_date"
                },
                {
                    "type": "string",
                    "label": "终止日期",
                    "align": "center",
                    "width": "180px",
                    "prop": "end_date"
                },
                {
                    "type": "string",
                    "label": "申请人",
                    "align": "center",
                    "width": "180px",
                    "prop": "apply_user",
                    "dict": {
                      "dictKey": "apply_user",
                      "format": "value-label",
                      "labelField": "user_name",
                      "valueField": "user_id",
                      "doRequest": {
                        "url": "/SUMP/iccsCall/iccsEurekaCall",
                        "method": "POST",
                        "params": {
                          "servicecode": "ap1154"
                        }
                      }
                    }
                },
                {
                    "type": "string",
                    "label": "申请时间",
                    "align": "center",
                    "width": "180px",
                    "prop": "apply_time"
                },
                {
                    "type": "string",
                    "label": "申请原因",
                    "align": "center",
                    "width": "180px",
                    "prop": "apply_reason"
                },
                {
                    "type": "string",
                    "label": "审核状态",
                    "align": "center",
                    "width": "180px",
                    "prop": "check_status",
                    "dict": {
                      "dictType": "E_CHECK_STATUS",
                      "dictKey": [
                        "E_CHECK_STATUS"
                      ],
                      "format": "value-label"
                    }
                },
                {
                    "type": "string",
                    "label": "审核人",
                    "align": "center",
                    "width": "180px",
                    "prop": "check_user",
                    "dict": {
                      "dictKey": "check_user",
                      "format": "value-label",
                      "labelField": "user_name",
                      "valueField": "user_id",
                      "doRequest": {
                        "url": "/SUMP/iccsCall/iccsEurekaCall",
                        "method": "POST",
                        "params": {
                          "servicecode": "ap1154"
                        }
                      }
                    }
                },
                {
                    "type": "string",
                    "label": "审核时间",
                    "align": "center",
                    "width": "180px",
                    "prop": "check_time"
                },
                {
                    "type": "string",
                    "label": "审核说明",
                    "align": "center",
                    "width": "180px",
                    "prop": "check_desc"
                }
            ]
        }
    }
}
