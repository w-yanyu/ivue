{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "controls": [
        {
          "apply_reason": {
            "control": "textarea",
            "span": 22,
            "rows":"6",
            "label": "申请原因",
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          }
        }
      ],
      "btns": [
          {
            "type": "closeNavTab",
            "target": "dialog",
            "label": "申请",
            "doRequest": {
              "url": "/SUMP/iccsCall/iccsEurekaCall",
              "method": "POST",
              "params": {
                "servicecode": "pl2739",
                "apply_reason": "${{form.apply_reason}}",
                "plan_amt": "${{params.plan_amt}}",
                "plan_rate":"${{params.plan_rate}}",
                "settlement_type":"${{params.settlement_type}}",
                "start_date": "${{params.start_date}}",
                "end_date":"${{params.end_date}}",
                "specified_date":"${{params.specified_date}}",
                "pay_type":"${{params.pay_type}}",
                "case_id": "${{params.case_id}}"
              }
            }
          },
          {
            "type": "close",
            "label": "返回"
          }
        ]
    }
  }
}
