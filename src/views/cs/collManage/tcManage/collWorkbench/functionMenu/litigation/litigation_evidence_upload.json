{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "scope": "formData",
      "controls": [
        {
          "litigation_stage_code": {
            "align": "left",
            "control": "inputTree",
            "label": "法诉阶段",
            "idField": "litigation_stage_code",
            "nodeTypeField": "type",
            "pIdField": "litigation_parent_stage_code",
            "labelField": "stage_name",
            "nodeTypes": {
              "branchTeam": {
                "icon": "el-icon-news",
                "children": [
                  "branchTeam"
                ]
              }
            },
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0073",
                "query_type": "branchTeam"
              }
            },
            "format": "value-label",
            "rules": [
              {
                "required": true,
                "message": "请选择法诉阶段"
              }
            ]
          },
          "file": {
            "doRequest": {
              "url": "/SUMP/minioCall/upload",
              "method": "post",
              "params": {
                "org_id": "$g{{user_info.org_id}}"
              },
              "resultSet": {
                "file_url": "path",
                "file_name": "oldName",
                "file_size": "size"
              }
            },
            "width": "240px",
            "control": "file",
            "limit": 1,
            "limitSize": 10,
            "label": "上传文件",
            "btnLable": "请选择文件"
          },
          "file_name": {
            "control": "hidden"
          },
          "file_url": {
            "control": "hidden"
          }
        },
        {
          "remark": {
            "label": "备注",
            "control": "textarea",
            "align": "left",
            "autosize": {
              "minRows": 4
            },
            "rules": [
              {
                "max": "200",
                "message": "长度不能超过200个字符"
              }
            ]
          }
        }
      ],
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "cb0297",
              "case_id": "${{params.case_id}}",
              "case_no": "${{params.case_no}}",
              "cust_id": "${{params.cust_id}}",
              "org_id": "${{params.org_id}}",
              "oa_org_id": "${{params.oa_org_id}}"
            }
          }
        },
        {
          "type": "close",
          "label": "取消"
        }
      ]
    }
  }
}