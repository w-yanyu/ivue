{
    "module": "v_search_btn_datagrid",
    "layout": {
      "toolbar": {
        "btns": [
          {
            "type": "add",
            "icon": "el-icon-circle-plus",
            "target": "dialog",
            "label": "新增法诉记录",
            "toRequest": {
                "method": "GET",
                "url": "/views/cs/collManage/tcManage/collWorkbench/functionMenu/litigation/litigation_stage_select.json",
                "params": {
                  "litigation_stage_code":"${{params.litigation_stage_code}}",
                  "case_id":"${{params.case_id}}"
                }
            },
            "props": {
                "width": "85%",
                "title": "字段类型"
            }
          }
        ]
      },
        "datagrid": {
            "isInitSearch": true,
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "isInitSearch": true,
              "params": {
                "servicecode":"cb00105",
                "appId": "cbOnl",
                "coll_kind":"02",
                "case_id":"${{params.case_id}}"
              }
            },
            "thead": [
                {
                    "type":"string",
                    "label": "一级法诉阶段",
                    "align":"center",
                    "prop": "parent_stage_name"
                },
                {
                    "type":"string",
                    "label": "二级法诉阶段",
                    "align":"center",
                    "prop": "stage_name"
                },
                {
                    "type":"string",
                    "label": "登记时间",
                    "align":"center",
                    "prop": "data_create_time"
                  },
                  {
                    "type":"string",
                    "label": "登记用户",
                    "align":"center",
                    "prop": "data_create_user"
                  },
                  {
                      "type":"string",
                      "label": "更新时间",
                      "align":"center",
                      "prop": "data_update_time"
                  },{
                  "label": "操作",
                  "width": "200%",
                  "fixed": "right",
                  "align":"center",
                  "btns": [
                      {
                          "type": "delete",
                          "label": "删除",
                          "doRequest": {
                              "url": "/SUMP/icmcall/icmRPCCall",
                              "method": "POST",
                              "params": {
                                  "servicecode": "cb00104",
                                  "appId": "cbOnl",
                                  "serial_no":"${{current.serial_no}}"
                              }
                          },
                          "confirm": {
                            "title": "提醒",
                            "msg": "确定删除当前法诉记录吗？",
                            "confirmButtonText": "确定",
                            "cancelButtonText": "取消"
                          }
                      },
                      {
                          "type": "edit",
                          "label": "维护",
                          "toRequest": {
                            "method": "GET",
                            "url": "/views/cs/collManage/tcManage/collWorkbench/functionMenu/litigation/litigation_work_update.json",
                            "params": {
                              "serial_no":"${{current.serial_no}}"
                            }
                        }
                      },
                      {
                          "type": "edit",
                          "label": "详情",
                          "toRequest": {
                            "method": "GET",
                            "url": "/views/cs/collManage/tcManage/collWorkbench/functionMenu/litigation/litigation_work_detail_main.json",
                            "params": {
                              "litigation_stage_code":"${{current.litigation_stage_code}}"
                            },
                            "props": {
                              "width": "60%",
                              "title": "详情"
                          }
                        }
                      }
                    ]
             }
          ]

        }
    }
  }

