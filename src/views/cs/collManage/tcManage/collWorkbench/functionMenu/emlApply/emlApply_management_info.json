{
  "module": "v_search_btn_datagrid",
  "layout": {
    "toolbar": {
      "btns": []
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "case_id": "${{params.case_id}}",
          "servicecode": "cb0116",
          "appId": "cbOnl"
        }
      },
      "thead": [
        {
          "type": "string",
          "label": "邮件模板",
          "align": "center",
          "prop": "template_code"
        },
        {
          "type": "string",
          "label": "邮件地址",
          "align": "center",
          "prop": "email_address"
        },
        {
          "type": "string",
          "label": "邮件内容",
          "align": "center",
          "prop": "email_content"
        },
        {
          "type": "string",
          "label": "申请人",
          "align": "center",
          "prop": "apply_user"
        },
        {
          "type": "string",
          "label": "申请时间",
          "align": "center",
          "prop": "apply_time"
        },
        {
          "type": "string",
          "label": "审核状态",
          "align": "center",
          "prop": "approval_status",
          "dict": {
            "dictType": "CS_E_CHECK_STATUS",
            "dictKey": [
              "CS_E_CHECK_STATUS"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "label": "审核人",
          "align": "center",
          "prop": "approval_user"
        },
        {
          "type": "string",
          "label": "审核时间",
          "align": "center",
          "prop": "approval_time"
        },
        {
          "type": "string",
          "label": "审核说明",
          "align": "center",
          "prop": "approval_desc"
        },
        {
          "label": "操作",
          "fixed": "left",
          "align": "center",
          "width": "180px",
          "btns": [
            {
              "type": "delete",
              "label": "撤回",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "pl1522",
                  "apply_id": "${{current.apply_id}}"
                }
              },
              "show": {
                "prop": "approval_status",
                "is": [
                  "NC"
                ]
              }
            }
          ]
        }
      ]
    }
  }
}