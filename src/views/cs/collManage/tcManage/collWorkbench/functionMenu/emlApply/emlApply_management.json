{
  "module": "v-inputDataForm-btn",
  "layout": {
    "scope": "templateScope",
    "form": {
      "btns":[],
      "controls": [
        {
          "top": {
            "control":"dynamic",
            "span":24,
            "toRequest":{
              "url":"/views/cs/collManage/tcManage/collWorkbench/functionMenu/emlApply/emlApply_management_apply.json",
              "method": "post",
              "params": {
                "case_id":"${{params.case_id}}",
                "org_id": "${{params.org_id}}",
                "oa_org_id": "${{params.oa_org_id}}"
              }
            }
          },
          "bottom": {
            "control":"dynamic",
            "span":24,
            "toRequest":{
              "url":"/views/cs/collManage/tcManage/collWorkbench/functionMenu/emlApply/emlApply_management_info.json",
              "method": "post",
              "params": {
                "case_id":"${{params.case_id}}",
                "org_id": "${{params.org_id}}",
                "oa_org_id": "${{params.oa_org_id}}"
              }
            }
          }
        }
      ]
    }
  }
}
