{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "btns": []
    },
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "target": "dialog",
          "label": "新增",
          "toRequest": {
            "method": "GET",
            "url": "/views/cs/collManage/tcManage/collWorkbench/functionMenu/phoneManage/phoneManage_management_add.json",
            "params": {
              "org_id": "${{params.org_id}}",
              "cust_id": "${{params.cust_id}}",
              "case_id": "${{params.case_id}}"
            }
          },
          "props": {
            "title": "新增联系人电话信息",
            "width": "80%"
          }
        },
        {
          "type": "add",
          "target": "dialog",
          "label": "失联修复"
        },
        {
          "type": "close",
          "label": "返回"
        }
      ]
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "cb0091",
          "appId": "cbOnl",
          "case_id": "${{params.case_id}}"
        }
      },
      "thead": [
        {
          "type": "string",
          "label": "联系人姓名",
          "align": "center",
          "prop": "relation_cust_name"
        },
        {
          "type": "string",
          "label": "客户关系",
          "align": "center",
          "prop": "relationship",
          "dict": {
            "dictType": "CS_E_RELATIONSHIP",
            "dictKey": [
              "CS_E_RELATIONSHIP"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "label": "电话类型",
          "align": "center",
          "prop": "phone_type",
          "dict": {
            "dictType": "CS_E_PHONE_TYPE",
            "dictKey": [
              "CS_E_PHONE_TYPE"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "label": "电话号码",
          "align": "center",
          "prop": "phone_no"
        },
        {
          "type": "string",
          "label": "是否有效",
          "align": "center",
          "prop": "is_valid",
          "dict": {
            "dictType": "CS_E_YESORNO",
            "dictKey": [
              "CS_E_YESORNO"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "textarea",
          "label": "备注",
          "align": "center",
          "prop": "remark"
        },
        {
          "label": "操作",
          "fixed": "right",
          "align": "center",
          "btns": [
            {
              "type": "edit",
              "label": "维护",
              "toRequest": {
                "url": "/views/cs/collManage/tcManage/collWorkbench/functionMenu/phoneManage/phoneManage_management_modify.json",
                "method": "POST",
                "params": {
                  "case_id": "${{params.case_id}}",
                  "relation_cust_id": "${{current.relation_cust_id}}"
                }
              },
              "props": {
                "title": "联系人电话维护",
                "width": "80%"
              }
            }
          ]
        }
      ]
    }
  }
}
