{
  "module": "v_realonlyForm_btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "cb0096",
          "case_id": "${{params.case_id}}"
        }
      },
      "controls": [
        {
          "total_overdue_amount": {
            "control": "string",
            "disabled": true,
            "label": "逾期金额",
            "format": {
              "decimal": "2"
            }
          },
          "overdue_principal": {
            "control": "string",
            "disabled": true,
            "label": "逾期本金",
            "format": {
              "decimal": "2"
            }
          },
          "overdue_interest": {
            "control": "string",
            "disabled": true,
            "label": "逾期利息",
            "format": {
              "decimal": "2"
            }
          },
          "overdue_days": {
            "control": "string",
            "disabled": true,
            "label": "逾期天数"
          },
          "total_debt_period": {
            "control": "string",
            "disabled": true,
            "label": "逾期期数"
          }
        }
      ]
    }
  }
}
