{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "btns": []
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "cb0097",
          "loan_no": "${{params.loan_no}}",
          "case_id": "${{params.case_id}}",
          "case_no": "${{params.case_no}}",
          "cust_id": "${{params.cust_id}}",
          "org_id": "${{params.org_id}}",
          "oa_org_id": "${{params.oa_org_id}}"
        }
      },
      "pagination": {
        "pageSizes": [
          5,
          10,
          15,
          20
        ],
        "pageSize": 5,
        "show": true
      },
      "thead": [
        {
          "label": "序号",
          "align": "center",
          "type": "index"
        },
        {
          "type": "string",
          "label": "借据编号",
          "align": "center",
          "width": "180px",
          "prop": "loan_no"
        },
        {
          "type": "string",
          "label": "期数",
          "align": "center",
          "width": "180px",
          "prop": "current_period"
        },
        {
          "type": "string",
          "label": "到期日",
          "align": "center",
          "width": "180px",
          "prop": "current_period_due_date"
        },
        {
          "type": "currency",
          "label": "宽限期供到期日期",
          "align": "center",
          "width": "180px",
          "prop": "grace_period_maturity_date"
        },
        {
          "type": "currency",
          "label": "计息本金",
          "align": "center",
          "width": "180px",
          "prop": "interest_bearing_principal",
          "format": {
            "decimal": "2"
          }
        },
        {
          "type": "currency",
          "label": "期供本息",
          "align": "center",
          "width": "180px",
          "prop": "installment_principal_interest",
          "format": {
            "decimal": "2"
          }
        },
        {
          "type": "currency",
          "label": "期供本金",
          "align": "center",
          "width": "180px",
          "prop": "installment_principal",
          "format": {
            "decimal": "2"
          }
        },
        {
          "type": "currency",
          "label": "期供利息",
          "align": "center",
          "width": "180px",
          "prop": "installment_interest",
          "format": {
            "decimal": "2"
          }
        },
        {
          "type": "string",
          "label": "累计罚息",
          "align": "center",
          "width": "180px",
          "prop": "total_penalty_interest",
          "format": {
            "decimal": "2"
          }
        },
        {
          "type": "currency",
          "label": "累计复利",
          "align": "center",
          "width": "180px",
          "prop": "total_compound_interest",
          "format": {
            "decimal": "2"
          }
        },
        {
          "type": "currency",
          "label": "应计利息",
          "align": "center",
          "width": "180px",
          "prop": "actual_accrued_interest",
          "format": {
            "decimal": "2"
          }
        },
        {
          "type": "currency",
          "label": "应计罚息",
          "align": "center",
          "width": "180px",
          "prop": "actual_accrued_penalty_interest",
          "format": {
            "decimal": "2"
          }
        },
        {
          "type": "string",
          "label": "应计复利",
          "align": "center",
          "width": "180px",
          "prop": "accrual_compound_interest"
        },
        {
          "type": "string",
          "label": "应收利息",
          "align": "center",
          "width": "180px",
          "prop": "accrued_interest_receivable"
        },
        {
          "type": "string",
          "label": "应收罚息",
          "align": "center",
          "width": "180px",
          "prop": "penalty_interest_receivable"
        },
        {
          "type": "string",
          "label": "应收复利",
          "align": "center",
          "width": "180px",
          "prop": "compound_interest_receivable"
        },
        {
          "type": "string",
          "label": "逾期还款费用",
          "align": "center",
          "width": "180px",
          "prop": "overdue_repayment_fee"
        },
        {
          "type": "string",
          "label": "是否结息标志",
          "align": "center",
          "width": "180px",
          "prop": "interest_settled_ind",
          "dict": {
            "dictType": "CS_E_YESORNO",
            "dictKey": [
              "CS_E_YESORNO"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "label": "是否结清",
          "align": "center",
          "width": "180px",
          "prop": "settled_ind",
          "dict": {
            "dictType": "CS_E_YESORNO",
            "dictKey": [
              "CS_E_YESORNO"
            ],
            "format": "value-label"
          }
        }
      ]
    }
  }
}
