{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "cb0098",
          "case_no": "${{params.case_no}}",
          "cust_id": "${{params.cust_id}}",
          "org_id": "${{params.org_id}}",
          "oa_org_id": "${{params.oa_org_id}}",
          "case_id": "${{params.case_id}}"
        }
      },
      "pagination": {
        "pageSizes": [
          5,
          10,
          15,
          20
        ],
        "pageSize": 5,
        "show": true
      },
      "thead": [
        {
          "type": "string",
          "label": "催收时间",
          "align": "center",
          "prop": "coll_time"
        },
        {
          "type": "string",
          "label": "催收员",
          "align": "center",
          "prop": "collector",
          "dict": {
            "dictKey": [
              "collector"
            ],
            "format": "value-label",
            "labelField": "user_name",
            "valueField": "user_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0084",
                "appId": "cbOnl"
              }
            }
          }
        },
        {
          "type": "string",
          "label": "客户关系",
          "align": "center",
          "prop": "relationship",
          "dict": {
            "dictType": "CS_E_RELATIONSHIP",
            "dictKey": [
              "CS_E_RELATIONSHIP"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "label": "联系姓名",
          "align": "center",
          "prop": "relation_cust_name"
        },
        {
          "type": "string",
          "label": "电话号码",
          "align": "center",
          "prop": "phone_no"
        },
        {
          "type": "string",
          "label": "电话状态",
          "align": "center",
          "prop": "phone_status",
          "dict": {
            "dictType": "CS_E_PHONE_STATUS",
            "dictKey": [
              "CS_E_PHONE_STATUS"
            ],
            "format": "value-label"
          }
        },
        {
          "type":"string",
          "label": "内容类型",
          "align":"center",
          "prop": "message_type",
          "dict": {
            "dictType": "CS_E_MESSAGE_TYPE",
            "dictKey": [
              "CS_E_MESSAGE_TYPE"
            ],
            "format": "value-label"
          },
          "btns": [
            {
              "type": "look",
              "show": {
                "expression": "'${{current.message_type}}' === 'VOICE'"
              },
              "toRequest": {
                "url": "@voiceAudio",
                "method": "get",
                "params": {
                  "case_id": "$g{{params.case_id}}"
                }
              },
              "props": {
                "width": "45%",
                "title": "语音详情"
              }
            },
            {
              "type": "look",
              "show": {
                "expression": "'${{current.message_type}}' === 'IVR'"
              },
              "toRequest": {
                "url":"/views/cs/assistManage/caseDetail/messageDetail/ivr_info.json",
                "method": "POST",
                "params": {
                  "case_id": "$g{{params.case_id}}"
                }
              },
              "props": {
                "width": "45%",
                "title": "IVR语音详情"
              }
            },
            {
              "type": "look",
              "show": {
                "expression": "'${{current.message_type}}' === 'SMS'"
              },
              "toRequest": {
                "url":"/views/cs/assistManage/caseDetail/messageDetail/sms_info.json",
                "method": "POST",
                "params": {
                  "case_id": "$g{{params.case_id}}"
                }
              },
              "props": {
                "width": "45%",
                "title": "短信详情"
              }
            },
            {
              "type": "look",
              "show": {
                "expression": "'${{current.message_type}}' === 'EMAIL'"
              },
              "toRequest": {
                "url":"/views/cs/assistManage/caseDetail/messageDetail/email_info.json",
                "method": "POST",
                "params": {
                  "case_id": "$g{{params.case_id}}"
                }
              },
              "props": {
                "width": "45%",
                "title": "邮件详情"
              }
            }
          ]
        },
        {
          "type": "string",
          "label": "催收结果",
          "align": "center",
          "prop": "coll_result",
          "dict": {
            "dictKey": [
              "coll_result"
            ],
            "format": "value-label",
            "labelField": "coll_result_name",
            "valueField": "coll_result_code",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0087",
                "is_valid": "Y",
                "appId": "cbOnl"
              }
            }
          }
        },
        {
          "type": "string",
          "label": "是否有效",
          "align": "center",
          "prop": "is_valid_result",
          "dict": {
            "dictType": "CS_E_YESORNO",
            "dictKey": [
              "CS_E_YESORNO"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "label": "备注",
          "align": "center",
          "prop": "remark"
        }
      ]
    }
  }
}
