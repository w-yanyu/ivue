{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "appId": "cbOnl",
          "servicecode": "cb0085",
          "case_id": "${{params.case_id}}",
          "case_no": "${{params.case_no}}",
          "cust_id": "${{params.cust_id}}",
          "org_id": "${{params.org_id}}",
          "oa_org_id": "${{params.oa_org_id}}"
        }
      },
      "pagination": {
        "pageSizes": [
          5,
          10,
          15,
          20
        ],
        "pageSize": 5,
        "show": true
      },
      "thead": [
        {
          "label": "操作",
          "fixed": "left",
          "align": "center",
          "width": "100px",
          "btns": [
            {
              "type": "custom",
              "label": "查看还款计划",
              "toRequest": {
                "url": "/views/cs/collManage/tcManage/collWorkbench/mainMenu/basic_info/repayPlan.json",
                "method": "get",
                "params": {
                  "loan_no": "${{current.loan_no}}",
                  "case_id": "${{params.case_id}}",
                  "case_no": "${{params.case_no}}",
                  "cust_id": "${{params.cust_id}}",
                  "org_id": "${{params.org_id}}",
                  "oa_org_id": "${{params.oa_org_id}}"
                }
              },
              "props": {
                "width": "80%",
                "title": "还款计划"
              }
            }
          ]
        },
        {
          "type": "string",
          "label": "借据号",
          "align": "center",
          "prop": "loan_no"
        },
        {
          "type": "string",
          "label": "客户编号",
          "align": "center",
          "prop": "cust_id"
        },
        {
          "type": "string",
          "label": "客户姓名",
          "align": "center",
          "prop": "cust_name"
        },
        {
          "type": "string",
          "label": "贷款产品",
          "align": "center",
          "prop": "product_name"
        },
        {
          "type": "string",
          "label": "发放日",
          "align": "center",
          "prop": "disbursement_date"
        },
        {
          "type": "currency",
          "label": "贷款金额",
          "align": "center",
          "prop": "loan_amount",
          "format": {
            "decimal": "2"
          }
        },
        {
          "type": "currency",
          "label": "当前余额",
          "align": "center",
          "prop": "current_balance",
          "format": {
            "decimal": "2"
          }
        },
        {
          "type": "currency",
          "label": "正常余额",
          "align": "center",
          "prop": "normal_balance",
          "format": {
            "decimal": "2"
          }
        },
        {
          "type": "currency",
          "label": "逾期本金",
          "align": "center",
          "prop": "ovdue_principal",
          "format": {
            "decimal": "2"
          }
        },
        {
          "type": "currency",
          "label": "逾期利息",
          "align": "center",
          "prop": "overdue_interest",
          "format": {
            "decimal": "2"
          }
        },
        {
          "type": "currency",
          "label": "逾期余额",
          "align": "center",
          "prop": "overdue_balance",
          "format": {
            "decimal": "2"
          }
        },
        {
          "type": "currency",
          "label": "本金罚息",
          "align": "center",
          "prop": "principal_penalty_interest",
          "format": {
            "decimal": "2"
          }
        },
        {
          "type": "currency",
          "label": "利息罚息",
          "align": "center",
          "prop": "interest_penalty",
          "format": {
            "decimal": "2"
          }
        },
        {
          "type": "currency",
          "label": "逾期金额",
          "align": "center",
          "prop": "total_overdue_amount",
          "format": {
            "decimal": "2"
          }
        },
        {
          "type": "string",
          "label": "本金逾期天数",
          "align": "center",
          "prop": "overdue_days"
        },
        {
          "type": "string",
          "label": "利息逾期天数",
          "align": "center",
          "prop": "oweinterestdays"
        },
        {
          "type": "string",
          "label": "还款帐号",
          "align": "center",
          "prop": "repayment_account"
        },
        {
          "type": "string",
          "label": "贷款用途",
          "align": "center",
          "prop": "loan_purpose",
          "dict": {
            "dictType": "CS_E_LOAN_PURPOSE",
            "dictKey": [
              "CS_E_LOAN_PURPOSE"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "label": "渠道",
          "align": "center",
          "prop": "system_channel_no",
          "dict": {
            "dictType": "CS_E_SYSTEM_CHANNEL_NO",
            "dictKey": [
              "CS_E_SYSTEM_CHANNEL_NO"
            ],
            "format": "value-label"
          }
        }
      ]
    }
  }
}
