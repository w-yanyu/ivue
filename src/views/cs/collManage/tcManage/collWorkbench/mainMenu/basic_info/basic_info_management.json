{
  "module": "dynamicTabs",
  "layout": {
    "tabs": {
      "scope": "formDate",
      "tabHeader": [
        {
          "id": "information",
          "label": "案件基本信息",
          "closable": false,
          "disabled": false,
          "icon": "el-icon-date",
          "toRequest": {
            "url": "/views/cs/collManage/tcManage/collWorkbench/mainMenu/basic_info/baseInfo.json",
            "method": "post",
            "params": {
              "case_id": "${{params.case_id}}",
              "case_no": "${{params.case_no}}",
              "cust_id": "${{params.cust_id}}",
              "org_id": "${{params.org_id}}",
              "oa_org_id": "${{params.oa_org_id}}"
            }
          }
        },
        {
          "id": "individual",
          "label": "客户信息",
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "toRequest": {
            "url": "/views/cs/collManage/tcManage/collWorkbench/mainMenu/basic_info/indivInfo.json",
            "method": "post",
            "params": {
              "case_id": "${{params.case_id}}",
              "case_no": "${{params.case_no}}",
              "cust_id": "${{params.cust_id}}",
              "org_id": "${{params.org_id}}",
              "oa_org_id": "${{params.oa_org_id}}"
            }
          }
        },
        {
          "id": "accountInfo",
          "label": "账户信息",
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "hide": "${{params.ishide1}}",
          "toRequest": {
            "url": "/views/cs/collManage/tcManage/collWorkbench/mainMenu/basic_info/accountInfo.json",
            "method": "post",
            "params": {
              "case_id": "${{params.case_id}}",
              "case_no": "${{params.case_no}}",
              "cust_id": "${{params.cust_id}}",
              "org_id": "${{params.org_id}}",
              "oa_org_id": "${{params.oa_org_id}}"
            }
          }
        },
        {
          "id": "cardInfo",
          "label": "卡片信息",
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "hide": "${{params.ishide1}}",
          "toRequest": {
            "url": "/views/cs/collManage/tcManage/collWorkbench/mainMenu/basic_info/cardInfo.json",
            "method": "post",
            "params": {
              "case_id": "${{params.case_id}}",
              "case_no": "${{params.case_no}}",
              "cust_id": "${{params.cust_id}}",
              "org_id": "${{params.org_id}}",
              "oa_org_id": "${{params.oa_org_id}}"
            }
          }
        },
        {
          "id": "dueBill",
          "label": "借据信息",
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "hide": "${{params.ishide2}}",
          "toRequest": {
            "url": "/views/cs/collManage/tcManage/collWorkbench/mainMenu/basic_info/dueBill.json",
            "method": "post",
            "params": {
              "case_id": "${{params.case_id}}",
              "case_no": "${{params.case_no}}",
              "cust_id": "${{params.cust_id}}",
              "org_id": "${{params.org_id}}",
              "oa_org_id": "${{params.oa_org_id}}"
            }
          }
        },
        {
          "id": "repaymentMsg",
          "label": "还款信息",
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "toRequest": {
            "url": "/views/cs/collManage/tcManage/collWorkbench/mainMenu/basic_info/repaymentInfo.json",
            "method": "post",
            "params": {
              "case_id": "${{params.case_id}}",
              "case_no": "${{params.case_no}}",
              "cust_id": "${{params.cust_id}}",
              "org_id": "${{params.org_id}}",
              "oa_org_id": "${{params.oa_org_id}}"
            }
          }
        },
        {
          "id": "queueCollrec",
          "label": "催记信息",
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "toRequest": {
            "url": "@collrecDetail",
            "method": "get",
            "params": {
              "case_id": "${{params.case_id}}",
              "case_no": "${{params.case_no}}",
              "cust_id": "${{params.cust_id}}",
              "org_id": "${{params.org_id}}",
              "oa_org_id": "${{params.oa_org_id}}"
            }
          }
        },
        {
          "id": "queueRecord",
          "label": "流转信息",
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "toRequest": {
            "url": "/views/cs/collManage/tcManage/collWorkbench/mainMenu/basic_info/queueRecordInfo.json",
            "method": "post",
            "params": {
              "case_id": "${{params.case_id}}",
              "case_no": "${{params.case_no}}",
              "cust_id": "${{params.cust_id}}",
              "org_id": "${{params.org_id}}",
              "oa_org_id": "${{params.oa_org_id}}"
            }
          }
        },
        {
          "id": "litigation",
          "label": "法诉登记",
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "toRequest": {
            "url": "/views/cs/collManage/tcManage/collWorkbench/functionMenu/litigation/litigation_work_main.json",
            "method": "post",
            "params": {
              "case_id": "${{params.case_id}}",
              "case_no": "${{params.case_no}}",
              "cust_id": "${{params.cust_id}}",
              "org_id": "${{params.org_id}}",
              "oa_org_id": "${{params.oa_org_id}}"
            }
          }
        }
      ],
      "defaultActiveKey": "information",
      "position": "top",
      "styleClass": "card"
    }
  }
}
