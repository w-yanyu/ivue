{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "cb0114",
          "case_id": "${{params.case_id}}",
          "case_no": "${{params.case_no}}",
          "cust_id": "${{params.cust_id}}",
          "org_id": "${{params.org_id}}",
          "oa_org_id": "${{params.oa_org_id}}"
        }
      },
      "pagination": {
        "pageSizes": [
          5,
          10,
          15,
          20
        ],
        "pageSize": 5,
        "show": true
      },
      "thead": [
        {
          "type": "currency",
          "label": "还款金额",
          "align": "center",
          "prop": "total_repayment_amount",
          "format": {
            "decimal": "2"
          }
        },
        {
          "type": "string",
          "label": "交易码",
          "align": "center",
          "prop": "trxn_code"
        },
        {
          "type": "string",
          "label": "交易币种",
          "align": "center",
          "prop": "currency_code",
          "dict": {
            "dictType": "MS_CURRENCY",
            "dictKey": [
              "MS_CURRENCY"
            ],
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "label": "交易日期",
          "align": "center",
          "prop": "trxn_date"
        },
        {
          "type": "string",
          "label": "交易时间",
          "align": "center",
          "prop": "tran_timestamp"
        }
      ]
    }
  }
}
