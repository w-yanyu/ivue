{
  "module": "v_realonlyForm_btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "cb0096",
          "case_id": "${{params.case_id}}",
          "case_no": "${{params.case_no}}",
          "cust_id": "${{params.cust_id}}",
          "org_id": "${{params.org_id}}",
          "oa_org_id": "${{params.oa_org_id}}"
        }
      },
      "controls": [
        {
          "case_id": {
            "control": "string",
            "label": "案件编号",
            "span": "6"
          },
          "cust_name": {
            "control": "string",
            "label": "客户姓名",
            "span": "8"
          },
          "sex": {
            "control": "string",
            "label": "性别",
            "span": "8",
            "dict": {
              "dictType": "CS_E_SEX",
              "dictKey": [
                "CS_E_SEX"
              ],
              "format": "value-label"
            }
          },
          "age": {
            "control": "string",
            "disabled": true,
            "label": "年齡",
            "span": "6"
          },
          "cert_no": {
            "control": "string",
            "label": "证件号码",
            "span": "8"
          },
          "loan_amount": {
            "control": "string",
            "disabled": true,
            "label": "贷款金额",
            "span": "8",
            "format": {
              "decimal": "2"
            }
          },
          "overdue_amount": {
            "control": "string",
            "disabled": true,
            "label": "逾期金额",
            "span": "6",
            "format": {
              "decimal": "2"
            }
          },
          "overdue_principal": {
            "control": "string",
            "disabled": true,
            "label": "逾期本金",
            "span": "8",
            "format": {
              "decimal": "2"
            }
          },
          "overdue_interest": {
            "control": "string",
            "disabled": true,
            "label": "逾期利息",
            "span": "8",
            "format": {
              "decimal": "2"
            }
          },
          "overdue_penalty_interest": {
            "control": "string",
            "disabled": true,
            "label": "逾期罚息",
            "span": "6",
            "format": {
              "decimal": "2"
            }
          },
          "overdue_fee": {
            "control": "string",
            "disabled": true,
            "label": "逾期费用",
            "span": "8",
            "format": {
              "decimal": "2"
            }
          },
          "overdue_days": {
            "control": "string",
            "label": "逾期天数",
            "span": "8"
          },
          "total_debt_period": {
            "control": "string",
            "label": "逾期期数",
            "span": "6"
          },
          "case_hands": {
            "control": "string",
            "label": "手别",
            "span": "8"
          },
          "billing_date": {
            "control": "string",
            "label": "账单日",
            "span": "8"
          },
          "outsourcing_org_name": {
            "control": "string",
            "label": "委外公司",
            "span": "6"
          },
          "collector": {
            "control": "string",
            "label": "催收员",
            "span": "8"
          },
          "depute_date": {
            "control": "string",
            "label": "委托时间",
            "span": "8"
          },
          "phone_status": {
            "control": "string",
            "label": "最近电话状态",
            "span": "6",
            "dict": {
              "dictType": "CS_E_PHONE_STATUS",
              "dictKey": [
                "CS_E_PHONE_STATUS"
              ],
              "format": "value-label"
            }
          },
          "coll_result": {
            "control": "string",
            "label": "最近催收结果",
            "span": "8",
            "dict": {
              "dictKey": [
                "coll_result"
              ],
              "format": "value-label",
              "labelField": "coll_result_name",
              "valueField": "coll_result_code",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "cb0087",
                  "is_valid": "Y",
                  "appId": "cbOnl"
                }
              }
            }
          },
          "cust_label": {
            "control": "string",
            "label": "客户标签",
            "span": "8"
          },
          "case_score": {
            "control": "string",
            "label": "案件评分",
            "value":"86",
            "span": "6"
          },
          "is_merger_loan": {
            "control": "string",
            "label": "是否逾期多笔",
            "span": "8",
            "dict": {
              "dictType": "CS_E_YESORNO",
              "dictKey": [
                "CS_E_YESORNO"
              ],
              "format": "value-label"
            }
          }
        }
      ]
    }
  }
}
