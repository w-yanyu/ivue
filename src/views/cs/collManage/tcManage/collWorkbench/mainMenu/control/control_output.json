{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "btns": [
        {
          "type": "closeNavTab",
          "label": "申请",
          "doRequest": {
            "method": "POST",
            "url": "/SUMP/icmcall/icmRPCCall",
            "params": {
              "servicecode": "cb0111",
              "assist_type": "01",
              "case_id": "${{params.case_id}}",
              "case_no": "${{params.case_no}}",
              "cust_id": "${{params.cust_id}}",
              "org_id": "${{params.org_id}}",
              "oa_org_id": "${{params.oa_org_id}}",
              "queue_code": "${{form.queue_code}}",
              "apply_reason": "${{form.apply_reason}}",
              "coll_type": "TC"
            }
          },
          "#eventAfter": {
            "doParentCallback": true
          }
        },
        {
          "type": "close",
          "label": "返回"
        }
      ],
      "controls": [
        {
          "now_queue": {
            "control": "remoteSelect",
            "label": "当前队列",
            "width": "180px",
            "align": "center",
            "span": "20",
            "rows": 6,
            "default": "${{params.queue_code}}",
            "disabled": true,
            "labelField": "queue_name",
            "valueField": "queue_code",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0082",
                "coll_type": "TC",
                "queue_type": "C",
                "is_valid": "Y"
              }
            },
            "format": "value-label",
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          },
          "queue_code": {
            "control": "remoteSelect",
            "label": "可转队列",
            "width": "180px",
            "align": "center",
            "span": "20",
            "rows": 6,
            "labelField": "queue_name",
            "valueField": "queue_code",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0082",
                "coll_type": "TC",
                "queue_type": "C",
                "is_valid": "Y"
              }
            },
            "format": "value-label",
            "rules": [
              {
                "expression": "'${{form.queue_code}}' !== '${{form.now_queue}}'",
                "message": "不能转相同队列！"
              },
              {
                "required": true,
                "message": "请输入"
              }
            ]
          },
          "apply_reason": {
            "control": "textarea",
            "label": "转出原因",
            "width": "180px",
            "align": "center",
            "span": "20",
            "rows": 12,
            "placeholder": "请输入",
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          }
        }
      ]
    }
  }
}
