{
  "module": "v_search_btn_datagrid",
  "pageId": "page001",
  "layout": {
    "form": {
      "foldLineNumber": 3,
      "scope": "formData",
      "controls": [
        {
          "branch_id": {
            "label": "创建机构",
            "control": "remoteSelect",
            "labelField": "branch_name",
            "valueField": "branch_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2142",
                "user_id": "$g{{user_info.userCode}}",
                "branch_id": "$g{{user_info.branch_id}}",
                "brch_relation_code": "01"
              }
            },
            "format": "value-label"
          },
          "data_create_user": {
            "label": "创建人",
            "control": "remoteSelect",
            "labelField": "user_name",
            "valueField": "user_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0084"
              }
            },
            "format": "value-label"
          }
        },
        {
          "tran_chinese_name": {
            "control": "text",
            "label": "批量任务名称"
          },
          "template_code": {
            "control": "text",
            "label": "模板编号"
          }
        },
        {
          "daterange": {
            "control": "dateTimePicker",
            "label": "生成时间",
            "type": "daterange",
            "unlinkPanels": true,
            "valueFormat": "yyyyMMdd",
            "startPlaceholder": "请选择开始时间",
            "endPlaceholder": "请选择结束时间",
            "startField": "start_date",
            "endField": "end_date"
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "appId": "cbOnl",
          "servicecode": "cb0284"
        }
      },
      "sortDb": true,
      "thead": [
        {
          "type": "string",
          "align": "center",
          "label": "任务编号",
          "prop": "task_id"
        },
        {
          "type": "string",
          "align": "center",
          "label": "创建法人",
          "dict": {
            "dictKey": [
              "org_id"
            ],
            "labelField": "org_name",
            "valueField": "org_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2311",
                "org_id": "${{current.org_id}}"
              }
            },
            "format": "value-label"
          },
          "prop": "org_id"
        },
        {
          "type": "string",
          "align": "center",
          "label": "创建机构",
          "dict": {
            "dictKey": [
              "branch_id"
            ],
            "labelField": "branch_name",
            "valueField": "branch_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2142",
                "branch_id": "${{current.branch_id}}"
              }
            },
            "format": "value-label"
          },
          "prop": "branch_id"
        },
        {
          "type": "string",
          "align": "center",
          "label": "模板代码",
          "prop": "template_code"
        },
        {
          "type": "string",
          "align": "center",
          "label": "批量交易码",
          "prop": "tran_code"
        },
        {
          "type": "string",
          "align": "center",
          "label": "批量任务名称",
          "prop": "tran_chinese_name"
        },
        {
          "type": "string",
          "align": "center",
          "label": "案件总数量",
          "prop": "total_count"
        },
        {
          "type": "string",
          "align": "center",
          "label": "生成时间",
          "prop": "data_create_time"
        },
        {
          "label": "操作",
          "align": "center",
          "fixed": "right",
          "width": "100%",
          "btns": [
            {
              "type": "look",
              "label": "批次任务详情",
              "toRequest": {
                "url": "/views/cs/collManage/tcManage/litigationFile/litigation_file_info.json",
                "method": "POST",
                "params": {
                  "task_id": "${{current.task_id}}"
                }
              },
              "props": {
                "title": "批次任务详情"
              }
            }
          ]
        }
      ]
    }
  }
}