                    "dict": {
                        "dictKey": [
                            "agency_collector"
                        ],
                        "format": "value-label",
                        "labelField": "user_name",
                        "valueField": "user_id",
                        "doRequest": {
                            "url": "/SUMP/icmcall/icmRPCCall",
                            "method": "POST",
