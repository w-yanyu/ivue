{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "scope": "formData",
      "initRequest": "${{params}}",
      "btns": [
        {
          "type": "submit",
          "label": "确认",
          "doRequest": {
            "method": "POST",
            "url": "/SUMP/icmcall/icmRPCCall",
            "params": {
              "servicecode": "cb0191",
              "appId": "cbOnl",
              "branch_id": "${{params.branch_id}}",
              "case_id": "${{params.case_id}}",
              "cust_name": "${{params.cust_name}}",
              "cert_no": "${{params.cert_no}}",
              "cert_type": "${{params.cert_type}}",
              "overdue_days": "${{params.overdue_days}}",
              "approval_status": "${{params.approval_status}}",
              "queue_code": "${{params.queue_code}}",
              "classify_code": "${{params.classify_code}}",
              "case_status": "${{params.case_status}}",
              "stage_code": "${{params.stage_code}}",
              "start_date": "${{params.start_date}}",
              "end_date": "${{params.end_date}}",
              "ovd_amt_total_1": "${{params.ovd_amt_total_1}}",
              "ovd_amt_total_2": "${{params.ovd_amt_total_2}}",
              "collect_date_1": "${{params.collect_date_1}}",
              "collect_date_2": "${{params.collect_date_2}}"
            }
          }
        }
      ],
      "controls": [
        {
          "approval_desc": {
            "control": "textarea",
            "label": "审核说明",
            "width": "180px",
            "align": "center",
            "span": "20",
            "rows": 6,
            "placeholder": "请输入",
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          }
        }
      ]
    }
  }
}
