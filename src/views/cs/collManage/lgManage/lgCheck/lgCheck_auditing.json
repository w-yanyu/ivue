{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "btns": [
        {
          "type": "submit",
          "icon": "el-icon-check",
          "label": "核准",
          "doRequest": {
            "method": "POST",
            "url": "/SUMP/icmcall/icmRPCCall",
            "params": {
              "servicecode": "cb0192",
              "approval_status": "P",
              "approval_desc": "${{params.approval_desc}}",
              "list01": "${{params.selectedData}}"
            }
          }
        },
        {
          "type": "submit",
          "icon": "el-icon-close",
          "label": "拒绝",
          "doRequest": {
            "method": "POST",
            "url": "/SUMP/icmcall/icmRPCCall",
            "params": {
              "servicecode": "pl4665",
              "apply_id": "${{params.selectedData}}"
            }
          }
        }
      ],
      "controls": [
        {
          "approval_desc": {
            "control": "textarea",
            "label": "审核说明",
            "width": "180px",
            "align": "center",
            "span": "20",
            "rows": 6,
            "placeholder": "请输入",
            "rules": [
              {
                "required": true
              }
            ]
          }
        }
      ]
    }
  }
}
