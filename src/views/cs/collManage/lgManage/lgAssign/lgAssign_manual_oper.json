{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "btns": [
        {
          "type": "submit",
          "icon": "el-icon-check",
          "label": "确认",
          "doRequest": {
            "method": "POST",
            "url": "/SUMP/icmcall/icmRPCCall",
            "params": {
              "servicecode": "cb0194",
              "case_id": "${{params.selectedData}}"
            }
          }
        },
        {
          "type": "close",
          "label": "取消"
        }
      ],
      "controls": [
        {
          "oa_org_id": {
            "hidden": true,
            "label": "法诉公司",
            "control": "remoteSelect",
            "labelField": "outsourcing_org_name",
            "valueField": "outsourcing_org_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0052",
                "length": 100,
                "outsourcing_org_type": "2",
                "org_id": "$g{{user_info.org_id}}"
              }
            },
            "events": {
              "emit": [
                "operation"
              ]
            },
            "format": "value-label",
            "rules": [
              {
                "required": true,
                "message": "请选择法诉公司"
              }
            ]
          },
          "commission_plan_id": {
            "control": "text",
            "label": "佣金方案编号"
          },
          "start_date": {
            "label": "委托起始日",
            "control": "dateTimePicker",
            "span": 12,
            "format": "yyyy-MM-dd"
          },
          "end_date": {
            "label": "委托结束日",
            "control": "dateTimePicker",
            "span": 12,
            "format": "yyyy-MM-dd"
          }
        }
      ]
    }
  }
}
