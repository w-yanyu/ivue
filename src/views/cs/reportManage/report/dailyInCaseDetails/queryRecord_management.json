{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "scope": "formData",
      "controls": [
        {
          "report_date": {
            "control": "dateTimePicker",
            "valueFormat": "yyyyMMdd",
            "label": "统计时间"
          },
          "source_org_id": {
            "label": "案件所属法人",
            "control": "remoteSelect",
            "labelField": "org_name",
            "valueField": "org_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2311"
              }
            },
            "format": "value-label"
          }
        },
        {
          "source_branch_id": {
            "label": "案件所属机构",
            "control": "remoteSelect",
            "labelField": "branch_name",
            "valueField": "branch_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2142",
                "user_id": "$g{{user_info.userCode}}",
                "brch_relation_code": "01"
              }
            },
            "format": "value-label"
          },
          "oa_org_id": {
            "hidden": true,
            "label": "委外公司",
            "control": "remoteSelect",
            "labelField": "outsourcing_org_name",
            "valueField": "outsourcing_org_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0052",
                "org_id": "$g{{user_info.org_id}}"
              }
            },
            "events": {
              "emit": [
                "operation"
              ]
            },
            "format": "value-label"
          }
        }
      ]
    },
    "toolbar": {
      "btns": []
    },
    "datagrid": {
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "cb0181"
        }
      },
      "thead": [
        {
          "type": "string",
          "align": "center",
          "label": "统计日期",
          "prop": "report_date"
        },
        {
          "type": "string",
          "align": "center",
          "label": "客户姓名",
          "prop": "cust_name"
        },
        {
          "type": "string",
          "align": "center",
          "label": "证件号码",
          "prop": "cert_no"
        },
        {
          "type": "string",
          "align": "center",
          "label": "催收类型",
          "dict": {
            "dictType": "CS_COLL_TYPE",
            "dictKey": [
              "CS_COLL_TYPE"
            ],
            "format": "value-label"
          },
          "prop": "coll_type"
        },
        {
          "type": "string",
          "align": "center",
          "label": "委外公司",
          "dict": {
            "dictKey": [
              "outsourcing_org_id"
            ],
            "labelField": "outsourcing_org_name",
            "valueField": "outsourcing_org_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0052",
                "outsourcing_org_id": "${{current.oa_org_id}}"
              }
            },
            "format": "value-label"
          },
          "prop": "oa_org_id"
        },
        {
          "type": "string",
          "align": "center",
          "label": "催收机构",
          "dict": {
            "dictKey": [
              "branch_id"
            ],
            "labelField": "branch_name",
            "valueField": "branch_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2142",
                "branch_id": "${{current.branch_id}}"
              }
            },
            "format": "value-label"
          },
          "prop": "branch_id"
        },
        {
          "type": "string",
          "align": "center",
          "label": "催收组",
          "prop": "coll_team",
          "dict": {
            "dictKey": [
              "coll_team"
            ],
            "format": "value-label",
            "labelField": "coll_team_name",
            "valueField": "coll_team",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0041",
                "appId": "cbOnl"
              }
            }
          }
        },
        {
          "type": "string",
          "align": "center",
          "label": "入催时间",
          "prop": "collect_date"
        },
        {
          "type": "string",
          "align": "center",
          "label": "账户未偿金额(元)",
          "format": {
            "thousand": ","
          },
          "prop": "outstanding_amt"
        },
        {
          "type": "string",
          "align": "center",
          "label": "最低还款额(元)",
          "format": {
            "thousand": ","
          },
          "prop": "min_payamt"
        },
        {
          "type": "string",
          "align": "center",
          "label": "案件所属法人",
          "dict": {
            "dictKey": [
              "source_org_id"
            ],
            "labelField": "org_name",
            "valueField": "org_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2311",
                "org_id": "${{current.source_org_id}}"
              }
            },
            "format": "value-label"
          },
          "prop": "source_org_id"
        },
        {
          "type": "string",
          "align": "center",
          "label": "案件所属机构",
          "dict": {
            "dictKey": [
              "source_branch_id"
            ],
            "labelField": "branch_name",
            "valueField": "branch_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2142",
                "branch_id": "${{current.source_branch_id}}"
              }
            },
            "format": "value-label"
          },
          "prop": "source_branch_id"
        }
      ]
    }
  }
}