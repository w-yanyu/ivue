{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "scope": "formData",
      "controls": [
        {
          "report_date": {
            "control": "dateTimePicker",
            "valueFormat": "yyyyMMdd",
            "label": "统计时间"
          },"source_org_id": {
            "label": "案件所属法人",
            "control": "remoteSelect",
            "labelField": "org_name",
            "valueField": "org_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2311",
                "org_id": "$g{{user_info.org_id}}"
              }
            },
            "format": "value-label"
          }
        },{
          "source_branch_id": {
            "label": "案件所属机构",
            "control": "remoteSelect",
            "labelField": "branch_name",
            "valueField": "branch_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2142",
                "user_id": "$g{{user_info.userCode}}"
              }
            },
            "format": "value-label"
          },"oa_org_id": {
            "hidden": true,
            "label": "委外公司",
            "control": "remoteSelect",
            "labelField": "outsourcing_org_name",
            "valueField": "outsourcing_org_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0052",
                "org_id": "$g{{user_info.org_id}}"
              }
            },
            "events": {
              "emit": [
                "operation"
              ]
            },
            "format": "value-label"
          }
        }
      ]
    },
    "toolbar": {
      "btns": [

      ]
    },
    "datagrid": {
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "cb0183"
        }
      },
      "thead": [
        {
          "type": "string",
          "align": "center",
          "label": "统计日期",
          "prop": "report_date"
        },
        {
          "type": "string",
          "align": "center",
          "label": "委外公司",
          "width":150,
          "dict": {
            "dictKey": [
              "outsourcing_org_id"
            ],
            "labelField": "outsourcing_org_name",
            "valueField": "outsourcing_org_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0052",
                "outsourcing_org_id": "${{current.oa_org_id}}"
              }
            },
            "format": "value-label"
          },
          "prop": "oa_org_id"

        },{
          "type": "string",
          "align": "center",
          "label": "催收机构",
          "width":100,
          "dict": {
            "dictKey": [
              "branch_id"
            ],
            "labelField": "branch_name",
            "valueField": "branch_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2142",
                "branch_id": "${{current.branch_id}}"
              }
            },
            "format": "value-label"
          },
          "prop": "branch_id"
        },
        {
          "type": "string",
          "align": "center",
          "label": "催收组",
          "prop": "coll_team"
        },
        {
          "type": "string",
          "align": "center",
          "label": "当天案件处理数",
          "prop": "deal_cnt"
        },
        {
          "type": "string",
          "align": "center",
          "label": "当天完成案件金额(元)",
          "format": {
            "thousand": ","
          },
          "prop": "finish_amt"
        },
        {
          "type": "string",
          "align": "center",
          "label": "案件总金额(元)",
          "format": {
            "thousand": ","
          },
          "prop": "total_case_amt"
        },
        {
          "type": "string",
          "align": "center",
          "label": "案件处理总数",
          "prop": "total_case"
        },
        {
          "type": "string",
          "align": "center",
          "label": "Ptp次数",
          "prop": "ptp_cnt"
        },
        {
          "type": "string",
          "align": "center",
          "label": "Ptp百分比(%)  ",
          "prop": "ptp_rate"
        },
        {
          "type": "string",
          "align": "center",
          "label": "迁徙率(%)",
          "prop": "move_rate"
        }
      ]
    }
  }
}
