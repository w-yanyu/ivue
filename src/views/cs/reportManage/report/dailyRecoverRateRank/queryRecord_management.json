{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "scope": "formData",
      "controls": [
        {
          "report_date": {
            "control": "dateTimePicker",
            "valueFormat": "yyyyMMdd",
            "label": "统计时间"
          },"source_org_id": {
            "label": "案件所属法人",
            "control": "remoteSelect",
            "labelField": "org_name",
            "valueField": "org_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2311",
                "org_id": "$g{{user_info.org_id}}"
              }
            },
            "format": "value-label"
          }
        },{
          "source_branch_id": {
            "label": "案件所属机构",
            "control": "remoteSelect",
            "labelField": "branch_name",
            "valueField": "branch_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2142",
                "user_id": "$g{{user_info.userCode}}"
              }
            },
            "format": "value-label"
          },"oa_org_id": {
            "hidden": true,
            "label": "委外公司",
            "control": "remoteSelect",
            "labelField": "outsourcing_org_name",
            "valueField": "outsourcing_org_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0052",
                "org_id": "$g{{user_info.org_id}}"
              }
            },
            "events": {
              "emit": [
                "operation"
              ]
            },
            "format": "value-label"
          }
        }
      ]
    },
    "toolbar": {
      "btns": [

      ]
    },
    "datagrid": {
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "cb0184"
        }
      },
      "thead": [
        {
          "type": "string",
          "align": "center",
          "label": "统计日期",
          "prop": "report_date"
        },
        {
          "type": "string",
          "align": "center",
          "label": "委外公司",
          "width":150,
          "dict": {
            "dictKey": [
              "outsourcing_org_id"
            ],
            "labelField": "outsourcing_org_name",
            "valueField": "outsourcing_org_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0052",
                "outsourcing_org_id": "${{current.oa_org_id}}"
              }
            },
            "format": "value-label"
          },
          "prop": "oa_org_id"

        },{
          "type": "string",
          "align": "center",
          "label": "催收机构",
          "width":100,
          "dict": {
            "dictKey": [
              "branch_id"
            ],
            "labelField": "branch_name",
            "valueField": "branch_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2142",
                "branch_id": "${{current.branch_id}}"
              }
            },
            "format": "value-label"
          },
          "prop": "branch_id"
        },
        {
          "type": "string",
          "align": "center",
          "label": "催收组",
          "width": 100,
          "prop": "coll_team"
        },
        {
          "type": "string",
          "align": "center",
          "label": "催收员",
          "width": 100,
          "prop": "collector"
        },
        {
          "type": "string",
          "align": "center",
          "label": "入催时贷款余额(元)",
          "format": {
            "thousand": ","
          },
          "prop": "collect_loan_balance"
        },
        {
          "type": "string",
          "align": "center",
          "label": "当日回收金额(元)",
          "format": {
            "thousand": ","
          },
          "prop": "day_pmt_amt"
        },
        {
          "type": "string",
          "align": "center",
          "label": "累计还款金额(元)",
          "format": {
            "thousand": ","
          },
          "prop": "pmt_total_amt"
        },
        {
          "type": "string",
          "align": "center",
          "label": "回收率(%)",
          "prop": "recycle_rate"
        },
        {
          "type": "string",
          "align": "center",
          "label": "排名",
          "prop": "recycle_rank"
        },
        {
          "type": "string",
          "align": "center",
          "label": "当日回收利息(元)",
          "format": {
            "thousand": ","
          },
          "prop": "day_pmt_amt2"
        },
        {
          "type": "string",
          "align": "center",
          "label": "当日回收本金(逾期)(元)",
          "format": {
            "thousand": ","
          },
          "prop": "day_pmt_amt3"
        },
        {
          "type": "string",
          "align": "center",
          "label": "当日回收本金(正常)(元)",
          "format": {
            "thousand": ","
          },
          "prop": "day_pmt_amt4"
        },
        {
          "type": "string",
          "align": "center",
          "label": "当日回收表内欠息(元)",
          "format": {
            "thousand": ","
          },
          "prop": "day_pmt_amt5"
        },
        {
          "type": "string",
          "align": "center",
          "label": "当日回收表外欠息(元)",
          "format": {
            "thousand": ","
          },
          "prop": "day_pmt_amt6"
        }
      ]
    }
  }
}
