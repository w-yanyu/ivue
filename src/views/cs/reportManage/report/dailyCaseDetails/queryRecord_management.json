{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "scope": "formData",
      "controls": [
        {
          "report_date": {
            "control": "dateTimePicker",
            "valueFormat": "yyyyMMdd",
            "label": "统计时间"
          },"source_org_id": {
            "label": "案件所属法人",
            "control": "remoteSelect",
            "labelField": "org_name",
            "valueField": "org_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2311",
                "org_id": "$g{{user_info.org_id}}"
              }
            },
            "format": "value-label"
          }
        },{
          "source_branch_id": {
            "label": "案件所属机构",
            "control": "remoteSelect",
            "labelField": "branch_name",
            "valueField": "branch_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2142",
                "user_id": "$g{{user_info.userCode}}"
              }
            },
            "format": "value-label"
          },"oa_org_id": {
            "hidden": true,
            "label": "委外公司",
            "control": "remoteSelect",
            "labelField": "outsourcing_org_name",
            "valueField": "outsourcing_org_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0052",
                "org_id": "$g{{user_info.org_id}}"
              }
            },
            "events": {
              "emit": [
                "operation"
              ]
            },
            "format": "value-label"
          }
        }
      ]
    },
    "toolbar": {
      "btns": [

      ]
    },
    "datagrid": {
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "cb0180"
        }
      },
      "thead": [
        {
          "type": "string",
          "align": "center",
          "label": "统计日期",
          "prop": "report_date"
        },
        {
          "type": "string",
          "align": "center",
          "label": "客户姓名",
          "prop": "cust_name"
        },
        {
          "type": "string",
          "align": "center",
          "label": "客户编号",
          "width":100,
          "prop": "cust_id"
        },
        {
          "type": "string",
          "align": "center",
          "label": "逾期天数",
          "prop": "over_due_day"
        },
        {
          "type": "string",
          "align": "center",
          "label": "入催时贷款余额(元)",
          "format": {
            "thousand": ","
          },
          "prop": "collect_loan_balance"
        },
        {
          "type": "string",
          "align": "center",
          "label": "入组时间",
          "prop": "entry_team_time"
        },
        {
          "type": "string",
          "align": "center",
          "label": "入组金额(元)",
          "format": {
            "thousand": ","
          },
          "prop": "entry_team_amt"
        },
        {
          "type": "string",
          "align": "center",
          "label": "案件类型",
          "prop": "classify_name"
        },
        {
          "type": "string",
          "align": "center",
          "width":150,
          "label": "委外公司",
          "dict": {
            "dictKey": [
              "outsourcing_org_id"
            ],
            "labelField": "outsourcing_org_name",
            "valueField": "outsourcing_org_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0052",
                "outsourcing_org_id": "${{current.oa_org_id}}"
              }
            },
            "format": "value-label"
          },
          "prop": "oa_org_id"

        },
        {
          "type": "string",
          "align": "center",
          "label": "催收机构",
          "width":100,
          "dict": {
            "dictKey": [
              "branch_id"
            ],
            "labelField": "branch_name",
            "valueField": "branch_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2142",
                "branch_id": "${{current.branch_id}}"
              }
            },
            "format": "value-label"
          },
          "prop": "branch_id"
        },
        {
          "type": "string",
          "align": "center",
          "label": "催收组",
          "prop": "coll_team"
        },
        {
          "type": "string", 
          "align": "center",
          "label": "催收员",
          "width":100,
          "prop": "collector"
        },
        {
          "type": "string",
          "align": "center",
          "label": "逾期阶段",
          "prop": "stage_name"
        },
        {
          "type": "string",
          "align": "center",
          "label": "存量金额(元)",
          "format": {
            "thousand": ","
          },
          "prop": "stock_amt"
        },
        {
          "type": "string",
          "align": "center",
          "label": "回款金额(元)",
          "format": {
            "thousand": ","
          },
          "prop": "guarn_pay"
        },
        {
          "type": "string",
          "align": "center",
          "label": "带走金额(元)",
          "format": {
            "thousand": ","
          },
          "prop": "taken_amt"
        },
        {
          "type": "string",
          "align": "center",
          "label": "案件所属法人",
          "dict": {
            "dictKey": [
              "source_org_id"
            ],
            "labelField": "org_name",
            "valueField": "org_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2311",
                "org_id": "${{current.source_org_id}}"
              }
            },
            "format": "value-label"
          },
          "prop": "source_org_id"
        },
        {
          "type": "string",
          "align": "center",
          "label": "案件所属机构",
          "dict": {
            "dictKey": [
              "source_branch_id"
            ],
            "labelField": "branch_name",
            "valueField": "branch_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2142",
                "branch_id": "${{current.source_branch_id}}"
              }
            },
            "format": "value-label"
          },
          "prop": "source_branch_id"
        }
      ]
    }
  }
}
