{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "scope": "formData",
      "controls": [
        {
          "start_date": {
            "control": "dateTimePicker",
            "valueFormat": "yyyyMMdd",
            "label": "起始日期"
          },"end_date": {
            "control": "dateTimePicker",
            "valueFormat": "yyyyMMdd",
            "label": "结束日期"
          }
        },{
          "source_org_id": {
            "label": "案件所属法人",
            "control": "remoteSelect",
            "labelField": "org_name",
            "valueField": "org_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2311",
                "org_id": "$g{{user_info.org_id}}"
              }
            },
            "format": "value-label"
          },"source_branch_id": {
            "label": "案件所属机构",
            "control": "remoteSelect",
            "labelField": "branch_name",
            "valueField": "branch_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2142",
                "user_id": "$g{{user_info.userCode}}"
              }
            },
            "format": "value-label"
          }

        },{
          "oa_org_id": {
            "label": "委外公司",
            "control": "remoteSelect",
            "labelField": "outsourcing_org_name",
            "valueField": "outsourcing_org_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0052",
                "org_id": "$g{{user_info.org_id}}"
              }
            },
            "events": {
              "emit": [
                "operation"
              ]
            },
            "format": "value-label"
          }
        }
      ]
    },
    "toolbar": {
      "btns": [

      ]
    },
    "datagrid": {
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "cb0186"
        }
      },
      "thead": [
        {
          "type": "string",
          "align": "center",
          "label": "统计日期",
          "prop": "report_date"
        },
        {
          "type": "string",
          "align": "center",
          "label": "委外公司",
          "width":150,
          "dict": {
            "dictKey": [
              "outsourcing_org_id"
            ],
            "labelField": "outsourcing_org_name",
            "valueField": "outsourcing_org_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0052",
                "outsourcing_org_id": "${{current.oa_org_id}}"
              }
            },
            "format": "value-label"
          },
          "prop": "oa_org_id"

        },  
        {
          "type": "string",
          "align": "center",
          "label": "短信发送量",
          "prop": "send_count"
        },
        {
          "type": "string",
          "align": "center",
          "label": "外呼量",
          "prop": "out_call_count"
        },
        {
          "type": "string",
          "align": "center",
          "label": "外呼时长(秒)",
          "prop": "out_call_seconds"
        },
        {
          "type": "string",
          "align": "center",
          "label": "短信成本金额(元)",
          "format": {
            "thousand": ","
          },
          "prop": "message_cost_amt"
        },
        {
          "type": "string",
          "align": "center",
          "label": "外呼成本金额(元)",
          "format": {
            "thousand": ","
          },
          "prop": "out_call_cost_amt"
        },
        {
          "type": "string",
          "align": "center",
          "label": "案件所属法人",
          "dict": {
            "dictKey": [
              "source_org_id"
            ],
            "labelField": "org_name",
            "valueField": "org_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2311",
                "org_id": "${{current.source_org_id}}"
              }
            },
            "format": "value-label"
          },
          "prop": "source_org_id"
        },
        {
          "type": "string",
          "align": "center",
          "label": "案件所属机构",
          "dict": {
            "dictKey": [
              "source_branch_id"
            ],
            "labelField": "branch_name",
            "valueField": "branch_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2142",
                "branch_id": "${{current.source_branch_id}}"
              }
            },
            "format": "value-label"
          },
          "prop": "source_branch_id"
        }
      ]
    }
  }
}
