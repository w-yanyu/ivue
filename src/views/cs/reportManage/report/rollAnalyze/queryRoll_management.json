{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "scope": "formData",
      "controls": [
        {
          "start_date": {
            "control": "dateTimePicker",
            "valueFormat": "yyyyMMdd",
            "label": "起始日期"
          },"end_date": {
            "control": "dateTimePicker",
            "valueFormat": "yyyyMMdd",
            "label": "结束日期"
          }
        },{
          "source_org_id": {
            "label": "案件所属法人",
            "control": "remoteSelect",
            "labelField": "org_name",
            "valueField": "org_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2311",
                "org_id": "$g{{user_info.org_id}}"
              }
            },
            "format": "value-label"
          },"source_branch_id": {
            "label": "案件所属机构",
            "control": "remoteSelect",
            "labelField": "branch_name",
            "valueField": "branch_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2142",
                "user_id": "$g{{user_info.userCode}}"
              }
            },
            "format": "value-label"
          }

        },{
          "statistical_frequency":{
            "control": "select",
            "label": "报表频度",
            "dict": {
                "dictType": "MS_E_CYCLETYPE",
                "dictKey": [
                    "MS_E_CYCLETYPE"
                ],
                "format": "value-label"
            }
          },
          "oa_org_id": {
            "label": "委外公司",
            "control": "remoteSelect",
            "labelField": "outsourcing_org_name",
            "valueField": "outsourcing_org_id",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "cb0052",
                "org_id": "$g{{user_info.org_id}}"
              }
            },
            "events": {
              "emit": [
                "operation"
              ]
            },
            "format": "value-label"
          }
        }
      ]
    },
    "toolbar": {
      "btns": [

      ]
    },
    "datagrid": {
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
