{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "appl_id": {
            "label": "申请编号",
            "control": "text"
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ic1126"
        }
      },
      "thead": [
        {
          "label": "申请编号",
          "width": "160px",
          "prop": "appl_id",
          "align": "center"
        },
        {
          "label": "流程节点实例",
          "width": "160px",
          "prop": "task_inst_id",
          "align": "center"
        },
        {
          "label": "主节点id",
          "prop": "parent_id",
          "align": "center"
        },
        {
          "label": "流程实例编号",
          "prop": "proc_inst_id",
          "align": "center",
          "width": "160px"
        },
        {
          "label": "流程产品编号",
          "prop": "wfprod_cd",
          "align": "center",
          "width": "160px"
        },
        {
          "label": "流程节点编号",
          "prop": "task_def_name",
          "align": "center",
          "width": "160px"
        },
        {
          "label": "进入时间",
          "prop": "enter_time",
          "align": "center",
          "width": "160px"
        },
        {
          "label": "处理时间",
          "prop": "deal_time",
          "align": "center",
          "width": "160px"
        },
        {
          "label": "处理人",
          "prop": "appv_user",
          "align": "center",
          "width": "160px"
        },
        {
          "label": "审批结果",
          "prop": "appv_code",
          "align": "center",
          "width": "160px",
          "dict": {
            "dictType": "BPM_E_SIGN_RESULT",
            "dictKey": [
                "BPM_E_SIGN_RESULT"
            ]
          },
          "format": "label"
        },
        {
          "label": "是上次审批结果",
          "prop": "last_appv_flag",
          "align": "center",

          "width": "160px",
          "dict": {
            "dictType": "E_YES___",
            "dictKey": [
                "E_YES___"
            ]
          },
          "format": "label"
        },
        {
          "label": "审批结果描述",
          "prop": "appv_desc",
          "align": "center",
          "width": "160px"
        }
      ]
    }
  }
}
