{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "controls": [
                {
                    "idtfno": {
                        "control": "text",
                        "span": 6,
                        "label": "证件号码",
                        "rules": [
                            {
                                "max": 20,
                                "message": "长度不能超过20个字符"
                            }
                        ]
                    },
                  "custna": {
                    "control": "text",
                    "span": 6,
                    "label": "客户姓名"
                  },
                  "applid": {
                    "control": "text",
                    "span": 6,
                    "label": "申请编号"
                  },
                  "lncatp": {
                    "control": "select",
                    "label": "流程类型:",
                    "align":"center",
                    "span": 6,
                    "dict": {
                      "dictType": "E_CSLNCATP",
                      "dictKey": [
                        "E_CSLNCATP"
                      ],
                      "format": "label"
                    }
                  }
                },
                {

                    "apstdt": {
                        "control": "dateTimePicker",
                        "label": "申请起始日期",
                        "width": "240px",
                        "format": "yyyyMMdd"
                    },
                    "apendt": {
                        "control": "dateTimePicker",
                        "label": "申请截止日期",
                        "width": "240px",
                        "format": "yyyyMMdd"
                    },
                  "lncfno": {
                    "control": "text",
                    "label": "借据号",
                    "span": 6
                  },
                  "areacenter": {
                    "control": "remoteSelect",
                    "label": "区域中心",
                    "span": 6,
                    "labelField": "brachName",
                    "valueField": "branchNo",
                    "doRequest": {
                      "url": "/SUMP/icmcall/icmRPCCall",
                      "method": "POST",
                      "params": {
                        "servicecode": "RLMSPLTS1068"
                      }
                    },
                    "format": "label"
                  }

                }
            ]
        },
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": true,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "AS_RLMS_BPM_QRDONE",
                    "usercd": "$g{{user_info.userCode}}",
                    "sizePage": 10
                }
            },
            "thead": [
                {
                    "label": "序号",
                    "type": "index"
                },
                {
                  "label": "区域中心",
                  "prop": "areacentername",
                  "visible": "true",
                  "align":"center"
                },
                {
                    "label": "申请编号",
                    "prop": "appl_id",

                    "align": "center"
                },
              {
                "label": "借据号",
                "align":"center",
                "prop": "loanno"
              },
                {
                    "label": "客户姓名",
                    "prop": "custna",
                    "align": "center"
                },
                {
                    "label": "证件号码",
                    "prop": "idtfno",
                    "align": "center"
                },
                {
                  "label": "申请人",
                  "prop": "creator",
                  "align":"center"
                },
                {
                    "label": "申请日期",
                    "prop": "appldt",
                    "align": "center"
                },
                {
                    "label": "结案时间",
                    "prop": "end_time",
                    "align": "center"
                },
                {
                    "label": "当前节点",
                    "prop": "taskna",
                    "align": "center"
                },
                {
                    "label": "当前处理人",
                    "prop": "curusr",
                    "align": "center"
                },
              {
                "label": "流程类型",
                "align":"center",
                "prop": "proc_inst_name"
              },
                {
                    "label": "操作",
                    "fixed": "right",
                    "align": "center",
                    "btns": [
                        {
                          "type": "edit",
                          "label": "查看详情",
                          "toRequest": {
                            "url": "@csAlredyToDoDetails",
                            "method": "GET",
                            "target":"navTab",
                            "params": {
                              "framecd": "warnMan"
                            }
                          },
                          "props": {
                            "title": "查看详情"
                          }
                        },
                        {
                            "type": "edit",
                            "label": "流程跟踪",
                            "toRequest": {
                                "url": "@CSProcessImgView",
                                "method": "GET",
                                "params": {
                                    "from": "doneList"
                                }
                            },
                            "props": {
                                "width": "1200px",
                                "title": "流程跟踪图"
                            }
                        }
                    ]
                }
            ]
        }
    }
}
