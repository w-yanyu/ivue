{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "idtfno": {
            "control": "text",
            "label": "证件号码",
            "span": 6,
            "rules": [
              {
                "max": 18,
                "message": "长度不能超过18个字符"
              }
            ]
          },
          "custna": {
            "control": "text",
            "label": "客户姓名",
            "span": 6,
            "rules": [
              {
                "max": 20,
                "message": "长度不能超过20个字符"
              }
            ]
          },
          "applid": {
            "control": "text",
            "label": "申请编号",
            "span": 6
          },
          "lncfno": {
            "control": "text",
            "label": "借据号",
            "span": 6
          }
        },{
          "apstdt": {
            "control": "dateTimePicker",
            "label": "申请起始日期",
            "span": 6
          },
          "apendt": {
            "control": "dateTimePicker",
            "label": "申请截止日期",
            "span": 6
          },
          "lncatp": {
            "control": "select",
            "label": "流程类型:",
            "align":"center",
            "span": 6,
            "dict": {
              "dictType": "E_CSLNCATP",
              "dictKey": [
                "E_CSLNCATP"
              ],
              "format": "label"
            }
          },
          "areacenter": {
            "control": "remoteSelect",
            "label": "区域中心",
            "span": 6,
            "labelField": "brachName",
            "valueField": "branchNo",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "RLMSPLTS1068"
              }
            },
            "format": "label"
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "rlms_qrwado",
          "usercd": "$g{{user_info.userCode}}"
        }
      },
      "thead": [
        {
          "label": "区域中心",
          "prop": "areacentername",
          "visible": "true",
          "align":"center"
        },
        {
          "label": "申请编号",
          "align":"center",
          "prop": "appl_id"
        },
        {
          "label": "借据号",
          "align":"center",
          "prop": "loanno"
        },
        {
          "label": "客户姓名",
          "align":"center",
          "prop": "custna"
        },
        {
          "label": "客户编号",
          "align":"center",
          "prop": "custid"
        },
        {
          "label": "证件号码",
          "align":"center",
          "prop": "idtfno"
        },
        {
          "label": "申请人",
          "prop": "creator",
          "align":"center"
        },
        {
          "label": "申请日期",
          "align":"center",
          "prop": "appldt"
        },
        {
          "label": "当前节点",
          "align":"center",
          "prop": "taskna"
        },
        {
          "label": "当前处理人",
          "align":"center",
          "prop": "curusr"
        },
        {
          "label": "流程类型",
          "align":"center",
          "prop": "proc_inst_name"
        },
        {
          "label": "操作",
          "align":"center",
          "btns": [
            {
              "type": "edit",
              "label": "办理",
              "toRequest": {
                "url": "@csWaitToDoDeal",
                "method": "GET",
                "target":"navTab",
                "params": {
                  "framecd": "warnMan"
                }
              },
              "props": {
                "title": "待办事项办理"
              }
            },
            {
              "type": "edit",
              "label": "流程跟踪",
              "toRequest": {
                "url": "@CSProcessImgView",
                "method": "GET",
                "params": {
                  "from": "backlog"
                }
              },
              "props": {
                "width": "1200px",
                "title": "流程跟踪图"
              }
            },
            {
              "type": "delete",
              "label": "取消认领",
              "confirm":{
                "title":"提示信息",
                "msg":"确认要取消认领该任务吗？",
                "confirmButtonText":"确定",
                "cancelButtonText":"取消"
              },
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "RLMSKGTS0101",
                  "usercd": "$g{{current.usercd}}",
                  "tkinid": "${{current.task_inst_id}}"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
