{
    "module": "v_search_btn_datagrid",
    "layout": {
      "datagrid": {
        "scope": "searchTable",
        "isInitSearch": true,
        "doRequest": {
          "url": "/SUMP/icmcall/icmRPCCall",
          "method": "POST",
          "params": {
            "servicecode": "AS_RLMS_BPM_HISTORY",
            "appl_id":"${{params.applid}}"

          }
        },
        "thead": [
         {
            "label": "操作人",
            "prop": "user_cd",
            "align": "center"
          },
          {
            "label": "审批时间",
            "prop": "submit_time",
            "align": "center"
          },
          {
            "label": "审批结果",
            "prop": "appv_result",
            "align": "center"
          },
          {
            "label": "岗位名称",
            "prop": "task_inst_name",
            "align": "center"

          },
          {
            "label": "审批意见",
            "prop": "remark",
            "align": "center"
          }
        ]
      }
    }
    }
