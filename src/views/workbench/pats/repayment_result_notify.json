{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {

      "controls": [
        {
          "repaymentDealTime": {
            "control": "dateTimePicker",
            "label": "还款日期",
            "rules": [
              {
                "max": 8,
                "message": "长度不能超过8个字符"
              }
            ]
          },
          "repaymentNo": {
            "control": "text",
            "label": "外部订单号",
            "rules": [
              {
                "max": 64,
                "message": "长度不能超过64个字符"
              }
            ]
          }

        },
        {
          "ownChannelCode": {
            "control": "select",
            "label": "渠道",
            "disabled": false,
            "value": "",
            "dict": {
              "dictType": "E_CHANNELCODE",
              "dictKey": [
                "E_CHANNELCODE"
              ],
              "format": "label"
            }
          },
          "notifyStatus": {
            "control": "select",
            "label": "渠道方通知状态",
            "disabled": false,
            "value": "Y",
            "dict": {
              "dictType": "E_NOTIFY_STATUS",
              "dictKey": [
                "E_NOTIFY_STATUS"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择通知状态"
              }
            ]
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "PATSCMSS0002",
          "flag": "SEL"
        }
      },
      "thead": [
        {
          "type": "index",
          "align": "center"
        },
        {
          "label": "订单号",
          "width": 170,
          "prop": "orderNo",
          "align": "left"
        },
        {
          "label": "提现平台单号",
          "width": 170,
          "prop": "paymentNo",
          "align": "left"
        },
        {
          "label": "外部订单号",
          "width": 160,
          "prop": "repaymentNo",
          "align": "left"
        },
        {
          "label": "渠道",
          "width": 100,
          "prop": "ownChannelCode",
          "align": "center",
          "dict": {
            "dictType": "E_CHANNELCODE",
            "dictKey": [
              "E_CHANNELCODE"
            ],
            "format": "label"
          }
        },
        {
          "label": "还款金额",
          "width": 100,
          "prop": "repaymentAmount",
          "align": "center"
        },
        {
          "label": "还款类型",
          "prop": "repaymentType",
          "width": 80,
          "align": "center",
          "dict": {
            "dictType": "E_REPAYMENT_TYPE",
            "dictKey": [
              "E_REPAYMENT_TYPE"
            ],
            "format": "label"
          }
        },
        {
          "label": "渠道方通知状态",
          "prop": "notifyStatus",
          "align": "center",
          "dict": {
            "dictType": "E_NOTIFY_STATUS",
            "dictKey": [
              "E_NOTIFY_STATUS"
            ],
            "format": "label"
          }
        },
        {
          "label": "伙伴中心状态",
          "prop": "ipmsStatus",
          "align": "center",
          "dict": {
            "dictType": "E_IPMS_STATUS",
            "dictKey": [
              "E_IPMS_STATUS"
            ],
            "format": "label"
          }
        },
        {
          "label": "交易状态",
          "prop": "trxnStatus",
          "width": 80,
          "align": "center",
          "dict": {
            "dictType": "E_TRXN_STATUS",
            "dictKey": [
              "E_TRXN_STATUS"
            ],
            "format": "label"
          }
        },
        {
          "label": "异常错误信息",
          "width": 200,
          "prop": "retText",
          "align": "center"
        },
        {
          "label": "客户号",
          "width": 120,
          "prop": "custNo",
          "align": "left"
        },
        {
          "label": "还款时间",
          "width": 150,
          "prop": "repaymentDealTime",
          "align": "left",
          "type": "dateTimePicker",
          "format": "yyyy-MM-dd HH:mm:ss"
        },
        {
          "label": "操作",
          "fixed": "right",
          "width": 80,
          "align":"center",
          "btns": [
            {
              "type": "look",
              "label": "重发通知",
              "show": {
                "prop": "notifyStatus",
                "is": ["Y", "D"]
              },
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "PATSCMSS0002",
                  "flag": "SEND",
                  "repaymentDealTime": ""
                }
              }
            }

          ]
        }
      ]
    }
  }
}
