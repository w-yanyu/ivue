{
    "module": "v-inputDataForm-btn",
    "layout": {
      "form": {
        "initDataOnce":true,
        "initRequest": {
          "url": "/SUMP/icmcall/icmRPCCall",
          "method": "POST",
          "params": {
            "servicecode": "PATSCMSS3002",
            "orderNo": "${{params.orderNo}}",
            "tranType": "${{params.tranType}}"
          }
        },
        "btns": [
            {
                "type": "submit",
                "label": "保存",
                "icon": "el-icon-tickets",
                "doRequest": {
                    "url": "/SUMP/icmcall/icmRPCCall",
                    "method": "POST",
                    "params": {
                      "servicecode": "PATSCMSS1003",
                      "orderNo": "${{params.orderNo}}",
                      "tranType": "${{params.tranType}}",
                      "trxnSq": "${{params.trxnSq}}",
                      "afAmount": "${{form.afAmount}}"
                    }
                },
                "props": {
                    "width": "65%",
                    "title":  "重入帐"
                }
            }
        ],
        "controls": [
          {
            "orderNo": {
              "control": "text",
              "label": "订单号",
              "disabled": true
            },
            "paymentNo": {
              "control": "text",
              "label": "提现平台号",
              "disabled": true
            },
            "repaymentNo": {
              "label": "外部订单号",
              "control": "text",
              "disabled": true
            },
            "ownChannelCode": {
              "control": "select",
              "label": "渠道",
              "dict": {
                "format": "label",
                "dictKey": [
                  "E_CHANNELCODE"
                ],
                "dictType": "E_CHANNELCODE"
              },
              "disabled": true
            },
            "repaymentAmount": {
              "control": "text",
              "label": "还款金额",
              "disabled": true
            },
            "afAmount": {
              "control": "text",
              "label": "修改金额",
              "value": "${{form.repaymentAmount}}",
              "disabled": false
            },
            "repaymentType": {
              "control": "select",
              "label": "还款类型",
              "dict": {
                "format": "label",
                "dictKey": [
                  "E_REPAYMENT_TYPE"
                ],
                "dictType": "E_REPAYMENT_TYPE"
              },
              "disabled": true
            },
            "notifyStatus": {
              "control": "select",
              "label": "渠道方通知状态",
              "dict": {
                "dictType": "E_NOTIFY_STATUS",
                "dictKey": [
                  "E_NOTIFY_STATUS"
                ],
                "format": "label"
              },
              "disabled": true
            },
            "ipmsStatus": {
              "control": "select",
              "label": "伙伴中心状态",
              "dict": {
                "dictType": "E_IPMS_STATUS",
                "dictKey": [
                  "E_IPMS_STATUS"
                ],
                "format": "label"
              },
              "disabled": true
            },
            "trxnStatus":{
              "label": "交易状态",
              "control": "select",
              "dict": {
                "format": "label",
                "dictKey": [
                  "E_TRXN_STATUS"
                ],
                "dictType": "E_TRXN_STATUS"
              },
              "disabled": true
            }
          },
          {
            "repaymentDealTime":{
              "label": "还款时间",
              "control": "dateTimePicker",
              "disabled": true,
              "valueFormat": "yyyyMMdd HHmmss",
              "format": "yyyy-MM-dd HH:mm:ss"
            }
          }
      ]
      }
    }
  }
  