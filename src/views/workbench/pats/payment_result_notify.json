{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "paymentTime": {
            "control": "dateTimePicker",
            "label": "提现日期",
            "value":""
          },
          "paymentNo": {
            "control": "text",
            "label": "外部订单号",
            "rules": [
              {
                "max": 30,
                "message": "长度不能超过30个字符"
              }
            ]
          }
        },
        {
          "ownChannelCode": {
            "control": "select",
            "label": "渠道",
            "disabled": false,
            "value": "",
            "dict": {
              "dictType": "E_CHANNELCODE",
              "dictKey": [
                "E_CHANNELCODE"
              ],
              "format": "label"
            }
          },
          "notifyStatus": {
            "control": "select",
            "label": "渠道方通知状态",
            "disabled": false,
            "value": "Y",
            "dict": {
              "dictType": "E_NOTIFY_STATUS",
              "dictKey": [
                "E_NOTIFY_STATUS"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择交易状态"
              }
            ]
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "PATSCMSS0001",
          "flag": "SEL"
        }
      },

      "thead": [
        {
          "type": "index",
          "align": "center"
        },
        {
          "label": "借据号",
          "width": 160,
          "prop": "loanReceiptNo",
          "align": "left"
        },
        {
          "label": "订单号",
          "width": 160,
          "prop": "orderNo",
          "align": "left"
        },
        {
          "label": "渠道",
          "width": 80,
          "prop": "ownChannelCode",
          "align": "center",
          "dict": {
            "dictType": "E_CHANNELCODE",
            "dictKey": [
              "E_CHANNELCODE"
            ],
            "format": "label"
          }
        },
        {
          "label": "外部订单号",
          "prop": "paymentNo",
          "align": "left"
        },
        {
          "label": "提现金额",
          "prop": "paymentAmount",
          "align": "center"
        },
        {
          "label": "渠道方通知状态",
          "prop": "notifyStatus",
          "width": 160,
          "align": "center",
          "dict": {
            "dictType": "E_NOTIFY_STATUS",
            "dictKey": [
              "E_NOTIFY_STATUS"
            ],
            "format": "label"
          }
        },
        {
          "label": "伙伴中心状态",
          "prop": "ipmsStatus",
          "align": "center",
          "dict": {
            "dictType": "E_IPMS_STATUS",
            "dictKey": [
              "E_IPMS_STATUS"
            ],
            "format": "label"
          }
        },
        {
          "label": "交易状态",
          "prop": "trxnStatus",
          "align": "center",
          "dict": {
            "dictType": "E_TRXN_STATUS",
            "dictKey": [
              "E_TRXN_STATUS"
            ],
            "format": "label"
          }
        },
        {
          "label": "异常错误信息",
          "prop": "retText",
          "align": "center"
        },
        {
          "label": "客户号",
          "width": 120,
          "prop": "custNo",
          "align": "left"
        },
        {
          "label": "提现时间",
          "prop": "paymentTime",
          "align": "left",
          "type": "dateTimePicker",
          "format": "yyyy-MM-dd HH:mm:ss"
        },
        {
          "label": "操作",
          "fixed": "right",
          "width": 100,
          "align":"center",
          "btns": [
            {
              "type": "edit",
              "label": "重发通知",
              "show": {
                "expression": "'${{current.ownChannelCode}}' === 'JDJR' && '${{current.notifyStatus}}' !== 'N'"
              },
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "PATSCMSS0001",
                  "flag": "SEND",
                  "paymentTime": ""
                }
              },
              "props": {
                "width": "65%",
                "title": "提现结果通知发送"
              }
            }
          ]
        }
      ]
    }
  }
}
