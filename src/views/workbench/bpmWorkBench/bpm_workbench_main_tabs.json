{
  "module": "dynamicTabs",
  "layout": {
    "tabs": {
      "tabHeader": [
        {
          "id": "tab1",
          "label": "待认领案件",
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "toRequest": {
            "url": "@BpmClaimWorkBench",
            "method": "post"
          }
        },
        {
          "id": "tab2",
          "label": "待处理案件",
          "closable": false,
          "disabled": false,
          "icon": "el-icon-date",
          "toRequest": {
            "url": "@BpmDealWorkBench",
            "method": "post"
          }
        },
        {
          "id": "tab3",
          "label": "已处理案件",
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "toRequest": {
            "url": "/views/workbench/bpmWorkBench/bpm_already_do/workbench_already_do_view.json",
            "method": "post"
          }
        }
       ],
      "defaultActiveKey": "tab1",
      "position": "top",
      "styleClass": "card"
    }
  }
}
