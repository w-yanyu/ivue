{
  "module": "v_search_btn_datagrid",
  "layout": {

    "toolbar": {
      "btns": [
        {
          "type": "add",
          "target": "dialog",
          "label": "随机认领",
          "toRequest": {
            "method": "GET",
            "url": "/views/workbench/into_pool/workbench_into_pool_random.json"
          },
          "props": {
            "title": "随机认领",
            "width": "600px"
          }
        }
      ]
    },
    "form": {
      "controls": [
        {
          "crdtcd": {
            "label": "产品编号",
            "control": "remoteSelect",
            "valueField": "crdtcd",
            "labelField": "prodna",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "post",
              "params": {
                "servicecode": "AS_ICMS_PD_QRPDLS",
                "length": 100
              }
            }
          },
          "applid": {
            "control": "text",
            "label": "申请编号"
          }
        },
        {
          "certno": {
            "control": "text",
            "label": "证件号",
            "rules": [
              {
                "max": 18,
                "message": "长度不能超过18个字符"
              }
            ]
          },
          "mobino": {
            "control": "text",
            "label": "手机号码",
            "rules": [
              {
                "max": 13,
                "message": "长度不能超过13个字符"
              }
            ]
          }
        },
        {
          "apstdt": {
            "control": "dateTimePicker",
            "label": "进件起始日期",
            "format": "yyyyMMdd"
          },
          "apendt": {
            "control": "dateTimePicker",
            "label": "进件截止日期",
            "format": "yyyyMMdd"
          }
        },
        {
          "custna": {
            "control": "text",
            "label": "申请人姓名",
            "rules": [
              {
                "max": 13,
                "message": "长度不能超过13个字符"
              }
            ]
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "seleqj"
        }
      },
      "thead": [
        {
          "label": "序号",
          "type" :  "index"
        },
        {
          "label": "任务ID",
          "prop": "tkinid",
          "visible": "true",
          "width": 270
        },
        {
          "label": "申请编号",
          "prop": "applid",
          "width": 160
        },
        {
          "label": "申请人姓名",
          "prop": "custna"
        },
        {
          "label": "证件号码",
          "prop": "certno",
          "width": 160
        },
        {
          "label": "手机号",
          "prop": "mobile",
          "width": 120
        },
        {
          "label": "产品名称",
          "prop": "prodna"
        },
        {
          "label": "进件时间",
          "prop": "appldt"
        },
        {
          "label": "当前节点",
          "prop": "tkname",
          "width": 120
        },
        {
          "label": "操作",
          "fixed": "right",
          "width": 80,
          "align":"center",
          "btns": [
            {
              "type": "delete",
              "label": "领取任务",
              "confirm":{
                "title":"提示信息",
                "msg":"确认要领取该任务吗？",
                "confirmButtonText":"确定",
                "cancelButtonText":"取消"
              },
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "clmtsk",
                  "usercd": "$g{{user_info.userCode}}"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
