{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "crdtcd": {
            "label": "产品编号",
            "control": "remoteSelect",
            "valueField": "crdtcd",
            "labelField": "prodna",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "post",
              "params": {
                "servicecode": "AS_ICMS_PD_QRPDLS"
              }
            },
            "##rules": [
              {
                "required": true,
                "message": "请选择产品编号"
              }
            ]
          },
          "applid": {
            "control": "text",
            "label": "申请编号"
          }
        },
        {
          "idtfno": {
            "control": "text",
            "label": "证件号码",
            "rules": [
              {
                "max": 20,
                "message": "长度不能超过20个字符"
              }
            ]
          },
          "mobino": {
            "control": "text",
            "label": "手机号码"
          }
        },
        {
          "apstdt": {
            "control": "dateTimePicker",
            "label": "进件起始日期"
          },
          "apendt": {
            "control": "dateTimePicker",
            "label": "进件截止日期"
          }
        },
        {
          "custna": {
            "control": "text",
            "label": "申请人姓名"
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "AS_ICMS_BPM_QRDONE",
          "usercd": "$g{{user_info.userCode}}",
          "sizePage": 10
        }
      },
      "thead": [
        {
          "label": "序号",
          "type": "index"
        },
        {
          "label": "申请编号",
          "prop": "appl_id",
          "width": 170,
          "align": "center"
        },
        {
          "label": "申请人姓名",
          "prop": "custna",
          "align": "center"
        },
        {
          "label": "证件号码",
          "prop": "idtfno",
          "width": 160,
          "align": "center"
        },
        {
          "label": "产品名称",
          "prop": "prodna",
          "align": "center"
        },
        {
          "label": "进件日期",
          "prop": "appldt",
          "align": "center"
        },
        {
          "label": "审批时间",
          "prop": "last_upd_dt",
          "align": "center"
        },
        {
          "label": "当前节点",
          "prop": "taskna",
          "align": "center"
        },
        {
          "label": "当前处理人",
          "prop": "curusr",
          "align": "center"
        },
        {
          "label": "操作",
          "fixed": "right",
          "width": 130,
          "align": "center",
          "btns": [
            {
              "type": "edit",
              "label": "详情",
              "toRequest": {
                "url": "@AlreadyDoDetail",
                "method": "GET",
                "target": "navTab",
                "params": {

                }
              },
              "props": {
                "title": "已办事项详情"
              }
            },
            {
              "type": "edit",
              "label": "流程跟踪",
              "toRequest": {
                "url": "@ProcessImgView",
                "method": "GET",
                "params": {
                  "from": "doneList"
                }
              },
              "props": {
                "width": "1200px",
                "title": "流程跟踪图"
              }
            }
          ]
        }
      ]
    }
  }
}
