{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [],
      "btns": []
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "applyHstQuery",
          "certNo": "${{params.certNo}}"
        }
      },
      "thead": [
        {
          "label": "进件编号",
          "prop": "applid",
          "align": "center"
        },
        {
          "label": "产品",
          "prop": "prodName",
          "align": "center"
        },
        {
          "label": "进件时间",
          "prop": "applyTime",
          "align": "center"
        },
        {
          "label": "当前流程",
          "prop": "currStep",
          "align": "center"
        }
      ]
    }
  }
}