{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "controls": [
                {
                    "idtfno": {
                        "control": "text",
                        "span": 6,
                        "label": "证件号码",
                        "rules": [
                            {
                                "max": 20,
                                "message": "长度不能超过20个字符"
                            }
                        ]
                    },
                  "custna": {
                    "control": "text",
                    "span": 6,
                    "label": "客户姓名"
                  },
                  "applid": {
                    "control": "text",
                    "span": 6,
                    "label": "申请编号"
                  },
                  "lncatp": {
                    "control": "select",
                    "label": "流程类型:",
                    "align":"center",
                    "span": 6,
                    "dict": {
                      "dictType": "E_LNCATP2",
                      "dictKey": [
                        "E_LNCATP2"
                      ],
                      "format": "label"
                    }
                  }
                },
                {

                    "apstdt": {
                        "control": "dateTimePicker",
                        "label": "申请起始日期",
                        "span": 6,
                        "format": "yyyyMMdd"
                    },
                    "apendt": {
                        "control": "dateTimePicker",
                        "label": "申请截止日期",
                      "span": 6,
                        "format": "yyyyMMdd"
                    },
                  "lncfno": {
                    "control": "text",
                    "label": "借据号",
                    "span": 6
                  }

                }
            ]
        },
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": true,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "PLMSKGTS0102",
                    "usercd": "$g{{user_info.userCode}}",
                    "sizePage": 10
                }
            },
            "thead": [
                {
                    "label": "序号",
                    "type": "index"
                },
                {
                    "label": "申请编号",
                    "prop": "appl_id",

                    "align": "center"
                },
              {
                "label": "借据号",
                "align":"center",
                "prop": "lncfno"
              },
                {
                    "label": "客户姓名",
                    "prop": "custna",
                    "align": "center"
                },
                {
                    "label": "证件号码",
                    "prop": "idtfno",

                    "align": "center"
                },
                {
                    "label": "申请人",
                    "align": "center",
                    "prop": "creator"
                },
                {
                    "label": "申请日期",
                    "prop": "appldt",
                    "align": "center"
                },
                {
                    "label": "结案时间",
                    "prop": "end_time",
                    "align": "center"
                },
                {
                    "label": "当前节点",
                    "prop": "taskna",
                    "align": "center"
                },
                {
                    "label": "当前处理人",
                    "prop": "curusr",
                    "align": "center"
                },
                {
                  "label": "流程类型",
                  "prop": "lncatp",
                  "align": "center",
                  "dict": {
                    "dictType": "E_LNCATP2",
                    "dictKey": [
                      "E_LNCATP2"
                    ],
                    "format": "label",
                    "labelField": "dictName",
                    "valueField": "dictId"
                  }
                },
                {
                    "label": "操作",
                    "fixed": "right",
                    "align": "center",
                    "btns": [
                        {
                            "type": "edit",
                            "label": "查看详情",
                            "toRequest": {
                              "url": "@AlredyToDoDetails",
                              "method": "GET",
                              "target":"navTab",
                              "params": {
                                "framecd": "warnMan"
                              }
                            },
                            "props": {
                              "title": "查看详情"
                            }
                          },
                        {
                            "type": "edit",
                            "label": "流程跟踪",
                            "toRequest": {
                                "url": "@DHProcessImgView",
                                "method": "GET",
                                "params": {
                                    "from": "doneList"
                                }
                            },
                            "props": {
                                "width": "1200px",
                                "title": "流程跟踪图"
                            }
                        }
                    ]
                }
            ]
        }
    }
}
