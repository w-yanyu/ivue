{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "plms_cltsks",
          "tkcount":"${{form.tkcount}}",
          "usercd": "$g{{user_info.userCode}}"
        }
      },
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/rest/auth/userRole",
            "method": "POST",
            "params": {
              "servicecode": "plms_cltsks",
              "tkcount":"${{form.tkcount}}",
              "usercd": "$g{{user_info.userCode}}"
            }
          }
        }
      ],
      "controls": [
        {
          "tkcount": {
            "control": "inputNumber",
            "label": "自动认领笔数",
            "width": "160px",
            "span": 18,
            "min": 1,
            "value": 1
          }
        }
      ]
    }
  }
}
