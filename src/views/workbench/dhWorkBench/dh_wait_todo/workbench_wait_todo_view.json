{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "idtfno": {
            "control": "text",
            "label": "证件号码",
            "span": 6,
            "rules": [
              {
                "max": 18,
                "message": "长度不能超过18个字符"
              }
            ]
          },
          "custna": {
            "control": "text",
            "label": "申请人姓名",
            "span": 6,
            "rules": [
              {
                "max": 20,
                "message": "长度不能超过20个字符"
              }
            ]
          },
          "applid": {
            "control": "text",
            "label": "申请编号",
            "span": 6
          },
          "lncfno": {
            "control": "text",
            "label": "借据号",
            "span": 6
          }
        },{
          "apstdt": {
            "control": "dateTimePicker",
            "label": "申请起始日期",
            "span": 6
          },
          "apendt": {
            "control": "dateTimePicker",
            "label": "申请截止日期",
            "span": 6
          },
          "lncatp": {
            "control": "select",
            "label": "流程类型:",
            "align":"center",
            "span": 6,
            "dict": {
              "dictType": "E_LNCATP2",
              "dictKey": [
                "E_LNCATP2"
              ],
              "format": "label"
            }
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "plms_qrwado",
          "usercd": "$g{{user_info.userCode}}"
        }
      },
      "thead": [
        {
          "label": "申请编号",
          "align":"center",
          "prop": "appl_id"
        },
        {
          "label": "借据号",
          "align":"center",
          "prop": "lncfno"
        },
        {
          "label": "申请人姓名",
          "align":"center",
          "prop": "custna"
        },
        {
          "label": "申请人编号",
          "align":"center",
          "prop": "custid"
        },
        {
          "label": "证件号码",
          "align":"center",
          "prop": "idtfno"
        },
        {
          "label": "申请日期",
          "align":"center",
          "prop": "appldt"
        },
        {
          "label": "当前节点",
          "align":"center",
          "prop": "taskna"
        },
        {
          "label": "当前处理人",
          "align":"center",
          "prop": "curusr"
        },
        {
          "label": "流程类型",
          "align":"center",
          "prop": "proc_inst_name"
        },
        {
          "label": "操作",
          "align":"center",
          "btns": [
            {
              "type": "edit",
              "label": "办理",
              "toRequest": {
                "url": "@dhWaitToDoDeal",
                "method": "GET",
                "target":"navTab",
                "params": {
                  "framecd": "warnMan"
                }
              },
              "props": {
                "title": "待办事项办理"
              }
            },
            {
              "type": "edit",
              "label": "流程跟踪",
              "toRequest": {
                "url": "@DHProcessImgView",
                "method": "GET",
                "params": {
                  "from": "backlog"
                }
              },
              "props": {
                "width": "1200px",
                "title": "流程跟踪图"
              }
            }
          ]
        }
      ]
    }
  }
}
