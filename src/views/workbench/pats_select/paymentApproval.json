{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "controls": [
                {
                    "applyNo": {
                        "control": "text",
                        "label": "渠道方申请单号",
                        "rules": [
                            {
                                "max": 28,
                                "message": "长度不能超过18个字符"
                            }
                        ]
                    },
                    "paymentNo": {
                        "control": "text",
                        "label": "渠道方提现单号",
                        "rules": [
                            {
                                "max": 28,
                                "message": "长度不能超过18个字符"
                            }
                        ]
                    }
                },
                {
                    "trxnStatus": {
                        "label": "订单状态",
                        "control": "select",
                        "dict": {
                            "format": "label",
                            "dictKey": [
                                "E_TRXN_STATUS"
                            ],
                            "dictType": "E_TRXN_STATUS"
                        }
                    },
                    "ownChannelCode": {
                        "label": "渠道类型",
                        "control": "select",
                        "dict": {
                            "format": "label",
                            "dictKey": [
                                "E_OWN_CHANNEL_CODE4_STATUS"
                            ],
                            "dictType": "E_OWN_CHANNEL_CODE4_STATUS"
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择渠道类型"
                            }
                        ]
                    }
                },
                {
                    "stardt": {
                        "control": "dateTimePicker",
                        "label": "申请起始日期",
                        "editable": true,
                        "format": "yyyyMMdd"
                    },
                    "enddt": {
                        "control": "dateTimePicker",
                        "label": "申请截止日期",
                        "editable": true,
                        "format": "yyyyMMdd"
                    }
                },
                {
                    "certificateNo": {
                        "control": "text",
                        "label": "身份证号",
                        "rules": [
                            {
                                "max": 28,
                                "message": "长度不能超过18个字符"
                            }
                        ]
                    },
                    "userTel": {
                        "control": "text",
                        "label": "手机号",
                        "rules": [
                            {
                                "max": 28,
                                "message": "长度不能超过18个字符"
                            }
                        ]
                    }
                }
            ]
        },
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": false,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "PATSCMSS5002"
                }
            },
            "thead": [
                {
                    "label": "渠道",
                    "prop": "ownChannelCode",
                    "align": "center",
                    "dict": {
                        "dictType": "E_OWN_CHANNEL_CODE4_STATUS",
                        "dictKey": [
                            "E_OWN_CHANNEL_CODE4_STATUS"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "渠道方申请单号",
                    "prop": "applyNo",
                    "align": "center"
                },
                {
                    "label": "渠道方提现单号",
                    "prop": "paymentNo",
                    "align": "center"
                },
                {
                    "label": "提现单号（内部）",
                    "prop": "mhtPaymentNo",
                    "align": "center"
                },
                {
                    "label": "提现金额",
                    "prop": "paymentAmount",
                    "align": "center"
                },
                {
                    "label": "错误码",
                    "prop": "retCode",
                    "align": "center"
                },
                {
                    "label": "错误信息",
                    "prop": "retText",
                    "align": "center"
                },
                {
                    "label": "身份证号",
                    "prop": "certificateNo",
                    "align": "center"
                },
                {
                    "label": "手机号",
                    "prop": "userTel",
                    "align": "center"
                },
                {
                    "label": "用户id",
                    "prop": "userId",
                    "align": "center"
                },
                {
                    "label": "订单号",
                    "prop": "orderNo",
                    "align": "center"
                },
                {
                    "label": "提现审核状态",
                    "prop": "paymentStatus",
                    "align": "center",
                    "dict": {
                        "dictType": "E_PAYMENT_STATUS",
                        "dictKey": [
                            "E_PAYMENT_STATUS"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "订单状态",
                    "prop": "trxnStatus",
                    "align": "center",
                    "dict": {
                        "dictType": "E_TRXN_STATUS",
                        "dictKey": [
                            "E_TRXN_STATUS"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "创建时间",
                    "prop": "createTime",
                    "align": "center"
                },
                {
                    "label": "更新时间",
                    "prop": "updateTime",
                    "align": "center"
                }
            ]
        }
    }
}