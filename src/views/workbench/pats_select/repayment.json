{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "controls": [
                {
                    "applyNo": {
                        "control": "text",
                        "label": "渠道方申请单号",
                        "rules": [
                            {
                                "max": 28,
                                "message": "长度不能超过18个字符"
                            }
                        ]
                    },
                    "repaymentNo": {
                        "control": "text",
                        "label": "渠道方还款流水",
                        "rules": [
                            {
                                "max": 28,
                                "message": "长度不能超过18个字符"
                            }
                        ]
                    }
                },
                {
                    "trxnStatus": {
                        "label": "订单状态",
                        "control": "select",
                        "dict": {
                            "format": "label",
                            "dictKey": [
                                "E_TRXN_STATUS"
                            ],
                            "dictType": "E_TRXN_STATUS"
                        }
                    },
                    "ownChannelCode": {
                        "label": "渠道类型",
                        "control": "select",
                        "dict": {
                            "format": "label",
                            "dictKey": [
                                "E_OWN_CHANNEL_CODE2_STATUS"
                            ],
                            "dictType": "E_OWN_CHANNEL_CODE2_STATUS"
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择渠道类型"
                            }
                        ]
                    }
                },
                {
                    "stardt": {
                        "control": "dateTimePicker",
                        "label": "申请起始日期",
                        "editable": true,
                        "format": "yyyyMMdd"
                    },
                    "enddt": {
                        "control": "dateTimePicker",
                        "label": "申请截止日期",
                        "editable": true,
                        "format": "yyyyMMdd"
                    }
                },
                {
                    "custId": {
                        "control": "text",
                        "label": "客户id",
                        "rules": [
                            {
                                "max": 28,
                                "message": "长度不能超过18个字符"
                            }
                        ]
                    }
                }
            ]
        },
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": false,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "PATSCMSS5004"
                }
            },
            "thead": [
                {
                    "label": "渠道",
                    "prop": "ownChannelCode",
                    "align": "center",
                    "dict": {
                        "dictType": "E_OWN_CHANNEL_CODE2_STATUS",
                        "dictKey": [
                            "E_OWN_CHANNEL_CODE2_STATUS"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "渠道方申请单号",
                    "prop": "applyNo",
                    "align": "center"
                },
                {
                    "label": "渠道方提现单号",
                    "prop": "paymentNo",
                    "align": "center"
                },
                {
                    "label": "渠道方还款流水",
                    "prop": "repaymentNo",
                    "align": "center"
                },
                {
                    "label": "还款金额",
                    "prop": "repaymentAmount",
                    "align": "center"
                },
                {
                    "label": "还款期数",
                    "prop": "repaymentPeriod",
                    "align": "center"
                },
                {
                    "label": "错误码",
                    "prop": "retCode",
                    "align": "center"
                },
                {
                    "label": "错误信息",
                    "prop": "retText",
                    "align": "center"
                },
                {
                    "label": "订单号",
                    "prop": "orderNo",
                    "align": "center"
                },
                {
                    "label": "客户号",
                    "prop": "custId",
                    "align": "center"
                },
                {
                    "label": "还款状态",
                    "prop": "repaymentAmount",
                    "align": "center",
                    "dict": {
                        "dictType": "E_REPAYMENT_STATUS",
                        "dictKey": [
                            "E_REPAYMENT_STATUS"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "伙伴中心通知状态",
                    "prop": "ipmsStatus",
                    "align": "center",
                    "dict": {
                        "dictType": "E_IPMS_STATUS",
                        "dictKey": [
                            "E_IPMS_STATUS"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "发送账户中心状态",
                    "prop": "sendIcoreStatus",
                    "align": "center",
                    "dict": {
                        "dictType": "E_SENDLCORE_STATUS",
                        "dictKey": [
                            "E_SENDLCORE_STATUS"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "渠道方通知状态",
                    "prop": "notifyStatus",
                    "align": "center",
                    "dict": {
                        "dictType": "E_NOTIFY_STATUS",
                        "dictKey": [
                            "E_NOTIFY_STATUS"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "订单状态",
                    "prop": "trxnStatus",
                    "align": "center",
                    "dict": {
                        "dictType": "E_TRXN_STATUS",
                        "dictKey": [
                            "E_TRXN_STATUS"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "创建时间",
                    "prop": "createTime",
                    "align": "center"
                },
                {
                    "label": "更新时间",
                    "prop": "updateTime",
                    "align": "center"
                }
            ]
        }
    }
}