{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": { "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "post",
                "params": {
                  "servicecode": "PATSJDSS5001",
                  "method":"add"
                }
              },
              "btns":[
				{
					"type":"submit",
					"label":"提交"
				}
			],
            "scope": "myForm",
            "controls": [
                {
                    "applyNo": {
                        "control": "text",
                        "label": "平台申请号",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入平台申请号"
                            }
                        ]
                    },
                    "notifyType": {
                        "label": "通知类型",
                        "control": "select",
                        "dict": {
                            "format": "label",
                            "dictKey": [
                                "E_JD_NOTIFY_STATUS"
                            ],
                            "dictType": "E_JD_NOTIFY_STATUS"
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请选通知类型"
                            }
                        ]
                    },
                    "changeType": {
                        "label": "变更类型",
                        "control": "select",
                        "dict": {
                            "format": "label",
                            "dictKey": [
                                "E_CHANGE_TYPE_STATUS"
                            ],
                            "dictType": "E_CHANGE_TYPE_STATUS"
                        }
                    },
                    "userId": {
                        "control": "text",
                        "label": "用户ID",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入用户ID"
                            }
                        ]
                    },
                    "totalAmount": {
                        "control": "text",
                        "label": "总额度（单位:元）",
                        "rules": [
                            {
                                "max": 28,
                                "message": "长度不能超过18个字符"
                            }
                        ]
                    },
                    "loanSurplusAmount": {
                        "control": "text",
                        "label": "剩余可用额度（单位:元）",
                        "rules": [
                            {
                                "max": 28,
                                "message": "长度不能超过18个字符"
                            }
                        ]
                    },
                    "changeAmount": {
                        "control": "text",
                        "label": "调整额度（单位:元）",
                        "rules": [
                            {
                                "max": 28,
                                "message": "长度不能超过18个字符"
                            }
                        ]
                    },
                    "changeTime": {
                        "control": "text",
                        "label": "调整时间(yyyyMMddHHmmss)",
                        "editable": true,
                        "format": "yyyyMMddHHmmss",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入调整时间"
                            }
                        ]
                    },
                    "changeOperator": {
                        "control": "text",
                        "label": "操作人",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入操作人"
                            }
                        ]
                    },
                    "expiryTime": {
                        "control": "text",
                        "label": "有效日期(yyyyMMddHHmmss)",
                        "editable": true,
                        "format": "yyyyMMddHHmmss"
                    },
                    "annualRate": {
                        "control": "text",
                        "label": "年化利率（单位:%）",
                        "rules": [
                            {
                                "max": 28,
                                "message": "长度不能超过18个字符"
                            }
                        ]
                    },
                    "dailyInterestRate": {
                        "control": "text",
                        "label": "日利率（单位:%）",
                        "rules": [
                            {
                                "max": 28,
                                "message": "长度不能超过18个字符"
                            }
                        ]
                    }
                }
            ]
        }
        
    }
}