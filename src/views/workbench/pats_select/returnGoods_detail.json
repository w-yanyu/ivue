{
  "module": "v-realonlyForm-btn",
  "layout": {
    "initDataOnce": true,
    "form": {
      "initRequest": "${{params}}",
      "btns": [
        {
          "type": "custom",
          "target": "dialog",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "PATSIMSS4001",
              "selectName": "returnGoodsQryInfo",
              "orderNo": "${{params.orderNo}}",
              "ownChannelCode": "${{params.ownChannelCode}}",
              "loanNo": "${{params.loanNo}}"
            }
          },
          "label": "更新卡号",
          "confirm": {
            "title": "提示信息",
            "msg": "确认要更新卡号吗？",
            "confirmButtonText": "确定",
            "cancelButtonText": "取消"
          }
        },
        {
          "type": "custom",
          "target": "dialog",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "PATSIMSS4002",
              "selectName": "returnGoodsQryInfo",
              "orderNo": "${{params.orderNo}}",
              "ownChannelCode": "${{params.ownChannelCode}}"
            }
          },
          "label": "重新触发",
          "confirm": {
            "title": "提示信息",
            "msg": "确认要重新触发吗？",
            "confirmButtonText": "确定",
            "cancelButtonText": "取消"
          }
        }
      ],
      "controls": [
        {
          "tranDate": {
            "control": "string",
            "label": "交易日期",
            "value": "${{form.tranDate}}",
            "width": "150px"
          },
          "loanNo": {
            "control": "string",
            "label": "借记号",
            "width": "150px",
            "value": "${{form.loanNo}}"
          }
        },
        {
          "username": {
            "control": "string",
            "label": "客户姓名",
            "width": "150px",
            "value": "${{form.username}}"
          },
          "retType": {
            "control": "string",
            "label": "退货类型",
            "width": "150px",
            "value": "${{form.retType}}",
            "dict": {
              "dictType": "E_RET_TYPE",
              "dictKey": [
                "E_RET_TYPE"
              ],
              "format": "label"
            }
          }
        },
        {
          "retAmt": {
            "control": "string",
            "label": "申请退货金额",
            "width": "150px",
            "value": "${{form.retAmt}}"
          },
          "trxnStatus": {
            "control": "string",
            "label": "退货状态",
            "width": "150px",
            "value": "${{form.trxnStatus}}",
            "dict": {
              "format": "label",
              "dictKey": [
                "E_TRANS_STATUS"
              ],
              "dictType": "E_TRANS_STATUS"
            }
          }
        },
        {
          "batStatus": {
            "control": "string",
            "label": "支付状态",
            "width": "150px",
            "value": "${{form.batStatus}}",
            "dict": {
              "dictType": "E_TRANS_STATUS_COMM",
              "dictKey": [
                "E_TRANS_STATUS_COMM"
              ],
              "format": "label"
            }
          },
          "capitalType": {
            "control": "string",
            "label": "资金处理方式",
            "width": "150px",
            "value": "${{form.capitalType}}",
            "dict": {
              "dictType": "E_CAPITAL_TYPE",
              "dictKey": [
                "E_CAPITAL_TYPE"
              ],
              "format": "label"
            }
          }
        },
        {
          "cardNo": {
            "control": "string",
            "label": "卡号",
            "width": "150px",
            "value": "${{form.cardNo}}"
          },
          "retRealAmt": {
            "control": "string",
            "label": "实际退款金额",
            "width": "150px",
            "value": "${{form.retRealAmt}}"
          }
        },
        {
          "loanBal": {
            "control": "string",
            "label": "贷余",
            "width": "150px",
            "value": "${{form.loanBal}}"
          },
          "remainderFee": {
            "control": "string",
            "label": "剩余息费",
            "width": "150px",
            "value": "${{form.remainderFee}}"
          }
        }
      ]
    }
  }
}