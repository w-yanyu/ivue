{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "applyNo": {
            "control": "text",
            "label": "渠道方申请单号",
            "rules": [
              {
                "max": 28,
                "message": "长度不能超过18个字符"
              }
            ]
          },
          "trxnStatus": {
            "label": "订单状态",
            "control": "select",
            "dict": {
              "format": "label",
              "dictKey": [
                "E_TRXN_STATUS"
              ],
              "dictType": "E_TRXN_STATUS"
            }
          }
        },
        {
          "ownChannelCode": {
            "label": "渠道类型",
            "control": "select",
            "dict": {
              "format": "label",
              "dictKey": [
                "E_OWN_CHANNEL_CODE3_STATUS"
              ],
              "dictType": "E_OWN_CHANNEL_CODE3_STATUS"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择渠道类型"
              }
            ]
          }
        },
        {
          "stardt": {
            "control": "dateTimePicker",
            "label": "申请起始日期",
            "editable": true,
            "format": "yyyyMMdd"
          },
          "enddt": {
            "control": "dateTimePicker",
            "label": "申请截止日期",
            "editable": true,
            "format": "yyyyMMdd"
          }
        },
        {
          "userName": {
            "control": "text",
            "label": "姓名",
            "rules": [
              {
                "max": 28,
                "message": "长度不能超过18个字符"
              }
            ]
          },
          "custId": {
            "control": "text",
            "label": "客户id",
            "rules": [
              {
                "max": 28,
                "message": "长度不能超过18个字符"
              }
            ]
          },
          "certNo": {
            "control": "text",
            "label": "身份证号",
            "rules": [
              {
                "max": 28,
                "message": "长度不能超过18个字符"
              }
            ]
          },
          "userTel": {
            "control": "text",
            "label": "手机号",
            "rules": [
              {
                "max": 28,
                "message": "长度不能超过18个字符"
              }
            ]
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "PATSCMSS5001"
        }
      },
      "thead": [
        {
          "label": "渠道",
          "prop": "ownChannelCode",
          "align": "center",
          "dict": {
            "dictType": "E_OWN_CHANNEL_CODE3_STATUS",
            "dictKey": [
              "E_OWN_CHANNEL_CODE3_STATUS"
            ],
            "format": "label"
          }
        },
        {
          "label": "渠道方申请单号",
          "prop": "applyNo",
          "align": "center"
        },
        {
          "label": "授信申请流水号（内部）",
          "prop": "myApplyNo",
          "align": "center"
        },
        {
          "label": "申请金额",
          "prop": "applyAmont",
          "align": "center"
        },
        {
          "label": "错误码",
          "prop": "retCode",
          "align": "center"
        },
        {
          "label": "错误信息",
          "prop": "retText",
          "align": "center"
        },
        {
          "label": "身份证号",
          "prop": "certNo",
          "align": "center"
        },
        {
          "label": "手机号",
          "prop": "userTel",
          "align": "center"
        },
        {
          "label": "用户id",
          "prop": "userId",
          "align": "center"
        },
        {
          "label": "客户号",
          "prop": "custId",
          "align": "center"
        },
        {
          "label": "订单号",
          "prop": "orderNo",
          "align": "center"
        },
        {
          "label": "授信审批状态",
          "prop": "clapplStatus",
          "align": "center",
          "dict": {
            "dictType": "E_CLAPPL_STATUS",
            "dictKey": [
              "E_CLAPPL_STATUS"
            ],
            "format": "label"
          }
        },
        {
          "label": "订单状态",
          "prop": "trxnStatus",
          "align": "center",
          "dict": {
            "dictType": "E_TRXN_STATUS",
            "dictKey": [
              "E_TRXN_STATUS"
            ],
            "format": "label"
          }
        },
        {
          "label": "伙伴中心通知状态",
          "prop": "ipmsStatus",
          "align": "center",
          "dict": {
            "dictType": "E_IPMS_STATUS",
            "dictKey": [
              "E_IPMS_STATUS"
            ],
            "format": "label"
          }
        },
        {
          "label": "渠道方通知状态",
          "prop": "notifyStatus",
          "align": "center",
          "dict": {
            "dictType": "E_NOTIFY_STATUS",
            "dictKey": [
              "E_NOTIFY_STATUS"
            ],
            "format": "label"
          }
        },
        {
          "label": "创建时间",
          "prop": "createTime",
          "align": "center"
        },
        {
          "label": "更新时间",
          "prop": "updateTime",
          "align": "center"
        }
      ]
    }
  }
}