{
  "module": "v_search_btn_datagrid",
  "layout": {

    "form": {
      "controls": [
        {
          "applid": {
            "control": "text",
            "label": "申请书编号",
            "span": 8,
            "rules": [
              {
                "max": 18,
                "message": "长度不能超过18个字符"
              }
            ]
          }
        }

      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "custom",
          "target": "dialog",

          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "trddjb",
              "istgal":"0",
              "selectName": "deadjb"

            }
          },

          "label": "触发异步唤醒",
          "confirm":{
            "title":"提示信息",
            "msg":"确认要触发异步唤醒吗？",
            "confirmButtonText":"确定",
            "cancelButtonText":"取消"
          }

        },
        {
          "type": "custom",
          "target": "dialog",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "trddjb",
              "istgal":"1"
            }
          },
          "label": "全选触发异步唤醒",
          "confirm":{
            "title":"提示信息",
            "msg":"确认要全选触发异步唤醒吗？",
            "confirmButtonText":"确定",
            "cancelButtonText":"取消"
          }

        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "onlySaveCheckedData":true,
        "params": {
          "servicecode": "qrddjb"
        }
      },

      "thead": [
        {
          "type": "selection",
          "align": "center"
        },
        {
          "label": "申请书编号",
          "prop": "applid",
          "align": "center"
        },
        {
          "label": "当前节点",
          "prop": "cutask",
          "align": "center"
        },
        {
          "label": "任务id",
          "prop": "jobbid",
          "align": "center"
        },
        {
          "label": "客户名称",
          "prop": "custna",
          "align": "center"

        },
        {
          "label": "异常信息",
          "prop": "expmsg",
          "align": "center"

        },
        {
          "label": "操作",
          "fixed": "right",
          "width": "200",
          "align": "center",
          "btns": [
            {

              "label": "作废",
              "type": "edit",
              "props": {
                "title": "流程作废",
                "width": "35%"
              },
              "toRequest": {
                "method": "GET",
                "url": "/views/workbench/asynchronous/asynchronous_del_do.json"
              }

            }
          ]
        }
      ]
    }
  }
}
