{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "confirm":{
            "title":"提示信息",
            "msg":"确认要作废该笔流程吗？",
            "confirmButtonText":"确定",
            "cancelButtonText":"取消"
          },
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "post",
            "params": {
              "servicecode": "delasynchrous",
              "method":"del",
              "applid": "${{params.applid}}",
              "jobbid": "${{params.jobbid}}"
            }
          }
        }
      ],
      "controls": [
        {
          "refuwy": {
            "label": "作废原因",
            "width": "148px",
            "control": "textarea",
            "span":24,
            "autosize":{"minRows": 2,"maxRows":4},
            "rules": [
              {
                "max": "200",
                "message": "长度不能超过200个字符"
              }
            ]
          }
        }

      ]
    }
  }
}
