{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "btnPosition":"center",
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ICMSLNTS0003",
          "applid":""
        }
      },
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "param": {
          "servicecode": "ICMSLNTS0004",
          "applid":""
        }
      },
      "controlsGroup": [
        {
          "title": "基本信息",
          "accordion": false,
          "expand": true,
          "controls": [
            {
              "custna": {
                "control": "text",
                "label": "客户姓名:",
                "disabled": true
              },
              "gender": {
                "control": "select",
                "label": "性别:",
                "disabled": true,
                "dict": {
                  "dictType": "MS_E_GENDER",
                  "dictKey": [
                    "MS_E_GENDER"
                  ],
                  "format": "label"
                }
              }
            },
            {
              "mobino": {
                "control": "text",
                "label": "手机号码:",
                "disabled": true
              }
            }

          ]
        },
        {
          "title": "单位信息",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "expand": true,
          "controls": [
            {
              "emplna": {
                "control": "text",
                "label": "单位名称:",
                "disabled": true
              },
              "custna": {
                "control": "text",
                "label": "单位电话:",
                "disabled": true
              }
            },
            {
              "emptel": {
                "control": "text",
                "label": "单位地址:",
                "disabled": true
              }
            }
          ]
        },
        {
          "title": "住宅信息",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "expand": true,
          "controls": [
            {
              "area1": {
                "control": "dynamic",
                "modelName": "area1",
                "toRequest": {
                  "url": "/views/workbench/AOther/CusContactInfo.json"
                },
                "isShowChildFrom": true
              }
            }
          ]
        },
        {
          "title": "联系人信息",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "expand": true,
          "controls": [
            {
              "house_addr": {
                "control": "text",
                "label": "住宅地址:",
                "disabled": true
              }
            }
          ]
        },
        {
          "title": "审核结果",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "expand": true,
          "controls": [
            {
              "result": {
                "control": "radio",
                "option": [
                  {
                    "label": "通过",
                    "value": "S"
                  },
                  {
                    "label": "拒绝",
                    "value": "F"
                  }
                ]
              },
              "memo": {
                "control": "textarea",
                "label": "备注",
                "span": 12
              }
            }
          ]
        }
      ]
    }
  }
}
