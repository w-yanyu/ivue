{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "qritem",
           "instid":"${{params.proc_inst_id}}"

        }
      },
      "thead": [
        {
          "type": "index"
        },
        {
          "label": "活动定义名称",
          "prop": "task_def_name",
          "align":"center"
        },
        {
          "label": "代理用户",
          "prop": "agent_cd",
          "align":"center"
        },
        {
          "label":"处理机构",
          "prop": "brch_cd",
          "align":"center"
        },
        {
          "label": "原因",
          "prop": "reason_cd",
          "align":"center"

        },
        {
          "label": "处理时间",
          "prop": "submit_time",
          "align":"center"
        },
        {
          "label": "备注",
          "prop": "remark",
          "align":"center"
        }


      ]
    }
  }
}
