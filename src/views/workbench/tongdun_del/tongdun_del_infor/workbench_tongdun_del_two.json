{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ICMSLNTS0228list",
          "applid": "${{params.applid}}"
        }
      },

      "thead": [
        {
          "type": "index",
          "align": "center"
        },
        {
          "label": "规则编码",
          "prop": "ruleCode",
          "align": "center"
        },
        {
          "label": "规则风险决策结果",
          "prop": "ruleDealType",
          "align": "center"
        },
        {
          "label": "规则名称",
          "prop": "ruleName",
          "align": "center"
        },
        {
          "label": "对内拒绝原因码",
          "prop": "internalRejectReason",
          "align": "center"
        },
        {
          "label": "对内拒绝原因名称",
          "prop": "internalRejectReasonName",
          "align": "center"
        },
        {
          "label": "对外拒绝原因码",
          "prop": "externalRejectReason",
          "align": "center"
        },
        {
          "label": "对外拒绝原因名称",
          "prop": "externalRejectReasonName",
          "align": "center"
        },
        {
          "label": "重新申请时限",
          "prop": "reApplyTimeLimit",
          "align": "center"
        }
      ]
    }
  }
}
