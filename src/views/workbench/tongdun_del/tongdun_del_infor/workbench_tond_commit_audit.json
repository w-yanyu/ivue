{
  "module": "v-inputDataForm-btn",
    "layout": {
    "form": {
      "scope": "templateScope",
      "initDataOnce":true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ICMSLNTS0133",
          "applid": "${{params.applid}}",
          "curStep": "${{params.curStep}}"
        }
      },
      "btns":[
        {
          "type":"submit",
          "label":"提交"
        }
      ],
      "controls": [
        {
          "subtyp": {
            "control": "select",
            "label": "处理请求:",
            "filterable": false,
            "options": [
              {
                "label": "拒绝请求",
                "value": "0"
              },
              {
                "label": "同意请求",
                "value": "1"
              }
            ]
          }
        },
        {
          "delMessage": {
            "control": "textarea",
            "label": "处理建议",
            "placeholder":"请输入"
          }
        }
      ]
    }
  }
}
