{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {

      "controls": [
        {
          "applid": {
            "control": "text",
            "label": "申请号",
            "rules": [
              {
                "max": 30,
                "message": "长度不能超过30个字符"
              }
            ]
          },
          "custid": {

            "control": "text",
            "label": "客户号",
            "rules": [
              {
                "max": 30,
                "message": "长度不能超过20个字符"
              }
            ]
          }

        },
        {
          "custna": {

            "control": "text",
            "label": "客户名称",
            "rules": [
              {
                "max": 80,
                "message": "长度不能超过80个字符"
              }
            ]
          }

        }

      ]
    },
  
  "toolbar": {
    "btns": [
      {
        "type": "custom",
        "target": "dialog",
        "doRequest": {
          "url": "/SUMP/icmcall/icmRPCCall",
          "method": "POST",
          "params": {
            "servicecode": "ICMSLNTS0149",
            "istgal":"1"
          }
        },
        "label": "一键处理",
        "confirm":{
          "title":"提示信息",
          "msg":"确认要一键处理吗？",
          "confirmButtonText":"确定",
          "cancelButtonText":"取消"
        }

      }
    ]
  },

    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ICMSLNTS0091",
          "type": "1"
        }
      },

      "thead": [
        {
          "type": "index",
          "align": "center"
        },
        {
          "label": "申请编号",
          "prop": "applid",
          "align": "center"
        },
        {
          "label": "客户编号",
          "prop": "custid",
          "align": "center"
        },
        {
          "label": "客户名称",
          "prop": "custna",
          "align": "center"
        },
        {
          "label": "当前步骤",
          "prop": "curStep",
          "align": "center",
          "dict": {
            "dictType": "E_CURSTRP",
            "dictKey": [
              "E_CURSTRP"
            ],
            "format": "label"
          }
        },
        {
          "label": "决策流调用状态码",
          "prop": "reasonCode",
          "align": "center"
        },
        {
          "label": "决策流调用状态描述",
          "prop": "reasonDesc",
          "align": "center"
        },
        {
          "label": "处理状态",
          "prop": "type",
          "align": "center",
          "dict": {
            "dictType": "E_DEALTYPE",
            "dictKey": [
              "E_DEALTYPE"
            ],
            "format": "label"
          }

        },
        {
          "label": "操作",
          "fixed": "right",
          "width": 200,
          "align":"center",
          "btns": [

            {
              "type": "look",
              "label": "查看",
              "toRequest": {
                "url": "/views/integrated/credit_application/credit_application_result_second.json",
                "method": "POST"
              },
              "props": {
                "width": "1200px",
                "title": "查看"
              }
            },
            {
              "type": "look",
              "label": "处理",
              "toRequest": {
                "url": "/views/workbench/tongdun_del/tongdun_del_infor/workbench_tongdun_tab.json",
                "method": "post"
              },
              "props": {
                "width": "65%",
                "title": "申请查询"
              }
            }

          ]
        }
      ]
    }
  }
}
