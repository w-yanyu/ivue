{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "scope": "formData",
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": "",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "AS_ICMS_PD_PROLIIN"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "prodCd": {
                        "control": "text",
                        "label": "产品编号",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入产品代码"
                            },
                            {
                                "max": 18,
                                "message": "长度不能超过18个字符"
                            }
                        ]
                    },
                    "prodName": {
                        "control": "text",
                        "label": "产品名称",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入产品名称"
                            },
                            {
                                "max": 100,
                                "message": "长度不能超过100个字符"
                            }
                        ]
                    }
                },
                {
                    "prodType": {
                        "control": "select",
                        "label": "产品类别",
                        "width": "130px",
                        "dict": {
                            "dictType": "E_PROTYPE",
                            "dictKey": [
                                "E_PROTYPE"
                            ],
                            "format": "label",
                            "value": "A"
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择产品类别"
                            }
                        ]
                    }
                }
            ]
        }
    }
}