{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": "",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "AS_ICMS_PD_PROEXIN"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "prodCd": {
                        "control": "hidden",
                        "label": "产品编号",
                        "value": "${{params.prodCd}}"
                    },
                    "prodName": {
                        "control": "hidden",
                        "label": "产品名称",
                        "value": "${{params.prodName}}"
                    },
                    "exclusiveProdCd": {
                        "control": "text",
                        "label": "兼容产品号",
                        "width": "150px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入兼容产品号"
                            },
                            {
                                "max": 10,
                                "message": "长度不能超过10个字符"
                            }
                        ]
                    },
                    "exclusiveProdName": {
                        "control": "text",
                        "label": "兼容产品名称",
                        "width": "150px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入兼容产品名称"
                            },
                            {
                                "max": 100,
                                "message": "长度不能超过100个字符"
                            }
                        ]
                    }
                },
                {
                    "exclusiveProdCdOld": {
                        "control": "text",
                        "label": "兼容老系统产品号",
                        "width": "150px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入兼容老系统产品号"
                            },
                            {
                                "max": 100,
                                "message": "长度不能超过100个字符"
                            }
                        ]
                    },
                    "enable": {
                        "control": "select",
                        "label": "是否兼容",
                        "width": "150px",
                        "dict": {
                            "dictType": "E_PROENABLE",
                            "dictKey": [
                                "E_PROENABLE"
                            ],
                            "format": "label",
                            "value": "N"
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择是否兼容"
                            }
                        ]
                    }
                }
            ]
        }
    }
}