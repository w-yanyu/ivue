{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "applid": {
            "control": "text",
            "label": "申请编号",
            "rules": [
              {
                "max": 30,
                "message": "长度不能超过30个字符"
              },
              {
                "required": false,
                "message": "请输入申请编号"
              }
            ]
          },
          "applNo": {
            "control": "text",
            "label": "外部申请单号",
            "rules": [
              {
                "max": 30,
                "message": "长度不能超过30个字符"
              }
            ]
          }
        },
        {
          "custid": {
            "control": "text",
            "label": "客户编号",
            "rules": [
              {
                "max": 30,
                "message": "长度不能超过30个字符"
              }
            ]
          },
          "custna": {
            "control": "text",
            "label": "客户名称",
            "rules": [
              {
                "max": 80,
                "message": "长度不能超过80个字符"
              }
            ]
          }
        },
        {
          "certTp": {
            "control": "select",
            "label": "证件类型",
            "dict": {
              "dictType": "E_IDTYPT",
              "dictKey": [
                "E_IDTYPT"
              ],
              "format": "label"
            },
            "rules": [
              {
                "max": 20,
                "message": "长度不能超过20个字符"
              }
            ]
          },
          "certNum": {
            "control": "text",
            "label": "证件号码",
            "rules": [
              {
                "max": 30,
                "message": "长度不能超过30个字符"
              }
            ]
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ICMSLNTS0088",
          "page_id": "/views/pc/pc_result/pc_result.json"
        }
      },
      "thead": [
        {
          "label": "序号",
          "type": "index",
          "align": "center"
        },
        {
          "label": "申请编号",
          "prop": "applid",
          "align": "center",
          "width": 180
        },
        {
          "label": "外部申请单号",
          "prop": "applNo",
          "align": "center"
        },
        {
          "label": "客户编号",
          "prop": "custid",
          "align": "center"
        },
        {
          "label": "客户名称",
          "prop": "custna",
          "align": "center"
        },
        {
          "label": "证件类型",
          "prop": "certTp",
          "align": "center",
          "dict": {
            "dictType": "E_CRETFORPC",
            "dictKey": [
              "E_CRETFORPC"
            ],
            "format": "label"
          }
        },
        {
          "label": "证件号码",
          "prop": "certNum",
          "align": "center",
          "width": 180
        },
        {
          "label": "申请产品号",
          "prop": "prodCd",
          "align": "center"
        },
        {
          "label": "申请产品名称",
          "prop": "prodNa",
          "align": "center"
        },
        {
          "label": "已有业务产品源系统",
          "prop": "origSys",
          "align": "center",
          "dict": {
            "dictType": "E_ORIGSYS",
            "dictKey": [
              "E_ORIGSYS"
            ],
            "format": "label"
          }
        },
        {
          "label": "已有产品号",
          "prop": "ownProdCd",
          "align": "center"
        },
        {
          "label": "已有产品名称",
          "prop": "ownProdNa",
          "align": "center"
        },
        {
          "label": "兼容标志",
          "prop": "enable",
          "align": "center",
          "dict": {
            "dictType": "E_PROENABLE",
            "dictKey": [
              "E_PROENABLE"
            ],
            "format": "label"
          }
        }
      ]
    }
  }
}
