{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "dfpacd": {
            "control": "text",
            "label": "合作方编号：",
            "rules": [
              {
                "max": 20,
                "message": "长度不能超过20个字符"
              }
            ]
          },
          "ofpana": {
            "control": "text",
            "label": "合作方名称",
            "rules": [
              {
                "max": 20,
                "message": "长度不能超过20个字符"
              }
            ]
          }

        }

      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "qrofma"
        }
      },
      "thead": [
        {
          "type": "index"
        },
        {
          "label": "合作方编号",
          "prop": "dfpacd",
          "align":"center"
        },
        {
          "label": "合作方名称",
          "prop": "ofpana",
          "align":"center"
        },

        {
          "label": "合作方合同编号",
          "prop": "cortno",
          "align":"center"
        },
        {
          "label": "合作方规模金额",
          "prop": "partsz",
          "align":"center"
        },
        {
          "label": "单日最大放款金额",
          "prop": "datmax",
          "align":"center"
        },
        {
          "label": "单日最大放款笔数",
          "prop": "maxnum",
          "align":"center"
        },
        {
          "label": "单笔最大放款额",
          "prop": "sinmax",
          "align":"center"
        },

        {
          "label": "单笔最小放款额",
          "prop": "sinmin",
          "align":"center"
        },
        {
          "label": "所属机构",
          "prop": "blbrch",
          "align":"center"
        },

        {
          "label": "操作",
          "width": 70,
          "btns": [{
            "type": "bringback",
            "label": "选择"
          }]
        }
      ]
    }
  }
}
