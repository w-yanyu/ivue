{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "bsquad"
            }
          }
        }
      ],

      "controls": [
        {
          "dfpmot": {
            "control": "text",
            "label": "月度最大放款限额(元)",
            "width": "50%",

            "rules": [
              {
                "required": true,
                "message": "月度最大放款限额"
              }

            ]

          },

           "dfpacd": {
            "control": "text",
            "label": "合作方编号",
             "disabled": true,
             "value": "${{params.dfpacd}}",
            "width": "50%"
          }

        },
        {
          "qutana": {
            "control": "text",
            "label": "限额对象名称",
            "width": "50%",
            "rules": [
              {
                "required": true,
                "message": "请输入限额对象名称"
              }

            ]
          },
          "ofpana": {
            "control": "text",
            "label": "合作方名称",
            "disabled": true,
            "width": "50%",
            "value": "${{params.ofpana}}"
          }
        },
        {

          "cortno": {
            "control": "text",
            "label": "合作方合同编号",
            "width": "50%",
            "disabled": true,
            "value": "${{params.cortno}}"
          },
          "partsz": {
            "control": "text",
            "label": "合作方规模金额",
            "width": "50%",
            "disabled": true,
            "value": "${{params.partsz}}"
          }
        },
        {
          "sinmin": {
            "control": "text",
            "label": "单笔最小放款额度(元)",
            "width": "50%",
            "disabled": true

          },

          "maxnum": {
            "control": "text",
            "label": "单日最大放款笔数(笔)",
            "width": "50%",
            "disabled": true

          }
        },
        {
          "dfpdat": {
            "control": "text",
            "label": "单日最大放款限额(元)",
            "width": "50%",

            "disabled": true

          },
          "sinmax": {
            "control": "text",
            "label": "单笔最大放款额度(元)",
            "disabled": true,
            "width": "50%"

          }
        },
        {


          "idd": {

            "control": "lookup",
            "placeholder": "请选择信息...",
            "label": "协议信息：",
            "prop": "id",
            "width": "50%",
            "edit": true,
            "keyField":{

              "dfpdat": "datmax",
              "maxnum": "maxnum",
              "partsz": "partsz",
              "cortno": "cortno",
              "ofpana": "ofpana",
              "dfpacd": "dfpacd",
              "sinmin": "sinmin",
              "sinmax": "sinmax",
              "params": "params",
              "prodna":"prodna"
            },
            "toRequest": {
              "url": "/views/bl/businesslimit/businesslimit_manager_add3.json",
              "method": "post",
              "params": {
                "qutaid": "${{params.qutaid}}"
              }
            },
            "props": {
              "title": "协议信息选择"
            }
          },
          "qutaty": {
            "label": "限额类型",
            "disabled": true,
            "width": "50%",
            "value": "04",
            "rules": [
              {
                "required": true,
                "message": "请选择业务限额类型"
              }

            ],
            "dict": {
              "dictType": "E_LIMIT",
              "dictKey": [
                "E_LIMIT"
              ],
              "format": "label"
            }
          }
        },


        {

          "brchcd": {
            "value": [

              {
                "branch": "11"
              }

            ]
          }
        }
      ]
    }
  }
}
