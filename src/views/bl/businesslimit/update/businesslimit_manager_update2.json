{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "crdtcd": {
            "control": "text",
            "label": "可售产品编号：",
            "rules": [
              {
                "max": 20,
                "message": "长度不能超过20个字符"
              }
            ]
          },
          "prodna": {
            "control": "text",
            "label": "产品名称",
            "rules": [
              {
                "max": 20,
                "message": "长度不能超过20个字符"
              }
            ]
          }

        }

      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "qrpdls",
          "clstat": "00"
        }
      },
      "thead": [
        {
          "type": "index"
        },
        {
          "label": "可售产品编号",
          "prop": "crdtcd",
          "align":"center"
        },
        {
          "label": "产品名称",
          "prop": "prodna",
          "align":"center",
          "dict": {
            "dictType": "E_IDTFTP",
            "dictKey": [
              "E_IDTFTP"
            ],
            "format": "label"
          }
        },
        {
          "label": "操作",
          "width": 70,
          "btns": [{
            "type": "bringback",
            "label": "选择"
          }]
        }
      ]
    }
  }
}
