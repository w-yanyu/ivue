{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {

      "initDataOnce":true,
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "bsquvi",
          "qutaid": "${{params.qutaid}}"
        }
      },

      "btns": [

        {
          "type": "submit",
          "label": "修改",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "bsquup",
              "qutaid": "${{params.qutaid}}",
              "brchcd": [
                {
                  "branch": "11"
                }

              ]
            }
          }
        }
      ],

      "controls": [
        {

          "prodam": {
            "control": "text",
            "label": "单日最大放款限额(元)：",
            "width": "50%",
            "rules": [
              {
                "required": true,
                "message": "请输入单日最大放款限额(元)"
              }

            ]

          },
           "prodcd": {
            "control": "text",
            "label": "产品编号",
             "prop": "prodcd",
             "disabled": true,
            "width": "50%"
          }

        },
        {
          "qutana": {
            "control": "text",
            "label": "限额对象名称",
            "disabled": true,
            "width": "50%"
          },
          "prodna": {
            "control": "text",
            "label": "产品名称",
            "disabled": true,
            "width": "50%"
          }
        },
        {


          "prodmo": {
            "control": "text",
            "label": "月度最大放款限额(元)",
            "width": "50%",
            "rules": [
              {
                "required": true,
                "message": "月度最大放款限额"
              }

            ]

          },
          "idd": {

            "span": 7,
            "control": "lookup",
            "placeholder": "请选择信息...",
            "label": "协议信息",
            "prop": "idd",
            "width": "50%",
            "edit": true,
            "keyField":{
              "prodcd":"crdtcd",
              "prodna":"prodna"
            },
            "toRequest": {
              "url": "/views/bl/businesslimit/update/businesslimit_manager_update2.json",
              "method": "post",
              "params": {
                "qutaid": "${{params.qutaid}}"
              }
            },
            "props": {
              "width": "87%",
              "title": "协议信息选择"
            }
          }
        },
        {


          "qutaty": {
            "label": "限额类型",
            "disabled": true,
            "width": "50%",
            "value": "${{params.qutaty}}",
            "dict": {
              "dictType": "E_LIMIT",
              "dictKey": [
                "E_LIMIT"
              ],
              "format": "label"
            },
            "events":{
              "condition": {
                "01":{
                  "tabs":[
                    {
                      "id":"01",
                      "type":"123"
                    },{
                      "id": "02",
                      "type": "hide"
                    },{
                      "id": "03",
                      "type": "hide"
                    },
                    {
                      "id": "04",
                      "type": "hide"
                    }

                  ]
                },
                "02":{
                  "tabs":[
                    {
                      "id":"01",
                      "type":"hide"
                    },{
                      "id": "02",
                      "type": "123"
                    },{
                      "id": "03",
                      "type": "hide"
                    },
                    {
                      "id": "04",
                      "type": "hide"
                    }

                  ]
                },
                "03":{
                  "tabs":[
                    {
                      "id":"01",
                      "type":"hide"
                    },{
                      "id": "02",
                      "type": "hide"
                    },{
                      "id": "03",
                      "type": "123"
                    },
                    {
                      "id": "04",
                      "type": "hide"
                    }

                  ]
                },
                "04":{
                  "tabs":[
                    {
                      "id":"01",
                      "type":"hide"
                    },{
                      "id": "02",
                      "type": "hide"
                    },{
                      "id": "03",
                      "type": "hide"
                    },
                    {
                      "id": "04",
                      "type": "123"
                    }

                  ]
                }
              }
            }
          },
          "brchcd": {
            "span": 8,
            "value": [

              {
                "branch": "11"
              }

            ]
          }
        }
      ]
    }
  }
}
