{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {

      "initDataOnce":true,
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "bsquvi",
          "qutaid": "${{params.qutaid}}"
        }
      },
      "btns": [
        {
          "type": "submit",
          "label": "修改",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "bsquup",

              "brchcd": [
                {
                  "branch": "11"
                }

              ]
            }
          }
        }
      ],

      "controls": [
        {

          "dfpdat": {
            "control": "text",
            "label": "单日最大放款限额(元)",
            "width": "50%",

            "disabled": true

          },
           "dfpacd": {
            "control": "text",
            "label": "合作方编号",
             "disabled": true,

            "width": "50%"
          }

        },
        {
          "qutana": {
            "control": "text",
            "label": "限额对象名称",
            "disabled": true,
            "width": "50%",
            "rules": [
              {
                "required": true,
                "message": "请输入限额对象名称"
              }

            ]
          },
          "ofpana": {
            "control": "text",
            "label": "合作方名称",
            "disabled": true,
            "width": "50%"

          }
        },
        {

          "cortno": {
            "control": "text",
            "label": "合作方合同编号",
            "width": "50%",
            "disabled": true

          },
          "dfpasz": {
            "control": "text",
            "label": "合作方规模金额",
            "width": "50%",
            "disabled": true
          }
        },
        {
          "dfpmot": {
            "control": "text",
            "label": "月度最大放款限额(元)",
            "width": "50%",

            "rules": [
              {
                "required": true,
                "message": "月度最大放款限额"
              }

            ]

          },

          "maxnum": {
            "control": "text",
            "label": "单日最大放款笔数(笔)",
            "width": "50%",
            "disabled": true

          }
        },
        {

          "sinmax": {
            "control": "text",
            "label": "单笔最大放款额度(元)",
            "disabled": true,
            "width": "50%"

          },
          "sinmin": {
            "control": "text",
            "label": "单笔最小放款额度(元)",
            "width": "50%",
            "disabled": true


          }
        },
        {




          "idd": {
            "control": "lookup",
            "placeholder": "请选择信息...",
            "label": "协议信息",
            "prop": "idd",
            "width": "50%",
            "edit": true,
            "keyField":{
              "dfpdat": "datmax",
              "maxnum": "maxnum",
              "dfpasz": "partsz",
              "cortno": "cortno",
              "ofpana": "ofpana",
              "dfpacd": "dfpacd",
              "sinmin": "sinmin",
              "sinmax": "sinmax",
              "params": "params",
              "prodna":"prodna"
            },
            "toRequest": {
              "url": "/views/bl/businesslimit/update/businesslimit_manager_update4.json",
              "method": "post",
              "params": {
                "qutaid": "${{params.qutaid}}"
              }
            },
            "props": {
              "width": "87%",
              "title": "协议信息选择"
            }


          },
          "qutaty": {
            "label": "限额类型",
            "disabled": true,
            "width": "50%",
            "value": "${{params.qutaty}}",
            "dict": {
              "dictType": "E_LIMIT",
              "dictKey": [
                "E_LIMIT"
              ],
              "format": "label"
            },
            "events":{
              "condition": {
                "01":{
                  "tabs":[
                    {
                      "id":"01",
                      "type":"123"
                    },{
                      "id": "02",
                      "type": "hide"
                    },{
                      "id": "03",
                      "type": "hide"
                    },
                    {
                      "id": "04",
                      "type": "hide"
                    }

                  ]
                },
                "02":{
                  "tabs":[
                    {
                      "id":"01",
                      "type":"hide"
                    },{
                      "id": "02",
                      "type": "123"
                    },{
                      "id": "03",
                      "type": "hide"
                    },
                    {
                      "id": "04",
                      "type": "hide"
                    }

                  ]
                },
                "03":{
                  "tabs":[
                    {
                      "id":"01",
                      "type":"hide"
                    },{
                      "id": "02",
                      "type": "hide"
                    },{
                      "id": "03",
                      "type": "123"
                    },
                    {
                      "id": "04",
                      "type": "hide"
                    }

                  ]
                },
                "04":{
                  "tabs":[
                    {
                      "id":"01",
                      "type":"hide"
                    },{
                      "id": "02",
                      "type": "hide"
                    },{
                      "id": "03",
                      "type": "hide"
                    },
                    {
                      "id": "04",
                      "type": "123"
                    }

                  ]
                }
              }
            }
          }
        },


        {
          "brchcd": {
            "span": 8,
            "value": [

              {
                "branch": "11"
              }

            ]
          }
        }
      ]
    }
  }
}
