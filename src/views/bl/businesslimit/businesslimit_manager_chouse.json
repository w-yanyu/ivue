{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "bsquad"
            }
          }
        }
      ],

      "controls": [
        {
          "channa": {
            "control": "text",
            "label": "渠道名称",
            "disabled": true,
            "width": "50%"
          },

          "chdamo": {
            "control": "text",
            "label": "单日最大放款限额(元)",
            "width": "50%",
            "rules": [
              {
                "required": true,
                "message": "请输入单日最大放款限额"
              }

            ]

          }
        },
        {
          "chanid": {
            "control": "text",
            "label": "渠道编号",
            "disabled": true,
            "width": "50%"
          },
          "qutana": {
            "control": "text",
            "label": "限额对象名称",
            "width": "50%",
            "rules": [
              {
                "required": true,
                "message": "请输入限额对象名称"
              }

            ]
          }

        },
        {

          "chgamo": {
            "control": "text",
            "label": "渠道规模限额(元)",
            "width": "50%",
            "rules": [
              {
                "required": true,
                "message": "渠道规模限额(元)不得为空"
              }
            ]
          },
          "chmoth": {
            "control": "text",
            "label": "月度最大放款限额(元)",
            "width": "50%",
            "rules": [
              {
                "required": true,
                "message": "请输入法人名称"
              }

            ]

          }
        },
        {

          "idd": {
            "control": "lookup",
            "placeholder": "请选择信息...",
            "label": "协议信息",
            "prop": "id",
            "width": "50%",
            "edit": true,
            "keyField":{
              "chanid": "chanid",
              "channa": "channa"
            },
            "toRequest": {
              "url": "/views/bl/businesslimit/businesslimit_manager_add.json",
              "method": "post",
              "params": {
                "qutaid": "${{params.qutaid}}"
              }
            },
            "props": {
              "title": "协议信息选择"
            }
          },

          "qutaty": {
            "label": "业务限额类型",
            "disabled": true,
            "width": "50%",
            "value": "01",
            "rules": [
              {
                "required": true,
                "message": "请选择业务限额类型"
              }

            ],
            "dict": {
              "dictType": "E_LIMIT",
              "dictKey": [
                "E_LIMIT"
              ],
              "format": "label"
            }
          }
        },{

          "brchcd": {
            "value": [
              {
                "branch": "1"
              }

            ]
          }
        }

      ]
    }
  }
}
