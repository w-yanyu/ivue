{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "controls": [{
                    "colt_no": {
                        "label": "押品编号",
                        "control": "text"
                    },
                    "ln_no": {
                        "label": "业务合同号",
                        "control": "text"
                    }
                },
                {
                    "cust_name": {
                        "label": "客户姓名",
                        "control": "text"
                    }
                }
            ]
        },
        "datagrid": {
            "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "post",
                "params": {
                    "servicecode": "co3011",
                    "maintenanceStatus": "${{params.maintenanceStatus}}"
                }
            },
            "thead": [{
                    "label": "档案号",
                    "prop": "doc_no",
                    "align": "center"
                },
                {
                    "label": "客户名称",
                    "prop": "cust_name",
                    "align": "center"
                },
                {
                    "label": "押品编号",
                    "prop": "colt_no",
                    "align": "center"
                },
                {
                    "label": "押品描述",
                    "prop": "colt_desc",
                    "align": "center"
                },
                {
                    "label": "押品类型名称",
                    "prop": "colt_name",
                    "align": "center"
                },

                {
                    "label": "状态",
                    "prop": "state",
                    "align": "center",
                    "type": "leaf",
                    "dict": {
                        "dictType": "CO_E_STATUS",
                        "dictKey": [
                            "CO_E_STATUS"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "操作",
                    "width": "80px",
                    "btns": [{
                        "type": "bringback",
                        "label": "选择"
                    }],
                    "align": "center"
                }
            ]
        }
    }
}
