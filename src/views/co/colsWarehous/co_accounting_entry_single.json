{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "scope": "templateScope",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "outlistid": "cplOutput",
          "servicecode": "fa7076",
          "txn_rung_num_32": "${{params.txn_rung_num_32}}",
          "txn_dt_8":  "${{params.txn_dt_8}}"
        }
      },
      "thead": [
        {
          "align": "center",
          "label": "交易日期",
          "prop": "txn_dt_8"
        },
        {
          "align": "center",
          "label": "用户流水号",
          "width": 210,
          "prop": "txn_rung_num_32"
        },
        {
          "align": "center",
          "label": "货币代号",
          "prop": "ccy_code_num",
          "dict": {
            "dictType": "MS_CURRENCY",
            "dictKey": [
              "MS_CURRENCY"
            ],
            "format": "label"
          }
        },
        {
          "align": "center",
          "label": "收付标志",
          "prop": "db_cr_flg",
          "dict": {
            "dictType": "LN_E_DB_CR_FLG2",
            "dictKey": [
              "LN_E_DB_CR_FLG2"
            ],
            "format": "label"
          }
        },
        {
          "align": "center",
          "label": "科目号",
          "prop": "sbj_num"
        },
        {
          "align": "center",
          "label": "交易金额",
          "prop": "acctg_amt"
        },
        {
          "align": "center",
          "label": "账户名称",
          "width": 180,
          "prop": "acct_nm"
        },
        {
          "align": "center",
          "label": "交易码",
          "width": 180,
          "prop": "txn_code"
        }
      ]
    }
  }
}
