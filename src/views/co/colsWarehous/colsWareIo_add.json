{
    "module": "v_inputDataForm_btn",
    "layout": {
      "form": {
        "name": "detail",
        "btns": [],
        "controls": [
          {
            "doc_no": {
              "label": "档案编号",
              "width": "140px",
              "control": "lookup",
              "placeholder": "请选择档案编号",
              "disabled": false,
              "edit": true,
              "value": "",
              "keyField": {
                "doc_no": "doc_no",
                "colt_no": "colt_no",
                "cust_name": "cust_name",
                "cust_no": "cust_no"
              },
              "toRequest": {
                "url": "/views/co/colsWarehous/start/collSelect.json",
                "method": "get",
                "params": {
                  "maintenanceStatus": "1"
                }
              },
              "props": {
                "title": "档案选择",
                "width": "80%"
              },
              "rules": [
                {
                  "required": true,
                  "message": "请选择档案编号"
                }
              ]
            },
            "input_date": {
              "label": "入库日期",
              "width": "140px",
              "control": "dateTimePicker",
              "rules": [
                {
                  "required": true,
                  "message": "请选择入库日期"
                }
              ]
            },
            "colt_no": {
              "label": "抵质押品编号",
              "control":"hidden"
            }
          },
          {
            "ware_house_code": {
              "label": "库房",
              "width": "140px",
              "control": "lookup",
              "edit": true,
              "keyField": {
                "ware_house_code": "ware_house_code"
              },
              "toRequest": {
                "url": "/views/co/colsWarehous/start/warehouse_lookup.json",
                "method": "get",
                "params": {

                }
              }
            },
            "input_reason": {
              "label": "入库原因",
              "width": "140px",
              "control": "select",
              "multiple": false,
              "dict": {
                "dictType": "CO_E_INPUTRN",
                "dictKey": [
                  "CO_E_INPUTRN"
                ],
                "format": "label"
              },
              "rules": [
                {
                  "required": true,
                  "message": "请选择入库原因"
                }
              ]
            }
          },
          {
            "branch_bank_no": {
              "label": "放款网点",
              "control": "text",
              "width": "140px",
              "rules": [
                {
                  "required": true,
                  "message": "请输入放款网点"
                }
              ]
            },
            "othr_cert_no": {
              "label": "权证号",
              "width": "140px",
              "control": "text",
              "rules": [
                {
                  "required": true,
                  "message": "请输入权证号"
                }
              ]
            }
          },
          {
            "remark": {
              "label": "备注",
              "width": "140px",
              "rows": 4,
              "span": 24,
              "control": "textarea"
            },
            "deal_type": {
              "label": "交易类型",
              "width": "140px",
              "value": "in",
              "control": "hidden"
            },
            "state": {
              "label": "状态",
              "width": "140px",
              "value": "I",
              "control": "hidden"
            },
            "appv_state": {
              "label": "审核状态",
              "width": "140px",
              "value": "NR",
              "control": "hidden"
            },
            "cust_name": {
              "label": "客户姓名",
              "control": "hidden"
            },
            "cust_no":{
              "label": "客户编号",
              "control": "hidden"
          }
          },
          {
            "toDbsx": {
              "span": 24,
              "control": "dynamic",
              "#isShowChildrenForm": false,
              "toRequest": {
                "method": "GET",
                "url": "@FlowSubmitBtn",
                "params": {
                  "appId": "coOnl",
                  "servicecode": "co3001",
                  "finalUrl": "@CommWaitToDoDeal"
                }
              }
            }
          }
        ]

      }
    }
  }
