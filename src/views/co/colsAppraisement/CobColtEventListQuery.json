{
    "module": "v_search_btn_datagrid",
    "layout": {
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": true,
            "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "co4035",
                    "colt_no": "${{params.colt_no}}"  
                }
            },
            "pagination": {
                "pageSizes": [
                    10,
                    15,
                    20
                ],
                "show": true,
                "pageSize": 10
            },
            "thead": [
                {
                    "type": "index",
                    "label": "序号"
                },         
                {                
                    "prop": "event_date",
                    "label": "发生时间"
                },             
                {
                    "label": "业务类型",
                    "prop": "event_type",
                    "align": "center"
                },
                {
                    "label": "业务描述",
                    "prop": "event_desc",
                    "align": "center"
                  
                }
            ]
        }
    }
}