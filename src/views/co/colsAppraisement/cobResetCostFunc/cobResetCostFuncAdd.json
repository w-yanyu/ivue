{
    "module": "v-inputDataForm-btn",
    "layout": {
        "form": {
            "btns": [],
            "controls": [
                {
                    "purpri": {
                        "control": "currency",
                        "label": "购置价格(元)",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入购置价格"
                            }
                        ]
                    },
                    "frerat": {
                        "control": "currency",
                        "label": "运杂费率(%)",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入运杂费率"
                            }
                        ]
                    }
                },
                {
                    "eqinra": {
                        "control": "currency",
                        "label": "设备安装费率(%)",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入设备安装费率"
                            }
                        ]
                    },
                    "othera": {
                        "control": "currency",
                        "label": "其他费率(%)",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入其他费率"
                            }
                        ]
                    }
                },
                {
                    "retoco": {
                        "control": "currency",
                        "label": "重置总成本",
                        "disabled": true,
                        "compute": "${{form.purpri}}+${{form.purpri}}*${{form.frerat}}/100+${{form.purpri}}*${{form.eqinra}}/100+${{form.purpri}}*${{form.othera}}/100",
                        "span": 12
                    }
                },
                {
                    "stusye": {
                        "control": "currency",
                        "label": "尚可使用年限",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入尚可使用年限"
                            }
                        ]
                    },
                    "prusye": {
                        "control": "currency",
                        "label": "规定使用年限",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入规定使用年限"
                            }
                        ]
                    }
                },
                {
                    "yenera": {
                        "control": "currency",
                        "label": "年限法成新率",
                        "compute": "${{form.stusye}}/${{form.prusye}}",
                        "span": 12
                    },
                    "yenrwe": {
                        "control": "currency",
                        "label": "年限法成新率权重",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入年限法成新率权重"
                            }
                        ]
                    }
                },
                {
                    "obnera": {
                        "control": "currency",
                        "label": "观察法成新率",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入预付费用计提减值"
                            }
                        ]
                    },
                    "obnrwe": {
                        "control": "currency",
                        "label": "观察法成新率权重",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入观察法成新率权重"
                            }
                        ]
                    }
                },
                {
                    "excaur": {
                        "control": "currency",
                        "label": "预计可被利用的产能比率",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入预计可被利用的产能比率"
                            }
                        ]
                    },
                    "endeva": {
                        "control": "currency",
                        "label": "实体性贬值",
                        "disabled": true,
                        "span": 12,
                        "compute": "${{form.retoco}}-${{form.retoco}}*${{form.yenera}}*${{form.yenrwe}}/100-${{form.retoco}}*${{form.obnera}}*${{form.obnrwe}}/100",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入预计可被利用的产能比率"
                            }
                        ]
                    }
                },
                {
                    "fudeva": {
                        "control": "currency",
                        "label": "功能性贬值",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入预计可被利用的产能比率"
                            }
                        ]
                    },
                    "ecdeva": {
                        "control": "currency",
                        "label": "经济性贬值",
                        "disabled": true,
                        "span": 12,
                        "compute": "${{form.retoco}}-${{form.retoco}}*${{form.excaur}}*0.007",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入预计可被利用的产能比率"
                            }
                        ]
                    },
                    "servicecode": {
                        "control": "hidden",
                        "value": "co4027"
                    },
                    "colt_no": {
                        "control": "hidden",
                        "value": "${{params.colt_no}}"
                    },
                    "cust_name": {
                        "control": "hidden",
                        "value": "${{params.custna}}"
                    },
                    "cust_ni": {
                        "control": "hidden",
                        "value": "${{params.custid}}"
                    },
                    "oevafu": {
                        "control": "hidden",
                        "value": "${{params.colt_eval_meth_type}}"
                    },
                    "person_no": {
                        "control": "hidden",
                        "value": "${{params.person_no}}"
                    }
                },
                {
                    "toDbsx": {
                        "span": 24,
                        "control": "dynamic",
                        "#isShowChildrenForm": false,
                        "toRequest": {
                            "method": "GET",
                            "url": "@ColsAppraisementSubmitBtn"
                        }
                    }
                }
            ]
        }
    }
}
