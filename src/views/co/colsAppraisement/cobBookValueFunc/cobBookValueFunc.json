{
    "module": "v-inputDataForm-btn",
    "layout": {
        "form": {
            "btns": [],
            "controls": [
                {
                    "is_self_cyy": {
                        "control": "select",
                        "label": "是否本币",
                        "span": 12,
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        },
                        "showItem": {
                            "1": [
                              "eval_val"
                            ],
                            "0": [
                                "exchange_rate","colt_num"
                              ]
                          },
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择是否本币"
                            }
                        ]
                    },
                    "exchange_rate": {
                        "initShow": false,
                        "control": "currency",
                        "label": "汇率",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入汇率"
                            }
                        ]
                    },
                    "colt_num": {
                        "initShow": false,
                        "control": "currency",
                        "label": "数量",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入数量"
                            }
                        ]
                    },
                    "eval_val": {
                        "initShow": false,
                        "control": "currency",
                        "label": "账面价值(元)",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入购置价格"
                            }
                        ]
                    },
                    "colt_no": {
                        "control": "hidden",
                        "value": "${{params.colt_no}}"
                    },
                    "colt_eval_meth": {
                        "control": "hidden",
                        "value": "${{params.colt_eval_meth_type}}"
                    },
                    "person_no": {
                        "control": "hidden",
                        "value": "${{params.person_no}}"
                    },
                    "cust_name": {
                        "control": "hidden",
                        "value": "${{params.custna}}"
                    },
                    "cust_no": {
                        "control": "hidden",
                        "value": "${{params.custid}}"
                    }
                },
                {
                  "toDbsx": {
                    "span": 24,
                    "control": "dynamic",
                    "#isShowChildrenForm": false,
                    "toRequest": {
                      "method": "GET",
                      "url": "@FlowSubmitBtn",
                      "params": {
                        "appId": "coOnl",
                        "servicecode": "co4030",
                        "finalUrl": "@CommWaitToDoDeal",
                        "trans_type": "ADD"
                      }
                    }
                  }
                }



            ]
        }
    }
}
