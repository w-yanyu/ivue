{
    "module": "v_realonlyForm_btn",
    "layout": {
        "form": {
            "initRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "post",
                "params": {
                    "servicecode": "co4033",
                    "apply_no": "${{params.apply_no}}"
                }
            },
            "controlsGroup": [
                {
                    "title": "估值信息",
                    "tooltip": "",
                    "titleIcon": "",
                    "showHint": "",
                    "accordion": false,
                    "expand": true,
                    "controls": [
                        {
                            "eval_type": {
                                "control": "select",
                                "label": "评估类型",
                                "span": 12,
                                "aligin": "center",
                                "disabled": true,
                                "dict": {
                                    "dictType": "CO_E_EVALTP",
                                    "dictKey": [
                                        "CO_E_EVALTP"
                                    ],
                                    "format": "label"
                                }
                            },
                            "colt_eval_meth": {
                                "control": "select",
                                "label": "评估方法",
                                "span": 12,
                                "aligin": "center",
                                "disabled": true,
                                "dict": {
                                    "dictType": "CO_E_COLT_EVALUTION_METH",
                                    "dictKey": [
                                        "CO_E_COLT_EVALUTION_METH"
                                    ],
                                    "format": "label"
                                }
                            },
                            "eval_val": {
                                "control": "currency",
                                "label": "评估价值",
                                "span": 12,
                                "aligin": "center",
                                "disabled": true
                            },
                            "is_self_cyy": {
                                "control": "select",
                                "label": "是否本币",
                                "span": 12,
                                "dict": {
                                    "dictType": "MS_E_YESORNO",
                                    "dictKey": [
                                        "MS_E_YESORNO"
                                    ],
                                    "format": "label"
                                },
                                "disabled": true,
                                "showItem": {
                                    "1": [
                                        "eval_val"
                                    ],
                                    "0": [
                                      "exchange_rate","colt_num"
                                    ]
                                }
                            },
                            "colt_no": {
                                "control": "hidden",
                                "disabled": true,
                                "span": 12
                            },
                            "exchange_rate": {
                                "control": "text",
                                "label": "汇率",
                                "span": 12,
                                "disabled": true
                            },
                            "colt_num": {
                                "control": "text",
                                "label": "数量",
                                "span": 12,
                                "disabled": true
                            }
                        }
                    ]
                },
                {
                    "title": "押品信息",
                    "tooltip": "",
                    "titleIcon": "",
                    "showHint": "",
                    "accordion": false,
                    "expand": true,
                    "controls": [
                        {
                            "id2": {
                                "control": "dynamic",
                                "isShowChildForm": false,
                                "span": 24,
                                "toRequest": {
                                    "url": "/views/co/colsAppraisement/cobColt.json",
                                    "method": "get",
                                    "params": {
                                        "colt_no": "${{form.colt_no}}"
                                    }
                                }
                            }
                        }
                    ]
                }
            ]
        }
    }
}
