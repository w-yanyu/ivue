{
    "module": "v-inputDataForm-btn",
    "layout": {
        "form": {

          "scope": "formData",
          "controls": [
                {
                    "last_colt_eval_val": {
                        "control": "currency",
                        "label": "最近一年押品评估价值(元)",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入最近一年押品评估价值"
                            }
                        ]
                    },
                    "cur_review_point": {
                        "control": "currency",
                        "label": "当前评估对应点数",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入当前评估对应点数"
                            }
                        ]
                    }
                },
                {
                    "last_review_point": {
                        "control": "currency",
                        "label": "最近一次评估对应点数",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入最近一次评估对应点数"
                            }
                        ]
                    },
                    "cur_eval_date": {
                        "control": "dateTimePicker",
                        "editable": true,
                        "format": "yyyyMMdd",
                        "label": "前评估对应年月",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入前评估对应年月"
                            }
                        ]
                    }
                },
                {
                    "last_eval_date": {
                        "control": "dateTimePicker",
                        "editable": true,
                        "format": "yyyyMMdd",
                        "label": "最近一次评估对应年月",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入最近一次评估对应年月"
                            }
                        ]
                    },
                    "year_phy_loss_rate": {
                        "control": "currency",
                        "label": "年度物理折损率(%)",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入年度物理折损率(%)"
                            }
                        ]
                    },
                    "colt_no": {
                        "control": "hidden",
                        "value": "${{params.colt_no}}"
                    },
                    "cust_name": {
                        "control": "hidden",
                        "value": "${{params.custna}}"
                    },
                    "cust_no": {
                        "control": "hidden",
                        "value": "${{params.custid}}"
                    },
                    "colt_eval_meth": {
                        "control": "hidden",
                        "value": "${{params.colt_eval_meth_type}}"
                    },
                    "person_no": {
                        "control": "hidden",
                        "value": "${{params.person_no}}"
                    }
                },
            {
              "toDbsx": {
                "span": 24,
                "control": "dynamic",
                "#isShowChildrenForm": false,
                "toRequest": {
                  "method": "GET",
                  "url": "@FlowSubmitBtn",
                  "params": {
                    "appId": "coOnl",
                    "servicecode": "co4029",
                    "finalUrl": "@CommWaitToDoDeal",
                    "trans_type": "ADD"
                  }
                }
              }
            }


            ],
          "btns": [

          ]
        }
    }
}
