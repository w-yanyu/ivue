{
    "module": "v_realonlyForm_btn",
    "layout": {
        "form": {
            "initRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "post",
                "params": {
                    "servicecode": "co4032",
                    "apply_no": "${{params.apply_no}}"
                }
            },
            "controlsGroup": [
                {
                    "title": "估值信息",
                    "tooltip": "",
                    "titleIcon": "",
                    "showHint": "",
                    "accordion": false,
                    "expand": true,
                "controls": [
                {
                    "oevaty": {
                        "control": "select",
                        "label": "评估类型",
                        "span": 12,
                        "aligin": "center",
                        "disabled": true,
                        "dict": {
                            "dictType": "CO_E_EVALTP",
                            "dictKey": [
                                "CO_E_EVALTP"
                            ],
                            "format": "label"
                        }
                    },
                    "oevafu": {
                        "control": "select",
                        "label": "评估方法",
                        "span": 12,
                        "aligin": "center",
                        "disabled": true,
                        "dict": {
                            "dictType": "CO_E_COLT_EVALUTION_METH",
                            "dictKey": [
                                "CO_E_COLT_EVALUTION_METH"
                            ],
                            "format": "label"
                        }
                    },
                    "oevava": {
                        "control": "currency",
                        "label": "评估价值",
                        "span": 12,
                        "aligin": "center",
                        "disabled": true
                    },
                    "ryrent": {
                        "control": "currency",
                        "label": "实际年租金(元)",
                        "disabled": true,
                        "span": 12
                    },
                    "ovvape": {
                        "control": "currency",
                        "label": "平均空置率(%)",
                        "disabled": true,
                        "span": 12
                    },
                    "protax": {
                        "control": "currency",
                        "label": "房产税率(%)",
                        "disabled": true,
                        "span": 12
                    },
                    "baatax": {
                        "control": "currency",
                        "label": "营业税及附加税费率(%)",
                        "disabled": true,
                        "span": 12
                    },
                    "colt_no": {
                        "control": "hidden",
                        "disabled": true,
                        "span": 12
                    },
                    "mantax": {
                        "control": "currency",
                        "label": "管理税率(%)",
                        "disabled": true,
                        "span": 12
                    },
                    "othtax": {
                        "control": "currency",
                        "label": "其他税率(%)",
                        "disabled": true,
                        "span": 12
                    }
                },
                {
                    "inctax": {
                        "control": "currency",
                        "label": "所得税率(%)",
                        "disabled": true,
                        "span": 12
                    },
                    "distax": {
                        "control": "currency",
                        "label": "折现率(%)",
                        "disabled": true,
                        "span": 12
                    }
                },
                {
                    "reprde": {
                        "control": "currency",
                        "label": "剩余收益期限(年)",
                        "disabled": true,
                        "span": 12
                    }
                }
            ]
        },
        {
            "title": "押品信息",
            "tooltip": "",
            "titleIcon": "",
            "showHint": "",
            "accordion": false,
            "expand": true,
            "controls": [
                {
                    "id2": {
                        "control": "dynamic",
                        "isShowChildForm": false,
                        "span": 24,
                        "toRequest": {
                            "url": "/views/co/colsAppraisement/cobColt.json",
                            "method": "get",
                            "params": {
                                "colt_no": "${{form.colt_no}}"
                            }
                        }
                    }
                }
            ]
        }
    ]
}
}
}
