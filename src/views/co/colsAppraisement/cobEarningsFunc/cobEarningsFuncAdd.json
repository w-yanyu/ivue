{
    "module": "v-inputDataForm-btn",
    "layout": {
        "form": {
            "scope": "formData",
            "btns": [],
            "controls": [
                {
                    "ryrent": {
                        "control": "currency",
                        "label": "实际年租金(元)",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入实际年租金"
                            }
                        ]
                    },
                    "ovvape": {
                        "control": "currency",
                        "label": "平均空置率(%)",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入平均空置率"
                            }
                        ]
                    }
                },
                {
                    "protax": {
                        "control": "currency",
                        "label": "房产税率(%)",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入房产税率"
                            }
                        ]
                    },
                    "baatax": {
                        "control": "currency",
                        "label": "营业税及附加税费率(%)",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入营业税及附加税费率"
                            }
                        ]
                    }
                },
                {
                    "mantax": {
                        "control": "currency",
                        "label": "管理税率(%)",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入预付费用计提减值"
                            }
                        ]
                    },
                    "othtax": {
                        "control": "currency",
                        "label": "其他税率(%)",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入其他税率(%)"
                            }
                        ]
                    }
                },
                {
                    "inctax": {
                        "control": "currency",
                        "label": "所得税率(%)",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入预付费用计提减值"
                            }
                        ]
                    },
                    "distax": {
                        "control": "currency",
                        "label": "折现率(%)",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入其他税率(%)"
                            }
                        ]
                    }
                },
                {
                    "reprde": {
                        "control": "currency",
                        "label": "剩余收益期限(年)",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入出质股权比例"
                            }
                        ]
                    },
                    "servicecode": {
                        "control": "hidden",
                        "value": "co4028"
                    },
                    "colt_no": {
                        "control": "hidden",
                        "value": "${{params.colt_no}}"
                    },
                    "custna": {
                        "control": "hidden",
                        "value": "${{params.custna}}"
                    },
                    "custid": {
                        "control": "hidden",
                        "value": "${{params.custid}}"
                    },
                    "oevafu": {
                        "control": "hidden",
                        "value": "${{params.colt_eval_meth_type}}"
                    },
                    "person_no": {
                        "control": "hidden",
                        "value": "${{params.person_no}}"
                    }
                },
                {
                    "toDbsx": {
                        "span": 24,
                        "control": "dynamic",
                        "#isShowChildrenForm": false,
                        "toRequest": {
                            "method": "GET",
                            "url": "@ColsAppraisementSubmitBtn"
                        }
                    }
                }

            ]
        }
    }
}
