{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "post",
                "params": {
                    "servicecode": "co4017",
                    "funcid": "1"
                }
            },
            "btns": [
                {
                    "type": "custom",
                    "label": "保存",
                    "icon": "",
                    "doRequest": {
                        "url": "/SUMP/call/RPCCall",
                        "method": "post",
                        "params": {
                            "caozuolx": "0",
                            "servicecode": "ic1010",
                            "detail": {
                                "lend_no":"${{params.lend_no}}",
                                "ln_farm_class":"${{form.ln_farm_class}}",
                                "farm_size":"${{form.farm_size}}",
                                "ln_farm_purpose":"${{form.ln_farm_purpose}}",
                                "ln_farm_credit":"${{form.ln_farm_credit}}",
                                "main_form":"${{form.main_form}}",
                                "farm_purpose":"${{form.farm_purpose}}",
                                "farm_corp_size":"${{form.farm_corp_size}}",
                                "farm_corp_develop":"${{form.farm_corp_develop}}",
                                "support_poor":"${{form.support_poor}}",
                                "city_corp_develop":"${{form.city_corp_develop}}"
                               }
                        }
                    },
                  "eventAfter": {
                    "callback": "resetCallback",
                    "doLastCallback": false
                  }
                },
                {
                    "type": "custom",
                    "label": "删除",
                    "icon": "",
                    "doRequest": {
                        "url": "/SUMP/call/RPCCall",
                        "method": "post",
                        "params": {
                            "caozuolx": "2",
                            "servicecode": "ic1010",
                            "detail": {
                             "lend_no":"${{form.lend_no}}"
                            }
                        }
                    },
                    "eventAfter": {
                        "callback": "resetCallback"
                    }
                }
            ],
            "controls": [
                {
                    "funcid": {
                        "control": "hidden",
                        "value":"${{params.funcid}}"
                    },
                    "cprova": {
                        "control": "text",
                        "label": "工程完工价值",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入工程完工价值"
                            }
                        ]
                    },
                    "propet": {
                        "control": "text",
                        "label": "工程完工进度(%)",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入工程完工进度"
                            }
                        ]
                    }
                },
                {
                    "valdis": {
                        "control": "text",
                        "label": "价值折扣率(%)",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入价值折扣率"
                            }
                        ]
                    },
                    "baatax": {
                        "control": "text",
                        "label": "营业税及附加税费率(%)",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入营业税及附加税费率"
                            }
                        ]
                    }
                },
                {
                    "lavtax": {
                        "control": "text",
                        "label": "土地增值税税率(%)",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入土地增值税税率"
                            }
                        ]
                    },
                    "sataxp": {
                        "control": "text",
                        "label": "销售费率(%)",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入销售费率"
                            }
                        ]
                    }
                },
                {
                    "mataxp": {
                        "control": "text",
                        "label": "管理费率(%)",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入管理费率"
                            }
                        ]
                    },
                    "intaxp": {
                        "control": "text",
                        "label": "所得税率",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入所得税率"
                            }
                        ]
                    }
                }

            ]
        }
    }
}
