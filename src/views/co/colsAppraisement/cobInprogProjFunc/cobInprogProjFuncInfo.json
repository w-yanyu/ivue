{
    "module": "v_realonlyForm_btn",
    "layout": {
        "form": {
            "initRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "post",
                "params": {
                    "servicecode": "co4017",
                    "apply_no": "${{params.apply_no}}"
                }
            },
            "controlsGroup": [
                {
                    "title": "估值信息",
                    "tooltip": "",
                    "titleIcon": "",
                    "showHint": "",
                    "accordion": false,
                    "expand": true,
                    "controls": [
                        {
                            "eval_type": {
                                "control": "select",
                                "label": "评估类型",
                                "span": 12,
                                "aligin": "center",
                                "disabled": true,
                                "dict": {
                                    "dictType": "CO_E_EVALTP",
                                    "dictKey": [
                                        "CO_E_EVALTP"
                                    ],
                                    "format": "label"
                                }
                            },
                            "colt_eval_meth": {
                                "control": "select",
                                "label": "评估方法",
                                "span": 12,
                                "aligin": "center",
                                "disabled": true,
                                "dict": {
                                    "dictType": "CO_E_COLT_EVALUTION_METH",
                                    "dictKey": [
                                        "CO_E_COLT_EVALUTION_METH"
                                    ],
                                    "format": "label"
                                }
                            },
                            "eval_val": {
                                "control": "string",
                                "label": "评估价值",
                                "disabled": true,
                                "span": 12
                            }
                        },
                        {
                            "completed_val": {
                                "control": "text",
                                "label": "工程完工价值",
                                "disabled": true,
                                "span": 12
                            },
                            "completed_rate_prog": {
                                "control": "text",
                                "label": "工程完工进度(%)",
                                "span": 12,
                                "disabled": true
                            },
                            "colt_no": {
                                "control": "hidden",
                                "disabled": true,
                                "span": 12
                            }
                        },
                        {
                            "discount_val_rate": {
                                "control": "text",
                                "label": "价值折扣率(%)",
                                "span": 12,
                                "disabled": true
                            },
                            "business_surcharge_rate": {
                                "control": "text",
                                "label": "营业税及附加税费率(%)",
                                "span": 12,
                                "disabled": true
                            }
                        },
                        {
                            "land_value_added_tax_rate": {
                                "control": "text",
                                "label": "土地增值税税率(%)",
                                "span": 12,
                                "disabled": true
                            },
                            "selling_inserst_rate": {
                                "control": "text",
                                "label": "销售费率(%)",
                                "span": 12,
                                "disabled": true
                            }
                        },
                        {
                            "manage_fee_rate": {
                                "control": "text",
                                "label": "管理费率(%)",
                                "span": 12,
                                "disabled": true
                            },
                            "income_tax_rate": {
                                "control": "text",
                                "label": "所得税率",
                                "span": 12,
                                "disabled": true
                            }
                        }
                    ]
                },
                {
                    "title": "押品信息",
                    "tooltip": "",
                    "titleIcon": "",
                    "showHint": "",
                    "accordion": false,
                    "expand": true,
                    "controls": [
                        {
                            "id2": {
                                "control": "dynamic",
                                "isShowChildForm": false,
                                "span": 24,
                                "toRequest": {
                                    "url": "/views/co/colsAppraisement/cobColt.json",
                                    "method": "get",
                                    "params": {
                                        "colt_no": "${{form.colt_no}}"
                                    }
                                }
                            }
                        }
                    ]
                }
            ]
        }
    }
}
