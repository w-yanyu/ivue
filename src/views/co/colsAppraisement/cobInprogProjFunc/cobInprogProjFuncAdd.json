{
    "module": "v-inputDataForm-btn",
    "layout": {
        "form": {
            "scope": "formData",
            "btns": [],
            "controls": [
                {
                    "completed_floor_area": {
                        "control": "currency",
                        "label": "完工后建筑面积",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入工程完工价值"
                            }
                        ]
                    },
                    "presell_val": {
                        "control": "currency",
                        "label": "预售价值",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入预售价值"
                            }
                        ]
                    }
                },
                {
                    "completed_val": {
                        "control": "currency",
                        "label": "工程完工价值",
                        "span": 12,
                        "disabled": true,
                        "compute": "${{formData.completed_floor_area}}*${{formData.presell_val}}",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入工程完工价值"
                            }
                        ]
                    },
                    "completed_rate_prog": {
                        "control": "currency",
                        "label": "工程完工进度",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入工程完工进度"
                            }
                        ]
                    }
                },
                {
                    "discount_val_rate": {
                        "control": "currency",
                        "label": "价值折扣率(%)",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入价值折扣率"
                            }
                        ]
                    },
                    "business_surcharge_rate": {
                        "control": "currency",
                        "label": "营业税及附加税费率(%)",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入营业税及附加税费率"
                            }
                        ]
                    }
                },
                {
                    "land_value_added_tax_rate": {
                        "control": "currency",
                        "label": "土地增值税税率(%)",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入土地增值税税率"
                            }
                        ]
                    },
                    "selling_inserst_rate": {
                        "control": "currency",
                        "label": "销售费率(%)",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入销售费率"
                            }
                        ]
                    }
                },
                {
                    "manage_fee_rate": {
                        "control": "currency",
                        "label": "管理费率(%)",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入管理费率"
                            }
                        ]
                    },
                    "income_tax_rate": {
                        "control": "currency",
                        "label": "所得税率",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入所得税率"
                            }
                        ]
                    },
                    "servicecode": {
                        "control": "hidden",
                        "value": "co4026"
                    },
                    "colt_no": {
                        "control": "hidden",
                        "value": "${{params.colt_no}}"
                    },
                    "cust_name": {
                        "control": "hidden",
                        "value": "${{params.custna}}"
                    },
                    "cust_no": {
                        "control": "hidden",
                        "value": "${{params.custid}}"
                    },
                    "colt_eval_meth": {
                        "control": "hidden",
                        "value": "${{params.colt_eval_meth_type}}"
                    },
                    "person_no": {
                        "control": "hidden",
                        "value": "${{params.person_no}}"
                    }
                },
                {
                  "toDbsx": {
                    "span": 24,
                    "control": "dynamic",
                    "#isShowChildrenForm": false,
                    "toRequest": {
                      "method": "GET",
                      "url": "@FlowSubmitBtn",
                      "params": {
                        "appId": "coOnl",
                        "servicecode": "co4026",
                        "finalUrl": "@CommWaitToDoDeal",
                        "trans_type": "ADD"
                      }
                    }
                  }
                }
            ]
        }
    }
}
