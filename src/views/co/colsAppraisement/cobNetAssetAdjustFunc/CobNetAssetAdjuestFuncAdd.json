{
    "module": "v-inputDataForm-btn",
    "layout": {
        "form": {
            "btns": [],
            "controls": [
                {
                    "total_net_asset_amt": {
                        "control": "currency",
                        "label": "净资产总额(元)",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入净资产总额"
                            }
                        ]
                    },
                    "receivable_account": {
                        "control": "currency",
                        "label": "应收账款(元)",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入应收账款"
                            }
                        ]
                    }
                },
                {
                    "receivable_account_impairment": {
                        "control": "currency",
                        "label": "应收账款计提减值(元)",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入应收账款计提减值"
                            }
                        ]
                    },
                    "prepaid_fee": {
                        "control": "currency",
                        "label": "预付费用(元)",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入预付费用"
                            }
                        ]
                    }
                },
                {
                    "prepaid_fee_impairment": {
                        "control": "currency",
                        "label": "预付费用计提减值(元)",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入预付费用计提减值"
                            }
                        ]
                    }
                },
                {
                    "pledge_stock_perc": {
                        "control": "currency",
                        "label": "出质股权比例%",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入出质股权比例"
                            }
                        ]
                    },

                    "colt_no": {
                        "control": "hidden",
                        "value": "${{params.colt_no}}"
                    },
                    "cust_name": {
                        "control": "hidden",
                        "value": "${{params.custna}}"
                    },
                    "cust_no": {
                        "control": "hidden",
                        "value": "${{params.custid}}"
                    },
                    "colt_eval_meth": {
                        "control": "hidden",
                        "value": "${{params.colt_eval_meth_type}}"
                    },
                    "person_no": {
                        "control": "hidden",
                        "value": "${{params.person_no}}"
                    }
                },
                {
                  "toDbsx": {
                    "span": 24,
                    "control": "dynamic",
                    "#isShowChildrenForm": false,
                    "toRequest": {
                      "method": "GET",
                      "url": "@FlowSubmitBtn",
                      "params": {
                        "appId": "coOnl",
                        "servicecode": "co4023",
                        "finalUrl": "@CommWaitToDoDeal",
                        "trans_type": "ADD"
                      }
                    }
                  }
                }
            ]
        }
    }
}
