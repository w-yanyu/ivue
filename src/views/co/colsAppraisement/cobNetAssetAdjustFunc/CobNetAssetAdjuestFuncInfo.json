{
    "module": "v_realonlyForm_btn",
    "layout": {
        "form": {
            "initRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "post",
                "params": {
                    "servicecode": "co4022",
                    "apply_no": "${{params.appl_id}}"
                }
            },
            "controlsGroup": [
                {
                    "title": "估值信息",
                    "tooltip": "",
                    "titleIcon": "",
                    "showHint": "",
                    "accordion": false,
                    "expand": true,
                    "controls": [
                        {
                            "eval_type": {
                                "control": "select",
                                "label": "评估类型",
                                "span": 12,
                                "aligin": "center",
                                "disabled": true,
                                "dict": {
                                    "dictType": "CO_E_EVALTP",
                                    "dictKey": [
                                        "CO_E_EVALTP"
                                    ],
                                    "format": "label"
                                }
                            },
                            "colt_eval_meth": {
                                "control": "select",
                                "label": "评估方法",
                                "span": 12,
                                "aligin": "center",
                                "disabled": true,
                                "dict": {
                                    "dictType": "CO_E_COLT_EVALUTION_METH",
                                    "dictKey": [
                                        "CO_E_COLT_EVALUTION_METH"
                                    ],
                                    "format": "label"
                                }
                            },
                            "total_net_asset_amt": {
                                "control": "currency",
                                "label": "净资产总额(元)",
                                "span": 12,
                                "disabled": true
                            },
                            "receivable_account": {
                                "control": "currency",
                                "label": "应收账款(元)",
                                "span": 12,
                                "disabled": true
                            }
                        },
                        {
                            "receivable_account_impairment": {
                                "control": "currency",
                                "label": "应收账款计提减值(元)",
                                "span": 12,
                                "disabled": true
                            },
                            "prepaid_fee": {
                                "control": "currency",
                                "label": "预付费用(元)",
                                "span": 12,
                                "disabled": true
                            }
                        },
                        {
                            "prepaid_fee_impairment": {
                                "control": "currency",
                                "label": "预付费用计提减值(元)",
                                "span": 12,
                                "disabled": true
                            },
                            "colt_no": {
                                "control": "hidden",
                                "disabled": true,
                                "span": 12
                            },
                            "eval_val": {
                                "control": "currency",
                                "label": "评估价值",
                                "span": 12,
                                "aligin": "center",
                                "disabled": true
                            }
                        },
                        {
                            "pledge_stock_perc": {
                                "control": "currency",
                                "label": "出质股权比例%",
                                "span": 12,
                                "disabled": true
                            }
                        }
                    ]
                },
                {
                    "title": "押品信息",
                    "tooltip": "",
                    "titleIcon": "",
                    "showHint": "",
                    "accordion": false,
                    "expand": true,
                    "controls": [
                        {
                            "id2": {
                                "control": "dynamic",
                                "isShowChildForm": false,
                                "span": 24,
                                "toRequest": {
                                    "url": "/views/co/colsAppraisement/cobColt.json",
                                    "method": "get",
                                    "params": {
                                        "colt_no": "${{form.colt_no}}"
                                    }
                                }
                            }
                        }
                    ]
                }
            ]
        }
    }
}
