{
    "module": "v_inputDataForm_btn",
    "layout": {
      "form": {
        "name": "detail",
        "initRequest": "${{params}}",
        "controls": [
        {
            "col_eval_no": {
                "control": "text",
                "label": "押品评估编号",
                "width": "130px",
                "disabled":true
            },
            "re_eval_flag": {
              "label": "是否需价值重估",
              "width": "140px",
              "control": "select",
              "disabled":true,
              "dict": {
                "dictType": "E_SHIFOUBZ",
                "dictKey": [
                  "E_SHIFOUBZ"
                ],
                "format": "label"
              }
            },
            "eval_type": {
              "label": "评估方式",
              "width": "140px",
              "control": "select",
              "disabled":true,
              "dict": {
                "dictType": "E_PTYPE",
                "dictKey": [
                  "E_PTYPE"
                ],
                "format": "label"
              }
            },
              "in_eval_type": {
                "label": "内部评估方式",
                "width": "140px",
                "control": "select",
                "disabled":true,
                "dict": {
                  "dictType": "E_INPTYPE",
                  "dictKey": [
                    "E_INPTYPE"
                  ],
                  "format": "label"
                }
              },
              "colt_eval_meth_type": {
                "label": "评估方法类型",
                "control": "remoteSelect",
                "disabled":true,
                "width": "140px",
                "labelField": "func_name",
                "valueField": "colt_eval_meth_type",
                "doRequest": {
                    "url": "/SUMP/icmcall/icmRPCCall",
                    "method": "POST",
                    "params": {
                        "servicecode": "co4020"
                    }
                },
                "format": "label"
              },
              "auto_eval_cyc": {
                "label": "自动评估频率",
                "width": "140px",
                "control": "select",
                "disabled":true,
                "dict": {
                  "dictType": "E_PCYCLE",
                  "dictKey": [
                    "E_PCYCLE"
                  ],
                  "format": "label"
                }
              }
        }
        ],
        "btns": [
          {
          }
        ]
      }
    }
  }
