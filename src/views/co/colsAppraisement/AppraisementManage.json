{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "controls": [{
                "oevaid": {
                    "label": "押品评估序号",
                    "control": "text"
                }
            }]
        },
        "toolbar": {
            "btns": [{
                "type": "add",
                "label": "押品出库新增",
                "toRequest": {
                    "url": "/views/cols/colsDelivery/start/startAdd.json",
                    "method": "get",
                    "params": {}
                },
                "props": {
                    "title": "押品出库新增",
                    "width": "80%"
                }
            }]
        },
        "datagrid": {
            "isInitSearch": true,
            "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "post",
                "params": {
                    "servicecode": "co4005",
                    "deal_type": "out",
                    "appv_status": "NR"
                }
            },
            "pagination": {
                "pageSizes": [10, 15, 20],
                "pageSize": 10
            },
            "thead": [{
                    "type": "index",
                    "align": "center"
                },
                {
                    "type": "leaf",
                    "label": "押品评估序号",
                    "align": "center",
                    "prop": "oevaid"
                },
                {
                    "label": "评估类型",
                    "prop": "oevaty",
                    "align": "center",
                    "type": "leaf",
                    "dict": {
                        "dictType": "CO_E_EVALTP",
                        "dictKey": [
                            "CO_E_EVALTP"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "评估方法",
                    "prop": "oevafu",
                    "align": "center",
                    "type": "leaf",
                    "dict": {
                        "dictType": "CO_E_COLT_EVALUTION_METH",
                        "dictKey": [
                            "CO_E_COLT_EVALUTION_METH"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "评估机构编码",
                    "prop": "organize_no",
                    "align": "center",
                    "type": "leaf"
                },
                {
                    "label": "人员编码",
                    "prop": "person_no",
                    "align": "center",
                    "type": "leaf"
                },
                {
                    "label": "操作",
                    "width": "220px",
                    "align": "center",
                    "fixed": "right",
                    "btns": [{
                            "type": "edit",
                            "label": "抵质押品修改",
                            "toRequest": {
                                "url": "/views/cols/colsDelivery/start/startUpdate.json",
                                "method": "get"
                            },
                            "props": {
                                "title": "抵质押品修改",
                                "width": "80%"
                            }
                        },
                        {
                            "type": "edit",
                            "label": "业务提交",
                            "doRequest": {
                                "url": "/SUMP/call/RPCCall",
                                "method": "post",
                                "params": {
                                    "servicecode": "co3002",
                                    "doc_tx_no": "${{current.doc_tx_no}}",
                                    "appv_status": "PA"
                                }
                            },
                            "confirm": {
                                "title": "提示信息",
                                "msg": "确定业务提交吗？",
                                "confirmButtonText": "确定",
                                "cancelButtonText": "取消"
                            }
                        },
                        {
                            "label": "抵质押品查看",
                            "type": "look",
                            "toRequest": {
                                "url": "${{current.url_query}}",
                                "method": "get",
                                "params": {
                                    "asset_list_id": "${{current.asset_list_id}}"
                                }
                            },
                            "props": {
                                "title": "抵质押品信息",
                                "width": "80%"
                            }
                        }
                    ]
                }
            ]
        }
    }
}
