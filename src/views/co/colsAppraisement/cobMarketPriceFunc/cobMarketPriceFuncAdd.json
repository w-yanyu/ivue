{
    "module": "v-inputDataForm-btn",
    "layout": {
        "form": {
            "btns": [],
            "controls": [
                {
                    "maket_eval_type": {
                        "control": "text",
                        "label": "市场评估类型",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入市场评估类型"
                            }
                        ]
                    },
                    "maket_price": {
                        "control": "text",
                        "label": "市场价格",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入市场价格"
                            }
                        ]
                    }
                },
                {
                    "count": {
                        "control": "text",
                        "label": "押品数量",
                        "span": 12,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入押品数量"
                            }
                        ]
                    },
                    "servicecode": {
                        "control": "hidden",
                        "value": "co4019"
                    },
                    "colt_no": {
                        "control": "hidden",
                        "value": "${{params.colt_no}}"
                    },
                    "cust_name": {
                        "control": "hidden",
                        "value": "${{params.custna}}"
                    },
                    "cust_no": {
                        "control": "hidden",
                        "value": "${{params.custid}}"
                    },
                    "colt_eval_meth": {
                        "control": "hidden",
                        "value": "${{params.colt_eval_meth_type}}"
                    },
                    "person_no": {
                        "control": "hidden",
                        "value": "${{params.person_no}}"
                    }
                },
                {
                  "toDbsx": {
                    "span": 24,
                    "control": "dynamic",
                    "#isShowChildrenForm": false,
                    "toRequest": {
                      "method": "GET",
                      "url": "@FlowSubmitBtn",
                      "params": {
                        "appId": "coOnl",
                        "servicecode": "co4019",
                        "finalUrl": "@CommWaitToDoDeal",
                        "trans_type": "ADD",
                        "eval_type": "1"
                      }
                    }
                  }
                }
            ]
        }
    }
}
