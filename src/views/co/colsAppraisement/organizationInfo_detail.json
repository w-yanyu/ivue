{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "organize_name": {
            "label": "评估机构名称",
            "control": "text"
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "co4010",
          "trantp": "SELECT"
        }
      },
      "pagination": {
        "pageSizes": [
          10,
          15,
          20
        ],
        "pageSize": 10
      },
      "thead": [
        {
          "type": "index",
          "align": "center"
        },
        {
          "label": "机构编码",
          "prop": "organize_no",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "机构名称",
          "prop": "organize_name",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "评估机构性质",
          "prop": "organize_nature",
          "align": "center",
          "type": "leaf",
          "dict": {
            "dictType": "E_PGJGXZ",
            "dictKey": [
              "E_PGJGXZ"
            ],
            "format": "label"
          }
        },
        {
          "label": "评估资质",
          "prop": "organize_grade",
          "align": "center",
          "type": "leaf",
          "dict": {
            "dictType": "E_PGZZDJ",
            "dictKey": [
              "E_PGZZDJ"
            ],
            "format": "label"
          }
        },
        {
          "label": "评估资质有效期",
          "prop": "organize_indate",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "所属分支行",
          "prop": "organize_branch",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "统一社会代码证",
          "prop": "organize_code",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "注册评估师人数",
          "prop": "appraiser_number",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "评估业务范围",
          "prop": "evaluate_scope",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "法人代表名称",
          "prop": "corp_name",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "法人代表身份证号",
          "prop": "corp_no",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "成立日期",
          "prop": "setup_date",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "注册资金",
          "prop": "register_money",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "联系方式",
          "prop": "phone_no",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "状态",
          "prop": "organize_status",
          "align": "center",
          "type": "leaf",
          "dict": {
            "dictType": "E_PSTATE",
            "dictKey": [
              "E_PSTATE"
            ],
            "format": "label"
          }
        },
        {
          "label": "公司简介",
          "prop": "company_intro",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "操作",
          "align": "center",
          "fixed": "right",
          "btns": [
            {
              "type": "bringback",
              "label": "选择"
            }
          ]
        }
      ]
    }
  }
}