                    }]
                },
                "colt_prod_cd": {
                    "label": "抵质押品产品编号",
                    "width": "140px",
                    "control": "text",
                    "rules": [{
                        "required": true,
                        "message": "请输入抵质押品产品编号"
                    }]
                },
                "other_colt_type": {
                    "label": "押品系统大类",
                    "width": "140px",
                    "control": "text",
                    "rules": [{
                        "required": true,
                        "message": "请输入押品系统大类"
                    }]
                },
                "other_colt_prod_cd": {
                    "label": "押品系统小类",
                    "width": "140px",
                    "control": "text",
