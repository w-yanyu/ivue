{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "name":"detail",
            "initRequest": "${{params}}",
            "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "co1003",
                    "tran_type":"2"
                }
            },
            "controls": [{
                "colt_rela_id": {
                    "label": "押品对应序号",
                    "width": "140px",
                    "disabled":true,
                    "control": "text",
                    "rules": [{
                        "required": true,
                        "message": "请输入押品对应序号"
                    }]
                },
                "colt_type": {
                    "label": "资产类型",
                    "width": "140px",
