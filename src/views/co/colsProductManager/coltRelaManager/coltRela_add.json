{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "name":"detail",
            "controls": [{
                "colt_rela_id": {
                    "label": "押品对应序号",
                    "width": "140px",
                    "control": "text",
                    "rules": [{
                        "required": true,
                        "message": "请输入押品对应序号"
                    }]
                },
                "colt_type": {
                    "label": "资产类型",
                    "width": "140px",
                    "control": "text",
                    "rules": [{
                        "required": true,
                        "message": "请输入资产类型"
                    }]
                },
                "colt_prod_cd": {
                    "label": "抵质押品产品编号",
                    "width": "140px",
                    "control": "text",
                    "rules": [{
                        "required": true,
                        "message": "请输入抵质押品产品编号"
                    }]
                },
                "other_colt_type": {
                    "label": "押品系统大类",
                    "width": "140px",
                    "control": "text",
                    "rules": [{
                        "required": true,
                        "message": "请输入押品系统大类"
                    }]
                },
                "other_colt_prod_cd": {
                    "label": "押品系统小类",
                    "width": "140px",
                    "control": "text",
                    "rules": [{
                        "required": true,
                        "message": "请输入押品系统小类"
                    }]
                }
            }],
            "initRequest": "${{params}}",
            "btns": [{
                "type": "submit",
                "label": "提交",
                "doRequest": {
                    "url": "/SUMP/call/RPCCall",
                    "method": "POST",
                    "params": {
                        "servicecode": "co1003",
                        "tran_type":"1"
                    }
                }
            }]
        }
    }
}
