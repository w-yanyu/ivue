{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "name": "detail",
            "controls": [{
                "colt_catg": {
                    "label": "抵质押品大类",
                    "width": "140px",
                    "control": "text",
                    "rules": [{
                        "required": true,
                        "message": "请输入抵质押品大类"
                    }]
                },
                "colt_catg_desc": {
                    "label": "抵质押品大类说明",
                    "width": "140px",
                    "control": "text",
                    "rules": [{
                        "required": true,
                        "message": "请输入抵质押品大类说明"
                    }]
                },
                "colt_coeff": {
                    "label": "折算系数(%)",
                    "width": "140px",
                    "control": "text",
                    "rules": [{
                        "required": true,
                        "message": "请输入折算系数"
                    }]
                },
                "is_low_risk": {
                    "label": "是否低风险",
                    "width": "140px",
                    "control": "select",
                    "multiple": false,
                    "value": "",
                    "dict": {
                        "dictType": "MS_E_YESORNO",
                        "dictKey": [
                            "MS_E_YESORNO"
                        ],
                        "format": "label"
                    },
                    "rules": [{
                        "required": true,
                        "message": "请选择是否低风险"
                    }]
                },
                "remark": {
                    "label": "备注",
                    "width": "140px",
                    "control": "text"
                }
            }],
            "btns": [{
                "type": "submit",
                "label": "保存",
                "doRequest": {
                    "url": "/SUMP/icmcall/icmRPCCall",
                    "method": "POST",
                    "params": {
                      "servicecode": "co1001",
                      "tran_type":"1"
                    }
                }
            },{
              "type": "reset",
              "label": "重置"
            }]
        }
    }
}
