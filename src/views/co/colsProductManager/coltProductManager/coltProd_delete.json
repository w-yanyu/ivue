{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "name": "detail",
            "controls": [{
                "colt_cat": {
                    "label": "抵质押品大类",
                    "width": "140px",
                    "control": "text",
                    "value": "${{params.id}}",
                    "rules": [{
                        "required": true,
                        "message": "请输入抵质押品大类"
                    }]
                },
                "colt_cat_desc": {
                    "label": "抵质押品大类说明",
                    "width": "140px",
                    "control": "text",
                    "rules": [{
                        "required": true,
                        "message": "请输入抵质押品大类说明"
                    }]
                },
                "colt_coeff": {
                    "label": "折算系数（%）",
                    "width": "140px",
                    "control": "text",
                    "rules": [{
                        "required": true,
                        "message": "请输入折算系数"
                    }]
                },
                "low_flag": {
                    "label": "是否低风险",
                    "width": "140px",
                    "control": "select",
                    "multiple": false,
                    "value": "",
                    "dict": {
                        "dictType": "MS_E_YESORNO",
                        "dictKey": [
                            "MS_E_YESORNO"
                        ],
                        "format": "label"
                    },
                    "rules": [{
                        "required": true,
                        "message": "请选择是否低风险"
                    }]
                },
                "remark": {
                    "label": "备注",
                    "width": "140px",
                    "control": "text",
                    "rules": [{
                        "required": true,
                        "message": "请输入备注"
                    }]
                },
                "is_auth_cat": {
                    "label": "是否授权押品类型",
                    "width": "140px",
                    "control": "select",
                    "multiple": false,
                    "value": "",
                    "dict": {
                        "dictType": "MS_E_YESORNO",
                        "dictKey": [
                            "MS_E_YESORNO"
                        ],
                        "format": "label"
                    },
                    "rules": [{
                        "required": true,
                        "message": "请选择是否授权押品类型"
                    }]
                }
            }],
            "btns": [{
                "type": "submit",
                "label": "提交",
                "doRequest": {
                    "url": "/SUMP/call/RPCCall",
                    "method": "POST",
                    "params": {
                        "servicecode": "co1001",
                        "caozuolx": "0"
                    }
                }
            }]
        }
    }
}