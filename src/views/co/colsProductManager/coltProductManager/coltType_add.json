{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "name": "detail",
            "controls": [{
                "colt_type": {
                    "label": "抵质押品类型编号",
                    "width": "140px",
                    "control": "text",
                    "rules": [{
                        "required": true,
                        "message": "请输入抵质押品类型编号"
                    }]
                },
                "guar_type": {
                    "label": "担保方式",
                    "width": "140px",
                    "control": "select",
                    "multiple": false,
                    "disable": false,
                    "value": "${{params.tree_id}}",
                    "dict": {
                        "dictType": "MS_E_GUAR_TYPE",
                        "dictKey": [
                            "MS_E_GUAR_TYPE"
                        ],
                        "format": "label"
                    },
                    "rules": [{
                        "required": true,
                        "message": "请选择担保方式"
                    }]
                },
                "colt_name": {
                    "label": "抵质押品名称",
                    "width": "140px",
                    "control": "text",
                    "rules": [{
                        "required": true,
                        "message": "请输入抵质押品名称"
                    }]
                },
                "colt_table": {
                    "label": "抵质押品表",
                    "width": "140px",
                    "control": "text",
                    "rules": [{
                        "message": "请输入抵质押品表"
                    }]
                },
                "entity_name": {
                    "label": "数据实体名称",
                    "width": "140px",
                    "control": "hidden",
                    "rules": [{
                        "message": "请输入数据实体名称"
                    }]
                },
                "url_input": {
                    "label": "输入页面",
                    "width": "140px",
                    "control": "text",
                    "rules": [{
                        "message": "请输入输入页面"
                    }]
                },
                "url_update": {
                    "label": "修改页面",
                    "width": "140px",
                    "control": "text",
                    "rules": [{
                        "message": "请输入修改页面"
                    }]
                },
                "url_query": {
                    "label": "查询页面",
                    "width": "140px",
                    "control": "text",
                    "rules": [{
                        "message": "请输入查询页面"
                    }]
                },
                "contract_valid_state": {
                    "label": "是否有效",
                    "width": "140px",
                    "control": "select",
                    "multiple": false,
                    "value": "",
                    "dict": {
                        "dictType": "MS_E_YESORNO",
                        "dictKey": [
                            "MS_E_YESORNO"
                        ],
                        "format": "label"
                    },
                    "rules": [{
                        "required": true,
                        "message": "请选择是否有效"
                    }]
                },
                "colt_sys_type": {
                    "label": "押品系统类型",
                    "width": "140px",
                    "control": "hidden",
                    "multiple": false,
                    "value": "",
                    "dict": {
                        "dictType": "CO_E_GUAROPT",
                        "dictKey": [
                            "CO_E_GUAROPT"
                        ],
                        "format": "label"
                    },
                    "rules": [{
                        "message": "请选择押品系统类型"
                    }]
                },
                "grace_term": {
                    "label": "抵质押宽限期(天)",
                    "width": "140px",
                    "control": "text",
                    "rules": [{
                        "message": "请输入抵质押宽限期"
                    }]
                },
                "sort_no": {
                    "label": "显示顺序 ",
                    "width": "140px",
                    "control": "hidden",
                    "rules": [{
                        "message": "请输入显示顺序"
                    }]
                }
            }],
            "btns": [{
                "type": "submit",
                "label": "提交",
                "doRequest": {
                    "url": "/SUMP/call/RPCCall",
                    "method": "POST",
                    "params": {
                        "servicecode": "co1005",
                        "tran_type": "1"
                    }
                }
            }]
        }
    }
}
