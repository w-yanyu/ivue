{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "controls": [
              {
                "colt_eval_no": {
                  "label": "押品评估编号",
                  "control": "text"
                }

              }
            ]
          },
      "datagrid": {
        "isInitSearch": true,
        "doRequest": {
          "url": "/SUMP/call/RPCCall",
          "method": "POST",
          "params": {
            "servicecode": "co1016"
          }
        },
        "thead": [
            {
              "type": "index",
              "align": "center"
            },
            {
              "label": "押品评估编号",
              "prop": "colt_eval_no",
              "align": "center",
              "type": "leaf"
            },
            {
              "label": "是否需价值重估",
              "prop": "is_need_reeval",
              "dict": {
                  "dictType": "MS_E_YESORNO",
                  "dictKey": [
                    "MS_E_YESORNO"
                  ],
                  "format": "label"
                },
              "align": "center",
              "type": "leaf"
            },
            {
              "label": "评估方式",
              "prop": "eval_way",
              "dict": {
                "dictType": "CO_E_EVALTYPE",
                "dictKey": [
                  "CO_E_EVALTYPE"
                ],
                "format": "label"
              },
              "align": "center",
              "type": "leaf"
            },
            {
              "label": "内部评估方式",
              "prop": "in_eval_type",
              "dict": {
                  "dictType": "CO_E_INEVALTYPE",
                  "dictKey": [
                    "CO_E_INEVALTYPE"
                  ],
                  "format": "label"
                },
              "align": "center",
              "type": "leaf"
            },
            {
              "label": "评估方法类型",
              "prop": "eval_meth_type_name",
              "align": "center",
              "type": "leaf"
            },
            {
              "label": "自动评估频率",
              "prop": "auto_eval_freq",
          
              "align": "center",
              "type": "leaf"
            },
            {
              "label": "自动评估频率单位",
              "prop": "auto_eval_freq_unit",
              "dict": {
                  "dictType": "CO_E_PCYCLE",
                  "dictKey": [
                    "CO_E_PCYCLE"
                  ],
                  "format": "label"
                },
              "align": "center",
              "type": "leaf"
            },
          {
            "label": "操作",
            "align": "center",
            "btns": [
              {
                "type": "bringback",
                "label": "选择"
              }
            ]
          }
        ]
      }
    }
  }
