{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "post",
                "params": {
                    "servicecode": "co1006",
                    "colt_type": "${{params.tree_id}}"
                }
            },
            "name": "detail",
            "controls": [{
                "colt_type": {
                    "label": "抵质押品类型编号",
                    "width": "140px",
                    "control": "text",
                    "rules": [{
                        "required": true,
                        "message": "请输入抵质押品类型编号"
                    }]
                },
                "guar_type": {
                    "label": "担保方式",
                    "width": "140px",
                    "control": "select",
                    "multiple": false,

                    "dict": {
                        "dictType": "MS_E_GUAR_TYPE",
                        "dictKey": [
                            "MS_E_GUAR_TYPE"
                        ],
                        "format": "label"
                    },
                    "rules": [{
                        "required": true,
                        "message": "请选择担保方式"
                    }]
                },
                "colt_name": {
                    "label": "抵质押品名称",
                    "width": "140px",
                    "control": "text",
                    "rules": [{
                        "required": true,
                        "message": "请输入抵质押品名称"
                    }]
                },
                "colt_table": {
                    "label": "抵质押品表",
                    "width": "140px",
                    "control": "text",
                    "rules": [{
                        "message": "请输入抵质押品表"
                    }]
                },
                "entity_name": {
                    "label": "数据实体名称",
                    "width": "140px",
                    "control": "hidden",
                    "rules": [{
                        "message": "请输入数据实体名称"
                    }]
                },
                "url_input": {
                    "label": "输入页面",
                    "width": "140px",
                    "control": "text",
                    "rules": [{
                        "message": "请输入输入页面"
                    }]
                },
                "url_update": {
                    "label": "修改页面",
                    "width": "140px",
                    "control": "text",
                    "rules": [{
                        "message": "请输入修改页面"
                    }]
                },
                "url_query": {
                    "label": "查询页面",
                    "width": "140px",
                    "control": "text",
                    "rules": [{
                        "message": "请输入查询页面"
                    }]
                },
                "contract_valid_state": {
                    "label": "是否有效",
                    "width": "140px",
                    "control": "select",
                    "multiple": false,
                    "value": "",
                    "dict": {
                        "dictType": "MS_E_YESORNO",
                        "dictKey": [
                            "MS_E_YESORNO"
                        ],
                        "format": "label"
                    },
                    "rules": [{
                        "required": true,
                        "message": "请选择是否有效"
                    }]
                },
                "colt_sys_type": {
                    "label": "押品系统类型",
                    "width": "140px",
                    "control": "hidden",
                    "multiple": false,
                    "value": "",
                    "dict": {
                        "dictType": "CO_E_GUAROPT",
                        "dictKey": [
                            "CO_E_GUAROPT"
                        ],
                        "format": "label"
                    },
                    "rules": [{
                        "message": "请选择押品系统类型"
                    }]
                },
                "grace_term": {
                    "label": "抵质押宽限期(天)",
                    "width": "140px",
                    "control": "text",
                    "rules": [{
                        "message": "请输入抵质押宽限期"
                    }]
                },
                "sort_no": {
                    "label": "显示顺序 ",
                    "width": "140px",
                    "control": "hidden",
                    "rules": [{
                        "message": "请输入显示顺序"
                    }]
                },
               "eval_meth_type_name": {
                    "label": "押品评估方法",
                    "width": "140px",
                    "control": "lookup",
                    "edit": true,
                    "keyField": {
                      "colt_eval_no": "colt_eval_no",
                      "eval_meth_type_name": "eval_meth_type_name",
                      "eval_way":"eval_way",
                      "in_eval_type":"in_eval_type",
                      "auto_eval_freq":"auto_eval_freq",
                      "auto_eval_freq_unit":"auto_eval_freq_unit"

                    },
                    "toRequest": {
                      "url": "/views/co/colsProductManager/coltProductManager/coltParameter_lookup.json",
                      "method": "get",
                      "params": {

                      }
                    },
                    "props": {
                      "title": "查询押品参数信息"
                    }
                  },
                  "eval_way": {
                    "label": "评估方式",
                    "width": "140px",
                    "control": "select",
                    "disabled":true,
                    "dict": {
                        "dictType": "CO_E_EVALTYPE",
                        "dictKey": [
                          "CO_E_EVALTYPE"
                        ],
                        "format": "label"
                      }
                },
                "in_eval_type": {
                    "label": "内评评估方式",
                    "width": "140px",
                    "control": "select",
                    "disabled":true,
                    "dict": {
                        "dictType": "CO_E_INEVALTYPE",
                        "dictKey": [
                          "CO_E_INEVALTYPE"
                        ],
                        "format": "label"
                      }
                },
                "auto_eval_freq": {
                    "label": "自动评估频率",
                    "width": "140px",
                    "control": "text",
                    "disabled":true
                },
                "auto_eval_freq_unit": {
                    "label": "自动评估频率单位",
                    "width": "140px",
                    "control": "select",
                    "disabled":true,
                    "dict": {
                        "dictType": "CO_E_PCYCLE",
                        "dictKey": [
                          "CO_E_PCYCLE"
                        ],
                        "format": "label"
                      }
                },
                  "colt_eval_no": {
                    "label": "押品评估编号",
                    "width": "140px",
                    "control": "hidden"
                }
            }],
            "btns": [{
                "type": "submit",
                "label": "提交",
                "doRequest": {
                    "url": "/SUMP/call/RPCCall",
                    "method": "POST",
                    "params": {
                        "servicecode": "co1005",
                        "tran_type": "2"
                    }
                }
            }]
        }
    }
}
