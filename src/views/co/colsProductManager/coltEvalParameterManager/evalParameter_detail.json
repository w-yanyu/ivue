{
    "module": "v_inputDataForm_btn",
    "layout": {
      "form": {
        "name": "detail",
        "initRequest": "${{params}}",
        "controls": [
        {
            "colt_eval_no": {
                "control": "text",
                "label": "押品评估编号",
                "width": "140px",
                "disabled":true
            },
            "is_need_reeval": {
              "label": "是否需价值重估",
              "width": "140px",
              "control": "select",
              "disabled":true,
              "dict": {
                "dictType": "MS_E_YESORNO",
                "dictKey": [
                  "MS_E_YESORNO"
                ],
                "format": "label"
              }
            },
            "eval_way": {
              "label": "评估方式",
              "width": "140px",
              "control": "select",
              "disabled":true,
              "dict": {
                "dictType": "CO_E_EVALTYPE",
                "dictKey": [
                  "CO_E_EVALTYPE"
                ],
                "format": "label"
              },
              "showItem": {
                "0": ["in_eval_type","colt_eval_meth_type","auto_eval_freq","auto_eval_freq_unit"],
                "1": []
              }
            },
            "colt_eval_meth_type": {
              "label": "评估方法类型",
              "control": "remoteSelect",
              "disabled":true,
              "width": "140px",
              "labelField": "eval_meth_type_name",
              "valueField": "colt_eval_meth_type",
              "doRequest": {
                  "url": "/SUMP/icmcall/icmRPCCall",
                  "method": "POST",
                  "params": {
                      "servicecode": "co4020"
                  }
              },
              "format": "label"
            },
              "in_eval_type": {
                "label": "内部评估方式",
                "width": "140px",
                "control": "select",
                "disabled":true,
                "dict": {
                  "dictType": "CO_E_INEVALTYPE",
                  "dictKey": [
                    "CO_E_INEVALTYPE"
                  ],
                  "format": "label"
                },
                "showitem":{
                  "0": ["auto_eval_freq","auto_eval_freq_unit"]
                               
                }
              }        
        },
        {
          
          "auto_eval_freq": {
            "label": "自动评估频率",
            "width": "140px",
            "control": "text",
            "disabled":true
            
          },
          "auto_eval_freq_unit": {
            "label": "",
            "width": "140px",
            "span": 1,
            "control": "select",
            "disabled":true,
            "dict": {
              "dictType": "CO_E_PCYCLE",
              "dictKey": [
                "CO_E_PCYCLE"
              ],
              "format": "label"
            }
          }
        }
        ],
        "btns": [
          {
          }
        ]
      }
    }
  }
