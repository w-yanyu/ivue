{
    "module": "v_search_btn_datagrid",
    "layout": {
      "form": {
        "controls": [
          {
            "colt_eval_no": {
              "label": "押品评估编号",
              "control": "text"
            }

          }
        ]
      },
      "toolbar": {
        "btns": [
          {
            "type": "add",
            "label": "新增",
            "toRequest": {
              "url": "/views/co/colsProductManager/coltEvalParameterManager/evalParameter_add.json",
              "method": "get",
              "params": {}
            },
            "props": {
              "title": "押品估值参数新增",
              "width": "80%"
            }
          }
        ]
      },
      "datagrid": {
        "isInitSearch": true,
        "name":"detail",
        "doRequest": {
          "url": "/SUMP/call/RPCCall",
          "method": "post",
          "params": {
            "servicecode": "co1016",
            "params": {
            }
          }
        },
        "pagination": {
          "pageSizes": [
            10,
            15,
            20
          ],
          "pageSize": 10
        },
        "thead": [
          {
            "type": "index",
            "align": "center"
          },
          {
            "label": "押品评估编号",
            "prop": "colt_eval_no",
            "align": "center",
            "type": "leaf"
          },
          {
            "label": "是否需价值重估",
            "prop": "is_need_reeval",
            "dict": {
                "dictType": "MS_E_YESORNO",
                "dictKey": [
                  "MS_E_YESORNO"
                ],
                "format": "label"
              },
            "align": "center",
            "type": "leaf"
          },
          {
            "label": "评估方式",
            "prop": "eval_way",
            "dict": {
              "dictType": "CO_E_EVALTYPE",
              "dictKey": [
                "CO_E_EVALTYPE"
              ],
              "format": "label"
            },
            "align": "center",
            "type": "leaf"
          },
          {
            "label": "评估方法类型",
            "prop": "colt_eval_meth_type",
            "align": "center",
            "type": "leaf",
            "dict": {
              "dictType": "CO_E_EVAL_MTEH_TYPE",
              "dictKey": [
                "CO_E_EVAL_MTEH_TYPE"
              ],
              "format": "label"
            }
          },
          {
            "label": "内部评估方式",
            "prop": "in_eval_type",
            "align": "center",
            "type": "leaf",
            "dict": {
              "dictType": "CO_E_INEVALTYPE",
              "dictKey": [
                "CO_E_INEVALTYPE"
              ],
              "format": "label"
            }
          },
          {
            "label": "自动评估频率",
            "prop": "auto_eval_freq",
        
            "align": "center",
            "type": "leaf"
          },
          {
            "label": "自动评估频率单位",
            "prop": "auto_eval_freq_unit",
            "dict": {
                "dictType": "CO_E_PCYCLE",
                "dictKey": [
                  "CO_E_PCYCLE"
                ],
                "format": "label"
              },
            "align": "center",
            "type": "leaf"
          },
          {
            "label": "操作",
            "width": "150px",
            "align": "center",
            "fixed": "right",
            "btns": [
              {
                "type": "edit",
                "label": "维护",
                "toRequest": {
                  "url": "/views/co/colsProductManager/coltEvalParameterManager/evalParameter_update.json",
                  "method": "get",
                  "params": {}
                },
                "props": {
                  "title": "押品估值参数维护",
                  "width": "80%"
                }
              },
              {
                "type": "look",
                "label": "详情",
                "toRequest": {
                  "url": "/views/co/colsProductManager/coltEvalParameterManager/evalParameter_detail.json",
                  "method": "get",
                  "params": {}
                },
                "props": {
                  "title": "押品估值参数详情",
                  "width": "80%"
                }
              },
              {
                "type": "delete",
                "label": "删除",
                "doRequest": {
                  "url": "/SUMP/call/RPCCall",
                  "method": "DELETE",
                  "params": {
                    "servicecode": "co1021",
                    "tran_type": "3",
                    "detail": {
                      "colt_eval_no": "${{current.colt_eval_no}}"
                    }
                  }
                }
              }
            ]
          }
        ]
      }
    }
  }
