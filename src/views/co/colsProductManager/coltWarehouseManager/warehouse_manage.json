{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "ware_house_code": {
            "label": "库房编号",
            "control": "text"
          },
          "wh_name": {
            "label": "库房名称",
            "control": "text"
          }
        },
        {
          "wh_level": {
            "label": "库房级别",
            "control": "select",
            "dict": {
              "dictType": "CO_E_WH_LEVEL",
              "dictKey": [
                "CO_E_WH_LEVEL"
              ],
              "format": "label"
            }
          },
          "wh_user_name": {
            "label": "库房主管",
            "control": "lookup",
            "edit": true,
            "keyField": {
              "wh_user_name": "user_name",
              "wh_user_no": "user_id"
            },
            "toRequest": {
              "url": "/views/co/colsProductManager/coltWarehouseManager/user-manager-lookup.json",
              "method": "post",
              "params": {

              }
            },
            "props": {
              "title": "查询用户信息"
            }
          },
          "wh_user_no": {
            "label": "库房主管no",
            "control": "hidden"
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "label": "新增",
          "toRequest": {
            "url": "/views/co/colsProductManager/coltWarehouseManager/warehouse_add.json",
            "method": "get",
            "params": {}
          },
          "props": {
            "title": "抵质押品库房新增",
            "width": "80%"
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/call/RPCCall",
        "method": "post",
        "params": {
          "servicecode": "co3021",
          "params": {
          }
        }
      },
      "pagination": {
        "pageSizes": [
          10,
          15,
          20
        ],
        "pageSize": 10
      },
      "thead": [
        {
          "type": "index",
          "align": "center"
        },
        {
          "label": "库房编号",
          "prop": "ware_house_code",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "库房名称",
          "prop": "wh_name",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "库房级别",
          "prop": "wh_level",
          "dict": {
            "dictType": "CO_E_WH_LEVEL",
            "dictKey": [
              "CO_E_WH_LEVEL"
            ],
            "format": "label"
          },
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "库房主管",
          "prop": "wh_user_name",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "库房地址",
          "prop": "wh_address",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "库房电话",
          "prop": "wh_phone",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "库房类型",
          "prop": "wh_type",
          "align": "center",
          "dict": {
            "dictType": "CO_E_WH_TYPE",
            "dictKey": [
              "CO_E_WH_TYPE"
            ],
            "format": "label"
          },
          "type": "leaf"
        },
        {
          "label": "备注",
          "prop": "remark",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "操作",
          "width": "150px",
          "align": "center",
          "fixed": "right",
          "btns": [
            {
              "type": "edit",
              "label": "维护",
              "toRequest": {
                "url": "/views/co/colsProductManager/coltWarehouseManager/warehouse_update.json",
                "method": "get",
                "params": {}
              },
              "props": {
                "title": "抵质押品库房维护",
                "width": "80%"
              }
            },
            {
              "type": "delete",
              "label": "删除",
              "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "DELETE",
                "params": {
                  "servicecode": "co3024",
                  "detail": {
                    "ware_house_code": "${{current.ware_house_code}}"
                  }
                }
              }
            }
          ]
        }
      ]
    }
  }
}
