{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "scope": "formData",
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "doRequest": {
            "url": "/SUMP/call/RPCCall",
            "method": "POST",
            "params": {
              "servicecode": "co0042",
              "brand_no": "${{params.tree_pid}}",
              "car_kind_no": "${{params.tree_id}}"
            }
          }
        }
      ],
      "controls": [
        {
          "car_model_no": {
            "control": "text",
            "label": "车型编号",
            "width": "140px",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请输入车型编号"
              }
            ]
          },
          "car_model_name": {
            "control": "text",
            "label": "车型名称",
            "width": "140px",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请输入车型名称"
              }
            ]
          },
          "brand_state": {
            "control": "select",
            "label": "使用状态",
            "width": "140px",
            "span": 12,
            "value": "1",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择使用状态"
              }
            ]
          },
          "sizetype": {
            "control": "text",
            "label": "尺寸类型",
            "width": "140px",
            "span": 12
          },
          "displacement2": {
            "control": "text",
            "label": "排量",
            "width": "140px",
            "span": 12,
            "format":{
              "decimal": 2
            }
          },
          "price": {
            "control": "currency",
            "label": "厂商指导价",
            "width": "140px",
            "span": 12,
            "format":{
              "thousand": ",",
              "decimal": 2
            }
          },
          "productionstate": {
            "control": "text",
            "label": "生产状态",
            "width": "140px",
            "span": 12
          },
          "salestate": {
            "control": "text",
            "label": "销售状态",
            "width": "140px",
            "span": 12
          },
          "yeartype": {
            "control": "dateTimePicker",
            "label": "年款",
            "width": "140px",
            "span": 12,
            "type": "year"
          },
          "listdate": {
            "control": "dateTimePicker",
            "label": "上市日期",
            "width": "140px",
            "span": 12,
            "format": "yyyyMMdd"
          },
          "seatnum": {
            "control": "text",
            "label": "乘员人数 (个)",
            "width": "140px",
            "span": 12,
            "rules": [
              {
                "type": "number",
                "message": "请输入整数"
              }
            ]
          },
          "geartype": {
            "control": "select",
            "label": "变速箱类型",
            "width": "140px",
            "span": 12,
            "dict": {
              "dictType": "CO_E_GEAR_TYPE",
              "dictKey": [
                "CO_E_GEAR_TYPE"
              ],
              "format": "label"
            }
          },
          "gearnum": {
            "control": "text",
            "label": "档位数",
            "width": "140px",
            "span": 12,
            "rules": [
              {
                "type": "number",
                "message": "请输入整数"
              }
            ]
          },
          "compartnum": {
            "control": "text",
            "label": "车厢数",
            "width": "140px",
            "span": 12,
            "rules": [
              {
                "type": "number",
                "message": "请输入整数"
              }
            ]
          }
        },
        {
          "logo": {
            "label": "车型图标",
            "width": "140px",
            "span": 12,
            "control": "text",
            "events": {
              "condition": {
                "$default": {
                  "control": {
                    "img": {
                      "value": "${{formData.logo}}"
                    }
                  }
                },
                "": {
                  "control": {
                    "img": {
                      "value": "${{formData.logo}}"
                    }
                  }
                }
              }
            },
            "rules": [
              {
                "required": true,
                "message": "请输入车型图标"
              }
            ]
          },
          "fileList": {
            "control": "dynamic",
            "isShowChildForm": true,
            "span": 4,
            "toRequest": {
              "url": "@logoUploadButton",
              "method": "get",
              "params": {
                "file_path": "logo",
                "formKey": "fileList",
                "formLabel": "附件上传"
              }
            }
          },
          "img": {
            "control": "img",
            "value": "${{formData.logo}}",
            "span": 12,
            "width": "200",
            "height": "200",
            "fit": "fill"
          }
        }
      ]
    }
  }
}
