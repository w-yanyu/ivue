{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "organize_name": {
            "label": "评估机构名称",
            "control": "text"
          },
          "organize_grade": {
            "label": "评估资质",
            "control": "select",
            "dict": {
              "dictType": "E_PGZZDJ",
              "dictKey": [
                "E_PGZZDJ"
              ],
              "format": "label"
            }
          },
          "organize_branch": {
            "label": "所属分支行",
            "control": "text"
          },
          "organize_status": {
            "label": "评估状态",
            "control": "select",
            "dict": {
              "dictType": "E_PSTATE",
              "dictKey": [
                "E_PSTATE"
              ],
              "format": "label"
            }
          },
          "evaluate_scope": {
            "control": "checkbox",
            "label": "评估业务范围",
            "span": 24,
            "options": [
              {
                "label": "金融质押品",
                "value": "金融质押品"
              },
              {
                "label": "应收账款及收费权",
                "value": "应收账款及收费权"
              },
              {
                "label": "无形资产",
                "value": "无形资产"
              },
              {
                "label": "动产",
                "value": "动产"
              },
              {
                "label": "交通工具",
                "value": "交通工具"
              },
              {
                "label": "土地使用权",
                "value": "土地使用权"
              },
              {
                "label": "特殊房地产",
                "value": "特殊房地产"
              },
              {
                "label": "机器设备",
                "value": "机器设备"
              },
              {
                "label": "房产",
                "value": "房产"
              },
              {
                "label": "资源类权利",
                "value": "资源类权利"
              },
              {
                "label": "浮动存货",
                "value": "浮动存货"
              },
              {
                "label": "其他抵押品",
                "value": "其他抵押品"
              },
              {
                "label": "其他质押品",
                "value": "其他质押品"
              }
            ]
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "co4010",
          "trantp": "SELECT"
        }
      },
      "pagination": {
        "pageSizes": [
          10,
          15,
          20
        ],
        "pageSize": 10
      },
      "thead": [
        {
          "type": "index",
          "align": "center"
        },
        {
          "label": "机构编码",
          "prop": "organize_no",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "机构名称",
          "prop": "organize_name",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "评估机构性质",
          "prop": "organize_nature",
          "align": "center",
          "type": "leaf",
          "dict": {
            "dictType": "E_PGJGXZ",
            "dictKey": [
              "E_PGJGXZ"
            ],
            "format": "label"
          }
        },
        {
          "label": "评估资质",
          "prop": "organize_grade",
          "align": "center",
          "type": "leaf",
          "dict": {
            "dictType": "E_PGZZDJ",
            "dictKey": [
              "E_PGZZDJ"
            ],
            "format": "label"
          }
        },
        {
          "label": "评估资质有效期",
          "prop": "organize_indate",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "所属分支行",
          "prop": "organize_branch",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "统一社会代码证",
          "prop": "socy_unify_credit_code",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "注册评估师人数",
          "prop": "appraiser_number",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "评估业务范围",
          "prop": "evaluate_scope",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "法人代表名称",
          "prop": "corp_name",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "法人代表身份证号",
          "prop": "corp_no",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "成立日期",
          "prop": "setup_date",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "注册资金",
          "prop": "register_money",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "联系方式",
          "prop": "phone_no",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "状态",
          "prop": "organize_status",
          "align": "center",
          "type": "leaf",
          "dict": {
            "dictType": "E_PSTATE",
            "dictKey": [
              "E_PSTATE"
            ],
            "format": "label"
          }
        },
        {
          "label": "公司简介",
          "prop": "company_intro",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "操作",
          "width": "150px",
          "align": "center",
          "fixed": "right",
          "btns": [
            {
              "type": "look",
              "label": "审核记录",
              "toRequest": {
                "url": "/",
                "method": "POST"
              },
              "props": {
                "title": "审核记录"
              }
            },
            {
              "type": "look",
              "label": "详情",
              "toRequest": {
                "url": "/views/cols/colsOrganizationInfo/organizationInfo_detail_show.json",
                "method": "post",
                "params": {}
              },
              "props": {
                "title": "评估机构信息详情",
                "width": "80%"
              }
            }
          ]
        }
      ]
    }
  }
}
