{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "branch_name": {
            "label": "评估机构名称",
            "control": "text"
          },
          "organize_grade": {
            "label": "评估资质",
            "control": "select",
            "dict": {
              "dictType": "CO_E_EVALORGGRADE",
              "dictKey": [
                "CO_E_EVALORGGRADE"
              ],
              "format": "label"
            }
          },
          "organize_branch": {
            "label": "所属分支行",
            "control": "text"
          },
          "organize_state": {
            "label": "评估机构状态",
            "control": "select",
            "dict": {
              "dictType": "CO_E_COLT_EVALUTION_STATE",
              "dictKey": [
                "CO_E_COLT_EVALUTION_STATE"
              ],
              "format": "label"
            }
          },
          "evaluate_scope": {
            "control": "checkbox",
            "label": "评估业务范围",
            "span": 24,
            "options": [
              {
                "label": "金融质押品",
                "value": "金融质押品"
              },
              {
                "label": "应收账款及收费权",
                "value": "应收账款及收费权"
              },
              {
                "label": "无形资产",
                "value": "无形资产"
              },
              {
                "label": "动产",
                "value": "动产"
              },
              {
                "label": "交通工具",
                "value": "交通工具"
              },
              {
                "label": "土地使用权",
                "value": "土地使用权"
              },
              {
                "label": "特殊房地产",
                "value": "特殊房地产"
              },
              {
                "label": "机器设备",
                "value": "机器设备"
              },
              {
                "label": "房产",
                "value": "房产"
              },
              {
                "label": "资源类权利",
                "value": "资源类权利"
              },
              {
                "label": "浮动存货",
                "value": "浮动存货"
              },
              {
                "label": "其他抵押品",
                "value": "其他抵押品"
              },
              {
                "label": "其他质押品",
                "value": "其他质押品"
              }
            ]
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "label": "新增",
          "toRequest": {
            "url": "/views/co/colsOrganizationInfo/organizationInfo_add_start.json",
            "method": "post",
            "params": {}
          },
          "props": {
            "title": "评估机构信息新增",
            "width": "80%"
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "co4010"
        }
      },
      "pagination": {
        "pageSizes": [
          10,
          15,
          20
        ],
        "pageSize": 10
      },
      "thead": [
        {
          "type": "index",
          "align": "center"
        },
        {
          "label": "机构编码",
          "prop": "branch_id",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "机构名称",
          "prop": "branch_name",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "评估机构性质",
          "prop": "organize_nature",
          "align": "center",
          "type": "leaf",
          "dict": {
            "dictType": "CO_E_EVAORGNATURE",
            "dictKey": [
              "CO_E_EVAORGNATURE"
            ],
            "format": "label"
          }
        },
        {
          "label": "评估资质",
          "prop": "organize_grade",
          "align": "center",
          "type": "leaf",
          "dict": {
            "dictType": "CO_E_EVALORGGRADE",
            "dictKey": [
              "CO_E_EVALORGGRADE"
            ],
            "format": "label"
          }
        },
        {
          "label": "评估资质有效期",
          "prop": "eval_valid_date",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "所属分支行",
          "prop": "organize_branch",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "统一社会代码证",
          "prop": "socy_unify_credit_code",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "注册评估师人数",
          "prop": "certified_appraiser_num",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "评估业务范围",
          "prop": "evaluate_scope",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "法人代表名称",
          "prop": "corp_name",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "法人代表身份证号",
          "prop": "corp_no",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "成立日期",
          "prop": "setup_date",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "注册资金",
          "prop": "reg_capital",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "联系方式",
          "prop": "contact_info",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "状态",
          "prop": "organize_state",
          "align": "center",
          "type": "leaf",
          "dict": {
            "dictType": "CO_E_COLT_EVALUTION_STATE",
            "dictKey": [
              "CO_E_COLT_EVALUTION_STATE"
            ],
            "format": "label"
          }
        },
        {
          "label": "公司简介",
          "prop": "company_intro",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "操作",
          "width": "150px",
          "align": "center",
          "fixed": "right",
          "btns": [
            {
              "type": "edit",
              "label": "维护",
              "toRequest": {
                "url": "/views/co/colsOrganizationInfo/organizationInfo_edit.json",
                "method": "post",
                "params": {}
              },
              "props": {
                "title": "评估机构信息维护",
                "width": "80%"
              },
              "show":{
                "prop":"organize_state",
                "is":["2"]
               }
            },{
              "type": "custom",
              "label": "详情",
              "toRequest": {
                "url": "/views/co/colsOrganizationInfo/organizationInfo_detail_show.json",
                "method": "post",
                "params": {}
              },
              "props": {
                "title": "评估机构信息详情",
                "width": "80%"
              }
            }
          ]
        }
      ]
    }
  }
}
