{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/call/RPCCall",
        "method": "post",
        "params": {
          "servicecode": "co4010",
          "apply_no": "${{params.apply_no}}"
        }
      },
      "btns": [],
      "controls": [
        {
          "trans_type": {
            "control": "hidden",
            "label": "操作类型",
            "value":"UDP"
          },
          "branch_id": {
            "label": "机构编码:",
            "control": "hidden",
            "value": "${{params.branch_id}}"
          },
          "branch_name": {
            "label": "机构名称",
            "width": "130px",
            "control": "text",
            "value": "${{params.branch_name}}",
            "rules": [
              {
                "required": true,
                "message": "请输入机构名称"
              }
            ]
          },
          "organize_branch": {
            "label": "所属分支行",
            "control": "text",
            "width": "130px",
            "value": "${{params.organize_branch}}"
          },
          "socy_unify_credit_code": {
            "label": "统一社会代码证",
            "control": "text",
            "width": "130px",
            "value": "${{params.socy_unify_credit_code}}",
            "rules": [
              {
                "required": true,
                "message": "请输入统一社会代码证"
              }
            ]
          },
          "organize_nature": {
            "label": "评估机构性质",
            "width": "130px",
            "control": "select",
            "value": "${{params.organize_nature}}",
            "dict": {
              "dictType": "CO_E_EVAORGNATURE",
              "dictKey": [
                "CO_E_EVAORGNATURE"
              ]
            },
            "rules": [
              {
                "required": true,
                "message": "请选择机构性质"
              }
            ]
          },
          "certified_appraiser_num": {
            "label": "注册评估师人数",
            "control": "inputNumber",
            "width": "130px"
          },
          "evaluate_scope": {
            "control": "checkbox",
            "label": "评估业务范围",
            "width": "130px",
            "value": "${{params.evaluate_scope}}",
            "span": 24,
            "options": [
              {
                "label": "金融质押品",
                "value": "金融质押品"
              },
              {
                "label": "应收账款及收费权",
                "value": "应收账款及收费权"
              },
              {
                "label": "无形资产",
                "value": "无形资产"
              },
              {
                "label": "动产",
                "value": "动产"
              },
              {
                "label": "交通工具",
                "value": "交通工具"
              },
              {
                "label": "土地使用权",
                "value": "土地使用权"
              },
              {
                "label": "特殊房地产",
                "value": "特殊房地产"
              },
              {
                "label": "机器设备",
                "value": "机器设备"
              },
              {
                "label": "房产",
                "value": "房产"
              },
              {
                "label": "资源类权利",
                "value": "资源类权利"
              },
              {
                "label": "浮动存货",
                "value": "浮动存货"
              },
              {
                "label": "其他抵押品",
                "value": "其他抵押品"
              },
              {
                "label": "其他质押品",
                "value": "其他质押品"
              }
            ],
            "rules": [
              {
                "required": true,
                "message": "请选择评估业务范围"
              }
            ]
          },
          "organize_grade": {
            "label": "评估资质",
            "control": "select",
            "width": "130px",
            "value": "${{params.organize_grade}}",
            "dict": {
              "dictType": "CO_E_EVALORGGRADE",
              "dictKey": [
                "CO_E_EVALORGGRADE"
              ]
            }
          },
          "eval_valid_date": {
            "label": "评估资质有效期",
            "width": "130px",
            "value": "${{params.eval_valid_date}}",
            "control": "dateTimePicker",
            "format": "yyyyMMdd"
          },
          "corp_name": {
            "label": "法人代表名称",
            "control": "text",
            "width": "130px",
            "value": "${{params.corp_name}}"
          },
          "corp_no": {
            "label": "法人代表身份证号",
            "control": "text",
            "width": "130px",
            "value": "${{params.corp_no}}"
          },
          "reg_capital": {
            "label": "注册资金(元)",
            "control": "currency",
            "width": "130px",
            "value": "${{params.reg_capital}}"
          },
          "setup_date": {
            "label": "成立日期",
            "width": "130px",
            "control": "dateTimePicker",
            "format": "yyyyMMdd",
            "value": "${{params.setup_date}}"
          },
          "contact_info": {
            "label": "联系方式",
            "control": "text",
            "width": "130px",
            "value": "${{params.contact_info}}"
          },
          "organize_state": {
            "label": "状态",
            "control": "select",
            "width": "130px",
            "value": "${{params.organize_state}}",
            "dict": {
              "dictType": "CO_E_COLT_EVALUTION_STATE",
              "dictKey": [
                "CO_E_COLT_EVALUTION_STATE"
              ]
            }
          },
          "company_intro": {
            "label": "公司简介",
            "control": "textarea",
            "span": "24",
            "width": "130px",
            "value": "${{params.company_intro}}"
          }
        },
        {
          "toDbsx": {
            "span": 24,
            "control": "dynamic",
            "#isShowChildrenForm": false,
            "toRequest": {
              "method": "GET",
              "url": "@FlowSubmitBtn",
              "params": {
                "appId": "coOnl",
                "servicecode": "co4009",
                "finalUrl": "@CommWaitToDoDeal",
                "trans_type": "UPD",
                "is_start": "1"

              }
            }
          }
        }
      ]
    }
  }
}
