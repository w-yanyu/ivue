{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "name": "detail",
            "controls": [{
                    "doc_no": {
                        "label": "档案编号",
                        "width": "140px",
                        "control": "lookup",
                        "placeholder": "请选择档案编号",
                        "disabled": false,
                        "edit": true,
                        "value": "",
                        "keyField": {
                            "doc_no": "doc_no"
                        },
                        "toRequest": {
                            "url": "/views/co/colsWarehous/start/collSelect.json",
                            "method": "get",
                            "params": {
                                "maintenanceStatus": "2"
                            }
                        },
                        "props": {
                            "title": "档案选择",
                            "width": "80%"
                        },
                        "rules": [{
                            "required": true,
                            "message": "请选择档案编号"
                        }]
                    },
                    "out_date": {
                        "label": "出库日期",
                        "width": "140px",
                        "control": "dateTimePicker",
                        "rules": [{
                            "required": true,
                            "message": "请选择出库日期"
                        }]
                    }
                },
                {
                    "out_reason": {
                        "label": "出库原因",
                        "width": "140px",
                        "control": "select",
                        "multiple": false,
                        "value": "",
                        "dict": {
                            "dictType": "CO_E_OUTPUTRN",
                            "dictKey": [
                                "CO_E_OUTPUTRN"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": true,
                            "message": "请选择出库原因"
                        }]
                    }
                },
                {
                    "remark": {
                        "label": "备注",
                        "width": "140px",
                        "rows": 4,
                        "span": 24,
                        "control": "textarea"
                    },
                    "deal_type": {
                        "label": "交易类型",
                        "width": "140px",
                        "value": "out",
                        "control": "hidden"
                    },
                    "state": {
                        "label": "状态",
                        "width": "140px",
                        "value": "R",
                        "control": "hidden"
                    },
                    "appv_state": {
                        "label": "审核状态",
                        "width": "140px",
                        "value": "NR",
                        "control": "hidden"
                    }
                }
            ],
            "btns": [{
                "type": "submit",
                "label": "提交",
                "doRequest": {
                    "url": "/SUMP/call/RPCCall",
                    "method": "POST",
                    "params": {
                        "servicecode": "co3001",
                        "deal_type": "out"
                    }
                }
            }]
        }
    }
}
