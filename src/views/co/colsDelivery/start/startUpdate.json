{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "name": "detail",
            "controls": [
                {
                    "doc_tx_no": {
                        "label": "档案交易号",
                        "control": "hidden"
                    },
                  "state": {
                    "label": "出入库状态",
                    "control": "hidden"
                  },
                    "doc_no": {
                        "label": "档案编号",
                        "control": "text",
                        "disabled": true,
                        "width": "140px"
                    },
                    "out_date": {
                        "label": "出库日期",
                        "width": "140px",
                        "control": "dateTimePicker",
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择出库日期"
                            }
                        ]
                    }
                },
                {
                    "out_reason": {
                        "label": "出库原因",
                        "width": "140px",
                        "control": "select",
                        "multiple": false,
                        "value": "",
                        "dict": {
                            "dictType": "CO_E_OUTPUTRN",
                            "dictKey": [
                                "CO_E_OUTPUTRN"
                            ],
                            "format": "label"
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择出库原因"
                            }
                        ]
                    },
                    "brch_cd": {
                        "label": "放款网点",
                        "control": "text",
                        "disabled": true,
                        "width": "140px"
                    }
                },
                {
                    "remark": {
                        "label": "备注",
                        "width": "140px",
                        "rows": 4,
                        "span": 24,
                        "control": "textarea"
                    },
                    "deal_type": {
                        "label": "交易类型",
                        "width": "140px",
                        "value": "out",
                        "control": "hidden"
                    }
                }
            ],
            "btns": [
                {
                    "type": "submit",
                    "label": "维护",
                    "doRequest": {
                        "url": "/SUMP/call/RPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "co3003",
                            "deal_type": "out"
                        }
                    }
                }
            ]
        }
    }
}
