{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "person_no": {
            "label": "评估人员编号",
            "control": "text"
          },
          "person_name": {
            "label": "评估人员名称",
            "control": "text"
          },
          "organize_dept": {
            "label": "所属部门",
            "control": "text"
          },
          "person_status": {
            "label": "评估状态",
            "control": "select",
            "dict": {
              "dictType": "CO_E_COLT_EVALUTION_STATE",
              "dictKey": [
                "CO_E_COLT_EVALUTION_STATE"
              ],
              "format": "label"
            }
          },
          "eval_cert": {
            "control": "checkbox",
            "label": "评估资质证书：",
            "span": 24,
            "options": [
              {
                "label": "注册资产评估师",
                "value": "注册资产评估师"
              },
              {
                "label": "注册房地产评估师",
                "value": "注册房地产评估师"
              },
              {
                "label": "注册土地评估师",
                "value": "注册土地评估师"
              },
              {
                "label": "其他",
                "value": "其他"
              }
            ]
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "label": "新增",
          "toRequest": {
            "url": "/views/co/colsPersonInfo/personInfo_add_start.json",
            "method": "post",
            "params": {}
          },
          "props": {
            "title": "评估人员信息新增",
            "width": "80%"
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "co4008",
          "trans_type": "SELECT"
        }
      },
      "pagination": {
        "pageSizes": [
          10,
          15,
          20
        ],
        "pageSize": 10
      },
      "thead": [
        {
          "type": "index",
          "align": "center"
        },
        {
          "label": "人员编码",
          "prop": "person_no",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "人员名称",
          "prop": "person_name",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "所在部门",
          "prop": "organize_dept",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "评估资质证书",
          "prop": "eval_cert",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "办公固定电话",
          "prop": "work_telephone",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "手机号",
          "prop": "mobile_no",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "参与我行评估次数",
          "prop": "count",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "评估累计金额",
          "prop": "eval_count_money",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "状态",
          "prop": "person_status",
          "align": "center",
          "type": "leaf",
          "dict": {
            "dictType": "CO_E_COLT_EVALUTION_STATE",
            "dictKey": [
              "CO_E_COLT_EVALUTION_STATE"
            ],
            "format": "label"
          }
        },
        {
          "label": "备注",
          "prop": "remark",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "操作",
          "width": "150px",
          "align": "center",
          "fixed": "right",
          "btns": [
            {
              "type": "edit",
              "label": "维护",
              "toRequest": {
                "url": "/views/co/colsPersonInfo/personInfo_edit.json",
                "method": "post",
                "params": {}
              },
              "props": {
                "title": "评估人员信息维护",
                "width": "80%"
              },
              "show":{
                "prop":"person_status",
                "is":["2"]
               }
            },
            {
              "type": "custom",
              "label": "详情",
              "toRequest": {
                "url": "/views/co/colsPersonInfo/personInfo_detail_show.json",
                "method": "post",
                "params": {}
              },
              "props": {
                "title": "评估人员信息详情",
                "width": "80%"
              }
            }
          ]
        }
      ]
    }
  }
}
