{
  "module": "v-realonlyForm-btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/call/RPCCall",
        "method": "post",
        "params": {
          "servicecode": "co4008",
          "appl_id": "${{params.applid}}"
        }
      },
      "controls": [
        {
          "person_no": {
            "label": "评估人员编号",
            "width": "130px",
            "control": "string"
          },
          "person_name": {
            "label": "人员名称",
            "width": "130px",
            "control": "string"
          },
          "organize_dept": {
            "label": "所在部门",
            "width": "130px",
            "control": "string"
          },
          "work_telephone": {
            "label": "办公固定电话",
            "control": "string",
            "width": "130px"
          },
          "eval_cert": {
            "control": "checkbox",
            "disabled": true,
            "label": "评估资质证书",
            "width": "130px",
            "span": "24",
            "options": [
              {
                "label": "注册资产评估师",
                "value": "注册资产评估师"
              },
              {
                "label": "注册房地产评估师",
                "value": "注册房地产评估师"
              },
              {
                "label": "注册土地评估师",
                "value": "注册土地评估师"
              },
              {
                "label": "其他",
                "value": "其他"
              }
            ]
          },
          "mobile_no": {
            "label": "手机号",
            "control": "string",
            "width": "130px"
          },
          "count": {
            "label": "参与我行评估次数",
            "control": "string",
            "width": "130px"
          },
          "eval_count_money": {
            "label": "评估累计金额(元)",
            "control": "string",
            "width": "130px"
          },
          "person_status": {
            "label": "状态",
            "control": "select",
            "width": "130px",
            "disabled": true,
            "dict": {
              "dictType": "CO_E_COLT_EVALUTION_STATE",
              "dictKey": [
                "CO_E_COLT_EVALUTION_STATE"
              ],
              "format": "label"
            }
          },
          "remark": {
            "label": "备注",
            "control": "textarea",
            "disabled": true,
            "width": "130px",
            "span": "24"
          }
        }
      ]
    }
  }
}
