{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/call/RPCCall",
        "method": "post",
        "params": {
          "servicecode": "co4008",
          "appl_id": "${{params.applid}}"
        }
      },
      "btns": [],
      "controls": [
        {
          "person_no": {
            "control": "hidden",
            "label": "楼盘ID",
            "value":"Y"
          },
          "person_name": {
            "label": "人员名称",
            "width": "130px",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入人员名称"
              }
            ]
          },
          "organize_dept": {
            "label": "所在部门",
            "width": "130px",
            "control": "text"
          },
          "eval_cert": {
            "control": "checkbox",
            "label": "评估资质证书",
            "width": "130px",
            "span": "24",
            "options": [
              {
                "label": "注册资产评估师",
                "value": "注册资产评估师"
              },
              {
                "label": "注册房地产评估师",
                "value": "注册房地产评估师"
              },
              {
                "label": "注册土地评估师",
                "value": "注册土地评估师"
              },
              {
                "label": "其他",
                "value": "其他"
              }
            ],
            "rules": [
              {
                "required": true,
                "message": "请选择资质证书"
              }
            ]
          },
          "work_telephone": {
            "label": "办公固定电话",
            "control": "text",
            "width": "130px"
          },
          "mobile_no": {
            "label": "手机号",
            "control": "text",
            "width": "130px"
          },
          "count": {
            "label": "参与我行评估次数",
            "control": "inputNumber",
            "width": "130px"
          },
          "eval_count_money": {
            "label": "评估累计金额(元)",
            "control": "currency",
            "width": "130px"
          },
          "person_status": {
            "label": "状态",
            "control": "select",
            "default": "1",
            "disabled": true,
            "width": "130px",
            "dict": {
              "dictType": "CO_E_COLT_EVALUTION_STATE",
              "dictKey": [
                "CO_E_COLT_EVALUTION_STATE"
              ],
              "format": "label"
            }
          },
          "remark": {
            "label": "备注",
            "control": "textarea",
            "width": "130px",
            "span": "24"
          }
        },
        {
          "toDbsx": {
            "span": 24,
            "control": "dynamic",
            "#isShowChildrenForm": false,
            "toRequest": {
              "method": "GET",
              "url": "@FlowSubmitBtn",
              "params": {
                "appId": "coOnl",
                "servicecode": "co4007",
                "finalUrl": "@CommWaitToDoDeal",
                "trans_type": "UPD",
                "is_start": "1"

              }
            }
          }
        }
      ]
    }
  }
}
