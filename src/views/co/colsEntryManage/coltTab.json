{
    "module": "v-formTabs",
    "layout": {
        "tabs": {
            "defaultActiveKey": "tab1",
            "position": "top",
            "styleClass": "card",
            "tabHeader": [
                {
                    "id": "tab1",
                    "label": "押品概况",
                    "closable": false,
                    "disabled": false,
                    "icon": "el-icon-date",
                    "toRequest": {
                        "url": "/views/co/colsEntryManage/cobAsset/assetView/cobColtTab.json",
                        "params": {
                            "colt_no":"${{params.colt_no}}"
                        }
                    }
                },
                {
                    "id": "tab2",
                    "label": "基本信息",
                    "closable": true,
                    "disabled": false,
                    "toRequest": {
                        "url": "/views/co/colsEntryManage/cobColtDetail.json",
                        "params": {
                            "colt_no":"${{params.colt_no}}"
                        }
                    }
                },
                {
                    "id": "tab3",
                    "label": "估值信息",
                    "closable": true,
                    "disabled": false,
                    "toRequest": {
                        "url": "/views/co/colsEntryManage/assessmentListQuery.json",
                        "params": {
                            "colt_no":"${{params.colt_no}}"
                        }
                    }
                },
                {
                    "id": "tab4",
                    "label": "使用信息",
                    "closable": true,
                    "disabled": false,
                    "toRequest": {
                        "url": "/views/co/colsQuotaFrz/colsQuotaQuery/colsQuota_rela.json",
                        "params": {
                            "colt_no":"${{params.colt_no}}"
                        }
                    }
                }
            ]
        }
    }
}
