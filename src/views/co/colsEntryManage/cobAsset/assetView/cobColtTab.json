{
    "module": "v_realonlyForm_btn",
    "layout": {
        "form": {
            "controlsGroup": [{
                    "title": "押品信息",
                    "tooltip": "",
                    "titleIcon": "",
                    "showHint": "",
                    "accordion": false,
                    "expand": true,
                    "controls": [{
                        "id4": {
                            "control": "dynamic",
                            "isShowChildForm": false,
                            "span": 24,
                            "toRequest": {
                                "url": "/views/co/colsEntryManage/cobAsset/assetView/cobColtInfo.json",
                                "method": "get",
                                "params": {
                                    "colt_no": "${{params.colt_no}}"
                                }
                            }
                        }
                    }]
                },
                {
                    "title": "押品事件",
                    "tooltip": "",
                    "titleIcon": "",
                    "showHint": "",
                    "accordion": false,
                    "expand": true,
                    "controls": [{
                        "id2": {
                            "control": "dynamic",
                            "isShowChildForm": false,
                            "span": 24,
                            "toRequest": {
                                "url": "/views/co/colsEntryManage/cobAsset/assetView/CobColtEventListQuery.json",
                                "method": "get",
                                "params": {
                                    "colt_no": "${{params.colt_no}}"
                                }
                            }
                        }
                    }]
                }

            ]
        }
    }
}
