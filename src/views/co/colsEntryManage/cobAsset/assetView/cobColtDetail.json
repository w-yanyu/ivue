{
    "module": "v_realonlyForm_btn",
    "layout": {
        "form": {
            "initRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "co2051",
                    "colt_no": "${{params.colt_no}}"
                }
            },
            "controls": [
                {
                    "colt_no": {
                        "control": "string",
                        "label": "押品编号"
                    },
                    "asset_list_id": {
                        "control": "string",
                        "label": "资产清单序号"
                    },
                    "colt_ppsl_id": {
                        "control": "string",
                        "label": "抵质押品申请序号"
                    },
                    "colt_ctr_id": {
                        "control": "string",
                        "label": "押品担保合同序号"
                    },
                    "sec_type": {
                        "control": "select",
                        "disabled": true,
                        "label": "担保方式",
                        "dict": {
                            "dictType": "E_SECTP",
                            "dictKey": [
                                "E_SECTP"
                            ],
                            "format": "value-label"
                        }
                    },
                    "colt_prod_cd": {
                        "control": "select",
                        "label": "担保方式",
                        "disabled": true,
                        "dict": {
                            "dictKey": [
                                "E_COLT_PROD_CD"
                            ],
                            "format": "[value]label",
                            "valueField": "colt_prod_cd",
                            "labelField": "colt_prod_name",
                            "doRequest": {
                                "url": "/SUMP/call/RPCCall",
                                "method": "POST",
                                "params": {
                                    "servicecode": "co1013"
                                }
                            }
                        }
                    },
                    "cust_id": {
                        "control": "string",
                        "label": "客户序号",
                        "disabled": true
                    },
                    "colt_flag": {
                        "control": "select",
                        "disabled": true,
                        "label": "担保方式",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "value-label"
                        }
                    },
                    "colt_desc": {
                        "control": "string",
                        "label": "押品描述",
                        "disabled": true
                    },
                    "exp_dt": {
                        "control": "string",
                        "label": "到期日",
                        "disabled": true
                    },
                    "store_addr": {
                        "label": "存放地址",
                        "control": "string"
                    },
                    "master_flag": {
                        "label": "是否主担保物",
                        "disabled": true,
                        "control": "select",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ]
                        }
                    },
                    "eval_value_price": {
                        "label": "第一次评估价(元)",
                        "control": "string"
                    },
                    "eval_value_ccy": {
                        "label": "第一次评估币种",
                        "control": "select",
                        "disabled": true,
                        "dict": {
                            "dictType": "MS_CURRENCY",
                            "dictKey": [
                                "MS_CURRENCY"
                            ]
                        }
                    },
                    "value_price": {
                        "label": "我行认可价(元)",
                        "control": "string"
                    },
                    "value_ccy": {
                        "label": "我行认可价币种",
                        "disabled": true,
                        "control": "select",
                        "dict": {
                            "dictType": "MS_CURRENCY",
                            "dictKey": [
                                "MS_CURRENCY"
                            ]
                        }
                    },
                    "sec_value": {
                        "label": "抵质押额(元)",
                        "control": "string"
                    },
                    "sec_ratio": {
                        "label": "抵质押率(%)",
                        "control": "currency",
                        "disabled": true
                    },
                    "own_cert_type": {
                        "label": "他项权证类型",
                        "control": "select",
                        "initShow": false,
                        "disabled": true,
                        "dict": {
                            "dictType": "E_OWNCERTP",
                            "dictKey": [
                                "E_OWNCERTP"
                            ]
                        }
                    },
                    "own_cert_no": {
                        "label": "他项权证号码",
                        "control": "string",
                        "initShow": false
                    },
                    "own_cert_type2": {
                        "label": "他项权证类型2",
                        "initShow": false,
                        "disabled": true,
                        "control": "select",
                        "dict": {
                            "dictType": "E_OWNCERTP",
                            "dictKey": [
                                "E_OWNCERTP"
                            ]
                        }
                    },
                    "own_cert_no2": {
                        "label": "他项权证号码2",
                        "initShow": false,
                        "control": "string"
                    },
                    "own_cert_type3": {
                        "label": "他项权证类型3",
                        "initShow": false,
                        "disabled": true,
                        "control": "select",
                        "dict": {
                            "dictType": "E_OWNCERTP",
                            "dictKey": [
                                "E_OWNCERTP"
                            ]
                        }
                    },
                    "own_cert_no3": {
                        "label": "他项权证号码3",
                        "control": "string",
                        "initShow": false
                    },
                    "own_cert_type4": {
                        "label": "他项权证类型4",
                        "initShow": false,
                        "disabled": true,
                        "control": "select",
                        "dict": {
                            "dictType": "E_OWNCERTP",
                            "dictKey": [
                                "E_OWNCERTP"
                            ]
                        }
                    },
                    "own_cert_no4": {
                        "label": "他项权证号码4",
                        "control": "string",
                        "initShow": false
                    }
                },
                {
                    "remark": {
                        "label": "备注",
                        "control": "textarea",
                        "span": 24,
                        "disabled": true,
                        "autosize": {
                            "minRows": 3,
                            "maxRows": 5
                        },
                        "maxlength": 100,
                        "showWordLimit": true
                    }
                }
            
        ]
    }
}
}