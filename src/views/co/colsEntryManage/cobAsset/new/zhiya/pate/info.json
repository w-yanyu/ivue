{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "btns": [],
      "name": "list01",
      "controls": [
        {
          "asset_list_id": {
            "label": "资产清单序号",
            "control": "hidden"
          },
          "cert_no": {
            "label": "证书号码",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入证书号码"
              }
            ]
          },
          "patent_term": {
            "label": "专利有效期",
            "control": "dateTimePicker",
            "format": "yyyyMMdd",
            "rules": [
              {
                "required": true,
                "message": "请输入专利有效期"
              }
            ]
          },
          "stock_company_name": {
            "label": "股份公司名称",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入股份公司名称"
              }
            ]
          },
          "become_cash_perc": {
            "label": "变现可能性(%)",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入变现可能性(%)"
              }
            ]
          },
          "out_eval_price": {
            "label": "评估价值(元)",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入商位使用权评估价值(元)"
              }
            ]
          },
          "eval_time": {
            "label": "评估时间",
            "control": "dateTimePicker",
            "format": "yyyyMMdd",
            "rules": [
              {
                "required": true,
                "message": "请输入评估时间"
              }
            ]
          },
          "eval_branch": {
            "label": "评估机构",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入评估机构"
              }
            ]
          },
          "is_repeat_pledge": {
            "label": "是否重复质押",
            "control": "select",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ]
            },
            "rules": [
              {
                "required": true,
                "message": "请选择是否重复质押"
              }
            ]
          },
          "pre_become_cash_perc": {
            "label": "预计变现程度(%)",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入预计变现程度"
              }
            ]
          },
          "is_ple_legal": {
            "label": "质押是否合法",
            "control": "select",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ]
            },
            "rules": [
              {
                "required": true,
                "message": "请选择质押是否合法"
              }
            ]
          },
          "remark": {
            "label": "备注",
            "control": "textarea",
            "span": 24,
            "autosize":{"minRows": 3,"maxRows":5},
            "maxlength": 100,
            "showWordLimit": true
          }
        }
      ]
    }
  }
}
