{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "btns": [],
          "name": "list01",
          "controls": [{
            "asset_list_id": {
              "label": "资产清单序号",
              "control": "hidden"
            },
            "arge_type": {
              "label": "贵金属品种",
              "control": "select",
              "dict": {
                "dictType": "CO_E_ARGETP",
                "dictKey": [
                  "CO_E_ARGETP"
                ]
              },
              "rules": [{
                "required": true,
                "message": "请选择贵金属品种"
              }]
            },
            "ownership": {
              "label": "权属",
              "control": "select",
              "dict": {
                "dictType": "CO_E_AUTH",
                "dictKey": [
                  "CO_E_AUTH"
                ]
              },
              "rules": [{
                "required": true,
                "message": "请选择权属"
              }]
            },
            "gold_no": {
              "label": "贵金属交易编码",
              "control": "text",
              "rules": [{
                "required": true,
                "message": "请输入贵金属交易编码"
              }]
            },
            "arge_percent": {
              "label": "成色",
              "control": "text",
              "rules": [{
                "required": true,
                "message": "请输入成色"
              }]
            },
            "gold_weight": {
              "label": "标准重量（克）",
              "control": "text",
              "rules": [{
                "required": true,
                "message": "请输入标准重量"
              }]
            },
            "arge_pled_weight": {
              "label": "质押重量（克）",
              "control": "text"
            }
          },
            {
              "remark": {
                "label": "备注",
                "control": "textarea",
                "span": 24,
                "autosize": { "minRows": 3, "maxRows": 5 },
                "maxlength": 100,
                "showWordLimit": true
              }
            }
          ]
        }
    }
}
