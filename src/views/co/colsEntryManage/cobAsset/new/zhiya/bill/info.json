{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "btns": [],
          "name": "list01",
          "controls": [{
            "asset_list_id": {
              "label": "资产清单序号",
              "control": "hidden"
            },
            "bill_no": {
              "label": "票据号码",
              "control": "text",
              "rules": [{
                "required": true,
                "message": "请输入票据号码"
              }]
            },
            "draft_type": {
              "label": "汇票种类",
              "control": "select",
              "dict": {
                "dictType": "CO_E_DRAFTTYPE",
                "dictKey": [
                  "CO_E_DRAFTTYPE"
                ]
              },
              "rules": [{
                "required": true,
                "message": "请选择汇票种类"
              }]
            },
            "drawer_name": {
              "label": "出票人名称",
              "control": "text",
              "rules": [{
                "required": true,
                "message": "请输入出票人名称"
              }]
            },
            "face_amt": {
              "label": "汇票面额(元)",
              "control": "text",
              "rules": [{
                "required": true,
                "message": "请输入汇票面额"
              }]
            },
            "ccy_code": {
              "label": "币种",
              "control": "string",
              "value": "CNY",
              "dict": {
                "dictType": "MS_CURRENCY",
                "dictKey": [
                  "MS_CURRENCY"
                ]
              },
              "rules": [{
                "required": true,
                "message": "请选择币种"
              }]
            },
            "guar_end_date": {
              "label": "担保到期日期",
              "control": "dateTimePicker",
              "format": "yyyyMMdd",
              "rules": [{
                "required": true,
                "message": "请输入担保到期日期"
              }]
            },
            "drawer_acct_no": {
              "label": "出票人账号",
              "control": "text",
              "rules": [{
                "required": true,
                "message": "请输入出票人账号"
              }]
            },
            "open_acct_bank": {
              "label": "开户行",
              "control": "text",
              "rules": [{
                "required": true,
                "message": "请输入开户行"
              }]
            },
            "drawer_bank": {
              "label": "出票行",
              "control": "text",
              "rules": [{
                "required": true,
                "message": "请输入出票行"
              }]
            },
            "payee_name": {
              "label": "收款人名称",
              "control": "text",
              "rules": [{
                "required": true,
                "message": "请输入收款人名称"
              }]
            },
            "payee_acct_no": {
              "label": "收款人账号",
              "control": "text",
              "rules": [{
                "required": true,
                "message": "请输入收款人账号"
              }]
            },
            "payee_bank": {
              "label": "收款人开户行",
              "control": "text",
              "rules": [{
                "required": true,
                "message": "请输入收款人开户行"
              }]
            },
            "issue_dt": {
              "label": "汇票签发日期",
              "control": "dateTimePicker",
              "format": "yyyyMMdd"
            },
            "is_endorsement": {
              "label": "是否被书",
              "control": "select",
              "dict": {
                "dictType": "MS_E_YESORNO",
                "dictKey": [
                  "MS_E_YESORNO"
                ]
              }
            },
            "endorsor": {
              "label": "前手被书人",
              "control": "text"
            },
            "inquery_date": {
              "label": "查询出发日",
              "control": "dateTimePicker"
            },
            "inquery_usr": {
              "label": "查询人",
              "control": "text"
            },
            "inquery_type": {
              "label": "查询形式",
              "control": "text"
            },
            "receive_date": {
              "label": "收到复查日",
              "control": "text"
            },
            "guar_name": {
              "label": "保证人名称",
              "control": "text"
            },
            "asset_status": {
              "label": "汇票状态",
              "control": "select",
              "dict": {
                "dictType": "MS_E_YESORNO",
                "dictKey": [
                  "MS_E_YESORNO"
                ]
              }
            },
            "remark": {
              "label": "备注",
              "control": "textarea",
              "span": 24,
              "autosize": { "minRows": 3, "maxRows": 5 },
              "maxlength": 100,
              "showWordLimit": true
            }
          }]
        }
    }
}
