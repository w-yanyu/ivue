{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "btns": [],
            "name": "list01",
            "initRequest": "${{params}}",
          "controls": [{
            "asset_list_id": {
              "label": "资产清单序号",
              "control": "hidden"
            },
            "qualitative": {
              "label": "出质人",
              "control": "text",
              "rules": [{
                "required": true,
                "message": "请输入出质人"
              }]
            },
            "debtor": {
              "label": "债务人",
              "control": "text",
              "rules": [{
                "required": true,
                "message": "请输入债务人"
              }]
            },
            "accdue_type": {
              "label": "收付类型",
              "control": "text",
              "rules": [{
                "required": true,
                "message": "请输入收付类型"
              }]
            },
            "accdue_name": {
              "label": "涉及单位名称",
              "control": "text",
              "rules": [{
                "required": true,
                "message": "请输入涉及单位名称"
              }]
            },
            "ccy_code": {
              "label": "币种",
              "control": "select",
              "dict": {
                "dictType": "MS_CURRENCY",
                "dictKey": [
                  "MS_CURRENCY"
                ]
              },
              "rules": [{
                "required": true,
                "message": "请输入币种"
              }]
            },
            "accdue_amt": {
              "label": "账款金额(元)",
              "control": "text",
              "rules": [{
                "required": true,
                "message": "请输入账款金额"
              }]
            },
            "debtperiod_unit": {
              "label": "账龄单位",
              "control": "select",
              "dict": {
                "dictType": "CO_E_DATETP",
                "dictKey": [
                  "CO_E_DATETP"
                ]
              },
              "rules": [{
                "required": true,
                "message": "请输入账龄单位"
              }]
            },
            "accdue_age": {
              "label": "账龄",
              "control": "text",
              "rules": [{
                "required": true,
                "message": "请输入账龄"
              }]
            },
            "accdue_start_date": {
              "label": "发生日期",
              "control": "dateTimePicker",
              "format": "yyyyMMdd",
              "rules": [{
                "required": true,
                "message": "请选择发生日期"
              }]
            },
            "accdue_end_date": {
              "label": "统计截止日期",
              "control": "dateTimePicker",
              "format": "yyyyMMdd",
              "rules": [{
                "required": true,
                "message": "请选择统计截止日"
              }]
            }
          },
            {
              "accdue_cause": {
                "label": "账款形成原因",
                "control": "textarea",
                "span": 24,
                "autosize": { "minRows": 2, "maxRows": 4 },
                "rules": [{
                  "max": "400",
                  "message": "必填项且长度不能超过400个字符"
                },
                  {
                    "required": true,
                    "message": "请输入账款形成原因"
                  }
                ]
              }
            },
            {
              "remark": {
                "label": "备注",
                "control": "textarea",
                "span": 24,
                "autosize": { "minRows": 2, "maxRows": 4 },
                "rules": [{
                  "max": "400",
                  "message": "必填项且长度不能超过400个字符"
                }]
              }
            }
          ]
        }
    }
}

