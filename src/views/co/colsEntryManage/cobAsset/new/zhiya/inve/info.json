{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "btns": [],
      "name": "list01",
      "controls": [
        {
          "asset_list_id": {
            "label": "资产清单序号",
            "control": "hidden"
          },
          "goods_name": {
            "label": "货物名称",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入货物名称"
              }
            ]
          },
          "inve_no": {
            "label": "存货单号",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入存货单号"
              }
            ]
          },
          "unit": {
            "label": "单位",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入单位"
              }
            ]
          },
          "car_num": {
            "label": "数量",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入数量"
              }
            ]
          },
          "inve_state": {
            "label": "货存状态",
            "control": "select",
            "dict": {
              "dictType": "CO_E_INVESTATE",
              "dictKey": [
                "CO_E_INVESTATE"
              ]
            },
            "rules": [
              {
                "required": true,
                "message": "请选择货存状态"
              }
            ]
          },
          "set_pleinve_amt": {
            "label": "设定抵押存货金额(元)",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入设定抵押存货金额"
              }
            ]
          },
          "min_pledge_val": {
            "label": "最低保持存货价值(元)",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入最低保持存货价值"
              }
            ]
          },
          "goods_kind": {
            "label": "货物种类",
            "control": "select",
            "dict": {
              "dictType": "CO_E_GOODSKD",
              "dictKey": [
                "CO_E_GOODSKD"
              ]
            },
            "rules": [
              {
                "required": true,
                "message": "请选择货物种类"
              }
            ]
          }
        },
        {
          "remark": {
            "label": "备注",
            "control": "textarea",
            "span": 24,
            "autosize":{"minRows": 3,"maxRows":5},
            "maxlength": 100,
            "showWordLimit": true
          }
        }
      ]
    }
  }
}
