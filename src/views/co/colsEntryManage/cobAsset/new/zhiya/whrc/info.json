{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "btns": [],
      "name": "list01",
      "controls": [
        {
          "asset_list_id": {
            "label": "资产清单序号",
            "control": "hidden"
          },
          "whrc_distributor": {
            "label": "仓单出据人",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入仓单出据人"
              }
            ]
          },
          "whrc_no": {
            "label": "仓单号码",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入仓单号码"
              }
            ]
          },
          "whrc_total_val": {
            "label": "市场总价值",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入市场总价值"
              }
            ]
          },
          "ccy_code": {
            "label": "币种",
            "control": "select",
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": [
                "MS_CURRENCY"
              ]
            },
            "rules": [
              {
                "required": true,
                "message": "请选择币种"
              }
            ]
          },
          "goods_name": {
            "label": "货物名称",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入货物名称"
              }
            ]
          },
          "goods_num": {
            "label": "货物数量",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入货物数量"
              }
            ]
          },
          "goods_num_unit": {
            "label": "货物数量单位",
            "control": "select",
            "dict": {
              "dictType": "CO_E_MATTERU",
              "dictKey": [
                "CO_E_MATTERU"
              ]
            },
            "rules": [
              {
                "required": true,
                "message": "请选择货物数量单位"
              }
            ]
          }
        },
        {
          "remark": {
            "label": "备注",
            "control": "textarea",
            "span": 24,
            "autosize":{"minRows": 3,"maxRows":5},
            "maxlength": 100,
            "showWordLimit": true
          }
        }
      ]
    }
  }
}
