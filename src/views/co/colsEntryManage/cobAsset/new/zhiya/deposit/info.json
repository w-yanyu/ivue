{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "btns": [],
      "name": "list01",
      "controls": [
        {
          "asset_list_id": {
            "label": "资产清单序号",
            "control": "hidden"
          },
          "is_acct": {
            "label": "是否我行存单",
            "control": "select",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ]
            },
            "rules": [
              {
                "required": true,
                "message": "请选择是否我行存单"
              }
            ]
          },
          "acct_seq": {
            "label": "存单凭证号",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入存单凭证号"
              }
            ]
          },
          "acct_no": {
            "label": "帐号",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入帐号"
              }
            ]
          },
          "deposit_acct_name": {
            "label": "存单户名",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入存单户名"
              }
            ]
          },
          "bill_bank_name": {
            "label": "出具行名称",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入出具行名称"
              }
            ]
          },
          "acct_type": {
            "label": "存单类型",
            "control": "select",
            "dict": {
              "dictType": "CO_E_ACCTTYPE",
              "dictKey": [
                "CO_E_ACCTTYPE"
              ]
            },
            "rules": [
              {
                "required": true,
                "message": "请选择是否我方"
              }
            ]
          },
          "ccy_code": {
            "label": "币种",
            "control": "select",
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": [
                "MS_CURRENCY"
              ]
            },
            "rules": [
              {
                "required": true,
                "message": "请选择币种"
              }
            ]
          },
          "amt": {
            "label": "存单面额(元)",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入存单面额"
              }
            ]
          },
          "sec_end_date": {
            "label": "到期日期",
            "control": "dateTimePicker",
            "format": "yyyyMMdd",
            "rules": [
              {
                "required": true,
                "message": "请输入到期日期"
              }
            ]
          },
          "start_date": {
            "label": "存单存入日期 ",
            "control": "dateTimePicker",
            "format": "yyyyMMdd"
          }
        },
        {
          "remark": {
            "label": "备注",
            "control": "textarea",
            "span": 24,
            "autosize": {
              "minRows": 3,
              "maxRows": 5
            },
            "maxlength": 100,
            "showWordLimit": true
          }
        }
      ]
    }
  }
}
