{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "btns": [],
      "name": "list01",
      "controls": [
        {
          "asset_list_id": {
            "label": "资产清单序号",
            "control": "hidden"
          },
          "stock_cd": {
            "label": "股票代码",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入股票代码"
              }
            ]
          },
          "stock_name": {
            "label": "股票名称",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入股票名称"
              }
            ]
          },
          "fund_card_no": {
            "label": "资金卡号",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入资金卡号"
              }
            ]
          },
          "exchange": {
            "label": "所在交易所",
            "control": "text"
          },
          "stock_property": {
            "label": "股票性质",
            "control": "select",
            "dict": {
              "dictType": "CO_E_STOCKPY",
              "dictKey": [
                "CO_E_STOCKPY"
              ]
            }
          },
          "stock_acct_no": {
            "label": "股东帐号",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入股东帐号"
              }
            ]
          },
          "stock_owned_amt": {
            "label": "持股数量",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入持股数量"
              }
            ]
          },
          "stock_owned_pct": {
            "label": "持股比例(%)",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入持股比例"
              }
            ]
          },
          "stock_unit_price": {
            "label": "每股价值",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入每股价值"
              }
            ]
          },
          "stock_curr_val": {
            "label": "市值",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入市值"
              }
            ]
          },
          "orig_val": {
            "label": "原值",
            "control": "text"
          },
          "issue_company": {
            "label": "发行企业",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入发行企业"
              }
            ]
          },
          "issue_start_date": {
            "label": "发行日期",
            "control": "dateTimePicker",
            "format": "yyyyMMdd"
          },
          "matu_date": {
            "label": "到期日期",
            "control": "dateTimePicker",
            "format": "yyyyMMdd",
            "rules": [
              {
                "required": true,
                "message": "请输入到期日期"
              }
            ]
          },
          "is_restrained": {
            "label": "是否流通限制",
            "control": "select",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ]
            },
            "rules": [
              {
                "required": true,
                "message": "请选择是否流通限制"
              }
            ]
          },
          "matterpledge_num": {
            "label": "质押数量(股)",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入质押数量"
              }
            ]
          },
          "last_per_share_bal": {
            "label": "上季度每股净资产余额(元)",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入上季度每股净资产余额"
              }
            ]
          },
          "ccy_code": {
            "label": "币种",
            "control": "select",
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": [
                "MS_CURRENCY"
              ]
            }
          },
          "stock_type": {
            "label": "股权种类",
            "control": "select",
            "dict": {
              "dictType": "CO_E_STOCKTP",
              "dictKey": [
                "CO_E_STOCKTP"
              ]
            }
          },
          "trustee_code": {
            "label": "托管机构代码",
            "control": "text"
          },
          "list_country_code": {
            "label": "上市国家及区域",
            "control": "text"
          },
          "is_main_stock_index": {
            "label": "是否被涵盖在主要的股票指数",
            "control": "select",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ]
            }
          }
        },
        {
          "remark": {
            "label": "备注",
            "control": "textarea",
            "span": 24,
            "autosize":{"minRows": 3,"maxRows":5},
            "maxlength": 100,
            "showWordLimit": true
          }
        }
      ]
    }
  }
}
