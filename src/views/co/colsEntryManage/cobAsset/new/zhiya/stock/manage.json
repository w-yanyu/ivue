{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "doRequest": {
        "url": "/SUMP/call/RPCCall",
        "method": "post",
        "params":{
          "opera_type":"1",
          "servicecode": "co2029"
        }
      },
      "controlsGroup": [
        {
          "title": "资产信息",
          "tooltip": "资产信息",
          "titleIcon": "el-icon-info",
          "expand": true,
          "accordion": false,
          "controls": [
            {
              "list03": {
                "span": 24,
                "control": "dynamic",
                "toRequest": {
                  "url": "/views/co/colsEntryManage/cobAsset/new/info_property.json",
                  "params":{
                    "cust_no":"${{params.cust_no}}",
                    "owner_cust_id":"${{params.cust_no}}",
                    "colt_type":"${{params.colt_type}}",
                    "colt_prod_cd":"${{params.colt_prod_cd}}",
                    "guar_type":"${{params.guar_type}}",
                    "asset_type":"${{params.asset_type}}",
                    "owner":"${{params.cust_name}}",
                    "cert_no":"${{params.cert_no}}",
                    "owner_adrr":"${{params.issuing_addr}}",
                    "cert_type": "${{params.cert_type}}",
                    "owner_cust_type": "${{params.cust_type}}"
                  }
                }
              }
            }
          ]
        },
        {
          "title": "抵质押品信息",
          "tooltip": "抵质押品信息",
          "titleIcon": "el-icon-info",
          "expand": true,
          "accordion": false,
          "controls": [
            {
              "list01": {
                "span": 24,
                "control": "dynamic",
                "toRequest": {
                  "url": "/views/co/colsEntryManage/cobAsset/new/zhiya/stock/info.json",
                  "params":{
                    "cust_no":"${{params.cust_no}}",
                    "colt_type":"${{params.colt_type}}",
                    "colt_prod_cd":"${{params.colt_prod_cd}}",
                    "guar_type":"${{params.guar_type}}",
                    "asset_type":"${{params.asset_type}}"
                  }
                }
              }
            }
          ]
        },
        {
          "title": "抵质押品申请信息",
          "tooltip": "抵质押品申请信息",
          "titleIcon": "el-icon-info",
          "expand": true,
          "accordion": false,
          "controls": [
            {
              "list02": {
                "span": 24,
                "control": "dynamic",
                "toRequest": {
                  "url": "/views/co/colsEntryManage/cobAsset/new/info_apply.json",
                  "params":{
                    "cust_no":"${{params.cust_no}}",
                    "cust_name":"${{params.cust_name}}",
                    "colt_type":"${{params.colt_type}}",
                    "colt_prod_cd":"${{params.colt_prod_cd}}",
                    "guar_type":"${{params.guar_type}}",
                    "asset_type":"${{params.asset_type}}"
                  }
                }
              }
            }
          ]
        },
        {
          "title": "影像信息",
          "scope": "formData",
          "tooltip": "影像信息",
          "titleIcon": "el-icon-info",
          "expand": true,
          "accordion": false,
          "controls": [
            {
              "list04": {
                "span": 24,
                "isShowChildForm": true,
                "control": "dynamic",
                "toRequest": {
                  "url": "@info_image",
                  "params":{
                    "showFlag":true
                  }
                }
              }
            }
          ]
        }
      ]
    }
  }
}
