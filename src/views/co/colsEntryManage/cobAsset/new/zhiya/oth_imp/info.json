{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "btns": [],
      "name": "list01",
      "controls": [
        {
          "asset_list_id": {
            "label": "资产清单序号",
            "control": "hidden"
          },
          "oth_imp_amt": {
            "label": "资产金额(元)",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入资产金额"
              }
            ]
          },
          "ccy_code": {
            "label": "币种",
            "control": "select",
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": [
                "MS_CURRENCY"
              ]
            }
          },
          "asset_num": {
            "label": "质押物数量",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入质押物数量"
              }
            ]
          },
          "start_date": {
            "label": "购入/起始日期",
            "control": "dateTimePicker",
            "format": "yyyyMMdd",
            "rules": [
              {
                "required": true,
                "message": "请输入购入/起始日期"
              }
            ]
          },
          "matu_date": {
            "label": "到期日期",
            "control": "dateTimePicker",
            "format": "yyyyMMdd",
            "rules": [
              {
                "required": true,
                "message": "请输入到期日期"
              }
            ]
          }
        },
        {
          "asset_desc": {
            "label": "资产描述",
            "control": "textarea",
            "span": 24,
            "autosize":{"minRows": 3,"maxRows":5},
            "rules": [
              {
                "required": true,
                "message": "请输入资产描述"
              }
            ]
          }
        },
        {
          "remark": {
            "label": "备注",
            "control": "textarea",
            "span": 24,
            "autosize":{"minRows": 3,"maxRows":5},
            "maxlength": 100,
            "showWordLimit": true
          }
        }
      ]
    }
  }
}
