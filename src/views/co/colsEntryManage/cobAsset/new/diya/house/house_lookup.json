{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "house_id": {
            "control": "text",
            "label": "楼盘编号",
            "rules": [
              {
                "max": 50,
                "message": "长度不能超过50个字符"
              }
            ]
          },
          "data_state": {
            "control": "hidden",
            "value":2
          },
          "house_name": {
            "control": "text",
            "label": "楼盘名称",
            "rules": [
              {
                "max": 50,
                "message": "长度不能超过30个字符"
              }
            ]
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "co4016",
          "page_id": "/views/co/colsHouseMsg/colsHouseMsg_see/colsHouseMsg_see.json"
        }
      },
      "thead": [
        {
          "label": "序号",
          "type" :  "index"
        },
        {
          "label": "楼盘编号",
          "prop": "house_id",
          "align":"center"
        },
        {
          "label": "楼盘名称",
          "prop": "house_name",
          "align":"center"
        },
        {
          "label":"物业信息",
          "prop": "property_msg",
          "align":"center"
        },
        {
          "label": "建筑面积信息",
          "prop": "area_msg",
          "align":"center"
        },
        {
          "label": "均售价(元/平米)",
          "prop": "avg_prise",
          "align":"center"
        },
        {
          "label": "楼盘地址",
          "prop": "house_addr",
          "align":"center"
        },
        {
          "label": "预售证",
          "prop": "presell_cert",
          "align":"center"
        },
        {
          "label": "房地产产权证",
          "prop": "prop_cert",
          "align":"center"
        },
        {
          "label": "操作",
          "align": "center",
          "fixed": "right",
          "btns": [{
            "type": "bringback",
            "label": "选择"
          }]
        }
      ]
    }
  }
}
