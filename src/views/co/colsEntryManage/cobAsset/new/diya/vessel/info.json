{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "btns": [],
      "initRequest": "${{params}}",
      "name": "list01",
      "controls": [
        {
          "asset_list_id": {
            "label": "资产清单序号",
            "control": "hidden"
          },
          "ves_name": {
            "label": "船舶名称",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入船舶名称"
              }
            ]
          },
          "ves_brand": {
            "label": "品牌",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入品牌"
              }
            ]
          },
          "ves_type": {
            "label": "船舶类型",
            "control": "select",
            "dict": {
              "dictType": "CO_E_VESSELTYPE",
              "dictKey": [
                "CO_E_VESSELTYPE"
              ]
            }
          },
          "engine_no": {
            "label": "发动机号",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入发动机号"
              }
            ]
          },
          "country_id": {
            "label": "制造国家",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入制造国家"
              }
            ]
          },
          "use_year": {
            "label": "使用年限",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入使用年限"
              }
            ]
          },
          "depreciation_rate": {
            "label": "折旧率",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入折旧率"
              }
            ]
          },
          "discunt_depreciation_amt": {
            "label": "累计折旧金额(元)",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入累计折旧金额"
              }
            ]
          },
          "ves_lic_no": {
            "label": "船舶牌号",
            "control": "text"
          },
          "ves_cert_no": {
            "label": "船籍证书",
            "control": "text"
          },
          "ton": {
            "label": "吨数",
            "control": "text"
          },
          "inspection_no": {
            "label": "校验证号码",
            "control": "text"
          },
          "vise_cert_no": {
            "label": "签证簿编号",
            "control": "text"
          },
          "sec_cert_no": {
            "label": "安检证编号",
            "control": "text"
          },
          "port_name": {
            "label": "所在港口",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入所在港口"
              }
            ]
          },
          "produce_date": {
            "label": "生产日期",
            "control": "dateTimePicker",
            "format": "yyyyMMdd",
            "rules": [
              {
                "required": true,
                "message": "请输入生产日期"
              }
            ]
          },
          "issue_admin1": {
            "label": "发证机关1",
            "control": "text"
          },
          "op_cert_no": {
            "label": "营运证编号",
            "control": "text"
          },
          "issue_admin2": {
            "label": "发证机关2",
            "control": "text"
          },
          "drive_lic_no": {
            "label": "驾驶证编号",
            "control": "text"
          },
          "issue_admin3": {
            "label": "发证机关3",
            "control": "text"
          },
          "driving_cert_no": {
            "label": "行车编号",
            "control": "text"
          },
          "location": {
            "label": "所在地",
            "control": "text"
          },
          "insurant_cert_no": {
            "label": "证件号码",
            "control": "text"
          },
          "manufacturer": {
            "label": "生产厂商",
            "control": "text"
          },
          "factory_model": {
            "label": "厂家型号",
            "control": "text"
          },
          "produce_area": {
            "label": "产地",
            "control": "text"
          },
          "qualified_cert_no": {
            "label": "合格证编号",
            "control": "text"
          },
          "main_purpose": {
            "label": "主要用途",
            "control": "text"
          },
          "pur_contract_no": {
            "label": "购销合同编号",
            "control": "text"
          },
          "remark": {
            "label": "备注",
            "control": "textarea",
            "span": 24,
            "autosize":{"minRows": 3,"maxRows":5},
            "maxlength": 100,
            "showWordLimit": true
          }
        }
      ]
    }
  }
}

