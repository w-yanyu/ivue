{
  "module": "v-realonlyForm-btn",
  "layout": {
    "form": {
      "btns": [],
      "name": "list02",
      "initRequest": "${{parentInitData}}",
      "controls": [
        {
          "colt_type": {
            "label": {
              "default": "资产类型",
              "i18n": "I10.asset-type"
                      },
            "control": "remoteSelect",
            "disabled": true,
            "valueField": "colt_type",
            "labelField": "colt_name",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "post",
              "params": {
                "guar_type": "${{params.guar_type}}",
                "servicecode": "co1012"
              }
            }
          },
          "colt_prod_cd": {
            "label": {
              "default": "抵质押品产品",
              "i18n": "I10.collateral-products"
                      },
            "control": "remoteSelect",
            "disabled": true,
            "valueField": "colt_prod_cd",
            "labelField": "colt_prod_name",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "post",
              "params": {
                "servicecode": "co1013",
                "colt_type": "${{params.colt_type}}"
              }
            }
          },
          "guar_type": {
            "label": {
              "default": "担保方式",
              "i18n": "I10.guarantee-method"
                      },
            "control": "select",
            "disabled": true,
            "dict": {
              "dictType": "MS_E_GUAR_TYPE",
              "dictKey": [
                "MS_E_GUAR_TYPE"
              ],
              "format": "label"
            },
            "showItem": {
              "2": [
                "own_cert_type",
                "own_cert_type2",
                "own_cert_type3",
                "own_cert_type4",
                "own_cert_no",
                "own_cert_no2",
                "own_cert_no3",
                "own_cert_no4"
              ]
            },
            "rules": [{
              "required": true,
              "message": "请选择担保方式"
            }]
          },
          "count": {
            "label": {
              "default": "数量",
              "i18n": "I10.number"
                      },
            "control": "text",
            "disabled": true,
            "rules": [{
              "required": true,
              "message": "请输入数量并且长度不超过19"
            }]
          },
          "matu_date": {
            "label": {
              "default": "到期日",
              "i18n": "I10.due-date"
                      },
            "disabled": true,
            "control": "dateTimePicker",
            "format": "yyyyMMdd"
          },
          "store_addr": {
            "label": {
              "default": "存放地址",
              "i18n": "I10.Storage-address"
                      },
            "control": "text",
            "disabled": true,
            "rules": [{
              "required": true,
              "message": "请输入存放地址"
            }]
          },
          "is_master": {
            "label": {
              "default": "是否主担保物",
              "i18n": "I10.main-collateral"
                      },
            "control": "select",
            "disabled": true,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ]
            },
            "rules": [{
              "required": true,
              "message": "请选择是否为主担保物"
            }]
          },
          "sec_val": {
            "label": {
              "default": "抵质押额（元）",
              "i18n": "I10.Amount-of-mortgage-and-pledge"
                      },
            "control": "currency",
            "disabled": true,
            "rules": [{
              "required": true,
              "message": "请输入抵质押额"
            }]
          },
          "sec_rate": {
            "label": {
              "default": "抵质押率",
              "i18n": "I10.Mortgage-and-pledge-ratio"
                      },
            "control": "currency",
            "disabled": true,
            "rules": [{
              "required": true,
              "message": "请输入抵质押率"
            }]
          },
          "own_cert_type": {
            "label": {
              "default": "他项权证类型",
              "i18n": "I10.Other-types-of-warrants"
                      },
            "control": "select",
            "disabled": true,
            "initShow": false,
            "dict": {
              "dictType": "CO_E_OWNCERTP",
              "dictKey": [
                "CO_E_OWNCERTP"
              ]
            }
          },
          "own_cert_no": {
            "label": {
              "default": "他项权证号码",
              "i18n": "I10.Other-number-of-warrants"
                      },
            "control": "text",
            "disabled": true,
            "initShow": false
          },
          "own_cert_type2": {
            "label": {
              "default": "他项权证类型2",
              "i18n": "I10.Other-types-of-warrants2"
                      },
            "initShow": false,
            "control": "select",
            "disabled": true,
            "dict": {
              "dictType": "CO_E_OWNCERTP",
              "dictKey": [
                "CO_E_OWNCERTP"
              ]
            }
          },
          "own_cert_no2": {
            "label": {
              "default": "他项权证号码2",
              "i18n": "I10.Other-number-of-warrants2"
                      },
            "initShow": false,
            "control": "text",
            "disabled": true
          },
          "own_cert_type3": {
            "label": {
              "default": "他项权证类型3",
              "i18n": "I10.Other-types-of-warrants3"
                      },
            "initShow": false,
            "control": "select",
            "disabled": true,
            "dict": {
              "dictType": "CO_E_OWNCERTP",
              "dictKey": [
                "CO_E_OWNCERTP"
              ]
            }
          },
          "own_cert_no3": {
            "label": {
              "default": "他项权证号码3",
              "i18n": "I10.Other-number-of-warrants3"
                      },
            "control": "text",
            "disabled": true,
            "initShow": false
          },
          "own_cert_type4": {
            "label": {
              "default": "他项权证类型4",
              "i18n": "I10.Other-types-of-warrants4"
                      },
            "initShow": false,
            "control": "select",
            "disabled": true,
            "dict": {
              "dictType": "CO_E_OWNCERTP",
              "dictKey": [
                "CO_E_OWNCERTP"
              ]
            }
          },
          "own_cert_no4": {
            "label": {
              "default": "他项权证号码4",
              "i18n": "I10.Other-number-of-warrants4"
                      },
            "control": "text",
            "disabled": true,
            "initShow": false
          }
        },
        {
          "remark": {
            "label": {
              "default": "备注",
              "i18n": "I10.notes"
                      },
            "control": "textarea",
            "disabled": true,
            "span": 24,
            "autosize": {
              "minRows": 3,
              "maxRows": 5
            },
            "maxlength": 100,
            "showWordLimit": true
          }
        }
      ]
    }
  }
}
