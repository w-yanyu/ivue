{
    "module": "v-realonlyForm-btn",
    "layout": {
        "form": {
            "initRequest": "${{parentInitData}}",
            "btns": [],
            "name": "list01",
            "controls": [{
                    "asset_list_id": {
                        "label": "资产清单序号",
                        "control": "hidden"
                    },
                    "is_acct": {
                        "label": "是否我行存单",
                        "control": "select",
                        "disabled": true,
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ]
                        }
                    },
                    "acct_seq": {
                        "label": "存单凭证号",
                        "disabled": true,
                        "control": "text"
                    },
                    "acct_no": {
                        "label": "帐号",
                        "control": "text",
                        "disabled": true
                    },
                    "deposit_acct_name": {
                        "label": "存单户名",
                        "disabled": true,
                        "control": "text"
                    },
                    "bill_bank_name": {
                        "label": "出具行名称",
                        "disabled": true,
                        "control": "text"
                    },
                    "acct_type": {
                        "label": "存单类型",
                        "disabled": true,
                        "control": "select",
                        "dict": {
                            "dictType": "CO_E_ACCTTYPE",
                            "dictKey": [
                                "CO_E_ACCTTYPE"
                            ]
                        }
                    },
                    "ccy_code": {
                        "label": "币种",
                        "control": "select",
                        "value": "CNY",
                        "disabled": true,
                        "dict": {
                            "dictType": "MS_CURRENCY",
                            "dictKey": [
                                "MS_CURRENCY"
                            ]
                        }
                    },
                    "amt": {
                        "label": "存单面额(元)",
                        "disabled": true,
                        "control": "text"
                    },
                    "sec_end_date": {
                        "label": "到期日期",
                        "control": "dateTimePicker",
                        "disabled": true,
                        "format": "yyyyMMdd"
                    },
                    "start_date": {
                        "label": "存单存入日期 ",
                        "disabled": true,
                        "control": "dateTimePicker",
                        "format": "yyyyMMdd"
                    }
                },
                {
                    "remark": {
                        "label": "备注",
                        "disabled": true,
                        "control": "textarea",
                        "span": 24,
                        "autosize": { "minRows": 3, "maxRows": 5 },
                        "maxlength": 100,
                        "showWordLimit": true
                    }
                }
            ]
        }
    }
}
