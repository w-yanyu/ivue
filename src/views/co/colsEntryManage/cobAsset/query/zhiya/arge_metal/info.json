{
    "module": "v-realonlyForm-btn",
    "layout": {
        "form": {
            "initRequest": "${{parentInitData}}",
            "btns": [],
            "name": "list01",
            "controls": [{
                    "asset_list_id": {
                        "label": "资产清单序号",
                        "control": "hidden"
                    },
                    "arge_type": {
                        "label": "贵金属品种",
                        "disabled": true,
                        "control": "select",
                        "dict": {
                            "dictType": "CO_E_ARGETP",
                            "dictKey": [
                                "CO_E_ARGETP"
                            ]
                        }
                    },
                    "reside": {
                        "label": "权属",
                        "control": "select",
                        "disabled": true,
                        "dict": {
                            "dictType": "CO_E_AUTH",
                            "dictKey": [
                                "CO_E_AUTH"
                            ]
                        }
                    },
                    "gold_no": {
                        "label": "贵金属交易编码",
                        "disabled": true,
                        "control": "text"
                    },
                    "arge_percent": {
                        "label": "成色",
                        "disabled": true,
                        "control": "text"
                    },
                    "gold_weight": {
                        "label": "标准重量（克）",
                        "disabled": true,
                        "control": "text"
                    },
                    "arge_pled_weight": {
                        "label": "质押重量（克）",
                        "disabled": true,
                        "control": "text"
                    }
                },
                {
                    "remark": {
                        "label": "备注",
                        "control": "textarea",
                        "disabled": true,
                        "span": 24,
                        "autosize": { "minRows": 3, "maxRows": 5 },
                        "maxlength": 100,
                        "showWordLimit": true
                    }
                }
            ]
        }
    }
}
