{
    "module": "v-realonlyForm-btn",
    "layout": {
        "form": {
            "btns": [],
            "name": "list01",
            "initRequest": "${{parentInitData}}",
            "controls": [{
                    "asset_list_id": {
                        "label": "资产清单序号",
                        "disabled": true,
                        "control": "hidden"
                    },
                    "qualitative": {
                        "label": "出质人",
                        "control": "text",
                        "disabled": true
                    },
                    "debtor": {
                        "label": "债务人",
                        "disabled": true,
                        "control": "text"
                    },
                    "accdue_type": {
                        "label": "收付类型",
                        "control": "text",
                        "disabled": true
                    },
                    "accdue_name": {
                        "label": "涉及单位名称",
                        "disabled": true,
                        "control": "text"
                    },
                    "ccy_code": {
                        "label": "币种",
                      "disabled": true,
                      "control": "select",
                        "dict": {
                            "dictType": "MS_CURRENCY",
                            "dictKey": [
                                "MS_CURRENCY"
                            ]
                        }
                    },
                    "accdue_amt": {
                        "label": "账款金额(元)",
                        "control": "text",
                        "disabled": true
                    },
                    "debtperiod_unit": {
                        "label": "账龄单位",
                        "disabled": true,
                        "control": "select",
                        "dict": {
                            "dictType": "CO_E_DATETP",
                            "dictKey": [
                                "CO_E_DATETP"
                            ]
                        }
                    },
                    "accdue_age": {
                        "label": "账龄",
                        "disabled": true,
                        "control": "text"
                    },
                    "accdue_start_date": {
                        "label": "发生日期",
                        "disabled": true,
                        "control": "dateTimePicker",
                        "format": "yyyyMMdd"
                    },
                    "accdue_end_date": {
                        "label": "统计截止日期",
                        "disabled": true,
                        "control": "dateTimePicker",
                        "format": "yyyyMMdd"
                    }
                },
                {
                    "accdue_cause": {
                        "label": "账款形成原因",
                        "disabled": true,
                        "control": "textarea",
                        "span": 24,
                        "autosize": { "minRows": 2, "maxRows": 4 }
                    }
                },
                {
                    "remark": {
                        "label": "备注",
                        "disabled": true,
                        "control": "textarea",
                        "span": 24,
                        "autosize": { "minRows": 2, "maxRows": 4 }
                    }
                }
            ]
        }
    }
}
