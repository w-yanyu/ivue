{
    "module": "v-realonlyForm-btn",
    "layout": {
        "form": {
            "initRequest": "${{parentInitData}}",
            "btns": [],
            "name": "list01",
            "controls": [{
                "asset_list_id": {
                    "label": "资产清单序号",
                    "control": "hidden"
                },
                "imm_catg": {
                    "label": "无形资产类型",
                    "control": "text",
                    "disabled": true
                },
                "imm_type": {
                    "label": "无形资产子类型",
                    "disabled": true,
                    "control": "text"
                },
                "cert_no": {
                    "label": "证书号码",
                    "disabled": true,
                    "control": "text"
                },
                "valid_date": {
                    "label": "有效期",
                    "disabled": true,
                    "control": "dateTimePicker"
                },
                "ccy_code": {
                    "label": "币种",
                    "control": "select",
                    "disabled": true,
                    "value": "CNY",
                    "dict": {
                        "dictType": "MS_CURRENCY",
                        "dictKey": [
                            "MS_CURRENCY"
                        ]
                    }
                },
                "orig_val": {
                    "label": "原值",
                    "control": "text",
                    "disabled": true
                },
                "curr_val": {
                    "label": "现值",
                    "disabled": true,
                    "control": "text"
                },
                "remark": {
                    "label": "备注",
                    "disabled": true,
                    "control": "textarea",
                    "span": 24,
                    "autosize": { "minRows": 3, "maxRows": 5 },
                    "maxlength": 100,
                    "showWordLimit": true
                }
            }]
        }
    }
}
