{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{parentInitData}}",
      "btns": [],
      "name": "list01",
      "controls": [
        {
          "asset_list_id": {
            "label": "资产清单序号",
            "control": "hidden"
          },
          "land_no": {
            "label": "土地编号",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入土地编号"
              }
            ]
          },
          "land_name": {
            "label": "土地名称",
            "control": "text"
          },
          "land_use_cert_no": {
            "label": "土地使用权证编号",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入土地使用权证编号"
              }
            ]
          },
          "address": {
            "label": "地址",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入地址"
              }
            ]
          },
          "auth_src": {
            "label": "权属来源",
            "control": "select",
            "dict": {
              "dictType": "CO_E_AUTHSR",
              "dictKey": [
                "CO_E_AUTHSR"
              ]
            },
            "rules": [
              {
                "required": true,
                "message": "请选择权属来源"
              }
            ]
          },
          "land_use_start_date": {
            "label": "土地使用权生效日",
            "control": "dateTimePicker",
            "format": "yyyyMMdd",
            "rules": [
              {
                "required": true,
                "message": "请输入土地使用权生效日"
              }
            ]
          },
          "land_start_term": {
            "label": "土地使用权有效期(年)",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入土地使用权有效期"
              }
            ]
          },
          "occupy_state": {
            "label": "占用形态",
            "control": "select",
            "dict": {
              "dictType": "CO_E_OCCUPS",
              "dictKey": [
                "CO_E_OCCUPS"
              ]
            },
            "rules": [
              {
                "required": true,
                "message": "请选择占用形态"
              }
            ],
            "showItem": {
              "2": [
                "rent_end_date",
                "lessee"
              ]
            }
          },
          "rent_end_date": {
            "label": "出租到期日",
            "control": "dateTimePicker",
            "format": "yyyyMMdd",
            "initShow": false,
            "rules": [
              {
                "required": true,
                "message": "请输入出租到期日"
              }
            ]
          },
          "lessee": {
            "label": "承租人",
            "control": "text",
            "initShow": false,
            "rules": [
              {
                "required": true,
                "message": "请输入承租人"
              }
            ]
          },
          "land_purpose": {
            "label": "土地用途",
            "control": "select",
            "dict": {
              "dictType": "CO_E_ATPURP",
              "dictKey": [
                "CO_E_ATPURP"
              ]
            },
            "rules": [
              {
                "required": true,
                "message": "请选择土地用途"
              }
            ]
          },
          "land_property": {
            "label": "土地性质",
            "control": "select",
            "dict": {
              "dictType": "CO_E_ASSTTP",
              "dictKey": [
                "CO_E_ASSTTP"
              ]
            },
            "rules": [
              {
                "required": true,
                "message": "请选择土地性质"
              }
            ],
            "showItem": {
              "3": [
                "is_transact_pledge_reg"
              ]
            }
          },
          "is_transact_pledge_reg": {
            "label": "是否办理抵押登记",
            "control": "select",
            "initShow": false,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ]
            }
          },
          "scope": {
            "label": "四周范围",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入四周范围"
              }
            ]
          },
          "own_valid_date": {
            "label": "他项权利有效期",
            "control": "dateTimePicker",
            "format": "yyyyMMdd",
            "rules": [
              {
                "required": true,
                "message": "请输入他项权利有效期"
              }
            ]
          },
          "area": {
            "label": "土地面积(㎡)",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入面积"
              }
            ]
          },
          "pledge_area": {
            "label": "抵押面积(㎡)",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入抵押面积"
              }
            ]
          },
          "is_adhering_object": {
            "label": "地上是否有附着物",
            "control": "select",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ]
            },
            "rules": [
              {
                "required": true,
                "message": "请选择地上是否有附着物"
              }
            ],
            "showItem": {
              "Y": [
                "adheringobj_type",
                "adheringobj_area",
                "is_pledge_together",
                "adheringobj_detail"
              ]
            }
          },
          "adheringobj_type": {
            "label": "附着物种类",
            "control": "select",
            "initShow": false,
            "dict": {
              "dictType": "CO_E_OBJTP",
              "dictKey": [
                "CO_E_OBJTP"
              ]
            },
            "rules": [
              {
                "required": true,
                "message": "请选择附着物种类"
              }
            ]
          },
          "adheringobj_area": {
            "label": "附着物面积(㎡)",
            "control": "text",
            "initShow": false,
            "rules": [
              {
                "required": true,
                "message": "请输入附着物面积"
              }
            ]
          },
          "is_pledge_together": {
            "label": "是否同时抵押",
            "initShow": false,
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入是否同时抵押"
              }
            ]
          },
          "adheringobj_detail": {
            "label": "描述",
            "control": "textarea",
            "placeholder": "请描述名称，是否同时抵押等",
            "span": 24,
            "autosize":{"minRows": 3,"maxRows":5},
            "maxlength": 100,
            "showWordLimit": true,
            "initShow": false,
            "rules": [
              {
                "required": true,
                "message": "请输入描述"
              }
            ]
          },
          "land_section_code": {
            "label": "地块编号",
            "control": "text"
          },
          "issue_admin": {
            "label": "发证机关",
            "control": "text"
          },
          "issue_admin": {
            "label": "发证机关地址",
            "control": "text"
          },
          "land_gain_type": {
            "label": "土地取得方式",
            "control": "select",
            "dict": {
              "dictType": "CO_E_LANDSELTYPE",
              "dictKey": [
                  "CO_E_LANDSELTYPE"
              ],
              "format": "label"
           }
          },
          "chart_no": {
            "label": "发证机关图号",
            "control": "text"
          },
          "own_cert_no": {
            "label": "他项权证号码",
            "control": "text"
          },
          "own_cust_name": {
            "label": "他项权利人名称",
            "control": "text"
          },
          "remark": {
            "label": "备注",
            "control": "textarea",
            "span": 24,
            "autosize":{"minRows": 3,"maxRows":5},
            "maxlength": 100,
            "showWordLimit": true
          }
        }
      ]
    }
