{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{parentInitData}}",
      "btns": [],
      "name": "list01",
      "controls": [
        {
          "asset_list_id": {
            "label": "资产清单序号",
            "control": "hidden"
          },
          "veh_brand": {
            "label": "品牌",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入品牌"
              },
              {
                "max": "10",
                "message": "最多输入10位字符"
              }
            ]
          },
          "veh_model": {
            "label": "型号",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入型号"
              }
            ]
          },
          "pur_contract_no": {
            "label": "购销合同编号",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入购销合同编号"
              }
            ]
          },
          "factory_model": {
            "label": "厂家型号",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入厂家型号"
              }
            ]
          },
          "frame_no": {
            "label": "车架编号",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入车架编号"
              }
            ]
          },
          "auth_src": {
            "label": "权属来源",
            "control": "select",
            "dict": {
              "dictType": "CO_E_AUTHSR",
              "dictKey": [
                "CO_E_AUTHSR"
              ]
            }
          },
          "occupy_state": {
            "label": "占用形态",
            "control": "select",
            "dict": {
              "dictType": "CO_E_OCCUPS",
              "dictKey": [
                "CO_E_OCCUPS"
              ]
            },
            "showItem": {
              "2": [
                "rent_end_date",
                "lessee"
              ]
            },
            "rules": [
              {
                "required": true,
                "message": "请选择占用形态"
              }
            ]
          },
          "rent_end_date": {
            "label": "出租到期日",
            "control": "dateTimePicker",
            "initShow": false,
            "format": "yyyyMMdd",
            "rules": [
              {
                "required": true,
                "message": "请输入出租到期日"
              }
            ]
          },
          "lessee": {
            "label": "承租人",
            "initShow": false,
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入承租人"
              }
            ]
          },
          "import_flag": {
            "label": "是否进口",
            "control": "select",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ]
            },
            "rules": [
              {
                "required": true,
                "message": "请选择是否进口"
              }
            ]
          },
          "main_purpose": {
            "label": "主要用途",
            "control": "textarea",
            "span": 24,
            "autosize": {
              "minRows": 3,
              "maxRows": 5
            },
            "maxlength": 100,
            "showWordLimit": true,
            "rules": [
              {
                "required": true,
                "message": "请输入主要用途"
              }
            ]
          },
          "car_num": {
            "label": "车辆数量",
            "control": "text"
          },
          "produce_area": {
            "label": "产地",
            "control": "text"
          },
          "manufacturer": {
            "label": "生产厂商",
            "control": "text"
          },
          "driving_cert_no": {
            "label": "行车编号",
            "control": "text"
          },
          "qualified_cert_no": {
            "label": "合格证编号",
            "control": "text"
          },
          "engine_no": {
            "label": "发动机编号",
            "control": "text"
          },
          "seating_num": {
            "label": "座位数",
            "control": "text"
          },
          "load": {
            "label": "载重",
            "control": "text"
          },
          "displacement": {
            "label": "排量",
            "control": "text"
          },
          "produce_date": {
            "label": "生产日期",
            "control": "dateTimePicker",
            "format": "yyyyMMdd"
          },
          "is_used_veh": {
            "label": "是否二手车",
            "control": "select",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ]
            }
          },
          "origin_purchase_date": {
            "label": "初始购置日期",
            "control": "dateTimePicker",
            "format": "yyyyMMdd"
          },
          "buy_date": {
            "label": "购入日期",
            "control": "dateTimePicker",
            "format": "yyyyMMdd"
          },
          "purchase_amt": {
            "label": "购入价格",
            "control": "text"
          },
          "invoice_no": {
            "label": "发票编号",
            "control": "text"
          },
          "use_year": {
            "label": "使用年限",
            "control": "text"
          },
          "veh_lic_no": {
            "label": "车辆牌号",
            "control": "text"
          },
          "location": {
            "label": "所在地",
            "control": "text"
          },

          "dr_lic_no": {
            "label": "驾驶证号_出租车专用",
            "control": "text"
          },
          "dr_lic_auth": {
            "label": "驾驶证发证机关_出租车专用",
            "control": "text"
          },
          "op_cert_no": {
            "label": "营运证编号",
            "control": "text"
          },
          "op_cert_auth": {
            "label": "营运证发证机关_出租车专用",
            "control": "text"
          },
          "remark": {
            "label": "备注",
            "control": "textarea",
            "span": 24,
            "autosize": {
              "minRows": 3,
              "maxRows": 5
            },
            "maxlength": 100,
            "showWordLimit": true
          }
        }
      ]
    }
  }
}
