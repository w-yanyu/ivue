            "label": "主要材料",
            "control": "textarea",
            "span": 24,
            "autosize": {
              "minRows": 3,
              "maxRows": 5
            },
            "maxlength": 40,
            "showWordLimit": true,
            "rules": [
              {
                "required": true,
                "message": "请输入主要材料"
              }
            ]
          },
          "location": {
            "label": "所在地",
            "control": "textarea",
            "span": 24,
            "autosize": {
              "minRows": 3,
              "maxRows": 5
            },
            "maxlength": 40,
            "showWordLimit": true,
            "rules": [
              {
                "required": true,
                "message": "请输入所在地"
              }
            ]
          },
          "equip_desc": {
            "label": "机器设备描述",
            "control": "textarea",
            "span": 24,
            "autosize": {
              "minRows": 3,
              "maxRows": 5
            },
            "maxlength": 40,
            "showWordLimit": true
          },
          "main_purpose": {
            "label": "主要用途",
            "control": "textarea",
            "span": 24,
            "autosize": {
              "minRows": 3,
              "maxRows": 5
            },
            "maxlength": 40,
            "showWordLimit": true,
            "rules": [
              {
                "required": true,
                "message": "请输入主要用途"
              }
            ]
          },
          "used_desc": {
            "label": "现在使用情况",
            "control": "textarea",
            "span": 24,
            "autosize": {
              "minRows": 3,
              "maxRows": 5
            },
            "maxlength": 40,
            "showWordLimit": true
          },
          "auth_name": {
            "label": "权证名称",
            "control": "textarea",
            "span": 24,
            "autosize": {
              "minRows": 3,
              "maxRows": 5
            },
            "maxlength": 40,
            "showWordLimit": true
          },
          "remark": {
            "label": "备注",
            "control": "textarea",
            "span": 24,
            "autosize": {
              "minRows": 3,
              "maxRows": 5
            },
            "maxlength": 100,
            "showWordLimit": true
          }
        }
