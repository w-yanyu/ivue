            "control": "text"
          },
          "equip_model": {
            "label": "机器型号",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入机器型号"
              }
            ]
          },
          "equip_brand": {
            "label": "品牌",
            "control": "text"
          },
          "equip_type": {
            "label": "设备类型",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入设备类型"
              }
            ]
          },
          "pur_contract_no": {
            "label": "购销合同编号",
            "control": "text"

