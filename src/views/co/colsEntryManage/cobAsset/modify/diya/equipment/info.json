          "buy_price": {
            "label": "购入价格(元)",
            "control": "text"
          },
          "is_dpct": {
            "label": "是否折旧",
            "control": "select",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ]
            },
            "showItem": {
              "Y": [
                "depreciation_rate",
                "dpct_start_date",
                "dpct_end_date",
                "discunt_depreciation_amt"
              ]
            }
          },
          "depreciation_rate": {
            "label": "折旧率",
            "control": "text"
          },
          "dpct_start_date": {
            "label": "折旧起期",
            "control": "dateTimePicker",
            "format": "yyyyMMdd"

          },
          "dpct_end_date": {
            "label": "折旧终期",
            "control": "dateTimePicker",
            "format": "yyyyMMdd"

          },
          "discunt_depreciation_amt": {
            "label": "累计折旧金额(元)",
            "control": "text"
          },
          "occupy_state": {
            "label": "占用形态",
            "control": "select",
            "dict": {
              "dictType": "CO_E_OCCUPS",
              "dictKey": [
                "CO_E_OCCUPS"
              ]
            },
            "showItem": {
              "2": [
                "tenant_end",
                "tenant"
              ]
            },
              "rules": [
              {
                "required": true,
                "message": "请输入占用形态"
              }
            ]
          },
          "rent_end_date": {
            "label": "出租到期日",
