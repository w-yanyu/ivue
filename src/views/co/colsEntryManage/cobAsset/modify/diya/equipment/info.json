            "label": "是否海关监控",
            "control": "select",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ]
            },
            "showItem": {
              "Y": [
                "customhouse_agree"
              ]
            },
            "rules": [
              {
                "required": true,
                "message": "请选择是否海关监控"
              }
            ]
          },
          "customhouse_agree": {
            "label": "海关是否同意抵押",
            "control": "select",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ]
            }
          }
