{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{parentInitData}}",
      "btns": [],
      "name": "list01",
      "controls": [
        {
          "asset_list_id": {
            "label": "资产清单序号",
            "control": "hidden"
          },
          "asset_loc": {
            "label": "资产位置",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入资产位置"
              }
            ]
          },
          "occupy_state": {
            "label": "占用形态",
            "control": "select",
            "dict": {
              "dictType": "CO_E_OCCUPS",
              "dictKey": [
                "CO_E_OCCUPS"
              ]
            }
          },
          "auth_src": {
            "label": "权属来源",
            "control": "select",
            "dict": {
              "dictType": "CO_E_AUTHSR",
              "dictKey": [
                "CO_E_AUTHSR"
              ]
            }
          },
          "car_num": {
            "label": "数量",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入质押数量"
              }
            ]
          },
          "asset_desc": {
            "label": "资产描述",
            "control": "textarea",
            "span": 24,
            "autosize":{"minRows": 3,"maxRows":5},
            "rules": [
              {
                "required": true,
                "message": "请输资产描述"
              }
            ]
          },
          "remark": {
            "label": "备注",
            "control": "textarea",
            "span": 24,
            "autosize":{"minRows": 3,"maxRows":5}
          }
        }
      ]
    }
  }
}
