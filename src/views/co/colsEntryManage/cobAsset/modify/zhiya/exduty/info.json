{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{parentInitData}}",
      "btns": [],
      "name": "list01",
      "controls": [
        {
          "asset_list_id": {
            "label": "资产清单序号",
            "control": "hidden"
          },
          "owner": {
            "label": "所有权人",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入所有权人"
              }
            ]
          },
          "borrower_relation": {
            "label": "与借款人关系",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入与借款人关系"
              }
            ]
          },
          "out_eval_price": {
            "label": "商位使用权评估价值(元)",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入商位使用权评估价值(元)"
              }
            ]
          },
          "start_date": {
            "label": "起始日期",
            "control": "dateTimePicker",
            "format": "yyyyMMdd",
            "rules": [
              {
                "required": true,
                "message": "请输入起始日期"
              }
            ]
          },
          "matu_date": {
            "label": "到期日期",
            "control": "dateTimePicker",
            "format": "yyyyMMdd",
            "rules": [
              {
                "required": true,
                "message": "请输入到期日期"
              }
            ]
          },
          "debtor": {
            "label": "债务人",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入债务人"
              }
            ]
          },
          "surate": {
            "label": "担保债权金额(元)",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入担保债权金额(元)"
              }
            ]
          },
          "become_cash_perc": {
            "label": "变现可能性(百分比)",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入变现可能性(百分比)"
              }
            ]
          },
          "is_repeat_pledge": {
            "label": "是否重复质押",
            "control": "select",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ]
            }
          },
          "pre_become_cash_perc": {
            "label": "预计变现程度(百分比)",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入预计变现程度(百分比)"
              }
            ]
          },
          "is_ple_legal": {
            "label": "质押是否合法",
            "control": "select",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ]
            }
          },
          "remark": {
            "label": "备注",
            "control": "textarea",
            "span": 24,
            "autosize": {
              "minRows": 3,
              "maxRows": 5
            },
            "maxlength": 100,
            "showWordLimit": true
          }
        }
      ]
    }
  }
}
