{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": "${{parentInitData}}",
            "btns": [],
            "name": "list01",
            "controls": [{
                "asset_list_id": {
                    "label": "资产清单序号",
                    "control": "hidden"
                },
                "imm_catg": {
                    "label": "无形资产类型",
                    "control": "text",
                    "rules": [{
                        "required": true,
                        "message": "请输入无形资产类型"
                    }]
                },
                "imm_type": {
                    "label": "无形资产子类型",
                    "control": "text",
                    "rules": [{
                        "required": true,
                        "message": "请输入无形资产子类型"
                    }]
                },
                "cert_no": {
                    "label": "证书号码",
                    "control": "text",
                    "rules": [{
                        "required": true,
                        "message": "请输入证书号码"
                    }]
                },
                "valid_date": {
                    "label": "有效期",
                    "control": "dateTimePicker",
                    "rules": [{
                        "required": true,
                        "message": "请输入有效期"
                    }]
                },
                "ccy_code": {
                    "label": "币种",
                    "control": "select",
                    "dict": {
                        "dictType": "MS_CURRENCY",
                        "dictKey": [
                            "MS_CURRENCY"
                        ]
                    },
                    "rules": [{
                        "required": true,
                        "message": "请选择币种"
                    }]
                },
                "orig_val": {
                    "label": "原值",
                    "control": "text",
                    "rules": [{
                        "required": true,
                        "message": "请输入原值"
                    }]
                },
                "curr_val": {
                    "label": "现值",
                    "control": "text",
                    "rules": [{
                        "required": true,
                        "message": "请输入现值"
                    }]
                },
                "remark": {
                    "label": "备注",
                    "control": "textarea",
                    "span": 24,
                    "autosize": { "minRows": 3, "maxRows": 5 },
                    "maxlength": 100,
                    "showWordLimit": true
                }
            }]
        }
    }
}
