{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "btns": [],
      "initRequest": "${{parentInitData}}",
      "name": "list01",
      "controls": [
        {
          "asset_list_id": {
            "label": "资产清单序号",
            "control": "hidden"
          },
          "forest_rights_type": {
            "label": "树种",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入树种"
              }
            ]
          },
          "addr": {
            "label": "位置",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入位置"
              }
            ]
          },
          "area": {
            "label": "面积(平方)",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入面积"
              }
            ]
          },
          "crop_date": {
            "label": "种植时间",
            "control": "dateTimePicker",
            "format": "yyyyMMdd",
            "rules": [
              {
                "required": true,
                "message": "请输入种植时间"
              }
            ]
          },
          "term_growth": {
            "label": "生长期限(月)",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入生长期限"
              }
            ]
          },
          "nums": {
            "label": "株数",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入株数"
              }
            ]
          },
          "plant_spacing": {
            "label": "株距(cm)",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入株距"
              }
            ]
          },
          "diameter": {
            "label": "直径(cm)",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入直径"
              }
            ]
          },
          "remark": {
            "label": "备注",
            "control": "textarea",
            "span": 24,
            "autosize":{"minRows": 3,"maxRows":5},
            "maxlength": 100,
            "showWordLimit": true
          }
        }
      ]
    }
  }
}
