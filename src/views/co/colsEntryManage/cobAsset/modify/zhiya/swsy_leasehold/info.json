{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{parentInitData}}",
      "btns": [],
      "name": "list01",
      "controls": [
        {
          "asset_list_id": {
            "label": "资产清单序号",
            "control": "hidden"
          },
          "stock_company_name": {
            "label": "股份公司名称",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入股份公司名称"
              }
            ]
          },
          "pledge_reg_unit": {
            "label": "质押登记单位",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入质押登记单位"
              }
            ]
          },
          "pledge_cert_name": {
            "label": "质押凭证名称",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入质押凭证名称"
              }
            ]
          },
          "pledge_cert_no": {
            "label": "质押凭证编号",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入质押凭证编号"
              }
            ]
          },
          "owner": {
            "label": "所以权人",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入所有权人"
              }
            ]
          },
          "borrower_relation": {
            "label": "与借款人关系",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入与借款人关系"
              }
            ]
          },
          "out_eval_price": {
            "label": "商位使用权评估价值(元)",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入商位使用权评估价值(元)"
              }
            ]
          },
          "start_date": {
            "label": "质押起始日期",
            "control": "dateTimePicker",
            "format": "yyyyMMdd",
            "rules": [
              {
                "required": true,
                "message": "请输入起始日期"
              }
            ]
          },
          "pledge_end_date": {
            "label": "质押到期日期",
            "control": "dateTimePicker",
            "format": "yyyyMMdd",
            "rules": [
              {
                "required": true,
                "message": "请输入质押到期日期"
              }
            ]
          },
          "surate": {
            "label": "担保债权金额(元)",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入担保债权金额(元)"
              }
            ]
          },
          "remark": {
            "label": "备注",
            "control": "textarea",
            "span": 24,
            "autosize":{"minRows": 3,"maxRows":5},
            "maxlength": 100,
            "showWordLimit": true
          }
        }
      ]
    }
  }
}
