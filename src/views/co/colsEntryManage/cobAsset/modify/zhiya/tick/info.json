{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{parentInitData}}",
      "btns": [],
      "name": "list01",
      "controls": [
        {
          "asset_list_id": {
            "label": "资产清单序号",
            "control": "hidden"
          },
          "tick_instlm_year": {
            "label": "国债期数(年)",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入国债期数(年)"
              }
            ]
          },
          "tick_instlm_instlm": {
            "label": "国债期数(期)",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入国债期数(期)"
              }
            ]
          },
          "interest_rate": {
            "label": "票面利率(%)",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入利率"
              }
            ]
          },
          "money": {
            "label": "票面金额(元)",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入金额"
              }
            ]
          },
          "matu_date": {
            "label": "到期日",
            "control": "dateTimePicker",
            "format": "yyyyMMdd",
            "rules": [
              {
                "required": true,
                "message": "请输入到期日期"
              }
            ]
          }
        },
        {
          "remark": {
            "label": "备注",
            "control": "textarea",
            "span": 24,
            "autosize":{"minRows": 3,"maxRows":5},
            "maxlength": 100,
            "showWordLimit": true
          }
        }
      ]
    }
  }
}
