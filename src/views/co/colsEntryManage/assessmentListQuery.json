{
    "module": "v_search_btn_datagrid",
    "layout": {
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": true,
            "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "co4005",
                    "colt_no": "${{params.colt_no}}"
                }
            },
            "pagination": {
                "pageSizes": [
                    10,
                    15,
                    20
                ],
                "show": true,
                "pageSize": 10
            },
            "thead": [
                {
                    "type": "index",
                    "label": "序号"
                },
                {
                    "prop": "colt_eval_id",
                    "label": "评估序号"
                },
                {
                    "label": "抵质押品编号",
                    "prop": "colt_no",
                    "align": "center"
                },
                {
                    "label": "评估类型",
                    "prop": "eval_type",
                    "align": "center",
                    "dict": {
                        "dictType": "CO_E_EVALTP",
                        "dictKey": [
                            "CO_E_EVALTP"
                        ],
                        "format": "value-label"
                    }
                },
                {
                    "label": "评估方法",
                    "prop": "colt_eval_meth",
                    "align": "center",
                    "dict": {
                        "dictType": "CO_E_COLT_EVALUTION_METH",
                        "dictKey": [
                            "CO_E_COLT_EVALUTION_METH"
                        ],
                        "format": "value-label"
                    }
                },
                {
                    "label": "评估价值",
                    "prop": "eval_val",
                    "align": "center"
                },
                {
                    "label": "外部评估机构编码",
                    "prop": "branch_id",
                    "align": "center"
                },
                {
                    "label": "内部人员编码",
                    "prop": "person_no",
                    "align": "center"
                },
                {
                    "label": "申请号",
                    "prop": "apply_no",
                    "align": "center"
                },
                {
                    "label": "状态",
                    "prop": "data_state",
                    "align": "center",
                    "dict": {
                        "dictType": "CO_E_COLT_EVALUTION_STATE",
                        "dictKey": [
                            "CO_E_COLT_EVALUTION_STATE"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "评估时间",
                    "prop": "eval_time",
                    "align": "center"
                },
                {
                    "label": "操作",
                    "width": "220px",
                    "align": "center",
                    "fixed": "right",
                    "btns": [
                        {
                            "label": "押品评估详情",
                            "type": "look",
                            "toRequest": {
                                "url": "@evaluateResult",
                                "method": "get",
                                "params": {
                                    "applid": "${{current.apply_no}}"
                                }
                            },
                            "props": {
                                "title": "押品估值详情",
                                "width": "80%"
                            }
                        }
                    ]
                }
            ]
        }
    }
}
