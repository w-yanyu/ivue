{
  "module": "v-realonlyForm-btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/call/RPCCall",
        "method": "post",
        "params": {
          "servicecode": "co4016",
          "appl_id": "${{params.applid}}"
        }
      },
      "initDataOnce": true,
      "controls": [
        {
          "house_id": {
            "control": "text",
            "label": "楼盘编号",
            "closable": true,
            "disabled": true,
            "width": "130px"
          },
          "house_name": {
            "control": "text",
            "label": "楼盘名称",
            "closable": true,
            "disabled": true,
            "width": "130px"
          },
          "property_msg": {
            "control": "text",
            "label": "物业信息",
            "closable": true,
            "disabled": true,
            "width": "130px"
          },
          "area_msg": {
            "control": "text",
            "label": "建筑面积",
            "width": "130px",
            "closable": true,
            "disabled": true
          },
          "avg_prise": {
            "control": "currency",
            "label": "均售价(元/平方米)",
            "width": "130px",
            "closable": true,
            "disabled": true
          },
          "house_addr": {
            "control": "text",
            "label": "楼盘地址",
            "width": "130px",
            "closable": true,
            "disabled": true
          },
          "presell_cert": {
            "control": "text",
            "label": "预售证",
            "width": "130px",
            "closable": true,
            "disabled": true
          },
          "prop_cert": {
            "control": "text",
            "label": "房地产产权证",
            "width": "130px",
            "closable": true,
            "disabled": true
          },
          "house_developer": {
            "control": "text",
            "label": "开发商",
            "width": "130px",
            "closable": true,
            "disabled": true
          },
          "traffic_status": {
            "control": "textarea",
            "label": "交通情况",
            "width": "130px",
            "closable": true,
            "disabled": true,
            "span": 24,
            "autosize": {
              "minRows": 3,
              "maxRows": 5
            },
            "maxlength": 100,
            "showWordLimit": true
          },
          "life_serv_msg": {
            "control": "textarea",
            "label": "生活设施情况",
            "width": "130px",
            "closable": true,
            "disabled": true,
            "span": 24,
            "autosize": {
              "minRows": 3,
              "maxRows": 5
            },
            "maxlength": 100,
            "showWordLimit": true
          },
          "school_msg": {
            "control": "textarea",
            "label": "学校情况",
            "width": "130px",
            "closable": true,
            "disabled": true,
            "span": 24,
            "autosize": {
              "minRows": 3,
              "maxRows": 5
            },
            "maxlength": 100,
            "showWordLimit": true
          },
          "hospital_msg": {
            "control": "textarea",
            "label": "就医情况",
            "width": "130px",
            "closable": true,
            "disabled": true,
            "span": 24,
            "autosize": {
              "minRows": 3,
              "maxRows": 5
            },
            "maxlength": 100,
            "showWordLimit": true
          }
        }
      ]
    }
  }
}
