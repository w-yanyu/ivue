{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "house_id": {
            "control": "text",
            "label": "楼盘编号",
            "rules": [
              {
                "max": 50,
                "message": "长度不能超过30个字符"
              }
            ]
          },
          "house_name": {
            "control": "text",
            "label": "楼盘名称",
            "rules": [
              {
                "max": 50,
                "message": "长度不能超过30个字符"
              }
            ]
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "co4016",
          "page_id": "/views/cols/colsHouseMsg/colsHouseMsg_see/colsHouseMsg_see.json"
        }
      },
      "thead": [
        {
          "label": "序号",
          "type" :  "index"
        },
        {
          "label": "楼盘编号",
          "prop": "house_id",
          "align":"center"
        },
        {
          "label": "楼盘名称",
          "prop": "house_name",
          "align":"center"
        },
        {
          "label":"物业信息",
          "prop": "property_msg",
          "align":"center"
        },
        {
          "label": "建筑面积信息",
          "prop": "area_msg",
          "align":"center"
        },
        {
          "label": "均售价(元/平米)",
          "prop": "avg_prise",
          "align":"center"
        },
        {
          "label": "楼盘地址",
          "prop": "house_addr",
          "align":"center"
        },
        {
          "label": "预售证",
          "prop": "presell_cert",
          "align":"center"
        },
        {
          "label": "房地产产权证",
          "prop": "prop_cert",
          "align":"center"
        },
        {
          "label": "操作",
          "fixed": "right",
          "width": 200,
          "align":"center",
          "btns": [
            {
              "type": "look",
              "label": "详情",
              "toRequest": {
                "url": "/views/co/colsHouseMsg/colsHouseMsg_see/colsHouseMsg_detail.json",
                "method": "POST"
              },
              "props": {
                "width": "80%",
                "title": "楼盘信息详细"
              }
            },
            {
              "type": "look",
              "label": "审核记录",
              "toRequest": {
                "url": "",
                "method": "POST"
              },
              "props": {
                "width": "65%",
                "title": "审核记录"
              }
            }
          ]
        }
      ]
    }
  }
}
