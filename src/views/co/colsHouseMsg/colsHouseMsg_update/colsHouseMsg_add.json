{
    "module": "v-inputDataForm-btn",
    "layout": {
        "form": {
            "scope": "formData",
            "btns": [],
            "controls": [
                {

                    "is_start": {
                        "control": "hidden",
                        "label": "是否启动流程",
                        "value":"1"
                    },
                    "house_name": {
                        "control": "text",
                        "label": "楼盘名称",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入楼盘名称"
                            },
                            {
                                "max": 20,
                                "message": "长度不能超过20个字符"
                            }
                        ]
                    },
                    "property_msg": {
                        "control": "text",
                        "label": "物业信息",
                        "width": "130px",
                        "rules": [
                            {
                                "max": 20,
                                "message": "长度不能超过20个字符"
                            }
                        ]
                    },
                    "area_msg": {
                        "control": "text",
                        "label": "建筑面积",
                        "width": "130px",
                        "rules": [
                            {
                                "max": 20,
                                "message": "长度不能超过20个字符"
                            }
                        ]
                    },
                    "avg_prise": {
                        "control": "currency",
                        "label": "均售价(元/平方米)",
                        "width": "130px",
                        "thousand": ","
                    },
                    "house_addr": {
                        "control": "text",
                        "label": "楼盘地址",
                        "width": "130px",
                        "rules": [
                            {
                                "max": 20,
                                "message": "长度不能超过20个字符"
                            }
                        ]
                    },
                    "presell_cert": {
                        "control": "text",
                        "label": "预售证",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入预售证"
                            },
                            {
                                "max": 20,
                                "message": "长度不能超过20个字符"
                            }
                        ]
                    },
                    "prop_cert": {
                        "control": "text",
                        "label": "房地产产权证",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入房地产产权证"
                            },
                            {
                                "max": 30,
                                "message": "长度不能超过30个字符"
                            }
                        ]
                    },
                    "house_developer": {
                        "control": "text",
                        "label": "开发商",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入开发商"
                            },
                            {
                                "max": 20,
                                "message": "长度不能超过20个字符"
                            }
                        ]
                    },
                    "traffic_status": {
                        "control": "textarea",
                        "label": "交通情况",
                        "width": "130px",
                        "placeholder": "(100个字以内,若没有请填“无”)",
                        "span": 24,
                        "autosize": {
                            "minRows": 3,
                            "maxRows": 5
                        },
                        "maxlength": 100,
                        "showWordLimit": true
                    },
                    "life_serv_msg": {
                        "control": "textarea",
                        "label": "生活设施情况",
                        "width": "130px",
                        "placeholder": "(100个字以内,若没有请填“无”)",
                        "span": 24,
                        "autosize": {
                            "minRows": 3,
                            "maxRows": 5
                        },
                        "maxlength": 100,
                        "showWordLimit": true
                    },
                    "school_msg": {
                        "control": "textarea",
                        "label": "学校情况",
                        "width": "130px",
                        "placeholder": "(100个字以内,若没有请填“无”)",
                        "span": 24,
                        "autosize": {
                            "minRows": 3,
                            "maxRows": 5
                        },
                        "maxlength": 100,
                        "showWordLimit": true
                    },
                    "hospital_msg": {
                        "control": "textarea",
                        "label": "就医情况",
                        "width": "130px",
                        "placeholder": "(100个字以内,若没有请填“无”)",
                        "span": 24,
                        "autosize": {
                            "minRows": 3,
                            "maxRows": 5
                        },
                        "maxlength": 100,
                        "showWordLimit": true
                    }


                },
                {
                  "toDbsx": {
                    "span": 24,
                    "control": "dynamic",
                    "#isShowChildrenForm": false,
                    "toRequest": {
                      "method": "GET",
                      "url": "@FlowSubmitBtn",
                      "params": {
                        "appId": "coOnl",
                        "servicecode": "co4015",
                        "opera_type": "1",
                        "is_start": "1",
                        "finalUrl": "@CommWaitToDoDeal"
                      }
                    }
                  }
                }

            ]
        }
    }
}
