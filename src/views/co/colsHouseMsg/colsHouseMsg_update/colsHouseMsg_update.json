{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "house_id": {
            "control": "text",
            "label": "楼盘编号",
            "rules": [
              {
                "max": 50,
                "message": "长度不能超过50个字符"
              }
            ]
          },
          "house_name": {
            "control": "text",
            "label": "楼盘名称",
            "rules": [
              {
                "max": 50,
                "message": "长度不能超过50个字符"
              }
            ]
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "target": "dialog",
          "label": "新增",
          "toRequest": {
            "method": "POST",
            "url": "/views/co/colsHouseMsg/colsHouseMsg_update/colsHouseMsg_add.json",
            "params": {}
          },
          "props": {
            "title": "楼盘信息新增",
            "width": "1100px"
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "co4016",
          "page_id": "/views/co/colsHouseMsg/colsHouseMsg_update/colsHouseMsg_update.json"
        }
      },
      "thead": [
        {
          "label": "序号",
          "type": "index"
        },
        {
          "label": "楼盘编号",
          "prop": "house_id",
          "align": "center"
        },
        {
          "label": "楼盘名称",
          "prop": "house_name",
          "align": "center"
        },
        {
          "label": "物业信息",
          "prop": "property_msg",
          "align": "center"
        },
        {
          "label": "建筑面积信息",
          "prop": "area_msg",
          "align": "center"
        },
        {
          "label": "均售价(元/平米)",
          "prop": "avg_prise",
          "align": "center"
        },
        {
          "label": "楼盘地址",
          "prop": "house_addr",
          "align": "center"
        },
        {
          "label": "预售证",
          "prop": "presell_cert",
          "align": "center"
        },
        {
          "label": "开发商",
          "prop": "house_developer",
          "align": "center"
        },
        {
          "label": "状态",
          "prop": "data_state",
          "align": "center",
          "dict": {
            "dictType": "CO_E_COLT_EVALUTION_STATE",
            "format": "label",
            "dictKey": [
                "CO_E_COLT_EVALUTION_STATE"
            ]
          }
        },

        {
          "label": "操作",
          "fixed": "right",
          "width": 200,
          "align": "center",
          "btns": [
            {
              "type": "look",
              "label": "维护",
              "toRequest": {
                "url": "/views/co/colsHouseMsg/colsHouseMsg_update/colsHouseMsg_edit_start.json",
                "method": "POST"
              },
              "props": {
                "title": "楼盘信息维护"
              },
              "show":{
               "prop":"data_state",
               "is":["2"]
              }
            },
            {
              "type": "look",
              "label": "商户关联",
              "toRequest": {
                "url": "/views/co/colsHouseMsg/colsHouseMsg_update/add/bussiTemplateMenu.json",
                "method": "POST"
              },
              "props": {
                "title": "商户关联"
              }
            },
            {
              "type": "edit",
              "label":  "流程启动",
              "show": {
                "expression": "'${{current.data_state}}' === '4'"
              },
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "co5003",
                  "change_num": "${{current.house_id}}"

                }
              }
            }

          ]
        }
        
      ]
    }
  }
}
