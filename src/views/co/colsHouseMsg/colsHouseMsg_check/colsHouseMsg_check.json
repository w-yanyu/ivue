{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "house_id": {
            "control": "text",
            "label": "楼盘编号",
            "rules": [
              {
                "max": 50,
                "message": "长度不能超过10个字符"
              }
            ]
          },
          "house_name": {
            "control": "text",
            "label": "楼盘名称",
            "rules": [
              {
                "max": 50,
                "message": "长度不能超过100个字符"
              }
            ]
          }  
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "co4016",
          "page_id": "/views/cols/colsHouseMsg/colsHouseMsg_check/colsHouseMsg_check.json"
        }
      },
      "thead": [
        {
          "label": "序号",
          "type" :  "index"
        },
        {
          "label": "楼盘编号",
          "prop": "house_id",
          "align":"center"
        },
        {
          "label": "楼盘名称",
          "prop": "house_name",
          "align":"center"
        },
        {
          "label":"物业信息",
          "prop": "pm_msg",
          "align":"center"
        },
        {
          "label": "建筑面积信息",
          "prop": "area_msg",
          "align":"center"
        },
        {
          "label": "均售价(元/平米)",
          "prop": "avg_prise",
          "align":"center"
        },
        {
          "label": "楼盘地址",
          "prop": "house_adr",
          "align":"center"
        },
        {
          "label": "预售证",
          "prop": "sell_certify",
          "align":"center"
        },
        {
          "label": "房地产产权证",
          "prop": "prop_certify",
          "align":"center"
        },
        {
          "label": "失效日期",
          "prop": "exprdt",
          "align":"center"
        },
        {
          "label": "创建操作员",
          "prop": "tranus",
          "align":"center"
        },
        {
          "label": "创建日期",
          "prop": "gmt_create",
          "align":"center"
        },
        {
          "label": "操作",
          "fixed": "right",
          "width": 200,
          "align":"center",
          "btns": [
            {
              "type": "look",
              "label": "审核记录",
              "toRequest": {
                "url": "",
                "method": "POST"
              },
              "props": {
                "width": "65%",
                "title": "审核记录"
              }
            },
            {
              "type": "edit",
              "label": "审核",

              "toRequest": {
                "url": "",
                "method": "POST"

              },
              "props": {
                "title": "楼盘信息审核"
              }
            }

          ]
        }
      ]
    }
  }
}
