{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "bpm1015",
          "task_count":"${{form.task_count}}",
          "user_id": "$g{{user_info.userCode}}",
          "appId":"${{params.appId}}"
        }
      },
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "bpm1015",
              "task_count":"${{form.task_count}}",
              "user_id": "$g{{user_info.userCode}}",
              "appId":"${{params.appId}}"
            }
          }
        }
      ],
      "controls": [
        {
          "task_count": {
            "control": "inputNumber",
            "label": "自动认领笔数",
            "width": "160px",
            "span": 18,
            "min": 1,
            "value": 1
          }
        }
      ]
    }
  }
}