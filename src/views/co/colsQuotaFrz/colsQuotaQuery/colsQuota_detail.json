{
    "module": "v-realonlyForm-btn",
    "layout": {
        "form": {
            "initRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "post",
                "params": {
                    "servicecode": "co4062",
                    "apply_no": "${{params.apply_no}}",
                    "service_contract_no": "${{params.service_contract_no}}",
                    "sec_no": "${{params.sec_no}}"
                }
            },
      "controls": [
        {
            "ln_catg": {
                "control": "select",
                "label": "业务类型",
                "closable": true,
                "disabled": true,
                "width": "130px",
              "dict": {
                "dictType": "BPM_E_BUSI_TYPE",
                "dictKey": [
                  "BPM_E_BUSI_TYPE"
                ],
                "format": "label"
              }
            },
            "ln_prod_cd": {
                "control": "text",
                "label": "贷款产品编号",
                "closable": true,
                "disabled": true,
                "width": "130px"
            },
            "guar_type": {
                "control": "select",
                "label": "担保方式",
                "width": "130px",
                "closable": true,
                "disabled": true,
                "dict": {
                    "dictType": "MS_E_GUAR_TYPE",
                    "dictKey": [
                      "MS_E_GUAR_TYPE"
                    ],
                    "format": "label"
                  }
            },
            "sec_rule": {
                "control": "select",
                "label": "抵质押规则",
                "width": "130px",
                "closable": true,
                "disabled": true,
                "dict": {
                    "dictType": "CO_E_SECRULE",
                    "dictKey": [
                      "CO_E_SECRULE"
                    ],
                    "format": "label"
                  }
            },
            "sec_contract_amt": {
                "control": "currency",
                "label": "担保合同金额",
                "width": "130px",
                "closable": true,
                "disabled": true
            },
            "sec_occupy_amt": {
                "control": "currency",
                "label": "使用金额",
                "width": "130px",
                "closable": true,
                "disabled": true
            },
            "ccy_code": {
                "control": "select",
                "label": "币种",
                "width": "130px",
                "closable": true,
                "disabled": true,
                "dict": {
                    "dictType": "MS_CURRENCY",
                    "dictKey": [
                      "MS_CURRENCY"
                    ],
                    "format": "label"
                  }
            },
            "base_ccy_cd": {
                "control": "select",
                "label": "基准币种",
                "width": "130px",
                "closable": true,
                "disabled": true,
                "dict": {
                    "dictType": "MS_CURRENCY",
                    "dictKey": [
                      "MS_CURRENCY"
                    ],
                    "format": "label"
                  }
            },
            "base_amt": {
                "control": "currency",
                "label": "基准金额",
                "width": "130px",
                "closable": true,
                "disabled": true
            },
            "sec_state": {
                "control": "select",
                "label": "押品关联合同状态",
                "width": "130px",
                "closable": true,
                "disabled": true,
                "dict": {
                    "dictType": "CO_E_SECSTATE",
                    "dictKey": [
                      "CO_E_SECSTATE"
                    ],
                    "format": "label"
                  }
            }


        }

    ]
        }
    }
}


