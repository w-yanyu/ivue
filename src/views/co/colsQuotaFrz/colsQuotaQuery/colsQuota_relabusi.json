{
    "module": "v_search_btn_datagrid",
    "layout": {
        "datagrid": {
            "isInitSearch": true,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "post",
                "params": {
                    "servicecode": "co4061",
                    "apply_no": "${{params.apply_no}}"
                }
            },
            "pagination": {
                "pageSizes": [10, 15, 20],
                "pageSize": 10
            },
            "thead": [{
                    "type": "index",
                    "align": "center"
                },
                {
                    "label": "业务申请编号",
                    "prop": "apply_no",
                    "align": "center",
                    "type": "leaf"
                },
                {
                    "label": "业务类型",
                    "prop": "ln_catg",
                    "align": "center",
                    "type": "leaf",
                  "dict": {
                    "dictType": "BPM_E_BUSI_TYPE",
                    "dictKey": [
                      "BPM_E_BUSI_TYPE"
                    ],
                    "format": "label"
                  }
                },
                {
                    "label": "业务合同号",
                    "prop": "service_contract_no",
                    "align": "center",
                    "type": "leaf"
                },
                {
                    "label": "担保合同号",
                    "prop": "sec_no",
                    "align": "center",
                    "type": "leaf"
                },
                {
                    "label": "担保方式",
                    "prop": "guar_type",
                    "align": "center",
                    "type": "leaf",
                    "dict": {
                        "dictType": "MS_E_GUAR_TYPE",
                        "dictKey": [
                          "MS_E_GUAR_TYPE"
                        ],
                        "format": "label"
                      }
                },
                {
                    "label": "担保合同金额",
                    "prop": "sec_contract_amt",
                    "align": "center",
                    "type": "leaf"
                },
                {
                    "label": "操作",
                    "width": "150px",
                    "align": "center",
                    "fixed": "right",
                    "btns": [{
                            "type": "edit",
                            "label": "详情",
                            "toRequest": {
                                "url": "/views/co/colsQuotaFrz/colsQuotaQuery/colsQuota_detail.json",
                                "method": "post",
                                "params": {
                                }
                            },
                            "props": {
                                "title": "详情",
                                "width": "80%"
                            }
                        }
                    ]
                }
            ]
        }
    }
}
