{
    "module": "v_search_btn_datagrid",
    "layout": {
        "datagrid": {
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "post",
                "params": {
                    "servicecode": "co4060",
                    "colt_no": "${{params.colt_no}}"
                }
            },
            "pagination": {
                "pageSizes": [10, 15, 20],
                "pageSize": 10
            },
            "thead": [{
                    "type": "index",
                    "align": "center"
                },
                {
                    "label": "业务申请编号",
                    "prop": "apply_no",
                    "align": "center"
                },
                {
                    "label": "担保合同号",
                    "prop": "sec_no",
                    "align": "center",
                    "type": "leaf"
                },
                {
                    "label": "押品关联合同状态",
                    "prop": "sec_state",
                    "align": "center",
                    "dict": {
                        "dictType": "CO_E_SECSTATE",
                        "dictKey": [
                          "CO_E_SECSTATE"
                        ],
                        "format": "label"
                      }
                },
                {
                    "label": "操作",
                    "width": "150px",
                    "align": "center",
                    "fixed": "right",
                    "btns": [{
                            "type": "edit",
                            "label": "详情",
                            "toRequest": {
                                "url": "/views/co/colsQuotaFrz/colsQuotaQuery/colsQuota_relabusi.json",
                                "method": "get",
                                "params": {
                                }
                            },
                            "props": {
                                "title": "详情",
                                "width": "80%"
                            }
                        }
                    ]
                }
            ]
        }
    }
}
