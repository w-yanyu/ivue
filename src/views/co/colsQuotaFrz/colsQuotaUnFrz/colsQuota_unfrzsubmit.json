{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest":"${{params}}",
            "initDataOnce":true,
            "btns": [
                {
                    "type": "submit",
                    "label": "解冻审批提交",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "co4013",
                            "trap": "UPDATE",
                            "colt_no": "${{params.colt_no}}"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "cletal_value": {
                        "control": "currency",
                        "label": "押品总价值",
                        "width": "130px",
                        "closable": true,
                        "disabled": true
                    },
                    "cletal_occupy": {
                        "control": "currency",
                        "label": "已被关联业务占用额度",
                        "width": "130px",
                        "closable": true,
                        "disabled": true
                    },
                    "cletal_remain": {
                        "control": "currency",
                        "label": "剩余额度",
                        "width": "130px",
                        "closable": true,
                        "disabled": true
                    },
                    "fr_quota": {
                        "control": "currency",
                        "label": "已被冻结金额",
                        "width": "130px",
                        "closable": true,
                        "disabled": true
                    },
                    "unfr_quota": {
                        "control": "currency",
                        "label": "解冻额度",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入解冻额度"
                            }
                        ]
                    }
                }
                
            ]
        }
    }
}