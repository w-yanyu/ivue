{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "colt_no": {
            "control": "text",
            "label": "抵质押品编号",
            "rules": [
              {
                "max": 19,
                "message": "长度不能超过19个字符"
              }
            ]
          } 
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "co4014",
          "page_id": "/views/cols/colsQuotaFrz/colsQuotaFrzsel/colsQuota_frzsel.json"
        }
      },
      "thead": [
        {
          "label": "序号",
          "type" :  "index"
        },
        {
          "label": "抵质押品编号",
          "prop": "colt_no",
          "align":"center"
        },
        {
          "label": "抵押品名称",
          "prop": "colt_name",
          "align":"center"
        },
        {
          "label":"操作人",
          "prop": "cl_peo",
          "align":"center"
        },
        {
          "label": "操作金额",
          "prop": "cl_mn",
          "align":"center"
        },
        {
          "label": "操作结果",
          "prop": "cl_result",
          "align":"center"
        } ,
        {
          "label": "操作时间",
          "prop": "cl_dt",
          "align":"center"
        }
                          
      ]
    }
  }
}
