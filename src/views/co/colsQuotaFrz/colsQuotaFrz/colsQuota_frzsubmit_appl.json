{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "post",
                "params": {
                    "servicecode": "co4013",
                    "appl_id": "${{params.applid}}"
                }
            },
            "initDataOnce": true,
            "btns": [
                {}
            ],
            "name": "detail",
            "controls": [
                {
                    "limit_no": {
                        "control": "text",
                        "label": "额度编号",
                        "width": "130px",
                        "closable": true,
                        "disabled": true
                    },
                    "colt_no": {
                        "control": "text",
                        "label": "抵质押编号",
                        "width": "130px",
                        "closable": true,
                        "disabled": true
                    },
                    "colt_total_val": {
                        "control": "text",
                        "label": "押品总价值",
                        "width": "130px",
                        "closable": true,
                        "disabled": true
                    },
                    "cletal_remain": {
                        "control": "text",
                        "label": "可用额度",
                        "width": "130px",
                        "closable": true,
                        "disabled": true
                    },
                    "freeze_quota": {
                        "control": "text",
                        "label": "已冻结额度",
                        "width": "130px",
                        "closable": true,
                        "disabled": true
                    },
                    "pre_freeze_quota": {
                        "control": "text",
                        "label": "预冻结额度",
                        "width": "130px",
                        "closable": true,
                        "disabled": true
                    },
                    "quota_op_type": {
                        "control": "select",
                        "label": "额度交易类型",
                        "width": "130px",
                        "closable": true,
                        "disabled": true,
                        "dict": {
                            "dictType": "CO_E_QUOTAOPTYPE",
                            "dictKey": [
                                "CO_E_QUOTAOPTYPE"
                            ],
                            "format": "label"
                        },
                        "rules": [
                          {
                            "required": true,
                            "message": "请选择额度交易类型"
                          }
                        ]
                    },
                    "trxn_amt": {
                        "control": "text",
                        "label": "交易金额",
                        "width": "130px",
                        "closable": true,
                        "disabled": true
                    }
                },
                {
                    "toDbsx": {
                        "span": 24,
                        "control": "dynamic",
                        "#isShowChildrenForm": false,
                        "toRequest": {
                            "method": "GET"
                        }
                    }
                }
            ]
        }
    }
}
