{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "initDataOnce": true,
            "btns": [
                {}
            ],
            "controls": [
                {
                    "apply_no": {
                        "control": "text",
                        "label": "申请书编号",
                        "width": "130px",
                        "closable": true,
                        "disabled": true
                    },
                    "trxn_seq": {
                        "control": "text",
                        "label": "交易流水",
                        "width": "130px",
                        "closable": true,
                        "disabled": true
                    },
                    "limit_no": {
                        "control": "text",
                        "label": "额度编号",
                        "width": "130px",
                        "closable": true,
                        "disabled": true
                    },
                    "colt_no": {
                        "control": "text",
                        "label": "抵质押编号",
                        "width": "130px",
                        "closable": true,
                        "disabled": true
                    },
                    "colt_total_val": {
                        "control": "text",
                        "label": "押品总价值",
                        "width": "130px",
                        "closable": true,
                        "disabled": true
                    },
                    "remain_limit": {
                        "control": "text",
                        "label": "可用额度",
                        "width": "130px",
                        "closable": true,
                        "disabled": true
                    },
                    "freeze_quota": {
                        "control": "text",
                        "label": "已冻结额度",
                        "width": "130px",
                        "closable": true,
                        "disabled": true
                    },
                    "pre_freeze_quota": {
                        "control": "text",
                        "label": "预冻结额度",
                        "width": "130px",
                        "closable": true,
                        "disabled": true
                    },
                    "quota_op_type": {
                        "control": "select",
                        "label": "额度交易类型",
                        "width": "130px",
                        "closable": true,
                        "disabled": true,
                        "dict": {
                            "dictType": "CO_E_QUOTAOPTYPE",
                            "dictKey": [
                                "CO_E_QUOTAOPTYPE"
                            ],
                            "format": "label"
                        }
                    },
                    "trxn_amt": {
                        "control": "text",
                        "label": "交易金额",
                        "width": "130px",
                        "closable": true,
                        "disabled": true
                    }
                },
                {
                    "toDbsx": {
                        "span": 24,
                        "control": "dynamic",
                        "#isShowChildrenForm": false,
                        "toRequest": {
                            "method": "GET"
                        }
                    }
                }
            ]
        }
    }
}
