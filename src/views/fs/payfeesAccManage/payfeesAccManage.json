{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "controls": [
                {
                    "branch": {
                        "control": "remoteSelect",
                        "label": "区域中心",
                        "span": 8,
                        "default": "004100",
                        "labelField": "brachName",
                        "valueField": "branchNo",
                        "doRequest": {
                            "url": "/SUMP/icmcall/icmRPCCall",
                            "method": "POST",
                            "params": {
                                "servicecode": "RLMSPLTS1068"
                            }
                        },
                        "format": "label"
                    },
                    "accountType": {
                        "control": "select",
                        "span": 8,
                        "label": "账户类型",
                        "default": "0",
                        "dict": {
                            "dictType": "E_ACCOUNT_TYPE",
                            "dictKey": [
                                "E_ACCOUNT_TYPE"
                            ],
                            "format": "label"
                        }
                    },
                    "orgName": {
                        "control": "text",
                        "span": 8,
                        "label": "机构名称",
                        "rules": [
                            {
                                "max": 100,
                                "message": "长度不能超过100个字符"
                            }
                        ]
                    }
                },
                {
                    "receAccName": {
                        "control": "text",
                        "span": 8,
                        "label": "收款账户名称",
                        "rules": [
                            {
                                "max": 100,
                                "message": "长度不能超过100个字符"
                            }
                        ]
                    },
                    "receAccNo": {
                        "control": "text",
                        "span": 8,
                        "label": "收款账户账号",
                        "rules": [
                            {
                                "max": 100,
                                "message": "长度不能超过100个字符"
                            }
                        ]
                    }
                }
            ]
        },
        "toolbar": {
            "btns": [
                {
                  "type": "add",
                  "target": "dialog",
                  "label": "新增",
                  "toRequest": {
                    "method": "GET",
                    "url": "/views/fs/payfeesAccManage/addpayfeesManage.json",
                    "params": {}
                  },
                  "props": {
                    "title": "缴费账户管理新增"
                  }
                }
              ]
        },
        "datagrid": {
            "isInitSearch": true,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "RLMSPLTS1064"
                }
            },
            "thead": [
                {
                    "type": "index",
                    "label": "#",
                    "align": "center"
                },
                {
                    "type": "string",
                    "label": "区域中心",
                    "prop": "branch_name",
                    "align": "center"
                },
                {
                    "type": "string",
                    "label": "账户类型",
                    "prop": "accountType",
                    "align": "center",
                    "dict": {
                        "dictType": "E_ACCOUNT_TYPE",
                        "dictKey": [
                            "E_ACCOUNT_TYPE"
                        ],
                        "format": "label"
                    }
                },
                {
                    "type": "string",
                    "label": "机构名称",
                    "prop": "orgName",
                    "align": "center"
                },
                {
                    "type": "string",
                    "label": "收款账户名称",
                    "prop": "receAccName",
                    "align": "center"
                },
                {
                    "type": "string",
                    "label": "收款账户账号",
                    "prop": "receAccNo",
                    "align": "center"
                },
                {
                    "type": "string",
                    "label": "开户行",
                    "prop": "bank_name",
                    "align": "center"
                },
                {
                    "type": "string",
                    "label": "开户行行号",
                    "prop": "bank_no",
                    "align": "center"
                },
                {
                    "label": "操作",
                    "fixed": "right",
                    "align": "center",
                    "btns": [
                        {
                            "type": "edit",
                            "label": "维护",
                            "toRequest": {
                                "url": "/views/fs/payfeesAccManage/editpayfeesAccManage.json",
                                "params": {
                                    "branch": "${{current.branch}}",
                                    "accountType": "${{current.accountType}}"
                                }
                            },
                            "props": {
                                "title": "缴费账户管理维护"
                            }
                        },
                        {
                            "type": "delete",
                            "label": "删除",
                            "doRequest": {
                              "url": "/SUMP/icmcall/icmRPCCall",
                              "method": "POST",
                              "params": {
                                "servicecode": "RLMSPLTS1076"
                              }
                            }
                        }
                    ]
                }
            ]
        }
    }
}