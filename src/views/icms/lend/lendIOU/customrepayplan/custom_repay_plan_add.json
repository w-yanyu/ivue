{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "ar0620",
          "apply_no": "${{params.apply_no}}",
          "loan_no": "${{params.loan_no}}"
        }
      },
      "btns": [
        {
          "type": "submit",
          "label": "提交"
        }
      ],
      "controls": [
        {
        "matu_date": {
          "control": "dateTimePicker",
          "label": "到期日期",
          "span":8,
          "width": "200px",
          "format": "yyyyMMdd",
          "rules": [
            {
              "required": true,
              "message": "请选择到期日期"
            }
          ]
        },
        "prcpl_rpymt_amt": {
          "control": "currency",
          "label": "还本金额(元)",
          "span": 8,
          "width": "200px",
          "disabled": false,
          "rules": [
            {
              "required": true,
              "message": "请填写"
            }
          ]
          
        },
        "prim_key": {
          "control": "hidden",
          "label": "主键",
          "span": 8,
          "width": "200px",
          "value":"${{params.prim_key}}"
        },
        "apply_no": {
          "control": "hidden",
          "label": "申请号",
          "span": 12,
          "value":"${{params.apply_no}}"
        }
      }

      ]
    }
  }
}
