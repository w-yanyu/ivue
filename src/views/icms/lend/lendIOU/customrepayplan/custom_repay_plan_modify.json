{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "ar0621",
          "apply_no": "${{params.apply_no}}",
          "prim_key": "${{params.prim_key}}",
          "loan_no": "${{params.loan_no}}",
          "current_period_num": "${{params.current_period_num}}",
          "start_date": "${{params.start_date}}"
          
        }
      },
      "btns": [
        {
          "type": "submit",
          "label": "提交"
        }
      ],
      "controls": [
        {
        "matu_date": {
          "control": "dateTimePicker",
          "label": "到期日期",
          "span":8,
          "width": "200px",
          "format": "yyyyMMdd"
        },
        "prcpl_rpymt_amt": {
          "control": "currency",
          "label": "还本金额(元)",
          "span": 8,
          "width": "200px",
          "disabled": false
        },
        "rsdl_prcpl": {
          "control": "currency",
          "label": "剩余本金(元)",
          "span": 8,
          "width": "200px",
          "disabled": true
        },
        "prim_key": {
          "control": "hidden",
          "label": "主键",
          "span": 8,
          "width": "200px",
          "value":"${{params.prim_key}}"
        },
        "apply_no": {
          "control": "hidden",
          "label": "申请号",
          "span": 12,
          "value":"${{params.apply_no}}"
        }
      }

      ]
    }
  }
}
