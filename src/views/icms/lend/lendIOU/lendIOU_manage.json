{
  "module": "v_search_btn_datagrid",
  "layout": {
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "target": "dialog",
          "label": "新增",
          "toRequest": {
            "method": "GET",
            "url": "@LendIOUTab",
            "#target": "navTab",
            "params": {
              "pageType": "add",
              "lend_no": "${{current.lend_no}}",
              "apply_no": "${{params.apply_no}}"
            }
          },
          "props": {
            "width": "90%",
            "title": "贷款借据新增"
          }
        }
      ]
    },
    "datagrid": {
      "doRequest": {
        "url": "SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ar0544",
          "apply_no": "${{params.apply_no}}"
        }
      },
      "thead": [
        {
          "label": "放款序号",
          "align": "center",
          "prop": "lend_no"
        },
        {
          "label": "还款方式",
          "prop": "repay_type",
          "align": "center",
          "dict": {
            "dictType": "MS_E_REPAY_WAY",
            "dictKey": [
              "MS_E_REPAY_WAY"
            ],
            "labelField": "dictName",
            "valueField": "dictId"
          }
        },
        {
          "label": "分期期数",
          "align": "center",
          "prop": "installment_num"
        },
        {
          "label": "期限(月)",
          "align": "center",
          "prop": "periods"
        },
        {
          "label": "放款金额(元)",
          "align": "center",
          "prop": "lend_amt",
          "format":{
            "thousand":","
          }
        },
        {
          "label": "借据号",
          "align": "center",
          "prop": "loan_no"
        },
        {
          "label": "操作",
          "align": "center",
          "width": "200px",
          "btns": [
            {
              "type": "edit",
              "label": "维护",
              "target": "dialog",
              "toRequest": {
                "url": "@LendIOUTab",
                "method": "GET",
                "#target": "navTab",
                "params": {
                  "pageType": "edit",
                  "lend_no": "${{current.lend_no}}",
                  "apply_no": "${{params.apply_no}}"
                }
              },
              "props": {
                "width": "90%",
                "title": "贷款借据维护"
              }
            },
            {
              "type": "delete",
              "label": "删除",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "post",
                "params": {
                  "servicecode": "ar0543",
                  "lend_no": "${{current.lend_no}}",
                  "apply_no": "${{params.apply_no}}"
                }
              }
            },
            {
              "type": "look",
              "label": "详情",
              "toRequest": {
                "url": "@LendIOUTab",
                "method": "GET",
                "params": {
                  "pageType": "info",
                  "lend_no": "${{current.lend_no}}",
                  "apply_no": "${{params.apply_no}}"
                }
              },
              "props": {
                "width": "90%",
                "title": "贷款借据详情"
              }
            }
          ]
        }
      ]
    }
  }
}
