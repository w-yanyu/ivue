{
  "module": "v_realonlyForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "controls": [
        {
          "is_xfer": {
            "control": "select",
            "label": "是否转第三方",
            "span": 12,
            "disabled": true,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": false,
                "message": "必填项"
              }
            ]
          },
          "lend_amt": {
            "control": "currency",
            "label": "放款金额",
            "span": 12,
            "align": "left",
            "disabled": true,
            "rules": [
              {
                "required": false,
                "message": "必填项"
              }
            ],
            "format":{
              "thousand":","
            }
          },"exchange_rate": {
            "control": "currency",
            "label": "汇率",
            "disabled": true,
            "span": 12,
            "rules": [
              {
                "required": false,
                "message": "必填项"
              }
            ]
          },"base_amt": {
            "control": "currency",
            "label": "基准金额",
            "disabled": true,
            "compute": "${{form.lend_amt}}*${{form.exchange_rate}}",
            "span": 12,
            "align": "left",
            "rules": [
              {
                "required": false,
                "message": "必填项"
              }
            ],
            "format":{
              "thousand":","
            }
          },
          "lend_acct_no": {
            "control": "text",
            "span": 12,
            "disabled": true,
            "label": "放款账户",
            "rules": [
              {
                "required": false,
                "message": "必填项"
              }
            ]
          },
          "loan_acct_name": {
            "control": "text",
            "label": "放款账户名",
            "span": 12,
            "disabled": true,
            "rules": [
              {
                "required": false,
                "message": "必填项"
              }
            ]
          },
          "branch_org": {
            "control": "text",
            "label": "开户分支机构",
            "span": 12,
            "disabled": true,
            "rules": [
              {
                "required": false,
                "message": "必填项"
              }
            ]
          },
          "ccy_code": {
            "control": "select",
            "span": 12,
            "disabled": true,
            "label": "币种",
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": [
                "MS_CURRENCY"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": false,
                "message": "必填项"
              }
            ],
            "value": "156"
          },"base_ccy": {
            "control": "select",
            "span": 12,
            "label": "基准币种",
            "disabled":true,
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": [
                "MS_CURRENCY"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": false,
                "message": "必填项"
              }
            ],
            "value": "156"
          },
          "acct_type": {
            "control": "select",
            "span": 12,
            "disabled": true,
            "label": "账号类型",
            "dict": {
              "dictType": "AR_E_ACCT_TYPE",
              "dictKey": [
                "AR_E_ACCT_TYPE"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": false,
                "message": "必填项"
              }
            ],
            "value": "1"
          },
          "money_exchange_flag": {
            "control": "select",
            "span": 12,
            "disabled": true,
            "label": "钞汇标志",
            "dict": {
              "dictType": "AR_E_ACCT_NTTT_FLAG",
              "dictKey": [
                "AR_E_ACCT_NTTT_FLAG"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": false,
                "message": "必填项"
              }
            ],
            "value": "1"
          }
        }
      ]
    }
  }
}
