{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "initDataOnce": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "ar0572",
          "apply_no": "${{params.apply_no}}",
          "lend_no": "${{params.lend_no}}"
        }
      },
      "btns": [
        {
          "type": "submit",
          "label": "提交"
        }
      ],
      "controls": [
        {
          "apply_no": {
            "control": "hidden",
            "label": "申请号"
          },
          "is_acct_other_bank": {
            "control": "select",
            "label": "是否他行帐户",
            "span": 12,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "必填项"
              }
            ]
          },
          "transfer_acct_amt": {
            "control": "currency",
            "span": 12,
            "align": "left",
            "label": "转帐金额",
            "rules": [
              {
                "required": true,
                "message": "必填项"
              }
            ],
            "format":{
              "thousand":","
            }
          },
          "lend_acct_no": {
            "control": "text",
            "span": 12,
            "label": "账户",
            "rules": [
              {
                "required": true,
                "message": "必填项"
              }
            ]
          },
          "loan_acct_name": {
            "control": "text",
            "label": "转账账户名",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "必填项"
              }
            ]
          },
          "branch_org": {
            "control": "text",
            "label": "开户分支机构",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "必填项"
              }
            ]
          },
          "open_acct_bank": {
            "control": "text",
            "label": "开户行",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "必填项"
              }
            ]
          },
          "ccy_code": {
            "control": "select",
            "span": 12,
            "label": "币种",
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": [
                "MS_CURRENCY"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "必填项"
              }
            ],
            "value": "156"
          },
          "acct_type": {
            "control": "select",
            "span": 12,
            "label": "账号类型",
            "dict": {
              "dictType": "AR_E_ACCT_TYPE",
              "dictKey": [
                "AR_E_ACCT_TYPE"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "必填项"
              }
            ],
            "value": "1"
          },
          "money_exchange_flag": {
            "control": "select",
            "span": 12,
            "label": "钞汇标志",
            "dict": {
              "dictType": "AR_E_ACCT_NTTT_FLAG",
              "dictKey": [
                "AR_E_ACCT_NTTT_FLAG"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "必填项"
              }
            ],
            "value": "1"
          }
        }
      ]
    }
  }
}
