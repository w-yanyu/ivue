{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/call/RPCCall",
        "method": "post",
        "params": {
          "servicecode": "ar0521",
          "lend_no": "${{params.lend_no}}"
        }
      },
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/call/RPCCall",
            "method": "post",
            "params": {
              "caozuolx":"1",
              "servicecode": "ar0522",
              "lend_no": "${{params.lend_no}}",
              "legal_nature":"${{form.legal_nature}}",
              "subj_raly":"${{form.subj_raly}}",
              "plate_by_type":"${{form.plate_by_type}}",
              "by_loan_prod_type":"${{form.by_loan_prod_type}}",
              "repay_fund":"${{form.repay_fund}}",
              "by_loan_invest":"${{form.by_loan_invest}}"
            }
          }
        }
      ],
      "controls": [
        {
          "lend_no": {
            "control": "hidden"
          },
          "legal_nature": {
            "control": "select",
            "label": "按法律性质",
            "span": 12,
            "dict": {
              "dictType": "AR_E_LEGAL_NATURE",
              "dictKey": [
                "AR_E_LEGAL_NATURE"
              ],
              "format": "value-label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择按法律性质"
              }
            ]
          },
          "subj_raly": {
            "control": "select",
            "label": "按隶属关系",
            "span": 12,
            "dict": {
              "dictType": "AR_E_ACCORDING_AFFILIATION",
              "dictKey": [
                "AR_E_ACCORDING_AFFILIATION"
              ],
              "format": "value-label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择按隶属关系"
              }
            ]
          }
        },
        {
          "plate_by_type": {
            "control": "select",
            "label": "按类型",
            "span": 12,
            "dict": {
              "dictType": "AR_E_PLATE_BY_TYPE",
              "dictKey": [
                "AR_E_PLATE_BY_TYPE"
              ],
              "format": "value-label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择按类型"
              }
            ]
          },
          "by_loan_prod_type": {
            "control": "select",
            "label": "按贷款品种",
            "dict": {
              "dictType": "AR_E_LOAN_PROD_TYPE",
              "dictKey": [
                "AR_E_LOAN_PROD_TYPE"
              ],
              "format": "value-label"
            },
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请选择按贷款品种"
              }
            ]
          }
        },
        {
          "repay_fund": {
            "control": "select",
            "label": "按偿还资金来源",
            "span": 12,
            "dict": {
              "dictType": "AR_E_REPAYMENT_FUND",
              "dictKey": [
                "AR_E_REPAYMENT_FUND"
              ],
              "format": "value-label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择按偿还资金来源"
              }
            ]
          },
          "by_loan_invest": {
            "control": "text",
            "label": "按贷款投向",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请输入贷款投向"
              }
            ]
          }
        },
        {
          "by_loan_invest_name": {
            "control": "text",
            "label": "按贷款方向",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请输入贷款方向名称"
              }
            ]
          },
          "by_loan_mode": {
            "control": "text",
            "label": "按贷款方式",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请输入贷款方式"
              }
            ]
          }
        },
        {
          "by_loan_name": {
            "control": "text",
            "label": "按贷款方式名称",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请输入贷款方式名称"
              }
            ]
          },
          "valid_state": {
            "control": "select",
            "label": "按偿还资金来源",
            "span": 12,
            "dict": {
              "dictType": "AR_E_STATE",
              "dictKey": [
                "AR_E_STATE"
              ],
              "format": "value-label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择按偿还资金来源"
              }
            ]
          }
        }
      ]
    }
  }
}
