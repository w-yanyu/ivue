{
  "module": "v_realonlyForm_btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/call/RPCCall",
        "method": "post",
        "params": {
          "servicecode": "ar0521",
          "lend_no": "${{params.lend_no}}"
        }
      },
      "controls": [
        {
          "legal_nature": {
            "control": "select",
            "label": "按法律性质",
            "span": 12,
            "disabled": true,
            "dict": {
              "dictType": "AR_E_LEGAL_NATURE",
              "dictKey": [
                "AR_E_LEGAL_NATURE"
              ],
              "format": "label"
            }
          },
          "subj_raly": {
            "control": "select",
            "label": "按隶属关系",
            "span": 12,
            "disabled": true,
            "dict": {
              "dictType": "AR_E_ACCORDING_AFFILIATION",
              "dictKey": [
                "AR_E_ACCORDING_AFFILIATION"
              ],
              "format": "value-label"
            }
          }
        },
        {
          "plate_by_type": {
            "control": "select",
            "label": "按类型",
            "disabled": true,
            "span": 12,
            "dict": {
              "dictType": "AR_E_PLATE_BY_TYPE",
              "dictKey": [
                "AR_E_PLATE_BY_TYPE"
              ],
              "format": "value-label"
            }
          },
          "by_loan_prod_type": {
            "control": "select",
            "label": "按贷款品种",
            "disabled": true,
            "dict": {
              "dictType": "AR_E_LOAN_PROD_TYPE",
              "dictKey": [
                "AR_E_LOAN_PROD_TYPE"
              ],
              "format": "value-label"
            },
            "span": 12
          }
        },
        {
          "repay_fund": {
            "control": "select",
            "label": "按偿还资金来源",
            "span": 12,
            "disabled": true,
            "dict": {
              "dictType": "AR_E_REPAYMENT_FUND",
              "dictKey": [
                "AR_E_REPAYMENT_FUND"
              ],
              "format": "value-label"
            }
          },
          "by_loan_invest": {
            "control": "select",
            "label": "按贷款投向",
            "disabled": true,
            "span": 12
          }
        },
        {
          "by_loan_invest_name": {
            "control": "text",
            "label": "按贷款方向",
            "disabled": true,
            "span": 12
          },
          "by_loan_mode": {
            "control": "text",
            "label": "按贷款方式",
            "disabled": true,
            "span": 12
          }
        },
        {
          "by_loan_name": {
            "control": "text",
            "label": "按贷款方式名称",
            "disabled": true,
            "span": 12
          },
          "valid_state": {
            "control": "select",
            "label": "按偿还资金来源",
            "disabled": true,
            "span": 12,
            "dict": {
              "dictType": "AR_E_STATE",
              "dictKey": [
                "AR_E_STATE"
              ],
              "format": "value-label"
            }
          }
        }
      ]
    }
  }
}
