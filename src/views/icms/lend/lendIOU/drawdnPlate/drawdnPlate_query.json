{
  "module": "v_search_btn_datagrid",
  "layout": {

    "toolbar": {
      "btns": [
        {
          "type": "add",
          "target": "dialog",
          "label": "新增",
          "toRequest": {
            "method": "POST",
            "url": "/views/icms/lend/lendIOU/drawdnPlate/drawdnPlate_add.json",
            "params": {
              "lend_no":"${{params.lend_no}}",
              "loan_no":"${{params.loan_no}}"
            }
          },
          "props": {
            "width": "65%",
            "title": "涉融资平台信息新增"
          },
          "show": {
            "expression": "'${{params.pageType}}' != 'info'"
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": true,
      "scope": "searchTable",
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ar0521",
          "lend_no":"${{params.lend_no}}"
        }
      },
      "thead": [
        {
          "label": "放款序号",
          "prop": "lend_no"
        },
        {
          "label": "按法律性质",
          "prop": "legal_nature",
          "dict": {
            "dictType": "AR_E_LEGAL_NATURE",
            "dictKey": [
              "AR_E_LEGAL_NATURE"
            ],
            "format": "label"
          }
        },
        {
          "label": "按隶属关系",
          "prop": "subj_raly",
          "dict": {
            "dictType": "AR_E_ACCORDING_AFFILIATION",
            "dictKey": [
              "AR_E_ACCORDING_AFFILIATION"
            ],
            "format": "label"
          }
        },
        {
          "label": "按类型",
          "prop": "plate_by_type",
          "dict": {
            "dictType": "AR_E_PLATE_BY_TYPE",
            "dictKey": [
              "AR_E_PLATE_BY_TYPE"
            ],
            "format": "label"
          }
        },
        {
          "label": "按贷款品种",
          "prop": "by_loan_prod_type",
          "dict": {
            "dictType": "AR_E_LOAN_PROD_TYPE",
            "dictKey": [
              "AR_E_LOAN_PROD_TYPE"
            ],
            "format": "label"
          }
        },
        {
          "label": "按偿还资金来源",
          "prop": "repay_fund",
          "dict": {
            "dictType": "AR_E_REPAYMENT_FUND",
            "dictKey": [
              "AR_E_REPAYMENT_FUND"
            ],
            "format": "label"
          }
        },
        {
          "label": "按贷款投向",
          "prop": "by_loan_invest"
        },
        {
          "label": "按贷款方向名称",
          "prop": "by_loan_invest_name"
        },
        {
          "label": "按贷款方式",
          "prop": "by_loan_mode"
        },
        {
          "label": "按贷款方式名称",
          "prop": "by_loan_name"
        },
        {
          "label": "操作",
          "fixed": "right",
          "align": "center",
          "btns": [
            {
              "type": "edit",
              "label": "维护",
              "toRequest": {
                "url": "/views/icms/lend/lendIOU/drawdnPlate/drawdnPlate_modify.json",
                "method": "POST"
              },
              "props": {
                "width": "70%",
                "title": "涉融资平台信息维护"
              },
              "show": {
                "expression": "'${{params.pageType}}' != 'info'"
              }
            },
            {
              "type": "delete",
              "label": "删除",
              "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "post",
                "params": {
                  "caozuolx": "2",
                  "servicecode": "ar0523",
                  "detail": {
                    "lend_no":"${{params.lend_no}}"
                  }
                }
              },
              "confirm": {
                "title": "确定删除",
                "confirmButtonText": "确定",
                "cancelButtonText": "取消"
              },
              "show": {
                "expression": "'${{params.pageType}}' != 'info'"
              }
            },
            {
              "type": "look",
              "label": "详情",
              "toRequest": {
                "url": "/views/icms/lend/lendIOU/drawdnPlate/drawdnPlate_info.json",
                "method": "POST"
              },
              "props": {
                "width": "85%",
                "title": "涉融资平台信息详情"
              }
            }
          ]
        }
      ],
      "pagination": {
        "show": true
      }
    }
  }
}
