{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/call/RPCCall",
        "method": "post",
        "params": {
          "servicecode": "ar0531",
          "lend_no": "${{params.lend_no}}"
        }
      },
      "doRequest": {
        "url": "/SUMP/call/RPCCall",
        "method": "post",
        "params": {
          "caozuolx":"1",
          "servicecode": "ar0532"
        }
      },
      "controls": [
        {
          "lend_no": {
            "control": "hidden"
          },
          "project_name": {
            "control": "text",
            "label": "项目名称",
            "span": 8,
            "rules": [
              {
                "required": true,
                "message": "请输入项目名称"
              }
            ]
          },
          "proj_type": {
            "control": "text",
            "label": "项目类型",
            "span": 8,
            "rules": [
              {
                "required": true,
                "message": "请输入项目类型"
              }
            ]
          },
          "proj_total_invst": {
            "control": "currency",
            "label": "项目总投资",
            "span": 8,
            "rules": [
              {
                "required": true,
                "message": "请输入项目总投资"
              }
            ]
          }
        },
        {
          "capital": {
            "control": "currency",
            "label": "资本金(元)",
            "span": 8,
            "rules": [
              {
                "required": true,
                "message": "请输入资本金"
              }
            ]
          },
          "proj_loan_money": {
            "control": "currency",
            "label": "项目贷款金额(元)",
            "span": 8,
            "rules": [
              {
                "required": true,
                "message": "请输入项目贷款金额"
              }
            ]
          },
          "proj_loan_term": {
            "control": "text",
            "label": "项目贷款期限",
            "span": 8,
            "rules": [
              {
                "required": true,
                "message": "请输入项目贷款期限"
              }
            ]
          }
        },
        {
          "appr_docmt_no": {
            "control": "text",
            "label": "批文号",
            "span": 8
          },
          "proj_appr": {
            "control": "text",
            "label": "立项批文",
            "span": 8
          },
          "planning_permit_no": {
            "control": "text",
            "label": "规划许可证编号",
            "span": 8
          }
        },
        {
          "land_permmit_no": {
            "control": "text",
            "label": "建筑用地许可证编号",
            "span": 8
          },
          "eia_permmit_no": {
            "control": "text",
            "label": "环评许可证编号",
            "span": 8
          },
          "cstruct_permmit_no": {
            "control": "text",
            "label": "施工许可证编号",
            "span": 8
          }
        },
        {
          "other_permmit_name": {
            "control": "text",
            "label": "其他许可证名称",
            "span": 8
          },
          "other_permmit_no": {
            "control": "text",
            "label": "其他许可证编号",
            "span": 8
          },
          "cstruct_begin_date": {
            "control": "dateTimePicker",
            "format": "yyyyMMdd",
            "label": "开工日期",
            "span": 8,
            "rules": [
              {
                "required": true,
                "message": "请输入开工日期"
              }
            ]
          }
        }
      ]
    }
  }
}
