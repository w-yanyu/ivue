{
  "module": "v_search_btn_datagrid",
  "layout": {

    "toolbar": {
      "btns": [
        {
          "type": "add",
          "target": "dialog",
          "label": "新增",
          "toRequest": {
            "method": "POST",
            "url": "/views/icms/lend/lendIOU/inProjectInfo/inProjectInfo_add.json",
            "params": {
              "servicecode": "ar0532",
              "lend_no":"${{params.lend_no}}"
            }
          },
          "props": {
            "width": "65%",
            "title": "涉项目贷款信息新增"
          },
          "show": {
            "expression": "'${{params.pageType}}' != 'info'"
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": false,
      "scope": "searchTable",
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ar0531",
          "lend_no":"${{params.lend_no}}"
        }
      },
      "thead": [
        {
          "label": "放款序号",
          "prop": "lend_no"
        },
        {
          "label": "项目名称",
          "prop": "project_name"
        },
        {
          "label": "项目类型",
          "prop": "proj_type"
        },
        {
          "label": "项目总投资(元)",
          "prop": "proj_total_invst"
        },
        {
          "label": "资本金(元)",
          "prop": "capital"
        },
        {
          "label": "项目贷款金额(元)",
          "prop": "proj_loan_money"
        },
        {
          "label": "项目贷款期限",
          "prop": "proj_loan_term"
        },
        {
          "label": "批文号",
          "prop": "appr_docmt_no"
        },
        {
          "label": "立项批文",
          "prop": "proj_appr"
        },
        {
          "label": "规划许可证编号",
          "prop": "planning_permit_no"
        },
        {
          "label": "建筑用地许可证编号",
          "prop": "land_permmit_no"
        },
        {
          "label": "环评许可证编号",
          "prop": "eia_permmit_no"
        },
        {
          "label": "施工许可证编号",
          "prop": "cstruct_permmit_no"
        },
        {
          "label": "其他许可证名称",
          "prop": "other_permmit_name"
        },
        {
          "label": "其他许可证编号",
          "prop": "other_permmit_no"
        },
        {
          "label": "开工日期",
          "prop": "cstruct_begin_date",
          "control": "dateTimePicker",
          "format": "yyyyMMdd"
        },
        {
          "label": "操作",
          "fixed": "right",
          "align": "center",
          "btns": [
            {
              "type": "edit",
              "label": "维护",
              "toRequest": {
                "url": "/views/icms/lend/lendIOU/inProjectInfo/inProjectInfo_modify.json",
                "method": "POST"
              },
              "props": {
                "width": "70%",
                "title": "涉项目贷款信息维护"
              },
              "show": {
                "expression": "'${{params.pageType}}' != 'info'"
              }
            },
            {
              "type": "delete",
              "label": "删除",
              "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "post",
                "params": {
                  "caozuolx": "2",
                  "servicecode": "ar0533"
                }
              },
              "confirm": {
                "title": "确定删除",
                "confirmButtonText": "确定",
                "cancelButtonText": "取消"
              },
              "show": {
                "expression": "'${{params.pageType}}' != 'info'"
              }
            },
            {
              "type": "look",
              "label": "详情",
              "toRequest": {
                "url": "/views/icms/lend/lendIOU/inProjectInfo/inProjectInfo_info.json",
                "method": "POST"
              },
              "props": {
                "width": "85%",
                "title": "涉项目贷款信息详情"
              }
            }
          ]
        }
      ],
      "pagination": {
        "show": true
      }
    }
  }
}
