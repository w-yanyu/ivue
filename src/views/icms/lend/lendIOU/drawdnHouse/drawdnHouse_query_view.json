          "prop": "house_loan"
        },
        {
          "label": "企业收购保障性住房贷款",
          "align": "center",
          "prop": "firm_asecurity_house_loan"
        },
        {
          "label": "其他购买住房贷款",
          "align": "center",
          "prop": "other_buy_house_loan"
