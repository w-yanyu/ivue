          "type": "add",
          "target": "dialog",
          "label": "新增",
          "toRequest": {
            "method": "POST",
            "url": "/views/icms/lend/lendIOU/drawdnHouse/drawdnHouse_add.json",
            "params": {
              "lend_no":"${{params.lend_no}}"
            }
          },
          "props": {
            "width": "65%",
            "title": "涉房贷款信息新增"
          },
          "show": {
            "expression": "'${{params.pageType}}' != 'info'"
          }
        }
      ]
    },
