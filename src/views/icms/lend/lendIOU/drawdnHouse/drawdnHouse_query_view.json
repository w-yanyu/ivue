          "label": "本笔贷款是否用于保障性安居工程",
          "align": "center",
          "prop": "is_security_loan",
          "dict": {
            "dictType": "MS_E_YESORNO",
            "dictKey": [
              "MS_E_YESORNO"
            ]
          }
        },
        {
          "label": "操作",
          "fixed": "right",
          "align": "center",
          "btns": [
            {
              "type": "edit",
              "label": "维护",
              "toRequest": {
                "url": "/views/icms/lend/lendIOU/drawdnHouse/drawdnHouse_modify.json",
                "method": "POST"
              },
              "props": {
                "width": "70%",
                "title": "涉房贷款信息维护"
              },
              "show": {
                "expression": "'${{params.pageType}}' != 'info'"
              }
            },
            {
              "type": "delete",
              "label": "删除",
              "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "post",
                "params": {
                  "caozuolx": "2",
                  "servicecode": "ar0513",
                  "detail": {
                    "lend_no":"${{params.lend_no}}"
                  }
                }
              },
              "confirm": {
                "title": "确定删除",
                "confirmButtonText": "确定",
                "cancelButtonText": "取消"
              },
              "show": {
                "expression": "'${{params.pageType}}' != 'info'"
              }
            },
            {
              "type": "look",
              "label": "详情",
              "toRequest": {
                "url": "/views/icms/lend/lendIOU/drawdnHouse/drawdnHouse_info.json",
                "method": "POST"
              },
              "props": {
                "width": "85%",
                "title": "涉房贷款信息详情"
              }
            }
          ]
        }
