{
  "module": "v_realonlyForm_btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/call/RPCCall",
        "method": "post",
        "params": {
          "servicecode": "ar0511",
          "lend_no": "${{params.lend_no}}"
        }
      },
      "controls": [
        {
          "realty_deve_loan": {
            "control": "currency",
            "label": "1.房地产开发贷款",
            "disabled": true,
            "aligin": "center",
            "compute": "${{form.estate_deve_loan}}+${{form.house_deve_loan}}",
            "span": 10
          },
          "estate_deve_loan": {
            "control": "currency",
            "label": "1.1 地产开发贷款",
            "disabled": true,
            "compute": "${{form.gov_house_branch_loan}}+${{form.proj_house_loan}}+${{form.other_land_deve_loan}}",
            "span": 10
          }
        },
        {
          "gov_house_branch_loan": {
            "control": "currency",
            "label": "1.1.1 政府土地储备机构贷款",
            "disabled": true,
            "span": 10
          },
          "proj_house_loan": {
            "control": "currency",
            "label": "1.1.2 保障性安居工程土地开发贷款",
            "disabled": true,
            "span": 10
          }
        },
        {
          "other_land_deve_loan": {
            "control": "currency",
            "label": "1.1.3 其他地产开发贷款",
            "disabled": true,
            "span": 10
          },
          "house_deve_loan": {
            "control": "currency",
            "label": "1.2 房产开发贷款",
            "disabled": true,
            "compute": "${{form.housing_deve_loan}}+${{form.commercial_deve_house_loan}}+${{form.other_house_deve_loan}}",
            "span": 10
          }
        },
        {
          "housing_deve_loan": {
            "control": "currency",
            "label": "1.2.1 住房开发贷款",
            "disabled": true,
            "compute": "${{form.affordable_house_loan}}+${{form.cooperative_build_loan}}+${{form.other_house_loan}}",
            "span": 10
          },
          "affordable_house_loan": {
            "control": "currency",
            "label": "1.2.1.1 保障性住房开发贷款",
            "disabled": true,
            "span": 10
          }
        },
        {
          "cooperative_build_loan": {
            "control": "currency",
            "label": "1.2.1.2 合作建房贷款",
            "disabled": true,
            "span": 10
          },
          "other_house_loan": {
            "control": "currency",
            "disabled": true,
            "label": "1.2.1.3 其他住房开发贷款",
            "span": 10
          }
        },
        {
          "commercial_deve_house_loan": {
            "control": "currency",
            "label": "1.2.2 商业用房开发贷款",
            "disabled": true,
            "span": 10
          },
          "other_house_deve_loan": {
            "control": "currency",
            "label": "1.2.3 其他房产开发贷款",
            "disabled": true,
            "span": 10
          }
        },
        {
          "buy_house_loan": {
            "control": "currency",
            "label": "2.购房贷款",
            "disabled": true,
            "compute": "${{form.firm_buy_house_loan}}+${{form.other_buy_house_loan}}+${{form.pson_buy_house_loan}}",
            "span": 10
          },
          "firm_buy_house_loan": {
            "control": "currency",
            "label": "2.1 企业购房贷款",
            "disabled": true,
            "compute": "${{form.commercial_house_loan}}+${{form.house_loan}}",
            "span": 10
          }
        },
        {
          "commercial_house_loan": {
            "control": "currency",
            "label": "2.1.1 商业用房贷款",
            "disabled": true,
            "span": 10
          },
          "house_loan": {
            "control": "currency",
            "label": "2.1.2 住房贷款",
            "disabled": true,
            "compute": "${{form.firm_asecurity_house_loan}}+${{form.other_buy_house_loan}}",
            "span": 10
          }
        },
        {
          "firm_asecurity_house_loan": {
            "control": "currency",
            "label": "2.1.2.1 企业收购保障性住房贷款",
            "span": 10,
            "disabled": true
          },
          "other_buy_house_loan": {
            "control": "currency",
            "label": "2.1.2.2 其他购买住房贷款",
            "disabled": true,
            "span": 10
          }
        },
        {
          "gov_buy_house_loan": {
            "control": "currency",
            "label": "*2.2 机关团体购房贷款",
            "disabled": true,
            "compute": "${{form.hou_at}}+${{form.hou_au}}",
            "span": 10
          },
          "gov_security_house_loan": {
            "control": "currency",
            "label": "2.2.2.1 机关团体收购保障性住房贷款",
            "span": 10,
            "disabled": true
          }
        },
        {
          "other_gov_house_loan": {
            "control": "currency",
            "label": "2.2.2.2 机关团体其他购买住房贷款",
            "span": 10,
            "disabled": true
          },
          "pson_buy_house_loan": {
            "control": "currency",
            "label": "2.3 个人购房贷款",
            "disabled": true,
            "compute": "${{form.pson_busi_house_loan}}+${{form.pson_busi_house_loan}}+{{form.pson_house_count_loans}}+{{form.pson_house_area_loans}}",
            "span": 10
          }
        },
        {
          "pson_busi_house_loan": {
            "control": "currency",
            "label": "2.3.1 个人商业用房贷款",
            "span": 10,
            "disabled": true
          },
          "pson_house_loan": {
            "control": "currency",
            "label": "2.3.2 个人住房贷款（新建）",
            "disabled": true,
            "compute": "${{form.new_housing_loan}}+${{form.tran_house_loan}}",
            "span": 10
          }
        },
        {
          "new_housing_loan": {
            "control": "currency",
            "label": "2.3.2.1 新建房贷款",
            "disabled": true,
            "span": 10
          },
          "tran_house_loan": {
            "control": "currency",
            "label": "2.3.2.2 再交易房贷款",
            "span": 10,
            "disabled": true
          }
        },
        {
          "pson_house_count_loans": {
            "control": "currency",
            "label": "2.3.3 个人住房贷款（套数)",
            "disabled": true,
            "compute": "${{form.first_house_total}}+${{form.second_house_total}}+${{form.many_house_total}}",
            "span": 10
          },
          "first_house_total": {
            "control": "currency",
            "label": "2.3.3.1 第一套",
            "disabled": true,
            "span": 10
          }
        },
        {
          "second_house_total": {
            "control": "currency",
            "label": "2.3.3.2 第二套",
            "disabled": true,
            "span": 10
          },
          "many_house_total": {
            "control": "currency",
            "label": "2.3.3.3 第三套及以上",
            "disabled": true,
            "span": 10
          }
        },
        {
          "pson_house_area_loans": {
            "control": "currency",
            "label": "2.3.4 个人住房贷款（面积）",
            "disabled": true,
            "compute": "${{form.small_area_total}}+${{form.medium_area_total}}+${{form.large_total}}",
            "span": 10
          },
          "small_area_total": {
            "control": "currency",
            "label": "2.3.4.1 小于等于90平米",
            "disabled": true,
            "span": 10
          }
        },
        {
          "medium_area_total": {
            "control": "currency",
            "label": "2.3.4.2大于90平米小于等于144平米 ",
            "disabled": true,
            "span": 10
          },
          "large_total": {
            "control": "currency",
            "label": "2.3.4.3 大于144平米",
            "disabled": true,
            "span": 10
          }
        },
        {
          "securitized_realty_loan": {
            "control": "currency",
            "label": "3.证券化的房地产贷款",
            "disabled": true,
            "compute": "${{form.securitized_pson_house_loan}}+${{form.other_securitiz_realty_loan}}",
            "span": 10
          },
          "securitized_pson_house_loan": {
            "control": "currency",
            "label": "3.1 证券化个人住房贷款",
            "disabled": true,
            "span": 10
          }
        },
        {
          "other_securitiz_realty_loan": {
            "control": "currency",
            "disabled": true,
            "label": "3.2 其他证券化房地产贷款",
            "span": 10
          },
          "is_security_loan": {
            "control": "select",
            "disabled": true,
            "label": "本笔贷款是否用于保障性安居工程",
            "span": 10,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ]
            }
          }
        }
      ]
    }
  }
}
