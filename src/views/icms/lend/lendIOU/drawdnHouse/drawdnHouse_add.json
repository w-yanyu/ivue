{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "doRequest": {
        "url": "/SUMP/call/RPCCall",
        "method": "post",
        "params": {
          "caozuolx": "0",
          "servicecode": "ar0512",
          "lend_no": "${{params.lend_no}}",
          "realty_deve_loan": "${{form.realty_deve_loan}}",
          "estate_deve_loan": "${{form.estate_deve_loan}}",
          "gov_house_branch_loan": "${{form.gov_house_branch_loan}}",
          "other_land_deve_loan": "${{form.other_land_deve_loan}}",
          "house_deve_loan": "${{form.house_deve_loan}}",
          "housing_deve_loan": "${{form.housing_deve_loan}}",
          "affordable_house_loan": "${{form.affordable_house_loan}}",
          "cooperative_build_loan": "${{form.cooperative_build_loan}}",
          "other_house_loan": "${{form.other_house_loan}}",
          "commercial_deve_house_loan": "${{form.commercial_deve_house_loan}}",
          "other_house_deve_loan": "${{form.other_house_deve_loan}}",
          "buy_house_loan": "${{form.buy_house_loan}}",
          "firm_buy_house_loan": "${{form.firm_buy_house_loan}}",
          "commercial_house_loan": "${{form.commercial_house_loan}}",
          "house_loan": "${{form.house_loan}}",
          "firm_asecurity_house_loan": "${{form.firm_asecurity_house_loan}}",
          "other_buy_house_loan": "${{form.other_buy_house_loan}}",
          "gov_buy_house_loan": "${{form.gov_buy_house_loan}}",
          "gov_security_house_loan": "${{form.gov_security_house_loan}}",
          "other_gov_house_loan": "${{form.other_gov_house_loan}}",
          "securitized_realty_loan": "${{form.securitized_realty_loan}}",
          "securitized_pson_house_loan": "${{form.securitized_pson_house_loan}}",
          "other_securitiz_realty_loan": "${{form.other_securitiz_realty_loan}}",
          "is_security_loan": "${{form.is_security_loan}}",
          "pson_buy_house_loan": "${{form.pson_buy_house_loan}}",
          "pson_busi_house_loan": "${{form.pson_busi_house_loan}}",
          "pson_house_loan": "${{form.pson_house_loan}}",
          "new_housing_loan": "${{form.new_housing_loan}}",
          "tran_house_loan": "${{form.tran_house_loan}}",
          "pson_house_count_loans": "${{form.pson_house_count_loans}}",
          "pson_house_area_loans": "${{form.pson_house_area_loans}}",
          "first_house_total": "${{form.first_house_total}}",
          "second_house_total": "${{form.second_house_total}}",
          "many_house_total": "${{form.many_house_total}}",
          "small_area_total": "${{form.small_area_total}}",
          "medium_area_total": "${{form.medium_area_total}}",
          "large_total": "${{form.large_total}}"
        }
      },
      "name": "detail",
      "controls": [
        {
          "realty_deve_loan": {
            "control": "currency",
            "label": "1.房地产开发贷款",
            "disabled": true,
            "compute": "${{form.estate_deve_loan}}+${{form.house_deve_loan}}",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请选择按法律性质"
              }
            ]
          },
          "estate_deve_loan": {
            "control": "currency",
            "label": "1.1 地产开发贷款",
            "disabled": true,
            "compute": "${{form.gov_house_branch_loan}}+${{form.proj_house_loan}}+${{form.other_land_deve_loan}}",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请输入地产开发贷款"
              }
            ]
          }
        },
        {
          "gov_house_branch_loan": {
            "control": "currency",
            "label": "1.1.1 政府土地储备机构贷款",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请输入政府土地储备机构贷款"
              }
            ]
          },
          "proj_house_loan": {
            "control": "currency",
            "label": "1.1.2 保障性安居工程土地开发贷款",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请输入保障性安居工程土地开发贷款"
              }
            ]
          }
        },
        {
          "other_land_deve_loan": {
            "control": "currency",
            "label": "1.1.3 其他地产开发贷款",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请输入其他地产开发贷款"
              }
            ]
          },
          "house_deve_loan": {
            "control": "currency",
            "label": "1.2 房产开发贷款",
            "disabled": true,
            "compute": "${{form.housing_deve_loan}}+${{form.commercial_deve_house_loan}}+${{form.other_house_deve_loan}}",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请输入地产开发贷款"
              }
            ]
          }
        },
        {
          "housing_deve_loan": {
            "control": "currency",
            "label": "1.2.1 住房开发贷款",
            "disabled": true,
            "compute": "${{form.affordable_house_loan}}+${{form.cooperative_build_loan}}+${{form.other_house_loan}}",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请输入住房开发贷款"
              }
            ]
          },
          "affordable_house_loan": {
            "control": "currency",
            "label": "1.2.1.1 保障性住房开发贷款",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请输入保障性住房开发贷款"
              }
            ]
          }
        },
        {
          "cooperative_build_loan": {
            "control": "currency",
            "label": "1.2.1.2 合作建房贷款",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请输入合作建房贷款"
              }
            ]
          },
          "other_house_loan": {
            "control": "currency",
            "label": "1.2.1.3 其他住房开发贷款",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请输入其他房产开发贷款"
              }
            ]
          }
        },
        {
          "commercial_deve_house_loan": {
            "control": "currency",
            "label": "1.2.2 商业用房开发贷款",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请输入商业用房开发贷款"
              }
            ]
          },
          "other_house_deve_loan": {
            "control": "currency",
            "label": "1.2.3 其他房产开发贷款",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请输入其他房产开发贷款"
              }
            ]
          }
        },
        {
          "buy_house_loan": {
            "control": "currency",
            "label": "2.购房贷款",
            "disabled": true,
            "compute": "${{form.firm_buy_house_loan}}+${{form.other_buy_house_loan}}+${{form.pson_buy_house_loan}}",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请输入其他房产开发贷款"
              }
            ]
          },
          "firm_buy_house_loan": {
            "control": "currency",
            "label": "2.1 企业购房贷款",
            "disabled": true,
            "compute": "${{form.commercial_house_loan}}+${{form.house_loan}}",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请输入企业购房贷款"
              }
            ]
          }
        },
        {
          "commercial_house_loan": {
            "control": "currency",
            "label": "2.1.1 商业用房贷款",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请输入商业用房贷款"
              }
            ]
          },
          "house_loan": {
            "control": "currency",
            "label": "2.1.2 住房贷款",
            "disabled": true,
            "compute": "${{form.firm_asecurity_house_loan}}+${{form.other_buy_house_loan}}",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请输入住房贷款"
              }
            ]
          }
        },
        {
          "firm_asecurity_house_loan": {
            "control": "currency",
            "label": "2.1.2.1 企业收购保障性住房贷款",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请输入企业收购保障性住房贷款"
              }
            ]
          },
          "other_buy_house_loan": {
            "control": "currency",
            "label": "2.1.2.2 其他购买住房贷款",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请输入其他购买住房贷款"
              }
            ]
          }
        },
        {
          "gov_buy_house_loan": {
            "control": "currency",
            "label": "*2.2 机关团体购房贷款",
            "disabled": true,
            "compute": "${{form.hou_at}}+${{form.hou_au}}",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请输入机关团体购房贷款"
              }
            ]
          },
          "gov_security_house_loan": {
            "control": "currency",
            "label": "2.2.2.1 机关团体收购保障性住房贷款",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请输入机关团体收购保障性住房贷款"
              }
            ]
          }
        },
        {
          "other_gov_house_loan": {
            "control": "currency",
            "label": "2.2.2.2 机关团体其他购买住房贷款",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请输入机关团体其他购买住房贷款"
              }
            ]
          },
          "pson_buy_house_loan": {
            "control": "currency",
            "label": "2.3 个人购房贷款",
            "disabled": true,
            "compute": "${{form.pson_busi_house_loan}}+${{form.pson_house_loan}}+${{form.pson_house_count_loans}}+${{form.pson_house_area_loans}}",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请输入个人购房贷款"
              }
            ]
          }
        },
        {
          "pson_busi_house_loan": {
            "control": "currency",
            "label": "2.3.1 个人商业用房贷款",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请输入个人商业用房贷款"
              }
            ]
          },
          "pson_house_loan": {
            "control": "currency",
            "label": "2.3.2 个人住房贷款（新建）",
            "disabled": true,
            "compute": "${{form.new_housing_loan}}+${{form.tran_house_loan}}",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请输入个人住房贷款(新建)"
              }
            ]
          }
        },
        {
          "new_housing_loan": {
            "control": "currency",
            "label": "2.3.2.1 新建房贷款",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请输入新建房贷款"
              }
            ]
          },
          "tran_house_loan": {
            "control": "currency",
            "label": "2.3.2.2 再交易房贷款",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请输入再交易房贷款"
              }
            ]
          }
        },
        {
          "pson_house_count_loans": {
            "control": "currency",
            "label": "2.3.3 个人住房贷款（套数)",
            "disabled": true,
            "compute": "${{form.first_house_total}}+${{form.second_house_total}}+${{form.many_house_total}}",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请输入个人住房贷款（套数）"
              }
            ]
          },
          "first_house_total": {
            "control": "currency",
            "label": "2.3.3.1 第一套",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请输入第一套"
              }
            ]
          }
        },
        {
          "second_house_total": {
            "control": "currency",
            "label": "2.3.3.2 第二套",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请输入第二套"
              }
            ]
          },
          "many_house_total": {
            "control": "currency",
            "label": "2.3.3.3 第三套及以上",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请输入第三套"
              }
            ]
          }
        },
        {
          "pson_house_area_loans": {
            "control": "currency",
            "label": "2.3.4 个人住房贷款（面积）",
            "disabled": true,
            "compute": "${{form.small_area_total}}+${{form.medium_area_total}}+${{form.large_total}}",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请输入个人住房贷款（面积）"
              }
            ]
          },
          "small_area_total": {
            "control": "currency",
            "label": "2.3.4.1 小于等于90平米",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请输入小于等于90平米的总额"
              }
            ]
          }
        },
        {
          "medium_area_total": {
            "control": "currency",
            "label": "2.3.4.2大于90平米小于等于144平米 ",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请输入大于90平米小于等于144平米的总额"
              }
            ]
          },
          "large_total": {
            "control": "currency",
            "label": "2.3.4.3 大于144平米",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请输入大于144平米的总额"
              }
            ]
          }
        },
        {
          "securitized_realty_loan": {
            "control": "currency",
            "label": "3.证券化的房地产贷款",
            "disabled": true,
            "compute": "${{form.securitized_pson_house_loan}}+${{form.other_securitiz_realty_loan}}",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请输入证券化的房地产贷款"
              }
            ]
          },
          "securitized_pson_house_loan": {
            "control": "currency",
            "label": "3.1 证券化个人住房贷款",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请输入证券化个人住房贷款"
              }
            ]
          }
        },
        {
          "other_securitiz_realty_loan": {
            "control": "currency",
            "label": "3.2 其他证券化房地产贷款",
            "span": 12,
            "rules": [
              {
                "required": true,
                "message": "请输入其他证券化房地产贷款"
              }
            ]
          },
          "is_security_loan": {
            "control": "select",
            "label": "本笔贷款是否用于保障性安居工程",
            "span": 12,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "value-label"
            },
            "rules": [
              {
                "required": true,
                "message": "请输入其他证券化房地产贷款"
              }
            ]
          }
        }
      ]
    }
  }
}
