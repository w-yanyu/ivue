{
  "module": "v_realonlyForm_btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/call/RPCCall",
        "method": "post",
        "params": {
          "servicecode": "ar0501",
          "lend_no":"${{params.lend_no}}"
        }
      },
      "controls": [
        {
          "loan_farm_class": {
            "control": "select",
            "label": "涉农贷款信息登记",
            "span": 12,
            "disabled": true,
            "dict": {
              "dictType": "AR_E_LOAN_FARM_CLASS",
              "dictKey": [
                "AR_E_LOAN_FARM_CLASS"
              ],
              "format": "value-label"
            },
            "showItem": {
              "01": ["loan_farm_purpose", "loan_farm_credit"],
              "02": ["main_form", "loan_farm_purpose", "is_farm_corp_size", "is_farm_corp_deve","is_support_poor"],
              "03": ["main_form", "loan_farm_purpose", "is_farm_corp_deve"],
              "04": []
            }
          },
          "firm_scale": {
            "control": "select",
            "label": "企业规模",
            "disabled": true,
            "span": 12,
            "dict": {
              "dictType": "AR_E_ENT_SCALE",
              "dictKey": [
                "AR_E_ENT_SCALE"
              ],
              "format": "value-label"
            }

          }
        },
        {
          "loan_farm_purpose": {
            "initShow": false,
            "control": "select",
            "label": "农户贷款用途",
            "disabled": true,
            "span": 12,
            "dict": {
              "dictType": "AR_E_LOAN_FARM_PURPOSE",
              "dictKey": [
                "AR_E_LOAN_FARM_PURPOSE"
              ],
              "format": "value-label"
            }
          },
          "loan_farm_credit": {
            "initShow": false,
            "control": "select",
            "disabled": true,
            "label": "农户贷款分类",
            "dict": {
              "dictType": "AR_E_LOAN_FARM_CREDIT",
              "dictKey": [
                "AR_E_LOAN_FARM_CREDIT"
              ],
              "format": "value-label"
            },
            "span": 12
          },
          "main_form": {
            "initShow": false,
            "control": "select",
            "disabled": true,
            "label": "主体形式",
            "span": 12,
            "dict": {
              "dictType": "E_MAIN_FARM",
              "dictKey": [
                "E_MAIN_FARM"
              ],
              "format": "value-label"
            }
          }
        },
        {
          "is_farm_corp_size": {
            "initShow": false,
            "disabled": true,
            "control": "select",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "value-label"
            },
            "label": "是否农村中小企业贷款",
            "span": 12
          },
          "is_farm_corp_deve": {
            "initShow": false,
            "control": "select",
            "disabled": true,
            "label": "是否农村企业及各类组织农业综合开发贷款",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "value-label"
            },
            "span": 12
          }
        },
        {
          "is_support_poor": {
            "initShow": false,
            "control": "select",
            "disabled": true,
            "label": "是否扶贫贴息",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "value-label"
            },
            "span": 12
          },
          "is_city_corp_deve": {
            "initShow": false,
            "disabled": true,
            "control": "select",
            "label": "是否城市企业农业综合开发",
            "span": 12,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ]
            }
          }
        },
        {

        }
      ]
    }
  }
}
