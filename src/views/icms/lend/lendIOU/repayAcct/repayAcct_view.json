{
  "module": "v_realonlyForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "controls": [
        {
          "pay_way": {
            "control": "select",
            "label": "付款方式",
            "span": 12,
            "disabled": true,
            "dict": {
              "dictType": "AR_E_PAYMENT_METHOD",
              "dictKey": [
                "AR_E_PAYMENT_METHOD"
              ],
              "format": "label"
            }
          },
          "bulk_ded_type": {
            "control": "select",
            "label": "扣款类型",
            "span": 12,
            "disabled": true,
            "dict": {
              "dictType": "AR_E_DEDUCTION_TYPE",
              "dictKey": [
                "AR_E_DEDUCTION_TYPE"
              ],
              "format": "label"
            }
          }
        },
        {
          "lend_acct_no": {
            "control": "text",
            "label": "账号",
            "span": 12,
            "disabled": true
          },"loan_acct_name": {
            "control": "text",
            "label": "户名",
            "span": 12,
            "disabled": true
          }
        },
        {
          "branch_org": {
            "control": "text",
            "label": "所属机构",
            "span": 12,
            "disabled": true
          },"ccy_code": {
            "control": "select",
            "label": "币种",
            "span": 12,
            "disabled": true,
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": [
                "MS_CURRENCY"
              ],
              "format": "label"
            }
          }
        },
        {
          "acct_type": {
            "control": "select",
            "label": "帐户类型",
            "dict": {
              "dictType": "AR_E_ACCT_TYPE",
              "dictKey": [
                "AR_E_ACCT_TYPE"
              ],
              "format": "label"
            },
            "span": 12,
            "disabled": true
          },
          "money_exchange_flag": {
            "control": "select",
            "label": "钞汇标志",
            "span": 12,
            "disabled": true,
            "dict": {
              "dictType": "AR_E_ACCT_NTTT_FLAG",
              "dictKey": [
                "AR_E_ACCT_NTTT_FLAG"
              ],
              "format": "label"
            }
          }
        },
        {
          "apply_no": {
            "control": "hidden",
            "label": "申请号",
            "span": 12,
            "disabled": true,
            "value": "${{params.apply_no}}"
          },
          "lend_no": {
            "control": "hidden",
            "label": "放款序号",
            "value": "${{params.lend_no}}"
          }
        }
      ]
    }
  }
}