{
  "module": "v_realonlyForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "controls": [
        {
          "pay_way": {
            "control": "select",
            "label": "付款方式",
            "span": 12,
            "disabled": true,
            "dict": {
              "dictType": "AR_E_PAYMENT_METHOD",
              "dictKey": [
                "AR_E_PAYMENT_METHOD"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": false,
                "message": "请输入付款方式"
              }
            ],
            "value": "1"
          },
          "bulk_ded_type": {
            "control": "select",
            "label": "扣款类型",
            "span": 12,
            "disabled": true,
            "dict": {
              "dictType": "AR_E_DEDUCTION_TYPE",
              "dictKey": [
                "AR_E_DEDUCTION_TYPE"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": false,
                "message": "此项不能为空"
              }
            ],
            "value": "3"
          }
        },
        {
          "priority_no": {
            "control": "select",
            "label": "扣款优先等级",
            "span": 12,
            "disabled": true,
            "dict": {
              "dictType": "AR_E_PAYMENT_PRIORITY",
              "dictKey": [
                "AR_E_PAYMENT_PRIORITY"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": false,
                "message": "此项不能为空"
              }
            ],
            "value": "1"
          },
          "lend_acct_no": {
            "control": "text",
            "label": "账号",
            "span": 12,
            "disabled": true,
            "rules": [
              {
                "required": false,
                "message": "此项不能为空"
              }
            ]
          }
        },
        {
          "branch_org": {
            "control": "text",
            "label": "所属机构",
            "span": 12,
            "disabled": true,
            "rules": [
              {
                "required": false,
                "message": "此项不能为空"
              }
            ]
          },
          "loan_acct_name": {
            "control": "text",
            "label": "户名",
            "span": 12,
            "disabled": true,
            "rules": [
              {
                "required": false,
                "message": "此项不能为空"
              }
            ]
          }
        },
        {
          "acct_type": {
            "control": "select",
            "label": "帐户类型",
            "dict": {
              "dictType": "AR_E_ACCT_TYPE",
              "dictKey": [
                "AR_E_ACCT_TYPE"
              ],
              "format": "label"
            },
            "span": 12,
            "disabled": true,
            "rules": [
              {
                "required": false,
                "message": "此项不能为空"
              }
            ],
            "value": "1"
          },
          "money_exchange_flag": {
            "control": "select",
            "label": "钞汇标志",
            "span": 12,
            "disabled": true,
            "dict": {
              "dictType": "AR_E_ACCT_NTTT_FLAG",
              "dictKey": [
                "AR_E_ACCT_NTTT_FLAG"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": false,
                "message": "此项不能为空"
              }
            ],
            "value": "1"
          }
        },
        {
          "ccy_code": {
            "control": "select",
            "label": "币种",
            "span": 12,
            "disabled": true,
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": [
                "MS_CURRENCY"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": false,
                "message": "此项不能为空"
              }
            ],
            "value": "156"
          }
        },
        {
          "apply_no": {
            "control": "hidden",
            "label": "申请号",
            "span": 12,
            "disabled": true,
            "value": "${{params.apply_no}}"
          },
          "lend_no": {
            "control": "hidden",
            "label": "放款序号",
            "value": "${{params.lend_no}}"
          }
        }
      ]
    }
  }
}