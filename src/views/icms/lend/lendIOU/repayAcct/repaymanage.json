{
  "module": "v-inputDataForm-btn",
  "layout": {
    "scope": "templateScope",
    "form": {
      "btns": [],
      "controlsGroup": [
        {
          "title": "还款账户信息",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "accordion": false,
          "expand": true,
          "controls": [
            {
              "top": {
                "control": "dynamic",
                "span": 24,
                "toRequest": {
                  "url": "/views/icms/lend/lendIOU/repayAcct/repayAcct_manage.json",
                  "method": "post",
                  "params": {
                    "apply_no": "${{params.apply_no}}",
                    "lend_no": "${{params.lend_no}}",
                    "pageType":"${{params.pageType}}"
                  }
                }
              }
            }
          ]
        },
        {
          "title": "多还款账户列表",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "accordion": false,
          "expand": true,
          "controls": [
            {
              "bottom": {
                "control": "dynamic",
                "span": 24,
                "toRequest": {
                  "url": "/views/icms/lend/lendIOU/repayAcct/repayAcctList_manage.json",
                  "method": "post",
                  "params": {
                    "apply_no": "${{params.apply_no}}",
                    "lend_no": "${{params.lend_no}}",
                    "pageType":"${{params.pageType}}"
                  }
                }
              }
            }
          ]
        }
      ]
    }
  }
}
