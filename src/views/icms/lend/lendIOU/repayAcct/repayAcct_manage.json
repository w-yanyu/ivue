{
  "module": "v_search_btn_datagrid",
  "layout": {

    "toolbar": {
      "btns": [
        {
          "type": "add",
          "target": "dialog",
          "label": "新增",
          "toRequest": {
            "method": "GET",
            "url": "/views/icms/lend/lendIOU/repayAcct/repayAcct_add.json",
            "params": {
              "apply_no": "${{params.apply_no}}",
              "lend_no": "${{params.lend_no}}"
            }
          },
          "show": {
           "expression": "'${{params.pageType}}' != 'info'"
          },
          "props": {
            "width": "50%",
            "title":  "新增还款账户"
          }
        }
      ]
    },
    "datagrid": {
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params":{
          "servicecode":"ar0563",
          "apply_no": "${{params.apply_no}}",
          "lend_no": "${{params.lend_no}}"
        }
      },
      "pagination": {
        "pageSizes": [
          10,
          15,
          20,
          50,
          100
        ],
        "pageSize": 10
      },
      "thead": [
        {
          "label": "付款方式",
          "prop": "pay_way",
          "align": "center",
          "dict": {
            "dictType": "AR_E_PAYMENT_METHOD",
            "dictKey": [
              "AR_E_PAYMENT_METHOD"
            ],
            "labelField": "dictName",
            "valueField": "dictId"
          }
        },
        {
          "label":"扣款类型",
          "prop": "bulk_ded_type",
          "align": "center",
          "dict": {
            "dictType": "AR_E_DEDUCTION_TYPE",
            "dictKey": [
              "AR_E_DEDUCTION_TYPE"
            ],
            "labelField": "dictName",
            "valueField": "dictId"
          }
        },
        {
          "label":  "账号",
          "prop": "lend_acct_no",
          "align": "center"
        },
        {
          "label":  "所属机构",
          "prop": "branch_org",
          "align": "center"
        },
        {
          "label":  "户名",
          "prop": "loan_acct_name",
          "align": "center"
        },
        {
          "label":  "帐户类型",
          "prop": "acct_type",
          "align": "center",
          "dict": {
            "dictType": "AR_E_ACCT_TYPE",
            "dictKey": [
              "AR_E_ACCT_TYPE"
            ],
            "labelField": "dictName",
            "valueField": "dictId"
          }
        },
        {
          "label":  "钞汇标志",
          "prop": "money_exchange_flag",
          "align": "center",
          "dict": {
            "dictType": "AR_E_ACCT_NTTT_FLAG",
            "dictKey": [
              "AR_E_ACCT_NTTT_FLAG"
            ],
            "labelField": "dictName",
            "valueField": "dictId"
          }
        },
        {
          "label":  "币种",
          "prop": "ccy_code",
          "align": "center",
          "dict": {
            "dictType": "MS_CURRENCY",
            "dictKey": [
              "MS_CURRENCY"
            ],
            "labelField": "dictName",
            "valueField": "dictId"
          }
        },
        {
          "label": "操作",
          "width":150,
          "align": "center",
          "btns": [
            {
              "type": "edit",
              "label": "维护",
              "toRequest": {
                "url": "/views/icms/lend/lendIOU/repayAcct/repayAcct_modify.json",
                "method": "get",
                "props":{
                  "title":"维护还款账户"
                }
              },
              "show": {
               "expression": "'${{params.pageType}}' != 'info'"
              }
            },
            {
              "type": "edit",
              "label": "详情",
              "toRequest": {
                "url": "/views/icms/lend/lendIOU/repayAcct/repayAcct_view.json",
                "method": "get",
                "props":{
                  "title":"还款账户详情"
                }
              }
            },
            {
              "type": "delete",
              "label": "删除",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "post",
                "params":{
                  "servicecode":"ar0564"
                }
              },
              "show": {
               "expression": "'${{params.pageType}}' != 'info'"
              }
            }
          ]
        }
      ]
    }
  }
}
