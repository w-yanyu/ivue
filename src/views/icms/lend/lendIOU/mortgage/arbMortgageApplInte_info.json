{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "ar1664",
          "apply_no": "${{params.apply_no}}"
        }
      },
      "btns": [],
      "controls": [
        {
          "loan_kind": {
            "control": "select",
            "label": "贷款种类",
            "disabled": true,
            "span": 8,
            "width": "200px",
            "dict": {
              "dictType": "AR_E_LOAN_TYPE",
              "dictKey": [
                "AR_E_LOAN_TYPE"
              ],
              "format": "label"
            }
          },
          "house_type": {
            "control": "select",
            "label": "房屋类型",
            "span": 8,
            "width": "200px",
            "disabled": true,
            "dict": {
              "dictType": "AR_E_HOUSE_TYPE",
              "dictKey": [
                "AR_E_HOUSE_TYPE"
              ],
              "format": "label"
            }
          },
          "buy_price": {
            "control": "currency",
            "label": "购买价格(元)",
            "span": 8,
            "width": "200px",
            "disabled": true
          },
          "house_address": {
            "control": "text",
            "label": "房屋所在城市",
            "span": 8,
            "width": "200px",
            "disabled": true
          },
          "area": {
            "control": "text",
            "label": "面积(㎡)",
            "span": 8,
            "width": "200px",
            "disabled": true
          },
          "loan_amt": {
            "control": "currency",
            "label": "贷款金额(元)",
            "span": 8,
            "width": "200px",
            "disabled": true
          },
          "open_acct_bank": {
            "control": "text",
            "label": "预约业务办理网点",
            "span": 8,
            "width": "200px",
            "disabled": true
          },
          "bank_addr": {
            "control": "text",
            "label": "开户行地址",
            "span": 8,
            "width": "200px",
            "disabled": true
          },
          "city": {
            "control": "text",
            "label": "办理城市",
            "span": 8,
            "width": "200px",
            "disabled": true
          },
          "house_id": {
            "control": "text",
            "label": "楼盘编号",
            "span": 8,
            "width": "200px",
            "disabled": true
          },
          "house_name": {
            "control": "text",
            "label": "楼盘名称",
            "span": 8,
            "width": "200px",
            "disabled": true
          }
        }
      ]
    }
  }
}
