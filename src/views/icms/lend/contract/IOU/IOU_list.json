{
  "module": "v_search_btn_datagrid",
  "layout": {
    "toolbar": {
      "btns": []
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ar0706",
          "loan_contract_no": "${{params.loan_contract_no}}"
        }
      },
      "thead": [
        {
          "label": "借据号",
          "prop": "loan_no",
          "align": "center"
        },
        {
          "label": "放款金额",
          "prop": "lend_amt",
          "align": "center"
        },
        {
          "label": "币种",
          "prop": "ccy_code",
          "align": "center",
          "dict": {
            "dictType": "MS_CURRENCY",
            "dictKey": [
              "MS_CURRENCY"
            ],
            "format": "label"
          }
        },
        {
          "label": "借据状态",
          "prop": "loan_status",
          "align": "center",
          "dict": {
            "dictType": "AR_E_LOAN_STATUS",
            "dictKey": [
              "AR_E_LOAN_STATUS"
            ],
            "format": "label"
          }
        },
        {
          "label": "操作",
          "fixed": "right",
          "width": 140,
          "align": "center",
          "btns": [
            {
              "type": "edit",
              "label": "错误信息",
              "toRequest": {
                "url": "/views/icms/lend/contract/IOU/IOU_erro_info.json",
                "method": "GET",
                "params": {
                }
              },
              "show": {
                "expression": "'${{current.loan_status}}' === 'LNSB'||'${{current.loan_status}}' === 'ARSB'"
              },
              "props": {
                "title": "错误信息"
              }
            }
          ]
        }
      ]
    }
  }
}
