{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "foldLineNumber": 3,
      "controls": [
        {
          "appId": {
            "control": "select",
            "label": "业务模块",
            "selectFirstValue": true,
            "disabled": true,
            "dict": {
              "dictType": "MS_APPID",
              "dictKey": [
                "MS_APPID"
              ],
              "format": "value-label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择业务模块"
              }
            ]
          },
          "appl_id": {
            "control": "text",
            "label": "申请编号",
            "rules": [
              {
                "max": 64,
                "message": "长度不能超过64个字符"
              }
            ]
          }
        },
        {
          "prod_name": {
            "control": "text",
            "label": "产品名称",
            "rules": [
              {
                "max": 80,
                "message": "长度不能超过80个字符"
              }
            ]
          },
          "mobile_no": {
            "control": "text",
            "label": "手机号码",
            "rules": [
              {
                "pattern":"^1[3-9]\\d{9}$",
                "message": "请输入正确的手机号码"
              }
            ]
          }
        },
        {
          "cust_id": {
            "control": "text",
            "label": "客户编号",
            "rules": [
              {
                "max": 18,
                "message": "长度不能超过18个字符"
              }
            ]
          },
          "cust_name": {
            "control": "text",
            "label": "客户名称",
            "rules": [
              {
                "max": 18,
                "message": "长度不能超过18个字符"
              }
            ]
          }
        },
        {
          "cert_type": {
            "control": "select",
            "label": "证件类型",
            "dict": {
              "dictType": "MS_E_CRET_TYPE",
              "dictKey": [
                "MS_E_CRET_TYPE"
              ],
              "format": "label"
            }
          },
          "cert_no": {
            "control": "text",
            "label": "证件号码",
            "rules": [{
              "pattern": "^[A-Za-z0-9]+$",
              "max": 50,
              "message": "请输入正确证件号码,长度50字符以内"
          }]
          }
        },
        {
          "time_out_flag": {
            "control": "select",
            "label": "是否超时",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          }
        }
      
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "bpm1001",
          "cur_user_id": "$g{{user_info.userCode}}",
          "appId": "${{form.appId}}"
        }
      },
      "thead": [
        {
          "label": "申请编号",
          "prop": "appl_id",
          "width": 200,
          "align": "center"
        },
        {
          "label": "客户名称",
          "prop": "cust_name",
          "width": 150,
          "align": "center"
        },
        {
          "label": "手机号码",
          "prop": "mobile_no",
          "width": 150,
          "align": "center"
        },
        {
          "label": "证件号码",
          "prop": "cert_no",
          "width": 200,
          "align": "center"
        },
        {
          "label": "产品名称",
          "prop": "prod_name",
          "width": 200,
          "align": "center"
        },
        {
          "label": "流程产品名称",
          "prop": "wf_prod_name",
          "width": 200,
          "align": "center"
        },
        {
          "label": "进件日期",
          "prop": "system_date",
          "width": 100,
          "align": "center"
        },
        {
          "label": "当前节点",
          "prop": "task_define_name",
          "width": 150,
          "align": "center"
        },
        
        {
          "label": "处理状态",
          "prop": "appl_state",
          "dict": {
            "dictType": "CT_E_APPLST",
            "dictKey": [
              "CT_E_APPLST"
            ],
            "format": "label"
          },
          "align": "center"
        },
        {
          "label": "当前处理用户名称",
          "prop": "user_name",
          "value": "${{current.cur_user_id}}-${{current.user_name}}",
          "align": "center"
        },
        {
          "label": "当前处理角色名称",
          "prop": "role_name",
          "value": "${{current.cur_wfrl_id}}-${{current.role_name}}",
          "align": "center"
        },
        {
          "label": "停留时间（天）",
          "prop": "cost_time",
          "width": 150,
          "align": "center"
        },
        {
          "label": "处理截止时间",
          "prop": "end_time",
          "width": 150,
          "align": "center"
        },
        {
          "label": "操作",
          "fixed": "right",
          "width": 240,
          "align": "center",
          "btns": [
            {
              "type": "edit",
              "label": "办理",
              "toRequest": {
                "url": "@CommWaitToDoDeal",
                "method": "GET",
                "target": "navTab",
                "noRefreshTab": true,
                "params": {
                  "frame_cd": "${{current.task_url1}}",
                  "cust_name": "${{current.cust_name}}",
                  "appl_state": "${{current.appl_state}}",
                  "system_date": "${{current.system_date}}",
                  "task_inst_id": "${{current.task_inst_id}}",
                  "appId": "${{form.appId}}"
                }
              },
              "props": {
                "title": "待办事项办理"
              }
            },
            {
              "type": "edit",
              "label": "流程跟踪",
              "toRequest": {
                "url": "@ProcessImgView",
                "method": "GET",
                "params": {
                  "from": "backlog",
                  "appId": "${{form.appId}}"
                }
              },
              "props": {
                "width": "1200px",
                "title": "流程跟踪图"
              }
            },
            {
              "type": "edit",
              "label": "转签",
              "show": {
                "prop": "sign_tran_flag",
                "is": [
                  "1"
                ]
              },
              "toRequest": {
                "url": "/views/msap/bpm/processManagement/design/process_transign_manager.json",
                "method": "GET",
                "params": {
                  "appId": "${{form.appId}}",
                  "task_inst_id": "${{current.task_inst_id}}",
                  "sign_mode": "0"
                }
              },
              "props": {
                "title": "流程转签"
              }
            },
            {
              "type": "edit",
              "label": "取消签收",
              "show": {
                "prop": "sign_cancel_flag",
                "is": [
                  "1"
                ]
              },
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "bpm1021",
                  "task_inst_id": "${{current.task_inst_id}}",
                  "appId": "${{form.appId}}"
                }
              },
              "confirm": {
                "title": "提示信息",
                "msg": "确认要取消签收吗？",
                "confirmButtonText": "确定",
                "cancelButtonText": "取消"
              }
            }
          ]
        }
      ]
    }
  }
}
