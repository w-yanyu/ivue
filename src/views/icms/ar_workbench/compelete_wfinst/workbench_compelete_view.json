{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "foldLineNumber": 3,
      "controls": [
        {
          "appId": {
            "control": "select",
            "label": "业务模块",
            "selectFirstValue": true,
            "dict": {
              "dictType": "MS_APPID",
              "dictKey": [
                "MS_APPID"
              ],
              "format": "value-label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择业务模块"
              }
            ]
          },
          "appl_id": {
            "control": "text",
            "label": "申请编号",
            "rules": [
              {
                "max": 64,
                "message": "长度不能超过64个字符"
              }
            ]
          }
        },
        {
          "cust_name": {
            "control": "text",
            "label": "申请人姓名",
            "rules": [
              {
                "max": 18,
                "message": "长度不能超过18个字符"
              }
            ]
          },
          "cert_no": {
            "control": "text",
            "label": "证件号码",
            "rules": [
              {
                "max": 20,
                "message": "长度不能超过20个字符"
              }
            ]
          }
        },
        {
          "mobile_no": {
            "control": "text",
            "label": "手机号码",
            "rules": [
              {
                "pattern":"^1[3-9]\\d{9}$",
                "message": "请输入正确的手机号码"
              }
            ]
          },
          "query_hist_flag": {
            "control": "select",
            "label": "是否从历史数据中查询",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "value-label"
            }
          }
        },
        {
          "appl_start_date": {
            "control": "dateTimePicker",
            "label": "进件起始日期"
          },
          "appl_end_date": {
            "control": "dateTimePicker",
            "label": "进件截止日期"
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "bpm1028",
          "sizePage": 10,
          "appId": "${{form.appId}}",
          "qryInfo": {
            "user_id": "$g{{user_info.userCode}}"
          }
        }
      },
      "thead": [
        {
          "label": "序号",
          "type": "index"
        },
        {
          "label": "申请编号",
          "prop": "appl_id",
          "align": "center"
        },
        {
          "label": "申请人姓名",
          "prop": "cust_name",
          "align": "center"
        },
        {
          "label": "证件号码",
          "prop": "cert_no",
          "align": "center"
        },
        {
          "label": "产品名称",
          "prop": "prod_name",
          "align": "center"
        },
        {
          "label": "进件日期",
          "prop": "system_date",
          "align": "center"
        },
        {
          "label": "流程状态",
          "prop": "wf_cur_status",
          "align": "center",
          "dict": {
            "dictType": "BPM_E_STATUS",
            "dictKey": [
              "BPM_E_STATUS"
            ],
            "format": "label"
          }
        },
        {
          "label": "操作",
          "fixed": "right",
          "align": "center",
          "btns": [
            {
              "type": "edit",
              "label": "详情",
              "toRequest": {
                "url": "@CommAlreadyDoDetail",
                "method": "GET",
                "target": "navTab",
                "params": {
                  "frame_cd": "${{current.task_url2}}",
                  "cust_name": "${{current.cust_name}}",
                  "appl_state": "${{current.appl_state}}",
                  "appl_date": "${{current.appl_date}}",
                  "task_inst_id": "${{current.task_inst_id}}",
                  "appId": "${{form.appId}}",
                  "wf_inst_id": "${{current.wf_inst_id}}",
                  "cust_no": "${{current.cust_id}}"
                }
              },
              "props": {
                "title": "已办事项详情"
              }
            },
            {
              "type": "edit",
              "label": "流程跟踪",
              "toRequest": {
                "url": "@ProcessImgView",
                "method": "GET",
                "params": {
                  "appId": "${{form.appId}}",
                  "query_hist_flag": "${{form.query_hist_flag}}",
                  "from": "doneList"
                }
              },
              "props": {
                "width": "1200px",
                "title": "流程跟踪图"
              }
            }
          ]
        }
      ]
    }
  }
}