{
  "module": "dynamicTabs",
  "layout": {
    "form": {
      "controls": [
          {
              "appId": {
                  "control": "select",
                  "label": "业务模块",
                  "selectFirstValue":true,
                  "dict": {
                      "dictType": "MS_APPID",
                      "dictKey": [
                          "MS_APPID"
                      ],
                      "format": "value-label"
                  },
                  "rules": [
                      {
                          "required": true,
                          "message": "请选择业务模块"
                      }]
              }
            }]
          },
    "tabs": {
      "tabHeader": [
        {
          "id": "tab1",
          "label": {
            "default": "待办事项", 
             "i18n": "K0001.to-do-list"
                            },
          "closable": false,
          "disabled": false,
          "icon": "el-icon-date",
          "toRequest": {
            "url": "/views/icms/ar_workbench/wait_todo/workbench_wait_todo_view.json",
            "method": "post"
          }
        },
        {
          "id": "tab2",
          "label": {
            "default": "已办事项", 
             "i18n": "K0001.Completed-tems"
                            },
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "toRequest": {
            "url": "/views/icms/ar_workbench/already_do/workbench_already_do_view.json",
            "method": "post"
          }
        },

        {
          "id": "tab5",
          "label": {
            "default": "进件池", 
             "i18n": "K0001.Incoming-pool"
                            },
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "toRequest": {
            "url": "/views/icms/ar_workbench/into_pool/workbench_into_pool_view.json",
            "method": "post"
          }
        },

        {
          "id": "tab6",
          "label": "已办结事项",
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "toRequest": {
            "url": "/views/icms/ar_workbench/compelete_wfinst/workbench_compelete_view.json",
            "method": "post"
          }
        },
        {
          "id": "tab7",
          "label": "转办事项",
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "toRequest": {
            "url": "/views/msap/bpm/workbench/agent_todo/workbench_agent_todo_view.json",
            "method": "post"
          }
        }
      ],
      "defaultActiveKey": "tab1",
      "position": "top",
      "styleClass": "border-card"
    }
  }
}
