{
    "module": "v_search_btn_datagrid",
    "layout": {
      "initRequest":"${{params}}",
      "toolbar": {
        "btns": [
          {
            "type": "add",
            "target": "dialog",
            "label": "新增",
            "toRequest": {
              "method": "GET",
              "params": {"apply_no":"${{params.apply_no}}"},
              "url": "/views/icms/internationalBusiness/ArbInvoiceInfo/arb_invoice_info_add.json"
            },
            "props": {
              "width": "90%",
              "title": "发票信息新增"
            }
          }
        ]
      },
      "datagrid": {
        "isInitSearch": false,
        "doRequest": {
          "url": "/SUMP/icmcall/icmRPCCall",
          "method": "POST",
          "params": {
            "apply_no":"${{params.apply_no}}",
            "servicecode":"ar1324"
          }
        },
        "pagination": {
          "pageSizes": [
            10,
            15,
            20,
            50,
            100
          ],
          "pageSize": 10
        },
        "thead": [
          {
            "label": "序号",
            "type": "index",
            "align": "center"
          },
          {
            "label": "申请号",
            "prop": "apply_no",
            "align": "center"
          },
          {
            "label": "借据号",
            "prop": "loan_no",
            "align": "center"
          },
          {
            "label": "发票号码",
            "prop": "invoice_no",
            "align": "center"
          },
          {
            "label": "发票金额",
            "prop": "invoice_amt",
            "align": "center",
            "format":{
              "thousand":","
            }
          },
          {
            "label": "发票金额（折基准币种）",
            "prop": "invoice_base_amt",
            "align": "center",
            "format":{
              "thousand":","
            }
          }, {
            "label": "汇率",
            "prop": "exchange_rate",
            "align": "center"
          },{
            "label": "发票币种",
            "prop": "ccy_code",
            "dict":
                {
                  "dictType":"MS_CURRENCY",
                  "dictKey": "MS_CURRENCY",
                  "format":"label"
                },
            "align": "center"
          },{
            "label": "基准币种",
            "prop": "base_ccy",
            "dict":
                {
                  "dictType":"MS_CURRENCY",
                  "dictKey": "MS_CURRENCY",
                  "format":"label"
                },
            "align": "center"
          },{
            "label": "开票日期",
            "prop": "invoice_date",
            "align": "center"
          },{
            "label": "买方(购买方)",
            "prop": "buyer",
            "align": "center"
          },{
            "label": "卖方(销售方)",
            "prop": "seller",
            "align": "center"
          },
          {
            "label": "操作",
            "fixed": "right",
            "width": 200,
            "align": "center",
            "btns": [

              {
                "type": "edit",
                "target": "dialog",
                "label": "维护",
                "toRequest": {
                  "method": "GET",
                  "url": "/views/icms/internationalBusiness/ArbInvoiceInfo/arb_invoice_info_edit.json"
                },
                "props": {
                  "width": "90%",
                  "title": "发票信息编辑"
                }
              },
              {
                "type": "delete",
                "label": "删除",
                "doRequest": {
                  "url": "/SUMP/icmcall/icmRPCCall",
                  "method": "POST",
                  "params": {
                    "servicecode": "ar1323",
                    "apply_no": "${{current.apply_no}}",
                    "invoice_no":"${{current.invoice_no}}"
                  }
                }
              }
            ]
          }
        ]
      }
    }
  }
