{
    "module": "v_search_btn_datagrid",
    "layout": {
      "initRequest":"${{params}}",
      "datagrid": {
        "isInitSearch": false,
        "doRequest": {
          "url": "/SUMP/icmcall/icmRPCCall",
          "method": "POST",
          "params": {
            "apply_no":"${{params.apply_no}}",
            "servicecode":"ar1324"
          }
        },
        "pagination": {
          "pageSizes": [
            10,
            15,
            20,
            50,
            100
          ],
          "pageSize": 10
        },
        "thead": [
          {
            "label": "序号",
            "type": "index",
            "align": "center"
          },
          {
            "label": "申请号",
            "prop": "apply_no",
            "align": "center"
          },
          {
            "label": "借据号",
            "prop": "loan_no",
            "align": "center"
          },
          {
            "label": "发票号码",
            "prop": "invoice_no",
            "align": "center"
          },
          {
            "label": "发票金额",
            "prop": "invoice_amt",
            "align": "center",
            "format":{
              "thousand":","
            }
          },
          {
            "label": "发票金额（折基准币种）",
            "prop": "invoice_base_amt",
            "align": "center",
            "format":{
              "thousand":","
            }
          }, {
            "label": "汇率",
            "prop": "exchange_rate",
            "align": "center"
          },{
            "label": "发票币种",
            "prop": "ccy_code",
            "dict":
                {
                  "dictType":"MS_CURRENCY",
                  "dictKey": "MS_CURRENCY",
                  "format":"label"
                },
            "align": "center"
          },{
            "label": "基准币种",
            "prop": "base_ccy",
            "dict":
                {
                  "dictType":"MS_CURRENCY",
                  "dictKey": "MS_CURRENCY",
                  "format":"label"
                },
            "align": "center"
          },{
            "label": "开票日期",
            "prop": "invoice_date",
            "align": "center"
          },{
            "label": "买方(购买方)",
            "prop": "buyer",
            "align": "center"
          },{
            "label": "卖方(销售方)",
            "prop": "seller",
            "align": "center"
          }
        ]
      }
    }
  }
