{
    "module": "v_search_btn_datagrid",
    "layout": {
      "toolbar": {
        "btns": [
          {
            "type": "add",
            "target": "dialog",
            "label": "新增",
            "toRequest": {
              "method": "GET",
              "url": "/views/icms/internationalBusiness/ArbInterBusiContract/arb_inter_busi_contract_add.json"
            },
            "props": {
              "width": "90%",
              "title": "国际业务申请合同"
            }
          }
        ]
      },
      "datagrid": {
        "isInitSearch": false,
        "doRequest": {
          "url": "/SUMP/icmcall/icmRPCCall",
          "method": "POST",
          "params": {
            "servicecode": "ar1308"
          }
        },
        "pagination": {
          "pageSizes": [
            10,
            15,
            20,
            50,
            100
          ],
          "pageSize": 10
        },
        "thead": [
          {
            "label": "序号",
            "type": "index",
            "align": "center"
          },
          {
            "label": "合同号",
            "prop": "contract_no",
            "align": "center"
          },
          {
            "label": "申请号",
            "prop": "apply_no",
            "align": "center"
          },
          {
            "label": "押汇类型",
