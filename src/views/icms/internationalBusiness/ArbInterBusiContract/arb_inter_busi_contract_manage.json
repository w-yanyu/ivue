          {
            "label": "宽限期(天)",
            "prop": "grace_period_days",
            "align": "center"
          },
          {
            "label": "是否签订贸易融资主协议",
            "prop": "signed_or_not",
            "align": "center"
          },
          {
            "label": "贸易融资主协议编号",
            "prop": "agreement_no",
            "align": "center"
          },
          {
