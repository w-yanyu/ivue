          {
            "label": "信用证类型",
            "prop": "credit_of_type",
            "align": "center"
          },
          {
            "label": "手续费金额",
            "prop": "procedure_amt",
            "align": "center",
            "format":{
              "thousand":","
            }
          },
          {
            "label": "手续费率(‰)",
            "prop": "procedure_rates",
            "align": "center"
          },
          {
            "label": "溢短装类型",
            "prop": "more_or_less_type",
            "align": "center"
          },
          {
            "label": "溢短装比例",
            "prop": "more_or_less_ratio",
            "align": "center"
          },
          {
            "label": "付款期限(天)",
            "prop": "payment_term",
            "align": "center"
          },
          {
            "label": "资金来源",
            "prop": "capital_src",
            "align": "center"
          },
          {
            "label": "付款方式",
            "prop": "pay_way",
            "align": "center"
          },
          {
            "label": "合同金额",
            "prop": "contract_valid_amt",
            "align": "center",
            "format":{
              "thousand":","
            }
          },
          {
            "label": "备注",
            "prop": "remark",
            "align": "center"
          },
          {
            "label": "操作",
            "fixed": "right",
            "width": 200,
            "align": "center",
            "btns": [

              {
                "type": "add",
                "target": "dialog",
                "label": "修改",
                "toRequest": {
                  "method": "GET",
                  "url": "/views/icms/internationalBusiness/ArbInterBusiContract/arb_inter_busi_contract_edit.json"
                },
                "props": {
                  "width": "90%",
                  "title": "国际业务合同编辑"
                }
              },
              {
                "type": "delete",
                "label": "删除",
                "doRequest": {
                  "url": "",
                  "method": "POST",
                  "params": {
                    "servicecode": "ar1307",
                    "contract_no":"${{current.contract_no}}",
                    "apply_no": "${{current.apply_no}}"
                  }
                }
              }
            ]
          }
        ]
      }
    }
  }
  