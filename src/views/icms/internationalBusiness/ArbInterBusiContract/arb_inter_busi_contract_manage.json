            "align": "center"
          },
          {
            "label": "福费廷类型",
            "prop": "foiting_type",
            "align": "center"
          },
          {
            "label": "统计标签",
            "prop": "statistical_tag",
            "align": "center"
          },
          {
            "label": "申请日期",
            "prop": "apply_date",
            "align": "center"
          },
          {
            "label": "起始日期",
            "prop": "start_date",
            "align": "center"
          },
          {
            "label": "到期日期",
            "prop": "matu_date",
            "align": "center"
          },
          {
            "label": "可售产品编号",
            "prop": "sale_prod_cd",
            "align": "center"
          },
          {
            "label": "申请币种",
            "prop": "ccy_code",
            "align": "center"
          },
          {
            "label": "汇率",
            "prop": "exchange_rate",
            "align": "center"
          },
          {
            "label": "基准币种",
            "prop": "base_ccy",
            "align": "center"
          },
          {
            "label": "申请金额",
            "prop": "apply_amt",
            "align": "center",
            "format":{
              "thousand":","
            }
          },
          {
            "label": "审批金额",
            "prop": "appr_amt",
            "align": "center",
            "format":{
              "thousand":","
            }
          },
          {
            "label": "申请基准金额",
            "prop": "apply_base_amt",
            "align": "center",
            "format":{
              "thousand":","
            }
          },
          {
            "label": "申请期限(月)",
            "prop": "apply_term",
            "align": "center"
          },
          {
            "label": "审批期限",
            "prop": "apply_term",
            "align": "center"
          },
          {
            "label": "利率",
            "prop": "interest_rate",
            "align": "center"
          },
          {
            "label": "业务办理网点",
            "prop": "busi_tran_bank_no",
            "align": "center"
