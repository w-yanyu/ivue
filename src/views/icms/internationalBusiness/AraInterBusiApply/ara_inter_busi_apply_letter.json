{
  "module": "v-form-btn-dynamic",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ar1304",
          "apply_no": "${{params.apply_no}}"
        }
      },
      "controls": [
        {
          "contract_no": {
            "control": "text",
            "label": "合同号",
            "disabled": true,
            "width": "200px",
            "span": 7
          },
          "apply_no": {
            "control": "text",
            "label": "申请号",
            "disabled": true,
            "width": "200px",
            "span": 7
          },
          "statistical_tag": {
            "control": "text",
            "label": "统计标签",
            "rules": [
              {
                "required": true,
                "message": "请选择统计标签"
              }
            ],
            "width": "200px",
            "span": 7
          },
          "apply_date": {
            "control": "dateTimePicker",
            "label": "申请日期",
            "rules": [
              {
                "required": true,
                "message": "请选择申请日期"
              }
            ],
            "width": "200px",
            "span": 7
          },
          "start_date": {
            "control": "dateTimePicker",
            "label": "起始日期",
            "rules": [
              {
                "required": true,
                "message": "请选择起始日期"
              }
            ],
            "width": "200px",
            "span": 7
          },
          "matu_date": {
            "control": "dateTimePicker",
            "label": "到期日期",
            "rules": [
              {
                "required": true,
                "message": "请选择到期日期"
              }
            ],
            "width": "200px",
            "span": 7
          },
          "sale_prod_cd": {
            "control": "hidden",
            "label": "可售产品编号",
            "disabled": true,
            "width": "200px",
            "span": 7
          },
          "ccy_code": {
            "control": "select",
            "label": "申请币种",
            "rules": [
              {
                "required": true,
                "message": "请选择申请币种"
              }
            ],
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": "MS_CURRENCY",
              "format": "label"
            },
            "width": "200px",
            "span": 7
          },
          "base_ccy": {
            "control": "select",
            "label": "基准币种",
            "rules": [
              {
                "required": true,
                "message": "请选择基准币种"
              }
            ],
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": "MS_CURRENCY",
              "format": "label"
            },
            "width": "200px",
            "span": 7
          },
          "exchange_rate": {
            "control": "currency",
            "align": "left",
            "label": "汇率",
            "rules": [
              {
                "required": true,
                "message": "请选择汇率"
              }
            ],
            "width": "200px",
            "span": 7
          },
          "apply_amt": {
            "control": "currency",
            "label": "申请金额",
            "rules": [
              {
                "required": true,
                "message": "请选择申请金额"
              }
            ],
            "width": "200px",
            "span": 7,
            "align": "left",
            "format": {
              "thousand": ","
            }
          },
          "apply_base_amt": {
            "control": "currency",
            "label": "申请基准金额",
            "rules": [
              {
                "required": true,
                "message": "请选择申请基准金额"
              }
            ],
            "compute": "${{form.apply_amt}}*${{form.exchange_rate}}",
            "width": "200px",
            "span": 7,
            "align": "left",
            "format": {
              "thousand": ","
            }
          },
          "appr_amt": {
            "control": "hidden",
            "label": "审批金额",
            "rules": [
              {
                "required": true,
                "message": "请选择审批金额"
              }
            ],
            "width": "200px",
            "span": 7,
            "align": "left",
            "format": {
              "thousand": ","
            }
          },

          "branch_id": {
            "control": "text",
            "disabled": true,
            "label": "机构号",
            "width": "200px",
            "span": 7
          },
          "credit_of_type": {
            "control": "select",
            "label": "信用证类型",
            "rules": [
              {
                "required": true,
                "message": "请选择信用证类型"
              }
            ],
            "dict": {
              "dictType": "AR_E_CREDIT_OF_TYPE",
              "dictKey": "AR_E_CREDIT_OF_TYPE",
              "format": "label"
            },
            "width": "200px",
            "span": 7
          },
          "procedure_rates": {
            "control": "currency",
            "align": "left",
            "label": "手续费率(‰)",
            "rules": [
              {
                "required": true,
                "message": "请选择手续费率"
              }
            ],
            "width": "200px",
            "span": 7
          },
          "procedure_amt": {
            "control": "currency",
            "label": "手续费金额",
            "compute": "${{form.apply_amt}}*${{form.procedure_rates}}*0.001",
            "rules": [
              {
                "required": true,
                "message": "请选择手续费金额"
              }
            ],
            "width": "200px",
            "span": 7,
            "align": "left",
            "format": {
              "thousand": ","
            }
          },

          "more_or_less_type": {
            "control": "select",
            "label": "溢短装类型",
            "rules": [
              {
                "required": true,
                "message": "请选择溢短装类型"
              }
            ],
            "dict": {
              "dictType": "AR_E_MORE_OR_LESS_TYPE",
              "dictKey": "AR_E_MORE_OR_LESS_TYPE",
              "format": "label"
            },
            "width": "200px",
            "span": 7
          },
          "more_or_less_ratio": {
            "control": "text",
            "label": "溢短装比例(%)",
            "rules": [
              {
                "required": true,
                "message": "请选择溢短装比例"
              }
            ],
            "width": "200px",
            "span": 7
          },
          "payment_term": {
            "control": "text",
            "label": "付款期限(天）",
            "rules": [
              {
                "required": true,
                "message": "请选择付款期限"
              }
            ],
            "width": "200px",
            "span": 7
          },
          "capital_src": {
            "control": "select",
            "label": "资金来源",
            "rules": [
              {
                "required": true,
                "message": "请选择资金来源"
              }
            ],
            "dict": {
              "dictType": "AR_E_PRCPL_SRC",
              "dictKey": "AR_E_PRCPL_SRC",
              "format": "label"
            },
            "width": "200px",
            "span": 7
          },
          "pay_way": {
            "control": "select",
            "label": "付款方式",
            "rules": [
              {
                "required": true,
                "message": "请选择付款方式"
              }
            ],
            "dict": {
              "dictType": "AR_E_PAYMENT_METHOD",
              "dictKey": "AR_E_PAYMENT_METHOD",
              "format": "label"
            },
            "width": "200px",
            "span": 7
          },

          "busi_tran_bank_no": {
            "control": "text",
            "label": "业务办理网点",
            "rules": [
              {
                "required": true,
                "message": "请选择业务办理网点"
              }
            ],
            "width": "200px",
            "span": 7
          },
          "specific_purpose": {
            "control": "textarea",
            "label": "具体用途",
            "rules": [
              {
                "required": true,
                "message": "请选择具体用途"
              }
            ],
            "width": "200px",
            "span": 21
          },
          "other_covenants": {
            "control": "textarea",
            "label": "其他约定事项",
            "width": "200px",
            "span": 21
          },
          "remark": {
            "control": "hidden",
            "label": "备注",
            "width": "200px",
            "span": 21
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "submit",
          "label": "保存",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "ar1302",
              "apply_no": "${{params.apply_no}}"
            }
          }
        }
      ]
    }
  }
}
