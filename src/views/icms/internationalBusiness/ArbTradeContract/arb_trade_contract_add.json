{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "ar1315"
            }
          }
        }
      ],
      "controls": [
        {
          "apply_no": {
            "control": "text",
            "disabled": true,
            "label": "申请号",
            "width": "200px",
            "span": 7
          },
          "loan_no": {
            "control": "text",
            "label": "借据号",
            "width": "200px",
            "span": 7
          },
          "trade_contract_no": {
            "control": "text",
            "label": "贸易合同号",
            "width": "200px",
            "span": 7
          },
          "contract_amt": {
            "control": "currency",
            "label": "贸易合同金额",
            "width": "200px",
            "span": 7,
            "format":{
              "thousand":","
            }
          },
          "exchange_rate": {
            "control": "currency",
            "label": "汇率",
            "width": "200px",
            "span": 7
          },
          "contract_base_amt": {
            "control": "currency",
            "label": "贸易合同金额（折基准币种）",
            "compute": "${{form.contract_amt}}*${{form.exchange_rate}}",
            "width": "200px",
            "span": 7,
            "format":{
              "thousand":","
            }

          },
          "ccy_code": {
            "control": "select",
            "label": "贸易合同币种",
            "width": "200px",
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": "MS_CURRENCY",
              "format": "label"
            },
            "span": 7
          },
          "base_ccy": {
            "control": "select",
            "label": "基准币种",
            "disabled": true,
            "width": "200px",
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": "MS_CURRENCY",
              "format": "label"
            },
            "value": "156",
            "span": 7
          },
          "loan_contract_sign_date": {
            "control": "dateTimePicker",
            "editable": false,
            "format": "yyyyMMdd",
            "label": "贸易合同签订日期",
            "width": "200px",
            "span": 7
          },
          "start_date": {
            "control": "dateTimePicker",
            "editable": false,
            "format": "yyyyMMdd",
            "label": "贸易起始日期",
            "width": "200px",
            "span": 7
          },
          "matu_date": {
            "control": "dateTimePicker",
            "editable": false,
            "format": "yyyyMMdd",
            "label": "贸易到期日期",
            "width": "200px",
            "span": 7
          },
          "trade_contract_desc": {
            "control": "text",
            "label": "贸易合同内容",
            "width": "200px",
            "span": 7
          },
          "buyer": {
            "control": "text",
            "label": "买方(进口商)",
            "width": "200px",
            "span": 7
          },
          "seller": {
            "control": "text",
            "label": "卖方(出口商)",
            "width": "200px",
            "span": 7
          }
        }
      ]
    }
  }
}