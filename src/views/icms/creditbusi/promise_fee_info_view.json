{
  "module": "v-form-btn-dynamic",
  "layout": {
    "form": {
      "scope": "detail",
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ar3001",
          "apply_no": "${{params.apply_no}}"
        }
      },
      "controls": [
        {
          "chrg_pstg_acct_name": {
            "control": "text",
            "disabled": true,
            "label": "承诺费收取入账账户名",
            "span": "7"
          },
          "chrg_pstg_acct_num": {
            "control": "text",
            "disabled": true,
            "label": "承诺费收取入账账号",
            "span": "7"
          },
          "billing_cond": {
            "control": "select",
            "disabled": true,
            "label": "承诺费收取条件",
            "span": "7",
            "dict": {
              "dictType": "AR_E_BILLING_COND",
              "dictKey": [
                "AR_E_BILLING_COND"
              ],
              "format": "label"
            }
          }
        },
        {
          "chrg_dt": {
            "control": "dateTimePicker",
            "disabled": true,
            "format": "yyyy-MM-dd",
            "label": "承诺费收取开始日期",
            "valueFormat": "yyyyMMdd",
            "span": "7"
          },
          "charg_basis": {
            "control": "select",
            "label": "承诺费收取基准",
            "disabled": true,
            "span": "7",
            "dict": {
              "dictType": "AR_E_CHARG_BASIS",
              "dictKey": [
                "AR_E_CHARG_BASIS"
              ],
              "format": "label"
            }
          },
          "charg_calcula_type": {
            "control": "select",
            "disabled": true,
            "label": "承诺费收取计算方式",
            "span": "7",
            "dict": {
              "dictType": "AR_E_CHARG_CALCULA_TYPE",
              "dictKey": [
                "AR_E_CHARG_CALCULA_TYPE"
              ],
              "format": "label"
            }
          }
        },
        {
          "billing_value": {
            "control": "currency",
            "disabled": true,
            "label": "承诺费收取数值",
            "span": "7",
            "align": "left",
            "format": {
              "thousand": ","
            }
          }
        }
      ]
    }
  }
}