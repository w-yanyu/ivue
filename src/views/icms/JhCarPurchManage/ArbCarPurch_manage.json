{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ar2010",
          "apply_no": "${{params.apply_no}}"
        }
      },
      "thead": [

        {
          "type": "string",
          "label": "子订单号",
          "width": 220,
          "prop": "child_order_no",
          "align": "center"
        },

        {
          "type": "string",
          "label": "供应商名称",
          "prop": "supplier_name",
          "width": 100,
          "align": "center"
        },
        {
          "type": "string",
          "label": "品牌名称",
          "prop": "brand_name",
          "align": "center"
        },
        {
          "type": "string",
          "label": "车系编号",
          "prop": "car_kind_no",
          "align": "center"
        },
        {
          "type": "string",
          "label": "车系名称",
          "prop": "car_kind_name",
          "align": "center"
        },

        {
          "type": "string",
          "label": "车型名称",
          "width": 140,
          "prop": "car_model_name",
          "align": "center"
        },
        {
          "type": "string",
          "label": "厂商指导价",
          "prop": "price",
          "align": "center"
        },
        {
          "type": "string",
          "label": "车身结构",
          "prop": "sizetype",
          "align": "center"
        },
        {
          "type": "string",
          "label": "车身颜色",
          "prop": "color",
          "align": "center"
        },
        {
          "type": "string",
          "label": "采购单价",
          "format": {
            "thousand": ","
          },
          "prop": "purch_amount",
          "align": "center"
        },
        {
          "type": "string",
          "label": "合格证编号",
          "prop": "certconf_id",
          "align": "center"
        },
        {
          "type": "string",
          "label": "发证日期",
          "prop": "issue_date",
          "align": "center"
        },
        {
          "type": "string",
          "label": "车辆识别号/车架号",
          "prop": "vehicle_number",
          "align": "center"
        },
        {
          "type": "string",
          "label": "发动机编号",
          "prop": "engine_number",
          "align": "center"
        },
        {
          "type": "string",
          "label": "车辆制造日期",
          "prop": "manuf_date",
          "align": "center"
        },
        {
          "type": "string",
          "label": "借据号",
          "width": 180,
          "prop": "loan_no",
          "align": "center"
        },
        {
          "label": "操作",
          "align": "center",
          "width": "200px",
          "fixed": "right",
          "btns": [
            {
              "type": "look",
              "label": "填入合格证",
              "toRequest": {
                "url": "/views/icms/JhCarPurchManage/ArbCarPurch_info.json.json",
                "method": "POST"
              },
              "props": {
                "width": "70%",
                "title": "填入合格证"
              }
            }
          ]
        }
      ]
    }
  }
}
