            "control": "string",
            "label": "车辆识别号/车架号",
            "span": 8
          },
          "engine_number": {
            "control": "string",
            "label": "发动机编号",
            "span": 8
          },
          "manuf_date": {
            "control": "string",
            "label": "车辆制造日期",
            "span": 8
          },
          "loan_no": {
            "control": "string",
            "label": "借据号",
            "span": 8
          }
          
        }
