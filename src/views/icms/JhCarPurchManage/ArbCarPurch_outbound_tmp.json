{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "scope": "formData",
      "initRequest": "${{params}}",
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "ar2013",
              "inventory_status":3
            }
          }
        }
      ],
      "controls": [
        {
          "order_id": {
            "control": "text",
            "disabled":true,
            "label": "订单号",
            "width": "130px",
            "rules": [
            ]
          },"child_order_no": {
            "control": "text",
            "disabled":true,
            "label": "子订单编号",
            "width": "130px",
            "rules": [
            ]
          },"outbound_reason": {
            "control": "textarea",
            "rows": "5",
            "label": "临时移库原因",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入临时移库原因"
              }
            ]
          },"outbound_date": {
            "control": "dateTimePicker",
            "label": "临时移库时间",
            "type": "date",
            "format": "yyyyMMdd",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入临时移库时间"
              }
            ]
          }
        }
      ]
    }
  }
}
