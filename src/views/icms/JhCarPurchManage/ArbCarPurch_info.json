{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ar2011"
        }
      },
      "controls": [
        {
          "order_id": {
            "control": "string",
            "label": "订单号",
            "span": 8
          },
          "child_order_no": {
            "control": "string",
            "label": "子订单号",
            "span": 8
          },
          "supplier_no": {
            "control": "string",
            "label": "供应商编号",
            "span": 8
          },
          "supplier_name": {
            "control": "string",
            "label": "供应商名称",
            "span": 8
          },
          "brand_no": {
            "control": "string",
            "label": "品牌编号",
            "span": 8
          },
          "brand_name": {
            "control": "string",
            "label": "品牌名称",
            "span": 8
          },
          "car_kind_no": {
            "control": "string",
            "label": "车系编号",
            "span": 8
          },
          "car_kind_name": {
            "control": "string",
            "label": "车系名称",
            "span": 8
          },
          "car_model_no": {
            "control": "string",
            "label": "车型编号",
            "span": 8
          },
          "car_model_name": {
            "control": "string",
            "label": "车型名称",
            "span": 8
          },
          "price": {
            "control": "string",
            "label": "厂商指导价",
            "span": 8
          },
          "sizetype": {
            "control": "string",
            "label": "车身结构",
            "span": 8
          },
          "displacementml": {
            "control": "string",
            "label": "排量(mL)",
            "span": 8
          },
          "geartype": {
            "control": "string",
            "label": "变速箱类型",
            "span": 8
          },
          "color": {
            "control": "string",
            "label": "车身颜色",
            "span": 8
          },
          "purch_amount": {
            "control": "string",
            "label": "采购单价",
            "format": {
              "thousand": ","
            },
            "span": 8
          },
          
          "certconf_id": {
            "control": "lookup",
            "edit": false,
            "span": 8,
            "rules": [
              {
                "required": true,
                "message": "请选择合格证信息"
              }
            ],
            "label": "合格证编号",
            "keyField": {
              "certconf_id": "certconf_id",
              "issue_date": "issue_date",
              "vehicle_number": "vehicle_number",
              "engine_number": "engine_number",
              "manuf_date": "manuf_date"
            },
            "toRequest": {
              "url": "/views/icms/certconf/certconf_process_content.json",
              "method": "POST"
            },
            "props": {
              "width": "80%",
              "title": "请选择合格证信息"
            },
            "format": "label"
          },
          "issue_date": {
            "control": "string",
            "label": "发证日期",
            "span": 8
          },
          "vehicle_number": {
            "control": "string",
            "label": "车辆识别号/车架号",
            "span": 8
          },
          "engine_number": {
            "control": "string",
            "label": "发动机编号",
            "span": 8
          },
          "manuf_date": {
            "control": "string",
            "label": "车辆制造日期",
            "span": 8
          }
        }
      ]
    }
  }
}