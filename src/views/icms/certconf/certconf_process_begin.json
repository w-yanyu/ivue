{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "certconf_id": {
            "control": "text",
            "label": "合格证编号",
            "rules": [
              {
                "max": 64,
                "message": "长度不能超过64个字符"
              }
            ]
          },
          "cert_status": {
            "control": "select",
            "label": "合格证状态",
            "dict": {
              "dictType": "AR_E_CERT_STATUS",
              "dictKey": [
                "AR_E_CERT_STATUS"
              ],
              "labelField": "dictName",
              "valueField": "dictId",
              "format": "label"
            }
          }
        },
        {
          "vehicle_number": {
            "control": "text",
            "label": "车辆识别号/车架号",
            "rules": [
              {
                "max": 17,
                "message": "长度不能超过17个字符"
              }
            ]
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
          {
              "type": "add",
              "target": "dialog",
              "label": "入库保管",
              "toRequest": {
                  "method": "GET",
                  "url": "/views/icms/certconf/certconf_add_steps.json"
              },
              "props": {
                "width": "60%",
                "title": "入库保管"
              }
          },
          {
            "type": "add",
            "target": "dialog",
            "label": "提前申领",
            "toRequest": {
                "method": "GET",
                "url": "/views/icms/certconf/certconf_tqsl_steps.json"
            },
            "props": {
              "width": "60%",
              "title": "提前申领"
            }
          },
          {
            "type": "add",
            "target": "dialog",
            "label": "正常申领",
            "toRequest": {
                "method": "GET",
                "url": "/views/icms/certconf/certconf_zcsl_steps.json"
            },
            "props": {
              "width": "60%",
              "title": "正常申领"
            }
          },
          {
            "type": "add",
            "target": "dialog",
            "label": "经销商接收",
            "toRequest": {
                "method": "GET",
                "url": "/views/icms/certconf/certconf_jxsjs_steps.json"
            },
            "props": {
              "width": "60%",
              "title": "经销商接收"
            }
          },
          {
            "type": "add",
            "target": "dialog",
            "label": "合格证退回",
            "toRequest": {
                "method": "GET",
                "url": "/views/icms/certconf/certconf_hgzth_steps.json"
            },
            "props": {
              "width": "60%",
              "title": "合格证退回"
            }
          },
          {
            "type": "add",
            "target": "dialog",
            "label": "制造商接收",
            "toRequest": {
                "method": "GET",
                "url": "/views/icms/certconf/certconf_zzsjs_steps.json"
            },
            "props": {
              "width": "60%",
              "title": "制造商接收"
            }
          }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ar4000",
          "appId": "icmsOnl"
        }
      },
      "thead": [
        {
          "label": "序号",
          "type": "radio",
          "align": "center"
        },
        {
          "label": "合格证编号",
          "prop": "certconf_id",
          "width": 200,
          "align": "center"
        },
        {
          "label": "发证日期",
          "prop": "issue_date",
          "width": 200,
          "align": "center"
        },
        {
          "label": "车辆制造企业名称",
          "prop": "vehicle_manuf",
          "width": 200,
          "align": "center"
        },
        {
          "label": "合格证状态",
          "prop": "cert_status",
          "width": 200,
          "dict": {
            "dictType": "AR_E_CERT_STATUS",
            "dictKey": [
              "AR_E_CERT_STATUS"
            ],
            "format": "label"
          },
          "align": "center"
        },
        {
          "label": "车辆识别号/车架号",
          "prop": "vehicle_number",
          "width": 200,
          "align": "center"
        },
        {
          "label": "车辆品牌",
          "prop": "vehicle_brand",
          "width": 200,
          "align": "center"
        },
        {
          "label": "车辆型号",
          "prop": "vehicle_model",
          "width": 200,
          "align": "center"
        },
        {
          "label": "车身颜色",
          "prop": "vehicle_color",
          "width": 200,
          "align": "center"
        },
        {
          "label": "车辆制造日期",
          "prop": "manuf_date",
          "width": 200,
          "align": "center"
        },
        {
          "label": "合格证保管时间",
          "width": 200,
          "prop": "cert_keep_date",
          "align": "center"
        },
        {
          "label": "操作",
          "fixed": "right",
          "width": 240,
          "align": "center",
          "btns": [
            {
              "type": "edit",
              "label": "详情",
              "toRequest": {
                "url": "/views/icms/certconf/certconf_edit.json",
                "method": "GET",
                "params": {
                  "apply_no": "${{current.apply_no}}"
                }
              },
              "props": {
                "width": "1200px",
                "title": "合格证详情"
              }
            },
            {
              "type": "edit",
              "label": "轨迹跟踪",
              "toRequest": {
                "url": "@certconf_timeline",
                "method": "GET",
                "params": {
                  "certconf_id":"${{current.certconf_id}}"
                }
              },
              "props": {
                "title": "轨迹跟踪"
              }
            }
          ]
        }
      ]
    }
  }
}
