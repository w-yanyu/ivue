{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initDataOnce": true,
      "initRequest": "${{params}}",
      "btns": [
        {
          "type": "submit",
          "label": "保存",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "ar4001",
              "cert_receiver":"$g{{user_info.userCode}}"
            }
          }
        }
      ],
      "controls": [
        {
          "certconf_id": {
            "control": "string",
            "label": "合格证编号",
            "rules": [
              {
                "max": 64,
                "message": "长度不能超过64个字符"
              }
            ]
          },
          "issue_date": {
              "control": "dateTimePicker",
              "label": "发证日期"
          },
          "vehicle_manuf": {
              "control": "string",
              "label": "车辆制造商",
              "rules": [
                {
                  "max": 64,
                  "message": "长度不能超过64个字符"
                }
              ]
          },
          "vehicle_brand": {
              "control": "string",
              "label": "车辆品牌",
              "rules": [
                {
                  "max": 64,
                  "message": "长度不能超过64个字符"
                }
              ]
          },
          "vehicle_model": {
              "control": "string",
              "label": "车辆型号",
              "rules": [
                {
                  "max": 64,
                  "message": "长度不能超过64个字符"
                }
              ]
          },
          "vehicle_number": {
              "control": "string",
              "label": "车辆识别号/车架号",
              "rules": [
                {
                  "max": 64,
                  "message": "长度不能超过64个字符"
                }
              ]
            },
            "vehicle_color": {
              "control": "string",
              "label": "车身颜色",
              "rules": [
                {
                  "max": 64,
                  "message": "长度不能超过64个字符"
                }
              ]
          },
          "engine_number": {
              "control": "string",
              "label": "发动机编号",
              "rules": [
                {
                  "max": 64,
                  "message": "长度不能超过64个字符"
                }
              ]
            },
            "manuf_date": {
              "control": "dateTimePicker",
              "label": "车辆制造日期"
          },
          "cert_status": {
              "control": "select",
              "label": "合格证状态",
              "dict": {
                "dictType": "AR_E_CERT_STATUS",
                "dictKey": [
                  "AR_E_CERT_STATUS"
                ],
                "labelField": "dictName",
                "valueField": "dictId",
                "format": "label"
              }
            },
            "cert_keep_date": {
              "control": "string",
              "label": "合格证保管时间",
              "rules": [
                {
                  "max": 18,
                  "message": "长度不能超过18个字符"
                }
              ]
          },
          "cert_receiver": {
              "control": "string",
              "label": "合格证保管接收人",
              "rules": [
                {
                  "max": 20,
                  "message": "长度不能超过20个字符"
                }
              ]
            },
            "issuing_time": {
              "control": "dateTimePicker",
             "format": "yyyyMMdd",
              "label": "合格证发放时间",
              "rules": [
                {
                  "required": true,
                  "message": "请输入合格证发放时间"
                }
              ]
          },
          "issuing_per": {
            "control": "lookup",
            "label": "合格证发放人",
            "keyField": {
              "collector": "user_id",
              "issuing_per": "user_name"
            },
            "toRequest": {
              "url": "/views/ce/paramManage/assign/collTeam/user_manager_lookup.json",
              "method": "get"
            },
            "rules": [
              {
                "required": true,
                "message": "请输入合格证发放人"
              }
            ]
            },
            "distr_model": {
              "control": "select",
              "label": "发放方式",
              "dict": {
                "dictType": "AR_E_DISTR_MODEL",
                "dictKey": [
                  "AR_E_DISTR_MODEL"
                ],
                "labelField": "dictName",
                "valueField": "dictId",
                "format": "label"
              },
              "rules": [
                {
                  "required": true,
                  "message": "请输入发放方式"
                }
              ]
          },
          "delivery_num": {
              "control": "text",
              "label": "快递单号",
              "rules": [
                {
                  "required": true,
                  "message": "请输入快递单号"
                },
                {
                  "max": 64,
                  "message": "长度不能超过64个字符"
                }
              ]

            },
            "cert_acce_time": {
              "control": "string",
              "label": "经销商接收时间",
              "rules": [
                {
                  "max": 18,
                  "message": "长度不能超过18个字符"
                }
              ]
          },
          "cert_receives": {
              "control": "string",
              "label": "经销商接收人",
              "rules": [
                {
                  "max": 20,
                  "message": "长度不能超过20个字符"
                }
              ]
            },
            "return_time": {
              "control": "string",
              "label": "合格证退回时间",
              "rules": [
                {
                  "max": 18,
                  "message": "长度不能超过18个字符"
                }
              ]
          },
          "return_per": {
              "control": "string",
              "label": "合格证退回人",
              "rules": [
                {
                  "max": 20,
                  "message": "长度不能超过20个字符"
                }
              ]
            },
            "manuf_time": {
              "control": "string",
              "label": "制造商接收时间",
              "rules": [
                {
                  "max": 18,
                  "message": "长度不能超过18个字符"
                }
              ]
          },
          "manuf_per": {
              "control": "string",
              "label": "制造商接收人",
              "rules": [
                {
                  "max": 20,
                  "message": "长度不能超过20个字符"
                }
              ]
            }
        }
      ]
    }
  }
}
