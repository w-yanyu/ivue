{
    "module": "v_search_btn_datagrid",
    "layout": {
        
        "form": {
            "doRequest": {
                "url": "",
                "method": "POST",
                "params": {}
            },
            "controls": [
                {
                    "user_id": {
                        "control": "text",
                        "label": "用户ID",
                        "rules": [
                            {
                                "max": 32,
                                "message": "长度不能超过32个字符"
                            }
                        ]
                    },
                    "user_name": {
                        "control": "text",
                        "label": "用户姓名",
                        "rules": [
                            {
                                "max": 100,
                                "message": "长度不能超过100个字符"
                            }
                        ]
                    }
                }
                
            ]
        },

        "datagrid": {
            "isInitSearch": true,
            "doRequest": {
                "url": "/SUMP/rest/auth/userManager",
                "method": "POST", 
                "params": {
                    "servicecode": "ct2002"
                    
                }
            },
    
            "thead": [
                {
                    "type": "string",
                    "label": "用户ID",
                    "prop": "user_id",
                    "align": "center"
                },
                {
                    "type": "string",
                    "label": "用户姓名",
                    "prop": "user_name",
                    "align": "center"
                },
                {
                    "type": "string",
                    "label": "机构号",
                    "prop": "branch_id",
                    "align": "center"
                },
                {
                    "type": "string",
                    "label": "用户状态",
                    "prop": "user_status",
                    "dict": {
                        "dictType": "CT_E_TELLERSTAS",
                        "dictKey": [
                            "CT_E_TELLERSTAS"
                        ],
                        "format": "label"
                    },
                    "align": "center"
                },
                {
                    "label": "操作",
                    "align": "center",
                    "btns": [
                        {
                            "type": "bringback",
                            "label": "选择"
                        }
                    ]
                }
            ]
        }
    }
}