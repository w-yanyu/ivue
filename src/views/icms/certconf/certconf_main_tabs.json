{
    "module": "dynamicTabs",
    "layout": {
      "form": {
        "controls": [
            {
                "appId": {
                    "control": "select",
                    "label": "业务模块",
                    "selectFirstValue":true,
                    "dict": {
                        "dictType": "MS_APPID",
                        "dictKey": [
                            "MS_APPID"
                        ],
                        "format": "value-label"
                    },
                    "rules": [
                        {
                            "required": true,
                            "message": "请选择业务模块"
                        }]
                }
              }]
            },
      "tabs": {
        "tabHeader": [
          {
            "id": "tab1",
            "label": "入库保管",
            "closable": false,
            "disabled": false,
            "icon": "el-icon-date",
            "toRequest": {
              "url": "/views/icms/certconf/certconf_process_begin.json",
              "method": "post"
            }
          },
          {
            "id": "tab2",
            "label": "正常申领",
            "icon": "el-icon-date",
            "closable": false,
            "disabled": false,
            "toRequest": {
              "url": "/views/icms/certconf/certconf_zcsl_process.json",
              "method": "post"
            }
          },
          {
            "id": "tab3",
            "label": "提前申领",
            "icon": "el-icon-date",
            "closable": false,
            "disabled": false,
            "toRequest": {
              "url": "/views/icms/certconf/certconf_tqsl_process.json",
              "method": "post"
            }
          },
          {
            "id": "tab4",
            "label": "经销商接收",
            "icon": "el-icon-date",
            "closable": false,
            "disabled": false,
            "toRequest": {
              "url": "/views/icms/certconf/certconf_jxsjs_process.json",
              "method": "post"
            }
          },
          {
            "id": "tab5",
            "label": "合格证退回",
            "icon": "el-icon-date",
            "closable": false,
            "disabled": false,
            "toRequest": {
              "url": "/views/icms/certconf/certconf_hgzth_process.json",
              "method": "post"
            }
          },
          {
            "id": "tab6",
            "label": "制造商接收",
            "icon": "el-icon-date",
            "closable": false,
            "disabled": false,
            "toRequest": {
              "url": "/views/icms/certconf/certconf_zzsjs_process.json",
              "method": "post"
            }
          }
          
        ],
        "defaultActiveKey": "tab1",
        "position": "top",
        "styleClass": "card"
      }
    }
  }
  