{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "certconf_id": {
            "control": "text",
            "label": "合格证编号",
            "rules": [
              {
                "max": 64,
                "message": "长度不能超过64个字符"
              }
            ]
          },
          "vehicle_number": {
            "control": "text",
            "label": "车辆识别号/车架号",
            "rules": [
              {
                "max": 17,
                "message": "长度不能超过17个字符"
              }
            ]
          }
        }
      ]
    },
    "toolbar": {
      "btns": []
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ar4000",
          "appId": "icmsOnl",
          "cert_status":"1",
          "is_use":"0"
        }
      },
      "thead": [
        {
          "type": "index",
          "align": "center"
        },
        {
          "label": "合格证编号",
          "prop": "certconf_id",
          "width": 200,
          "align": "center"
        },
        {
          "label": "发证日期",
          "prop": "issue_date",
          "width": 200,
          "align": "center"
        },
        {
          "label": "车辆制造企业名称",
          "prop": "vehicle_manuf",
          "width": 200,
          "align": "center"
        },
        {
          "label": "车辆品牌",
          "prop": "vehicle_brand",
          "width": 200,
          "align": "center"
        },
        {
          "label": "车辆型号",
          "prop": "vehicle_model",
          "width": 200,
          "align": "center"
        },
        {
          "label": "车辆识别号/车架号",
          "prop": "vehicle_number",
          "width": 200,
          "align": "center"
        },
        {
          "label": "车身颜色",
          "prop": "vehicle_color",
          "width": 200,
          "align": "center"
        },
        {
          "label": "车辆制造日期",
          "prop": "manuf_date",
          "width": 200,
          "align": "center"
        },
        {
          "label": "合格证状态",
          "prop": "cert_status",
          "width": 200,
          "dict": {
            "dictType": "AR_E_CERT_STATUS",
            "dictKey": [
              "AR_E_CERT_STATUS"
            ],
            "format": "label"
          },
          "align": "center"
        },
        {
          "label": "合格证保管时间",
          "width": 200,
          "prop": "cert_keep_date",
          "align": "center"
        },
        {
          "label": "操作",
          "fixed": "right",
          "width": 70,
          "align": "center",
          "btns": [
            {
              "type": "bringback",
              "label": "选择"
            }
          ]
        }
      ]
    }
  }
}
