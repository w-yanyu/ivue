{
    "module": "v_search_btn_datagrid",
    "layout": {
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": true,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "ar1562",
                  "apply_no": "${{params.apply_no}}"

                }
            },
            "thead": [
             {
              "prop": "contract_no",
              "label": "承兑合同号",
              "align": "center"
            },
            {
              "label": "出票人名称",
              "prop": "cust_name",
              "align": "center"
            },
            {
              "label": "出票人结算账号",
              "prop": "settle_acct_no",
              "align": "center"
            },
            {
              "label": "出票人结算账号开户行",
              "prop": "busi_tran_bank_no",
              "align": "center"
            },
            {
              "label": "出票日期",
              "prop": "start_date",
              "align": "center"
            },
            {
              "label": "到期日期",
              "prop": "matu_date",
              "align": "center"
            },
                {
                    "label": "操作",
                    "align": "center",
                    "fixed": "right",
                    "btns": [{
                        "type": "bringback",
                        "label": "选择"
                    }]
                }
            ]
        }
    }
}
