{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "ar1580"
                }
            },
            "controls": [
                {
                    "payee_name": {
                        "control": "lookup",
                        "edit": false,
                        "width": "200px",
                        "span": 8,
                        "rules": [
                            {
                                "required": true,
                                "message": "收款人姓名"
                            }
                        ],
                        "label": "收款人姓名",
                        "keyField": {
                            "payee_acct_no": "payee_acct_no",
                            "payee_bank_name": "payee_bank_name",
                            "payee_bank": "payee_bank",
                            "payee_pk": "payee_pk"
                        },
                        "toRequest": {
                            "url": "/views/icms/accept/araPayeeInfo/info/ara_payee_info_lookup.json",
                            "method": "post",
                            "params": {
                                "apply_no": "${{params.apply_no}}"
                            }
                        },
                        "props": {
                            "width": "80%",
                            "title": "查询收款人"
                        },
                        "format": "label"
                    },
                    "apply_no": {
                        "control": "hidden",
                        "label": "申请号",
                        "width": "200px",
                        "span": 8,
                        "value": "${{params.apply_no}}",
                        "disabled": true
                    },
                   
                    "draft_amt": {
                        "control": "currency",
                        "width": "200px",
                        "decimal": "2",
                        "label": "汇票金额(元) ",
                        "span": 8,
                        "rules": [
                            {
                                "required": true,
                                "message": "此项为必填项"
                            }
                        ],
                        "align": "left",
                        "format": {
                            "thousand": ","
                        }
                    },
                    "payee_pk": {
                        "control": "hidden",
                        "label": "收款人主键",
                        "width": "200px",
                        "span": 8,
                        "disabled": true
                    },
                    "payee_acct_no": {
                        "disabled": true,
                        "width": "200px",
                        "control": "text",
                        "label": "收款人账号",
                        "span": 8
                    },
                    "payee_bank": {
                        "disabled": true,
                        "width": "200px",
                        "control": "text",
                        "label": "收款人开户行行号",
                        "span": 8
                    },
                    "payee_bank_name": {
                        "disabled": true,
                        "width": "200px",
                        "control": "text",
                        "label": "收款人开户行名称",
                        "span": 8
                    },
                    "drawer_name": {

                        "control": "lookup",
                        "edit": false,
                        "width": "200px",
                        "span": 8,
                        "rules": [
                            {
                                "required": true,
                                "message": "选择合同信息"
                            }
                        ],
                        "label": "出票人名称",
                        "keyField": {
                            "drawer_name": "cust_name",
                            "settle_acct_no": "settle_acct_no",
                            "drawer_bank": "busi_tran_bank_no",
                            "drawer_acct_no": "settle_acct_no",
                            "drawer_bank_name": "busi_tran_bank_name",
                            "drawer_start_date": "start_date",
                            "matu_date": "matu_date"
                        },
                        "toRequest": {
                            "url": "/views/icms/accept/araAcceptDraftApply/info/ara_accept_apply_lookup.json",
                            "method": "post",
                            "params": {
                                "apply_no": "${{params.apply_no}}"
                            }
                        },
                        "props": {
                            "width": "80%",
                            "title": "选择出票人"
                        },
                        "format": "label"
                    },
                    "drawer_acct_no": {
                        "disabled": true,
                        "width": "200px",
                        "control": "text",
                        "label": "出票人结算账号",
                        "span": 8
                    },
                    "drawer_bank": {
                        "disabled": true,
                        "width": "200px",
                        "control": "text",
                        "label": "出票人结算账号开户行",
                        "span": 8
                    },
                    "drawer_bank_name": {
                        "disabled": true,
                        "width": "200px",
                        "control": "text",
                        "label": "出票人结算账号开户行名称",
                        "span": 8
                    },

                    "drawer_start_date": {
                        "disabled": true,
                        "width": "200px",
                        "control": "dateTimePicker",
                        "format": "yyyy-MM-dd",
                        "valueFormat": "yyyyMMdd",
                        "label": "出票日期",
                        "span": 8
                    },
                    "matu_date": {
                        "disabled": true,
                        "width": "200px",
                        "control": "dateTimePicker",
                        "format": "yyyy-MM-dd",
                        "valueFormat": "yyyyMMdd",
                        "label": "到期日期",
                        "span": 8
                    }
                }
            ]
        }
    }
}
