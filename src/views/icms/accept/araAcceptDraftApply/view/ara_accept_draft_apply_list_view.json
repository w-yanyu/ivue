{
  "module": "v_search_btn_datagrid",
  "layout": {
    "toolbar": {
      "btns": [
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ar1583",
          "apply_no": "${{params.apply_no}}"
        }
      },
      "thead": [

        {
          "label": "承兑合同号",
          "prop": "contract_no",
          "align": "center"
        },
       
        {
          "label": "汇票编号",
          "prop": "draft_no",
          "align": "center"
        },
        {
          "label": "出票人名称",
          "prop": "drawer_name",
          "align": "center"
        },

        {
          "label": "汇票金额",
          "prop": "draft_amt",
          "align": "center"
        },
        {
          "label": "出票日期",
          "prop": "drawer_start_date",
          "align": "center"
        },
        {
          "label": "到期日期",
          "prop": "matu_date",
          "align": "center"
        },
        {
          "label": "批次号",
          "prop": "batch_no",
          "align": "center"
        },
        {
          "label": "操作",
          "align": "center",
          "fixed": "right",
          "width": 180,
          "btns": [
            {
              "type": "look",
              "label": "详情",
              "toRequest": {
                "url": "/views/icms/accept/araAcceptDraftApply/info/ara_accept_draft_apply_detail.json",
                "method": "get"
              },
              "props": {
                "width": "75%",
                "title": "查看汇票信息"
              }
            }
          ]
        }
      ]
    }
  }
}