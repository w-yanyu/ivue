{
    "module": "dynamicTabs",
    "layout": {
      "tabs": {
        "scope": "templateScope",
        "#initDataOnce":true,
        "tabHeader": [
          {
            "id": "dd1",
            "clickInit": true,
            "label": "协议申请",
            "icon": "el-icon-date",
            "closable": false,
            "disabled": false,
            "toRequest":{
              "url": "/views/icms/accept/creditAccept/view/accept_contract_info_view.json",
              "method": "post",
              "params": {
                "apply_no": "${{params.apply_no}}"
              }
            }
          },
          {
            "id": "dd2",
            "clickInit": true,
            "label": "收款人",
            "icon": "el-icon-date",
            "closable": false,
            "disabled": false,
            "toRequest":{
              "url": "/views/icms/accept/araPayeeInfo/view/ara_payee_info_list_view.json",
              "method": "post",
              "params": {
                "apply_no": "${{params.apply_no}}"
              }
            }
          }

        ],
        "defaultActiveKey": "dd1",
        "position": "top",
        "styleClass": "card"
      }
    }
  }
