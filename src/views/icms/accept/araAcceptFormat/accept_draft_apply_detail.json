{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "scope": "templateScope",
            "initDataOnce": true,
            "initRequest": "${{params}}",
            "btns": [
            ],
            "controls": [
                {
                    "payee_name": {
                        "control": "text",
                        "disabled": true,
                        "width": "200px",
                        "span": 8,
                        "label": "收款人姓名",
                        "format": "label"
                    },
                    "apply_no": {
                        "control": "hidden",
                        "label": "申请号",
                        "width": "200px",
                        "span": 8,
                        "value": "${{params.apply_no}}",
                        "disabled": true
                    },
                    "draft_amt": {
                        "disabled": true,
                        "control": "currency",
                        "decimal": "2",
                        "label": "汇票金额(元) ",
                        "width": "200px",
                        "span": 8,
                        "align": "left",
                        "format": {
                            "thousand": ","
                        }
                    },
                    "payee_acct_no": {
                        "disabled": true,
                        "control": "text",
                        "label": "收款人账号",
                        "width": "200px",
                        "span": 8
                    },
                    "payee_bank": {
                        "disabled": true,
                        "control": "text",
                        "label": "收款人开户行行号",
                        "width": "200px",
                        "span": 8
                    },
                    "payee_bank_name": {
                        "disabled": true,
                        "control": "text",
                        "label": "收款人开户行名称",
                        "width": "200px",
                        "span": 8
                    },
                    "drawer_name": {
                        "disabled": true,
                        "control": "text",
                        "edit": true,
                        "width": "200px",
                        "span": 8,
                        "label": "出票人名称"
                        
                    },
                    "drawer_acct_no": {
                        "disabled": true,
                        "control": "text",
                        "label": "出票人结算账号",
                        "width": "200px",
                        "span": 8
                    },
                    "drawer_bank": {
                        "disabled": true,
                        "control": "text",
                        "label": "出票人结算账号开户行",
                        "width": "200px",
                        "span": 8
                    },
                    "drawer_bank_name": {
                        "disabled": true,
                        "control": "text",
                        "label": "出票人结算账号开户行名称",
                        "width": "200px",
                        "span": 8
                    },
                    
                    "drawer_start_date": {
                        "disabled": true,
                        "control": "dateTimePicker",
                        "format": "yyyy-MM-dd",
                        "valueFormat": "yyyyMMdd",
                        "label": "出票日期",
                        "width": "200px",
                        "span": 8
                    },
                    "matu_date": {
                        "disabled": true,
                        "control": "dateTimePicker",
                        "format": "yyyy-MM-dd",
                        "valueFormat": "yyyyMMdd",
                        "label": "到期日期",
                        "width": "200px",
                        "span": 8
                    }
                }
            ]
        }
    }
}