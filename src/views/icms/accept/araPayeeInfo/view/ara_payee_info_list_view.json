{
  "module": "v_search_btn_datagrid",
  "layout": {
    "toolbar": {

    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ar1550",
          "apply_no": "${{params.apply_no}}"
        }
      },
      "thead": [
        {
          "prop": "apply_no",
          "label": "申请号",
          "align": "center"
        },
        {
          "label": "收款人名称",
          "prop": "payee_name",
          "align": "center"
        },
        {
          "label": "签票总额",
          "prop": "check_total_amt",
          "align": "center"
        },
        {
          "label": "收款人账号",
          "prop": "payee_acct_no",
          "align": "center"
        },
        {
          "label": "收款人开户行名称",
          "prop": "payee_bank_name",
          "align": "center"
        },
        {
          "label": "收款人开户行行号",
          "prop": "payee_bank",
          "align": "center"
        },
        {
          "label": "收款人开户行地址",
          "prop": "payee_bank_addr",
          "align": "center"
        },
        {
          "label": "操作",
          "align": "center",
          "btns": [
            {
              "type": "look",
              "label": "详情",
              "toRequest": {
                "url": "/views/icms/accept/araPayeeInfo/info/ara_payee_info_detail.json",
                "method": "get"
              },
              "props": {
                "width": "65%",
                "title": "收款人信息详情"
              }
            }
          ]
        }
      ]
    }
  }
}