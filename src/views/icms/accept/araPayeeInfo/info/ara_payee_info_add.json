{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "ar1551"
                }
            },
            "controls": [
                {
                    "payee_name": {
                        "control": "text",
                        "label": "收款人名称",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "收款人名称"
                            },
                            {
                                "max": 64,
                                "message": "长度不能超过64个字符"
                            }
                        ]
                    },
                    "check_total_amt": {
                        "disabled": true,
                        "control": "currency",
                        "label": "签票金额(由购销合同自动合计)：",
                        "width": "130px",
                        "align": "left",
                        "format": {
                            "thousand": ","
                        }
                    },
                    "apply_no": {
                        "control": "hidden",
                        "label": "申请号",
                        "width": "130px",
                        "value": "${{params.apply_no}}",
                        "disabled": true
                    }
                },
                {
                    "payee_acct_no": {
                        "control": "text",
                        "label": "收款人账号",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "收款人账号"
                            },
                            {
                                "max": 64,
                                "message": "长度不能超过64个字符"
                            }
                        ]
                    },
                    "payee_bank_name": {
                        "control": "text",
                        "label": "收款人开户行名称",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "收款人开户行名称"
                            },
                            {
                                "max": 64,
                                "message": "长度不能超过64个字符"
                            }
                        ]
                    }
                },
                {
                    "payee_bank": {
                        "control": "text",
                        "label": "收款人开户行行号",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "收款人开户行行号"
                            },
                            {
                                "max": 64,
                                "message": "长度不能超过64个字符"
                            }
                        ]
                    },
                    "payee_bank_addr": {
                        "control": "text",
                        "label": "收款人开户行地址",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "收款人开户行地址"
                            },
                            {
                                "max": 64,
                                "message": "长度不能超过64个字符"
                            }
                        ]
                    }
                }
            ]
        }
    }
}