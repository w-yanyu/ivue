{
    "module": "v_search_btn_datagrid",
    "layout": {
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": true,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "ar1550",
                  "apply_no": "${{params.apply_no}}"

                }
            },
            "thead": [
             {
              "prop": "apply_no",
              "label": "申请号",
              "align": "center"
            },
            {
              "label": "收款人名称",
              "prop": "payee_name",
              "align": "center"
            },
            {
              "label": "收款人账号",
              "prop": "payee_acct_no",
              "align": "center"
            },
            {
              "label": "收款人开户行名称",
              "prop": "payee_bank_name",
              "align": "center"
            },
            {
              "label": "收款人开户行行号",
              "prop": "payee_bank",
              "align": "center"
            },
            {
              "label": "收款人开户行地址",
              "prop": "payee_bank_addr",
              "align": "center"
            },
                {
                    "label": "操作",
                    "align": "center",
                    "fixed": "right",
                    "btns": [{
                        "type": "bringback",
                        "label": "选择"
                    }]
                }
            ]
        }
    }
}
