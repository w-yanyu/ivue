{
  "module": "dynamicTabs",
  "layout": {
    "tabs": {
      "scope": "templateScope",
      "#initDataOnce":true,
      "tabHeader": [
        {
          "id": "dd1",
          "scope": "baseInfo",
          "clickInit": true,
          "label": "协议申请",
          "closable": false,
          "disabled": false,
          "icon": "el-icon-date",
          "toRequest":{
            "url":"/views/icms/accept/creditAccept/info/accept_contract_main.json",
            "apply_no": "${{params.apply_no}}",
            "prod_cd": "${{params.prod_cd}}",
            "method": "post"
          }

        },
        {
          "id": "dd2",
          "clickInit": true,
          "label": "收款人",
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "toRequest":{
            "url": "/views/icms/accept/araPayeeInfo/info/ara_payee_info_list.json",
            "method": "post",
            "params": {
              "apply_no": "${{params.apply_no}}",
              "pageType": "detail"
            }
          }
        }
      ],
      "defaultActiveKey": "dd1",
      "position": "top",
      "styleClass": "card"
    }
  }
}
