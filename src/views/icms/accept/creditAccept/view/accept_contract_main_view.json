{
  "module": "v-form-btn-dynamic",
  "layout": {
    "form": {
      "initRequest":"${{params}}",
      "btns":[],
      "controlsGroup": [
        {
          "title": "授信信息",
          "tooltip": "step1",
          "titleIcon": "",
          "showHint": "",
          "accordion": false,
          "expand": true,
          "controls": [
            {
              "id1": {
                "control": "dynamic",
                "span": 24,
                "toRequest": {
                  "url": "/views/icms/accept/creditAccept/creditInfo/credit_info.json",
                  "method": "post",
                  "params": {
                    "apply_no":"${{params.apply_no}}",
                    "credit_prod_cd":"${{params.prod_cd}}"
                  }
                }
              }
            }
          ]
        },
        {
          "title": "承兑申请信息查看",
          "tooltip": "step2",
          "expand": true,
          "controls": [
            {
              "id2": {
                "control": "dynamic",
                "span": 24,
                "toRequest": {
                  "url": "/views/icms/accept/creditAccept/view/accept_contract_info_view.json",
                  "method": "get",
                  "params": {
                    "apply_no":"${{params.apply_no}}",
                    "credit_prod_cd":"${{params.prod_cd}}"
                  }
                }
              }
            }
          ]
        }
      ]
    }
  }
}
