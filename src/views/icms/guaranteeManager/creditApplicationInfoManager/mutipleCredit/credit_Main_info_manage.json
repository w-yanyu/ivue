{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "controlsGroup": [
        {
          "title": "授信产品配置",
          "tooltip": "授信产品配置",
          "titleIcon": "el-icon-info",
          "expand": true,
          "accordion": false,
          "controls": [
            {
              "list03": {
                "span": 24,
                "control": "dynamic",
                "toRequest": {
                  "url": "/views/icms/guaranteeManager/creditApplicationInfoManager/mutipleCredit/credit_Main_Money_info.json",
                  "params": {
                    "prod_name": "${{params.prod_name}}",
                    "cust_no": "${{params.cust_no}}",
                    "loan_prod_cd": "${{params.loan_prod_cd}}",
                    "base_ccy": "${{params.base_ccy}}",
                    "ccy_code": "${{params.ccy_code}}",
                    "exchange_rate": "${{params.exchange_rate}}",
                    "cycling_flag": "${{params.cycling_flag}}",
                    "credit_term": "${{params.credit_term}}",
                    "busi_assign_amt": "${{params.busi_assign_amt}}",
                    "base_busi_assign_amt": "${{params.base_busi_assign_amt}}",
                    "remark": "${{params.remark}}"
                  }
                }
              }
            }
          ]
        },
        {
          "title": "适用产品选择",
          "tooltip": "适用产品选择",
          "titleIcon": "el-icon-info",
          "expand": true,
          "accordion": false,
          "controls": [
            {
              "list01": {
                "span": 24,
                "control": "dynamic",
                "toRequest": {
                  "url": "@creditMainSaleProdRelaInfo",
                  "params": {
                    "apply_no": "${{params.apply_no}}",
                    "credit_prod_cd": "${{params.loan_prod_cd}}",
                    "cust_no": "${{params.cust_no}}",
                    "ccy_code": "${{params.ccy_code}}"
                  }
                }
              }
            }
          ]
        }
      ]
    }
  }
}

