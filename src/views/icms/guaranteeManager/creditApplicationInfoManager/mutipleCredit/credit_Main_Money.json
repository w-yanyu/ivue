{
  "module": "v_search_btn_datagrid",
  "layout": {
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "target": "dialog",
          "label": "新增",
          "toRequest": {
            "method": "GET",
            "url": "/views/icms/guaranteeManager/creditApplicationInfoManager/mutipleCredit/credit_Main_Money_add.json",
            "params": {
              "apply_no": "${{params.apply_no}}",
              "credit_prod_cd": "${{params.credit_prod_cd}}",
              "cust_no": "${{params.cust_no}}"
            }
          },
          "props": {
            "width": "75%",
            "title": "授信业务产品明细"
          },
          "show": {
            "expression": "'${{params.pageType}}' != 'detail'"
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ar0782",
          "apply_no": "${{params.apply_no}}"
        }
      },
      "thead": [
        {
          "label": "授信产品编号",
          "prop": "loan_prod_cd",
          "align": "center"
        },
        {
          "label": "授信产品名称",
          "prop": "prod_name",
          "align": "center"
        },
        {
          "label": "业务期限",
          "prop": "credit_term",
          "align": "center"
        },
        {
          "label": "是否循环",
          "prop": "cycling_flag",
          "align": "center",
          "dict": {
            "dictType": "MS_E_YESORNO",
            "dictKey": [
              "MS_E_YESORNO"
            ],
            "format": "label"
          }
        },
        {
          "label": "授信币种",
          "prop": "ccy_code",
          "align": "center",
          "dict": {
            "dictType": "MS_CURRENCY",
            "dictKey": [
              "MS_CURRENCY"
            ],
            "format": "label"
          }
        },
        {
          "label": "汇率",
          "prop": "exchange_rate",
          "align": "center"
        },
        {
          "label": "业务授信金额",
          "prop": "busi_assign_amt",
          "align": "center",
          "format":{
            "thousand": ","
          }
        },
        {
          "label": "授信可用金额",
          "prop": "avail_amt",
          "align": "center",
          "format":{
            "thousand": ","
          }
        },
        {
          "label": "操作",
          "fixed": "right",
          "width": "180px",
          "align": "center",
          "btns": [
            {
              "type": "look",
              "label": "详情",
              "toRequest": {
                "url": "/views/icms/guaranteeManager/creditApplicationInfoManager/mutipleCredit/credit_Main_info_manage.json",
                "method": "POST",
                "params": {
                  "apply_no": "${{params.apply_no}}",
                  "credit_prod_cd": "${{params.credit_prod_cd}}"
                }
              },
              "props": {
                "width": "75%",
                "title": "授信业务产品明细详情"
              }
            },
            {
              "type": "edit",
              "label": "维护",
              "toRequest": {
                "url": "/views/icms/guaranteeManager/creditApplicationInfoManager/mutipleCredit/credit_Main_Money_update_manage.json",
                "method": "GET",
                "params": {
                  "apply_no": "${{params.apply_no}}",
                  "credit_prod_cd": "${{params.credit_prod_cd}}"
                }
              },
              "props": {
                "width": "75%",
                "title": "授信业务产品明细维护"
              },
              "show": {
                "expression": "'${{params.pageType}}' != 'detail'"
              }
            },
            {
              "type": "delete",
              "label": "删除",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "ar0783"
                }
              },
              "confirm": {
                "title": "确定删除",
                "confirmButtonText": "确定",
                "cancelButtonText": "取消"
              },
              "show": {
                "expression": "'${{params.pageType}}' != 'detail'"
              }
            }
          ]
        }
      ]
    }
  }
}
