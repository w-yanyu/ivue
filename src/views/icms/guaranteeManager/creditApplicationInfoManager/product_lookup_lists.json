{
    "module": "v_search_btn_datagrid",
    "layout": {
      "form": {
        "controls": [
          {
            "sale_prod_cd": {
              "control": "text",
              "label": "可售产品编号"
            },
            "sale_prod_name": {
              "control": "text",
              "label": "可售产品名称"
            }
          }
        ]
      },
      "datagrid": {
        "scope": "searchTable",
        "isInitSearch": true,
        "doRequest": {
          "url": "/SUMP/icmcall/icmRPCCall",
          "method": "POST",
          "params": {
            "servicecode": "pd0329",
            "credit_prod_cd": "${{params.credit_prod_cd}}"
          }
        },
        "thead": [
          {
            "type": "string",
            "label": "可售产品名称",
            "prop": "sale_prod_name",
            "align": "center"
          },
          {
            "type": "string",
            "label": "可售产品编号",
            "prop": "sale_prod_cd",
            "align": "center"
          },
          {
            "type": "string",
            "label": "可售产品类型",
            "prop": "prod_type_name",
            "align": "center"
          },
          {
            "label": "操作",
            "align": "center",
            "width": "200px",
            "btns": [
              {
                "type": "bringback",
                "label": "选择"
              }
            ]
          }
        ]
      }
    }
  }
