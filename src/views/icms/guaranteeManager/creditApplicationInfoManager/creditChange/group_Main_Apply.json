{
     "module": "v-form-btn-dynamic",
    "layout": {
      "form": {
        "initRequest":"${{params}}",
        "btns":[],
        "controlsGroup": [
          {
            "title": "授信主信息",
            "tooltip": "step1",
            "titleIcon": "",
            "showHint": "",
            "accordion": false,
            "expand": true,
            "controls": [
              {
                "id1": {
                  "control": "dynamic",
                  "span": 24,
                  "toRequest": {
                    "url": "/views/icms/guaranteeManager/creditApplicationInfoManager/creditChange/credit_Main_Allocation.json",
                    "method": "post",
                    "params": {
                      "apply_no":"${{params.apply_no}}",
                      "credit_prod_cd":"${{params.prod_cd}}"
                    }
                  }
                }
              }
            ]
          },
          {
            "title": "授信金额分配",
            "tooltip": "step2",
            "expand": true,
            "controls": [
              {
                "id2": {
                  "control": "dynamic",
                  "span": 24,
                  "toRequest": {
                    "url": "/views/icms/guaranteeManager/creditApplicationInfoManager/creditChange/credit_Main_Money.json",
                    "method": "get",
                    "params": {
                        "apply_no":"${{params.apply_no}}",
                        "cust_id":"${{params.cust_id}}",
                        "credit_prod_cd":"${{params.prod_cd}}"
                    }
                  }
                }
              }
            ]
          }
        ]
      }
    }
  }
