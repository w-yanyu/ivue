{
    "module": "v_search_btn_datagrid",
    "layout": {
        
        "datagrid": {
            "isInitSearch": true,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "ar1405",
                    "apply_no": "${{params.apply_no}}"
                }
            },
            "thead": [
                {
                    "label": "可售产品编号",
                    "prop": "loan_prod_cd",
                    "align": "center"
                },
                {
                    "label": "可售产品名称",
                    "prop": "prod_name",
                    "align": "center"
                },
                {
                    "label": "业务期限",
                    "prop": "credit_term",
                    "align": "center"
                },
                {
                    "label": "是否循环",
                    "prop": "cycling_flag",
                    "align": "center",
                    "dict": {
                        "dictType": "MS_E_YESORNO",
                        "dictKey": [
                            "MS_E_YESORNO"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "授信币种",
                    "prop": "ccy_code",
                    "align": "center",
                    "dict": {
                        "dictType": "MS_CURRENCY",
                        "dictKey": [
                            "MS_CURRENCY"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "汇率",
                    "prop": "exchange_rate",
                    "align": "center"
                },
                {
                    "label": "业务授信金额",
                    "prop": "busi_assign_amt",
                    "align": "center",
                    "format":{
                        "thousand": ","
                    }
                },
                {
                    "label": "授信可用金额",
                    "prop": "avail_amt",
                    "align": "center",
                    "format":{
                      "thousand": ","
                    }
                },
                {
                    "label": "操作",
                    "fixed": "right",
                    "width": "180px",
                    "align": "center",
                    "btns": [
                        {
                            "type": "look",
                            "label": "详情",
                            "toRequest": {
                                "url": "/views/icms/guaranteeManager/creditApplicationInfoManager/creditChange/credit_Main_Money_info.json",
                                "method": "POST"
                            },
                            "props": {
                                "width": "75%",
                                "title": "授信业务产品明细详情"
                            }
                        }
                    ]
                }
            ]
        }
    }
}