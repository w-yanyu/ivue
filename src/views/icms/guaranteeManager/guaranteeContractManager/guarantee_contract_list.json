{
  "module": "dynamicTabs",
  "layout": {
    "tabs": {
      "scope": "templateScope",
      "initDataOnce": true,
      "initRequest": "${{params}}",
      "tabHeader": [
        {
          "id": "tab1",
          "clickInit": true,
          "label": "抵押合同申请",
          "closable": false,
          "disabled": false,
          "icon": "el-icon-date",
          "toRequest": {
            "url": "/views/icms/guaranteeManager/guaranteeContractManager/coltContractApply/colt_contract_apply_list.json",
            "params": {
              "guar_type": "2",
              "guar_contract_type": "0",
              "addTips":"添加抵押合同",
              "editTips":"修改抵押合同",
              "lookTips":"抵押合同详情",
              "importTips":"导入合同"
            }
          }
        },
        {
          "id": "tab2",
          "clickInit": true,
          "label": "质押合同申请",
          "closable": false,
          "disabled": false,
          "toRequest": {
            "url": "/views/icms/guaranteeManager/guaranteeContractManager/coltContractApply/colt_contract_apply_list.json",
            "params": {
              "guar_type": "3",
              "guar_contract_type": "1",
              "addTips":"添加质押合同",
              "editTips":"修改质押合同",
              "lookTips":"质押合同详情",
              "importTips":"导入合同"
            }
          }
        },
        {
          "id": "tab3",
          "clickInit": true,
          "label": "保证合同申请",
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "toRequest": {
            "url": "/views/icms/guaranteeManager/guaranteeContractManager/guarContractApply/guar_contract_apply_manager.json",
            "method": "post",
            "params": {
              "guar_contract_type": "2",
              "addTips":"添加保证合同",
              "editTips":"修改保证合同",
              "lookTips":"保证合同详情",
              "importTips":"导入合同"
            }
          }
        },
        {
          "id": "tab4",
          "clickInit": true,
          "label": "保证金合同申请",
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "toRequest": {
            "url": "/views/icms/guaranteeManager/guaranteeContractManager/marginContractApply/margin_contract_apply_manager.json",
            "method": "post",
            "params": {
              "guar_contract_type": "3",
              "addTips":"添加保证合同",
              "editTips":"修改保证合同",
              "lookTips":"保证合同详情",
              "importTips":"导入合同"
            }
          }
        },
        {
          "id": "tab5",
          "label": "信用申请",
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "toRequest": {
            "url": "/views/icms/guaranteeManager/guaranteeContractManager/creditContractApply/credit_contract_apply_info.json",
            "method": "post",
            "params": {
            }
          }
        }
      ],
      "defaultActiveKey": "tab1",
      "position": "top",
      "styleClass": "card"
    }
  }
}