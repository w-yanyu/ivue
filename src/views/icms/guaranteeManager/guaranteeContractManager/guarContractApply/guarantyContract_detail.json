{
    "module": "v-inputDataForm-btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "btns": [],
            "controlsGroup": [
                {
                    "title": "保证合同信息",
                    "tooltip": "step1",
                    "titleIcon": "",
                    "showHint": "",
                    "accordion": false,
                    "expand": true,
                    "controls": [
                        {
                            "id1": {
                                "control": "dynamic",
                                "span": 24,
                                "toRequest": {
                                    "url": "/views/us/usManager/creditApplicationInterface/guaranteeContract_view.json",
                                    "method": "get",
                                    "params": {
                                        "guar_rule": "${{params.guar_rule}}",
                                        "guar_modality": "${{params.guar_modality}}",
                                        "ccy_cd": "${{params.ccy_cd}}",
                                        "sign_date": "${{params.sign_date}}",
                                        "guar_start_date": "${{params.guar_start_date}}",
                                        "guar_end_date": "${{params.guar_end_date}}",
                                        "guar_amt": "${{params. guar_amt}}",
                                        "contract_base_amt": "${{params.contract_base_amt}}",
                                        "this_use_amt": "${{params.this_use_amt}}",
                                        "base_this_use_amt": "${{params.base_this_use_amt}}"
                                    }
                                }
                            }
                        }
                    ]
                },
                {
                    "title": "已关联的保证人",
                    "tooltip": "step2",
                    "titleIcon": "",
                    "showHint": "",
                    "expand": true,
                    "controls": [
                        {
                            "id2": {
                                "control": "dynamic",
                                "span": 24,
                                "toRequest": {
                                    "url": "/views/us/usManager/creditApplicationInterface/guarantor_view.json",
                                    "method": "get",
                                    "params": {
                                        "apply_no": "${{params.apply_no}}",
                                        "guar_ctr_ppsl_id":"${{params.guar_ctr_ppsl_id}}"
                                        
                                    }
                                }
                            }
                        }
                    ]
                }
            ]
        }
    }
}