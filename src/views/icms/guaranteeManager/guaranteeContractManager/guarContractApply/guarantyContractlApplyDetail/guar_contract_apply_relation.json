{
    "module": "v_search_btn_datagrid",
    "layout": {
        "datagrid": {
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "post",
                "params": {
                    "servicecode": "ar0725",
                    "apply_no": "${{params.apply_no}}",
                    "query_type": "1",
                    "guar_contract_no": "${{params.guar_contract_no}}"
                }
            },
            "thead": [
                {
                    "label": "保证人编号",
                    "prop": "appt_no",
                    "align": "center"
                },
                {
                    "label": "保证人名称",
                    "prop": "cust_name",
                    "align": "center"
                },
                {
                    "label": "保证金额",
                    "prop": "guar_amt",
                    "align": "center",
                    "format":{
                      "thousand": ","
                    }
                },
                {
                    "label": "币种",
                    "prop": "guar_ccy",
                    "align": "center",
                    "dict": {
                        "dictType": "MS_CURRENCY",
                        "dictKey": [
                            "MS_CURRENCY"
                        ],
                        "format": "label"
                    }
                }
            ]
        }
    }
}