{
    "module": "v-inputDataForm-btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "btns": [],
            "name": "myForm",
            "controls": [
                {
                    "guar_rule": {
                        "control": "select",
                        "label": "担保规则",
                        "span": 7,
                        "dict": {
                            "dictType": "AR_E_GUAR_RULE",
                            "dictKey": [
                                "AR_E_GUAR_RULE"
                            ],
                            "format": "label"
                        },
                        "disabled": true
                    },
                    "guar_modality": {
                        "control": "select",
                        "label": "担保形式",
                        "span": 7,
                        "dict": {
                            "dictType": "AR_E_GUAR_CATG",
                            "dictKey": [
                                "AR_E_GUAR_CATG"
                            ],
                            "format": "label"
                        },
                        "disabled": true
                    },
                    "ccy_code": {
                        "control": "select",
                        "label": "币种",
                        "span": 7,
                        "dict": {
                            "dictType": "MS_CURRENCY",
                            "dictKey": [
                                "MS_CURRENCY"
                            ],
                            "format": "label"
                        },
                        "disabled": true
                    }
                },
                {
                    "contract_amt": {
                        "control": "currency",
                        "label": "保证合同金额(元)",
                        "span": 7,
                        "disabled": true,
                        "align": "left",
                        "format":{
                          "thousand": ","
                        }
                    },
                    "exchange_rate": {
                        "control": "hidden",
                        "label": "折算汇率",
                        "span": 7,
                        "disabled": true
                    },
                    "contract_base_amt": {
                        "control": "currency",
                        "label": "合同金额(折算基准币种)",
                        "span": 7,
                        "####compute": "${{params.contract_amt}}*${{params.exchange_rate}}",
                        "value": "${{params.contract_amt}}",
                        "disabled": true,
                        "format":{
                            "thousand":","
                          }
                    },
                    "this_use_amt": {
                        "control": "currency",
                        "label": "本次使用金额(元)",
                        "span": 7,
                        "disabled": true,
                        "format":{
                            "thousand":","
                          }
                    }
                },
                {

                    "base_this_use_amt": {
                        "control": "currency",
                        "label": "本次使用金额(折算基准币种)",
                        "span": 7,
                        "####compute": "${{params.this_use_amt}}*${{params.exchange_rate}}",
                        "value": "${{params.this_use_amt}}",
                        "disabled": true,
                        "format":{
                            "thousand":","
                          }
                    },
                    "sign_date": {
                        "control": "dateTimePicker",
                        "label": "合同签订日期",
                        "span": 7,
                        "disabled": true
                    },
                    "guar_start_date": {
                        "control": "dateTimePicker",
                        "label": "合同起始日期",
                        "span": 7,
                        "disabled": true
                    }
                },
                {
                    "guar_end_date": {
                        "control": "dateTimePicker",
                        "label": "合同到期日期",
                        "span": 7,
                        "disabled": true
                    }
                }
            ]
        }
    }
}