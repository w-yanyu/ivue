{
    "module": "v_search_btn_datagrid",
    "layout": {
        "datagrid": {
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "post",
                "params": {
                    "servicecode": "ar0343",
                    "apply_no": "${{params.apply_no}}",
                    "colt_contract_no": "${{params.colt_contract_no}}",
                    "guar_type": "${{params.guar_type}}",
                    "type": "${{params.type}}"
                }
            },
            "thead": [
                {
                    "label": "抵质押品编号",
                    "prop": "colt_no",
                    "align": "center"
                },
                {
                    "label": "抵质押品种类",
                    "prop": "colt_type",
                    "align": "center"
                },
                {
                    "label": "抵质押品种类名称",
                    "prop": "colt_type_name",
                    "align": "center"
                },
                {
                    "label": "抵质押品产品",
                    "prop": "colt_prod_cd",
                    "align": "center"
                },
                {
                    "label": "抵质押品产品名称",
                    "prop": "colt_prod_name",
                    "align": "center"
                },
                {
                    "label": "抵质押金额",
                    "prop": "colt_amount",
                    "align": "center",
                    "format":{
                      "thousand": ","
                    }
                }
            ]
        }
    }
}