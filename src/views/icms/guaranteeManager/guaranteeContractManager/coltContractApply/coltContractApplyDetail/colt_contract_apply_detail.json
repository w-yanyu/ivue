{
    "module": "v-inputDataForm-btn",
    "layout": {
      "form": {
        "initRequest":"${{params}}",
        "btns":[],
        "controlsGroup": [
          {
            "title": "合同申请信息",
            "tooltip": "step1",
            "titleIcon": "",
            "showHint": "",
            "accordion": false,
            "expand": true,
            "controls": [
              {
                "id1": {
                  "control": "dynamic",
                  "span": 24,
                  "toRequest": {
                    "url": "/views/icms/guaranteeManager/guaranteeContractManager/coltContractApply/coltContractApplyDetail/colt_contract_apply_info.json",
                    "method": "get",
                    "params": {
                      "guar_rule":"${{params.guar_rule}}",
                      "guar_cust_no":"${{params.guar_cust_no}}",
                      "dispute_way":"${{params.dispute_way}}",
                      "ccy_code":"${{params.ccy_code}}",
                      "base_ccy":"${{params.base_ccy}}",
                      "exchange_rate":"${{params.exchange_rate}}",
                      "contract_amt":"${{params.contract_amt}}",
                      "contract_base_amt":"${{params.contract_base_amt}}",
                      "is_part_out":"${{params.is_part_out}}",
                      "this_use_amt":"${{params.this_use_amt}}",
                      "base_this_use_amt":"${{params.base_this_use_amt}}",
                      "pledged_type":"${{params.pledged_type}}",
                      "sign_date":"${{params.sign_date}}",
                      "guar_start_date":"${{params.guar_start_date}}",
                      "guar_end_date":"${{params.guar_end_date}}"
                    }
                  }
                }
              }
            ]
          },
          {
            "title": "已关联的抵质押品",
            "tooltip": "step2",
            "titleIcon": "",
            "showHint": "",
            "expand": true,
            "controls": [
              {
                "id2": {
                  "control": "dynamic",
                  "span": 24,
                  "toRequest": {
                    "url": "/views/icms/guaranteeManager/guaranteeContractManager/coltContractApply/coltContractApplyDetail/colt_contract_apply_relation.json",
                    "method": "get",
                    "params": {
                        "apply_no":"${{params.apply_no}}",
                        "colt_contract_no":"${{params.colt_contract_no}}",
                        "guar_type":"${{params.guar_type}}",
                        "type":"2"
                    }
                  }
                }
              }
            ]
          }
        ]
      }
    }
  }
