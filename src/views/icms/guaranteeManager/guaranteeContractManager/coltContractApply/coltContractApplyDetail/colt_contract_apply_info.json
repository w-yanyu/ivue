{
    "module": "v-inputDataForm-btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "btns": [],
            "name":"myForm",
            "controls": [
                {
                    "guar_rule": {
                        "control": "select",
                        "label": "担保规则",
                        "span": 7,
                        "dict": {
                            "dictType": "AR_E_GUAR_RULE",
                            "dictKey": [
                                "AR_E_GUAR_RULE"
                            ],
                            "format": "label"
                        },
                        "disabled": true
                    },
                    "guar_cust_no": {
                        "control": "text",
                        "label": "抵/质押人",
                        "span": 7,
                        "disabled": true
                    },
                    "dispute_way": {
                        "control": "select",
                        "label": "纠纷解决方式",
                        "span": 7,
                        "dict": {
                            "dictType": "AR_E_DISPUTE_WAY",
                            "dictKey": [
                                "AR_E_DISPUTE_WAY"
                            ],
                            "format": "label"
                        },
                        "disabled": true
                    }
                },
                {
                    "base_ccy": {
                        "control": "select",
                        "label": "基准币种",
                        "span": 7,
                        "dict": {
                            "dictType": "MS_CURRENCY",
                            "dictKey": [
                                "MS_CURRENCY"
                            ],
                            "format": "label"
                        },
                        "disabled": true
                    },
                    "ccy_code": {
                        "control": "select",
                        "label": "币种",
                        "span": 7,
                        "dict": {
                            "dictType": "MS_CURRENCY",
                            "dictKey": [
                                "MS_CURRENCY"
                            ],
                            "format": "label"
                        },                      
                        "disabled": true
                    },
              
                    "exchange_rate": {
                        "control": "string",
                        "label": "折算汇率",
                        "span": 7,
                        "disabled": true
                    }
                },
                {
                    "contract_amt": {
                        "control": "currency",
                        "label": "合同金额(元)",
                        "span": 7,
                        "disabled": true
                    },
                    "contract_base_amt": {
                        "control": "currency",
                        "label": "合同金额(折算基准币种)",
                        "span": 7,
                        "compute": "${{params.contract_amt}}*${{params.exchange_rate}}",
                        "disabled": true
                    },
                    "is_part_out": {
                        "control": "select",
                        "label": "是否允许押品部分出库",
                        "span": 7,
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        },
                        "disabled": true
                    }
                },
                {
                    "this_use_amt": {
                        "control": "currency",
                        "label": "本次使用金额(元)",
                        "span": 7,
                        "disabled": true
                    },
                    "base_this_use_amt": {
                        "control": "currency",
                        "label": "本次使用金额(折算基准币种)",
                        "span": 7,
                        "compute": "${{params.this_use_amt}}*${{params.exchange_rate}}",
                        "disabled": true
                    },
                    "pledged_type": {
                        "control": "select",
                        "label": "抵押担保类型",
                        "span": 7,
                        "dict": {
                            "dictType": "AR_E_PLEDGED_TYPE",
                            "dictKey": [
                                "AR_E_PLEDGED_TYPE"
                            ],
                            "format": "label"
                        },
                        "disabled": true
                    }
                },
                {
                    "sign_date": {
                        "control": "dateTimePicker",
                        "label": "合同签订日期",
                        "span": 7,
                        "disabled": true
                    },
                    "guar_start_date": {
                        "control": "dateTimePicker",
                        "label": "合同起始日期",
                        "span": 7,
                        "disabled": true
                    },
                    "guar_end_date": {
                        "control": "dateTimePicker",
                        "label": "合同到期日期",
                        "span": 7,
                        "disabled": true
                    }
                }
            ]
        }
    }
}
