{
  "module": "dynamicTabs",
  "layout": {
    "tabs": {
      "scope": "templateScope",
      "initDataOnce": true,
      "initRequest": "${{params}}",
      "tabHeader": [
        {
          "id": "tab1",
          "clickInit": true,
          "label": {
            "default": "抵质押合同申请", 
             "i18n": "K0001.Application-for-mortgage-and-pledge-contract"
                            },
          "closable": false,
          "disabled": false,
          "icon": "el-icon-date",
          "toRequest": {
            "url": "/views/icms/guaranteeManager/guaranteeContractManager/coltContractApply/colt_contract_apply_list.json",
            "params": {
              "guar_type": "2",
              "addTips": "添加抵押合同",
              "editTips": "修改抵押合同",
              "lookTips": "抵押合同详情",
              "pageType": "detail"
            }
          }
        },
        {
          "id": "tab2",
          "clickInit": true,
          "label": {
            "default": "质押合同申请", 
             "i18n": "K0001.Pledge-Contract-Application"
                            },
          "closable": false,
          "disabled": false,
          "toRequest": {
            "url": "/views/icms/guaranteeManager/guaranteeContractManager/coltContractApply/colt_contract_apply_list.json",
            "params": {
              "guar_type": "3",
              "addTips": "添加质押合同",
              "editTips": "修改质押合同",
              "lookTips": "质押合同详情",
              "pageType": "detail"
            }
          }
        },
        {
          "id": "tab3",
          "clickInit": true,
          "label": {
            "default": "保证合同申请", 
             "i18n": "K0001.Guarantee-contract-application"
                            },
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "toRequest": {
            "url": "/views/icms/guaranteeManager/guaranteeContractManager/guarContractApply/guar_contract_apply_manager.json",
            "method": "post",
            "params": {
              "pageType": "detail"
            }
          }
        },
        {
          "id": "tab4",
          "clickInit": true,
          "label": {
            "default": "保证金合同申请", 
             "i18n": "K0001.Deposit-contract-application"
                            },
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "toRequest": {
            "url": "/views/icms/guaranteeManager/guaranteeContractManager/marginContractApply/margin_contract_apply_manager.json",
            "method": "post",
            "params": {
              "pageType": "detail"}
          }
        },
        {
          "id": "tab5",
          "label": {
            "default": "申请信用", 
             "i18n": "K0001.Apply-for-credit"
                            },
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "toRequest": {
            "url": "/views/icms/guaranteeManager/guaranteeContractManager/creditContractApply/credit_contract_apply_info view.json",
            "method": "post",
            "params": {}
          }
        }
      ],
      "defaultActiveKey": "tab1",
      "position": "top",
      "styleClass": "card"
    }
  }
}