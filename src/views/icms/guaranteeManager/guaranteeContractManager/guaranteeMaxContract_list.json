{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "ar0443",
          "max_contract_no": "${{params.max_contract_no}}",
          "guar_contract_type": "${{params.guar_contract_type}}"
        }
      },
      "pagination": {
        "pageSizes": [
          10,
          15,
          20,
          50,
          100
        ],
        "pageSize": 10
      },
      "thead": [
        {
          "type": "index",
          "label": "序号",
          "width": "60px",
          "align": "center"
        },
        {
          "label": "业务合同号",
          "prop": "contract_no",
          "width": "160px",
          "align": "center"
        },
        {
          "label": "业务类型",
          "prop": "prod_type_name",
          "align": "center"
        },
        {
          "label": "产品名",
          "prop": "prod_name",
          "align": "center"
        },
        {
          "label": "最高担保合同金额",
          "prop": "contract_amt",
          "align": "center",
          "format":{
            "thousand": ","
          }
        },
        {
          "label": "使用金额",
          "prop": "this_use_amt",
          "align": "center",
          "format":{
            "thousand": ","
          }
        },
        {
          "label": "币种",
          "prop": "ccy_code",
          "align": "center",
          "dict": {
            "format": "label",
            "dictKey": [
              "MS_CURRENCY"
            ],
            "dictType": "MS_CURRENCY"
          }
        }
      ]
    }
  }
}

