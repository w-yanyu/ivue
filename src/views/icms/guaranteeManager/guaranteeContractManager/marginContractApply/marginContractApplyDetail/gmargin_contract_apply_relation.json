{
    "module": "v_search_btn_datagrid",
    "layout": {
        "datagrid": {
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "post",
                "params": {
                    "servicecode": "ar0404",
                    "apply_no": "${{params.apply_no}}",
                    "margin_contract_no": "${{params.margin_contract_no}}",
                    "query_type": "1"
                }
            },
            "thead": [
                {
                    "label": "保证金编号",
                    "prop": "margin_no",
                    "align": "center"
                },
                {
                    "label": "保证金账号",
                    "prop": "margin_acct",
                    "align": "center"
                },
                {
                    "label": "保证金金额",
                    "prop": "margin_amt",
                    "align": "center",
                    "format":{
                      "thousand": ","
                    }
                },
                {
                    "label": "保证金币种",
                    "prop": "ccy_code",
                    "align": "center",
                    "dict": {
                      "dictType": "MS_CURRENCY",
                      "dictKey": [
                        "MS_CURRENCY"
                      ]
                    }
                },
                {
                    "label": "账户名称",
                    "prop": "account_name",
                    "align": "center"
                }
            ]
        }
    }
}