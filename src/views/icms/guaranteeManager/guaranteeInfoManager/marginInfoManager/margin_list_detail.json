{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "scope": "myForm",
            "initRequest": "${{params}}",
            "btns": [],
          "controls": [
            {
              "margin_acct": {
                "label": "保证金主账户",
                "control": "text",
                "disabled": true,
                "span":7,
                "rules": [
                  {
                    "required": false,
                    "message": "保证金主账户必输"
                  }
                ]
              },
              "account_name": {
                "label": "账户名称",
                "control": "text",
                "disabled": true,
                "span":7,
                "rules": [
                  {
                    "required": false,
                    "message": "账户名称必输"
                  }
                ]
              },
              "ccy_code": {
                "label": "保证金币种",
                "control": "select",
                "disabled": true,
                "span":7,
                "dict": {
                  "dictType": "MS_CURRENCY",
                  "dictKey": [
                    "MS_CURRENCY"
                  ],
                  "format": "label"
                },
                "rules": [
                  {
                    "required": false,
                    "message": "货币代码必输"
                  }
                ]
              },
              "margin_amt": {
                "label": "保证金金额",
                "control": "currency",
                "disabled": true,
                "span":7,
                "rules": [
                  {
                    "required": false,
                    "message": "保证金金额必输"
                  }
                ],
                "align": "left",
                "format":{
                  "thousand": ",",
                  "decimal": "2"
                }
              },
              "exchange_rate": {
                "label": "汇率",
                "align": "left",
                "control": "currency",
                "disabled": true,
                "span":7,
                "rules": [
                  {
                    "required": false,
                    "message": "汇率必输"
                  }
                ]
              },
              "base_ccy": {
                "label": "基准币种",
                "control": "select",
                "disabled": true,
                "span":7,
                "dict": {
                  "dictType": "MS_CURRENCY",
                  "dictKey": [
                    "MS_CURRENCY"
                  ],
                  "format": "label"
                },
                "rules": [
                  {
                    "required": false,
                    "message": "基准币种必输"
                  }
                ]
              },
              "base_margin_amt": {
                "label": "保证金金额(折基准币种)",
                "control": "currency",
                "span":7,
                "disabled": true,
                "rules": [
                  {
                    "required": false,
                    "message": "保证金金额(折基准币种)必输"
                  }
                ],
                "compute": "${{myForm.margin_amt}}*${{myForm.exchange_rate}}",
                "align": "left",
                "format":{
                  "thousand": ",",
                  "decimal": "2"
                }
              },
              "apply_no": {
                "label": "申请号",
                "control": "hidden",
                "span":7,
                "disabled": true,
                "rules": [
                  {
                    "required": false,
                    "message": "申请号必输"
                  }
                ],
                "value": "${{params.apply_no}}"
              },
              "margin_acct_amt": {
                "label": "保证金账户金额",
                "control": "currency",
                "align": "left",
                "disabled": true,
                "span":7,
                "rules": [
                  {
                    "required": false,
                    "message": "请输入保证金账户金额"
                  }
                ],
                "format":{
                  "thousand": ",",
                  "decimal": "2"
                }
              },
              "remark": {
                "label": "备注",
                "control": "textarea",
                "width": "130px",
                "disabled": true,
                "span":21,
                "autosize":{
                  "minRows": 4
                },
                "rules": [
                  {
                    "max": "200",
                    "message": "长度不能超过200个字符"
                  }
                ]
              }
            }
          ]
        }
    }
}
