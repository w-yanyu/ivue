{
    "module": "v_search_btn_datagrid",
    "layout": {
        "datagrid": {
            "doRequest": {
                "isInitSearch": true,
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "post",
                "params": {
                    "servicecode": "ar0725",
                    "apply_no": "${{params.apply_no}}",
                    "cust_no": "${{params.cust_no}}",
                    "query_type": "0"
                }
            },
            "thead": [
                {
                    "type": "index",
                    "align": "center",
                    "label": "序号"
                },
                {
                    "label": "客户名称",
                    "align": "center",
                    "width": "160px",
                    "prop": "cust_name"
                },
                {
                    "label": "证件类型",
                    "prop": "cert_type",
                    "align": "center",
                    "width": "160px",
                    "dict": {
                        "dictType": "MS_E_CRET_TYPE",
                        "dictKey": [
                            "MS_E_CRET_TYPE"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "证件号码",
                    "align": "center",
                    "width": "160px",
                    "prop": "cert_no"
                },
                {
                    "label": "担保形式",
                    "prop": "guar_modality",
                    "align": "center",
                    "dict": {
                        "dictType": "AR_E_GUAR_CATG",
                        "dictKey": [
                            "AR_E_GUAR_CATG"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "担保比例",
                    "align": "center",
                    "prop": "guar_ratio"
                },
                {
                    "label": "担保金额",
                    "align": "center",
                    "prop": "guar_amt"
                },
                {
                    "label": "担保金额(折基准币种)",
                    "align": "center",
                    "prop": "guar_base_amt"
                },
                {
                    "label": "汇率",
                    "align": "center",
                    "prop": "exchange_rate"
                },
                {
                    "label": "是否担保公司担保",
                    "prop": "is_guar_company",
                    "align": "center",
                    "dict": {
                        "dictType": "MS_E_YESORNO",
                        "dictKey": [
                            "MS_E_YESORNO"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "担保币种",
                    "prop": "guar_ccy",
                    "align": "center",
                    "dict": {
                        "dictType": "MS_CURRENCY",
                        "dictKey": [
                            "MS_CURRENCY"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "基准币种",
                    "prop": "base_ccy",
                    "align": "center",
                    "dict": {
                        "dictType": "MS_CURRENCY",
                        "dictKey": [
                            "MS_CURRENCY"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "操作",
                    "width": "200px",
                    "fixed": "right",
                    "align": "center",
                    "btns": [
                        {
                            "apply_no": {
                                "control": "hidden",
                                "label": ""
                            },
                            "type": "add",
                            "label": "确定",
                            "doRequest": {
                                "url": "/SUMP/icmcall/icmRPCCall",
                                "method": "POST",
                                "params": {
                                    "servicecode": "ar0722",
                                    "apply_no": "${{params.apply_no}}"
                                }
                            },
                            "confirm": {
                                "title": "提示信息",
                                "msg": "确定引入该保证人信息吗？",
                                "confirmButtonText": "确定",
                                "cancelButtonText": "取消"
                            },
                            "eventAfter": {
                                "doParentCallback": true
                            }
                        }
                    ]
                }
            ]
        }
    }
}