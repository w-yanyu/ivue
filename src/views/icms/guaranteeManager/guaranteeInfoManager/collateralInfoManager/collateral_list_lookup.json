{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "controls": [
        {
          "cust_no": {
            "control": "text",
            "label": "客户编号",
            "value": "${{params.cust_no}}"
          },
          "colt_no": {
            "control": "text",
            "label": "押品编号"
          }
        }
      ]
    },
    "toolbar": {},
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "co2101"
        }
      },
      "thead": [
        {
          "label": "操作",
          "btns": [
            {
              "type": "bringback",
              "label": "选择"
            }
          ]
        },
        {
          "label": "客户编号",
          "prop": "cust_no",
          "align": "center"
        },
        {
          "label": "押品编号",
          "prop": "colt_no",
          "align": "center"
        },
        {
          "label": "资产清单序号",
          "prop": "asset_list_id",
          "align": "center"
        },
        {
          "label": "担保方式",
          "prop": "guar_type",
          "align": "center",
          "dict": {
            "dictType": "MS_E_GUAR_TYPE",
            "dictKey": [
              "MS_E_GUAR_TYPE"
            ],
            "labelField": "dictName",
            "valueField": "dictId"
          }
        },
        {
          "label": "资产类型",
          "prop": "colt_type",
          "align": "center"
        },
        {
          "label": "资产类型名称",
          "prop": "colt_type_name",
          "align": "center"
        },
        {
          "label": "抵质押品产品类型",
          "prop": "colt_prod_cd",
          "align": "center"
        },
        {
          "label": "抵质押品产品名称",
          "prop": "colt_prod_name",
          "align": "center"
        },
        {
          "label": "抵质押额",
          "prop": "sec_val",
          "align": "center",
          "format":{
            "thousand": ","
          }
        },
        {
          "label": "抵质押率（%）",
          "prop": "sec_rate",
          "align": "center"
        },
        {
          "label": "抵质押币种",
          "prop": "eval_cyy",
          "align": "center",
          "dict": {
            "dictType": "MS_CURRENCY",
            "dictKey": [
              "MS_CURRENCY"
            ],
            "format": "value-label"
          }
        }

      ]
    }
  }
}
