{
    "module": "dynamicTabs",
    "layout": {
      "tabs": {
        "scope": "templateScope",
        "#initDataOnce":true,
        "tabHeader": [
          {
            "id": "dd1",
            "scope": "baseInfo",
            "clickInit": true,
            "label": {
              "default": "担保方式", 
               "i18n": "K0001.guarantee-method"
                              },
            "closable": false,
            "disabled": false,
            "icon": "el-icon-date",
            "toRequest":{
              "url":"/views/icms/guaranteeManager/guaranteeInfoManager/guaranteeModeManager/guaran_ways_view.json",
              "method": "post"
            }

          },
          {
            "id": "dd2",
            "clickInit": true,
            "label": {
              "default": "抵质押品信息", 
               "i18n": "K0001.Collateral-information"
                              },
            "icon": "el-icon-date",
            "closable": false,
            "disabled": false,
            "toRequest":{
              "url": "/views/icms/guaranteeManager/guaranteeInfoManager/collateralInfoManager/collateral_list.json",
              "method": "post",
              "params": {
                "apply_no": "${{params.apply_no}}",
                "pageType": "detail"
              }
            }
          },
          {
            "id": "dd3",
            "clickInit": true,
            "label": {
              "default": "保证人", 
               "i18n": "K0001.guarantor"
                              },
            "icon": "el-icon-date",
            "closable": false,
            "disabled": false,
            "toRequest":{
              "url": "/views/icms/guaranteeManager/guaranteeInfoManager/guarantorManager/guarantor_list.json",
              "method": "post",
              "params": {
                "apply_no": "${{params.apply_no}}",
                "applicant_type": "GUAR",
                "cust_no": "${{params.cust_id}}",
                "pageType": "detail"
              }
            }
          },
          {
            "id": "dd4",
            "clickInit": true,
            "label": {
              "default": "保证金信息", 
               "i18n": "K0001.Margin-Information"
                              },
            "icon": "el-icon-date",
            "closable": false,
            "disabled": false,
            "toRequest":{
              "url": "/views/icms/guaranteeManager/guaranteeInfoManager/marginInfoManager/margin_list.json",
              "method": "post",
              "params": {
                "apply_no": "${{params.apply_no}}",
                "pageType": "detail"
              }
            }
          }
        ],
        "defaultActiveKey": "dd1",
        "position": "top",
        "styleClass": "card"
      }
    }
  }
