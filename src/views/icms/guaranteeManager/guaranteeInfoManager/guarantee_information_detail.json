{
    "module": "dynamicTabs",
    "layout": {
      "tabs": {
        "scope": "templateScope",
        "#initDataOnce":true,
        "tabHeader": [
          {
            "id": "dd1",
            "scope": "baseInfo",
            "clickInit": true,
            "label": "担保方式",
            "closable": false,
            "disabled": false,
            "icon": "el-icon-date",
            "toRequest":{
              "url":"/views/icms/guaranteeManager/guaranteeInfoManager/guaranteeModeManager/guaran_ways_info.json",
              "method": "post"
            }

          },
          {
            "id": "dd2",
            "clickInit": true,
            "label": "抵质押品信息",
            "icon": "el-icon-date",
            "closable": false,
            "disabled": false,
            "toRequest":{
              "url": "/views/icms/guaranteeManager/guaranteeInfoManager/collateralInfoManager/collateral_list.json",
              "method": "post",
              "params": {
                "apply_no": "${{params.apply_no}}"
              }
            }
          },
          {
            "id": "dd3",
            "clickInit": true,
            "label": "保证人",
            "icon": "el-icon-date",
            "closable": false,
            "disabled": false,
            "toRequest":{
              "url": "/views/icms/guaranteeManager/guaranteeInfoManager/guarantorManager/guarantor_list.json",
              "method": "post",
              "params": {
                "apply_no": "${{params.apply_no}}"
              }
            }
          },
          {
            "id": "dd4",
            "clickInit": true,
            "label": "保证金信息",
            "icon": "el-icon-date",
            "closable": false,
            "disabled": false,
            "toRequest":{
              "url": "/views/icms/guaranteeManager/guaranteeInfoManager/marginInfoManager/margin_list.json",
              "method": "post",
              "params": {
                "apply_no": "${{params.apply_no}}"
              }
            }
          }
        ],
        "defaultActiveKey": "dd1",
        "position": "top",
        "styleClass": "card"
      }
    }
  }
