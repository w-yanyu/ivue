{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "ar0222",
          "doc_no": "${{params.doc_no}}",
          "apply_no": "${{params.apply_no}}"
        }
      },
      "btns": [
        {
          "type": "submit",
          "label": "提交"
        }
      ],
      "controls": [
        {
          "file_path": {
            "control": "hidden",
            "label": "file_path"
          },
          "file_name": {
            "control": "hidden",
            "label": "file_name"
          },
          "file_size": {
            "control": "hidden",
            "label": "file_size"
          },
          "file_catalog": {
            "label": "文件分类",
            "control": "hidden",
            "width": "140px",
            "value": "wfdocfile"
          }
        },
        {
          "file": {
            "doRequest": {
              "url": "/SUMP/minioCall/upload",
              "method": "post",
              "params": {
              },
              "resultSet": {
                "file_path": "path",
                "file_name": "oldName",
                "file_size": "size"
              }
            },
            "width": "130px",
            "control": "file",
            "limit": 1,
            "limitSize": 10,
            "label": "上传文件",
            "btnLable": "请选择文件"
          }
        }
      ]
    }
  }
}
