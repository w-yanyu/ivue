{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "###doc_chklist_id": {
            "label": "核实项检查代码",
            "control": "hidden",
            "value": "${{params.doc_chklist_id}}"
          },
          "verify_cd": {
            "label": "核实项代码",
            "control": "text"
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "label": "新增",
          "toRequest": {
            "url": "/views/icms/verifyManager/verifyListManager/verfyLismanagedtl_add.json",
            "method": "get",
            "params": {
              "verify_list_cd": "${{params.verify_list_cd}}"
            }
          },
          "props": {
            "title": "核实项检查明细新增",
            "width": "80%"
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/call/RPCCall",
        "method": "post",
        "params": {
          "verify_list_cd": "${{params.verify_list_cd}}",
          "servicecode": "ar1161"
        }
      },
      "pagination": {
        "pageSizes": [
          10,
          15,
          20
        ],
        "pageSize": 10
      },
      "thead": [
        {
          "type": "index",
          "align": "center"
        },
        {
          "label": "核实项代码",
          "prop": "verify_cd",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "核实项描述",
          "prop": "verify_desc",
          "align": "center",
          "type": "leaf",
          "width": "260px"
        },
        {
          "label": "是否必需",
          "prop": "is_must",
          "align": "center",
          "type": "leaf",
          "dict": {
            "dictType": "MS_E_YESORNO",
            "dictKey": [
              "MS_E_YESORNO"
            ],
            "format": "label"
          }
        },
        {
          "label": "排序序号",
          "prop": "verify_sort",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "核实项复核检查状态",
          "prop": "check_state",
          "align": "center",
          "type": "leaf",
          "dict": {
            "dictType": "AR_E_APPL_STATE",
            "dictKey": [
              "AR_E_APPL_STATE"
            ],
            "format": "label"
          }
        },
        {
          "label": "操作",
          "width": "150px",
          "align": "center",
          "fixed": "right",
          "btns": [
            {
              "type": "edit",
              "label": "维护",
              "toRequest": {
                "url": "/views/icms/verifyManager/verifyListManager/verfyLismanagedtl_update.json",
                "method": "get",
                "params": {
                  "doc_chklist_id": "${{current.doc_chklist_id}}"
                }
              },
              "props": {
                "title": "核实项检查明细维护",
                "width": "80%"
              }
            },
            {
              "type": "delete",
              "label": "删除",
              "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "DELETE",
                "params": {
                  "servicecode": "ar1163"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
