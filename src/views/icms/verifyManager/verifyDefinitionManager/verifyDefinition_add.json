{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "controls": [
        {
          "verify_cd": {
            "label": "核实项代码",
            "width": "140px",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入核实项代码"
              }
            ]
          },
          "verify_desc": {
            "label": "核实项描述",
            "width": "140px",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入核实项描述"
              }
            ]
          },
          "busi_type": {
            "label": "业务类型",
            "width": "140px",
            "multiple": false,
            "value": "",
            "control": "remoteSelect",
            "labelField": "title",
            "valueField": "id",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "servicecode": "pd0144",
                "grupno": "0006",
                "pid": "KSCP"
              }
            },
            "events": {
              "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "post",
                "params": {
                  "servicecode": "pd0143",
                  "grupno": "0006",
                  "id": "${{form.busi_type}}"
                },
                "resultSet": {
                  "prod_type_name": "title"
                }
              }
            },
            "rules": [
              {
                "required": true,
                "message": "请选择业务类型"
              }
            ]
          },
          "prod_type_name": {
            "control": "hidden",
            "label": "资料类型名称",
            "width": "140px"
          },
          "verify_type_cd": {
            "label": "核实项分类代码",
            "width": "140px",
            "control": "remoteSelect",
            "labelField": "verify_type_name",
            "valueField": "verify_type_cd",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ar1101",
                "start": 1,
                "length": 1000
              }
            },
            "rules": [
              {
                "required": true,
                "message": "请选择核实项分类代码"
              }
            ]
          }
        }
      ],
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "doRequest": {
            "url": "/SUMP/call/RPCCall",
            "method": "POST",
            "params": {
              "servicecode": "ar1122"
            }
          }
        }
      ]
    }
  }
}
