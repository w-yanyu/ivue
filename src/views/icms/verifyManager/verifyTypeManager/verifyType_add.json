{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "controls": [
        {
          "verify_type_cd": {
            "label": "核实项分类代码",
            "width": "140px",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入核实项分类代码"
              }
            ]
          },
          "verify_type_name": {
            "label": "核实项分类名称",
            "width": "140px",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入核实项分类名称"
              }
            ]
          },
          "is_display": {
            "label": "是否显示",
            "width": "140px",
            "control": "select",
            "multiple": false,
            "value": "",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择是否显示"
              }
            ]
          },
          "seq_no": {
            "label": "排序号",
            "width": "140px",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入排序号"
              }
            ]
          },
          "remark": {
            "width": "140px",
            "label": "备注",
            "control": "textarea",
            "span": 24,
            "autosize":{"minRows": 3,"maxRows":5},
            "maxlength": 100,
            "showWordLimit": true
          }
        }
      ],
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "doRequest": {
            "url": "/SUMP/call/RPCCall",
            "method": "POST",
            "params": {
              "servicecode": "ar1102"
            }
          }
        }
      ]
    }
  }
}
