{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "controls": [
        {
          "doc_no": {
            "label": "资料编号",
            "control": "hidden",
            "rules": [
              {
                "required": false,
                "message": "请输入资料编号"
              }
            ]
          },
          "doc_name": {
            "label": "资料名称",
            "control": "text",
            "disabled": "true",
            "rules": [
              {
                "required": false,
                "message": "请输入资料名称"
              }
            ]
          },
          "is_person_doc": {
            "label": "是否个人资料",
            "control": "select",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ]
            }
          },
          "doc_type": {
            "label": "资料类型",
            "control": "remoteSelect",
            "labelField": "title",
            "valueField": "id",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "servicecode": "pd0144",
                "grupno": "0006",
                "pid": "KSCP"
              }
            }
          },
          "doc_type_no": {
            "label": "资料分类编号",
            "control": "text",
            "disabled": "true",
            "rules": [
              {
                "required": false,
                "message": "请输入资料分类编号"
              }
            ]
          }
        }
      ]
    }
  }
}