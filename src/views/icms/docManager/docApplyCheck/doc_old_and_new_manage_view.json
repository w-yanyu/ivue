{
  "module": "v-form-btn-dynamic",
  "layout": {
    "form": {
      "btns":[],
      "controlsGroup": [
        {
          "title": "贷款出账资料列表",
          "tooltip": "step1",
          "titleIcon": "",
          "showHint": "",
          "accordion": false,
          "expand": true,
          "controls": [
            {
              "id1": {
                "control": "dynamic",
                "span": 24,
                "toRequest": {
                  "url": "/views/icms/docManager/docApplyCheck/doc_manage_view.json",
                  "method": "post",
                  "params": {
                    "apply_no":"${{params.apply_no}}"
                  }
                }
              }
            }
          ]
        },
        {
          "title": "贷款申请资料列表",
          "tooltip": "step2",
          "expand": true,
          "controls": [
            {
              "id2": {
                "control": "dynamic",
                "span": 24,
                "toRequest": {
                  "url": "/views/icms/docManager/docApplyCheck/doc_manage_view.json",
                  "method": "get",
                  "params": {
                    "apply_no":"${{params.parent_apply_id}}"
                  }
                }
              }
            }
          ]
        }
      ]
    }
  }
}
