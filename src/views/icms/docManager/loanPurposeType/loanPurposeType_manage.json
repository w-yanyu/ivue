{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "amt_purpose_type_name": {
            "control": "text",
            "label": "贷款用途分类名称"
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "target": "dialog",
          "label": "新增",
          "toRequest": {
            "method": "POST",
            "url": "/views/icms/docManager/loanPurposeType/loanPurposeType_add.json"
          },
          "props": {
            "width": "65%",
            "title": "新增贷款用途分类"
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ar0234",
          "cust_class_list": "${{params.cust_class_list}}"
        }
      },
      "thead": [
        {
          "type":"string",
          "label": "贷款用途分类编号",
          "prop": "amt_purpose_type_no",
          "align": "center",
          "width": "250px"
        },
        {
          "type":"string",
          "label": "贷款用途分类名称",
          "prop": "amt_purpose_type_name",
          "align": "center",
          "width": "250px"
        },
        {
          "type": "string",
          "label": "适用客户类型",
          "prop": "cust_class",
          "align": "center",
          "dict": {
            "dictType": "AR_E_CUST_CLASS",
            "dictKey": [
              "AR_E_CUST_CLASS"
            ],
            "format": "label"
          },
          "width": "250px"
        },
        {
          "type":"string",
          "label": "备注",
          "prop": "remark",
          "align": "center"
        },
        {
          "label": "操作",
          "fixed": "right",
          "width": "200",
          "align": "center",
          "btns": [
            {
              "label": "维护",
              "type": "edit",
              "toRequest": {
                "method": "POST",
                "url": "/views/icms/docManager/loanPurposeType/loanPurposeType_modify.json"
              }
            },
            {
              "label": "明细",
              "type": "look",
              "toRequest": {
                "method": "POST",
                "url": "/views/icms/docManager/loanPurpose/loanPurpose_detail.json",
                "params": {

                }
              }
            },
            {
              "label": "删除",
              "type": "delete",
              "doRequest": {
                "method": "POST",
                "url": "/SUMP/icmcall/icmRPCCall",
                "params": {
                  "servicecode": "ar0237"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
