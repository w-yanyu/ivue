{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "amt_purpose_type_name": {
            "control": "text",
            "label": "贷款用途分类名称"
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ar0234",
          "cust_class_list": "${{params.cust_class_list}}"
        }
      },
      "thead": [
        {
          "type":"string",
          "label": "贷款用途分类编号",
          "prop": "amt_purpose_type_no",
          "align": "center"
        },
        {
          "type":"string",
          "label": "贷款用途分类名称",
          "prop": "amt_purpose_type_name",
          "align": "center"
        },
        {
          "type": "string",
          "label": "适用客户类型",
          "prop": "cust_class",
          "align": "center",
          "dict": {
            "dictType": "AR_E_CUST_CLASS",
            "dictKey": [
              "AR_E_CUST_CLASS"
            ],
            "format": "label"
          }
        },
        {
          "type":"string",
          "label": "备注",
          "prop": "remark",
          "align": "center"
        },
        {
          "label": "操作",
          "fixed": "right",
          "width": "200",
          "align": "center",
          "btns": [
            {
              "type": "bringback",
              "label": "选择"
            }
          ]
        }
      ]
    }
  }
}
