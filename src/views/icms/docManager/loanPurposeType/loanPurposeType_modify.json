{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "ar0236"
        }
      },
      "controls": [
        {
          "amt_purpose_type_no": {
            "label": "贷款用途分类编号",
            "control": "text",
            "width": "130px",
            "disabled": true,
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          },
          "amt_purpose_type_name": {
            "label": "贷款用途分类名称",
            "control": "text",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          },
          "cust_class": {
            "label": "适用客户类型",
            "control": "select",
            "width": "130px",
            "dict": {
              "dictType": "AR_E_CUST_CLASS",
              "dictKey": [
                "AR_E_CUST_CLASS"
              ],
              "exclude": [
                "30"
              ]
            },
            "rules": [
              {
                "required": true,
                "message": "请选择适用客户类型"
              }
            ]
          },
          "remark": {
            "label": "备注",
            "control": "textarea",
            "width": "130px",
            "span": "24",
            "rows": 4
          }
        }
      ]
    }
  }
}
