{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "doc_check_list_no": {
            "label": "资料检查编号",
            "control": "hidden",
            "value": "${{params.doc_check_list_no}}"
          },
          "doc_no": {
            "label": "资料编号",
            "control": "text"
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "label": "新增",
          "toRequest": {
            "url": "/views/icms/docManager/docCheckManager/docCheckDetailManager/docCheckDetl_add.json",
            "method": "get",
            "params": {
              "doc_check_list_no": "${{params.doc_check_list_no}}",
              "doc_list_type": "${{params.doc_list_type}}"
            }
          },
          "props": {
            "title": "资料检查明细新增",
            "width": "80%"
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "ar0161",
          "params": {}
        }
      },
      "pagination": {
        "pageSizes": [
          10,
          15,
          20
        ],
        "pageSize": 10
      },
      "thead": [
        {
          "type": "index",
          "align": "center"
        },
        {
          "type": "leaf",
          "label": "资料编号",
          "align": "center",
          "prop": "doc_no"
        },
        {
          "type": "leaf",
          "label": "资料描述",
          "width": 300,
          "align": "center",
          "prop": "doc_name"
        },
        {
          "label": "允许更新",
          "prop": "is_can_upload",
          "align": "center",
          "type": "leaf",
          "dict": {
            "dictType": "MS_E_YESORNO",
            "dictKey": [
              "MS_E_YESORNO"
            ],
            "format": "label"
          }
        },
        {
          "label": "是否必需",
          "prop": "is_must",
          "align": "center",
          "type": "leaf",
          "dict": {
            "dictType": "MS_E_YESORNO",
            "dictKey": [
              "MS_E_YESORNO"
            ],
            "format": "label"
          }
        },
        {
          "label": "资料查收状态",
          "prop": "doc_check_state",
          "align": "center",
          "type": "leaf",
          "dict": {
            "dictType": "AR_E_APPL_STATE",
            "dictKey": [
              "AR_E_APPL_STATE"
            ],
            "format": "label"
          }
        },
        {
          "label": "是否允许后补",
          "prop": "is_can_backup",
          "align": "center",
          "type": "leaf",
          "dict": {
            "dictType": "MS_E_YESORNO",
            "dictKey": [
              "MS_E_YESORNO"
            ],
            "format": "label"
          }
        },
        {
          "label": "上传标志",
          "prop": "upload_flag",
          "align": "center",
          "type": "leaf",
          "dict": {
            "dictType": "MS_E_YESORNO",
            "dictKey": [
              "MS_E_YESORNO"
            ],
            "format": "label"
          }
        },
        {
          "label": "资料复核检查状态",
          "prop": "check_state",
          "align": "center",
          "type": "leaf",
          "dict": {
            "dictType": "AR_E_APPL_STATE",
            "dictKey": [
              "AR_E_APPL_STATE"
            ],
            "format": "label"
          },
          "events": {
            "condition": {
              "2": {
                "control": {
                  "upper_lvl_gl_code": {
                    "rules": [
                      {
                        "required": "true",
                        "message": "请输入上级科目"
                      }
                    ]
                  }
                }
              }
            }
          }
        },
        {
          "label": "附件类型",
          "prop": "filetype",
          "align": "center",
          "type": "leaf",
          "dict": {
            "dictType": "AR_E_FILETYPE",
            "dictKey": [
              "AR_E_FILETYPE"
            ],
            "format": "label"
          }
        },
        {
          "label": "是否获取GPS信息",
          "prop": "is_access_info",
          "align": "center",
          "type": "leaf",
          "dict": {
            "dictType": "MS_E_YESORNO",
            "dictKey": [
              "MS_E_YESORNO"
            ],
            "format": "label"
          }
        },
        {
          "label": "是否对图片进行原图检查",
          "prop": "is_check_ps",
          "align": "center",
          "type": "leaf",
          "dict": {
            "dictType": "MS_E_YESORNO",
            "dictKey": [
              "MS_E_YESORNO"
            ],
            "format": "label"
          }
        },
        {
          "label": "是否购车发票验真",
          "prop": "is_check_car_invoice",
          "align": "center",
          "type": "leaf",
          "dict": {
            "dictType": "MS_E_YESORNO",
            "dictKey": [
              "MS_E_YESORNO"
            ],
            "format": "label"
          }
        },
        {
          "label": "操作",
          "width": "150px",
          "align": "center",
          "fixed": "right",
          "btns": [
            {
              "type": "edit",
              "label": "维护",
              "toRequest": {
                "url": "/views/icms/docManager/docCheckManager/docCheckDetailManager/docCheckDetl_update.json",
                "method": "get",
                "params": {}
              },
              "props": {
                "title": "资料检查明细维护",
                "width": "80%"
              }
            },
            {
              "type": "delete",
              "label": "删除",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "DELETE",
                "params": {
                  "servicecode": "ar0164"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
