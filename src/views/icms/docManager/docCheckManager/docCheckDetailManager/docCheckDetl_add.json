{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "controls": [
        {
          "doc_check_list_no": {
            "label": "资料检查列表编号",
            "control": "hidden",
            "width": "140px",
            "value": "${{params.doc_check_list_no}}"
          },
          "doc_no": {
            "label": "资料编号",
            "control": "lookup",
            "width": "140px",
            "edit": true,
            "disable": "true",
            "keyField": {
              "doc_no": "doc_no"
            },
            "toRequest": {
              "url": "/views/icms/docManager/docCheckManager/docCheckDetailManager/docDefinition_lookup.json",
              "method": "post",
              "params": {
                "doc_list_type": "${{params.doc_list_type}}"
              }
            },
            "props": {
              "title": "资料定义查询"
            },
            "format": "label",
            "rules": [
              {
                "required": true,
                "message": "请选择资料"
              }
            ]
          },
          "is_can_upload": {
            "label": "允许更新",
            "control": "select",
            "width": "140px",
            "rules": [
              {
                "required": true,
                "message": "请选择允许更新"
              }
            ],
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ]
            }
          },
          "is_must": {
            "label": "是否必须",
            "control": "select",
            "width": "140px",
            "rules": [
              {
                "required": true,
                "message": "请选择是否必须"
              }
            ],
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ]
            }
          },
          "doc_check_state": {
            "label": "资料查收状态",
            "control": "select",
            "width": "140px",
            "rules": [
              {
                "required": true,
                "message": "请选择资料查收状态"
              }
            ],
            "dict": {
              "dictType": "AR_E_APPL_STATE",
              "dictKey": [
                "AR_E_APPL_STATE"
              ]
            }
          },
          "apply_state": {
            "label": "申请状态",
            "control": "select",
            "width": "140px",
            "rules": [
              {
                "required": true,
                "message": "请选择申请状态"
              }
            ],
            "dict": {
              "dictType": "AR_E_APPL_STATE",
              "dictKey": [
                "AR_E_APPL_STATE"
              ]
            }
          },
          "is_can_backup": {
            "label": "是否允许后补",
            "control": "select",
            "width": "140px",
            "rules": [
              {
                "required": true,
                "message": "请选择是否允许后补"
              }
            ],
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ]
            }
          },
          "upload_flag": {
            "label": "上传标志",
            "control": "select",
            "width": "140px",
            "rules": [
              {
                "required": true,
                "message": "请选择上传标志"
              }
            ],
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ]
            }
          },
          "check_state": {
            "label": "资料复核检查状态",
            "control": "select",
            "width": "140px",
            "rules": [
              {
                "required": true,
                "message": "请选择资料复核检查状态"
              }
            ],
            "dict": {
              "dictType": "AR_E_APPL_STATE",
              "dictKey": [
                "AR_E_APPL_STATE"
              ]
            }
          },
          "filetype": {
            "label": "附件类型",
            "control": "select",
            "width": "140px",
            "rules": [
              {
                "required": true,
                "message": "请选择附件类型"
              }
            ],
            "dict": {
              "dictType": "AR_E_FILETYPE",
              "dictKey": [
                "AR_E_FILETYPE"
              ]
            },
            "showItem": {
              "$default": [
              ],
              "image": [
                "is_access_info",
                "is_check_ps",
                "is_check_car_invoice"
              ],
              "all": []
            }
          },
          "is_access_info": {
            "label": "是否获取GPS信息",
            "control": "select",
            "width": "140px",
            "rules": [
              {
                "required": true,
                "message": "请选择是否获取GPS信息"
              }
            ],
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ]
            }
          },
          "is_check_ps": {
            "label": "是否对图片进行原图检查",
            "control": "select",
            "width": "140px",
            "rules": [
              {
                "required": true,
                "message": "请选择是否对图片进行原图检查"
              }
            ],
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ]
            }
          },
          "is_check_car_invoice": {
            "label": "是否购车发票验真",
            "control": "select",
            "width": "140px",
            "rules": [
              {
                "required": true,
                "message": "请选择是否购车发票验真"
              }
            ],
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ]
            }
          }
        }
      ],
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "ar0162"
            }
          }
        }
      ]
    }
  }
}
