{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "controls": [
                {
                    "doc_type_no": {
                        "label": "资料分类编号",
                        "control": "text",
                        "disabled": true,
                        "width": "140px"
                    },
                    "doc_type_name": {
                        "label": "资料分类名称",
                        "control": "text",
                        "width": "140px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入资料分类名称"
                            }
                        ]
                    },
                    "is_display": {
                        "label": "是否显示",
                        "control": "select",
                        "width": "140px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择是否显示"
                            }
                        ],
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ]
                        }
                    },
                    "sort_no": {
                        "label": "排序序号",
                        "control": "text",
                        "width": "140px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入排序序号"
                            }
                        ]
                    },
                    "remark": {
                        "label": "备注",
                        "control": "text",
                        "width": "140px"
                    }
                }
            ],
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "ar0103"
                        }
                    }
                }
            ]
        }
    }
}
