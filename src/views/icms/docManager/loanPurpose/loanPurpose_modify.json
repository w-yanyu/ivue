{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "ar0249"
        }
      },
      "controls": [
        {
          "amt_purpose_no": {
            "label": "贷款用途编号",
            "control": "text",
            "disabled": true,
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          },
          "amt_purpose_name": {
            "label": "贷款用途名称",
            "control": "text",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          },
          "amt_purpose_type_no": {
            "label": "贷款用途分类编号",
            "control": "lookup",
            "width": "130px",
            "edit": false,
            "keyField": {
              "amt_purpose_type_no": "amt_purpose_type_no"
            },
            "toRequest": {
              "url": "/views/icms/docManager/loanPurposeType/loanPurposeType_lookup.json",
              "method": "get"
            },
            "multiple": false,
            "props": {
              "title": "选择贷款用途分类"
            },
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          }
        }
      ]
    }
  }
}
