{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ar0247",
          "amt_purpose_type_no": "${{params.amt_purpose_type_no}}"
        }
      },
      "thead": [
        {
          "type":"string",
          "label": "贷款用途编号",
          "prop": "amt_purpose_no",
          "align": "center"
        },
        {
          "type":"string",
          "label": "贷款用途名称",
          "prop": "amt_purpose_name",
          "align": "center"
        },
        {
          "type":"string",
          "label": "贷款用途分类编号",
          "prop": "amt_purpose_type_no",
          "align": "center"
        }
      ]
    }
  }
}
