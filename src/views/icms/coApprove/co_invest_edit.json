{
    "module": "v-form-btn-dynamic",
    "layout": {
      "form": {
        "initRequest": {
          "url": "/SUMP/icmcall/icmRPCCall",
          "method": "POST",
          "params": {
            "servicecode": "ar1203",
            "apply_no": "${{params.apply_no}}"
          }
        },
        "controls": [
          {
            "apply_no": {
              "control": "hidden",
              "span": 8
            },
            "deemed_income": {
              "control": "text",
              "label": "认定收入(月)(元)",
              "rules": [
                {
                  "required": true,
                  "message": "请输入"
                }
              ],
              "format": {
                "thousand": ","
              },
              "span": 8
            },  
            "is_work_unit": {
              "control": "select",
              "label": "工作单位是否一致",
              "span": 8,
              "rules": [
                {
                  "required": true,
                  "message": "请选择"
                }
              ],
              "dict": {
                "dictType": "MS_E_YESORNO",
                "dictKey": [
                  "MS_E_YESORNO"
                ],
                "labelField": "dictName",
                "valueField": "dictId",
                "format": "label"
              } 
            },"pledge_house_build_year": {
              "control": "dateTimePicker",
              "type":"year",
              "format":"yyyy",
              "rules": [
                {
                  "required": true,
                  "message": "请输入"
                }
              ],
              "label": "抵押房产建成年份",
              "span": 8
            }
          },
          {
            "is_borrower_will": {
              "control": "select",
              "label": "借款人借款意愿是否真实",
              "span": 8,
              "rules": [
                {
                  "required": true,
                  "message": "请选择"
                }
              ],
              "dict": {
                "dictType": "MS_E_YESORNO",
                "dictKey": [
                  "MS_E_YESORNO"
                ],
                "labelField": "dictName",
                "valueField": "dictId",
                "format": "label"
              } 
            },"is_pledger_will": {
              "control": "select",
              "label": "抵押人担保意愿是否真实  ",
              "span": 8,
              "rules": [
                {
                  "required": true,
                  "message": "请选择"
                }
              ],
              "dict": {
                "dictType": "MS_E_YESORNO",
                "dictKey": [
                  "MS_E_YESORNO"
                ],
                "labelField": "dictName",
                "valueField": "dictId",
                "format": "label"
              } 
            },"is_pledger_qualification": {
              "control": "select",
              "label": "抵押人是否具备资格",
              "span": 8,
              "rules": [
                {
                  "required": true,
                  "message": "请选择"
                }
              ],
              "dict": {
                "dictType": "MS_E_YESORNO",
                "dictKey": [
                  "MS_E_YESORNO"
                ],
                "labelField": "dictName",
                "valueField": "dictId",
                "format": "label"
              } 
            }
          },        {
            "is_ownership": {
              "control": "select",
              "label": "是否对押品享有所有权和处分权",
              "span": 8,
              "rules": [
                {
                  "required": true,
                  "message": "请选择"
                }
              ],
              "dict": {
                "dictType": "MS_E_YESORNO",
                "dictKey": [
                  "MS_E_YESORNO"
                ],
                "labelField": "dictName",
                "valueField": "dictId",
                "format": "label"
              } 
            },"is_signature": {
              "control": "select",
              "label": "签章是否真实",
              "span": 8,
              "rules": [
                {
                  "required": true,
                  "message": "请选择"
                }
              ],
              "dict": {
                "dictType": "MS_E_YESORNO",
                "dictKey": [
                  "MS_E_YESORNO"
                ],
                "labelField": "dictName",
                "valueField": "dictId",
                "format": "label"
              } 
            },"is_collateral_true": {
              "control": "select",
              "label": "押品实际情况是否与权属文件相符",
              "span": 8,
              "rules": [
                {
                  "required": true,
                  "message": "请选择"
                }
              ],
              "dict": {
                "dictType": "MS_E_YESORNO",
                "dictKey": [
                  "MS_E_YESORNO"
                ],
                "labelField": "dictName",
                "valueField": "dictId",
                "format": "label"
              } 
            }
          },{
            "is_information_true": {
              "control": "select",
              "label": "资料原件是否与影像件一致",
              "span": 8,
              "rules": [
                {
                  "required": true,
                  "message": "请选择"
                }
              ],
              "dict": {
                "dictType": "MS_E_YESORNO",
                "dictKey": [
                  "MS_E_YESORNO"
                ],
                "labelField": "dictName",
                "valueField": "dictId",
                "format": "label"
              } 
            },"is_first_lien_mortgage": {
              "control": "select",
              "label": "本笔贷款是否能设立第一顺位抵押权",
              "span": 8,
              "rules": [
                {
                  "required": true,
                  "message": "请选择"
                }
              ],
              "dict": {
                "dictType": "MS_E_YESORNO",
                "dictKey": [
                  "MS_E_YESORNO"
                ],
                "labelField": "dictName",
                "valueField": "dictId",
                "format": "label"
              } 
            },"is_right_of_habitation": {
              "control": "select",
              "label": "抵押物是否设置居住权",
              "span": 8,
              "rules": [
                {
                  "required": true,
                  "message": "请选择"
                }
              ],
              "dict": {
                "dictType": "MS_E_YESORNO",
                "dictKey": [
                  "MS_E_YESORNO"
                ],
                "labelField": "dictName",
                "valueField": "dictId",
                "format": "label"
              } 
            }
          },{
            "is_seal_up": {
              "control": "select",
              "label": "抵押物是否存在查封等限制情况",
              "span": 8,
              "rules": [
                {
                  "required": true,
                  "message": "请选择"
                }
              ],
              "dict": {
                "dictType": "MS_E_YESORNO",
                "dictKey": [
                  "MS_E_YESORNO"
                ],
                "labelField": "dictName",
                "valueField": "dictId",
                "format": "label"
              } 
            },"is_have_against_realization": {
              "control": "select",
              "label": "是否存在其他不利于抵押物变现的因素",
              "span": 8,
              "rules": [
                {
                  "required": true,
                  "message": "请选择"
                }
              ],
              "dict": {
                "dictType": "MS_E_YESORNO",
                "dictKey": [
                  "MS_E_YESORNO"
                ],
                "labelField": "dictName",
                "valueField": "dictId",
                "format": "label"
              } 
            },"is_comm_property": {
              "control": "select",
              "label": "是否共同财产",
              "span": 8,
              "rules": [
                {
                  "required": true,
                  "message": "请选择"
                }
              ],
              "dict": {
                "dictType": "MS_E_YESORNO",
                "dictKey": [
                  "MS_E_YESORNO"
                ],
                "labelField": "dictName",
                "valueField": "dictId",
                "format": "label"
              } 
            }
          },{
            "current_situation_of_mortgaged": {
              "control": "select",
              "label": "抵押物现状",
              "span": 8,
              "rules": [
                {
                  "required": true,
                  "message": "请选择"
                }
              ],
              "dict": {
                "dictType": "CO_E_OCCUPS",
                "dictKey": [
                  "CO_E_OCCUPS"
                ],
                "labelField": "dictName",
                "valueField": "dictId",
                "format": "label"
              } 
            },"mortgaged_realization_ability": {
              "control": "select",
              "label": "抵押物处置变现能力",
              "span": 8,
              "rules": [
                {
                  "required": true,
                  "message": "请选择"
                }
              ],
              "dict": {
                "dictType": "CO_E_CASHABILITY",
                "dictKey": [
                  "CO_E_CASHABILITY"
                ],
                "labelField": "dictName",
                "valueField": "dictId",
                "format": "label"
              } 
            }
          },
          {
            "certificate_title_number": {
              "control": "textarea",
              "label": "权利证书编号",
              "width": "180px",
              "align": "center",
              "span": "20",
              "rows": 6,
              "placeholder":"请输入",
              "rules": [
                {
                  "required": true,
                  "message": "请输入"
                }
              ]
            }
          },
          {
            "immovables_unit_number": {
              "control": "textarea",
              "label": "不动产单元号",
              "width": "180px",
              "align": "center",
              "span": "20",
              "rows": 6,
              "placeholder":"请输入",
              "rules": [
                {
                  "required": true,
                  "message": "请输入"
                }
              ]
            }
          },
          {
            "investigation_material":{
                  "control": "dynamic",
                  "label": "贷前调查材料",
                  "rules": [
                    {
                      "required": false,
                      "message": "请上传贷前调查材料"
                    }
                  ],
                  "span": "5",
                  "btnLable": "请选择文件",
                  "toRequest": {
                    "url": "@minioUploadButton",
                    "method": "get",
                    "params": {
                      "file_path": "investigation_material",
                      "formLabel": "附件上传"
                    }
                  }
              }
          }
        ]
      },
      "toolbar": {
        "btns": [
          {
            "type": "submit",
            "label": "保存",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ar1204",
                "apply_no": "${{params.apply_no}}"
                
              }
            }
          }
        ]
      }
    }
  }
  