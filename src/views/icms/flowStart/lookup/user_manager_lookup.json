{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "user_name": {
            "control": "text",
            "label": "用户姓名",
            "rules": [
              {
                "max": 100,
                "message": "长度不能超过100个字符"
              }
            ]    
        },
        
          "branch_id": {
            "control": "text",
            "label": "机构号",
            "rules": [
              {
                "max": 10,
                "message": "长度不能超过10个字符"
              }
            ]
          }
        }
      ]
    },
    "toolbar": {
      "btns": [  
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/rest/auth/userManager",
        "method": "POST",
        "params": {
          "servicecode": "ct2002",
          "user_status":"N"
        }
      },
      "pagination": {
        "show": true,
        "pageSize": 10
      },
      "thead": [
        {
          "label": "用户ID",
          "prop": "user_id"
        },
        {
          "label": "用户姓名",
          "prop": "user_name"
        },
        {
          "label": "机构号",
          "prop": "branch_id"
        },
        {
          "label": "操作",
          "align": "center",
          "width": "200px",
          "btns": [
            {
              "type": "bringback",
              "label": "选择"
            }
          ]
        }
      ]
    }
  }
}
