{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "scope": "houseInfo",
      "name": "houseInfo",
      "btns": [],
      "controls": [
        {
          
          "ppty_pri_name": {
            "label": "楼盘名称",
            "control": "lookup",
            "width": "40px",
            "span": 7,
            "edit": true,
            "keyField": {
              "house_id": "house_id",
              "ppty_pri_name": "house_name",
              "construct_area":"area_msg",
              "evaluated_unit_price":"avg_prise",
              "address":"house_addr",
              "presale_certno":"presell_cert",
              "hs_certno":"prop_certify"
            },
            "toRequest": {
              "url": "/views/co/colsEntryManage/cobAsset/new/diya/house/house_lookup.json",
              "method": "post",
              "params": {}
            },
            "format": "label",
            "rules": [
              {
                "required": true,
                "message": "请输入楼盘名称"
              }
            ]
          },
          "house_id": {
            "control": "hidden",
            "label": "楼盘id",
            "span": 8,
            "width": "150px",
            "value": "05"
          },
          "address": {
            "label": "地址",
            "width": "40px",
            "span": 7,
            "disabled":true,
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入地址"
              }
            ]
          },

          "ppty_status": {
            "label": "楼宇状态",
            "width": "40px",
            "span": 7,
            "control": "select",
            "dict": {
              "dictType": "CO_E_PPTYSTATUS",
              "dictKey": [
                "CO_E_PPTYSTATUS"
              ]
            },
            "showItem": {
              "3": [
                "presale_certno",
                "presale_date"
              ]
            },
            "rules": [
              {
                "required": true,
                "message": "请选择楼宇状态"
              }
            ]
          },

          "buy_price": {
            "control": "currency",
            "label": "购买价格(元)",
            "width": "40px",
            "span": 7,
            "disabled": false,
            "rules": [
              {
                "required": true,
                "message": "请输入购买价格"
              }
            ]
            
          },
          "house_cert_no": {
            "label": "房产证号码",
            "width": "40px",
            "span": 7,
            "control": "text",
            "disabled":false,
            "rules": [
              {
                "required": true,
                "message": "请输入房产证号码"
              }
            ]
          },
          "assetpr_type": {
            "label": "产权类型",
            "width": "40px",
            "span": 7,
            "control": "select",
            "dict": {
              "dictType": "CO_E_ASSTTP",
              "dictKey": [
                "CO_E_ASSTTP"
              ]
            },
            "showItem": {
              "3": [
                "is_transact_pledge_reg"
              ]
            },
            "rules": [
              {
                "required": true,
                "message": "请选择产权类型状态"
              }
            ]
          },
          "ppty_quality": {
            "label": "房屋质量",
            "width": "40px",
            "span": 7,
            "control": "select",
            "dict": {
              "dictType": "CO_E_PPTYQUALITY",
              "dictKey": [
                "CO_E_PPTYQUALITY"
              ]
            }
          },
          "bldg_structure": {
            "label": "房屋结构",
            "width": "40px",
            "span": 7,
            "control": "select",
            "dict": {
              "dictType": "CO_E_BLDGST",
              "dictKey": [
                "CO_E_BLDGST"
              ]
            }
          },
          "prov_code": {
            "label": "省份",
            "width": "40px",
            "span": 7,
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入省份"
              }
            ]
          },
          "region": {
            "label": "地区",
            "width": "40px",
            "span": 7,
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入地区"
              }
            ]
          },
          "house_style": {
            "label": "住宅类型",
            "width": "40px",
            "span": 7,
            "control": "select",
            "dict": {
              "dictType": "CO_E_HOUSETYPE",
              "dictKey": [
                "CO_E_HOUSETYPE"
              ]
            }
          },
          "ppty_estate": {
            "label": "小区名称",
            "width": "40px",
            "span": 7,
            "control": "text"
          },
          "usable_area": {
            "label": "使用面积(平方米)",
            "width": "40px",
            "span": 7,
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入使用面积"
              }
            ]
          },
          "tot_floor": {
            "label": "总楼层",
            "width": "40px",
            "span": 7,
            "control": "text"

          },
          "on_floor": {
            "label": "所在楼层",
            "width": "40px",
            "span": 7,
            "control": "text"
          },
          "ppty_market_type": {
            "label": "市场类型",
            "width": "40px",
            "span": 7,
            "control": "select",
            "dict": {
              "dictType": "CO_E_PPTYMARK",
              "dictKey": [
                "CO_E_PPTYMARK"
              ]
            }
          },
          "traffic_condation": {
            "label": "交通条件",
            "width": "40px",
            "span": 7,
            "control": "select",
            "dict": {
              "dictType": "CO_E_TRAFFICCD",
              "dictKey": [
                "CO_E_TRAFFICCD"
              ]
            }
          },
          "cash_ability": {
            "label": "变现能力",
            "width": "40px",
            "span": 7,
            "control": "select",
            "dict": {
              "dictType": "CO_E_CASHABILITY",
              "dictKey": [
                "CO_E_CASHABILITY"
              ]
            }
          }
          
        }
      ]
    }
  }
}
