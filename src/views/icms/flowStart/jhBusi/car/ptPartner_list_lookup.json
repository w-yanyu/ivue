{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pd0619",
          "sale_prod_cd": "${{params.sale_prod_cd}}"
        }
      },
      "thead": [
        {
          "type": "index"
        },
        {
          "label": "合作方编号",
          "prop": "part_no",
          "align": "center"
        },
        {
          "label": "客户编号",
          "prop": "cust_no",
          "align": "center"
        },
        {
          "label": "合作方名称",
          "prop": "center_name",
          "align": "center"
        },
        {
          "label": "合作方品牌",
          "prop": "center_brand",
          "align": "center"
        },
        {
          "label": "操作",
          "align": "center",
          "fixed": "right",
          "btns": [
            {
              "type": "bringback",
              "label": "选择"
            }
          ]
        }
      ]
    }
  }
}
