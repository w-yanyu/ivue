{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "scope": "formData",
      "initRequest": {
        "url": "/SUMP/call/RPCCall",
        "method": "post",
        "params": {
          "servicecode": "co0035",
          "car_kind_no": "${{params.tree_id}}"
        }
      },
      "btns": [],
      "controls": [
        {
          "brand_no": {
            "label": "brand_no",
            "control": "hidden",
            "width": "140px"
          },
          "brand_name": {
            "label": "brand_name",
            "control": "hidden",
            "width": "140px"
          }
        },
        {
          "car_kind_no": {
            "control": "text",
            "label": "车系编号",
            "width": "140px",
            "span": 12,
            "disabled": true
          },
          "car_kind_name": {
            "control": "text",
            "label": "车系名称",
            "width": "140px",
            "span": 12,
            "disabled": true
          },
          "brand_state": {
            "control": "select",
            "label": "使用状态",
            "width": "140px",
            "span": 12,
            "disabled": true,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "subsidiary_id": {
            "control": "text",
            "label": "子公司编号",
            "width": "140px",
            "span": 12,
            "disabled": true
          },
          "subsidiary": {
            "control": "text",
            "label": "子公司名称",
            "width": "140px",
            "span": 12,
            "disabled": true
          },
          "remark": {
            "label": "备注",
            "control": "textarea",
            "width": "140px",
            "span": 24,
            "disabled": true,
            "autosize": {
              "minRows": 4
            },
            "resize": "none"
          },
          "logo": {
            "control": "text",
            "label": "车系图标",
            "width": "140px",
            "span": 12,
            "disabled": true,
            "events": {
              "condition": {
                "$default": {
                  "control": {
                    "img": {
                      "value": "${{formData.logo}}"
                    }
                  }
                },
                "": {
                  "control": {
                    "img": {
                      "value": "${{formData.logo}}"
                    }
                  }
                }
              }
            }
          },
          "img": {
            "control": "img",
            "value": "${{formData.logo}}",
            "span": 12,
            "width": "150",
            "height": "150",
            "fit": "fill"
          }
        }
      ]
    }
  }
}
