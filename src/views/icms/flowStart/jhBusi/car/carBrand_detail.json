{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "scope": "formData",
      "name": "detail",
      "initRequest": {
        "url": "/SUMP/call/RPCCall",
        "method": "post",
        "params": {
          "servicecode": "co0022",
          "brand_no": "${{params.brand_no}}"
        }
      },
      "btns": [],
      "controls": [
        {
          "brand_no": {
            "control": "text",
            "label": "品牌编号",
            "width": "140px",
            "span": "12",
            "disabled": true
          },
          "brand_name": {
            "control": "text",
            "label": "品牌名称",
            "width": "140px",
            "span": "12",
            "disabled": true
          },
          "brand_state": {
            "control": "select",
            "label": "使用状态",
            "width": "140px",
            "span": "12",
            "disabled": true,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "initial": {
            "control": "text",
            "label": "品牌首字母",
            "width": "140px",
            "span": "12",
            "disabled": true
          },
          "logo": {
            "label": "品牌logo",
            "width": "140px",
            "span": "12",
            "control": "text",
            "disabled": true,
            "events": {
              "condition": {
                "$default": {
                  "control": {
                    "img": {
                      "value": "${{formData.logo}}"
                    }
                  }
                },
                "": {
                  "control": {
                    "img": {
                      "value": "${{formData.logo}}"
                    }
                  }
                }
              }
            }
          },
          "img": {
            "control": "img",
            "value": "${{formData.logo}}",
            "span": "12",
            "width": "150",
            "height": "150",
            "fit": "fill"
          }
        }
      ]
    }
  }
}
