{
    "module": "v-inputDataForm-btn",
    "layout": {
        "form": {
            "btns": [],
            "scope": "templateScope",
            "controls": [
                
                {
                    "cust_name": {
                        "control": "lookup",
                        "label": "客户姓名",
                        "width": "140px",
                        "span": 7,
                        "edit": true,
                        "rules": [
                            {
                                "required": true,
                                "message": "客户名称不能为空!"
                            }
                        ],
                        "keyField": {
                            "cust_name": "cust_name",
                            "cert_type": "cert_type",
                            "cert_no": "cert_no",
                            "cust_no": "cust_no",
                            "mobile_no": "phone_no",
                            "cust_type": "cust_type"
                        },
                        "toRequest": {
                            "url": "/views/icms/flowStart/lookup/customer_lookup.json",
                            "method": "get",
                            "####params": {
                                "valid_state": "1"
                            }
                        },
                        "props": {
                            "title": "选择客户",
                            "width": "75%"
                        }
                    },
                    "cust_no": {
                        "control": "text",
                        "label": "客户编号",
                        "disabled": true,
                        "width": "140px",
                        "span": 7
                    },
                    "mobile_no": {
                        "control": "text",
                        "label": "手机号码",
                        "disabled": true,
                        "width": "140px",
                        "span": 7,
                        "rules": [
                            {
                                "message": "手机号码必须为数字",
                                "type": "number"
                            },
                            {
                                "message": "手机号码不应超过11位",
                                "min": 0,
                                "max": 11
                            }
                        ]
                    }
                },
                {
                    "cust_type": {
                        "control": "select",
                        "label": "客户类型",
                        "width": "140px",
                        "disabled": true,
                        "span": 7,
                        "dict": {
                            "format": "label",
                            "dictKey": [
                                "US_E_CUST_TYPE"
                            ],
                            "dictType": "US_E_CUST_TYPE"
                        }
                    },
                    "cert_type": {
                        "control": "select",
                        "label": "证件类型",
                        "width": "140px",
                        "disabled": true,
                        "span": 7,
                        "rules": [
                            {
                                "required": false,
                                "message": "证件类型不能为空!"
                            }
                        ],
                        "dict": {
                            "dictType": "MS_E_CRET_TYPE",
                            "dictKey": [
                                "MS_E_CRET_TYPE"
                            ],
                            "format": "label"
                        }
                    },
                    "cert_no": {
                        "control": "text",
                        "label": "证件号码",
                        "disabled": true,
                        "width": "140px",
                        "span": 7,
                        "rules": [
                            {
                                "required": false,
                                "message": "证件号码不能为空!"
                            }
                        ]
                    }
                },
                {
                    "loan_no": {
                        "control": "lookup",
                        "label": "展期借据号",
                        "width": "140px",
                        "span": 7,
                        "edit": true,
                        "rules": [
                            {
                                "required": true,
                                "message": "展期借据号不能为空!"
                            }
                        ],
                        "keyField": {
                            "ext_amt": "nrl_prcpl",
                            "loan_no": "loan_due_bill_num",
                            "contract_no": "ctrct_num",
                            "orig_due_date":"matu_dt",
                            "repay_type":"rpymt_way",
                            "loan_form":"loan_form",
                            "sale_prod_name":"slbl_prod_nm",
                            "sale_prod_cd":"slbl_prod_code"
                        },
                        "toRequest": {
                            "url": "/views/icms/flowStart/lookup/loan_info_list_lookup.json",
                            "method": "get",
                            "params": {
                                "cust_no": "${{templateScope.cust_no}}"
                              }
                        },
                        "props": {
                            "title": "选择展期借据",
                            "width": "75%"
                        }
                    },
                    "contract_no": {
                        "control": "text",
                        "label": "合同号",
                        "disabled": true,
                        "width": "140px",
                        "span": 7
                    },
                    "busi_type": {
                        "span": 7,
                        "width": "130px",
                        "label": "业务类型",
                        "value": "LOAN",
                        "control": "remoteSelect",
                        "labelField": "title",
                        "valueField": "id",
                        "disabled": true,
                        "doRequest": {
                          "url": "/SUMP/call/RPCCall",
                          "method": "POST",
                          "params": {
                            "servicecode": "pd0144",
                            "grupno": "0006",
                            "pid": "KSCP"
                          }
                        }
                      }
                },

                {
                    "sale_prod_cd": {
                        "label": "可售产品编号",
                        "disabled": true,
                        "hidden": true,
                        "width": "140px",
                        "span": 7
                    },
                    "sale_prod_name": {
                        "label": "可售产品编号",
                        "disabled": true,
                        "hidden": true,
                        "width": "140px",
                        "span": 7

                    },
                    
                    "apply_type": {
                        "span": 7,
                        "label": "申请类型",
                        "width": "140px",
                        "hidden": true,
                        "value": "T"
                    },
                    "apply_type_name": {
                        "label": "申请类型",
                        "hidden": false,
                        "value": "1",
                        "control": "select",
                        "width": "140px",
                        "disabled": true,
                        "span": 7,
                        "dict": {
                            "dictType": "ICMS_EXTENSION",
                            "dictKey": [
                                "ICMS_EXTENSION"
                            ],
                            "format": "label"
                        }
                    },
                    "ext_amt": {
                        "control": "hidden",
                        "label": "展期金额",
                        "width": "140px",
                        "disabled": true,
                        "span": 7
                    },
                    "orig_due_date": {
                        "control": "hidden",
                        "label": "原到期日期",
                        "align": "center",
                        "disabled": true
                    },
                      "loan_form": {
                        "control": "hidden",
                        "label": "贷款形态",
                        "width": "80px",
                        "span": 7,
                        "disabled": true,
                        "dict": {
                            "dictType": "LN_E_LOAN_FORM",
                            "dictKey": [
                                "LN_E_LOAN_FORM"
                            ],
                            "format": "label"
                        }
                    },
                    "repay_type": {
                        "control": "hidden",
                        "label": "还款方式",
                        "width": "140px",
                        "span": 7,
                        "disabled": false,
                        "rules": [
                            {
                                "required": false,
                                "message": "还款方式不能为空!"
                            }
                        ],
                        "dict": {
                            "dictType": "MS_E_REPAY_WAY",
                            "dictKey": [
                                "MS_E_REPAY_WAY"
                            ],
                            "format": "label"
                        }
                    }
                },
                {
                    "toDbsx": {
                        "span": 24,
                        "unFieldList": [
                            "apply_type",
                            "apply_type_name",
                            "busi_type",
                            "prod_type_name",
                            "sale_prod_cd"
                        ],
                        "control": "dynamic",
                        "#isShowChildrenForm": false,
                        "toRequest": {
                            "method": "GET",
                            "url": "@FlowSubmitBtn",
                            "params": {
                                "appId": "icmsOnl",
                                "servicecode": "ar0027",
                                "finalUrl": "@CommWaitToDoDeal",
                                "trans_type": "ADD"
                            }
                        }
                    }
                }
            ]
        }
    }
}
