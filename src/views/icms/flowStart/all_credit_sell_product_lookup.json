{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "sale_prod_cd": {
            "control": "text",
            "label": "授信产品编号"
          },
          "sale_prod_name": {
            "control": "text",
            "label": "授信产品名称"
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pd0214",
          "prod_status": "00",
          "prod_type":"CL",
          "cust_class":"${{params.cust_class}}"
        }
      },
      "thead": [
        {
          "type": "string",
          "label": "授信产品名称",
          "prop": "sale_prod_name",
          "align": "center"
        },
        {
          "type": "string",
          "label": "授信产品编号",
          "prop": "sale_prod_cd",
          "align": "center"
        },
        {
          "type": "string",
          "label": "产品版本",
          "prop": "prod_version",
          "align": "center"
        },
        {
          "type": "string",
          "label": "授信产品类型",
          "prop": "prod_type_name",
          "align": "center"
        },
        {
          "type": "string",
          "label": "模板编号",
          "prop": "template_id",
          "align": "center"
        },
        {
          "type": "string",
          "label": "审批状态",
          "prop": "appr_status",
          "align": "center",
          "dict": {
            "dictType": "PD_E_APPR_STATUS",
            "dictKey": [
              "PD_E_APPR_STATUS"
            ],
            "format": "label-value"
          }
        },
        {
          "type": "string",
          "label": "产品状态",
          "prop": "prod_status",
          "align": "center",
          "dict": {
            "dictType": "PD_E_PROD_STATUS",
            "dictKey": [
              "PD_E_PROD_STATUS"
            ],
            "format": "label-value"
          }
        },
        {
          "label": "操作",
          "align": "center",
          "width": "200px",
          "btns": [
            {
              "type": "bringback",
              "label": "选择"
            }
          ]
        }
      ]
    }
  }
}
