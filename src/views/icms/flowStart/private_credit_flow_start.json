{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "btns": [],
      "scope": "templateScope",
      "controlsGroup": [
        {
          "title": "申请信息",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "accordion": false,
          "expand": true,
          "controls": [
            {
              "sale_prod_name": {
                "control": "lookup",
                "label": "授信产品",
                "width": "140px",
                "span": 7,
                "edit": true,
                "rules": [
                  {
                    "required": true,
                    "message": "可售产品不能为空!"
                  }
                ],
                "keyField": {
                  "sale_prod_name": "sale_prod_name",
                  "sale_prod_cd": "sale_prod_cd",
                  "busi_type": "prod_type"
                },
                "toRequest": {
                  "url": "/views/icms/flowStart/credit_sell_product_lookup.json",
                  "method": "get",
                  "params": {
                    "cust_class_list":"${{params.cust_class_list}}"
                  }
                },
                "props": {
                  "title": "选择授信产品",
                  "width": "75%"
                },
                "events": {
                  "doRequest": {
                    "url": "/SUMP/call/RPCCall",
                    "method": "POST",
                    "params": {
                      "servicecode": "ar0834",
                      "cert_type": "${{templateScope.cert_type}}",
                      "cert_no": "${{templateScope.cert_no}}",
                      "prod_cd": "${{templateScope.sale_prod_cd}}"
                    },
                    "resultSet": {
                      "pre_credit_amt": "pre_credit_amt",
                      "credit_term": "credit_term",
                      "loan_term": "loan_term",
                      "interest_rate": "interest_rate",
                      "repay_type_list_cd": "repay_type_list_cd"
                    }
                  }
                }
              },
              "sale_prod_cd": {
                "control": "text",
                "label": "可售产品编号",
                "disabled": true,
                "width": "140px",
                "span": 7,
                "rules": [
                  {
                    "required": false,
                    "message": "证件号码不能为空!"
                  }
                ]
              },
              "busi_type": {
                "width": "140px",
                "span": 7,
                "disabled": true,
                "label": "业务类型",
                "control": "remoteSelect",
                "labelField": "title",
                "valueField": "id",
                "doRequest": {
                  "url": "/SUMP/call/RPCCall",
                  "method": "POST",
                  "params": {
                    "servicecode": "pd0144",
                    "grupno": "0006",
                    "pid": "KSCP"
                  }
                },
                "events": {
                  "doRequest": {
                    "url": "/SUMP/call/RPCCall",
                    "method": "post",
                    "params": {
                      "servicecode": "pd0143",
                      "grupno": "0006",
                      "id": "${{templateScope.busi_type}}"
                    },
                    "resultSet": {
                      "prod_type_name": "title"
                    }
                  }
                }
              },
              "prod_type_name": {
                "control": "hidden",
                "label": "业务类型名称"
              }
            },
            {
              "cust_name": {
                "control": "lookup",
                "label": "客户姓名",
                "width": "140px",
                "span": 7,
                "edit": true,
                "rules": [
                  {
                    "required": true,
                    "message": "客户名称不能为空!"
                  }
                ],
                "keyField": {
                  "cust_name": "cust_name",
                  "cert_type": "cert_type",
                  "cert_no": "cert_no",
                  "cust_no": "cust_no",
                  "mobile_no": "mobile_no",
                  "cust_type": "cust_type"
                },
                "toRequest": {
                  "url": "/views/us/privateManage/customer_lookup.json",
                  "method": "get",
                  "####params": {
                    "valid_state": "1"
                  }
                },
                "props": {
                  "title": "选择客户",
                  "width": "75%"
                },
                "events": {
                  "doRequest": {
                    "url": "/SUMP/call/RPCCall",
                    "method": "POST",
                    "params": {
                      "servicecode": "ar0834",
                      "cert_type": "${{templateScope.cert_type}}",
                      "cert_no": "${{templateScope.cert_no}}",
                      "prod_cd": "${{templateScope.sale_prod_cd}}"
                    },
                    "resultSet": {
                      "pre_credit_amt": "pre_credit_amt",
                      "credit_term": "credit_term",
                      "loan_term": "loan_term",
                      "interest_rate": "interest_rate",
                      "repay_type_list_cd": "repay_type_list_cd"
                    }
                  }
                }
              },
              "cust_no": {
                "control": "text",
                "label": "客户编号",
                "disabled": true,
                "width": "140px",
                "span": 7
              },
              "mobile_no": {
                "control": "text",
                "label": "手机号码",
                "disabled": true,
                "width": "140px",
                "span": 7,
                "rules": [
                  {
                    "message": "手机号码必须为数字",
                    "type": "number"
                  },
                  {
                    "message": "手机号码不应超过11位",
                    "min": 0,
                    "max": 11
                  }
                ]
              }
            },
            {
              "cust_type": {
                "control": "select",
                "label": "客户类型",
                "width": "140px",
                "disabled": true,
                "span": 7,
                "dict": {
                  "format": "label",
                  "dictKey": [
                    "US_E_CUST_TYPE"
                  ],
                  "dictType": "US_E_CUST_TYPE"
                }
              },
              "cert_type": {
                "control": "select",
                "label": "证件类型",
                "width": "140px",
                "disabled": true,
                "span": 7,
                "rules": [
                  {
                    "required": false,
                    "message": "证件类型不能为空!"
                  }
                ],
                "dict": {
                  "dictType": "MS_E_CRET_TYPE",
                  "dictKey": [
                    "MS_E_CRET_TYPE"
                  ],
                  "format": "label"
                }
              },
              "cert_no": {
                "control": "text",
                "label": "证件号码",
                "disabled": true,
                "width": "140px",
                "span": 7,
                "rules": [
                  {
                    "required": false,
                    "message": "证件号码不能为空!"
                  }
                ]
              },
              "apply_type": {
                "span": 7,
                "label": "申请类型",
                "width": "140px",
                "hidden": true,
                "value": "S"
              },
              "apply_type_name": {
                "span": 7,
                "label": "申请类型名称",
                "width": "140px",
                "hidden": true,
                "value": "授信新申请"
              }
            },
            {
              "loan_amt": {
                "control": "hidden",
                "label": "授信金额",
                "width": "140px",
                "disabled": false,
                "span": 7,
                "rules": [
                  {
                    "required": false,
                    "message": "授信金额不能为空!"
                  }
                ],
                "format": {
                  "thousand": ","
                }
              },
              "loan_term": {
                "control": "hidden",
                "label": "贷款期限",
                "width": "140px",
                "disabled": false,
                "span": 7,
                "suffix": "M",
                "rules": [
                  {
                    "required": false,
                    "message": "贷款期限不能为空!"
                  }
                ]
              },
              "repay_type": {
                "control": "hidden",
                "label": "还款方式",
                "width": "140px",
                "span": 7,
                "disabled": false,
                "rules": [
                  {
                    "required": false,
                    "message": "还款方式不能为空!"
                  }
                ],
                "dict": {
                  "dictType": "MS_E_REPAY_WAY",
                  "dictKey": [
                    "MS_E_REPAY_WAY"
                  ],
                  "format": "label"
                }
              },
              "is_credit": {
                "control": "hidden",
                "label": "是否授信",
                "width": "140px",
                "value": "0",
                "disabled": true,
                "span": 7
              }
            },
            {
              "toDbsx": {
                "span": 24,
                "unFieldList": [
                  "apply_type"
                ],
                "control": "dynamic",
                "#isShowChildrenForm": false,
                "toRequest": {
                  "method": "GET",
                  "url": "@FlowSubmitBtn",
                  "params": {
                    "appId": "icmsOnl",
                    "servicecode": "ar0000",
                    "finalUrl": "/views/msap/bpm/workbench/workbench_main_tabs.json",
                    "trans_type": "ADD"
                  }
                }
              }
            }
          ]
        },
        {
          "title": "预授信信息",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "accordion": false,
          "expand": true,
          "controls": [
            {
              "pre_credit_amt": {
                "control": "currency",
                "label": "预授信金额(元)",
                "align": "left",
                "disabled": true,
                "width": "140px",
                "span": 7,
                "format": {
                  "thousand": ",",
                  "decimal": 2
                }
              },
              "credit_term": {
                "control": "string",
                "label": "授信期限(月)",
                "span": 7,
                "width": "140px"
              },
              "loan_term": {
                "control": "string",
                "label": "贷款期限(月)",
                "span": 7,
                "width": "140px"
              },
              "interest_rate": {
                "control": "string",
                "label": "利率(%)",
                "span": 7,
                "width": "140px"
              },
              "repay_type_list_cd": {
                "control": "select",
                "multiple": true,
                "disabled": true,
                "span": 14,
                "width": "140px",
                "label": "还款方式列表",
                "dict": {
                  "dictType": "MS_E_REPAY_WAY",
                  "dictKey": [
                    "MS_E_REPAY_WAY"
                  ],
                  "format": "label-value"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
