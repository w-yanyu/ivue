{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "co5001",
          "house_id": "${{params.house_id}}"
        }
      },
      "thead": [
        {
          "type": "string",
          "label": "合作商编号",
          "prop": "partner_no",
          "align": "center"
        },
        {
          "type": "string",
          "label": "合作商名称",
          "prop": "partner_name",
          "align": "center"
        },
        {
          "type": "string",
          "label": "商户收款账号",
          "prop": "acct_num",
          "align": "center"
        },
        {
          "type": "string",
          "label": "商户收款户名",
          "prop": "acct_nm",
          "align": "center"
        },
        {
          "type": "string",
          "label": "商户开户行",
          "prop": "open_acct_bank",
          "align": "center"
        },
        {
          "label": "操作",
          "align": "center",
          "width": "200px",
          "btns": [
            {
              "type": "bringback",
              "label": "选择"
            }
          ]
        }
      ]
    }
  }
}
