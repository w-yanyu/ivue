{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "house_name": {
            "control": "text",
            "label": "楼盘小区名称"
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pt0308",
          "version": "${{params.part_project_version}}",
          "part_project_no": "${{params.part_project_no}}"
        }
      },
      "thead": [
        {
          "type": "string",
          "label": "楼盘编号",
          "prop": "house_id",
          "align": "center"
        },
        {
          "type": "string",
          "label": "楼盘名称",
          "prop": "house_name",
          "align": "center"
        },
        {
          "type": "string",
          "label": "建筑面积信息(平米)",
          "prop": "built_area",
          "align": "center"
        },
        {
          "type": "string",
          "label": "楼盘地址",
          "prop": "house_addr",
          "align": "center"
        },
        {
          "label": "操作",
          "align": "center",
          "width": "200px",
          "btns": [
            {
              "type": "bringback",
              "label": "选择"
            }
          ]
        }
      ]
    }
  }
}
