{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "btns": []
    },
    "toolbar": {
      "btns": []
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/call/RPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ar0701",
          "cust_no": "${{params.cust_no}}"
        }
      },
      "thead": [
        {
          "label": "客户编号",
          "prop": "cust_no",
          "align": "center",
          "hidden": "true"
        },
        {
          "label": "客户名称",
          "prop": "cust_name",
          "align": "center",
          "hidden": "true"
        },
        {
          "label": "合同编号",
          "prop": "contract_no",
          "align": "center",
          "hidden": "true"
        },
        {
          "label": "可售产品",
          "prop": "sale_prod_cd",
          "align": "center"
        },
        {
          "label": "产品名称",
          "prop": "sale_prod_name",
          "align": "center"
        },
        {
          "label": "操作",
          "align": "center",
          "fixed": "right",
          "btns": [
            {
              "type": "bringback",
              "label": "选择"
            }
          ]
        }
      ]
    }
  }
}
