{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/call/RPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ct2173"
        }
      },
      "thead": [
        {
          "label": "机构号",
          "prop": "branch_id",
          "align": "center"
        },
        {
          "label": "机构名称",
          "prop": "branch_name",
          "align": "center"
        },
        {
          "label": "机构电话",
          "prop": "branch_phone",
          "align": "center"
        },
        {
          "label": "机构电子邮箱",
          "prop": "branch_mail",
          "align": "center"
        },
        {
          "label": "机构地址",
          "prop": "branch_address",
          "align": "center"
        },
        {
          "label": "操作",
          "align": "center",
          "fixed": "right",
          "btns": [{
            "type": "bringback",
            "label": "选择"
          }]
        }
      ]
    }
  }
}
