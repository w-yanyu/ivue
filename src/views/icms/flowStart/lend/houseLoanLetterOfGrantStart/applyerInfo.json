{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "scope": "applyerInfo",
      "name": "applyerInfo",
      "btns": [],
      "controls": [
        {
          "cert_day": {
            "control": "dateTimePicker",
            "format": "yyyyMMdd",
            "label": "证件生效日",
            "span": 8,
            "width": "150px",
            "rules": [
              {
                "required": true,
                "message": "证件生效日不能为空!"
              }
            ]
          },
          "card_end_day": {
            "control": "dateTimePicker",
            "format": "yyyyMMdd",
            "label": "证件到期日",
            "span": 8,
            "width": "150px",
            "rules": [
              {
                "required": true,
                "message": "证件到期日不能为空!"
              }
            ]
          },
          "issuing_addr": {
            "control": "areaCascade",
            "title": "请选择签发地址",
            "label": "签发地址",
            "span": 8,
            "width": "150px",
            "rules": [
              {
                "required": true,
                "message": "请输入签发地址"
              },
              {
                "max": 80,
                "message": "长度不能超过80个字符"
              }
            ]
          },
          "education": {
            "control": "select",
            "label": "学历",
            "span": 8,
            "width": "150px",
            "dict": {
              "dictType": "MS_E_MAXEDU",
              "dictKey": [
                "MS_E_MAXEDU"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "学历不能为空!"
              }
            ]
          },
          "marr_state": {
            "control": "select",
            "label": "婚姻状态",
            "span": 8,
            "width": "150px",
            "rules": [
              {
                "required": true,
                "message": "婚姻状态不能为空!"
              }
            ],
            "dict": {
              "dictType": "MS_E_MARITA",
              "dictKey": [
                "MS_E_MARITA"
              ],
              "format": "label"
            }
          },
          "spouse_name": {
            "control": "text",
            "label": "配偶姓名",
            "span": 8,
            "width": "150px"
          },
          "spouse_cert_type": {
            "control": "select",
            "label": "配偶证件类型",
            "span": 8,
            "width": "150px",
            "dict": {
              "dictType": "MS_E_CRET_TYPE",
              "dictKey": [
                "MS_E_CRET_TYPE"
              ],
              "format": "label"
            }
          },
          "spouse_cert_no": {
            "control": "text",
            "label": "配偶证件号码",
            "span": 8,
            "width": "150px"
          },
          "spouse_work_unit_name": {
            "control": "text",
            "label": "配偶工作单位名称",
            "span": 8,
            "width": "150px"
          },
          "spouse_work_phone": {
            "control": "text",
            "label": "配偶工作单位电话",
            "span": 8,
            "width": "150px"
          },
          "is_work": {
            "control": "select",
            "label": "是否有工作单位",
            "span": 8,
            "width": "150px",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "是否有工作单位不能为空!"
              }
            ]
          },
          "unit_nature": {
            "control": "select",
            "label": "单位性质",
            "span": 8,
            "width": "150px",
            "dict": {
              "dictType": "US_E_NUIT_NATURE",
              "dictKey": [
                "US_E_NUIT_NATURE"
              ],
              "format": "label"
            }
          },
          "year_income": {
            "control": "currency",
            "label": "年收入",
            "span": 8,
            "width": "150px",
            "format":{
              "thousand": ",",
              "decimal": 2
            },
            "rules": [
              {
                "required": true,
                "message": "年收入不能为空!"
              }
            ]
          },
          "country_code": {
            "control": "select",
            "label": "国籍",
            "span": 8,
            "width": "150px",
            "dict": {
              "dictType": "US_E_INCTRY",
              "dictKey": [
                "US_E_INCTRY"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "国籍不能为空!"
              }
            ]
          },
          "family_addr": {
            "control": "text",
            "label": "家庭住址",
            "span": 8,
            "width": "150px",
            "rules": [
              {
                "required": true,
                "message": "家庭住址不能为空!"
              }
            ]
          },
          "nation": {
            "control": "select",
            "label": "民族",
            "span": 8,
            "width": "150px",
            "dict": {
              "dictType": "MS_E_NATION",
              "dictKey": [
                "MS_E_NATION"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "民族不能为空!"
              }
            ]
          }
        }
      ]
    }
  }
}
