{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "scope": "houseInfo",
      "name": "houseInfo",
      "btns": [],
      "controls": [
        {
          "loan_kind": {
            "control": "hidden",
            "label": "贷款种类",
            "span": 8,
            "width": "150px",
            "value": "05"
          },
          "house_type": {
            "control": "select",
            "label": "房屋类型",
            "span": 8,
            "width": "150px",
            "dict": {
              "dictType": "AR_E_HOUSE_TYPE",
              "dictKey": [
                "AR_E_HOUSE_TYPE"
              ],
              "format": "label"
            }
          },
          "buy_price": {
            "control": "currency",
            "label": "购买价格",
            "span": 8,
            "width": "150px",
            "format":{
              "thousand": ",",
              "decimal": 2
            }
          },
          "house_address": {
            "control": "text",
            "label": "房屋位置",
            "span": 8,
            "width": "150px"
          },
          "area": {
            "control": "currency",
            "label": "面积",
            "span": 8,
            "width": "150px",
            "format":{
              "thousand": ",",
              "decimal": 2
            }
          },
          "open_acct_bank_name": {
            "control": "lookup",
            "label": "预约业务办理网点",
            "span": 8,
            "width": "150px",
            "keyField":{
              "open_acct_bank": "branch_id",
              "open_acct_bank_name": "branch_name",
              "city": "city",
              "bank_addr": "branch_address"
            },
            "toRequest": {
              "url": "/views/icms/flowStart/lend/houseLoanLetterOfGrantStart/branchSelect_look.json",
              "method": "post"
            },
            "props": {
              "title": "选择办理网点"
            },
            "rules": [
              {
                "required": true,
                "message": "预约业务办理网点不能为空!"
              }
            ]
          },
          "open_acct_bank": {
            "control": "hidden",
            "label": "预约业务办理网点",
            "span": 8,
            "width": "150px"
          },
          "bank_addr": {
            "control": "string",
            "label": "开户行地址",
            "span": 8,
            "width": "150px"
          },
          "city": {
            "control": "string",
            "label": "办理城市",
            "span": 8,
            "width": "150px"
          },
          "house_name": {
            "control": "lookup",
            "label": "楼盘",
            "span": 8,
            "width": "150px",
            "keyField": {
              "house_id": "house_id",
              "house_name": "house_name",
              "head_annotations_loan_whethe": "head_annotations_loan_whethe",
              "handle_estate_properties_whethe": "handle_estate_properties_whethe",
              "urban_properties_whethe": "urban_properties_whethe",
              "margin_deposit_ratio": "margin_deposit_ratio",
              "opening_fund_whethe": "opening_fund_whethe",
              "stage_guarantee_whethe": "stage_guarantee_whethe",
              "houses_number": "houses_number",
              "risk_rate_type": "risk_rate_type"
            },
            "toRequest": {
              "url": "/views/pt/partnerProjrctManage/patrs/building/building_part_lstdetail_lookup.json",
              "method": "post",
              "params": {
                "part_project_no": "${{params.part_project_no}}",
                "version": "${{params.part_project_version}}"
              }
            },
            "props": {
              "width": "80%",
              "title": "选择楼盘"
            }
          },
          "house_id": {
            "control": "text",
            "label": "楼盘编号",
            "span": 8,
            "width": "150px",
            "disabled": true
          },
          "head_annotations_loan_whethe": {
            "control": "select",
            "label": "是否总行已批住房开发贷款项目",
            "span": 8,
            "width": "150px",
            "disabled": true,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "handle_estate_properties_whethe": {
            "control": "select",
            "label": "是否已办理房产“大证”现房楼盘",
            "span": 8,
            "width": "150px",
            "disabled": true,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "urban_properties_whethe": {
            "control": "select",
            "label": "是否城区楼盘",
            "span": 8,
            "width": "150px",
            "disabled": true,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "margin_deposit_ratio": {
            "control": "currency",
            "label": "保证金缴存比例",
            "span": 8,
            "width": "150px",
            "disabled": true,
            "format":{
              "thousand": ",",
              "decimal": 2
            }
          },
          "opening_fund_whethe": {
            "control": "select",
            "label": "是否开立预售资金监管账户",
            "span": 8,
            "width": "150px",
            "disabled": true,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "stage_guarantee_whethe": {
            "control": "select",
            "label": "开发商是否提供阶段性担保",
            "span": 8,
            "width": "150px",
            "disabled": true,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "houses_number": {
            "control": "text",
            "label": "房屋总套数",
            "span": 8,
            "width": "150px",
            "disabled": true
          },
          "risk_rate_type": {
            "control": "select",
            "label": "风险评级分类",
            "span": 8,
            "width": "150px",
            "disabled": true,
            "dict": {
              "dictType": "PT_E_RISK_RATE_TYPE",
              "dictKey": [
                "PT_E_RISK_RATE_TYPE"
              ],
              "format": "label"
            }
          }
        }
      ]
    }
  }
}
