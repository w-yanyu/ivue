{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "scope": "flowCom",
      "name": "flowCom",
      "btns": [],
      "controls": [
        {
          "sale_prod_cd": {
            "control": "string",
            "label": "可售产品编号",
            "span": 8,
            "width": "150px",
            "value": "${{params.sale_prod_cd}}"
          },
          "sale_prod_name": {
            "control": "string",
            "label": "可售产品名称",
            "span": 8,
            "width": "150px",
            "value": "${{params.sale_prod_name}}"
          },
          "cust_name": {
            "control": "text",
            "label": "客户姓名",
            "span": 8,
            "width": "150px",
            "rules": [
              {
                "required": true,
                "message": "客户姓名不能为空!"
              }
            ]
          },
          "cert_type": {
            "control": "select",
            "label": "证件类型",
            "span": 8,
            "width": "150px",
            "rules": [
              {
                "required": true,
                "message": "证件类型不能为空!"
              }
            ],
            "dict": {
              "dictType": "MS_E_CRET_TYPE",
              "dictKey": [
                "MS_E_CRET_TYPE"
              ],
              "format": "label"
            }
          },
          "cert_no": {
            "control": "text",
            "label": "证件号码",
            "span": 8,
            "width": "150px",
            "rules": [
              {
                "required": true,
                "message": "证件号码不能为空!"
              }
            ]
          },
          "mobile_no": {
            "control": "text",
            "label": "手机号码",
            "span": 8,
            "width": "150px",
            "rules": [
              {
                "required": true,
                "message": "手机号码不能为空!"
              }
            ]
          },
          "loan_amt": {
            "control": "currency",
            "label": "贷款金额",
            "span": 8,
            "width": "150px",
            "format":{
              "thousand": ",",
              "decimal": 2
            },
            "rules": [
              {
                "required": true,
                "message": "贷款金额不能为空!"
              }
            ]
          },
          "loan_term": {
            "control": "inputNumber",
            "label": "贷款期限",
            "span": 8,
            "width": "150px",
            "controls": false,
            "rules": [
              {
                "required": true,
                "message": "贷款期限不能为空!"
              }
            ]
          }
        }
      ]
    }
  }
}
