{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "cust_no": {
            "control": "text",
            "label": "客户编号",
            "rules": [
              {
                "max": 32,
                "message": "长度不能超过32个字符"
              }
            ]
          },
          "order_id": {
            "control": "text",
            "label": "申请编号"


          },
          "cl_type_cd": {
            "control": "text",
            "label": "产品编号"

          }


        }
      ]
    },
    "toolbar": {

    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "cl0037",
          "status": "Y",
          "cl_level": "P"

        }
      },
      "thead": [
        {
          "label": "申请编号",
          "prop": "order_id",
          "align": "center"
        },
        {
          "label": "客户号",
          "prop": "cust_no",
          "align": "center"
        },
        {
          "label": "客户名称",
          "prop": "cust_name",
          "align": "center"
        },
        {
          "label": "产品编号",
          "prop": "cl_type_cd",
          "align": "center"
        },
        {
          "label": "额度金额",
          "prop": "cl_amt",
          "align": "center",
          "format":{
            "thousand":","
          }
        },
        {
          "label": "可用余额",
          "prop": "avail_amt",
          "align": "center",
          "format":{
            "thousand":","
          }
        },
        {
          "label": "产品名称",
          "prop": "cl_prod_name",
          "align": "center"
        },
        {
          "label": "额度类型名称",
          "prop": "cl_type_name",
          "align": "center"
        },
        {
          "label": "有效状态",
          "prop": "status",
          "align": "center",
          "dict": {
            "dictType": "CL_E_STATUS",
            "dictKey": [
              "CL_E_STATUS"
            ],
            "format": "label",
            "labelField": "dictName",
            "valueField": "dictId"
          }
        },
        {
          "label": "操作",
          "align": "center",
          "fixed": "right",
          "btns": [
            {
              "type": "custom",
              "label": "打印",
              "toRequest": {
                "url": "@LetterGuarantee",
                "params": {
                  "apply_no": "${{current.order_id}}"

                },
                "props": {
                  "width": "95%",
                  "height": "590px",
                  "title": "担保意向函"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
