{
  "module": "v_search_btn_datagrid",
  "layout": {
    "toolbar": {
      "btns": []
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ar1402",        
          "cust_no": "${{params.cust_no}}"
        }
      },
      "thead": [
        {
          "label": "额度申请号",
          "prop": "apply_no",
          "align": "center"
        },
        {
          "label": "额度合同号",
          "prop": "contract_no",
          "align": "center",
          "width": "160px"
        },
        {
          "label": "客户号",
          "prop": "cust_no",
          "align": "center"
        },
        {
          "label": "客户名称",
          "prop": "cust_name",
          "align": "center",
          "width": "130px"
        },
        {
          "label": "额度金额",
          "prop": "apply_amt",
          "align": "center",
          "width": "100px",
          "format":{
            "thousand":","
          }
        },
        {
          "label": "期限",
          "prop": "appr_term",
          "align": "center",
          "width": "100px"
        },
        {
          "label": "产品编号",
          "prop": "loan_prod_cd",
          "width": "160px",
          "align": "center"
        },
        {
          "label": "产品名称",
          "prop": "loan_prod_name",
          "width": "160px",
          "align": "center"
        },
        {
          "label": "生效日期",
          "prop": "effective_date",
          "align": "center"
        },
        {
          "label": "失效日期",
          "prop": "matu_date",
          "align": "center"
        },
        {
          "label": "操作",
          "align": "center",
          "fixed": "right",
          "btns": [
            {
              "type": "bringback",
              "label": "选择"
            }
          ]
        }
      ]
    }
  }
}
