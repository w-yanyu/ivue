{
  "module": "v_search_btn_datagrid",
  "layout": {
    "toolbar": {
      "btns": []
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ar1309",
          "cust_no": "${{params.cust_no}}"
        }
      },
      "thead": [
        {
          "label": "合同编号",
          "prop": "loan_contract_no",
          "align": "center"
        },
        {
          "label": "可售产品编号",
          "prop": "sale_prod_cd",
          "align": "center"
        },
        {
          "label": "可售产品名称",
          "prop": "sale_prod_name",
          "align": "center"
        },
        {
          "label": "审批期限",
          "prop": "appr_term",
          "align": "center"
        },
        {
          "label": "审批金额",
          "prop": "appr_amt",
          "align": "center",
          "format":{
            "thousand":","
          }
        },
        {
          "label": "合同余额",
          "prop": "contract_bal",
          "align": "center",
          "format":{
            "thousand":","
          }
        },
        {
          "label": "申请币种",
          "prop": "ccy_code",
          "dict": {
            "dictType": "MS_CURRENCY",
            "dictKey": "MS_CURRENCY",
            "format": "label"
          },
          "align": "center"
        },
        {
          "label": "操作",
          "align": "center",
          "fixed": "right",
          "btns": [
            {
              "type": "bringback",
              "label": "选择"
            }
          ]
        }
      ]
    }
  }
}
