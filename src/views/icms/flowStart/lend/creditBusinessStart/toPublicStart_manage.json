{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "scope": "formData",
      "btns": [],
      "controls": [
        {
          "cust_no": {
            "control": "hidden",
            "label": "cust_no"
          },
          "apply_no": {
            "control": "hidden",
            "label": "apply_no"
          },
          "loan_amt": {
            "control": "hidden",
            "label": "放款金额"
          },
          "cust_type": {
            "control": "hidden",
            "label": "cust_type"
          },
          "apply_type": {
            "control": "hidden",
            "width": "10px",
            "value": "O",
            "label": "申请类型"
          },
          "cust_name": {
            "control": "lookup",
            "label": "客户名称",
            "width": "10px",
            "keyField": {
              "cust_no": "cust_no",
              "cust_name": "cust_name",
              "cert_type": "cert_type",
              "cert_no": "cert_no",
              "mobile_no": "phone_no",
              "cust_type": "cust_type"
            },
            "events": {
              "set": {
                "limit_no": ""
              }
            },
            "toRequest": {
              "url": "/views/us/entManager/ent_cust_lookup.json",
              "method": "get"
            },
            "props": {
              "title": "选择客户",
              "width": "75%"
            },
            "format": "label",
            "rules": [
              {
                "required": true,
                "message": "请选择客户名称"
              }
            ]
          },
          "limit_no": {
            "control": "lookup",
            "#span": 12,
            "label": "额度编号",
            "width": "10px",
            "keyField": {
              "limit_no": "prod_credit_seq",
              "loan_amt": "available_use_limit",
              "apply_no": "apply_no",
              "sale_prod_cd": "prod_code",
              "sale_prod_name": "prod_name"
            },
            "toRequest": {
              "url": "/views/icms/flowStart/lend/creditBusinessStart/credit_limit_lookup2.json",
              "method": "get",
              "params": {
                "cust_no": "${{formData.cust_no}}",
                "prod_code": "00003501",
                "source_system": "001",
                "cust_class":"${{formData.cust_type}}"
              }
            },
            "props": {
              "title": "请选择额度编号"
            },
            "format": "label",
            "rules": [
              {
                "required": true,
                "message": "请选择额度编号"
              }
            ]
          },
          "busi_type": {
            "control": "hidden",
            "width": "10px",
            "value": "LOAN",
            "label": "业务类型"
          },
          "prod_type_name": {
            "control": "string",
            "width": "10px",
            "value": "贷款",
            "label": "业务类型名称"
          },
          "mobile_no": {
            "control": "text",
            "width": "10px",
            "label": "手机号码",
            "disabled": true,
            "rules": [
              {
                "required": false,
                "message": "请输入手机号码"
              }
            ]
          },
          "cert_type": {
            "control": "select",
            "label": "证件类型",
            "#span": 12,
            "width": "10px",
            "disabled": true,
            "dict": {
              "dictType": "MS_E_CRET_TYPE",
              "dictKey": [
                "MS_E_CRET_TYPE"
              ],
              "labelField": "dictName",
              "valueField": "dictId"
            }
          },
          "cert_no": {
            "control": "text",
            "#span": 12,
            "width": "10px",
            "label": "证件号码",
            "disabled": true
          },
          "sale_prod_cd": {
            "control": "text",
            "label": "可售产品",
            "#span": 12,
            "width": "10px",
            "disabled": true
          },
          "sale_prod_name": {
            "control": "text",
            "label": "产品名称",
            "#span": 12,
            "width": "10px",
            "disabled": true
          },
          "ccy_code": {
            "control": "select",
            "#span": 12,
            "label": "币种",
          "selectFirstValue": "true",
            "width": "10px",
            "disabled": true,
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": [
                "MS_CURRENCY"
              ],
              "format": "label"
            }
          },
          "toDbsx": {
            "span": 24,
            "control": "dynamic",
            "unFieldList": [
              "apply_type",
              "busi_type",
              "prod_type_name",
              "ccy_code"
            ],
            "#isShowChildrenForm": false,
            "toRequest": {
              "method": "GET",
              "url": "@FlowSubmitBtn",
              "params": {
                "appId": "icmsOnl",
                "servicecode": "ar0022",
                "finalUrl": "@CommWaitToDoDeal"
              }
            }
          }
        }
      ]
    }
  }
}
