{
    "module": "v-inputDataForm-btn",
    "layout": {
        "form": {
            "btns": [],
            "scope": "templateScope",
            "controls": [
                {
                    "sale_prod_name": {
                        "control": "lookup",
                        "label": "可售产品",
                        "width": "140px",
                        "span": 7,
                        "edit": true,
                        "rules": [
                            {
                                "required": true,
                                "message": "可售产品不能为空!"
                            }
                        ],
                        "keyField": {
                            "sale_prod_name": "sale_prod_name",
                            "sale_prod_cd": "sale_prod_cd",
                            "busi_type": "prod_type"
                        },
                        "toRequest": {
                            "url": "/views/icms/flowStart/letter_sell_product_lookup.json",
                            "method": "get"
                        },
                        "props": {
                            "title": "选择产品",
                            "width": "75%"
                        }
                    },
                    "sale_prod_cd": {
                        "control": "text",
                        "label": "可售产品编号",
                        "disabled": true,
                        "width": "140px",
                        "span": 7,
                        "rules": [
                            {
                                "required": false,
                                "message": "可售产品编号为空!"
                            }
                        ]
                    },
                    "busi_type": {
                        "width": "140px",
                        "span": 7,
                        "disabled": true,
                        "label": "业务类型",
                        "control": "remoteSelect",
                        "labelField": "title",
                        "valueField": "id",
                        "doRequest": {
                            "url": "/SUMP/call/RPCCall",
                            "method": "POST",
                            "params": {
                                "servicecode": "pd0144",
                                "grupno": "0006",
                                "pid": "KSCP"
                            }
                        },
                        "events": {
                            "doRequest": {
                                "url": "/SUMP/call/RPCCall",
                                "method": "post",
                                "params": {
                                    "servicecode": "pd0143",
                                    "grupno": "0006",
                                    "id": "${{templateScope.busi_type}}"
                                },
                                "resultSet": {
                                    "prod_type_name": "title"
                                }
                            }
                        }
                    },
                    "prod_type_name": {
                        "control": "hidden",
                        "label": "业务类型名称"
                    }
                },
                {
                    "cust_name": {
                        "control": "lookup",
                        "label": "客户姓名",
                        "width": "140px",
                        "span": 7,
                        "edit": true,
                        "rules": [
                            {
                                "required": true,
                                "message": "客户名称不能为空!"
                            }
                        ],
                        "keyField": {
                            "cust_name": "cust_name",
                            "cert_type": "cert_type",
                            "cert_no": "cert_no",
                            "cust_no": "cust_no",
                            "mobile_no": "phone_no",
                            "cust_type": "cust_type"
                        },
                        "toRequest": {
                            "url": "/views/us/entManager/ent_cust_lookup.json",
                            "method": "get",
                            "####params": {
                                "valid_state": "1"
                            }
                        },
                        "props": {
                            "title": "选择客户",
                            "width": "75%"
                        }
                    },
                    "cust_no": {
                        "control": "text",
                        "label": "客户编号",
                        "disabled": true,
                        "width": "140px",
                        "span": 7
                    },
                    "mobile_no": {
                        "control": "text",
                        "label": "电话号码",
                        "disabled": true,
                        "width": "140px",
                        "span": 7,
                        "rules": [
                            {
                                "message": "手机号码必须为数字",
                                "type": "number"
                            },
                            {
                                "message": "手机号码不应超过11位",
                                "min": 0,
                                "max": 11
                            }
                        ]
                    }
                },
                {
                    "cust_type": {
                        "control": "select",
                        "label": "客户类型",
                        "width": "140px",
                        "disabled": true,
                        "span": 7,
                        "dict": {
                            "format": "label",
                            "dictKey": [
                                "US_E_CUST_TYPE"
                            ],
                            "dictType": "US_E_CUST_TYPE"
                        }
                    },
                    "cert_type": {
                        "control": "select",
                        "label": "证件类型",
                        "width": "140px",
                        "disabled": true,
                        "span": 7,
                        "rules": [
                            {
                                "required": false,
                                "message": "证件类型不能为空!"
                            }
                        ],
                        "dict": {
                            "dictType": "MS_E_CRET_TYPE",
                            "dictKey": [
                                "MS_E_CRET_TYPE"
                            ],
                            "format": "label"
                        }
                    },
                    "cert_no": {
                        "control": "text",
                        "label": "证件号码",
                        "disabled": true,
                        "width": "140px",
                        "span": 7,
                        "rules": [
                            {
                                "required": false,
                                "message": "证件号码不能为空!"
                            }
                        ]
                    },
                    "apply_type": {
                        "span": 7,
                        "control": "string",
                        "label": "申请类型",
                        "width": "140px",
                        "hidden": true,
                        "value": "BHSQ"
                    },
                    "apply_type_name": {
                        "span": 7,
                        "control": "hidden",
                        "label": "申请类型名称",
                        "width": "140px",
                        "hidden": false,
                        "value": "保函开立申请"
                    }
                },
                {
                    "loan_amt": {
                        "control": "hidden",
                        "label": "授信金额",
                        "width": "140px",
                        "disabled": false,
                        "span": 7,
                        "rules": [
                            {
                                "required": false,
                                "message": "授信金额不能为空!"
                            }
                        ],
                        "format":{
                            "thousand":","
                          }
                    },
                    "loan_term": {
                        "control": "hidden",
                        "label": "贷款期限",
                        "width": "140px",
                        "disabled": false,
                        "span": 7,
                        "suffix": "M",
                        "rules": [
                            {
                                "required": false,
                                "message": "贷款期限不能为空!"
                            }
                        ]
                    },
                    "is_credit": {
                        "control": "hidden",
                        "label": "是否授信",
                        "width": "140px",
                        "value": "0",
                        "disabled": true,
                        "span": 7
                    }
                },
                {
                    "toDbsx": {
                        "span": 24,
                        "unFieldList": [
                            "apply_type",
                            "busi_type"
                        ],
                        "control": "dynamic",
                        "#isShowChildrenForm": false,
                        "toRequest": {
                            "method": "GET",
                            "url": "@FlowSubmitBtn",
                            "params": {
                                "appId": "icmsOnl",
                                "servicecode": "ar0003",
                                "finalUrl": "/views/msap/bpm/workbench/workbench_main_tabs.json",
                                "trans_type": "ADD"
                            }
                        }
                    }
                }
            ]
        }
    }
}
