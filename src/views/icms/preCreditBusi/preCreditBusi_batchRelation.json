{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "isInitSearch": true,
      "scope": "searchTable",
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ar0832",
          "batch_no": "${{params.batch_no}}"
        }
      },
      "thead": [
        {
          "label": "序号",
          "type": "index"
        },
        {
          "type": "string",
          "label": "白名单编号",
          "prop": "white_list_no",
          "align": "center",
          "width": "200px"
        },
        {
          "type": "string",
          "label": "产品编号",
          "prop": "prod_cd",
          "align": "center",
          "width": "100px"
        },
        {
          "type": "string",
          "label": "证件类型",
          "prop": "cert_type",
          "align": "center",
          "width": "100px",
          "dict": {
            "dictType": "MS_E_CRET_TYPE",
            "dictKey": [
              "MS_E_CRET_TYPE"
            ],
            "format": "label"
          }
        },
        {
          "type": "string",
          "label": "证件号码",
          "prop": "cert_no",
          "align": "center",
          "width": "200px"
        },
        {
          "type": "string",
          "label": "客户姓名",
          "prop": "cust_name",
          "align": "center",
          "width": "100px"
        },
        {
          "type": "string",
          "label": "国籍",
          "prop": "country_code",
          "align": "center",
          "dict": {
            "dictType": "US_E_INCTRY",
            "dictKey": [
              "US_E_INCTRY"
            ],
            "format": "label"
          }
        },
        {
          "type": "string",
          "label": "婚姻状态",
          "prop": "marr_state",
          "align": "center",
          "dict": {
            "dictType": "MS_E_MARITA",
            "dictKey": [
              "MS_E_MARITA"
            ],
            "format": "label"
          }
        },
        {
          "type": "string",
          "label": "手机号码",
          "prop": "mobile_no",
          "align": "center",
          "width": "150px"
        },
        {
          "type": "string",
          "label": "客户经理",
          "prop": "cust_manager_no",
          "align": "center",
          "width": "150px",
          "dict": {
            "dictType": "deciOnl",
            "dictKey": [],
            "labelField": "user_name",
            "valueField": "user_id",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "length": 1000,
                "start": 1,
                "servicecode": "ct2502"
              }
            },
            "format": "value-label"
          }
        },
        {
          "type": "string",
          "label": "预授信金额",
          "prop": "pre_credit_amt",
          "align": "center"
        },
        {
          "type": "string",
          "label": "授信期限(月)",
          "prop": "credit_term",
          "align": "center"
        },
        {
          "type": "string",
          "label": "预授信登记日期",
          "prop": "pre_credit_regi_date",
          "align": "center"
        },
        {
          "type": "string",
          "label": "预授信有效期(天)",
          "prop": "pre_credit_period",
          "align": "center"
        },
        {
          "type": "string",
          "label": "预授信失效日期",
          "prop": "pre_credit_end_date",
          "align": "center"
        },
        {
          "type": "string",
          "label": "执行状态",
          "prop": "pre_credit_task_status",
          "align": "center",
          "width": "100px",
          "dict": {
            "dictType": "AR_E_PRE_CREDIT_LIST_STATUS",
            "dictKey": [
              "AR_E_PRE_CREDIT_LIST_STATUS"
            ],
            "format": "label"
          }
        },
        {
          "label": "操作",
          "fixed": "right",
          "width": 150,
          "align": "center",
          "btns": [
            {
              "type": "look",
              "label": "详情",
              "toRequest": {
                "url": "/views/icms/preCreditBusi/preCreditList/preCreditList_task_info.json",
                "method": "POST"
              },
              "props": {
                "width": "65%",
                "title": "客户预授信详情"
              }
            }
          ]
        }
      ],
      "pagination": {
        "show": true
      }
    }
  }
}
