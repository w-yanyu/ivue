{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "amt_purpose_name": {
            "control": "text",
            "label": "用途名称"
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ar0247"
        }
      },
      "thead": [
        {
          "type":"string",
          "label": "贷款用途编号",
          "prop": "amt_purpose_no",
          "align": "center"
        },
        {
          "type":"string",
          "label": "贷款用途名称",
          "prop": "amt_purpose_name",
          "align": "center"
        },
        {
          "type":"string",
          "label": "贷款用途分类编号",
          "prop": "amt_purpose_type_no",
          "align": "center"
        },
        {
          "label": "操作",
          "fixed": "right",
          "width": "200",
          "align": "center",
          "btns": [
            {
              "type": "bringback",
              "label": "选择"
            }
          ]
        }
      ]
    }
  }
}
