{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "scope": "formData",
      "name": "preCreditInfo",
      "btns": [
        {
          "type": "submit",
          "label": "启动",
          "doRequest": {
            "url": "/SUMP/call/RPCCall",
            "method": "POST",
            "params": {
              "servicecode": "ar0036",
              "condition": {
                "cert_type": "${{formData.cert_type}}",
                "trxn_teller": "$g{{userInfo.userCode}}",
                "mobile_no": "${{formData.mobile_no}}",
                "teller_name": "$g{{userInfo.userName}}",
                "open_acct_bank": "",
                "cycling_flag": "0",
                "repay_cycle": "",
                "cust_type": "10",
                "cust_no": "",
                "cust_name": "${{formData.cust_name}}",
                "sale_prod_cd": "00003501",
                "contract_no": "",
                "effect_date": "",
                "bind_card_no": "",
                "cert_no": "${{formData.cert_no}}",
                "apply_type_name": "预授信申请",
                "is_inherit_files": "0",
                "sale_prod_name": "个人授信",
                "loan_use": "${{formData.amt_purpose_name}}",
                "branch_org": "$g{{userInfo.branch_id}}",
                "busi_type": "CL",
                "prod_type_name": "",
                "parent_apply_id": "",
                "trust_pay_flag": "0",
                "loan_inst_rate": 0,
                "apply_type": "YSX"
              }
            }
          }
        }
      ],
      "controls": [
        {
          "prod_name": {
            "control": "lookup",
            "label": "产品编号",
            "width": "50px",
            "span": 8,
            "rules": [
              {
                "required": true,
                "message": "可售产品不能为空!"
              }
            ],
            "keyField": {
              "prod_name": "sale_prod_name",
              "prod_cd": "sale_prod_cd"
            },
            "toRequest": {
              "url": "/views/icms/flowStart/all_credit_sell_product_lookup.json",
              "method": "get",
              "params": {
                "cust_class":"10"
              }
            },
            "props": {
              "title": "选择授信产品",
              "width": "75%"
            }
          },
          "prod_cd": {
            "control": "hidden",
            "label": "可售产品编号",
            "width": "50px",
            "span": 8,
            "value": "${{params.sale_prod_name}}"
          },
          "cust_name": {
            "control": "text",
            "label": "客户姓名",
            "width": "50px",
            "span": 8
          },
          "cert_type": {
            "control": "select",
            "label": "证件类型",
            "width": "50px",
            "span": 8,
            "dict": {
              "dictType": "MS_E_CRET_TYPE",
              "dictKey": [
                "MS_E_CRET_TYPE"
              ],
              "format": "label"
            }
          },
          "cert_no": {
            "control": "text",
            "label": "证件号码",
            "width": "50px",
            "span": 8
          },
          "cert_issue_date": {
            "control": "dateTimePicker",
            "label": "证件签发日期",
            "format": "yyyyMMdd",
            "width": "50px",
            "span": 8
          },
          "cert_expiry_date": {
            "control": "dateTimePicker",
            "label": "证件到期日期",
            "format": "yyyyMMdd",
            "width": "50px",
            "span": 8
          },
          "cert_addr": {
            "control": "text",
            "label": "证件签发地址",
            "width": "50px",
            "span": 8
          },
          "country_code": {
            "control": "select",
            "label": "国籍",
            "width": "50px",
            "span": 8,
            "dict": {
              "dictType": "US_E_INCTRY",
              "dictKey": [
                "US_E_INCTRY"
              ],
              "format": "label"
            }
          },
          "birth_date": {
            "control": "dateTimePicker",
            "label": "出生日期",
            "format": "yyyyMMdd",
            "width": "50px",
            "span": 8
          },
          "marr_state": {
            "control": "select",
            "label": "婚姻状态",
            "width": "50px",
            "span": 8,
            "dict": {
              "dictType": "MS_E_MARITA",
              "dictKey": [
                "MS_E_MARITA"
              ],
              "format": "label"
            }
          },
          "max_educat_backg": {
            "control": "select",
            "label": "最高学历",
            "width": "50px",
            "span": 8,
            "dict": {
              "dictType": "MS_E_MAXEDU",
              "dictKey": [
                "MS_E_MAXEDU"
              ],
              "format": "label"
            }
          },
          "max_educat_degree": {
            "control": "select",
            "label": "最高学位",
            "width": "50px",
            "span": 8,
            "dict": {
              "dictType": "MS_E_MAXDEG",
              "dictKey": [
                "MS_E_MAXDEG"
              ],
              "format": "label"
            }
          },
          "unit_nature": {
            "control": "select",
            "label": "单位性质",
            "width": "50px",
            "span": 8,
            "dict": {
              "dictType": "US_E_NUIT_NATURE",
              "dictKey": [
                "US_E_NUIT_NATURE"
              ],
              "format": "label"
            }
          },
          "unit_industry": {
            "control": "lookup",
            "width": "50px",
            "span": 8,
            "label": "单位所属行业",
            "placeholder": "点击行业",
            "toRequest": {
              "url": "@jsonLookUpTree",
              "method": "post",
              "params": {
                "treeTitle": "单位所属行业",
                "treeType": "1",
                "field_name": "unit_industry"
              }
            },
            "keyField": {
              "unit_industry": "node_name"
            }
          },
          "job_corp": {
            "control": "text",
            "label": "工作单位",
            "width": "50px",
            "span": 8
          },
          "year_income": {
            "control": "currency",
            "label": "年收入(元)",
            "width": "50px",
            "span": 8,
            "format": {
              "thousand": ",",
              "decimal": "2"
            }
          },
          "mobile_no": {
            "control": "text",
            "label": "手机号码",
            "width": "50px",
            "span": 8
          },
          "total_household": {
            "control": "text",
            "label": "家庭总负债金额(元)",
            "width": "50px",
            "span": 8
          },
          "live_state": {
            "control": "select",
            "label": "居住状况",
            "width": "50px",
            "span": 8,
            "dict": {
              "dictType": "AR_E_LIVE_STATE",
              "dictKey": [
                "AR_E_LIVE_STATE"
              ],
              "format": "label"
            }
          },
          "amt_purpose_no": {
            "control": "hidden",
            "label": "资金用途编号",
            "width": "50px",
            "span": 8
          },
          "amt_purpose_name": {
            "control": "lookup",
            "label": "资金用途",
            "width": "50px",
            "span": 8,
            "edit": false,
            "rows": 4,
            "keyField": {
              "amt_purpose_no": "amt_purpose_no",
              "loan_purpose_desc": "amt_purpose_name"
            },
            "toRequest": {
              "url": "/views/icms/preCreditBusi/preCreditList/amturpose_lookup.json",
              "method": "get"
            },
            "multiple": false,
            "props": {
              "title": "资金用途"
            }
          },
          "cust_manager_no": {
            "control": "hidden",
            "label": "客户经理编号",
            "value": "$g{{user_info.userCode}}",
            "width": "50px",
            "span": 8
          },
          "is_proxy_pay_wage_cust": {
            "control": "select",
            "label": "是否我行代发工资客户",
            "width": "50px",
            "span": 8,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "is_stock_busi_cust": {
            "control": "select",
            "label": "是否我行存量业务客户",
            "width": "50px",
            "span": 8,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "deposit_position": {
            "control": "select",
            "label": "在我行存款档位(万元)",
            "width": "50px",
            "span": 8,
            "dict": {
              "dictType": "AR_E_DEPOSIT_POSITION",
              "dictKey": [
                "AR_E_DEPOSIT_POSITION"
              ],
              "format": "label"
            }
          },
          "valid_loan_num": {
            "control": "text",
            "label": "有效贷款笔数",
            "width": "50px",
            "span": 8
          },
          "loan_bal": {
            "control": "currency",
            "label": "贷款余额(元)",
            "width": "50px",
            "span": 8,
            "format": {
              "thousand": ",",
              "decimal": 2
            }
          },
          "live_stability": {
            "control": "select",
            "label": "居住稳定性",
            "width": "50px",
            "span": 8,
            "dict": {
              "dictType": "AR_E_LIVE_STABILITY_TYPE",
              "dictKey": [
                "AR_E_LIVE_STABILITY_TYPE"
              ],
              "format": "label"
            }
          },
          "month_net_income": {
            "control": "currency",
            "label": "月净收入(元)",
            "width": "50px",
            "span": 8,
            "format": {
              "thousand": ",",
              "decimal": 2
            }
          }
        }
      ]
    }
  }
}
