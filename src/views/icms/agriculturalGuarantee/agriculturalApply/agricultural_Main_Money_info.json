{
  "module": "v-realonlyForm-btn",
  "layout": {
    "form": {
      "scope": "formData",
      "initRequest": "${{params}}",
      "controls": [
        {
          "prod_name": {
            "label": "业务产品",
            "control": "string",
            "width": "130px",
            "span": 7
          },
          "loan_prod_cd": {
            "label": "产品编号",
            "control": "hidden",
            "width": "130px",
            "format": "label"
          },
          "base_ccy": {
            "control": "string",
            "label": "基准币种",
            "width": "130px",
            "disabled": true,
            "span": 7,
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": [
                "MS_CURRENCY"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": false,
                "message": "请选择币种"
              }
            ] 
          },
          "ccy_code": {
            "control": "string",
            "label": "币种",
            "width": "130px",
            "disabled": true,
            "span": 7,
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": [
                "MS_CURRENCY"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": false,
                "message": "请选择币种"
              }
            ] 
          },
          "exchange_rate": {
            "control": "string",
            "label": "汇率",
            "width": "130px",
            "value": 1,
            "disabled": true,
            "span": 7,
            "rules": [
              {
                "required": false,
                "message": "请选择汇率"
              }
            ]
          },
          "cycling_flag": {
            "control": "string",
            "label": "是否可循环使用金额",
            "width": "130px",
            "span": 7,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": false,
                "message": "请选择是否可循环使用金额"
              }
            ]
          },
          "credit_term": {
            "control": "string",
            "label": "业务期限(月)",
            "width": "130px",
            "span": 7,
            "rules": [
              {
                "required": false,
                "message": "请选择业务期限(月)"
              }
            ]
          },
          "busi_assign_amt": {
            "control": "string",
            "label": "业务担保分配金额",
            "width": "130px",
            "span": 7,
            "rules": [
              {
                "required": false,
                "message": "请选择业务担保分配金额"
              }
            ],
            "align": "left",
            "format": {
              "thousand": ","
            }
          },
          "base_busi_assign_amt": {
            "control": "string",
            "label": "业务担保分配金额(折基准币种)",
            "width": "130px",
            "span": 7,
            "rules": [
              {
                "required": false,
                "message": "请选择业务担保分配金额(折基准币种)"
              }
            ],
            "compute": "${{formData.busi_assign_amt}}*${{formData.exchange_rate}}",
            "align": "left",
            "format": {
              "thousand": ","
            }
          },
          "cl_allocation_mode": {
            "control": "select",
            "label": "分配方式",
            "width": "130px",
            "span": 7,
            "disabled": true,
            "dict": {
              "dictType": "AR_E_CLALLOCATION_MODE",
              "dictKey": [
                "AR_E_CLALLOCATION_MODE"
              ],
              "format": "label"
            }
          },
          "remark": {
            "label": "备注",
            "control": "textarea",
            "disable": true,
            "width": "130px",
            "span": 21,
            "autosize": {
              "minRows": 4
            },
            "rules": [
              {
                "max": "200",
                "message": "长度不能超过200个字符"
              }
            ]
          }
        }
      ]
    }
  }
}
