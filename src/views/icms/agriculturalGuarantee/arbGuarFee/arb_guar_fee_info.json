{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "ar1615",
          "apply_no": "${{params.apply_no}}"
        }
      },
      "btns": [
        {
          "label": "提交",
          "type": "submit",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "post",
            "params": {
              "servicecode": "ar1616",
              "apply_no": "${{params.apply_no}}"
            }
          },
          "eventAfter": {
            "doLastCallback": false
          }
        }
      ],
      "controls": [
        {
          "apply_no": {
            "control": "hidden",
            "label": "申请号"
          },
          "apply_amt": {
            "control": "string",
            "label": "申请金额",
            "disabled": true,
            "span": 8,
            "align": "left",
            "format": {
              "thousand": ","
            }
          },
          "total_guar_fee_rate": {
            "control": "currency",
            "label": "总担保费率",
            "span": 8,
            "disabled": false,
            "align": "left",
            "format": {
              "thousand": ","
            },
            "rules": [
              {
                "required": true,
                "message": "此项为必填项"
              }
            ]
          },
          "dert_gura_fee_rate": {
            "control": "currency",
            "label": "减免担保费率",
            "span": 8,
            "disabled": false,
            "align": "left",
            "format": {
              "thousand": ","
            },
            "rules": [
              {
                "required": true,
                "message": "此项为必填项"
              }
            ]
          },
          "ccy_code": {
            "control": "select",
            "label": "币种",
            "span": 8,
            "disabled": true,
            "rules": [
              {
                "required": false,
                "message": "此项为必填项"
              }
            ],
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": [
                "MS_CURRENCY"
              ],
              "format": "label"
            }
          },
          "total_gura_fee": {
            "control": "currency",
            "label": "总担保费",
            "span": 8,
            "disabled": true,
            "align": "left",
            "compute": "${{form.apply_amt}}*${{form.total_guar_fee_rate}}/100",
            "format": {
              "thousand": ","
            },
            "rules": [
              {
                "required": false,
                "message": "此项为必填项"
              }
            ]
          },
          "base_total_gura_fee": {
            "control": "currency",
            "label": "总担保费(折基准币种)",
            "compute": "${{form.total_gura_fee}}*${{form.exchange_rate}}",
            "span": 8,
            "disabled": true,
            "align": "left",
            "format": {
              "thousand": ","
            },
            "rules": [
              {
                "required": false,
                "message": "此项为必填项"
              }
            ]
          },
          "base_ccy": {
            "control": "select",
            "label": "基准币种",
            "span": 8,
            "disabled": true,
            "rules": [
              {
                "required": false,
                "message": "此项为必填项"
              }
            ],
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": [
                "MS_CURRENCY"
              ],
              "format": "label"
            }
          },
          "dert_gura_fee": {
            "control": "currency",
            "label": "减免担保费",
            "span": 8,
            "disabled": true,
            "compute": "${{form.apply_amt}}*${{form.dert_gura_fee_rate}}/100",
            "align": "left",
            "format": {
              "thousand": ","
            },
            "rules": [
              {
                "required": false,
                "message": "此项为必填项"
              }
            ]
          },
          "base_dert_gura_fee": {
            "control": "currency",
            "label": "减免担保费(折基准币种)",
            "compute": "${{form.dert_gura_fee}}*${{form.exchange_rate}}",
            "span": 8,
            "disabled": true,
            "align": "left",
            "format": {
              "thousand": ","
            },
            "rules": [
              {
                "required": false,
                "message": "此项为必填项"
              }
            ]
          },
          "exchange_rate": {
            "disabled": true,
            "control": "currency",
            "label": "汇率",
            "align": "left",
            "span": 8
          },
          "actual_gura_fee": {
            "control": "currency",
            "label": "实收担保费",
            "span": 8,
            "disabled": true,
            "compute": "${{form.total_gura_fee}}-${{form.dert_gura_fee}}",
            "align": "left",
            "format": {
              "thousand": ","
            },
            "rules": [
              {
                "required": false,
                "message": "此项为必填项"
              }
            ]
          },
          "base_actual_gura_fee": {
            "control": "currency",
            "label": "实收担保费(折基准币种)",
            "compute": "${{form.base_total_gura_fee}}-${{form.base_dert_gura_fee}}",
            "span": 8,
            "disabled": true,
            "align": "left",
            "format": {
              "thousand": ","
            },
            "rules": [
              {
                "required": false,
                "message": "此项为必填项"
              }
            ]
          },
          "payee_acct_no": {
            "control": "text",
            "label": "收款账号",
            "span": 8,
            "disabled": false,
            "rules": [
              {
                "required": true,
                "message": "此项为必填项"
              },
              {
                "max": 64,
                "message": "长度不能超过64个字符"
              }
            ]
          },
          "payee_bank": {
            "control": "text",
            "label": "收款账号开户行行号",
            "span": 8,
            "disabled": false,
            "rules": [
              {
                "required": true,
                "message": "此项为必填项"
              },
              {
                "max": 64,
                "message": "长度不能超过64个字符"
              }
            ]
          },
          "payee_bank_name": {
            "control": "text",
            "label": "收款账号开户行名称",
            "span": 8,
            "disabled": false,
            "rules": [
              {
                "required": true,
                "message": "此项为必填项"
              },
              {
                "max": 64,
                "message": "长度不能超过64个字符"
              }
            ]
          },
          "pay_acct": {
            "control": "text",
            "label": "付款账号",
            "span": 8,
            "disabled": false,
            "rules": [
              {
                "required": true,
                "message": "此项为必填项"
              },
              {
                "max": 64,
                "message": "长度不能超过64个字符"
              }
            ]
          },
          "pay_acct_bank_no": {
            "control": "text",
            "label": "付款账号开户行行号",
            "span": 8,
            "disabled": false,
            "rules": [
              {
                "required": true,
                "message": "此项为必填项"
              },
              {
                "max": 64,
                "message": "长度不能超过64个字符"
              }
            ]
          },
          "pay_acct_bank_name": {
            "control": "text",
            "label": "付款账号开户行名称",
            "span": 8,
            "disabled": false,
            "rules": [
              {
                "required": true,
                "message": "此项为必填项"
              },
              {
                "max": 64,
                "message": "长度不能超过64个字符"
              }
            ]
          }

        }
      ]
    }
  }
}
