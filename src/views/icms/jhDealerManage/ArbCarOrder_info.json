{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "ar2001",
          "apply_no": "${{params.apply_no}}"
        }
      },
      "btns": [],
      "controls": [
        {
          "order_id": {
            "control": "string",
            "label": "订单号",
            "span": 8
          },
          "purch_total_volume": {
            "control": "string",
            "label": "采购总数(辆)",
            "span": 8
          },
          "purch_total_amount": {
            "control": "currency",
            "label": "采购总价(元)",
            "span": 8,
            "disabled": true,
            "format": {
              "thousand": ",",
              "decimal": "2"
            },
            "align": "left"
          },
          "repay_type": {
            "control": "select",
            "label": "还款方式",
            "span": 8,
            "disabled": true,
            "dict": {
              "dictType": "MS_E_REPAY_WAY",
              "dictKey": [
                "MS_E_REPAY_WAY"
              ],
              "format": "label"
            }
          },
          "start_date": {
            "control": "string",
            "label": "融资开始日期",
            "span": 8
          },
          "term": {
            "control": "string",
            "label": "融资期限(月)",
            "span": 8
          },
          "matu_date": {
            "control": "string",
            "label": "到期日期",
            "span": 8
          }
        }
      ]
    }
  }
}
