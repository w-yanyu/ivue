{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ar2006",
          "apply_no": "${{params.apply_no}}"
        }
      },
      "thead": [
        {
          "type": "string",
          "label": "供应商名称",
          "prop": "supplier_name",
          "align": "center"
        },
        {
          "type": "string",
          "label": "品牌编号",
          "prop": "brand_no",
          "align": "center"
        },
        {
          "type": "string",
          "label": "品牌名称",
          "prop": "brand_name",
          "align": "center"
        },
        {
          "type": "string",
          "label": "车系编号",
          "prop": "car_kind_no",
          "align": "center"
        },
        {
          "type": "string",
          "label": "车系名称",
          "prop": "car_kind_name",
          "align": "center"
        },
        {
          "type": "string",
          "label": "车型编号",
          "prop": "car_model_no",
          "align": "center"
        },
        {
          "type": "string",
          "label": "车型名称",
          "prop": "car_model_name",
          "align": "center"
        },
        {
          "type": "string",
          "label": "厂商指导价(万元)",
          "prop": "price",
          "align": "center",
          "format": {
            "thousand": ",",
            "decimal": 2
          }
        },
        {
          "type": "string",
          "label": "车身结构",
          "prop": "sizetype",
          "align": "center"
        },
        {
          "type": "string",
          "label": "排量",
          "prop": "displacementml",
          "align": "center"
        },
        {
          "type": "string",
          "label": "变速箱类型",
          "prop": "geartype",
          "align": "center"
        },
        {
          "type": "string",
          "label": "车身颜色",
          "prop": "color",
          "align": "center"
        },
        {
          "type": "string",
          "label": "采购数量(辆)",
          "prop": "purch_volume",
          "align": "center"
        },
        {
          "type": "string",
          "label": "采购单价(元)",
          "prop": "purch_amount",
          "align": "center",
          "format": {
            "thousand": ",",
            "decimal": 2
          }
        },
        {
          "type": "string",
          "label": "采购总价(元)",
          "prop": "purch_total_amount",
          "align": "center",
          "format": {
            "thousand": ",",
            "decimal": 2
          }
        },
        {
          "label": "操作",
          "align": "center",
          "width": "200px",
          "fixed": "right",
          "btns": [
            {
              "type": "look",
              "label": "详情",
              "toRequest": {
                "url": "/views/icms/jhDealerManage/ArbCarOrderDetail_info.json",
                "method": "GET"
              },
              "props": {
                "width": "70%",
                "title": "订单明细详情"
              }
            }
          ]
        }
      ]
    }
  }
}
