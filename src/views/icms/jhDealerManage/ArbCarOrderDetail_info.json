{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "btns": [],
      "controls": [
        {
          "supplier_name": {
            "control": "string",
            "label": "供应商名称",
            "span": 8
          },
          "brand_no": {
            "control": "string",
            "label": "品牌编号",
            "span": 8
          },
          "brand_name": {
            "control": "string",
            "label": "品牌名称",
            "span": 8
          },
          "car_kind_no": {
            "control": "string",
            "label": "车系编号",
            "span": 8
          },
          "car_kind_name": {
            "control": "string",
            "label": "车系名称",
            "span": 8
          },
          "car_model_no": {
            "control": "string",
            "label": "车型编号",
            "span": 8
          },
          "car_model_name": {
            "control": "string",
            "label": "车型名称",
            "span": 8
          },
          "price": {
            "control": "string",
            "label": "厂商指导价(万元)",
            "span": 8,
            "format": {
              "thousand": ",",
              "decimal": 2
            }
          },
          "sizetype": {
            "control": "string",
            "label": "车身结构",
            "span": 8
          },
          "displacementml": {
            "control": "string",
            "label": "排量",
            "span": 8
          },
          "geartype": {
            "control": "string",
            "label": "变速箱类型",
            "span": 8
          },
          "color": {
            "control": "string",
            "label": "车身颜色",
            "span": 8
          },
          "purch_volume": {
            "control": "string",
            "label": "采购数量(辆)",
            "span": 8
          },
          "purch_amount": {
            "control": "string",
            "label": "采购单价(元)",
            "span": 8,
            "format": {
              "thousand": ",",
              "decimal": 2
            }
          },
          "purch_total_amount": {
            "control": "string",
            "label": "采购总价(元)",
            "span": 8,
            "format": {
              "thousand": ",",
              "decimal": 2
            }
          }
        }
      ]
    }
  }
}
