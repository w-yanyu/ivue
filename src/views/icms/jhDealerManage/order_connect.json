{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "btns": [],
      "controlsGroup": [
        {
          "title": "订单汇总信息",
          "tooltip": "订单汇总信息",
          "titleIcon": "el-icon-info",
          "expand": true,
          "accordion": false,
          "controls": [
            {
              "order": {
                "span": 24,
                "control": "dynamic",
                "toRequest": {
                  "url": "/views/icms/jhDealerManage/ArbCarOrder_info.json",
                  "params":{
                    "apply_no": "${{params.apply_no}}"
                  }
                }
              }
            }
          ]
        },
        {
          "title": "订单明细信息",
          "tooltip": "订单明细信息",
          "titleIcon": "el-icon-info",
          "expand": true,
          "accordion": false,
          "controls": [
            {
              "orderDetail": {
                "span": 24,
                "control": "dynamic",
                "toRequest": {
                  "url": "/views/icms/jhDealerManage/ArbCarOrderDetail_manage.json",
                  "params":{
                    "apply_no": "${{params.apply_no}}"
                  }
                }
              }
            }
          ]
        }
      ]
    }
  }
}
