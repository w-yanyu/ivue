{
  "module": "v-realonlyForm-btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ar1328",
          "apply_no": "${{params.apply_no}}"
        }
      },
      "controls": [
        {
          "apply_no": {
            "control": "string",
            "disabled": true,
            "label": "申请号",
            "width": "200px",
            "span": 10
          },
          "loan_no": {
            "control": "hidden",
            "label": "借据号",
            "disabled": true,
            "edit": true,
            "width": "200px",
            "multiple": false,
            "props": {
              "title": "借据信息选择"
            },
            "span": 10
          },
          "this_bank_or_not": {
            "control": "string",
            "label": "是否本行",
            "disabled": true,
            "width": "200px",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            },
            "span": 10
          },
          "credit_of_no": {
            "control": "string",
            "label": "信用证号",
            "width": "200px",
            "disabled": true,
            "multiple": false,
            "span": 10
          },
          "credit_of_amt": {
            "control": "string",
            "label": "信用证金额",
            "disabled": true,
            "width": "200px",
            "span": 10,
            "align": "left",
            "format":{
              "thousand": ","
            }  
          },
          "credit_of_base_amt": {
            "control": "string",
            "label": "信用证金额（折基准币种）",
            "disabled": true,
            "width": "200px",
            "span": 10,
            "align": "left",
            "format":{
              "thousand": ","
            }  
          },
          "ccy_code": {
            "control": "string",
            "label": "信用证币种",
            "disabled": true,
            "width": "200px",
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": "MS_CURRENCY",
              "format": "label"
            },
            "span": 10
          },
          "base_ccy": {
            "control": "string",
            "label": "基准币种",
            "disabled": true,
            "width": "200px",
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": "MS_CURRENCY",
              "format": "label"
            },
            "span": 10
          },
          "exchange_rate": {
            "control": "string",
            "label": "汇率",
            "disabled": true,
            "width": "200px",
            "span": 10
          },
          "credit_of_valid_amt": {
            "control": "string",
            "label": "信用证余额",
            "disabled": true,
            "width": "200px",
            "span": 10,
            "align": "left",
            "format":{
              "thousand": ","
            }  
          }
        }
      ]
    },
    "toolbar": {
    }
  }
}
