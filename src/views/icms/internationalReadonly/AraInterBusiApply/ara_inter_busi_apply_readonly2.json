{
  "module": "v-realonlyForm-btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ar1304",
          "apply_no": "${{params.apply_no}}"
        }
      },
      "controls": [
        {
          "contract_no": {
            "control": "string",
            "label": "合同号",
            "width": "200px",
            "span": 10
          },
          "apply_no": {
            "control": "string",
            "label": "申请号",
            "disabled": true,
            "width": "200px",
            "span": 10
          },
          "bill_exchange_type": {
            "control": "string",
            "label": "押汇类型",
            "dict": {
              "dictType": "AR_E_BILL_EXCHANGE_TYPE",
              "dictKey": "AR_E_BILL_EXCHANGE_TYPE",
              "format": "label"
            },
            "width": "200px",
            "span": 10
          },
          "foiting_type": {
            "control": "string",
            "label": "福费廷类型",
            "dict": {
              "dictType": "AR_E_FOITING_TYPE",
              "dictKey": "AR_E_FOITING_TYPE",
              "format": "label"
            },
            "width": "200px",
            "span": 10
          },
          "statistical_tag": {
            "control": "string",
            "label": "统计标签",
            "rules": [
              {
                "required": false,
                "message": "请选择统计标签"
              }
            ],
            "width": "200px",
            "span": 10
          },
          "apply_date": {
            "control": "string",
            "format": "yyyyMMdd",
            "label": "申请日期",
            "rules": [
              {
                "required": false,
                "message": "请选择申请日期"
              }
            ],
            "width": "200px",
            "span": 10
          },
          "start_date": {
            "control": "string",
            "label": "起始日期",
            "rules": [
              {
                "required": false,
                "message": "请选择起始日期"
              }
            ],
            "width": "200px",
            "span": 10
          },
          "matu_date": {
            "control": "string",
            "label": "到期日期",
            "rules": [
              {
                "required": false,
                "message": "请选择到期日期"
              }
            ],
            "width": "200px",
            "span": 10
          },
          "sale_prod_cd": {
            "control": "string",
            "label": "可售产品编号",
            "disabled": true,
            "width": "200px",
            "span": 10
          },
          "ccy_code": {
            "control": "string",
            "label": "申请币种",
            "rules": [
              {
                "required": false,
                "message": "请选择申请币种"
              }
            ],
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": "MS_CURRENCY",
              "format": "label"
            },
            "width": "200px",
            "span": 10
          },
          "exchange_rate": {
            "control": "string",
            "label": "汇率",
            "rules": [
              {
                "required": false,
                "message": "请选择汇率"
              }
            ],
            "width": "200px",
            "span": 10
          },
          "base_ccy": {
            "control": "string",
            "label": "基准币种",
            "rules": [
              {
                "required": false,
                "message": "请选择基准币种"
              }
            ],
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": "MS_CURRENCY",
              "format": "label"
            },
            "width": "200px",
            "span": 10
          },
          "apply_amt": {
            "control": "string",
            "label": "申请金额",
            "rules": [
              {
                "required": false,
                "message": "请选择申请金额"
              }
            ],
            "width": "200px",
            "span": 10,
            "align": "left",
            "format":{
              "thousand": ","
            }  
          },
          "appr_amt": {
            "control": "hidden",
            "label": "审批金额",
            "rules": [
              {
                "required": false,
                "message": "请选择审批金额"
              }
            ],
            "width": "200px",
            "span": 10,
            "align": "left",
            "format":{
              "thousand": ","
            }  
          },
          "apply_base_amt": {
            "control": "string",
            "label": "申请基准金额",
            "rules": [
              {
                "required": false,
                "message": "请选择申请基准金额"
              }
            ],
            "width": "200px",
            "span": 10,
            "align": "left",
            "format":{
              "thousand": ","
            }  
          },
          "apply_term": {
            "control": "string",
            "label": "申请期限(月)",
            "rules": [
              {
                "required": false,
                "message": "请选择申请期限"
              }
            ],
            "width": "200px",
            "span": 10
          },
          "repay_type": {
            "control": "string",
            "label": "还款方式",
            "rules": [
              {
                "required": false,
                "message": "请选择还款方式"
              }
            ],
            "dict": {
              "dictType": "AR_E_REPAY_WAY",
              "dictKey": "AR_E_REPAY_WAY",
              "format": "label"
            },
            "width": "200px",
            "span": 10
          },
          "interest_rate": {
            "control": "string",
            "label": "利率",
            "rules": [
              {
                "required": false,
                "message": "请选择利率"
              }
            ],
            "width": "200px",
            "span": 10
          },
          "busi_tran_bank_no": {
            "control": "string",
            "label": "业务办理网点",
            "rules": [
              {
                "required": false,
                "message": "请选择业务办理网点"
              }
            ],
            "width": "200px",
            "span": 10
          },
          "grace_period_days": {
            "control": "string",
            "label": "宽限期(天)",
            "rules": [
              {
                "required": false,
                "message": "请选择宽限期"
              }
            ],
            "width": "200px",
            "span": 10
          },
          "signed_or_not": {
            "control": "string",
            "label": "是否签订贸易融资主协议",
            "rules": [
              {
                "required": false,
                "message": "请选择是否签订贸易融资主协议"
              }
            ],
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": "MS_E_YESORNO",
              "format": "label"
            },
            "events": {
              "condition": {
                "0": {
                  "control": {
                    "agreement_no": {
                      "value": "",
                      "disabled": true,
                      "rules": [
                        {
                          "required": false
                        }
                      ]
                    }
                  }
                },
                "1": {
                  "control": {
                    "agreement_no": {
                      "value": "",
                      "disabled": false,
                      "rules": [
                        {
                          "required": false,
                          "message": "请输入贸易融资主协议编号"
                        }
                      ]
                    }
                  }
                }
              }
            },
            "width": "200px",
            "span": 10
          },
          "agreement_no": {
            "control": "string",
            "label": "贸易融资主协议编号",
            "rules": [
              {
                "required": false,
                "message": "请选择贸易融资主协议编号"
              }
            ],
            "width": "200px",
            "span": 10
          },
          "other_covenants": {
            "control": "string",
            "label": "其他约定事项",
            "width": "200px",
            "span": 10
          },
          "specific_purpose": {
            "control": "string",
            "label": "具体用途",
            "width": "200px",
            "span": 10
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "submit",
          "label": "保存",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "ar1302",
              "apply_no": "${{params.apply_no}}",
              "contract_no": "${{current.contract_no}}"
            }
          }
        }
      ]
    }
  }
}
