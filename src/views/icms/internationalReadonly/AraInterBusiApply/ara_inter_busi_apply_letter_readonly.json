{
  "module": "v-realonlyForm-btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ar1304",
          "apply_no": "${{params.apply_no}}"
        }
      },
      "controls": [
        {
          "contract_no": {
            "control": "string",
            "label": "合同号",
            "disabled": true,
            "width": "200px",
            "span": 7
          },
          "apply_no": {
            "control": "string",
            "label": "申请号",
            "disabled": true,
            "width": "200px",
            "span": 7
          },
          "apply_date": {
            "control": "string",
            "format": "yyyyMMdd",
            "label": "申请日期",
            "width": "200px",
            "span": 7
          },
          "start_date": {
            "control": "string",
            "label": "起始日期",
            "width": "200px",
            "span": 7
          },
          "matu_date": {
            "control": "string",
            "label": "到期日期",
            "width": "200px",
            "span": 7
          },
          "sale_prod_cd": {
            "control": "string",
            "label": "可售产品编号",
            "disabled": true,
            "width": "200px",
            "span": 7
          },
          "ccy_code": {
            "control": "string",
            "label": "申请币种",
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": "MS_CURRENCY",
              "format": "label"
            },
            "width": "200px",
            "span": 7
          },
          "exchange_rate": {
            "control": "string",
            "label": "汇率",
            "width": "200px",
            "span": 7
          },
          "base_ccy": {
            "control": "string",
            "label": "基准币种",
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": "MS_CURRENCY",
              "format": "label"
            },
            "width": "200px",
            "span": 7
          },
          "apply_amt": {
            "control": "string",
            "label": "申请金额",
            "width": "200px",
            "span": 7,
            "align": "left",
            "format": {
              "thousand": ","
            }
          },
          "appr_amt": {
            "control": "string",
            "label": "审批金额",
            "width": "200px",
            "span": 7,
            "align": "left",
            "format": {
              "thousand": ","
            }
          },
          "apply_base_amt": {
            "control": "string",
            "label": "申请基准金额",
            "width": "200px",
            "span": 7,
            "align": "left",
            "format": {
              "thousand": ","
            }
          },
          "branch_id": {
            "control": "string",
            "disabled": true,
            "label": "机构号",
            "width": "200px",
            "span": 7
          },
          "credit_of_type": {
            "control": "string",
            "label": "信用证类型",
            "dict": {
              "dictType": "AR_E_CREDIT_OF_TYPE",
              "dictKey": "AR_E_CREDIT_OF_TYPE",
              "format": "label"
            },
            "width": "200px",
            "span": 7
          },
          "procedure_rates": {
            "control": "string",
            "label": "手续费率(‰)",
            "width": "200px",
            "align": "left",
            "span": 7
          },
          "procedure_amt": {
            "control": "currency",
            "label": "手续费金额",
            "width": "200px",
            "span": 7,
            "align": "left",
            "format": {
              "thousand": ","
            }
          },
         
          "more_or_less_type": {
            "control": "string",
            "label": "溢短装类型",
            "dict": {
              "dictType": "AR_E_MORE_OR_LESS_TYPE",
              "dictKey": "AR_E_MORE_OR_LESS_TYPE",
              "format": "label"
            },
            "width": "200px",
            "span": 7
          },
          "more_or_less_ratio": {
            "control": "string",
            "label": "溢短装比例",
            "width": "200px",
            "span": 7
          },
          "payment_term": {
            "control": "string",
            "label": "付款期限(天）",
            "width": "200px",
            "span": 7
          },
          "capital_src": {
            "control": "string",
            "label": "资金来源",
            "dict": {
              "dictType": "AR_E_PRCPL_SRC",
              "dictKey": "AR_E_PRCPL_SRC",
              "format": "label"
            },
            "width": "200px",
            "span": 7
          },
          "pay_way": {
            "control": "string",
            "label": "付款方式",
            "dict": {
              "dictType": "AR_E_PAYMENT_METHOD",
              "dictKey": "AR_E_PAYMENT_METHOD",
              "format": "label"
            },
            "width": "200px",
            "span": 7
          },
          "statistical_tag": {
            "control": "string",
            "label": "统计标签",
            "width": "200px",
            "span": 7
          },
          "busi_tran_bank_no": {
            "control": "string",
            "label": "业务办理网点",
            "width": "200px",
            "span": 7
          },
          "specific_purpose": {
            "control": "string",
            "label": "具体用途",
            "width": "200px",
            "span": 21
          },
          "other_covenants": {
            "control": "string",
            "label": "其他约定事项",
            "disable": true,
            "width": "200px",
            "span": 21
          },
          "remark": {
            "control": "hidden",
            "label": "备注",
            "width": "200px",
            "span": 21
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "submit",
          "label": "保存",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "ar1302",
              "apply_no": "${{params.apply_no}}",
              "contract_no": "${{params.contract_no}}"
            }
          }
        }
      ]
    }
  }
}