{
  "module": "v-form-btn-dynamic",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ar1304",
          "apply_no": "${{params.apply_no}}"
        }
      },
      "controls": [
        {
          "contract_no": {
            "control": "string",
            "label": "合同号",
            "width": "200px",
            "disabled": true,
            "span": 10
          },
          "apply_no": {
            "control": "text",
            "label": "申请号",
            "disabled": true,
            "width": "200px",
            "span": 10
          },
          "bill_exchange_type": {
            "control": "hidden",
            "label": "押汇类型",
            "dict": {
              "dictType": "AR_E_BILL_EXCHANGE_TYPE",
              "dictKey": "AR_E_BILL_EXCHANGE_TYPE",
              "format": "label"
            },
            "width": "200px",
            "disabled": true,
            "span": 10
          },
          "foiting_type": {
            "control": "hidden",
            "label": "福费廷类型",
            "dict": {
              "dictType": "AR_E_FOITING_TYPE",
              "dictKey": "AR_E_FOITING_TYPE",
              "format": "label"
            },
            "width": "200px",
            "disabled": true,
            "span": 10
          },
          "statistical_tag": {
            "control": "text",
            "label": "统计标签",
            "rules": [
              {
                "required": false,
                "message": "请选择统计标签"
              }
            ],
            "width": "200px",
            "disabled": true,
            "span": 10
          },
          "apply_date": {
            "control": "dateTimePicker",
            "format": "yyyyMMdd",
            "label": "申请日期",
            "rules": [
              {
                "required": false,
                "message": "请选择申请日期"
              }
            ],
            "width": "200px",
            "disabled": true,
            "span": 10
          },
          "start_date": {
            "control": "dateTimePicker",
            "label": "起始日期",
            "rules": [
              {
                "required": false,
                "message": "请选择起始日期"
              }
            ],
            "width": "200px",
            "disabled": true,
            "span": 10
          },
          "matu_date": {
            "control": "dateTimePicker",
            "label": "到期日期",
            "rules": [
              {
                "required": false,
                "message": "请选择到期日期"
              }
            ],
            "width": "200px",
            "disabled": true,
            "span": 10
          },
          "sale_prod_cd": {
            "control": "text",
            "label": "可售产品编号",
            "disabled": true,
            "width": "200px",
            "span": 10
          },
          "ccy_code": {
            "control": "select",
            "label": "申请币种",
            "rules": [
              {
                "required": false,
                "message": "请选择申请币种"
              }
            ],
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": "MS_CURRENCY",
              "format": "label"
            },
            "width": "200px",
            "disabled": true,
            "span": 10
          },
          "exchange_rate": {
            "control": "text",
            "label": "汇率",
            "rules": [
              {
                "required": false,
                "message": "请选择汇率"
              }
            ],
            "width": "200px",
            "disabled": true,
            "span": 10
          },
          "base_ccy": {
            "control": "select",
            "label": "基准币种",
            "rules": [
              {
                "required": false,
                "message": "请选择基准币种"
              }
            ],
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": "MS_CURRENCY",
              "format": "label"
            },
            "width": "200px",
            "disabled": true,
            "span": 10
          },
          "apply_amt": {
            "control": "currency",
            "label": "申请金额",
            "rules": [
              {
                "required": false,
                "message": "请选择申请金额"
              }
            ],
            "width": "200px",
            "disabled": true,
            "span": 10,
            "align": "left",
            "format":{
              "thousand": ","
            }  
          },
          "appr_amt": {
            "control": "hidden",
            "label": "审批金额",
            "rules": [
              {
                "required": false,
                "message": "请选择审批金额"
              }
            ],
            "width": "200px",
            "disabled": true,
            "span": 10,
            "format":{
              "thousand":","
            }
          },
          "apply_base_amt": {
            "control": "currency",
            "label": "申请基准金额",
            "rules": [
              {
                "required": false,
                "message": "请选择申请基准金额"
              }
            ],
            "width": "200px",
            "disabled": true,
            "span": 10,
            "align": "left",
            "format":{
              "thousand": ","
            }  
          },
          "apply_term": {
            "control": "text",
            "label": "申请期限(月)",
            "rules": [
              {
                "required": false,
                "message": "请选择申请期限"
              }
            ],
            "width": "200px",
            "disabled": true,
            "span": 10
          },
          "repay_type": {
            "control": "select",
            "label": "还款方式",
            "rules": [
              {
                "required": false,
                "message": "请选择还款方式"
              }
            ],
            "dict": {
              "dictType": "AR_E_REPAY_WAY",
              "dictKey": "AR_E_REPAY_WAY",
              "format": "label"
            },
            "width": "200px",
            "disabled": true,
            "span": 10
          },
          "interest_rate": {
            "control": "text",
            "label": "利率",
            "rules": [
              {
                "required": false,
                "message": "请选择利率"
              }
            ],
            "width": "200px",
            "disabled": true,
            "span": 10
          },
          "busi_tran_bank_no": {
            "control": "text",
            "label": "业务办理网点",
            "rules": [
              {
                "required": false,
                "message": "请选择业务办理网点"
              }
            ],
            "width": "200px",
            "disabled": true,
            "span": 10
          },
          "grace_period_days": {
            "control": "text",
            "label": "宽限期(天)",
            "rules": [
              {
                "required": false,
                "message": "请选择宽限期"
              }
            ],
            "width": "200px",
            "disabled": true,
            "span": 10
          },
          "signed_or_not": {
            "control": "select",
            "label": "是否签订贸易融资主协议",
            "rules": [
              {
                "required": false,
                "message": "请选择是否签订贸易融资主协议"
              }
            ],
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": "MS_E_YESORNO",
              "format": "label"
            },
            "width": "200px",
            "disabled": true,
            "span": 10
          },
          "agreement_no": {
            "control": "text",
            "label": "贸易融资主协议编号",
            "rules": [
              {
                "required": false,
                "message": "请选择贸易融资主协议编号"
              }
            ],
            "width": "200px",
            "disabled": true,
            "span": 10
          },
          "other_covenants": {
            "control": "textarea",
            "label": "其他约定事项",
            "width": "200px",
            "disabled": true,
            "span": 10
          },
          "specific_purpose": {
            "control": "textarea",
            "label": "具体用途",
            "width": "200px",
            "disabled": true,
            "span": 10
          }
        }
      ]
    },
    "toolbar": {
      "btns": []
    }
  }
}