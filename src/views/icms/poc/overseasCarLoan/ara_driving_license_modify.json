{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "post",
                "params": {
                    "servicecode": "ar0880",
                    "apply_no": "${{params.apply_no}}",
                    "parent_apply_id":"${{params.parent_apply_id}}"
                }
            },
            "btns": [
                {
                    "label": "提交",
                    "type": "submit",
                    "noClose": true,
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "post",
                        "params": {
                            "servicecode": "ar0881",
                            "apply_no": "${{params.apply_no}}"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "file": {
                        "doRequest": {
                            "url": "/SUMP/rest/recognize/uploadAndGet",
                            "method": "post",
                            "params": {
                                "appId": "icmsOnl",
                                "servicecode": "ct2559"
                            },
                            "resultSet": {
                                "birth_date": "birthday",
                                "cert_addr": "issuedBy",
                                "address": "address",
                                "gender": "gender",
                                "valid_from": "validFrom",
                                "version": "version",
                                "license_number": "licenseNumber",
                                "valid_date": "validDate",
                                "nationality": "nationality",
                                "issue_date": "issueDate",
                                "cust_name": "name",
                                "category": "category",
                                "valid_for": "validFor",
                                "file_number": "fileNumber"
                            }
                        },
                        "span": 8,
                        "control": "file",
                        "limit": 1,
                        "limitSize": 10,
                        "label": "上传驾驶证信息",
                        "btnLable": "请选择驾驶证图片"
                    },
                    "cust_name": {
                        "control": "text",
                        "label": "姓名",
                        "span": 8,
                        "disabled": false
                    },
                    "category": {
                        "control": "text",
                        "label": "准驾车型",
                        "span": 8,
                        "disabled": false
                    },
                    "birth_date": {
                        "control": "dateTimePicker",
                        "format": "yyyy-MM-dd",
                        "valueFormat": "yyyyMMdd",
                        "label": "出生日期",
                        "span": 8,
                        "disabled": false

                    },
                    "gender": {
                        "control": "select",
                        "label": "性别",
                        "disabled": false,

                        "span": "8",
                        "dict": {
                            "dictType": "MS_E_GENDER",
                            "dictKey": [
                                "MS_E_GENDER"
                            ],
                            "format": "label"
                        }
                    },
                    
                    "version": {
                        "control": "text",
                        "label": "驾驶证正本版本",
                        "disabled": false,
                        "span": 8
                    },

                    "license_number": {
                        "control": "text",
                        "label": "驾驶证号",
                        "span": 8,
                        "disabled": false
                    },
                    "file_number": {
                        "control": "text",
                        "label": "档案编号",
                        "span": 8,
                        "disabled": false
                    },
                    "valid_date": {
                        "control": "text",
                        "label": "有效期限",
                        "span": 8,
                        "disabled": false
                    },
                    "nationality": {
                        "control": "text",
                        "label": "国籍",
                        "span": 8,
                        "disabled": false
                    },
                    "issue_date": {
                        "disabled": false,
                        "control": "dateTimePicker",
                        "format": "yyyy-MM-dd",
                        "valueFormat": "yyyyMMdd",
                        "label": "初次领证日期",
                        "span": 8
                    },
                    "cert_addr": {
                        "label": "证件签发机构",
                        "disabled": false,
                        "control": "textarea",
                        "span": 24,
                        "autosize": {
                            "minRows": 3,
                            "maxRows": 5
                        },
                        "maxlength": 40,
                        "showWordLimit": true
                    },
                    "address": {
                        "label": "住址",
                        "disabled": false,
                        "control": "textarea",
                        "span": 24,
                        "autosize": {
                            "minRows": 3,
                            "maxRows": 5
                        },
                        "maxlength": 40,
                        "showWordLimit": true
                    }
                },
                {
                    "apply_no": {
                        "label": "申请号",
                        "control": "hidden",
                        "value": "${{params.apply_no}}"
                    }
                }
            ]
        }
    }
}