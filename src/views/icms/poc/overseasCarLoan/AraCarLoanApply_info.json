{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ar0869",
          "apply_no": "${{params.apply_no}}"
        }
      },
      "btns": [],
      "controls": [
        {
          "buy_car_city": {
            "control": "string",
            "label": "购车贷款办理城市",
            "span": 8,
            "width": "120px"
          },
          "produce_mfr": {
            "control": "string",
            "label": "经销商",
            "span": 8,
            "width": "120px"
          },
          "car_brand": {
            "control": "string",
            "label": "汽车品牌",
            "span": 8,
            "width": "120px"
          },
          "car_model": {
            "control": "string",
            "label": "汽车型号",
            "span": 8,
            "width": "120px"
          },
          "car_color": {
            "control": "string",
            "label": "汽车颜色",
            "span": 8,
            "width": "120px"
          },
          "import_flag": {
            "control": "select",
            "label": "是否进口",
            "span": 8,
            "width": "120px",
            "disabled": true,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "displacement": {
            "control": "string",
            "label": "排量",
            "span": 8,
            "width": "120px"
          },
          "main_purpose": {
            "control": "string",
            "label": "主要用途",
            "span": 8,
            "width": "120px",
            "dict": {
              "dictType": "AR_E_CAR_PURPOSE",
              "dictKey": [
                "AR_E_CAR_PURPOSE"
              ],
              "format": "label"
            }
          },
          "mfr_date": {
            "control": "string",
            "label": "生产日期",
            "span": 8,
            "width": "120px"
          },
          "buy_date": {
            "control": "string",
            "label": "购买日期",
            "span": 8,
            "width": "120px"
          },
          "purchase_amt": {
            "control": "string",
            "label": "购入价格",
            "span": 8,
            "width": "120px",
            "format": {
              "thousand": ",",
              "decimal": 2
            }
          },
          "is_used_veh": {
            "control": "select",
            "label": "是否二手车",
            "span": 8,
            "width": "120px",
            "disabled": true,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "down_payment_amt": {
            "control": "string",
            "label": "首付金额",
            "span": 8,
            "width": "120px",
            "format": {
              "thousand": ",",
              "decimal": 2
            }
          },
          "down_payment_ratio": {
            "control": "string",
            "label": "首付比例（%）",
            "span": 8,
            "width": "120px",
            "format": {
              "thousand": ",",
              "decimal": 2
            }
          }
        }
      ]
    }
  }
}
