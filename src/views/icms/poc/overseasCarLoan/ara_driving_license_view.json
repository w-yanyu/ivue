{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "post",
                "params": {
                    "servicecode": "ar0880",
                    "apply_no": "${{params.apply_no}}",
                    "parent_apply_id":"${{params.parent_apply_id}}"
                }
            },
            "btns": [
                {
                }
            ],
            "controls": [
                {
                    "cust_name": {
                        "control": "text",
                        "label": "姓名",
                        "span": 8,
                        "disabled": true
                    },
                    "category": {
                        "control": "text",
                        "label": "准驾车型",
                        "span": 8,
                        "disabled": true
                    },

                    "birth_date": {
                        "control": "dateTimePicker",
                        "format": "yyyy-MM-dd",
                        "valueFormat": "yyyyMMdd",
                        "label": "出生日期",
                        "span": 8,
                        "disabled": true

                    },
                    "gender": {
                        "control": "select",
                        "label": "性别",
                        "disabled": true,

                        "span": "8",
                        "dict": {
                            "dictType": "MS_E_GENDER",
                            "dictKey": [
                                "MS_E_GENDER"
                            ],
                            "format": "label"
                        }
                    },
                    "version": {
                        "control": "text",
                        "label": "驾驶证正本版本",
                        "disabled": true,
                        "span": 8
                    },

                    "license_number": {
                        "control": "text",
                        "label": "驾驶证号",
                        "span": 8,
                        "disabled": true
                    },
                    "file_number": {
                        "control": "text",
                        "label": "档案编号",
                        "span": 8,
                        "disabled": true
                    },
                    "valid_date": {
                        "control": "text",
                        "label": "有效期限",
                        "span": 8,
                        "disabled": true
                    },
                    "nationality": {
                        "control": "text",
                        "label": "国籍",
                        "span": 8,
                        "disabled": true
                    },
                    "issue_date": {
                        "disabled": true,
                        "control": "dateTimePicker",
                        "format": "yyyy-MM-dd",
                        "valueFormat": "yyyyMMdd",
                        "label": "初次领证日期",
                        "span": 8
                    },
                    "cert_addr": {
                        "label": "证件签发机构",
                        "disabled": true,
                        "control": "textarea",
                        "span": 24,
                        "autosize": {
                            "minRows": 3,
                            "maxRows": 5
                        },
                        "maxlength": 40,
                        "showWordLimit": true
                    },
                    "address": {
                        "label": "住址",
                        "disabled": true,
                        "control": "textarea",
                        "span": 24,
                        "autosize": {
                            "minRows": 3,
                            "maxRows": 5
                        },
                        "maxlength": 40,
                        "showWordLimit": true
                    }
                },
                {
                    "apply_no": {
                        "label": "申请号",
                        "control": "hidden",
                        "value": "${{params.apply_no}}"
                    }
                }
            ]
        }
    }
}