{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ar0871",
          "apply_no": "${{params.apply_no}}"
        }
      },
      "btns": [],
      "controlsGroup": [
        {
          "title": "投保参数",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "accordion": false,
          "expand": true,
          "controls": [
            {
              "is_bank_insured": {
                "control": "select",
                "label": "是否必须银行代投保",
                "span": 8,
                "disabled": true,
                "dict": {
                  "dictType": "MS_E_YESORNO",
                  "dictKey": [
                    "MS_E_YESORNO"
                  ],
                  "format": "label"
                }
              },
              "source_of_insured_funds": {
                "control": "select",
                "label": "投保资金来源",
                "span": 8,
                "disabled": true,
                "dict": {
                  "dictType": "MS_E_SOURCE_PAY_TYPE",
                  "dictKey": [
                    "MS_E_SOURCE_PAY_TYPE"
                  ],
                  "format": "label"
                }
              },
              "insurance_company": {
                "control": "string",
                "label": "保险公司",
                "span": 8
              },
              "insurance_calculation_model": {
                "control": "select",
                "label": "保险金计算模式",
                "span": 8,
                "disabled": true,
                "dict": {
                  "dictType": "AR_E_INSUR_CALCUL_MODEL",
                  "dictKey": [
                    "AR_E_INSUR_CALCUL_MODEL"
                  ],
                  "format": "label"
                },
                "showItem": {
                  "0": [],
                  "1": [
                    "insurance_rate"
                  ]
                }
              },
              "insurance_rate": {
                "control": "string",
                "label": "保险金比例",
                "initShow": false,
                "span": 8,
                "format": {
                  "thousand": ",",
                  "decimal": 2
                }
              },
              "insurance": {
                "control": "string",
                "label": "保险金(元)",
                "span": 8,
                "format": {
                  "thousand": ",",
                  "decimal": 2
                }
              },
              "ccy_code": {
                "label": "保险金币种",
                "control": "select",
                "span": 8,
                "disabled": true,
                "dict": {
                  "dictType": "MS_CURRENCY",
                  "dictKey": [
                    "MS_CURRENCY"
                  ]
                }
              }
            }
          ]
        },
        {
          "title": "缴税参数",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "expand": true,
          "controls": [
            {
              "is_bank_paid": {
                "control": "select",
                "label": "是否必须银行代缴",
                "span": 8,
                "disabled": true,
                "dict": {
                  "dictType": "MS_E_YESORNO",
                  "dictKey": [
                    "MS_E_YESORNO"
                  ],
                  "format": "label"
                }
              },
              "source_of_payment": {
                "control": "select",
                "label": "代缴款来源",
                "span": 8,
                "disabled": true,
                "dict": {
                  "dictType": "MS_E_SOURCE_PAY_TYPE",
                  "dictKey": [
                    "MS_E_SOURCE_PAY_TYPE"
                  ],
                  "format": "label"
                }
              },
              "tax_rate": {
                "control": "string",
                "label": "缴税税率（%）",
                "span": 8
              },
              "tax_amt": {
                "control": "string",
                "label": "缴税金额(元)",
                "span": 8,
                "format": {
                  "thousand": ",",
                  "decimal": 2
                }
              },
              "tax_ccy_code": {
                "label": "缴税币种",
                "control": "select",
                "span": 8,
                "disabled": true,
                "dict": {
                  "dictType": "MS_CURRENCY",
                  "dictKey": [
                    "MS_CURRENCY"
                  ]
                }
              }
            }
          ]
        }
      ]
    }
  }
}
