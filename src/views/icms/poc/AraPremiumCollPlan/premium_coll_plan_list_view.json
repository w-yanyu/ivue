{
  "module": "v_search_btn_datagrid",
  "layout": {
    "toolbar": {
      "btns": [
        {
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ar1662",
          "apply_no": "${{params.apply_no}}"
        }
      },
      "thead": [
        {
          "prop": "apply_no",
          "label": "申请号",
          "align": "center"
        },
        {
          "label": "保费收取计划编号",
          "prop": "premium_plan_no",
          "align": "center"
        },
        {
          "label": "保费收取计划名称",
          "prop": "premium_plan_name",
          "align": "center"
        },
        {
          "label": "保费收取方",
          "prop": "premium_receiver",
          "align": "center",
          "dict": {
            "dictType": "AR_E_PREMIUM_RECEIVER",
            "dictKey": [
              "AR_E_PREMIUM_RECEIVER"
            ],
            "format": "label"
          }
        },
        {
          "label": "保费收取方式",
          "prop": "premium_mode",
          "align": "center",
          "dict": {
            "dictType": "AR_E_PREMIUM_MODE",
            "dictKey": [
              "AR_E_PREMIUM_MODE"
            ],
            "format": "label"
          }
        },
        {
          "label": "操作",
          "width": "200px",
          "fixed": "right",
          "align": "center",
          "btns": [
            {
              "type": "look",
              "label": "详情",
              "toRequest": {
                "url": "/views/icms/poc/AraPremiumCollPlan/premium_coll_plan_detail.json",
                "method": "get"
              },
              "props": {
                "width": "65%",
                "title": "保费收取计划详情"
              }
            }
            
          ]
        }
      ]
    }
  }
}