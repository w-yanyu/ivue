{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "doRequest": {
        "url": "/SUMP/rest/importExcel/importExcelData",
        "method": "post",
        "params": {
          "servicecode": "${{params.servicecode}}",
          "appId":"${{params.appId}}"
        }
      },
      "btns":[
        {
          "type":"submit",
          "label":"提交"
        }
      ],
      "controls": [
        {
          "file": {
            "initShow": true,
            "width": "130px",
            "control": "file",
            "label": "导入文件数据",
            "type": "*.xls",
            "btnLable": "请选择excel",
            "multiple": false,
            "rules": [
              {
                "required": true,
                "message": "请上传excel文件"
              }
            ]
          }
        }
      ]
    }
  }
}
