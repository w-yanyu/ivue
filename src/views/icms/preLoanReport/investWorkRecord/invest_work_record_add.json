{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "controls": [
        {
          "apply_no": {
            "label": "申请号",
            "control": "hidden"
          },
          "cust_no": {
            "label": "客户编号",
            "control": "hidden"
          },
          "investig_group_flag": {
            "label": "调查工作记录分组标识",
            "control": "hidden"
          },
          "investig_time": {
            "label": "调查时间",
            "width": "140px",
            "control": "dateTimePicker",
            "format": "yyyyMMdd",
            "rules": [
              {
                "required": true,
                "message": "请输入调查时间"
              }
            ]
          },
          "investig_address": {
            "label": "调查地点",
            "control": "text",
            "width": "140px",
            "rules": [
              {
                "required": true,
                "message": "请输入调查地点"
              }
            ]
          },
          "investig_channel": {
            "label": "调查渠道",
            "control": "text",
            "width": "140px",
            "rules": [
              {
                "required": true,
                "message": "请输入调查渠道"
              }
            ]
          },
          "investig_desc": {
            "label": "调查内容",
            "control": "text",
            "width": "140px",
            "rules": [
              {
                "required": true,
                "message": "请输入调查内容"
              }
            ]
          },
          "remark": {
            "label": "备注信息",
            "control": "text",
            "width": "140px"
          }
        }
      ],
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "ar0902"
            }
          }
        }
      ]
    }
  }
}