{
  "module": "lr_tree_dynamic",
  "layout": {
    "refJs": "/refJs/treeMenu.js",
    "tree": {
      "scope": "treeTemplateParams",
      "title": "贷前调查报告录入",
      "hideDynamicArea": false,
      "isExpandOnClickNode": true,
      "isAccordion": true,
      "idField": "deptId",
      "pIdField": "parentPageCode",
      "labelField": "title",
      "isLeafField": "isLeaf",
      "stopClickNodeField": "stopNodeClick",
      "nodeTypeField": "type",
      "disabledField": "disabled",
      "expandId": [
        "-1"
      ],
      "initRequest": {
        "url": "/views/icms/preLoanReport/report_menu.json",
        "method": "POST"
      },
      "nodeTypes": {
        "A1001": {
          "icon": "el-icon-news",
          "toRequest": {
            "url": "/views/icms/preLoanReport/investWorkRecord/invest_work_record_manage.json",
            "method": "POST",
            "params": {
              "apply_no": "${{params.apply_no}}",
              "cust_no": "${{params.cust_no}}",
              "investig_group_flag": "0"
            }
          }
        },
        "A1002": {
          "icon": "el-icon-news",
          "toRequest": {
            "url": "/views/icms/preLoanReport/investWorkRecord/invest_work_record_manage.json",
            "method": "POST",
            "params": {
              "apply_no": "${{params.apply_no}}",
              "cust_no": "${{params.cust_no}}",
              "investig_group_flag": "1"
            }
          }
        },
        "A2001": {
          "icon": "el-icon-news",
          "toRequest": {
            "url": "/views/icms/preLoanReport/creditBaseState/creditSchemeSummary/credit_scheme_summary_manage.json",
            "method": "POST",
            "params": {
              "apply_no": "${{params.apply_no}}",
              "cust_no": "${{params.cust_no}}"
            }
          }
        },
        "A2002": {
          "icon": "el-icon-news",
          "toRequest": {
            "url": "/views/icms/preLoanReport/creditBaseState/credit_base_state_info.json",
            "method": "POST",
            "params": {
              "apply_no": "${{params.apply_no}}",
              "cust_no": "${{params.cust_no}}"
            }
          }
        },
        "A3000": {
          "icon": "el-icon-news",
          "toRequest": {
            "url": "/views/icms/preLoanReport/industryAnalysis/industry_analysis_info.json",
            "method": "POST",
            "params": {
              "apply_no": "${{params.apply_no}}",
              "cust_no": "${{params.cust_no}}"
            }
          }
        },
        "A4000": {
          "icon": "el-icon-news",
          "toRequest": {
            "url": "/views/icms/preLoanReport/otherSpecialMatter/other_special_matter_info.json",
            "method": "POST",
            "params": {
              "apply_no": "${{params.apply_no}}"
            }
          }
        },
        "A5001": {
          "icon": "el-icon-news",
          "toRequest": {
            "url": "/views/icms/preLoanReport/mainRiskHedgeMeasure/main_risk_hedge_measure_manage.json",
            "method": "POST",
            "params": {
              "apply_no": "${{params.apply_no}}",
              "cust_no": "${{params.cust_no}}"
            }
          }
        },
        "A6000": {
          "icon": "el-icon-news",
          "toRequest": {
            "url": "/views/icms/preLoanReport/creditAmtAnalysis/credit_amt_analysis_info.json",
            "method": "POST",
            "params": {
              "apply_no": "${{params.apply_no}}",
              "cust_no": "${{params.cust_no}}"
            }
          }
        },
        "A7000": {
          "icon": "el-icon-news",
          "toRequest": {
            "url": "/views/icms/preLoanReport/creditPricRepayAnalysis/credit_pric_repay_analysis_info.json",
            "method": "POST",
            "params": {
              "apply_no": "${{params.apply_no}}",
              "cust_no": "${{params.cust_no}}"
            }
          }
        },
        "A8000": {
          "icon": "el-icon-news",
          "toRequest": {
            "url": "/views/icms/preLoanReport/investVerdictOpinion/invest_verdict_opinion_info.json",
            "method": "POST",
            "params": {
              "apply_no": "${{params.apply_no}}",
              "cust_no": "${{params.cust_no}}"
            }
          }
        }
      }
    }
  }
}
