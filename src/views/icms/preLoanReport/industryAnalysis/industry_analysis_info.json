{
    "module": "v-inputDataForm-btn",
    "layout": {
        "form": {
            "name": "detail",
            "btns": [
                {
                    "label": "保存",
                    "type": "submit",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "ar0952",
                            "apply_no": "${{params.apply_no}}",
                            "cust_no": "${{params.cust_no}}"
                        }
                    },
                    "eventAfter": {
                        "doLastCallback": false
                    },
                    "show": {
                      "expression": "'${{params.pageType}}' != 'detail'"
                    }
                }
            ],
            "initRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "ar0951",
                    "apply_no": "${{params.apply_no}}"
                }
            },
            "controls": [
                {
                    "prim_key": {
                        "label": "主键",
                        "control": "hidden",
                        "width": "140px"
                    },
                    "is_busi_impact": {
                        "label": "是否受行业因素影响",
                        "control": "select",
                        "width": "140px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择是否受行业因素影响"
                            }
                        ],
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ]
                        }
                    },
                    "busi_impact_desc": {
                        "label": "受行业因素影响说明",
                        "control": "text",
                        "width": "140px"
                    },
                    "is_into_busi": {
                        "label": "是否列入节能减排和两高一剩行业",
                        "control": "select",
                        "width": "140px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择是否列入节能减排和两高一剩行业"
                            }
                        ],
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ]
                        }
                    },
                    "into_busi_desc": {
                        "label": "列入节能减排和两高一剩行业说明",
                        "control": "text",
                        "width": "140px"
                    },
                    "is_discount": {
                        "label": "是否享受政府财政、税收、供地等优惠",
                        "control": "select",
                        "width": "140px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择是否享受政府财政、税收、供地等优惠"
                            }
                        ],
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ]
                        }
                    },
                    "discount_desc": {
                        "label": "享受政府财政、税收、供地等优惠说明",
                        "control": "text",
                        "width": "140px"
                    },
                    "is_envirt_bounded": {
                        "label": "环保是否受限",
                        "control": "select",
                        "width": "140px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择环保是否受限"
                            }
                        ],
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ]
                        }
                    },
                    "envir_bounded_desc": {
                        "label": "环保是否受限说明",
                        "control": "text",
                        "width": "140px"
                    },
                    "is_rate_impact": {
                        "label": "是否进出口业务受汇率、贸易壁垒影响",
                        "control": "select",
                        "width": "140px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择是否进出口业务受汇率、贸易壁垒影响"
                            }
                        ],
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ]
                        }
                    },
                    "rate_impact_desc": {
                        "label": "进出口业务受汇率、贸易壁垒影响说明",
                        "control": "text",
                        "width": "140px"
                    },
                    "is_other_case": {
                        "label": "有无其它需要说明的情况",
                        "control": "select",
                        "width": "140px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择有无其它需要说明的情况"
                            }
                        ],
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ]
                        }
                    },
                    "other_case_desc": {
                        "label": "有无其它需要说明的情况说明",
                        "control": "text",
                        "width": "140px"
                    }
                }
            ]
        }
    }
}