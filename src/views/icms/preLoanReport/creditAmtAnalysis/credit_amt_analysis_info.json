{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "ar0931",
                    "apply_no": "${{params.apply_no}}"
                }
            },
            "name": "detail",
            "controls": [
                {
                    "prim_key": {
                        "label": "主键",
                        "control": "hidden",
                        "width": "140px"
                    },
                    "is_busi_scope": {
                        "label": "是否在申请人经营范围之内",
                        "control": "select",
                        "width": "140px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择是否在申请人经营范围之内"
                            }
                        ],
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ]
                        }
                    },
                    "is_legal": {
                        "label": "是否符合国家法律、产业政策和我行信贷政策",
                        "control": "select",
                        "width": "140px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择是否符合国家法律、产业政策和我行信贷政策"
                            }
                        ],
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ]
                        }
                    },
                    "is_reas": {
                        "label": "分析用途是否合理",
                        "control": "select",
                        "width": "140px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择分析用途是否合理"
                            }
                        ],
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ]
                        }
                    },
                    "service_amt": {
                        "label": "营运资金量",
                        "control": "text",
                        "width": "140px",
                        "format":{
                            "thousand":","
                          }
                    },
                    "firm_self_amt": {
                        "label": "企业自有资金",
                        "control": "text",
                        "width": "140px",
                        "format":{
                            "thousand":","
                          }
                    },
                    "current_amt": {
                        "label": "现有流动资金贷款",
                        "control": "text",
                        "width": "140px",
                        "format":{
                            "thousand":","
                          }
                    },
                    "other_amt": {
                        "label": "其他渠道提供营运资金",
                        "control": "text",
                        "width": "140px",
                        "format":{
                            "thousand":","
                          }
                    },
                    "cal_add_limit": {
                        "label": "测算的流动资金贷款新增需求量",
                        "control": "text",
                        "width": "140px",
                        "format":{
                            "thousand":","
                          }
                    },
                    "add_limit": {
                        "label": "追加的流动资金贷款额度",
                        "control": "text",
                        "width": "140px",
                        "format":{
                            "thousand":","
                          }
                    },
                    "limit_total": {
                        "label": "我行可提供的流动资金贷款额度合计",
                        "control": "text",
                        "width": "140px",
                        "format":{
                            "thousand":","
                          }
                    },
                    "apply_credit_desc": {
                        "label": "申请授信用途描述",
                        "width": "140px",
                        "placeholder": "(200个字以内,若没有请填“无”)",
                        "control": "textarea",
                        "span": 24,
                        "autosize": {
                            "minRows": 3,
                            "maxRows": 5
                        },
                        "maxlength": 200,
                        "showWordLimit": true,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入描述内容"
                            }
                        ]
                    },
                    "trade_background": {
                        "label": "本次授信用途的贸易背景",
                        "width": "140px",
                        "placeholder": "(200个字以内,若没有请填“无”)",
                        "control": "textarea",
                        "span": 24,
                        "autosize": {
                            "minRows": 3,
                            "maxRows": 5
                        },
                        "maxlength": 200,
                        "showWordLimit": true,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入贸易背景内容"
                            }
                        ]
                    },
                    "limit_analyse": {
                        "label": "第6条追加额度原因分析",
                        "width": "140px",
                        "placeholder": "(200个字以内,若没有请填“无”)",
                        "control": "textarea",
                        "span": 24,
                        "autosize": {
                            "minRows": 3,
                            "maxRows": 5
                        },
                        "maxlength": 200,
                        "showWordLimit": true
                    }
                }
            ],
            "btns": [
                {
                    "type": "submit",
                    "label": "保存",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "ar0932",
                            "apply_no": "${{params.apply_no}}",
                            "cust_no": "${{params.cust_no}}"
                        }
                    },
                    "show": {
                      "expression": "'${{params.pageType}}' != 'detail'"
                    }
                }
            ]
        }
    }
}