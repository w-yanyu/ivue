{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "scope": "formData",
            "initRequest": "${{params}}",
            "btns": [
                {
                    "type": "submit",
                    "label": "保存",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "ar0913"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "prim_key": {
                        "label": "主键",
                        "control": "hidden"
                    },
                    "apply_no": {
                        "label": "申请号",
                        "control": "hidden"
                    },
                    "cust_no": {
                        "label": "客户编号",
                        "control": "hidden"
                    },
                    "busi_type": {
                        "label": "具体业务品种",
                        "control": "select",
                        "width": "140px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择具体业务品种"
                            }
                        ],
                        "dict": {
                            "dictType": "AR_E_SPECIFIC_BUSI_TYPE",
                            "dictKey": [
                                "AR_E_SPECIFIC_BUSI_TYPE"
                            ]
                        }
                    },
                    "limit_amt": {
                        "label": "额度(元)",
                        "control": "currency",
                        "width": "140px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入额度"
                            }
                        ],
                        "format":{
                            "thousand":","
                          }
                    },
                    "credit_open_amt": {
                        "label": "授信敞口金额(元)",
                        "control": "currency",
                        "width": "140px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入授信敞口金额"
                            }
                        ],
                        "format":{
                            "thousand":","
                          }
                    },
                    "term": {
                        "label": "期限(月)",
                        "control": "text",
                        "width": "140px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入期限"
                            }
                        ]
                    },
                    "guar_type": {
                        "label": "担保方式",
                        "control": "select",
                        "width": "140px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择担保方式"
                            }
                        ],
                        "dict": {
                            "dictType": "AR_E_GUAR_TYPE",
                            "dictKey": [
                                "AR_E_GUAR_TYPE"
                            ]
                        }
                    },
                    "margin_perc": {
                        "label": "保证金比例",
                        "control": "text",
                        "width": "140px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入保证金比例"
                            }
                        ]
                    },
                    "interest_rate": {
                        "label": "利率",
                        "control": "text",
                        "width": "140px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入利率"
                            }
                        ]
                    }
                }
            ]
        }
    }
}