{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "role_id": {
            "control": "text",
            "label": "角色ID",
            "rules": [
              {
                "max": 64,
                "message": "长度不能超过64个字符"
              }
            ]
          },
          "role_name": {
            "control": "text",
            "label": "角色名称",
            "rules": [
              {
                "max": 100,
                "message": "长度不能超过100个字符"
              }
            ]
          }
        }
      ]
    },
    "toolbar": {
    
    },
    "datagrid": {
      "scope": "searchTable",
      "saveAllSelected": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "teller_id": "${{params.user_id}}",
          "servicecode": "ct2050"
        }
      },
      "thead": [
      
        {
          "label": "角色ID",
          "prop": "role_id"
        },
        {
          "label": "角色名称",
          "prop": "role_name"
        },
        {
          "label": "操作",
          "align": "center",
          "fixed": "right",
          "btns": [
            {
              "type": "bringback",
              "label": "选择"
            }
          ]
        }
      ]
    }
  }
}
