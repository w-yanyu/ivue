{
    "module": "dynamicTabs",
    "layout": {
      "tabs": {
        "tabHeader": [
          {
            "id": "tab1",
            "label": "大屏模版管理", 

            "closable": false,
            "disabled": false,
            "icon": "el-icon-date",
            "toRequest": {
              "url": "views/dynamicScreem/dynamicScreemManage/dynamic_screem_module_manage/dynamic_screem_module_manage.json",
              "method": "post",
              "params": {
                "apply_no": "${{params.apply_no}}"
              }
            }
          },
          {
            "id": "tab2",
            "label":  "大屏卡片管理",
            "icon": "el-icon-date",
            "closable": false,
            "disabled": false,
            "toRequest": {
              "url": "views/dynamicScreem/dynamicScreemManage/dynamic_screem_card_manage/dynamic_screem_card_manage.json",
              "method": "post",
              "params": {
                "apply_no": "${{params.apply_no}}"
              }
            }
          }
        ],
        "defaultActiveKey": "tab1",
        "position": "top",
        "styleClass": "card"
      }
    }
  }
  