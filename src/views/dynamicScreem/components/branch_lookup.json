{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "branch_id": {
            "control": "text",
            "label": "机构号",
            "rules": [
              {
                "max": 10,
                "message": "长度不能超过10个字符"
              }
            ]
          }
        },
        {
          "branch_name": {
            "control": "text",
            "label": "机构名称",
            "rules": [
              {
                "max": 100,
                "message": "长度不能超过100个字符"
              }
            ]
          }
        }
      ]
    },
    "toolbar": {
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/call/RPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ct2155",
          "page_id": "",
          "branch_function_class": "",
          "real_branch_ind": ""
        }
      },
      "thead": [
        {
          "label": "机构号",
          "prop": "branch_id",
          "align": "center"
        },
        {
          "label": "机构名称",
          "prop": "branch_name",
          "align": "center"
        },
        {
          "label": "实体机构标志",
          "prop": "real_branch_ind",
          "align": "center",
          "dict": {
            "dictType": "MS_E_YESORNO",
            "dictKey": [
              "MS_E_YESORNO"
            ],
            "format": "label"
          }
        },
        {
          "label": "操作",
          "align": "center",
          "fixed": "right",
          "btns": [
            {
              "type": "bringback",
              "label": "选择"
            }
          ]
        }
      ]
    }
  }
}
