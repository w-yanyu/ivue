{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/call/RPCCall",
        "method": "post",
        "params": {
          "servicecode": "pd1101",
          "due_dili_template_no": "${{params.node_cd}}",
          "due_dili_template_name": "${{params.node_name}}"
        }
      },
      "btns": [
        {
          "type": "submit",
          "label": "保存",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "post",
            "params": {
              "servicecode": "pd1102",
              "node_type": "${{params.node_type}}"
            }
          }
        },
        {
          "type": "reset",
          "label": "重置"
        },
        {
          "type": "look",
          "label": "模板预览",
          "icon": "",
          "toRequest": {
            "url": "@DueTemplateReportView",
            "method": "get",
            "params": {
              "group_num": "${{params.group_num}}",
              "node_cd": "${{params.node_cd}}"
            }
          },
          "props": {
            "width": "60%",
            "title": "模板预览"
          }
        }
      ],
      "controls": [
        {
          "due_dili_template_no": {
            "width": "25px",
            "span": "8",
            "control": "text",
            "disabled": true,
            "label": "模板编号"
          },
          "due_dili_template_name": {
            "width": "25px",
            "control": "text",
            "span": "8",
            "label": "模板名称",
            "rules": [
              {
                "required": true,
                "message": "必填项"
              }
            ]
          },
          "is_effective": {
            "width": "25px",
            "control": "select",
            "span": "8",
            "label": "是否生效",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "必填项"
              }
            ]
          }
        }
      ]
    }
  }
}
