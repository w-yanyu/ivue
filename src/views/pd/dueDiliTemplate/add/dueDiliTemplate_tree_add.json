{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "pd0901"
        }
      },
      "controls": [
        {
          "group_num": {
            "control": "hidden",
            "label": "分组号",
            "value": "0002",
            "span": 8
          },
          "node_cd": {
            "control": "text",
            "label": "节点编码",
            "span": 8
          },
          "node_name": {
            "control": "text",
            "label": "节点名称",
            "span": 8
          },
          "parent_node_cd": {
            "control": "hidden",
            "label": "父节点编码",
            "value": "${{params.node_cd}}",
            "span": 8
          },
          "node_type": {
            "control": "hidden",
            "label": "节点类型",
            "value": "${{params.node_type}}",
            "span": 8
          }
        }
      ]
    }
  }
}
