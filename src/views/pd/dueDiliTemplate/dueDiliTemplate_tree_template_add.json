{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "pd0353",
          "node_type": "2",
          "group_num": "${{params.group_num}}",
          "parent_node_cd": "${{params.node_cd}}",
          "is_leaf": "1"
        }
      },
      "controls": [
        {
          "node_cd": {
            "width": "25px",
            "span": "12",
            "control": "text",
            "label": "模板编号"
          },
          "node_name": {
            "width": "25px",
            "control": "text",
            "span": "12",
            "label": "模板名称",
            "rules": [
              {
                "required": true,
                "message": "必填项"
              }
            ]
          },
            "sort_no": {
              "width": "25px",
              "span": "12",
              "control": "text",
              "label": "序号"
            }

        }
      ]
    }
  }
}
