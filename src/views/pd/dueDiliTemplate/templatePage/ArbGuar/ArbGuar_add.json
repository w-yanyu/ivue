{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "ar1704",
          "due_item_no": "${{params.due_item_no}}",
          "apply_no": "${{params.apply_no}}",
          "cust_no": "${{params.cust_no}}"
        }
      },
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "ar1705",
          "due_item_no": "${{params.due_item_no}}",
          "apply_no": "${{params.apply_no}}",
          "cust_no": "${{params.cust_no}}"
        }
      },
      "controlsGroup": [
        {
          "title": "担保人情况",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "accordion": false,
          "expand": true,
          "controls": [
            {
              "apply_no": {
                "control": "hidden",
                "label": "申请号",
                "span": 8
              },
              "cust_no": {
                "control": "hidden",
                "label": "客户编号",
                "span": 8
              },
              "due_item_no": {
                "control": "hidden",
                "label": "调查项编号",
                "span": 8
              },
              "guarantor_name": {
                "control": "text",
                "label": "担保人姓名",
                "span": 8
              },
              "id_card": {
                "control": "text",
                "label": "身份证号码",
                "span": 8
              },
              "mobile_no": {
                "control": "text",
                "label": "手机号码",
                "span": 8
              },
              "job_corp": {
                "control": "text",
                "label": "工作单位",
                "span": 8
              },
              "job": {
                "control": "text",
                "label": "职务",
                "span": 8
              },
              "work_years": {
                "control": "text",
                "label": "工作年限",
                "align": "left",
                "span": 8
              },
              "ccy_code": {
                "control": "select",
                "label": "币种",
            "disabled": true,
                "span": 8,
                "value": "$g{{user_info.ccy_code}}",
                "dict": {
                  "dictType": "MS_CURRENCY",
                  "dictKey": [
                    "MS_CURRENCY"
                  ],
                  "format": "label"
                }
              },
              "year_income": {
                "control": "currency",
                "label": "年收入",
                "decimal": "2",
                "align": "left",
                "format": {
                  "thousand": ","
                },
                "span": 8
              },
              "home_address": {
                "control": "text",
                "label": "家庭住址",
                "span": 8
              },
              "major_asset": {
                "control": "text",
                "label": "主要资产",
                "span": 8
              }
            }
          ]
        },
        {
          "title": "抵押物情况",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "expand": true,
          "controls": [
            {
              "collateral_name": {
                "control": "text",
                "label": "抵押物名称",
                "span": 8
              },
              "area": {
                "control": "text",
                "label": "面积(㎡)",
                "align": "left",
                "span": 8
              },
              "owner": {
                "control": "text",
                "label": "所有权人",
                "span": 8
              },
              "eval_price": {
                "control": "currency",
                "label": "评估价",
                "decimal": "2",
                "align": "left",
                "format": {
                  "thousand": ","
                },
                "span": 8
              },
              "pled_price": {
                "control": "currency",
                "label": "抵押价",
                "align": "left",
                "format": {
                  "thousand": ","
                },
                "span": 8
              },
              "mortgage_rate": {
                "control": "currency",
                "label": "抵押率",
                "align": "left",
                "span": 8
              }
            }
          ]
        },
        {
          "title": "侧面了解",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "expand": true,
          "controls": [
            {
              "channel_name": {
                "control": "text",
                "label": "渠道名称",
                "span": 8
              },
              "phone_no": {
                "control": "text",
                "label": "固定电话",
                "span": 8
              },
              "size_condition": {
                "control": "text",
                "label": "侧面了解情况",
                "span": 8
              }
            }
          ]
        }
      ]
    }
  }
}
