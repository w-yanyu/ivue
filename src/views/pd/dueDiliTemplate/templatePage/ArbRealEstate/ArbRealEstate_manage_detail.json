{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ar1812",
          "apply_no": "${{params.apply_no}}",
          "cust_no": "${{params.cust_no}}",
          "due_item_no": "${{params.due_item_no}}"
        }
      },
      "thead": [
        {
          "type": "string",
          "label": "房产类别",
          "prop": "room_anal_no",
          "align": "center"
        },
        {
          "type": "string",
          "label": "产权证号",
          "prop": "prop_rigc_no",
          "align": "center"
        },
        {
          "type": "string",
          "label": "位置",
          "prop": "position",
          "align": "center"
        },
        {
          "type": "string",
          "label": "产权人",
          "prop": "prop_owner",
          "align": "center"
        },
        {
          "type": "string",
          "label": "共有人",
          "prop": "co_owner",
          "align": "center"
        },
        {
          "type": "string",
          "label": "面积(㎡)",
          "prop": "area",
          "align": "center"
        },
        {
          "type": "string",
          "label": "取得方式",
          "prop": "acqu_way",
          "align": "center"
        },
        {
          "type": "string",
          "label": "当前状况",
          "prop": "curr_situation",
          "align": "center"
        },
        {
          "type": "string",
          "label": "承租人",
          "prop": "lessee",
          "align": "center"
        },
        {
          "type": "string",
          "label": "币种",
            "disabled": true,
          "prop": "ccy_code",
          "dict": {
            "dictType": "MS_CURRENCY",
            "dictKey": [
              "MS_CURRENCY"
            ],
            "format": "label"
          },
          "align": "center"
        },
        {
          "type": "currency",
          "label": "年租金",
          "prop": "year_rent",
          "decimal": "2",
          "format": {
            "thousand": ","
          },
          "align": "center"
        },
        {
          "type": "currency",
          "label": "抵押贷款金额",
          "prop": "mort_loan_amt",
          "decimal": "2",
          "format": {
            "thousand": ","
          },
          "align": "center"
        },
        {
          "type": "string",
          "label": "贷款行",
          "prop": "loan_bank",
          "align": "center"
        },
        {
          "type": "string",
          "label": "备注",
          "prop": "remark",
          "align": "center"
        },
        {
          "label": "操作",
          "align": "center",
          "fixed":"right",
          "width": "200px",
          "btns": [
            {
              "type": "look",
              "label": "详情",
              "toRequest": {
                "url": "/views/pd/dueDiliTemplate/templatePage/ArbRealEstate/ArbRealEstate_info.json",
                "method": "GET",
                "params": {
                  "apply_no": "${{params.apply_no}}",
                  "cust_no": "${{params.cust_no}}",
                  "due_item_no": "${{params.due_item_no}}"
                }
              },
              "props": {
                "width": "60%",
                "title": "不动产信息详情"
              }
            }
          ]
        }
      ]
    }
  }
}
