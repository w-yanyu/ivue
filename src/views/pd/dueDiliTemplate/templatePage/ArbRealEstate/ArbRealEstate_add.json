{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "ar1813"
            }
          }
        }
      ],
      "controls": [
        {
          "apply_no": {
            "control": "hidden",
            "label": "申请号",
            "span": 8
          },
          "cust_no": {
            "control": "hidden",
            "label": "客户编号",
            "span": 8
          },
          "due_item_no": {
            "control": "hidden",
            "label": "调查项编号",
            "span": 8
          },
          "prop_rigc_no": {
            "label": "产权证号",
            "control": "text",
            "span": 8
          },
          "position": {
            "label": "位置",
            "control": "text",
            "span": 8
          },
          "prop_owner": {
            "label": "产权人",
            "control": "text",
            "span": 8
          },
          "co_owner": {
            "label": "共有人",
            "control": "text",
            "span": 8
          },
          "area": {
            "label": "面积(㎡)",
            "control": "text",
            "span": 8
          },
          "acqu_way": {
            "label": "取得方式",
            "control": "text",
            "span": 8
          },
          "curr_situation": {
            "label": "当前状况",
            "control": "text",
            "span": 8
          },
          "lessee": {
            "label": "承租人",
            "control": "text",
            "span": 8
          },
          "ccy_code": {
            "control":"select",
            "label": "币种",
            "disabled": true,
            "span": 8,
            "value": "$g{{user_info.ccy_code}}",
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": [
                "MS_CURRENCY"
              ],
              "format": "label"
            }
          },
          "year_rent": {
            "label": "年租金",
            "control": "currency",
            "span": 8,
            "decimal": "2",
            "align": "left",
            "format": {
              "thousand": ","
            }
          },
          "mort_loan_amt": {
            "label": "抵押贷款金额",
            "control": "currency",
            "span": 8,
            "decimal": "2",
            "align": "left",
            "format": {
              "thousand": ","
            }
          },
          "loan_bank": {
            "label": "贷款行",
            "control": "text",
            "span": 8
          },
          "remark": {
            "label": "备注",
            "control": "text",
            "span": 8
          }

        }
      ]
    }
  }
}
