{
    "module": "v-inputDataForm-btn",
    "layout": {
        "scope": "templateScope",
        "form": {
            "btns": [],
            "controls": [
                {
                    "top": {
                        "control": "dynamic",
                        "span": 24,
                        "toRequest": {
                            "url": "/views/pd/dueDiliTemplate/templatePage/ArbProjectFundPlan/ArbLoanApply_info.json",
                            "method": "post",
                            "params": {
                                "due_item_no": "${{params.due_item_no}}",
                                "apply_no": "${{params.apply_no}}",
                                "cust_no": "${{params.cust_no}}"
                            }
                        }
                    },
                    "bottom": {
                        "control": "dynamic",
                        "span": 24,
                        "toRequest": {
                            "url": "/views/pd/dueDiliTemplate/templatePage/ArbProjectFundPlan/ArbProjectFundPlan_manage_detail.json",
                            "method": "post",
                            "params": {
                                "due_item_no": "${{params.due_item_no}}",
                                "apply_no": "${{params.apply_no}}",
                                "cust_no": "${{params.cust_no}}"
                            }
                        }
                    }
                }
            ]
        }
    }
}