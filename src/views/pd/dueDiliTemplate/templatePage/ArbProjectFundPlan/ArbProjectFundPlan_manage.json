{
  "module": "v_search_btn_datagrid",
  "layout": {
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "target": "addlog",
          "label": "新增",
          "toRequest": {
            "method": "GET",
            "url": "/views/pd/dueDiliTemplate/templatePage/ArbProjectFundPlan/ArbProjectFundPlan_add.json",
            "params": {
              "apply_no": "${{params.apply_no}}",
              "cust_no": "${{params.cust_no}}",
              "due_item_no": "${{params.due_item_no}}"
            }
          },
          "props": {
            "width": "80%",
            "title": "项目资金计划信息新增"
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ar1840",
          "apply_no": "${{params.apply_no}}",
          "cust_no": "${{params.cust_no}}",
          "due_item_no": "${{params.due_item_no}}"
        }
      },
      "thead": [
        {
          "type": "string",
          "label": "项目编号",
          "prop": "project_no",
          "align": "center"
        },
        {
          "type": "string",
          "label": "项目资金使用计划",
          "prop": "project_fund_plan",
          "align": "center"
        },
        {
          "type": "string",
          "label": "项目资金使用计划金额",
          "prop": "project_fund_plan_amt",
          "decimal": "2",
          "format": {
            "thousand": ","
          },
          "align": "center"
        },
        {
          "type": "string",
          "label": "资金来源计划",
          "prop": "fund_source_plan",
          "align": "center"
        },
        {
          "type": "string",
          "label": "资金来源计划金额",
          "prop": "fund_source_plan_amt",
          "decimal": "2",
          "format": {
            "thousand": ","
          },
          "align": "center"
        },
        {
          "label": "操作",
          "align": "center",
          "fixed":"right",
          "width": "200px",
          "btns": [
            {
              "type": "look",
              "label": "详情",
              "toRequest": {
                "url": "/views/pd/dueDiliTemplate/templatePage/ArbProjectFundPlan/ArbProjectFundPlan_info.json",
                "method": "GET",
                "params": {
                  "apply_no": "${{params.apply_no}}",
                  "cust_no": "${{params.cust_no}}",
                  "due_item_no": "${{params.due_item_no}}"
                }
              },
              "props": {
                "width": "80%",
                "title": "项目资金计划信息详情"
              }
            },
            {
              "type": "look",
              "label": "维护",
              "toRequest": {
                "url": "/views/pd/dueDiliTemplate/templatePage/ArbProjectFundPlan/ArbProjectFundPlan_update.json",
                "method": "GET",
                "params": {
                  "apply_no": "${{params.apply_no}}",
                  "cust_no": "${{params.cust_no}}",
                  "due_item_no": "${{params.due_item_no}}"
                }
              },
              "props": {
                "width": "80%",
                "title": "项目资金计划信息维护"
              }
            },
            {
              "type": "custom",
              "label": "删除",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "ar1843"
                }
              },
              "confirm": {
                "title": "删除",
                "msg": "确定删除吗？",
                "confirmButtonText": "确定",
                "cancelButtonText": "取消"
              }
            }
          ]
        }
      ]
    }
  }
}
