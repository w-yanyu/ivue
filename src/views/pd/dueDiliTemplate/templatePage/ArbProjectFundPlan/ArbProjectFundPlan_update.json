{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "ar1842",
              "apply_no": "${{params.apply_no}}",
              "cust_no": "${{params.cust_no}}",
              "due_item_no": "${{params.due_item_no}}"
            }
          }
        }
      ],
      "controls": [
        {
          "apply_no": {
            "control": "hidden",
            "label": "申请号",
            "width": "200px",
            "span": 8
          },
          "cust_no": {
            "control": "hidden",
            "label": "客户编号",
            "width": "200px",
            "span": 8
          },
          "due_item_no": {
            "control": "hidden",
            "label": "调查项编号",
            "width": "200px",
            "span": 8
          },
          "project_no": {
            "control": "string",
            "label": "项目编号",
            "width": "200px",
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ],
            "span": 8
          },
          "project_fund_plan": {
            "control": "textarea",
            "label": "项目资金使用计划",
            "width": "200px",
            "span": 24,
            "rows": 6
          },
          "project_fund_plan_amt": {
            "control": "currency",
            "label": "项目资金使用计划金额",
            "align" : "left",
            "decimal": "2",
            "format": {
              "thousand": ","
            },
            "width": "200px",
            "span": 8
          },
          "fund_source_plan": {
            "control": "textarea",
            "label": "资金来源计划",
            "width": "200px",
            "span": 24,
            "rows": 6
          },
          "fund_source_plan_amt": {
            "control": "currency",
            "label": "资金来源计划金额",
            "align" : "left",
            "decimal": "2",
            "format": {
              "thousand": ","
            },
            "width": "200px",
            "span": 8
          }
        }
      ]
    }
  }
}
