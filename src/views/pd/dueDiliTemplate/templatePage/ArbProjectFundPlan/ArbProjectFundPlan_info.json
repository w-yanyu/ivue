{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "btns": [],
      "controls": [
        {
          "apply_no": {
            "control": "hidden",
            "label": "申请号",
            "width": "200px",
            "span": 8
          },
          "cust_no": {
            "control": "hidden",
            "label": "客户编号",
            "width": "200px",
            "span": 8
          },
          "due_item_no": {
            "control": "hidden",
            "label": "调查项编号",
            "width": "200px",
            "span": 8
          },
          "project_no": {
            "control": "string",
            "label": "项目编号",
            "width": "200px",
            "span": 8
          },
          "project_fund_plan": {
            "control": "textarea",
            "label": "项目资金使用计划",
            "width": "200px",
            "disabled": "true",
            "span": 24,
            "rows": 6
          },
          "project_fund_plan_amt": {
            "control": "currency",
            "label": "项目资金使用计划金额",
            "disabled": "true",
            "align" : "left",
            "decimal": "2",
            "format": {
              "thousand": ","
            },
            "width": "200px",
            "span": 8
          },
          "fund_source_plan": {
            "control": "textarea",
            "label": "资金来源计划",
            "disabled": "true",
            "width": "200px",
            "span": 24,
            "rows": 6
          },
          "fund_source_plan_amt": {
            "control": "currency",
            "label": "资金来源计划金额",
            "disabled": "true",
            "align" : "left",
            "decimal": "2",
            "format": {
              "thousand": ","
            },
            "width": "200px",
            "span": 8
          }
        }
      ]
    }
  }
}

