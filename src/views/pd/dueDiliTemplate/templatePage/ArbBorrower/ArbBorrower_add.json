{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "ar1724",
          "due_item_no": "${{params.due_item_no}}",
          "apply_no": "${{params.apply_no}}",
          "cust_no": "${{params.cust_no}}"
        }
      },
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "ar1725",
          "due_item_no": "${{params.due_item_no}}",
          "apply_no": "${{params.apply_no}}",
          "cust_no": "${{params.cust_no}}"
        }
      },
      "controls": [
        {
          "apply_no": {
            "control": "hidden",
            "label": "申请号",
            "span": 8
          },
          "cust_no": {
            "control": "hidden",
            "label": "客户编号",
            "span": 8
          },
          "due_item_no": {
            "control": "hidden",
            "label": "调查项编号",
            "span": 8
          },
          "borrower_name": {
            "control": "text",
            "label": "借款人姓名",
            "span": 8
          },
          "age": {
            "control": "text",
            "label": "年龄",
            "align":"left",
            "span": 8
          },
          "home_address": {
            "control": "text",
            "label": "家庭住址",
            "span": 8
          },
          "job_corp": {
            "control": "text",
            "label": "工作单位",
            "span": 8
          },
          "job": {
            "control": "text",
            "label": "职务",
            "span": 8
          },
          "work_experience": {
            "control": "text",
            "label": "从业经历",
            "span": 8
          },
          "cond_desc": {
            "control": "text",
            "label": "品行描述",
            "span": 8
          },
          "fame_desc": {
            "control": "text",
            "label": "声誉描述",
            "span": 8
          },
          "is_social_bad_behavior": {
            "control": "select",
            "label": "有无社会不良行为",
            "span": 8,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "is_fore_ecod": {
            "control": "select",
            "label": "有无对外经济纠纷",
            "span": 8,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          }
        }
      ]
    }
  }
}