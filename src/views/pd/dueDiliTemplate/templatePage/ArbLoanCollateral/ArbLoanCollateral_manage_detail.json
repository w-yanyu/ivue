{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ar1824",
          "apply_no": "${{params.apply_no}}",
          "cust_no": "${{params.cust_no}}",
          "due_item_no": "${{params.due_item_no}}"
        }
      },
      "thead": [
        {
          "type": "string",
          "label": "抵押产权类型编号",
          "prop": "morp_type_no",
          "align": "center"
        },
        {
          "type": "string",
          "label": "抵押产权类型",
          "prop": "morp_type",
          "align": "center"
        },
        {
          "type": "string",
          "label": "产权人",
          "prop": "prop_owner",
          "align": "center"
        },
        {
          "type": "string",
          "label": "房产产权证号",
          "prop": "houp_credit_number",
          "align": "center"
        },
        {
          "type": "string",
          "label": "土地产权证号",
          "prop": "land_credit_number",
          "align": "center"
        },
        {
          "type": "string",
          "label": "共有人",
          "prop": "co_owner",
          "align": "center"
        },
        {
          "type": "string",
          "label": "共有证号",
          "prop": "co_credit_number",
          "align": "center"
        },
        {
          "type": "string",
          "label": "位置",
          "prop": "position",
          "align": "center"
        },
        {
          "type": "string",
          "label": "面积(㎡)",
          "prop": "area",
          "align": "center"
        },
        {
          "type": "string",
          "label": "建筑年份",
          "prop": "built_year",
          "align": "center"
        },
        {
          "type": "string",
          "label": "币种",
            "disabled": true,
          "prop": "ccy_code",
          "dict": {
            "dictType": "MS_CURRENCY",
            "dictKey": [
              "MS_CURRENCY"
            ],
            "format": "label"
          },
          "align": "center"
        },
        {
          "type": "string",
          "label": "评估单价",
          "prop": "eval_unit_price",
          "decimal": "2",
          "format": {
            "thousand": ","
          },
          "align": "center"
        },
        {
          "type": "string",
          "label": "评估总价(分层)",
          "prop": "eval_amt",
          "decimal": "2",
          "format": {
            "thousand": ","
          },
          "align": "center"
        },
        {
          "type": "string",
          "label": "状态",
          "prop": "state",
          "align": "center"
        },
        {
          "type": "string",
          "label": "租金",
          "prop": "rent_amt",
          "decimal": "2",
          "format": {
            "thousand": ","
          },
          "align": "center"
        },
        {
          "type": "string",
          "label": "土地性质",
          "prop": "land_property",
          "align": "center"
        },
        {
          "type": "string",
          "label": "其他",
          "prop": "other",
          "align": "center"
        },
        {
          "label": "操作",
          "align": "center",
          "fixed":"right",
          "width": "200px",
          "btns": [
            {
              "type": "look",
              "label": "详情",
              "toRequest": {
                "url": "/views/pd/dueDiliTemplate/templatePage/ArbLoanCollateral/ArbLoanCollateral_info.json",
                "method": "GET",
                "params": {
                  "apply_no": "${{params.apply_no}}",
                  "cust_no": "${{params.cust_no}}",
                  "due_item_no": "${{params.due_item_no}}"
                }
              },
              "props": {
                "width": "60%",
                "title": "贷款的具体抵押物信息详情"
              }
            }
          ]
        }
      ]
    }
  }
}
