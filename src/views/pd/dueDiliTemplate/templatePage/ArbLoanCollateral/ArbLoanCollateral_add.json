{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "ar1825"
            }
          }
        }
      ],
      "controls": [
        {
          "apply_no": {
            "control": "hidden",
            "label": "申请号",
            "span": 8
          },
          "cust_no": {
            "control": "hidden",
            "label": "客户编号",
            "span": 8
          },
          "due_item_no": {
            "control": "hidden",
            "label": "调查项编号",
            "span": 8
          },
          "morp_type": {
            "label": "抵制产权类型",
            "control": "text",
            "span": 8
          },
          "prop_owner": {
            "label": "产权人",
            "span": 8,
            "control": "text"
          },
          "houp_credit_number": {
            "label": "房产产权证号",
            "span": 8,
            "control": "text"
          },
          "land_credit_number": {
            "label": "土地产权证号",
            "control": "text",
            "span": 8
          },
          "co_owner": {
            "label": "共有人",
            "control": "text",
            "span": 8
          },
          "co_credit_number": {
            "label": "共有证号",
            "control": "text",
            "span": 8
          },
          "position": {
            "label": "抵押物位置",
            "control": "text",
            "span": 8
          },
          "area": {
            "label": "面积(㎡)",
            "control": "text",
            "span": 8
          },
          "built_year": {
            "control": "dateTimePicker",
            "label": "建筑年份",
            "span": "8",
            "type": "year",
            "format": "yyyy"
          },
          "ccy_code": {
            "control":"select",
            "label": "币种",
            "disabled": true,
            "span": 8,
            "value": "$g{{user_info.ccy_code}}",
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": [
                "MS_CURRENCY"
              ],
              "format": "label"
            }
          },
          "eval_unit_price": {
            "label": "评估单价",
            "control": "currency",
            "decimal": "2",
            "format": {
              "thousand": ","
            },
            "align": "left",
            "span": 8
          },
          "eval_amt": {
            "label": "评估总价",
            "control": "currency",
            "span": 8,
            "decimal": "2",
            "format": {
              "thousand": ","
            },
            "align": "left"
          },
          "state": {
            "label": "状态",
            "control": "text",
            "span": 8
          },
          "rent_amt": {
            "label": "租金",
            "control": "currency",
            "span": 8,
            "decimal": "2",
            "format": {
              "thousand": ","
            },
            "align": "left"
          },
          "land_property": {
            "label": "土地性质",
            "control": "text",
            "span": 8
          },
          "other": {
            "label": "其他",
            "control": "text",
            "span": 8
          }
        }
      ]
    }
  }
}
