{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "ar1734",
          "due_item_no": "${{params.due_item_no}}",
          "apply_no": "${{params.apply_no}}",
          "cust_no": "${{params.cust_no}}"
        }
      },
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "ar1735",
          "due_item_no": "${{params.due_item_no}}",
          "apply_no": "${{params.apply_no}}",
          "cust_no": "${{params.cust_no}}"
        }
      },
      "controls": [
        {
          "apply_no": {
            "control": "hidden",
            "label": "申请号",
            "span": 8
          },
          "cust_no": {
            "control": "hidden",
            "label": "客户编号",
            "span": 8
          },
          "due_item_no": {
            "control": "hidden",
            "label": "调查项编号",
            "span": 8
          },
          "applicant_name": {
            "control": "text",
            "label": "申请人姓名",
            "span": 8
          },
          "sex_tp": {
            "control": "select",
            "label": "性别",
            "span": 8,
            "dict": {
              "dictType": "MS_E_GENDER",
              "dictKey": [
                "MS_E_GENDER"
              ],
              "format": "label"
            }
          },
          "id_card": {
            "control": "text",
            "label": "身份证号码",
            "span": 8
          },
          "age": {
            "control": "select",
            "label": "年龄",
            "span": 8,
            "dict": {
              "dictType": "AR_E_AGE_SCOPE",
              "dictKey": [
                "AR_E_AGE_SCOPE"
              ],
              "format": "label"
            }
          },
          "spec_age": {
            "control": "text",
            "label": "具体年龄",
            "span": 8
          },
          "family_number": {
            "control": "text",
            "label": "家庭人数",
            "align": "left",
            "span": 8
          },
          "mari_status": {
            "control": "select",
            "label": "婚姻状况",
            "span": 8,
            "dict": {
              "dictType": "AR_E_MARI_STATUS",
              "dictKey": [
                "AR_E_MARI_STATUS"
              ],
              "format": "label"
            },
            "showItem": {
              "$default": [
                "spouse_job",
                "spouse_job_detail",
                "children_job_detail",
                "children_job"
              ],
              "5": []
            }
          },
          "spouse_job": {
            "control": "select",
            "label": "配偶职业",
            "span": 8,
            "dict": {
              "dictType": "AR_E_SPOUSE_JOB_TYPE",
              "dictKey": [
                "AR_E_SPOUSE_JOB_TYPE"
              ],
              "format": "label"
            }
          },
          "spouse_job_detail": {
            "control": "text",
            "label": "配偶职业具体信息",
            "span": 8
          },
          "children_job": {
            "control": "select",
            "label": "子女职业",
            "span": 8,
            "dict": {
              "dictType": "AR_E_CHILEREN_JOB_TYPE",
              "dictKey": [
                "AR_E_CHILEREN_JOB_TYPE"
              ],
              "format": "label"
            }
          },
          "children_job_detail": {
            "control": "text",
            "label": "子女职业具体信息",
            "span": 8
          },
          "home_fix_phone": {
            "control": "select",
            "label": "家庭固定电话",
            "span": 8,
            "dict": {
              "dictType": "AR_E_HOME_FIXED_TELEPHONE",
              "dictKey": [
                "AR_E_HOME_FIXED_TELEPHONE"
              ],
              "format": "label"
            },
            "showItem": {
              "1": [
                "telephone_number"
              ]
            }
          },
          "telephone_number": {
            "control": "text",
            "label": "电话号码",
            "span": 8
          },
          "mobile_no": {
            "control": "text",
            "label": "手机号码",
            "span": 8
          },
          "hous_regi_place": {
            "control": "select",
            "label": "户籍所在地",
            "span": 8,
            "dict": {
              "dictType": "AR_E_HOUR_REGI_PLACE",
              "dictKey": [
                "AR_E_HOUR_REGI_PLACE"
              ],
              "format": "label"
            }
          },
          "resi_address": {
            "control": "text",
            "label": "居住地址",
            "span": 8
          },
          "live_year": {
            "control": "select",
            "label": "居住年限",
            "span": 8,
            "dict": {
              "dictType": "AR_E_YEAR_TYPE",
              "dictKey": [
                "AR_E_YEAR_TYPE"
              ],
              "format": "label"
            }
          },
          "spec_year": {
            "control": "text",
            "label": "具体年限",
            "span": 8
          },
          "room_status": {
            "control": "select",
            "label": "住房状况",
            "span": 8,
            "dict": {
              "dictType": "AR_E_ROOM_STATUS",
              "dictKey": [
                "AR_E_ROOM_STATUS"
              ],
              "format": "label"
            }
          },
          "other_property": {
            "control": "select",
            "label": "其他财产",
            "span": 8,
            "dict": {
              "dictType": "AR_E_OTHER_PROPERTY",
              "dictKey": [
                "AR_E_OTHER_PROPERTY"
              ],
              "format": "label"
            }
          },
          "property_owner": {
            "control": "text",
            "label": "财产所有人",
            "span": 8
          },
          "ccy_code": {
            "control": "select",
            "label": "币种",
            "disabled": true,
            "span": 8,
            "value": "$g{{user_info.ccy_code}}",
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": [
                "MS_CURRENCY"
              ],
              "format": "label"
            }
          },
          "cumu_amt": {
            "control": "currency",
            "label": "累计余额",
            "decimal": "2",
            "format": {
              "thousand": ","
            },
            "align": "left",
            "span": 8
          },
          "customer_type": {
            "control": "select",
            "label": "客户类型",
            "span": 8,
            "dict": {
              "dictType": "AR_E_CUSTOMER_TYPE",
              "dictKey": [
                "AR_E_CUSTOMER_TYPE"
              ],
              "format": "label"
            }
          },
          "customer_type_detail": {
            "control": "text",
            "label": "客户类型具体情况",
            "span": 8
          },
          "credit_record": {
            "control": "select",
            "label": "信贷记录",
            "span": 8,
            "dict": {
              "dictType": "AR_E_LOAN_RECORD",
              "dictKey": [
                "AR_E_LOAN_RECORD"
              ],
              "format": "label"
            },
            "showItem": {
              "5": [
                "special_note"
              ]
            }
          },
          "special_note": {
            "control": "textarea",
            "label": "特殊说明内容",
            "width": "200px",
            "span": 24,
            "rows": 6
          }
          }
      ]
    }
  }
}
