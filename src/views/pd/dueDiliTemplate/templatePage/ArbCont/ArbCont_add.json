{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "ar1801"
            }
          }
        }
      ],
      "controls": [
        {
          "apply_no": {
            "control": "hidden",
            "label": "申请号",
            "span": 8
          },
          "cust_no": {
            "control": "hidden",
            "label": "客户编号",
            "span": 8
          },
          "due_item_no": {
            "control": "hidden",
            "label": "调查项编号",
            "span": 8
          },
          "contact_person_type": {
            "label": "联系人类型",
            "control": "select",
            "dict": {
              "dictType": "AR_E_CONTACT_PERSON_TYPE",
              "dictKey": [
                "AR_E_CONTACT_PERSON_TYPE"
              ],
              "format": "label"
            },
            "span": 8
          },
          "contact_person_name": {
            "label": "联系人",
            "control": "text",
            "span": 8
          },
          "job_corp": {
            "label": "工作单位",
            "control": "text",
            "span": 8
          },
          "work_addr": {
            "label": "家庭地址或单位地址",
            "control": "text",
            "span": 8
          },
          "cont_phone": {
            "label": "联系人电话",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ],
            "span": 8
          }
        }
      ]
    }
  }
}
