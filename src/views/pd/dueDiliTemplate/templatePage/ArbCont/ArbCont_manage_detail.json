{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ar1800",
          "apply_no": "${{params.apply_no}}",
          "cust_no": "${{params.cust_no}}",
          "due_item_no": "${{params.due_item_no}}"
        }
      },
      "thead": [
        {
          "type": "string",
          "label": "联系人编号",
          "prop": "contact_person_no",
          "align": "center"
        },
        {
          "type": "string",
          "label": "联系人类型",
          "prop": "contact_person_type",
          "dict": {
            "dictType": "AR_E_CONTACT_PERSON_TYPE",
            "dictKey": [
              "AR_E_CONTACT_PERSON_TYPE"
            ],
            "format": "label"
          },
          "align": "center"
        },
        {
          "type": "string",
          "label": "联系人名",
          "prop": "contact_person_name",
          "align": "center"
        },
        {
          "type": "string",
          "label": "工作单位",
          "prop": "job_corp",
          "align": "center"
        },
        {
          "type": "string",
          "label": "工作单位地址",
          "prop": "work_addr",
          "align": "center"
        },
        {
          "type": "string",
          "label": "联系人电话",
          "prop": "cont_phone",
          "align": "center"
        },
        {
          "label": "操作",
          "align": "center",
          "fixed":"right",
          "width": "200px",
          "btns": [
            {
              "type": "look",
              "label": "详情",
              "fixed":"right",
              "toRequest": {
                "url": "/views/pd/dueDiliTemplate/templatePage/ArbCont/ArbCont_info.json",
                "method": "GET",
                "params": {
                  "apply_no": "${{params.apply_no}}",
                  "cust_no": "${{params.cust_no}}",
                  "due_item_no": "${{params.due_item_no}}"
                }
              },
              "props": {
                "width": "60%",
                "title": "联系人信息详情"
              }
            }
          ]
        }
      ]
    }
  }
}