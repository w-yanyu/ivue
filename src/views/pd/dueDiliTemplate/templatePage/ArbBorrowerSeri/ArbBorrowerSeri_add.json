{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "ar1861"
            }
          }
        }
      ],
      "controls": [
        {
          "apply_no": {
            "control": "hidden",
            "label": "申请号",
            "span": 8
          },
          "cust_no": {
            "control": "hidden",
            "label": "客户编号",
            "span": 8
          },
          "due_item_no": {
            "control": "hidden",
            "label": "调查项编号",
            "span": 8
          },
          "invb_name": {
            "control": "text",
            "label": "投资企业名称",
            "span": 8
          },
          "regist_time": {
            "control": "dateTimePicker",
            "format": "yyyyMMdd",
            "label": "注册时间",
            "span": 8
          },
          "position": {
            "control": "text",
            "label": "位置",
            "span": 8
          },
          "ccy_code": {
            "control":"select",
            "label": "币种",
            "disabled": true,
            "span": 8,
            "value": "$g{{user_info.ccy_code}}",
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": [
                "MS_CURRENCY"
              ],
              "format": "label"
            }
          },
          "reg_capital": {
            "control": "currency",
            "label": "注册资金",
            "decimal": "2",
            "align": "left",
            "format": {
              "thousand": ","
            },
            "span": 8
          },
          "shar_form": {
            "control": "text",
            "label": "股东构成",
            "span": 8
          },
          "cont_amt": {
            "control": "currency",
            "label": "出资份额",
            "decimal": "2",
            "align": "left",
            "format": {
              "thousand": ","
            },
            "span": 8
          },
          "equi_ratio": {
            "control": "text",
            "label": "股权占比",
            "span": 8
          },
          "business_area": {
            "control": "text",
            "label": "经营范围",
            "span": 8
          },
          "main_project": {
            "control": "text",
            "label": "主营项目",
            "span": 8
          },
          "busi_item": {
            "control": "text",
            "label": "经营项目",
            "span": 8
          },
          "is_inve_fixa": {
            "control": "select",
            "label": "固定资产投资是否完成",
            "span": 8,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "oper_scale": {
            "control": "text",
            "label": "经营规模",
            "span": 8
          },
          "popularity": {
            "control": "text",
            "label": "知名度",
            "span": 8
          },
          "locm_indp": {
            "control": "text",
            "label": "当地市场行业地位",
            "span": 8
          },
          "is_oper_credit": {
            "control": "select",
            "label": "行业管理部门规定的各项经营许可证是否已办妥",
            "span": 8,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "is_oper_proc": {
            "control": "select",
            "label": "经营手续是否齐全",
            "span": 8,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          }
        }
      ]
    }
  }
}
