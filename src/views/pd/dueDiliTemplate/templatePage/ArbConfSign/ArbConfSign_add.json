{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "ar1706",
          "due_item_no": "${{params.due_item_no}}",
          "apply_no": "${{params.apply_no}}",
          "cust_no": "${{params.cust_no}}"
        }
      },
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "ar1707",
          "due_item_no": "${{params.due_item_no}}",
          "apply_no": "${{params.apply_no}}",
          "cust_no": "${{params.cust_no}}"
        }
      },
      "controls": [
        {
          "apply_no": {
            "control": "hidden",
            "label": "申请号",
            "span": 8
          },
          "cust_no": {
            "control": "hidden",
            "label": "客户编号",
            "span": 8
          },
          "due_item_no": {
            "control": "hidden",
            "label": "调查项编号",
            "span": 8
          },
          "sign_date": {
            "label": "签字日期",
            "control": "dateTimePicker",
            "format": "yyyyMMdd",
            "span": 8
          },
          "cust_sign": {
            "control": "text",
            "label": "客户签字",
            "span": 8
          },
          "id_card": {
            "control": "text",
            "label": "身份证号码",
            "span": 8
          }
        }
      ]
    }
  }
}
