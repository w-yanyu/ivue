{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "ar1849",
          "due_item_no": "${{params.due_item_no}}",
          "apply_no": "${{params.apply_no}}",
          "cust_no": "${{params.cust_no}}"
        }
      },
      "controls": [
        {
          "apply_no": {
            "control": "hidden",
            "label": "申请号",
            "span": 8
          },
          "cust_no": {
            "control": "hidden",
            "label": "客户编号",
            "span": 8
          },
          "due_item_no": {
            "control": "hidden",
            "label": "调查项编号",
            "span": 8
          },
          "year": {
            "control": "dateTimePicker",
            "label": "年份",
            "span": "8",
            "width": "25px",
            "type": "year",
            "format": "yyyy"
          },
          "busi_name": {
            "control": "text",
            "label": "企业名称",
            "width": "25px",
            "span": 8
          },
          "ccy_code": {
            "control":"select",
            "label": "币种",
            "disabled": true,
            "span": 8,
            "value": "$g{{user_info.ccy_code}}",
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": [
                "MS_CURRENCY"
              ],
              "format": "label"
            }
          },
          "capi_accu": {
            "control": "currency",
            "label": "资本积累",
            "width": "25px",
            "span": 8,
            "thousand": ","
          },
          "doc_desc": {
            "label": "描述",
            "width": "25px",
            "placeholder": "(2000个字以内,若没有请填“无”)",
            "control": "textarea",
            "span": 24,
            "autosize": {
              "minRows": 20,
              "maxRows": 100
            },
            "maxlength": 2000,
            "showWordLimit": true
          }
        }
      ]
    }
  }
}
