{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "ar1700",
          "due_item_no": "${{params.due_item_no}}",
          "apply_no": "${{params.apply_no}}",
          "cust_no": "${{params.cust_no}}"
        }
      },
      "btns": [],
      "controls": [
        {
          "apply_no": {
            "control": "hidden",
            "label": "申请号",
            "width": "140px",
            "span": 8
          },
          "cust_no": {
            "control": "hidden",
            "label": "客户编号",
            "width": "140px",
            "span": 8
          },
          "cust_name": {
            "control": "string",
            "label": "客户姓名",
            "width": "140px",
            "span": 8
          },
          "mobile_no": {
            "control": "string",
            "label": "手机号码",
            "width": "140px",
            "span": 8
          },
          "sex_tp": {
            "control": "select",
            "label": "性别",
            "width": "140px",
            "span": 8,
            "disabled": true,
            "dict": {
              "dictType": "MS_E_GENDER",
              "dictKey": [
                "MS_E_GENDER"
              ],
              "format": "label"
            }
          },
          "due_item_no": {
            "control": "hidden",
            "label": "调查项编号",
            "width": "140px",
            "span": 8
          },
          "id_card": {
            "control": "string",
            "label": "身份证号码",
            "width": "140px",
            "span": 8
          },
          "home_address": {
            "control": "string",
            "label": "家庭住址",
            "width": "140px",
            "span": 8
          },
          "marr_state": {
            "control": "select",
            "label": "婚姻状态",
            "width": "140px",
            "span": 8,
            "disabled": true,
            "dict": {
              "dictType": "MS_E_MARITA",
              "dictKey": [
                "MS_E_MARITA"
              ],
              "format": "label"
            }
          },
          "max_educat_backg": {
            "control": "select",
            "label": "最高学历",
            "width": "140px",
            "span": 8,
            "disabled": true,
            "dict": {
              "dictType": "MS_E_MAXEDU",
              "dictKey": [
                "MS_E_MAXEDU"
              ],
              "format": "label"
            }
          },
          "live_info": {
            "control": "select",
            "label": "居住情况",
            "width": "140px",
            "span": 8,
            "disabled": true,
            "dict": {
              "dictType": "AR_E_LIVE_INFO",
              "dictKey": [
                "AR_E_LIVE_INFO"
              ],
              "format": "label"
            }
          },
          "asset_status":{
            "control": "checkbox",
            "span": "24",
            "width":"140px",
            "label": "资产情况",
            "dict": {
              "dictType": "AR_E_ASSET_STATUS",
              "dictKey": [
                "AR_E_ASSET_STATUS"
              ],
              "labelField": "dictName",
              "valueField": "dictId"
            },
            "disabled": true
          },
          "job_corp": {
            "control": "string",
            "label": "工作单位",
            "width": "140px",
            "span": 8
          },
          "work_addr": {
            "control": "string",
            "label": "工作单位地址",
            "width": "140px",
            "span": 8
          },
          "job": {
            "control": "string",
            "label": "职务",
            "width": "140px",
            "span": 8
          },
          "work_years": {
            "control": "string",
            "label": "工作年限",
            "align": "left",
            "width": "140px",
            "span": 8
          },
          "ccy_code": {
            "control":"select",
            "label": "币种",
            "disabled": true,
            "width": "140px",
            "span": 8,
            "value": "$g{{user_info.ccy_code}}",
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": [
                "MS_CURRENCY"
              ],
              "format": "label"
            },
            "disabled": true
          },
          "year_income": {
            "control": "currency",
            "label": "年收入",
            "decimal": "2",
            "disabled": true,
            "align": "left",
            "format": {
              "thousand": ","
            },
            "width": "140px",
            "span": 8
          },
          "work_type": {
            "control": "select",
            "label": "单位类型",
            "width": "140px",
            "span": 8,
            "disabled": true,
            "dict": {
              "dictType": "AR_E_WORK_TYPE",
              "dictKey": [
                "AR_E_WORK_TYPE"
              ],
              "format": "label"
            }
          },
          "income_impl": {
            "control": "checkbox",
            "span": "24",
            "width":"140px",
            "label": "收入落实",
            "dict": {
              "dictType": "AR_E_INCOME_IMPL",
              "dictKey": [
                "AR_E_INCOME_IMPL"
              ],
              "labelField": "dictName",
              "valueField": "dictId"
            },
            "disabled": true
          }
        }
      ]
    }
  }
}
