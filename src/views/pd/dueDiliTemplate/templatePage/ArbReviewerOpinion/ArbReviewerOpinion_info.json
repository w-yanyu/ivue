{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "ar1722",
          "due_item_no": "${{params.due_item_no}}",
          "apply_no": "${{params.apply_no}}",
          "cust_no": "${{params.cust_no}}"
        }
      },
      "btns": [],
      "controls": [
        {
          "apply_no": {
            "control": "hidden",
            "label": "申请号",
            "span": 8
          },
          "cust_no": {
            "control": "hidden",
            "label": "客户编号",
            "span": 8
          },
          "due_item_no": {
            "control": "hidden",
            "label": "调查项编号",
            "span": 8
          },
          "loan_data_inve_opin": {
            "control": "select",
            "label": "调查岗报送的贷款资料（包括佐证资料）与调查意见及其附表所反映的信息",
            "span": 24,
            "disabled": true,
            "dict": {
              "dictType": "AR_E_LOAN_DATA_INVE_INFO",
              "dictKey": [
                "AR_E_LOAN_DATA_INVE_INFO"
              ],
              "format": "label"
            }
          },
          "loav_opinion": {
            "control": "select",
            "label": "贷款调查人意见",
            "span": 8,
            "disabled": true,
            "dict": {
              "dictType": "AR_E_OPINION",
              "dictKey": [
                "AR_E_OPINION"
              ],
              "format": "label"
            }
          },
          "basc_type": {
            "control": "select",
            "label": "违约类型",
            "span": 8,
            "disabled": true,
            "dict": {
              "dictType": "AR_E_BREA_CONTRACT_TYPE",
              "dictKey": [
                "AR_E_BREA_CONTRACT_TYPE"
              ],
              "format": "label"
            }
          },
          "report_busi": {
            "control": "select",
            "label": "上报本业务",
            "span": 8,
            "disabled": true,
            "dict": {
              "dictType": "AR_E_OPINION",
              "dictKey": [
                "AR_E_OPINION"
              ],
              "format": "label"
            }
          },
          "ccy_code": {
            "control":"string",
            "label": "币种",
            "disabled": true,
            "span": 8,
            "value": "$g{{user_info.ccy_code}}",
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": [
                "MS_CURRENCY"
              ],
              "format": "label"
            }
          },
          "pled_price": {
            "control": "currency",
            "label": "抵押价",
            "align": "left",
            "decimal": "2",
            "format": {
              "thousand": ","
            },
            "disabled": true,
            "span": 8
          },
          "mortgage_rate": {
            "control": "currency",
            "label": "抵押率",
            "align": "left",
            "disabled": true,
            "span": 8
          },
          "revi_sign": {
            "control": "string",
            "label": "审核人签字",
            "span": 8
          },
          "sign_date": {
            "label": "签字日期",
            "control": "dateTimePicker",
            "format": "yyyyMMdd",
            "span": 8,
            "disabled": true
          }
        }
      ]
    }
  }
}
