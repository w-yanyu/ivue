{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "ar1714",
          "due_item_no": "${{params.due_item_no}}",
          "apply_no": "${{params.apply_no}}",
          "cust_no": "${{params.cust_no}}"
        }
      },
      "btns": [],
      "controls": [
        {
          "apply_no": {
            "control": "hidden",
            "label": "申请号",
            "span": 8
          },
          "cust_no": {
            "control": "hidden",
            "label": "客户编号",
            "span": 8
          },
          "due_item_no": {
            "control": "hidden",
            "label": "调查项编号",
            "span": 8
          },
          "is_compound_guarantee": {
            "control": "select",
            "label": "是否复合担保",
            "span": 8,
            "width": "200px",
            "disabled": true,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "compound_gua_spec_way": {
            "control": "checkbox",
            "label": "复合担保的具体方式",
            "dict": {
              "dictType": "AR_E_GUAR_SPEC_WAY",
              "dictKey": [
                "AR_E_GUAR_SPEC_WAY"
              ],
              "format": "label"
            },
            "width": "200px",
            "disabled": true,
            "span": 24
          }
        },
        {
          "is_part_guarantee": {
            "control": "select",
            "label": "是否部分担保",
            "span": 8,
            "width": "200px",
            "disabled": true,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "part_guar_spec_way": {
            "control": "checkbox",
            "label": "部分担保的具体方式",
            "dict": {
              "dictType": "AR_E_GUAR_SPEC_WAY",
              "dictKey": [
                "AR_E_GUAR_SPEC_WAY"
              ],
              "format": "label"
            },
            "width": "200px",
            "disabled": true,
            "span": 24
          }
        },
        {
          "guarantee_way": {
            "control": "select",
            "label": "采用的担保方式",
            "span": 8,
            "width": "200px",
            "disabled": true,
            "dict": {
              "dictType": "AR_E_GUARANTEE_WAY",
              "dictKey": [
                "AR_E_GUARANTEE_WAY"
              ],
              "format": "label"
            }
          },
          "is_double_check_pled": {
            "control": "select",
            "label": "是否双人上门核实抵押物",
            "disabled": true,
            "span": 8,
            "width": "200px",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "is_check_pled_script": {
            "control": "select",
            "label": "是否核实抵（质）押物权证原件",
            "disabled": true,
            "span": 8,
            "width": "200px",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "is_check_pled_uncontested": {
            "control": "select",
            "label": "是否确认抵（质）押物权属清晰无争议",
            "disabled": true,
            "span": 8,
            "width": "200px",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "is_lessee_declare": {
            "control": "select",
            "label": "是否有承租人申明",
            "disabled": true,
            "span": 8,
            "width": "200px",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "is_workable_second_abode": {
            "control": "select",
            "label": "是否落实第二居所",
            "disabled": true,
            "span": 8,
            "width": "200px",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "is_second_abode_pledge_home": {
            "control": "select",
            "label": "第二居所是否属于抵押人家庭自有",
            "disabled": true,
            "span": 8,
            "width": "200px",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          }
        }
      ]
    }
  }
}
