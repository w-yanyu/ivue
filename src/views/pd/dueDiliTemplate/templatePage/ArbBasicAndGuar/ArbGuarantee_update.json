{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "ar1806"
            }
          }
        }
      ],
      "controls": [
        {
          "apply_no": {
            "control": "hidden",
            "label": "申请号",
            "span": 8
          },
          "cust_no": {
            "control": "hidden",
            "label": "客户编号",
            "span": 8
          },
          "due_item_no": {
            "control": "hidden",
            "label": "调查项编号",
            "span": 8
          },
          "guar_no": {
            "control": "string",
            "label": "抵押物序号",
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ],
            "span": 8
          },
          "pledge_address": {
            "control": "text",
            "label": "抵押物地址",
            "span": 8
          },
          "is_pled_borrower": {
            "control": "select",
            "label": "抵押物所有权是否属于借款人及其配偶",
            "span": 8,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "personal_pled_type": {
            "control": "select",
            "label": "个人名下抵押物类型",
            "span": 8,
            "dict": {
              "dictType": "AR_E_PERSONAL_PLEDGE_TYPE",
              "dictKey": [
                "AR_E_PERSONAL_PLEDGE_TYPE"
              ],
              "format": "label"
            }
          },
          "is_check_pled_situ": {
            "control": "select",
            "label": "经核实，抵押物情况与评估报告是否一致",
            "span": 8,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "is_other_situation": {
            "control": "select",
            "label": "是否有其他情况",
            "span": 8,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            },
            "showItem": {
              "1": [
                "other_situation_explain"
              ]
            }
          },
          "other_situation_explain": {
            "control": "text",
            "label": "有其他情况的进行说明",
            "span": 8
          }
        }
      ]
    }
  }
}
