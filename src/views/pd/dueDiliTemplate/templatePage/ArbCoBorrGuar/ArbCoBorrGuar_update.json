{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "ar1846"
            }
          }
        }
      ],
      "controls": [
        {
          "apply_no": {
            "control": "hidden",
            "label": "申请号",
            "span": 8
          },
          "cust_no": {
            "control": "hidden",
            "label": "客户编号",
            "span": 8
          },
          "due_item_no": {
            "control": "hidden",
            "label": "调查项编号",
            "span": 8
          },
          "person_id": {
            "control": "hidden",
            "label": "人员编号",
            "span": 8
          },
          "person_type": {
            "control": "select",
            "label": "人员类别",
            "width": "200px",
            "span": 8,
            "dict": {
              "dictType": "AR_E_PEOPLE_TYPE",
              "dictKey": [
                "AR_E_PEOPLE_TYPE"
              ],
              "format": "label"
            }
          },
          "guarantor_name": {
            "control": "text",
            "label": "担保人姓名",
            "width": "200px",
            "span": 8
          },
          "applicant_rela": {
            "control": "text",
            "label": "与申请人关系",
            "width": "200px",
            "span": 8
          },
          "id_card": {
            "control": "text",
            "label": "身份证号码",
            "width": "200px",
            "span": 8
          },
          "telephone_number": {
            "control": "text",
            "label": "电话号码",
            "width": "200px",
            "span": 8
          },
          "resi_address": {
            "control": "text",
            "label": "居住地址",
            "width": "200px",
            "span": 8
          },
          "job_corp": {
            "control": "text",
            "label": "工作单位",
            "width": "200px",
            "span": 8
          },
          "job": {
            "control": "text",
            "label": "职务",
            "width": "200px",
            "span": 8
          },
          "work_addr": {
            "control": "text",
            "label": "工作单位地址",
            "width": "200px",
            "span": 8
          },
          "ccy_code": {
            "control":"select",
            "label": "币种",
            "disabled": true,
            "width": "200px",
            "span": 8,
            "value": "$g{{user_info.ccy_code}}",
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": [
                "MS_CURRENCY"
              ],
              "format": "label"
            }
          },
          "mont_income": {
            "control": "currency",
            "label": "月收入",
            "decimal": "2",
            "format": {
              "thousand": ","
            },
            "width": "200px",
            "span": 8
          },
          "major_asset": {
            "control": "text",
            "label": "主要资产",
            "width": "200px",
            "span": 8
          },
          "remark": {
            "control": "textarea",
            "label": "备注",
            "width": "200px",
            "span": 24,
            "rows": 6
          }
        }
      ]
    }
  }
}
