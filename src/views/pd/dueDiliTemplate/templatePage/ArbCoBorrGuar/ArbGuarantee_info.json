{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "btns": [],
      "controls": [
        {
          "apply_no": {
            "control": "hidden",
            "label": "申请号",
            "width": "200px",
            "span": 8,
            "disabled": true
          },
          "cust_no": {
            "control": "hidden",
            "label": "客户编号",
            "width": "200px",
            "span": 8,
            "disabled": true
          },
          "due_item_no": {
            "control": "hidden",
            "label": "调查项编号",
            "width": "200px",
            "span": 8,
            "disabled": true
          },
          "guar_no": {
            "control": "string",
            "label": "抵押物序号",
            "width": "200px",
            "span": 8
          },
          "collateral_name":{
            "control": "string",
            "label": "抵押物名称",
            "width": "200px",
            "span": 8
          },
          "pledge_address": {
            "control": "string",
            "label": "抵押物地址",
            "width": "200px",
            "span": 8
          },
          "property_owner": {
            "control": "string",
            "label": "财产所有人",
            "width": "200px",
            "span": 8
          },
          "area_or_beand": {
            "control": "string",
            "label": "建筑面积(㎡)/车辆品牌",
            "width": "200px",
            "span": 8
          },
          "prop_rigc_no": {
            "control": "string",
            "label": "产权证号",
            "width": "200px",
            "span": 8
          },
          "buy_date": {
            "control": "string",
            "label": "购入日期",
            "width": "200px",
            "span": 8
          },
          "ccy_code": {
            "control":"string",
            "label": "币种",
            "disabled": true,
            "width": "200px",
            "span": 8,
            "value": "$g{{user_info.ccy_code}}",
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": [
                "MS_CURRENCY"
              ],
              "format": "label"
            }
          },
          "purc_price": {
            "control": "currency",
            "label": "购入时价值",
            "decimal": "2",
            "align": "left",
            "format": {
              "thousand": ","
            },
            "disabled": true,
            "width": "200px",
            "span": 8
          },
          "curr_valu": {
            "control": "currency",
            "label": "当前估值",
            "decimal": "2",
            "align": "left",
            "format": {
              "thousand": ","
            },
            "disabled": true,
            "width": "200px",
            "span": 8
          },
          "mortgage_rate": {
            "control": "currency",
            "label": "抵押率",
            "align": "left",
            "disabled": true,
            "width": "200px",
            "span": 8
          }
        }
      ]
    }
  }
}
