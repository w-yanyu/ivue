{
  "module": "v_search_btn_datagrid",
  "layout": {
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "target": "addlog",
          "label": "新增",
          "toRequest": {
            "method": "GET",
            "url": "/views/pd/dueDiliTemplate/templatePage/ArbCoBorrGuar/ArbCoBorrGuar_add.json",
            "params": {
              "apply_no": "${{params.apply_no}}",
              "cust_no": "${{params.cust_no}}",
              "due_item_no": "${{params.due_item_no}}"
            }
          },
          "props": {
            "width": "60%",
            "title": "共借人及担保人信息新增"
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ar1844",
          "apply_no": "${{params.apply_no}}",
          "cust_no": "${{params.cust_no}}",
          "due_item_no": "${{params.due_item_no}}"
        }
      },
      "thead": [
        {
          "type": "string",
          "label": "人员编号",
          "prop": "person_id",
          "align": "center"
        },
        {
          "type": "string",
          "label": "人员类别",
          "prop": "person_type",
          "dict": {
            "dictType": "AR_E_PEOPLE_TYPE",
            "dictKey": [
              "AR_E_PEOPLE_TYPE"
            ],
            "format": "label"
          },
          "align": "center"
        },
        {
          "type": "string",
          "label": "担保人姓名",
          "prop": "guarantor_name",
          "align": "center"
        },
        {
          "type": "string",
          "label": "与申请人关系",
          "prop": "applicant_rela",
          "align": "center"
        },
        {
          "type": "string",
          "label": "身份证号码",
          "prop": "id_card",
          "align": "center"
        },
        {
          "type": "string",
          "label": "电话号码",
          "prop": "telephone_number",
          "align": "center"
        },
        {
          "type": "string",
          "label": "居住地址",
          "prop": "resi_address",
          "align": "center"
        },
        {
          "type": "string",
          "label": "工作单位",
          "prop": "job_corp",
          "align": "center"
        },
        {
          "type": "string",
          "label": "职务",
          "prop": "job",
          "align": "center"
        },
        {
          "type": "string",
          "label": "工作单位地址",
          "prop": "work_addr",
          "align": "center"
        },
        {
          "type": "string",
          "label": "币种",
            "disabled": true,
          "prop": "ccy_code",
          "dict": {
            "dictType": "MS_CURRENCY",
            "dictKey": [
              "MS_CURRENCY"
            ],
            "format": "label"
          },
          "align": "center"
        },
        {
          "type": "currency",
          "label": "月收入",
          "prop": "mont_income",
          "decimal": "2",
          "format": {
              "thousand": ","
            },
          "align": "center"
        },
        {
          "type": "string",
          "label": "主要资产",
          "prop": "major_asset",
          "align": "center"
        },
        {
          "type": "string",
          "label": "备注",
          "prop": "remark",
          "align": "center"
        },
        {
          "label": "操作",
          "align": "center",
          "fixed":"right",
          "width": "200px",
          "btns": [
            {
              "type": "look",
              "label": "详情",
              "toRequest": {
                "url": "/views/pd/dueDiliTemplate/templatePage/ArbCoBorrGuar/ArbCoBorrGuar_info.json",
                "method": "GET",
                "params": {
                  "apply_no": "${{params.apply_no}}",
                  "cust_no": "${{params.cust_no}}",
                  "due_item_no": "${{params.due_item_no}}"
                }
              },
              "props": {
                "width": "60%",
                "title": "共借人及担保人信息详情"
              }
            },
            {
              "type": "look",
              "label": "维护",
              "toRequest": {
                "url": "/views/pd/dueDiliTemplate/templatePage/ArbCoBorrGuar/ArbCoBorrGuar_update.json",
                "method": "GET",
                "params": {
                  "apply_no": "${{params.apply_no}}",
                  "cust_no": "${{params.cust_no}}",
                  "due_item_no": "${{params.due_item_no}}"
                }
              },
              "props": {
                "width": "60%",
                "title": "共借人及担保人信息维护"
              }
            },
            {
              "type": "custom",
              "label": "删除",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "ar1847"
                }
              },
              "confirm": {
                "title": "删除",
                "msg": "确定删除吗？",
                "confirmButtonText": "确定",
                "cancelButtonText": "取消"
              }
            }
          ]
        }
      ]
    }
  }
}
