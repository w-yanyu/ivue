{
  "module": "v_search_btn_datagrid",
  "layout": {
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "target": "addlog",
          "label": "新增",
          "toRequest": {
            "method": "GET",
            "url": "/views/pd/dueDiliTemplate/templatePage/ArbFammBankDebt/ArbFammBankDebt_add.json",
            "params": {
              "apply_no": "${{params.apply_no}}",
              "cust_no": "${{params.cust_no}}",
              "due_item_no": "${{params.due_item_no}}"
            }
          },
          "props": {
            "width": "60%",
            "title": "借款人家庭成员对银行负债情况信息新增"
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ar1816",
          "apply_no": "${{params.apply_no}}",
          "cust_no": "${{params.cust_no}}",
          "due_item_no": "${{params.due_item_no}}"
        }
      },
      "thead": [
        {
          "type": "string",
          "label": "借款人编号",
          "prop": "borrower_no",
          "align": "center"
        },
        {
          "type": "string",
          "label": "借款人姓名",
          "prop": "borrower_name",
          "align": "center"
        },
        {
          "type": "string",
          "label": "贷款品种",
          "prop": "loan_vari",
          "align": "center"
        },
        {
          "type": "string",
          "label": "币种",
            "disabled": true,
          "prop": "ccy_code",
          "dict": {
            "dictType": "MS_CURRENCY",
            "dictKey": [
              "MS_CURRENCY"
            ],
            "format": "label"
          },
          "align": "center"
        },
        {
          "type": "string",
          "label": "贷款金额",
          "prop": "loan_amt",
          "decimal": "2",
          "format": {
            "thousand": ","
          },
          "align": "center"
        },
        {
          "type": "string",
          "label": "借款起始日",
          "prop": "loan_stad",
          "align": "center"
        },
        {
          "type": "string",
          "label": "还款方式",
          "prop": "repay_type",
          "dict": {
            "dictType": "MS_E_REPAY_WAY",
            "dictKey": [
              "MS_E_REPAY_WAY"
            ],
            "labelField": "dictName",
            "valueField": "dictId"
          },
          "align": "center"
        },
        {
          "type": "string",
          "label": "当前金额",
          "prop": "curr_amt",
          "decimal": "2",
          "align": "center",
          "format": {
            "thousand": ","
          }
        },
        {
          "type": "string",
          "label": "贷款行",
          "prop": "loan_bank",
          "align": "center"
        },
        {
          "type": "string",
          "label": "累计逾期期数",
          "prop": "cumu_ovep",
          "align": "center"
        },
        {
          "type": "string",
          "label": "最高逾期期数",
          "prop": "highest_ovep",
          "align": "center"
        },
        {
          "type": "string",
          "label": "抵押物名称",
          "prop": "collateral_name",
          "align": "center"
        },
        {
          "type": "string",
          "label": "备注",
          "prop": "remark",
          "align": "center"
        },
        {
          "label": "操作",
          "align": "center",
          "fixed":"right",
          "width": "200px",
          "btns": [
            {
              "type": "look",
              "label": "详情",
              "toRequest": {
                "url": "/views/pd/dueDiliTemplate/templatePage/ArbFammBankDebt/ArbFammBankDebt_info.json",
                "method": "GET",
                "params": {
                  "apply_no": "${{params.apply_no}}",
                  "cust_no": "${{params.cust_no}}",
                  "due_item_no": "${{params.due_item_no}}"
                }
              },
              "props": {
                "width": "60%",
                "title": "借款人家庭成员对银行负债情况信息详情"
              }
            },
            {
              "type": "look",
              "label": "维护",
              "toRequest": {
                "url": "/views/pd/dueDiliTemplate/templatePage/ArbFammBankDebt/ArbFammBankDebt_update.json",
                "method": "GET",
                "params": {
                  "apply_no": "${{params.apply_no}}",
                  "cust_no": "${{params.cust_no}}",
                  "due_item_no": "${{params.due_item_no}}"
                }
              },
              "props": {
                "width": "60%",
                "title": "借款人家庭成员对银行负债情况信息维护"
              }
            },
            {
              "type": "custom",
              "label": "删除",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "ar1819"
                }
              },
              "confirm": {
                "title": "删除",
                "msg": "确定删除吗？",
                "confirmButtonText": "确定",
                "cancelButtonText": "取消"
              }
            }
          ]
        }
      ]
    }
  }
}
