{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "ar1818"
            }
          }
        }
      ],
      "controls": [
        {
          "apply_no": {
            "control": "hidden",
            "label": "申请号",
            "span": 8
          },
          "cust_no": {
            "control": "hidden",
            "label": "客户编号",
            "span": 8
          },
          "due_item_no": {
            "control": "hidden",
            "label": "调查项编号",
            "span": 8
          },
          "borrower_no":{
            "control": "string",
            "label": "借款人编号",
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ],
            "span": 8
          },
          "borrower_name": {
            "label": "借款人姓名",
            "control": "text",
            "span": 8
          },
          "loan_vari": {
            "label": "贷款品种",
            "control": "text",
            "span": 8
          },
          "ccy_code": {
            "control":"select",
            "label": "币种",
            "disabled": true,
            "span": 8,
            "value": "$g{{user_info.ccy_code}}",
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": [
                "MS_CURRENCY"
              ],
              "format": "label"
            }
          },
          "loan_amt": {
            "label": "贷款金额",
            "decimal": "2",
            "align": "left",
            "control": "currency",
            "format": {
              "thousand": ","
            },
            "span": 8
          },
          "loan_stad": {
            "label": "贷款起始日",
            "control": "dateTimePicker",
            "format": "yyyyMMdd",
            "span": 8
          },
          "repay_type": {
            "label": "还款方式",
            "control": "select",
            "dict": {
              "dictType": "MS_E_REPAY_WAY",
              "dictKey": [
                "MS_E_REPAY_WAY"
              ],
              "labelField": "dictName",
              "valueField": "dictId"
            },
            "span": 8
          },
          "curr_amt": {
            "label": "当前金额",
            "control": "currency",
            "decimal": "2",
            "align": "left",
            "format": {
              "thousand": ","
            },
            "span": 8
          },
          "loan_bank": {
            "label": "贷款行",
            "control": "text",
            "span": 8
          },
          "cumu_ovep": {
            "label": "累计逾期期数",
            "control": "text",
            "span": 8
          },
          "highest_ovep": {
            "label": "最高逾期期数",
            "control": "text",
            "span": 8
          },
          "collateral_name": {
            "label": "抵押物",
            "control": "text",
            "span": 8
          },
          "remark": {
            "label": "备注",
            "control": "text",
            "span": 8
          }
        }
      ]
    }
  }
}
