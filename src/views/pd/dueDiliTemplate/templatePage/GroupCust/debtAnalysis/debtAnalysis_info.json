{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "ar1702",
          "due_item_no": "${{params.due_item_no}}",
          "apply_no": "${{params.apply_no}}",
          "cust_no": "${{params.cust_no}}"
        }
      },
      "btns": [],
      "controls": [
        {
          "apply_no": {
            "control": "hidden",
            "label": "申请号",
            "span": 8
          },
          "cust_no": {
            "control": "hidden",
            "label": "客户编号",
            "span": 8
          },
          "due_item_no": {
            "control": "hidden",
            "label": "调查项编号",
            "span": 8
          },
          "desc1": {
            "label": "短期借款和应付票据",
            "width": "120px",
            "placeholder": "(2000个字以内,若没有请填“无”)",
            "control": "textarea",
            "span": 24,
            "rows":"30",
            "maxlength": 2000,
            "showWordLimit": true,
            "disabled": true
          },
          "desc2": {
            "label": "应付账款的对象和期限",
            "width": "120px",
            "placeholder": "(2000个字以内,若没有请填“无”)",
            "control": "textarea",
            "span": 24,
            "rows":"30",
            "maxlength": 2000,
            "showWordLimit": true,
            "disabled": true
          },
          "desc3": {
            "label": "应付税金和应付工资的金额、期限是否合理",
            "width": "120px",
            "placeholder": "(2000个字以内,若没有请填“无”)",
            "control": "textarea",
            "span": 24,
            "rows":"30",
            "maxlength": 2000,
            "showWordLimit": true,
            "disabled": true
          },
          "desc4": {
            "label": "长期负债的构成（其中银行借款所用建设项目的项目进度和效益，还款计划。）",
            "width": "120px",
            "placeholder": "(2000个字以内,若没有请填“无”)",
            "control": "textarea",
            "span": 24,
            "rows":"30",
            "maxlength": 2000,
            "showWordLimit": true,
            "disabled": true
          }
        }
      ]
    }
  }
}
