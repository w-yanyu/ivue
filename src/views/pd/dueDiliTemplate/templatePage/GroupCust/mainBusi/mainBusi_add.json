{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "ar1736",
          "due_item_no": "${{params.due_item_no}}",
          "apply_no": "${{params.apply_no}}",
          "cust_no": "${{params.cust_no}}"
        }
      },
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "ar1737",
          "due_item_no": "${{params.due_item_no}}",
          "apply_no": "${{params.apply_no}}",
          "cust_no": "${{params.cust_no}}",
          "#due_item_no": "custETA4",
          "#apply_no": "123123123123",
          "#cust_no": "UR00000000014904"
        }
      },
      "controls": [
        {
          "apply_no": {
            "control": "hidden",
            "label": "申请号",
            "width":"150px",
            "span": 8
          },
          "cust_no": {
            "control": "hidden",
            "label": "客户编号",
            "width":"150px",
            "span": 8
          },
          "due_item_no": {
            "control": "hidden",
            "label": "调查项编号",
            "width":"150px",
            "span": 8
          },
          "ent_industry_class": {
            "control": "text",
            "label": "核心企业的行业分类",
            "width":"150px",
            "span": 8
          },
          "oper_main_prod": {
            "control": "text",
            "label": "经营主要产品",
            "width":"150px",
            "span": 8
          },
          "oper_scale": {
            "control": "select",
            "label": "经营规模",
            "width":"150px",
            "span": 8,
            "dict": {
              "dictType": "AR_E_AGRIC_OPRT_SCALE",
              "dictKey": [
                "AR_E_AGRIC_OPRT_SCALE"
              ],
              "labelField": "dictName",
              "valueField": "dictId",
              "format": "label"
            }
          },
          "market_share_rate": {
            "control": "currency",
            "label": "市场占有率(%)",
            "width":"150px",
            "span": 8,
            "align": "left",
            "format": {
              "thousand": ",",
              "decimal": 2
            }
          },
          "assets_prop": {
            "control": "currency",
            "label": "在集团内的资产占比(%)",
            "width":"150px",
            "span": 8,
            "align": "left",
            "format": {
              "thousand": ",",
              "decimal": 2
            }
          },
          "profit_prop": {
            "control": "currency",
            "label": "在集团内的利润占比(%)",
            "width":"150px",
            "span": 8,
            "align": "left",
            "format": {
              "thousand": ",",
              "decimal": 2
            }
          },
          "locm_indp": {
            "control": "textarea",
            "label": "当地市场行业地位",
            "width":"150px",
            "span": 24,
            "rows":"30",
            "maxlength": 2000,
            "showWordLimit": true
          },
          "profitability": {
            "control": "textarea",
            "label": "盈利水平",
            "width":"150px",
            "span": 24,
            "rows":"30",
            "maxlength": 2000,
            "showWordLimit": true
          }
        }
      ]
    }
  }
}
