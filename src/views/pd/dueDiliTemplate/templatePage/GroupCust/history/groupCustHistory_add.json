{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "ar1702",
          "due_item_no": "${{params.due_item_no}}",
          "apply_no": "${{params.apply_no}}",
          "cust_no": "${{params.cust_no}}"
        }
      },
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "ar1703",
          "due_item_no": "${{params.due_item_no}}",
          "apply_no": "${{params.apply_no}}",
          "cust_no": "${{params.cust_no}}"
        }
      },
      "controls": [
        {
          "apply_no": {
            "control": "hidden",
            "label": "申请号",
            "span": 8
          },
          "cust_no": {
            "control": "hidden",
            "label": "客户编号",
            "span": 8
          },
          "due_item_no": {
            "control": "hidden",
            "label": "调查项编号",
            "span": 8
          },
          "desc1": {
            "label": "历史沿革",
            "width": "120px",
            "placeholder": "(2000个字以内,若没有请填“无”)",
            "control": "textarea",
            "span": 24,
            "rows":"30",
            "maxlength": 2000,
            "showWordLimit": true
          }
        }
      ]
    }
  }
}
