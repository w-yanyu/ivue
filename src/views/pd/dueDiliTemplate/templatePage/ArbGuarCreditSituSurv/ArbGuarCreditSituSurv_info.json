{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "ar1716",
          "due_item_no": "${{params.due_item_no}}",
          "apply_no": "${{params.apply_no}}",
          "cust_no": "${{params.cust_no}}"
        }
      },
      "btns": [],
      "controls": [
        {
          "apply_no": {
            "control": "hidden",
            "label": "申请号",
            "span": 8
          },
          "cust_no": {
            "control": "hidden",
            "label": "客户编号",
            "span": 8
          },
          "due_item_no": {
            "control": "hidden",
            "label": "调查项编号",
            "span": 8
          },
          "is_another_name": {
            "control": "select",
            "label": "其是否有曾用名或别名",
            "span": 24,
            "disabled": true,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            },
            "showItem": {
              "1": [
                "another_name"
              ]
            }
          },
          "another_name": {
            "control": "string",
            "label": "曾用名或别名",
            "span": 24
          },
          "is_surv_all_name_report": {
            "control": "select",
            "label": "是否已查询其所有姓名的信用报告",
            "span": 24,
            "disabled": true,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "is_query_system_confirm_identity": {
            "control": "select",
            "label": "其是否联网查询公民身份信息系统、确认上述保证人身份属实且合法有效",
            "span": 24,
            "disabled": true,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "is_own_chinese_nationality": {
            "control": "select",
            "label": "其是否拥有中华人民共和国国籍",
            "span": 24,
            "disabled": true,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "is_own_residence": {
            "control": "select",
            "label": "是否在我行辖内有固定住所",
            "span": 24,
            "disabled": true,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "is_own_full_civil_capacity": {
            "control": "select",
            "label": "是否具有完全民事行为能力",
            "span": 24,
            "disabled": true,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "is_own_bankrupt_action": {
            "control": "select",
            "label": "在其担任单位或组织的法定代表人、董事或高级管理人员期间，该单位或组织是否有破产、逃废银行债务等行为",
            "span": 24,
            "disabled": true,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "is_own_illegal_crime_action": {
            "control": "select",
            "label": "其是否有赌博、吸毒等不良行为或犯罪记录的【过失行为除外】",
            "span": 24,
            "disabled": true,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "is_own_bad_credit_record": {
            "control": "select",
            "label": "其是否有贷款逾期、欠息、信用卡恶意透支等不良信用记录",
            "span": 24,
            "disabled": true,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "is_own_good_credit_record": {
            "control": "select",
            "label": "是否有良好的信用记录和还款意愿",
            "span": 24,
            "disabled": true,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "is_age_adult": {
            "control": "select",
            "label": "其年龄是否在124（含）到60（含）周岁之间",
            "span": 24,
            "disabled": true,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "is_exceed_b_grade": {
            "control": "select",
            "label": "其年在我行零售内部评级的客户风险等级是否在B级（含）以上",
            "span": 24,
            "disabled": true,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "is_exceed_four_stars": {
            "control": "select",
            "label": "保证人为我行存量贷款客户的，其客户贡献星级是否在4星级（含）以上",
            "span": 24,
            "disabled": true,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "is_open_account": {
            "control": "select",
            "label": "其是否在我行开立个人结算账户",
            "span": 24,
            "disabled": true,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "is_guarantor_spouse_sign": {
            "control": "select",
            "label": "保证人的配偶是否签署同意提供担保的书面文件",
            "span": 24,
            "disabled": true,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "is_guarantor_satisfy_condition": {
            "control": "string",
            "label": "保证人满足的特定准入条件",
            "span": 24
          }
        }
      ]
    }
  }
}