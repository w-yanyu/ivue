{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "ar1742",
          "due_item_no": "${{params.due_item_no}}",
          "apply_no": "${{params.apply_no}}",
          "cust_no": "${{params.cust_no}}"
        }
      },
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "ar1743",
          "due_item_no": "${{params.due_item_no}}",
          "apply_no": "${{params.apply_no}}",
          "cust_no": "${{params.cust_no}}"
        }
      },
      "controls": [
        {
          "apply_no": {
            "control": "hidden",
            "label": "申请号",
            "span": 8
          },
          "cust_no": {
            "control": "hidden",
            "label": "客户编号",
            "span": 8
          },
          "due_item_no": {
            "control": "hidden",
            "label": "调查项编号",
            "span": 8
          },
          "conc_obse": {
            "label": "结论性意见",
            "width": "120px",
            "placeholder": "(2000个字以内,若没有请填“无”)",
            "control": "textarea",
            "span": 24,
            "rows":"30",
            "maxlength": 2000,
            "showWordLimit": true
          },
          "acco_mana_sign": {
            "control": "text",
            "label": "客户经理签字",
            "span": 8
          },
          "telephone_number": {
            "control": "text",
            "label": "电话号码",
            "span": 8
          },
          "sign_date": {
            "label": "签字日期",
            "control": "dateTimePicker",
            "format": "yyyyMMdd",
            "span": 8
          }
        }
      ]
    }
  }
}
