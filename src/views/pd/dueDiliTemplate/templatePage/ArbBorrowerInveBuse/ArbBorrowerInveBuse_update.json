{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "ar1858"
            }
          }
        }
      ],
      "controls": [
        {
          "apply_no": {
            "control": "hidden",
            "label": "申请号",
            "span": 8
          },
          "cust_no": {
            "control": "hidden",
            "label": "客户编号",
            "span": 8
          },
          "due_item_no": {
            "control": "hidden",
            "label": "调查项编号",
            "span": 8
          },
          "buse_no": {
            "label": "商贸企业编号",
            "control": "string",
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ],
            "span": 8
          },
          "invb_name": {
            "control": "text",
            "label": "投资企业名称",
            "span": 8
          },
          "regist_time": {
            "control": "dateTimePicker",
            "format": "yyyyMMdd",
            "label": "注册时间",
            "span": 8
          },
          "position": {
            "control": "text",
            "label": "位置",
            "span": 8
          },
          "ccy_code": {
            "control":"select",
            "label": "币种",
            "disabled": true,
            "span": 8,
            "value": "$g{{user_info.ccy_code}}",
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": [
                "MS_CURRENCY"
              ],
              "format": "label"
            }
          },
          "reg_capital": {
            "control": "currency",
            "label": "注册资金",
            "decimal": "2",
            "align": "left",
            "format": {
              "thousand": ","
            },
            "span": 8
          },
          "shar_form": {
            "control": "text",
            "label": "股东构成",
            "span": 8
          },
          "cont_amt": {
            "control": "currency",
            "label": "出资份额",
            "decimal": "2",
            "align": "left",
            "format": {
              "thousand": ","
            },
            "span": 8
          },
          "equi_ratio": {
            "control": "text",
            "label": "股权占比",
            "span": 8
          },
          "business_area": {
            "control": "text",
            "label": "经营范围",
            "span": 8
          },
          "main_project": {
            "control": "text",
            "label": "主营项目",
            "span": 8
          },
          "oper_way": {
            "control": "text",
            "label": "经营方式",
            "span": 8
          },
          "oper_main_prod": {
            "control": "text",
            "label": "经营主要产品",
            "span": 8
          },
          "is_rega_oper": {
            "control": "select",
            "label": "是否区域授权经营",
            "span": 8,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "upst_partner": {
            "control": "text",
            "label": "上游合作方",
            "span": 8
          },
          "down_partner": {
            "control": "text",
            "label": "下游合作方",
            "span": 8
          },
          "updo_seme": {
            "control": "text",
            "label": "上下游结算方式",
            "span": 8
          },
          "busi_manc": {
            "control": "text",
            "label": "企业管理情况",
            "span": 8
          },
          "mana_level": {
            "control": "text",
            "label": "经营管理水平",
            "span": 8
          },
          "busi_empl_number": {
            "control": "text",
            "label": "企业雇工人员数",
            "span": 8
          },
          "oper_place": {
            "control": "text",
            "label": "经营场所",
            "span": 8
          },
          "ware_condition": {
            "control": "text",
            "label": "仓库情况",
            "span": 8
          }
        }
      ]
    }
  }
}
