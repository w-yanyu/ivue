{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ar1856",
          "apply_no": "${{params.apply_no}}",
          "cust_no": "${{params.cust_no}}",
          "due_item_no": "${{params.due_item_no}}"
        }
      },
      "thead": [
        {
          "type": "string",
          "label": "商贸企业编号",
          "prop": "buse_no",
          "align": "center"
        },
        {
          "type": "string",
          "label": "投资企业名称",
          "prop": "invb_name",
          "align": "center"
        },
        {
          "type": "string",
          "format": "yyyyMMdd",
          "label": "注册时间",
          "prop": "regist_time",
          "align": "center"
        },
        {
          "type": "string",
          "label": "位置",
          "prop": "position",
          "align": "center"
        },
        {
          "type": "string",
          "label": "币种",
            "disabled": true,
          "prop": "ccy_code",
          "dict": {
            "dictType": "MS_CURRENCY",
            "dictKey": [
              "MS_CURRENCY"
            ],
            "format": "label"
          },
          "align": "center"
        },
        {
          "type": "string",
          "label": "注册资金",
          "decimal": "2",
          "format": {
            "thousand": ","
          },
          "prop": "reg_capital",
          "align": "center"
        },
        {
          "type": "string",
          "label": "股东构成",
          "prop": "shar_form",
          "align": "center"
        },
        {
          "type": "string",
          "label": "出资份额",
          "decimal": "2",
          "format": {
            "thousand": ","
          },
          "prop": "cont_amt",
          "align": "center"
        },
        {
          "type": "string",
          "label": "股权占比",
          "prop": "equi_ratio",
          "align": "center"
        },
        {
          "type": "string",
          "label": "经营范围",
          "prop": "business_area",
          "align": "center"
        },
        {
          "type": "string",
          "label": "主营项目",
          "prop": "main_project",
          "align": "center"
        },
        {
          "type": "string",
          "prop": "oper_way",
          "label": "经营方式",
          "align": "center"
        },
        {
          "type": "string",
          "prop": "oper_main_prod",
          "label": "经营主要产品",
          "align": "center"
        },
        {
          "type": "select",
          "prop": "is_rega_oper",
          "label": "是否区域授权经营",
          "align": "center",
          "dict": {
            "dictType": "MS_E_YESORNO",
            "dictKey": [
              "MS_E_YESORNO"
            ],
            "format": "label"
          }
        },
        {
          "type": "string",
          "prop": "upst_partner",
          "label": "上游合作方",
          "align": "center"
        },
        {
          "type": "string",
          "prop": "down_partner",
          "label": "下游合作方",
          "align": "center"
        },
        {
          "type": "string",
          "prop": "updo_seme",
          "label": "上下游结算方式",
          "align": "center"
        },
        {
          "type": "string",
          "prop": "busi_manc",
          "label": "企业管理情况",
          "align": "center"
        },
        {
          "type": "string",
          "prop": "mana_level",
          "label": "经营管理水平",
          "align": "center"
        },
        {
          "type": "string",
          "prop": "busi_empl_number",
          "label": "企业雇工人员数",
          "align": "center"
        },
        {
          "type": "string",
          "prop": "oper_place",
          "label": "经营场所",
          "align": "center"
        },
        {
          "type": "string",
          "prop": "ware_condition",
          "label": "仓库情况",
          "align": "center"
        },
        {
          "label": "操作",
          "prop": "proe_no",
          "align": "center",
          "fixed": "right",
          "width": "200px",
          "btns": [
            {
              "type": "look",
              "label": "详情",
              "toRequest": {
                "url": "/views/pd/dueDiliTemplate/templatePage/ArbBorrowerInveBuse/ArbBorrowerInveBuse_info.json",
                "method": "GET",
                "params": {
                  "apply_no": "${{params.apply_no}}",
                  "cust_no": "${{params.cust_no}}",
                  "due_item_no": "${{params.due_item_no}}"
                }
              },
              "props": {
                "width": "60%",
                "title": "借款人投资商贸企业信息详情"
              }
            }
          ]
        }
      ]
    }
  }
}
