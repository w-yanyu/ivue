{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "ar1740",
          "due_item_no": "${{params.due_item_no}}",
          "apply_no": "${{params.apply_no}}",
          "cust_no": "${{params.cust_no}}"
        }
      },
      "btns": [],
      "controlsGroup": [
        {
          "title": "借款人情况",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "accordion": false,
          "expand": true,
          "controls": [
            {
              "apply_no": {
                "control": "hidden",
                "label": "申请号",
                "span": 8
              },
              "cust_no": {
                "control": "hidden",
                "label": "客户编号",
                "span": 8
              },
              "due_item_no": {
                "control": "hidden",
                "label": "调查项编号",
                "span": 8
              },
              "borrower_name": {
                "control": "string",
                "label": "借款人姓名",
                "span": 8
              },
              "gender": {
                "control": "string",
                "label": "性别",
                "span": 8,
                "dict": {
                  "dictType": "MS_E_GENDER",
                  "dictKey": [
                    "MS_E_GENDER"
                  ],
                  "format": "label"
                }
              },
              "marital_status": {
                "control": "string",
                "label": "婚姻状况",
                "span": 8,
                "dict": {
                  "dictType": "AR_E_MARITAL_STATUS",
                  "dictKey": [
                    "AR_E_MARITAL_STATUS"
                  ],
                  "format": "label"
                }
              },
              "co_borrower_name": {
                "control": "string",
                "label": "共同(配偶)借款人姓名",
                "span": 8
              },
              "other_co_borrower_name": {
                "control": "string",
                "label": "其他共同借款人姓名",
                "span": 8
              }
            }
          ]
        },
        {
          "title": "现固定居所情况",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "expand": true,
          "controls": [
            {
              "home_address": {
                "control": "string",
                "label": "详细地址",
                "span": 8
              },
              "built_area": {
                "control": "string",
                "label": "建筑面积(㎡)",
                "span": 8
              },
              "is_rental_live": {
                "control": "string",
                "label": "是否租凭居住",
                "span": 8,
                "dict": {
                  "dictType": "MS_E_YESORNO",
                  "dictKey": [
                    "MS_E_YESORNO"
                  ],
                  "format": "label"
                }
              },
              "house_purchase_way": {
                "control": "string",
                "label": "自有住房购买方式",
                "span": 8,
                "dict": {
                  "dictType": "AR_E_HOUSE_PURCHASE_WAY",
                  "dictKey": [
                    "AR_E_HOUSE_PURCHASE_WAY"
                  ],
                  "format": "label"
                }
              }
            }
          ]
        },
        {
          "title": "个人身份及信用情况调查",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "expand": true,
          "controls": [
            {
              "is_another_name": {
                "control": "string",
                "label": "其是否有曾用名或别名",
                "span": 16,
                "dict": {
                  "dictType": "MS_E_YESORNO",
                  "dictKey": [
                    "MS_E_YESORNO"
                  ],
                  "format": "label"
                },
                "showItem": {
                  "1": [
                    "another_name"
                  ]
                }
              },
              "another_name": {
                "control": "string",
                "label": "曾用名或别名",
                "span": 24,
                "width":"240px"
              },
              "is_surv_all_name_report": {
                "control": "string",
                "label": "是否已查询其所有姓名的信用报告",
                "span": 24,
                "width":"240px",
                "dict": {
                  "dictType": "MS_E_YESORNO",
                  "dictKey": [
                    "MS_E_YESORNO"
                  ],
                  "format": "label"
                }
              },
              "is_query_system_confirm_identity": {
                "control": "string",
                "label": "其是否联网查询公民身份信息系统、确认上述保证人身份属实且合法有效",
                "span": 24,
                "width":"240px",
                "dict": {
                  "dictType": "MS_E_YESORNO",
                  "dictKey": [
                    "MS_E_YESORNO"
                  ],
                  "format": "label"
                }
              },
              "is_scale_rank": {
                "control": "string",
                "label": "我行零售内部评级系统对本借款人评定的客户评分风险等级是否在D级以上，或该笔贷款申请评分是否高于申请评分风险控制分值（470分）",
                "span": 24,
                "width":"240px",
                "dict": {
                  "dictType": "MS_E_YESORNO",
                  "dictKey": [
                    "MS_E_YESORNO"
                  ],
                  "format": "label"
                }
              },
              "pasd_opinion": {
                "control": "string",
                "label": "逾期情况核实认定意见",
                "span": 24,
                "width":"240px"
              }
            }
          ]
        }
      ]
    }
  }
}
