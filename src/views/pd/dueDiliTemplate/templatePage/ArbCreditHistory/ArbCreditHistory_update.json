{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "ar1830"
            }
          }
        }
      ],
      "controls": [
        {
          "apply_no": {
            "control": "hidden",
            "label": "申请号",
            "width": "200px",
            "span": 8
          },
          "cust_no": {
            "control": "hidden",
            "label": "客户编号",
            "width": "200px",
            "span": 8
          },
          "due_item_no": {
            "control": "hidden",
            "label": "调查项编号",
            "width": "200px",
            "span": 8
          },
          "credit_history_no": {
            "control": "hidden",
            "label": "信贷历史编号",
            "span": 8
          },
          "fina_source": {
            "control": "text",
            "label": "融资来源",
            "width": "200px",
            "span": 8
          },
          "ccy_code": {
            "control":"select",
            "label": "币种",
            "disabled": true,
            "width": "200px",
            "span": 8,
            "value": "$g{{user_info.ccy_code}}",
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": [
                "MS_CURRENCY"
              ],
              "format": "label"
            }
          },
          "loan_amt": {
            "control": "currency",
            "label": "贷款金额",
            "align": "left",
            "decimal": "2",
            "format": {
              "thousand": ","
            },
            "width": "200px",
            "span": 8
          },
          "term": {
            "control": "text",
            "label": "期限(月)",
            "width": "200px",
            "span": 8
          },
          "purpose": {
            "control": "text",
            "label": "用途",
            "width": "200px",
            "span": 8
          },
          "repay_way": {
            "control": "select",
            "label": "还款方式",
            "width": "200px",
            "span": 8,
            "dict": {
              "dictType": "MS_E_REPAY_WAY",
              "dictKey": [
                "MS_E_REPAY_WAY"
              ],
              "format": "label"
            }
          },
          "issue_date": {
            "control": "dateTimePicker",
            "label": "发放日期",
            "format": "yyyyMMdd",
            "width": "200px",
            "span": 8
          },
          "balance": {
            "control": "currency",
            "label": "余额",
            "decimal": "2",
            "align": "left",
            "format": {
              "thousand": ","
            },
            "width": "200px",
            "span": 8
          },
          "guar_way": {
            "control": "select",
            "label": "担保方式",
            "width": "200px",
            "span": 8,
            "dict": {
              "dictType": "MS_E_GUAR_TYPE",
              "dictKey": [
                "MS_E_GUAR_TYPE"
              ],
              "format": "label"
            }
          },
          "overdue_info": {
            "control": "textarea",
            "label": "逾期信息",
            "width": "200px",
            "span": 24,
            "rows": 6
          }
        }
      ]
    }
  }
}
