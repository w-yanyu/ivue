{
  "module": "v_search_btn_datagrid",
  "layout": {
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "target": "addlog",
          "label": "新增",
          "toRequest": {
            "method": "GET",
            "url": "/views/pd/dueDiliTemplate/templatePage/ArbCreditHistory/ArbCreditHistory_add.json",
            "params": {
              "apply_no": "${{params.apply_no}}",
              "cust_no": "${{params.cust_no}}",
              "due_item_no": "${{params.due_item_no}}"
            }
          },
          "props": {
            "width": "60%",
            "title": "信贷历史信息新增"
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ar1828",
          "apply_no": "${{params.apply_no}}",
          "cust_no": "${{params.cust_no}}",
          "due_item_no": "${{params.due_item_no}}"
        }
      },
      "thead": [
        {
          "type": "string",
          "label": "信贷历史编号",
          "prop": "credit_history_no",
          "align": "center"
        },
        {
          "type": "string",
          "label": "融资来源",
          "prop": "fina_source",
          "align": "center"
        },
        {
          "type": "string",
          "label": "币种",
            "disabled": true,
          "prop": "ccy_code",
          "dict": {
            "dictType": "MS_CURRENCY",
            "dictKey": [
              "MS_CURRENCY"
            ],
            "format": "label"
          },
          "align": "center"
        },
        {
          "type": "currency",
          "label": "贷款金额",
          "prop": "loan_amt",
          "decimal": "2",
          "format": {
            "thousand": ","
          },
          "align": "center"
        },
        {
          "type": "string",
          "label": "期限(月)",
          "prop": "term",
          "align": "center"
        },
        {
          "type": "string",
          "label": "用途",
          "prop": "purpose",
          "align": "center"
        },
        {
          "type": "string",
          "label": "还款方式",
          "prop": "repay_way",
          "dict": {
            "dictType": "MS_E_REPAY_WAY",
            "dictKey": [
              "MS_E_REPAY_WAY"
            ],
            "format": "label"
          },
          "align": "center"
        },
        {
          "type": "string",
          "label": "发放日期",
          "prop": "issue_date",
          "align": "center"
        },
        {
          "type": "currency",
          "label": "余额",
          "prop": "balance",
          "decimal": "2",
          "format": {
            "thousand": ","
          },
          "align": "center"
        },
        {
          "type": "string",
          "label": "担保方式",
          "prop": "guar_way",
          "dict": {
            "dictType": "MS_E_GUAR_TYPE",
            "dictKey": [
              "MS_E_GUAR_TYPE"
            ],
            "format": "label"
          },
          "align": "center"
        },
        {
          "type": "string",
          "label": "逾期信息",
          "prop": "overdue_info",
          "align": "center"
        },
        {
          "label": "操作",
          "align": "center",
          "fixed":"right",
          "width": "200px",
          "btns": [
            {
              "type": "look",
              "label": "详情",
              "toRequest": {
                "url": "/views/pd/dueDiliTemplate/templatePage/ArbCreditHistory/ArbCreditHistory_info.json",
                "method": "GET",
                "params": {
                  "apply_no": "${{params.apply_no}}",
                  "cust_no": "${{params.cust_no}}",
                  "due_item_no": "${{params.due_item_no}}"
                }
              },
              "props": {
                "width": "60%",
                "title": "信贷历史信息详情"
              }
            },
            {
              "type": "look",
              "label": "维护",
              "toRequest": {
                "url": "/views/pd/dueDiliTemplate/templatePage/ArbCreditHistory/ArbCreditHistory_update.json",
                "method": "GET",
                "params": {
                  "apply_no": "${{params.apply_no}}",
                  "cust_no": "${{params.cust_no}}",
                  "due_item_no": "${{params.due_item_no}}"
                }
              },
              "props": {
                "width": "60%",
                "title": "信贷历史信息维护"
              }
            },
            {
              "type": "custom",
              "label": "删除",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "ar1831"
                }
              },
              "confirm": {
                "title": "删除",
                "msg": "确定删除吗？",
                "confirmButtonText": "确定",
                "cancelButtonText": "取消"
              }
            }
          ]
        }
      ]
    }
  }
}
