{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "ar1821"
            }
          }
        }
      ],
      "controls": [
        {
          "apply_no": {
            "control": "hidden",
            "label": "申请号",
            "span": 8
          },
          "cust_no": {
            "control": "hidden",
            "label": "客户编号",
            "span": 8
          },
          "due_item_no": {
            "control": "hidden",
            "label": "调查项编号",
            "span": 8
          },
          "loan_type_no": {
            "label": "借款种类编号",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ],
            "span": 8
          },
          "loan_type": {
            "label": "借款种类",
            "control": "text",
            "span": 8
          },
          "loan_bank": {
            "label": "贷款行",
            "span": 8,
            "control": "text"
          },
          "ccy_code": {
            "control":"select",
            "label": "币种",
            "disabled": true,
            "span": 8,
            "value": "$g{{user_info.ccy_code}}",
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": [
                "MS_CURRENCY"
              ],
              "format": "label"
            }
          },
          "contract_amt": {
            "label": "合同金额",
            "control": "currency",
            "decimal": "2",
            "format": {
              "thousand": ","
            },
            "align": "left",
            "span": 8
          },
          "loan_term": {
            "label": "贷款期限(月)",
            "control": "text",
            "span": 8
          },
          "repay_type": {
            "label": "还款方式",
            "control": "select",
            "dict": {
              "dictType": "MS_E_REPAY_WAY",
              "dictKey": [
                "MS_E_REPAY_WAY"
              ],
              "labelField": "dictName",
              "valueField": "dictId"
            },
            "span": 8
          },
          "curr_bala": {
            "label": "当前余额",
            "control": "currency",
            "span": 8,
            "decimal": "2",
            "format": {
              "thousand": ","
            },
            "align": "left"
          },
          "spec_coll": {
            "label": "具体抵押物",
            "control": "text",
            "span": 8
          },
          "bad_record": {
            "label": "不良记录",
            "control": "text",
            "span": 8
          },
          "remark": {
            "label": "备注",
            "control": "text",
            "span": 8
          }
        }
      ]
    }
  }
}
