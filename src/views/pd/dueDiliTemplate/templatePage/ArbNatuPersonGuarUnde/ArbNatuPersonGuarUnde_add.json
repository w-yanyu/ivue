{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "ar1809"
            }
          }
        }
      ],
      "controls": [
        {
          "apply_no": {
            "control": "hidden",
            "label": "申请号",
            "span": 8
          },
          "cust_no": {
            "control": "hidden",
            "label": "客户编号",
            "span": 8
          },
          "due_item_no": {
            "control": "hidden",
            "label": "调查项编号",
            "span": 8
          },
          "pledge_person_name": {
            "label": "保证人姓名",
            "control": "text",
            "span": 8
          },
          "coll_scan_guar_comp_ability": {
            "label": "已收集并扫描的担保代偿能力证明材料",
            "control": "checkbox",
            "span": "24",
            "width":"200px",
            "dict": {
              "dictType": "AR_E_COMP_ABILITY",
              "dictKey": [
                "AR_E_COMP_ABILITY"
              ],
              "labelField": "dictName",
              "valueField": "dictId"
            }
          },
          "ccy_code": {
            "control":"select",
            "label": "币种",
            "disabled": true,
            "span": 8,
            "value": "$g{{user_info.ccy_code}}",
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": [
                "MS_CURRENCY"
              ],
              "format": "label"
            }
          },
          "existing_liability_balances": {
            "label": "现有负债余额(万元)",
            "control": "currency",
            "align": "left",
            "decimal": "2",
            "format": {
              "thousand": ","
            },
            "span": 8
          },
          "external_provided_guar_balance": {
            "label": "已对外提供担保余额(万元)",
            "control": "currency",
            "align": "left",
            "decimal": "2",
            "format": {
              "thousand": ","
            },
            "span": 8
          }

        }
      ]
    }
  }
}
