{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "ar1712",
          "due_item_no": "${{params.due_item_no}}",
          "apply_no": "${{params.apply_no}}",
          "cust_no": "${{params.cust_no}}"
        }
      },
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "ar1713",
          "due_item_no": "${{params.due_item_no}}",
          "apply_no": "${{params.apply_no}}",
          "cust_no": "${{params.cust_no}}"
        }
      },
      "controls": [
        {
          "apply_no": {
            "control": "hidden",
            "label": "申请号",
            "span": 8
          },
          "cust_no": {
            "control": "hidden",
            "label": "客户编号",
            "span": 8
          },
          "due_item_no": {
            "control": "hidden",
            "label": "调查项编号",
            "span": 8
          },
          "handle_bank_check_loan_purpose": {
            "control": "checkbox",
            "label": "经办行核实确认的贷款用途",
            "span": "24",
            "dict": {
              "dictType": "AR_E_LOAN_PURPOSE_TYPE",
              "dictKey": [
                "AR_E_LOAN_PURPOSE_TYPE"
              ],
              "labelField": "dictName",
              "valueField": "dictId"
            }
          }
        },
        {
          "handle_bank_check_loan_purpose_evidence": {
            "control": "checkbox",
            "label": "经办行已核实并收集的贷款用途佐证资料",
            "span": "24",
            "dict": {
              "dictType": "AR_E_LOAN_PURPOSE_EVIDENCE",
              "dictKey": [
                "AR_E_LOAN_PURPOSE_EVIDENCE"
              ],
              "labelField": "dictName",
              "valueField": "dictId"
            }
          }
        },
        {
          "is_loan_purpose": {
            "control": "select",
            "label": "贷款用途是否真实、合法、合规",
            "span": 12,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "is_loan_amt_purpose": {
            "control": "select",
            "label": "贷款金额是否与用途匹配",
            "span": 12,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "is_borrower_comm_supplier": {
            "control": "select",
            "label": "借款人与商品或服务提供方是否存在关联关系",
            "span": 12,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "loan_pay_supervisor_mode_basis": {
            "control": "text",
            "label": "贷款支付管理方式及其依据",
            "span": 12
          }
        }
      ]
    }
  }
}