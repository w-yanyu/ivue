{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "ar1838"
            }
          }
        }
      ],
      "controls": [
        {
          "apply_no": {
            "control": "hidden",
            "label": "申请号",
            "span": 8
          },
          "cust_no": {
            "control": "hidden",
            "label": "客户编号",
            "span": 8
          },
          "due_item_no": {
            "control": "hidden",
            "label": "调查项编号",
            "span": 8
          },
          "busi_no": {
            "control": "hidden",
            "label": "企业编号",
            "span": 8
          },
          "busi_name": {
            "control": "text",
            "label": "企业名称",
            "width": "200px",
            "span": 8
          },
          "major_supp": {
            "control": "text",
            "label": "主要供应商",
            "width": "200px",
            "span": 8
          },
          "major_supp_purp": {
            "control": "currency",
            "label": "主要供应商采购占比(%)",
            "width": "200px",
            "span": 8
          },
          "major_supp_payc": {
            "control": "textarea",
            "label": "主要供应商付款条件",
            "width": "200px",
            "span": 24,
            "rows": 6
          },
          "major_cust": {
            "control": "text",
            "label": "主要客户",
            "width": "200px",
            "span": 8
          },
          "major_cust_salc": {
            "control": "currency",
            "label": "主要客户销售占比(%)",
            "width": "200px",
            "span": 8
          },
          "major_cust_payc": {
            "control": "textarea",
            "label": "主要客户付款条件",
            "width": "200px",
            "span": 24,
            "rows": 6
          }
        }
      ]
    }
  }
}
