{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ar1836",
          "apply_no": "${{params.apply_no}}",
          "cust_no": "${{params.cust_no}}",
          "due_item_no": "${{params.due_item_no}}"
        }
      },
      "thead": [
        {
          "type": "string",
          "label": "企业编号",
          "prop": "busi_no",
          "align": "center"
        },
        {
          "type": "string",
          "label": "企业名称",
          "prop": "busi_name",
          "align": "center"
        },
        {
          "type": "string",
          "label": "主要供应商",
          "prop": "major_supp",
          "align": "center"
        },
        {
          "type": "currency",
          "label": "主要供应商采购占比",
          "prop": "major_supp_purp",
          "align": "center"
        },
        {
          "type": "string",
          "label": "主要供应商付款条件",
          "prop": "major_supp_payc",
          "align": "center"
        },
        {
          "type": "string",
          "label": "主要客户",
          "prop": "major_cust",
          "align": "center"
        },
        {
          "type": "currency",
          "label": "主要客户销售占比",
          "prop": "major_cust_salc",
          "align": "center"
        },
        {
          "type": "string",
          "label": "主要客户付款条件",
          "prop": "major_cust_payc",
          "align": "center"
        },
        {
          "label": "操作",
          "align": "center",
          "fixed":"right",
          "width": "200px",
          "btns": [
            {
              "type": "look",
              "label": "详情",
              "toRequest": {
                "url": "/views/pd/dueDiliTemplate/templatePage/ArbMainCustBusi/ArbMainCustBusi_info.json",
                "method": "GET",
                "params": {
                  "apply_no": "${{params.apply_no}}",
                  "cust_no": "${{params.cust_no}}",
                  "due_item_no": "${{params.due_item_no}}"
                }
              },
              "props": {
                "width": "60%",
                "title": "主要供应商/客户及生意结构描述信息详情"
              }
            }
          ]
        }
      ]
    }
  }
}
