{
  "module": "v_search_btn_datagrid",
  "layout": {
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "target": "addlog",
          "label": "新增",
          "toRequest": {
            "method": "GET",
            "url": "/views/pd/dueDiliTemplate/templatePage/ArbBusiAsso/ArbBusiAsso_add.json",
            "params": {
              "apply_no": "${{params.apply_no}}",
              "cust_no": "${{params.cust_no}}",
              "due_item_no": "${{params.due_item_no}}"
            }
          },
          "props": {
            "width": "60%",
            "title": "业务关联方信息新增"
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ar1832",
          "apply_no": "${{params.apply_no}}",
          "cust_no": "${{params.cust_no}}",
          "due_item_no": "${{params.due_item_no}}"
        }
      },
      "thead": [

        {
          "type": "string",
          "label": "关联人编号",
          "prop": "associate_no",
          "align": "center"
        },
        {
          "type": "string",
          "label": "关联人",
          "prop": "associate",
          "align": "center"
        },
        {
          "type": "string",
          "label": "关联关系",
          "prop": "asso_rela",
          "dict": {
            "dictType": "US_E_ENT_RELATION",
            "dictKey": [
              "US_E_ENT_RELATION"
            ],
            "format": "label"
          },
          "align": "center"
        },
        {
          "type": "currency",
          "label": "占所有权比例",
          "prop": "ownership_prop",
          "align": "center"
        },
        {
          "type": "string",
          "label": "币种",
            "disabled": true,
          "prop": "ccy_code",
          "dict": {
            "dictType": "MS_CURRENCY",
            "dictKey": [
              "MS_CURRENCY"
            ],
            "format": "label"
          },
          "align": "center"
        },
        {
          "type": "currency",
          "label": "年收益",
          "prop": "year_profit",
          "decimal": "2",
          "format": {
              "thousand": ","
            },
          "align": "center"
        },
        {
          "label": "操作",
          "align": "center",
          "fixed":"right",
          "width": "200px",
          "btns": [
            {
              "type": "look",
              "label": "详情",
              "toRequest": {
                "url": "/views/pd/dueDiliTemplate/templatePage/ArbBusiAsso/ArbBusiAsso_info.json",
                "method": "GET",
                "params": {
                  "apply_no": "${{params.apply_no}}",
                  "cust_no": "${{params.cust_no}}",
                  "due_item_no": "${{params.due_item_no}}"
                }
              },
              "props": {
                "width": "60%",
                "title": "业务关联方信息详情"
              }
            },
            {
              "type": "look",
              "label": "维护",
              "toRequest": {
                "url": "/views/pd/dueDiliTemplate/templatePage/ArbBusiAsso/ArbBusiAsso_update.json",
                "method": "GET",
                "params": {
                  "apply_no": "${{params.apply_no}}",
                  "cust_no": "${{params.cust_no}}",
                  "due_item_no": "${{params.due_item_no}}"
                }
              },
              "props": {
                "width": "60%",
                "title": "业务关联方信息维护"
              }
            },
            {
              "type": "custom",
              "label": "删除",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "ar1835"
                }
              },
              "confirm": {
                "title": "删除",
                "msg": "确定删除吗？",
                "confirmButtonText": "确定",
                "cancelButtonText": "取消"
              }
            }
          ]
        }
      ]
    }
  }
}
