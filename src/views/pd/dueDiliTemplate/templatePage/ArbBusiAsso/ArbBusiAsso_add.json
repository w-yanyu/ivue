{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "ar1833"
            }
          }
        }
      ],
      "controls": [
        {
          "apply_no": {
            "control": "hidden",
            "label": "申请号",
            "width": "200px",
            "span": 8
          },
          "cust_no": {
            "control": "hidden",
            "label": "客户编号",
            "width": "200px",
            "span": 8
          },
          "due_item_no": {
            "control": "hidden",
            "label": "调查项编号",
            "width": "200px",
            "span": 8
          },
          "associate": {
            "control": "text",
            "label": "关联人",
            "width": "200px",
            "span": 8
          },
          "asso_rela": {
            "control": "select",
            "label": "关联关系",
            "width": "200px",
            "span": 8,
            "dict": {
              "dictType": "US_E_ENT_RELATION",
              "dictKey": [
                "US_E_ENT_RELATION"
              ],
              "format": "label"
            }
          },
          "ownership_prop": {
            "control": "currency",
            "label": "占所有权比例",
            "align": "left",
            "width": "200px",
            "span": 8
          },
          "ccy_code": {
            "control": "select",
            "label": "币种",
            "disabled": true,
            "width": "200px",
            "span": 8,
            "value": "$g{{user_info.ccy_code}}",
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": [
                "MS_CURRENCY"
              ],
              "format": "label"
            }
          },
          "year_profit": {
            "control": "currency",
            "label": "年收益",
            "align": "left",
            "decimal": "2",
            "format": {
              "thousand": ","
            },
            "width": "200px",
            "span": 8
          }
        }
      ]
    }
  }
}
