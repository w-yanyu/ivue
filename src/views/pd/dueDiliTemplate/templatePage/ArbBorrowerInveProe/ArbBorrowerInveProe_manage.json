{
  "module": "v_search_btn_datagrid",
  "layout": {
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "target": "addlog",
          "label": "新增",
          "toRequest": {
            "method": "GET",
            "url": "/views/pd/dueDiliTemplate/templatePage/ArbBorrowerInveProe/ArbBorrowerInveProe_add.json",
            "params": {
              "apply_no": "${{params.apply_no}}",
              "cust_no": "${{params.cust_no}}",
              "due_item_no": "${{params.due_item_no}}"
            }
          },
          "props": {
            "width": "60%",
            "title": "借款人投资生产企业信息新增"
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ar1852",
          "apply_no": "${{params.apply_no}}",
          "cust_no": "${{params.cust_no}}",
          "due_item_no": "${{params.due_item_no}}"
        }
      },
      "thead": [
        {
          "type": "string",
          "label": "生产企业编号",
          "prop": "proe_no",
          "align": "center"
        },
        {
          "type": "string",
          "label": "投资企业名称",
          "prop": "invb_name",
          "align": "center"
        },
        {
          "type": "string",
          "format": "yyyyMMdd",
          "label": "注册时间",
          "prop": "regist_time",
          "align": "center"
        },
        {
          "type": "string",
          "label": "位置",
          "prop": "position",
          "align": "center"
        },
        {
          "type": "string",
          "label": "币种",
            "disabled": true,
          "prop": "ccy_code",
          "dict": {
            "dictType": "MS_CURRENCY",
            "dictKey": [
              "MS_CURRENCY"
            ],
            "format": "label"
          },
          "align": "center"
        },
        {
          "type": "string",
          "label": "注册资金",
          "decimal": "2",
          "format": {
            "thousand": ","
          },
          "prop": "reg_capital",
          "align": "center"
        },
        {
          "type": "string",
          "label": "股东构成",
          "prop": "shar_form",
          "align": "center"
        },
        {
          "type": "string",
          "label": "出资份额",
          "decimal": "2",
          "format": {
            "thousand": ","
          },
          "prop": "cont_amt",
          "align": "center"
        },
        {
          "type": "string",
          "label": "股权占比",
          "prop": "equi_ratio",
          "align": "center"
        },
        {
          "type": "string",
          "label": "经营范围",
          "prop": "business_area",
          "align": "center"
        },
        {
          "type": "string",
          "label": "主营项目",
          "prop": "main_project",
          "align": "center"
        },
        {
          "type": "string",
          "label": "企业固定资产投入情况",
          "prop": "busi_inve_fixa",
          "align": "center"
        },
        {
          "type": "string",
          "label": "固定资产投资是否完成",
          "prop": "is_inve_fixa",
          "align": "center",
          "dict": {
            "dictType": "MS_E_YESORNO",
            "dictKey": [
              "MS_E_YESORNO"
            ],
            "format": "label"
          }
        },
        {
          "type": "string",
          "label": "是否形成生产条件",
          "prop": "is_form_proc",
          "align": "center",
          "dict": {
            "dictType": "MS_E_YESORNO",
            "dictKey": [
              "MS_E_YESORNO"
            ],
            "format": "label"
          }
        },
        {
          "type": "string",
          "label": "年生产能力",
          "prop": "year_prod_capa",
          "align": "center"
        },
        {
          "type": "string",
          "label": "企业主要产品种类",
          "prop": "busi_stap_type",
          "align": "center"
        },
        {
          "type": "string",
          "label": "技术含量或主要用途",
          "prop": "tecc_or_maiu",
          "align": "center"
        },
        {
          "type": "string",
          "label": "成本价格优劣势",
          "prop": "cosp_adva_or_disa",
          "align": "center"
        },
        {
          "type": "string",
          "label": "上游供应渠道",
          "prop": "upst_supc",
          "align": "center"
        },
        {
          "type": "string",
          "label": "下游供应渠道",
          "prop": "down_supc",
          "align": "center"
        },
        {
          "type": "string",
          "label": "主要合作客户",
          "prop": "main_cooc",
          "align": "center"
        },
        {
          "type": "string",
          "label": "上下游结算方式",
          "prop": "updo_seme",
          "align": "center"
        },
        {
          "type": "string",
          "label": "采购、生产、销售、结算共占用时间的周期(月)",
          "prop": "purc_prod_sale_sett_time",
          "align": "center"
        },
        {
          "type": "string",
          "label": "企业管理架构",
          "prop": "busi_mana",
          "align": "center"
        },
        {
          "type": "string",
          "label": "股东分红情况",
          "prop": "shar_divi",
          "align": "center"
        },
        {
          "type": "string",
          "label": "股东合作融洽度",
          "prop": "shac_hade",
          "align": "center"
        },
        {
          "type": "string",
          "label": "经营管理水平",
          "prop": "mana_level",
          "align": "center"
        },
        {
          "type": "string",
          "label": "企业雇工人员数",
          "prop": "busi_empl_number",
          "align": "center"
        },
        {
          "type": "string",
          "label": "行业管理或环保部门的批复是否已办妥",
          "prop": "is_reply_done",
          "align": "center",
          "dict": {
            "dictType": "MS_E_YESORNO",
            "dictKey": [
              "MS_E_YESORNO"
            ],
            "format": "label"
          }
        },
        {
          "label": "操作",
          "align": "center",
          "fixed":"right",
          "width": "200px",
          "btns": [
            {
              "type": "look",
              "label": "详情",
              "toRequest": {
                "url": "/views/pd/dueDiliTemplate/templatePage/ArbBorrowerInveProe/ArbBorrowerInveProe_info.json",
                "method": "GET",
                "params": {
                  "apply_no": "${{params.apply_no}}",
                  "cust_no": "${{params.cust_no}}",
                  "due_item_no": "${{params.due_item_no}}"
                }
              },
              "props": {
                "width": "60%",
                "title": "借款人投资生产企业信息详情"
              }
            },
            {
              "type": "look",
              "label": "维护",
              "toRequest": {
                "url": "/views/pd/dueDiliTemplate/templatePage/ArbBorrowerInveProe/ArbBorrowerInveProe_update.json",
                "method": "GET",
                "params": {
                  "apply_no": "${{params.apply_no}}",
                  "cust_no": "${{params.cust_no}}",
                  "due_item_no": "${{params.due_item_no}}"
                }
              },
              "props": {
                "width": "60%",
                "title": "借款人投资生产企业信息维护"
              }
            },
            {
              "type": "custom",
              "label": "删除",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "ar1855"
                }
              },
              "confirm": {
                "title": "删除",
                "msg": "确定删除吗？",
                "confirmButtonText": "确定",
                "cancelButtonText": "取消"
              }
            }
          ]
        }
      ]
    }
  }
}
