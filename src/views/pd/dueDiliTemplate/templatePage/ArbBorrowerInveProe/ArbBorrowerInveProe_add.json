{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "ar1853"
            }
          }
        }
      ],
      "controls": [
        {
          "apply_no": {
            "control": "hidden",
            "label": "申请号",
            "span": 8
          },
          "cust_no": {
            "control": "hidden",
            "label": "客户编号",
            "span": 8
          },
          "due_item_no": {
            "control": "hidden",
            "label": "调查项编号",
            "span": 8
          },
          "invb_name": {
            "control": "text",
            "label": "投资企业名称",
            "span": 8
          },
          "regist_time": {
            "control": "dateTimePicker",
            "format": "yyyyMMdd",
            "label": "注册时间",
            "span": 8
          },
          "position": {
            "control": "text",
            "label": "位置",
            "span": 8
          },
          "ccy_code": {
            "control":"select",
            "label": "币种",
            "disabled": true,
            "span": 8,
            "value": "$g{{user_info.ccy_code}}",
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": [
                "MS_CURRENCY"
              ],
              "format": "label"
            }
          },
          "reg_capital": {
            "control": "currency",
            "label": "注册资金",
            "decimal": "2",
            "align": "left",
            "format": {
              "thousand": ","
            },
            "span": 8
          },
          "shar_form": {
            "control": "text",
            "label": "股东构成",
            "span": 8
          },
          "cont_amt": {
            "control": "currency",
            "label": "出资份额",
            "decimal": "2",
            "align": "left",
            "format": {
              "thousand": ","
            },
            "span": 8
          },
          "equi_ratio": {
            "control": "text",
            "label": "股权占比",
            "span": 8
          },
          "business_area": {
            "control": "text",
            "label": "经营范围",
            "span": 8
          },
          "main_project": {
            "control": "text",
            "label": "主营项目",
            "span": 8
          },
          "busi_inve_fixa": {
            "control": "text",
            "label": "企业固定资产投入情况",
            "span": 8
          },
          "is_inve_fixa": {
            "control": "select",
            "label": "固定资产投资是否完成",
            "span": 8,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "is_form_proc": {
            "control": "select",
            "label": "是否形成生产条件",
            "span": 8,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "year_prod_capa": {
            "control": "text",
            "label": "年生产能力",
            "span": 8
          },
          "busi_stap_type": {
            "control": "text",
            "label": "企业主要产品种类",
            "span": 8
          },
          "tecc_or_maiu": {
            "control": "text",
            "label": "技术含量或主要用途",
            "span": 8
          },
          "cosp_adva_or_disa": {
            "control": "text",
            "label": "成本价格优劣势",
            "span": 8
          },
          "upst_supc": {
            "control": "text",
            "label": "上游供应渠道",
            "span": 8
          },
          "down_supc": {
            "control": "text",
            "label": "下游供应渠道",
            "span": 8
          },
          "main_cooc": {
            "control": "text",
            "label": "主要合作客户",
            "span": 8
          },
          "updo_seme": {
            "control": "text",
            "label": "上下游结算方式",
            "span": 8
          },
          "purc_prod_sale_sett_time": {
            "control": "text",
            "label": "采购、生产、销售、结算共占用时间的周期(月)",
            "span": 8
          },
          "busi_mana": {
            "control": "text",
            "label": "企业管理架构",
            "span": 8
          },
          "shar_divi": {
            "control": "text",
            "label": "股东分红情况",
            "span": 8
          },
          "shac_hade": {
            "control": "text",
            "label": "股东合作融洽度",
            "span": 8
          },
          "mana_level": {
            "control": "text",
            "label": "经营管理水平",
            "span": 8
          },
          "busi_empl_number": {
            "control": "text",
            "label": "企业雇工人员数",
            "span": 8
          },
          "is_reply_done": {
            "control": "select",
            "label": "行业管理或环保部门的批复是否已办妥",
            "span": 8,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          }
        }
      ]
    }
  }
}
