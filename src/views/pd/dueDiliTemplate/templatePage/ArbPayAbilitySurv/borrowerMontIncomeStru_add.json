{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "scope": "formData",
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "ar1710",
          "due_item_no": "${{params.due_item_no}}",
          "apply_no": "${{params.apply_no}}",
          "cust_no": "${{params.cust_no}}"
        }
      },
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "ar1711",
          "due_item_no": "${{params.due_item_no}}",
          "apply_no": "${{params.apply_no}}",
          "cust_no": "${{params.cust_no}}"
        }
      },
      "controls": [
        {
          "apply_no": {
            "control": "hidden",
            "label": "申请号",
            "span": 8
          },
          "cust_no": {
            "control": "hidden",
            "label": "客户编号",
            "span": 8
          },
          "due_item_no": {
            "control": "hidden",
            "label": "调查项编号",
            "span": 8
          },
          "ccy_code": {
            "control":"select",
            "label": "币种",
            "disabled": true,
            "span": 8,
            "value": "$g{{user_info.ccy_code}}",
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": [
                "MS_CURRENCY"
              ],
              "format": "label"
            }
          },
          "personal_salary_total_income": {
            "control": "currency",
            "label": "个人薪金收入合计",
            "decimal": "2",
            "align": "left",
            "format": {
              "thousand": ","
            },
            "span": 24
          },
          "personal_salary_total_income_basis": {
            "control": "checkbox",
            "label": "个人薪资收入合计已认定的收入依据",
            "span": "24",
            "dict": {
              "dictType": "AR_E_SALARY_TOTAL_BASIS",
              "dictKey": [
                "AR_E_SALARY_TOTAL_BASIS"
              ],
              "labelField": "dictName",
              "valueField": "dictId"
            }
          },
          "property_rental_total_income": {
            "control": "currency",
            "label": "财产出租收入合计",
            "decimal": "2",
            "align": "left",
            "format": {
              "thousand": ","
            },
            "span": 24
          },
          "property_rental_total_income_basis": {
            "control": "checkbox",
            "label": "财产出租收入合计已认定的收入依据",
            "span": "24",
            "dict": {
              "dictType": "AR_E_PROR_TOTAL_BASIS",
              "dictKey": [
                "AR_E_PROR_TOTAL_BASIS"
              ],
              "labelField": "dictName",
              "valueField": "dictId"
            }
          },
          "personal_fina_assets_total": {
            "control": "currency",
            "label": "个人金融资产合计",
            "decimal": "2",
            "align": "left",
            "disabled": true,
            "compute": "${{formData.deposit}}+${{formData.national_debt}}+${{formData.fund}}",
            "format": {
              "thousand": ","
            },
            "span": 24
          },
          "personal_fina_assets_total_basis": {
            "control": "checkbox",
            "label": "个人金融资产合计已认定的收入依据",
            "span": "24",
            "dict": {
              "dictType": "AR_E_PERF_TOTAL_BASIS",
              "dictKey": [
                "AR_E_PERF_TOTAL_BASIS"
              ],
              "labelField": "dictName",
              "valueField": "dictId"
            }
          },
          "deposit": {
            "control": "currency",
            "label": "存款",
            "decimal": "2",
            "align": "left",
            "format": {
              "thousand": ","
            },
            "span": 8
          },
          "national_debt": {
            "control": "currency",
            "label": "国债",
            "decimal": "2",
            "align": "left",
            "format": {
              "thousand": ","
            },
            "span": 8
          },
          "fund": {
            "control": "currency",
            "label": "基金",
            "decimal": "2",
            "align": "left",
            "format": {
              "thousand": ","
            },
            "span": 8
          },
          "personal_inve_income_total": {
            "control": "currency",
            "label": "个人投资收益合计",
            "decimal": "2",
            "align": "left",
            "format": {
              "thousand": ","
            },
            "span": 24
          },
          "personal_inve_income_total_basis": {
            "control": "checkbox",
            "label": "个人投资收益合计已认定的收入依据",
            "span": "24",
            "dict": {
              "dictType": "AR_E_PERI_TOTAL_BASIS",
              "dictKey": [
                "AR_E_PERI_TOTAL_BASIS"
              ],
              "labelField": "dictName",
              "valueField": "dictId"
            }
          },
          "personal_operate_income_total": {
            "control": "currency",
            "label": "个人经营收益合计",
            "decimal": "2",
            "align": "left",
            "format": {
              "thousand": ","
            },
            "span": 24
          },
          "personal_operate_income_total_basis": {
            "control": "checkbox",
            "label": "个人经营收益合计已认定的收入依据",
            "span": "24",
            "dict": {
              "dictType": "AR_E_PERO_TOTAL_BASIS",
              "dictKey": [
                "AR_E_PERO_TOTAL_BASIS"
              ],
              "labelField": "dictName",
              "valueField": "dictId"
            }
          },
          "monthly_income_total": {
            "control": "string",
            "label": "月收入总计",
            "decimal": "2",
            "align": "left",
            "compute": "${{formData.personal_salary_total_income}}+${{formData.property_rental_total_income}}+${{formData.personal_inve_income_total}}+${{formData.personal_operate_income_total}}",
            "format": {
              "thousand": ","
            },
            "span": 8
          },
          "monthly_debt": {
            "control": "text",
            "label": "月负债情况",
            "span": 8
          },
          "other_personal_assets_surv": {
            "control": "textarea",
            "label": "除自有住房外其它个人资产概况(包括借款人夫妻共有的房产、汽车、股票等动产和不动产)",
            "span": 24,
            "rows": 6
          }
        }
      ]
    }
  }
}
