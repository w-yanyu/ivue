{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "ar1736",
          "due_item_no": "${{params.due_item_no}}",
          "apply_no": "${{params.apply_no}}",
          "cust_no": "${{params.cust_no}}"
        }
      },
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "ar1737",
          "due_item_no": "${{params.due_item_no}}",
          "apply_no": "${{params.apply_no}}",
          "cust_no": "${{params.cust_no}}"
        }
      },
      "controls": [
        {
          "apply_no": {
            "control": "hidden",
            "label": "申请号",
            "align": "left",
            "width": "200px",
            "span": 8
          },
          "cust_no": {
            "control": "hidden",
            "label": "客户编号",
            "align": "left",
            "width": "200px",
            "span": 8
          },
          "due_item_no": {
            "control": "hidden",
            "label": "调查项编号",
            "align": "left",
            "width": "200px",
            "span": 8
          },
          "busi_name": {
            "control": "text",
            "label": "企业名称",
            "align": "left",
            "width": "200px",
            "span": 8
          },
          "legal_repr": {
            "control": "text",
            "label": "法人代表",
            "align": "left",
            "width": "200px",
            "span": 8
          },
          "business_address": {
            "control": "text",
            "label": "经营地址",
            "align": "left",
            "width": "200px",
            "span": 8
          },
          "actu_operator": {
            "control": "text",
            "label": "实际经营人",
            "align": "left",
            "width": "200px",
            "span": 8
          },
          "site_ownership": {
            "control": "select",
            "label": "经营场地所有权",
            "align": "left",
            "width": "200px",
            "span": 8,
            "dict": {
              "dictType": "AR_E_SITE_OWNERSHIP",
              "dictKey": [
                "AR_E_SITE_OWNERSHIP"
              ],
              "format": "label"
            }
          },
          "work_expe": {
            "control": "select",
            "label": "从业经验",
            "align": "left",
            "width": "200px",
            "span": 8,
            "dict": {
              "dictType": "AR_E_YEAR_TYPE",
              "dictKey": [
                "AR_E_YEAR_TYPE"
              ],
              "format": "label"
            }
          },
          "work_experience_spec_year": {
            "control": "text",
            "label": "从业经验具体年限",
            "align": "left",
            "width": "200px",
            "span": 8
          },
          "oper_year": {
            "control": "select",
            "label": "经营年限",
            "align": "left",
            "width": "200px",
            "span": 8,
            "dict": {
              "dictType": "AR_E_YEAR_TYPE",
              "dictKey": [
                "AR_E_YEAR_TYPE"
              ],
              "format": "label"
            }
          },
          "oper_spec_year": {
            "control": "text",
            "label": "经营具体年限",
            "align": "left",
            "width": "200px",
            "span": 8
          },
          "scene_oper_year": {
            "control": "select",
            "label": "现场所经营年限",
            "align": "left",
            "width": "200px",
            "span": 8,
            "dict": {
              "dictType": "AR_E_YEAR_TYPE",
              "dictKey": [
                "AR_E_YEAR_TYPE"
              ],
              "format": "label"
            }
          },
          "scene_oper_spec_year": {
            "control": "text",
            "label": "现场所经营具体年限",
            "align": "left",
            "width": "200px",
            "span": 8
          },
          "main_oper_busi": {
            "control": "text",
            "label": "主营业务",
            "align": "left",
            "width": "200px",
            "span": 8
          },
          "busi_empl_number": {
            "control": "text",
            "label": "企业雇工人员数",
            "align": "left",
            "width": "200px",
            "span": 8
          },
          "oper_place_area": {
            "control": "text",
            "label": "经营场所面积(㎡)",
            "align": "left",
            "width": "200px",
            "span": 8
          },
          "info_coll_verf": {
            "control": "text",
            "label": "信息收集与核实",
            "align": "left",
            "width": "200px",
            "span": 8
          }
        }
      ]
    }
  }
}
