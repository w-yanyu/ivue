            }
          },
          "contextMenu": [
            {
              "type": "add",
              "label": "新增",
              "icon": "el-icon-circle-plus-outline",
              "toRequest": {
                "url": "/views/pd/dueDiliTemplate/dueDiliTemplate_tree_item_add.json",
                "method": "get",
                "params": {
                  "group_num": "0002",
                  "parent_node_cd": "${{params.parent_node_cd}}"
