                "url": "/views/pd/dueDiliTemplate/dueDiliTemplate_tree_template_add.json",
                "method": "get",
                "params": {
                  "group_num": "0002"
                }
              },
              "props": {
                "width": "80%",
                "title": "新增模板类型"
              },
              "eventAfter": {
                "callback": "renderTree"
              }
            },
            {
              "type": "divider"
            },
            {
              "type": "delete",
              "label": "删除",
              "icon": "el-icon-remove-outline",
              "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "post",
                "params": {
                  "servicecode": "pd0354",
                  "group_num": "0002",
                  "node_cd": "${{current.node_cd}}"
                }
              },
              "confirm":{
                "title":"提示信息",
                "msg":"确认删除吗？",
                "confirmButtonText":"确定",
                "cancelButtonText":"取消"
              },
              "eventAfter": {
                "callback": "renderTree"
              }
            }
          ]
        },
        "2": {
          "children": [
            "3"
          ],
          "toRequest": {
            "url": "@dueSpringboard",
            "method": "get",
