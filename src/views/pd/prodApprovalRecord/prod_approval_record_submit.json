{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pd0301"
        }
      },
      "controls": [
        {
          "busi_seq": {
            "label": "业务流水号",
            "control": "hidden"
          },
          "sale_prod_cd": {
            "label": "可售产品编号",
            "control": "hidden"
          },
          "prod_version": {
            "label": "产品版本",
            "control": "hidden"
          }
        },
        {
          "submit_type": {
            "label": "提交类型",
            "control": "select",
            "span": 8,
            "width": "140px",
            "rules": [
              {
                "required": true,
                "message": "请选择审批类型"
              }
            ],
            "dict": {
              "dictType": "PD_E_SUBMIT_TYPE",
              "dictKey": [
                "PD_E_SUBMIT_TYPE"
              ]
            }
          }
        },
        {
          "appro_opinion": {
            "label": "审批意见",
            "control": "text",
            "span": 8,
            "width": "140px",
            "rules": [
              {
                "required": true,
                "message": "请输入审批意见"
              }
            ]
          }
        }
      ]
    }
  }
}