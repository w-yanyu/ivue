{
    "module": "v_search_btn_datagrid",
    "layout": {
      "toolbar": {
        "btns": [
        ]
      },
      "datagrid": {
        "isInitSearch": true,
        "doRequest": {
          "url": "/SUMP/icmcall/icmRPCCall",
          "method": "post",
          "params": {
            "servicecode": "pd0154",
            "template_id": "${{params.template_id}}",
            "prod_version": "${{params.prod_version}}"
          }
        },
        "pagination": {
          "pageSizes": [
            10,
            15,
            20
          ],
          "pageSize": 10
        },
        "thead": [
          {
            "type": "index",
            "align": "center"
          },
          {
