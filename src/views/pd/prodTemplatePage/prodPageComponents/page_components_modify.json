{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "pd0162"
            }
          }
        }
      ],
      "controls": [
        {
          "page_template_name": {
            "control": "text",
            "label": "页面模板名称",
            "width": "150px",
            "span": 8
          },
          "page_template_no": {
            "control": "string",
            "label": "页面模板编号",
            "width": "150px",
            "span": 8
          },

          "page_affiliation_table_name": {
            "control": "text",
            "label": "数据库表描述",
            "width": "150px",
            "span": 8
          },
          "page_affiliation_table": {
            "control": "text",
            "label": "数据库表名",
            "width": "150px",
            "span": 8
          },
          "add_path": {
            "control": "text",
            "width": "150px",
            "span": 24,
            "label": "新增页面路径",
            "rules": [
              {
                "required": true,
                "message": "必填项"
              }
            ]
          },
          "modify_path": {
            "control": "text",
            "width": "150px",
            "span": 24,
            "label": "维护页面路径",
            "rules": [
              {
                "required": true,
                "message": "必填项"
              }
            ]
          },
          "detail_path": {
            "control": "text",
            "width": "150px",
            "span": 24,
            "label": "详情页面路径",
            "rules": [
              {
                "required": true,
                "message": "必填项"
              }
            ]
          }
        }
      ]
    }
  }
}