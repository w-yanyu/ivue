{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "controls": [
                {
                    "page_template_no": {
                        "control": "text",
                        "label": "页面模板编号",
                        "rules": [
                            {
                                "max": 64,
                                "message": "长度不能超过64个字符"
                            }
                        ]
                    },
                    "page_template_name": {
                        "control": "text",
                        "label": "页面模板名称",
                        "rules": [
                            {
                                "max": 100,
                                "message": "长度不能超过100个字符"
                            }
                        ]
                    }
                }
            ]
        },
        "toolbar": {
            "btns": [
                {
                }
            ]
        },
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": true,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "post",
                "params": {
                    "servicecode": "pd0163",
                    "apply_type": "${{params.apply_type}}",
                    "prod_type": "${{params.prod_type}}"

                }
            },
            "pagination": {
                "show": true,
                "pageSize": 10
            },
            "thead": [
                {
                    "label": "页面模板编号",
                    "align": "center",
                    "prop": "page_template_no"
                },
                {
                    "label": "页面模板名称",
                    "align": "center",
                    "prop": "page_template_name"
                },
                {
                    "label": "新增页面路径",
                    "align": "center",
                    "prop": "add_path"
                },
                {
                    "label": "维护页面路径",
                    "align": "center",
                    "prop": "modify_path"
                },
                {
                    "label": "详情页面路径",
                    "align": "center",
                    "prop": "detail_path"
                },
                {
                    "label": "数据库表描述",
                    "align": "center",
                    "prop": "page_affiliation_table_name"
                },
                {
                    "label": "数据库表名",
                    "align": "center",
                    "prop": "page_affiliation_table"
                },
                {
                    "label": "操作",
                    "align": "center",
                    "width": 180,
                    "btns": [{
                      "type": "bringback",
                      "label": "选择"
                    }]
                  }
            ]
        }
    }
}