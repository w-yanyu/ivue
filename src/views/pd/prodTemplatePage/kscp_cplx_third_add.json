{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "controls": [
                {
                    "grupno": {
                        "label": "可售产品组",
                        "width": "140px",
                        "control": "hidden",
                        "value": "${{params.grupno}}",
                        "disabled": true,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入可售产品组"
                            }
                        ]
                    },
                    "id": {
                        "label": "页面模板id",
                        "width": "140px",
                        "control": "text",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入页面模板id"
                            }
                        ]
                    },
                    "pid": {
                        "label": "父节点值",
                        "width": "140px",
                        "control": "hidden",
                        "value": "${{params.id}}",
                        "disabled": true,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入父节点值"
                            }
                        ]
                    },
                    "title": {
                        "label": "页面模板名称",
                        "width": "140px",
                        "control": "text"
                    },
                    "grupnm": {
                        "label": "描述",
                        "width": "140px",
                        "control": "hidden",
                        "value": "${{params.grupnm}}"
                    },
                    "isleaf": {
                        "label": "是否叶子节点 ",
                        "width": "140px",
                        "control": "hidden",
                        "value": "true",
                        "rules": [
                            {
                                "required": true,
                                "message": "是否叶子节点"
                            }
                        ]
                    },
                    "type": {
                        "label": "节点类型",
                        "width": "140px",
                        "control": "hidden",
                        "value": "${{params.type}}",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入修改页面"
                            }
                        ]
                    }
                }
            ],
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": "",
                    "doRequest": {
                        "url": "/SUMP/call/RPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "pd0141"
                        }
                    }
                }
            ]
        }
    }
}