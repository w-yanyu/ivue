{
  "module": "v_realonlyForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "controls": [
        {
          "page_template_no": {
            "width": "180px",
            "control": "string",
            "label": "页面模板编号"
          },
          "page_template_name": {
            "width": "180px",
            "control": "string",
            "label": "页面模板名称"
          }
        },
        {
          "prod_type": {
            "width": "180px",
            "control": "string",
            "disable":true,
            "label": "产品类型"
          },
          "apply_type": {
            "width": "180px",
            "control": "string",
            "disable":true,
            "label": "申请类型"
          }
        },
        {
          "must_flag": {
            "width": "180px",
            "control": "string",
            "label": "是否必须",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "valid_flag": {
            "width": "180px",
            "control": "string",
            "label": "是否有效",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          }
        },
        {
          "sort_no": {
            "control": "string",
            "width": "180px",
            "span": 24,
            "label": "优先级序号"
          }
        },
        {
          "add_path": {
            "control": "string",
            "width": "180px",
            "span": 24,
            "label": "新增页面路径"
          }
        },
        {
          "modify_path": {
            "control": "string",
            "width": "180px",
            "span": 24,
            "label": "维护页面路径"
          }
        },
        {
          "detail_path": {
            "control": "string",
            "width": "180px",
            "span": 24,
            "label": "详情页面路径"
          }
        }
      ]
    }
  }
}
