{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [],
      "btns":[]
    },
    "datagrid": {
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "pd0111",
          "template_id": "${{params.template_id}}"
        }
      },
      "thead": [
        {
          "label": "模板编号",
          "prop": "template_id",
          "align": "center"
        },
        {
          "label": "模板名称",
          "prop": "template_name",
          "align": "center"
        },
        {
          "label": "模板版本",
          "prop": "prod_version",
          "align": "center"
        },
        {
          "label": "产品类型",
          "prop": "prod_type_name",
          "align": "center"
        },
        {
          "label": "是否有效",
          "prop": "valid_flag",
          "align": "center",
          "dict": {
            "dictType": "MS_E_YESORNO",
            "dictKey": [
              "MS_E_YESORNO"
            ],
            "format": "label"
          }
        },
        {
          "label": "是否被引用",
          "prop": "is_stop_use",
          "align": "center",
          "dict": {
            "dictType": "MS_E_YESORNO",
            "dictKey": [
              "MS_E_YESORNO"
            ],
            "format": "label"
          }
        },
        {
          "label": "操作",
          "align": "center",
          "width": 180,
          "btns": [{
            "type": "bringback",
            "label": "选择"
          }]
        }
      ]
    }
  }
}
