{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "btns": [],
      "controls": [
        {
          "template_id": {
            "control": "string",
            "label": "产品模板编号",
            "value": "${{params.template_id}}"
          },
          "template_name": {
            "control": "string",
            "label": "产品模板名称",
            "value": "${{params.template_name}}"
          },
          "prod_version": {
            "control": "string",
            "label": "版本号",
            "value": "${{params.prod_version}}"
          },
          "prod_type_name": {
            "control": "string",
            "label": "产品模板类型",
            "value": "${{params.prod_type_name}}"
          },
          "valid_flag": {
            "control": "string",
            "label": "是否有效",
            "value": "${{params.valid_flag}}",
            "dict": {
              "format": "label",
              "dictKey": [
                  "MS_E_YESORNO"
              ],
              "dictType": "MS_E_YESORNO"
            }
          },
          "cust_class": {
            "control": "string",
            "label": "适用客户类型",
            "value": "${{params.cust_class}}",
            "dict": {
              "format": "label",
              "dictKey": [
                  "PD_E_CUST_CLASS"
              ],
              "dictType": "PD_E_CUST_CLASS"
            }
          }
        }
      ]
    }
  }
}
