{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "pd0159",
          "template_id": "${{params.template_id}}",
          "prod_version": "${{params.prod_version}}",
          "apply_type": "${{params.apply_type}}",
          "prod_type": "${{params.prod_type}}"
        }
      },
      "thead": [
        {
          "label": "页面编号",
          "prop": "page_template_no",
          "control": "text",
          "align": "center"
        },
        {
          "label": "页面名称",
          "prop": "page_template_name",
          "align": "center"
        },
        {
          "label": "申请类型",
          "prop": "apply_type_name",
          "align": "center"
        },
        {
          "label": "产品类型",
          "prop": "prod_type_name",
          "align": "center"
        },
        {
          "label": "是否必须",
          "prop": "must_flag",
          "align": "center",
          "dict": {
            "dictType": "MS_E_YESORNO",
            "dictKey": [
              "MS_E_YESORNO"
            ],
            "format": "label-value"
          }
        }
      ]
    }
  }
}
