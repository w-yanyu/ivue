{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "controls": [
        {
          "protocol_no": {
            "control": "text",
            "label": "协议编码",
            "rules": [
              {
                "required": true,
                "message": "长度不能超过32个字符"
              }
            ]
          },
          "protocol_name": {
            "control": "text",
            "label": "协议名称",
            "rules": [
              {
                "required": true,
                "message": "长度不能超过20个字符"
              }
            ]
          },
          "business_type": {
            "control": "select",
            "label": "业务类型",
            "dict": {
              "dictType": "PD_E_BUSI_TYPE",
              "dictKey": [
                "PD_E_BUSI_TYPE"
              ],
              "format": "label-value"
            },
            "rules": [
              {
                "required": true,
                "message": "长度不能超过10个字符"
              }
            ]
          }
        },
        {
          "remark": {
            "control": "text",
            "label": "备注",
            "width": "130px",
            "rules": [
              {
                "required": false
              },
              {
                "max": 15,
                "message": "长度不能超过15个字符"
              }
            ]
          }
        }
      ]
    }
  }
}
