{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pd0284",
          "sale_prod_cd": "00000015",
          "prod_version": "1"
        }
      },
      "pagination": {
        "show":false
      },
      "thead": [
        {
          "label": "序号",
          "type": "index",
          "align": "center"
        },
        {
          "label": "协议编号",
          "prop": "protocol_no",
          "align": "center"
        },
        {
          "label": "业务类型",
          "prop": "business_type",
          "align": "center"
        },
        {
          "label": "协议名称",
          "prop": "protocol_name",
          "align": "center"
        },
        {
          "label": "操作",
          "fixed": "right",
          "width": 200,
          "align": "center",
          "btns": [
            {
              "type": "custom",
              "label": "详情",
              "toRequest": {
                "url": "/views/product/agreementTemplateManage/agreementTemplate_detail.json",
                "method": "GET",
                "params": {
                  "path": "${{current.protocol_no}}"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
