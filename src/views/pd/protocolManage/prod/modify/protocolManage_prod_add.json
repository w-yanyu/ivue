{
  "module": "lr_transferTable",
  "layout": {
    "transfer": {
      "height":"500px",
      "left": {
        "filterable":false,
        "title": "未关联协议",
        "btn":{
          "label": "选择",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "pd0252",
              "sale_prod_cd": "${{params.sale_prod_cd}}",
              "prod_version": "${{params.prod_version}}"
            }
          },
          "eventAfter": {
            "callback": "restCallBack"
          }
        },
        "datagrid": {
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "pd0284",
              "sale_prod_cd": "${{params.sale_prod_cd}}",
              "prod_version": "${{params.prod_version}}"
            }
          },
          "pagination": {
            "show":false
          },
          "checked":"checked",
          "thead": [
            {
              "label": "序号",
              "type": "index",
              "align": "center"
            },
            {
              "label": "协议编号",
              "prop": "protocol_no",
              "align": "center"
            },
            {
              "label": "业务类型",
              "prop": "business_type",
              "align": "center",
              "dict": {
                "dictType": "PD_E_BUSI_TYPE",
                "dictKey": [
                  "PD_E_BUSI_TYPE"
                ],
                "format": "label-value"
              }
            },
            {
              "label": "协议名称",
              "prop": "protocol_name",
              "align": "center",
              "width": "180px",
              "btns": [
                {
                  "type": "look",
                  "toRequest": {
                    "url": "@lookAgreementBtn",
                    "params": {
                      "path": "${{current.protocol_path}}"
                    }
                  },
                  "props": {
                    "width": "75%",
                    "title": "预览"
                  }
                }
              ]
            }
          ]
        }
      },
      "right": {
        "filterable":false,
        "title":"已关联协议",
        "btn":{
          "label":"取消关联",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "pd0253"
            }
          },
          "eventAfter": {
            "callback": "restCallBack"
          }
        },
        "datagrid": {
          "scope": "left",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "pd0251",
              "sale_prod_cd": "${{params.sale_prod_cd}}",
              "prod_version": "${{params.prod_version}}"
            }
          },
          "pagination": {
            "show":false
          },
          "thead": [
            {
              "label": "序号",
              "type": "index",
              "align": "center"
            },
            {
              "label": "协议编号",
              "prop": "protocol_no",
              "align": "center"
            },
            {
              "label": "业务类型",
              "prop": "business_type",
              "align": "center",
              "dict": {
                "dictType": "PD_E_BUSI_TYPE",
                "dictKey": [
                  "PD_E_BUSI_TYPE"
                ],
                "format": "label-value"
              }
            },
            {
              "label": "协议名称",
              "prop": "protocol_name",
              "align": "center",
              "width": "180px",
              "btns": [
                {
                  "type": "look",
                  "toRequest": {
                    "url": "@lookAgreementBtn",
                    "params": {
                      "path": "${{current.protocol_path}}"
                    }
                  },
                  "props": {
                    "width": "75%",
                    "title": "预览"
                  }
                }
              ]
            },
            {
              "label": "操作",
              "fixed": "right",
              "width": 100,
              "align": "center",
              "btns": [
                {
                  "type": "custom",
                  "label": "详情",
                  "toRequest": {
                    "url": "/views/product/agreementTemplateManage/agreementTemplate_detail.json",
                    "method": "GET",
                    "params": {
                      "path": "${{current.protocol_no}}"
                    }
                  }
                }
              ]
            }
          ]
        }
      }
    }
  }
}
