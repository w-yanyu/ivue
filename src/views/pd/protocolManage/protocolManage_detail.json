{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "btns": [],
      "controls": [
        {
          "protocol_no": {
            "control": "string",
            "label": "协议编码",
            "width": "130px"
          },
          "protocol_name": {
            "control": "string",
            "label": "协议名称",
            "width": "130px"
          },
          "business_type": {
            "control": "select",
            "disabled": true,
            "label": "业务类型",
            "width": "130px",
            "dict": {
              "dictType": "PD_E_BUSI_TYPE",
              "dictKey": [
                "PD_E_BUSI_TYPE"
              ],
              "format": "label"
            }
          },
          "suit_cust_type": {
            "control": "select",
            "label": "适用客户类型",
            "disabled": true,
            "width": "130px",
            "dict": {
              "dictType": "PD_E_CUST_CLASS",
              "dictKey": [
                "PD_E_CUST_CLASS"
              ],
              "format": "label"
            }
          },
          "protocol_path": {
            "control": "string",
            "label": "协议模板路径",
            "width": "130px"
          },
          "protocol_status": {
            "control": "select",
            "label": "协议状态",
            "disabled": true,
            "width": "130px",
            "dict": {
              "dictType": "PD_E_PROTOCOL_STATUS",
              "dictKey": [
                "PD_E_PROTOCOL_STATUS"
              ],
              "format": "label"
            }
          },
          "protocol_take_node": {
            "control": "select",
            "label": "协议签订节点",
            "disabled": true,
            "width": "130px",
            "dict": {
              "dictType": "PD_E_PROTOCOL_NODE",
              "dictKey": [
                "PD_E_PROTOCOL_NODE"
              ],
              "format": "label"
            }
          },
          "protocol_take_type": {
            "control": "select",
            "label": "协议签订类型",
            "disabled": true,
            "width": "130px",
            "dict": {
              "dictType": "PD_E_PROTOCOL_TYPE",
              "dictKey": [
                "PD_E_PROTOCOL_TYPE"
              ],
              "format": "label"
            }
          },
          "effect_date": {
            "label": "生效日期",
            "disabled": true,
            "width": "130px",
            "control": "dateTimePicker",
            "editable": true,
            "format": "yyyyMMdd"
          }
        }
      ]
    }
  }
}
