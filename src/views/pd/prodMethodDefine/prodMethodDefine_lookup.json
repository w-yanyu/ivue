{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "method_type": {
            "control": "select",
            "label": "方法类别",
            "width": "120px",
            "span": 12,
            "dict": {
              "dictType": "PD_E_LHD_CHECK_METHOD_TYPE",
              "dictKey": [
                "PD_E_LHD_CHECK_METHOD_TYPE"
              ],
              "format": "label"
            }
          },
          "method_desc": {
            "control": "text",
            "label": "方法描述",
            "width": "120px",
            "span": 12
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pd1570"
        }
      },
      "thead": [
        {
          "type": "string",
          "label": "实现方法编号",
          "prop": "method_cd",
          "align": "center",
          "width": "200px"
        },
        {
          "type": "string",
          "label": "方法描述",
          "prop": "method_desc",
          "align": "center"
        },
        {
          "type": "string",
          "label": "方法类别",
          "prop": "method_type",
          "align": "center",
          "width": "150px",
          "dict": {
            "dictType": "PD_E_LHD_CHECK_METHOD_TYPE",
            "dictKey": [
              "PD_E_LHD_CHECK_METHOD_TYPE"
            ],
            "format": "label"
          }
        },
        {
          "type": "string",
          "label": "实现方法",
          "prop": "real_method",
          "align": "center"
        },
        {
          "label": "操作",
          "align": "center",
          "fixed": "right",
          "width": "200px",
          "btns": [
            {
              "type": "bringback",
              "label": "选择"
            }
          ]
        }
      ]
    }
  }
}
