{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "detail,format=pd0270,temp=pd0271",
          "pageTranType": "${{params.pageTranType}}",
          "sale_prod_cd": "${{params.sale_prod_cd}}",
          "prod_version": "${{params.prod_version}}",
          "apply_type": "${{params.apply_type}}"
        }
      },
      "thead": [
        {
          "label": "机构号",
          "prop": "branch_id",
          "control": "text",
          "align": "center"
        },
        {
          "label": "机构名称",
          "prop": "branch_name",
          "align": "center"
        }
      ]
    }
  }
}
