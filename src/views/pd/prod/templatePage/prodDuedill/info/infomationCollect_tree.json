{
  "module": "lr-tree-dynamic",
  "layout": {
    "tree": {
      "title": "尽职调查报告模板",
      "lazy": false,
      "idField": "node_cd",
      "pIdField": "parent_node_cd",
      "labelField": "node_name",
      "isLeafField": "is_leaf",
      "nodeTypeField": "node_type",
      "hideDynamicArea": true,
      "initRequest": {
        "url": "/SUMP/call/RPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pd1103",
          "sale_prod_cd": "${{params.prod_cd}}",
          "apply_type": "${{params.apply_type}}"
        }
      },
      "root": {
        "rootNode": {
          "name": "调查项",
          "value": "BASE",
          "disabled": true
        }
      }
    }
  }
}
