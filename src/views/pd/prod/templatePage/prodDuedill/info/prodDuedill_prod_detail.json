{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pd1101",
          "due_dili_template_no": "${{params.due_dili_template_no}}"
        }
      },
      "btns": [],
      "controls": [
        {
          "due_dili_template_no": {
            "control": "string",
            "label": "尽职调查模板编号",
            "width": "130px"
          },
          "due_dili_template_name": {
            "control": "string",
            "label": "尽职调查模板名称",
            "width": "130px"
          },
          "modify_path": {
            "control": "string",
            "label": "维护页面路径",
            "width": "130px"
          },
          "datail_path": {
            "control": "string",
            "label": "详情页面路径",
            "width": "130px"
          },
          "is_effective": {
            "control": "select",
            "label": "是否生效",
            "disabled": true,
            "width": "130px",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          }
        }
      ]
    }
  }
}
