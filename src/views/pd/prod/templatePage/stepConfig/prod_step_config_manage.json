{
  "module": "v_search_btn_datagrid",
  "layout": {
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "label": "新增",
          "toRequest": {
            "url": "/views/pd/prod/templatePage/stepConfig/prod_step_config_add.json",
            "method": "get",
            "params": {
              "sale_prod_cd": "${{params.sale_prod_cd}}",
              "prod_version": "${{params.prod_version}}",
              "apply_type": "${{params.apply_type}}"
            }
          },
          "props": {
            "title": "步骤配置信息新增",
            "width": "65%"
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "pd1529",
          "sale_prod_cd": "${{params.sale_prod_cd}}",
          "prod_version": "${{params.prod_version}}",
          "apply_type": "${{params.apply_type}}"
        }
      },
      "pagination": {
        "pageSizes": [
          10,
          15,
          20
        ],
        "pageSize": 10
      },
      "thead": [
        {
          "type": "index",
          "align": "center"
        },
        {
          "label": "步骤配置编号",
          "prop": "join_lend_config_no",
          "align": "center"
        },
        {
          "label": "处理表名",
          "prop": "pre_table_name",
          "align": "center"
        },
        {
          "label": "处理表注释",
          "prop": "table_name_desc",
          "align": "center"
        },
        {
          "label": "批量交易码",
          "prop": "tran_code",
          "align": "center"
        },
        {
          "label": "操作",
          "width": "150px",
          "align": "center",
          "fixed": "right",
          "btns": [
            {
              "type": "delete",
              "label": "删除",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "pd1528"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
