{
    "module": "v_search_btn_datagrid",
    "layout": {
      "datagrid": {
        "isInitSearch": true,
        "doRequest": {
          "url": "/SUMP/icmcall/icmRPCCall",
          "method": "post",
          "params": {
            "servicecode": "detail,format=pd1530,temp=pd1529",
            "pageTranType": "${{params.pageTranType}}",
            "sale_prod_cd": "${{params.sale_prod_cd}}",
            "prod_version": "${{params.prod_version}}",
            "apply_type": "${{params.apply_type}}"
          }
        },
        "pagination": {
          "pageSizes": [
            10,
            15,
            20
          ],
          "pageSize": 10
        },
        "thead": [
          {
            "type": "index",
            "align": "center"
          },
          {
            "label": "步骤配置编号",
            "prop": "join_lend_config_no",
            "align": "center"
          },
          {
            "label": "处理表名",
            "prop": "pre_table_name",
            "align": "center"
          },
          {
            "label": "处理表注释",
            "prop": "table_name_desc",
            "align": "center"
          },
          {
            "label": "批量交易码",
            "prop": "tran_code",
            "align": "center"
          }
        ]
      }
    }
  }
