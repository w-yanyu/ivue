{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "sale_prod_cd": {
            "control": "text",
            "label": "可售产品编号",
            "rules": [
              {
                "max": 32,
                "message": "长度不能超过32个字符"
              }
            ]
          }
        }
      ]
    },
    "toolbar": {
      "btns": [

      ]
    },
    "datagrid": {
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "detail,format=pd0604,temp=pd0601",
          "pageTranType": "${{params.pageTranType}}",
          "credit_prod_cd": "${{params.sale_prod_cd}}",
          "prod_version": "${{params.prod_version}}"
        }
      },
      "pagination": {
        "pageSizes": [
          10,
          15,
          20,
          50,
          100
        ],
        "pageSize": 10
      },
      "thead": [
        {
          "label": "产品编号",
          "prop": "credit_prod_cd",
          "width": "300px",
          "align": "center"
        },
        {
          "label": "阶段描述",
          "prop": "stage_desc",
          "align": "center"
        },
        {
          "label": "阶段序号",
          "prop": "stage_no",
          "width": "150px",
          "align": "center"
        },
        {
          "label": "阶段比例(%)",
          "prop": "stage_ratio",
          "width": "150px",
          "align": "center"
        }
      ]
    }
  }
}
