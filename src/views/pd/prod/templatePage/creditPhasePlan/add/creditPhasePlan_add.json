{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "pd0600"
        }
      },
      "controls": [
        {
          "servicecode": {
            "control": "hidden",
            "label": "新增交易码",
            "value": "pd0600"
          },
          "prod_version": {
            "control": "hidden",
            "label": "授信产品版本",
            "disabled": true,
            "value": "${{params.prod_version}}"
          },
          "credit_prod_cd": {
            "control": "hidden",
            "label": "授信产品编号",
            "disabled": true,
            "value": "${{params.credit_prod_cd}}"
          },
          "stage_desc": {
            "control": "text",
            "label": "阶段描述",
            "span": 8,
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          },
          "stage_no": {
            "control": "text",
            "label": "阶段序号",
            "span": 8,
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          },
          "stage_ratio": {
            "control": "text",
            "label": "阶段比例(%)",
            "span": 8,
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          }
        }
      ]
    }
  }
}