{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "pd0901",
          "sale_prod_cd": "${{params.sale_prod_cd}}",
          "prod_version": "${{params.prod_version}}"
        }
      },
      "btns": [],
      "controls": [
        {
          "servicecode": {
            "control": "hidden",
            "label": "新增交易码",
            "value": "pd0902"
          },
          "#prod_version": {
            "control": "hidden",
            "label": "授信产品版本",
            "disabled": true,
            "value": "${{params.prod_version}}"
          },
          "#sale_prod_cd": {
            "control": "hidden",
            "label": "可售产品编号",
            "span": 8
          },
          "bill_type": {
            "control": "select",
            "label": "票据类型",
            "span": 8,
            "dict": {
              "format": "label",
              "dictKey": [
                "PD_E_BILL_TYPE"
              ],
              "dictType": "PD_E_BILL_TYPE"
            },
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          },
          "default_currency": {
            "control": "select",
            "label": "默认币种",
            "span": 8,
            "dict": {
              "format": "label",
              "dictKey": [
                "MS_CURRENCY"
              ],
              "dictType": "MS_CURRENCY"
            }
          },
          "interest_rate": {
            "control": "text",
            "label": "利率",
            "span": 8
          },
          "guar_type": {
            "control": "select",
            "label": "担保方式",
            "span": 8,
            "dict": {
              "format": "label",
              "dictKey": [
                "MS_E_GUAR_TYPE"
              ],
              "dictType": "MS_E_GUAR_TYPE"
            }
          },
          "protocol_create_way": {
            "control": "select",
            "label": "中文协议号生成方式",
            "span": 8,
            "dict": {
              "format": "label",
              "dictKey": [
                "PD_E_PROTOCOL_WAY"
              ],
              "dictType": "PD_E_PROTOCOL_WAY"
            },
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          },
          "contract_create_way": {
            "control": "select",
            "label": "合同号生成阶段",
            "span": 8,
            "dict": {
              "format": "label",
              "dictKey": [
                "PD_E_CONTRACT_WAY"
              ],
              "dictType": "PD_E_CONTRACT_WAY"
            },
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          },
          "is_high_limit": {
            "control": "select",
            "label": "是否支持最高额担保",
            "span": 8,
            "dict": {
              "format": "label",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "dictType": "MS_E_YESORNO"
            }
          },
          "dicount_way": {
            "control": "select",
            "label": "贴现出票方式",
            "span": 8,
            "dict": {
              "format": "label",
              "dictKey": [
                "PD_E_DISCOUNT_TAKE"
              ],
              "dictType": "PD_E_DISCOUNT_TAKE"
            },
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          },
          "is_margin": {
            "control": "select",
            "label": "是否允许使用第三方保证金",
            "span": 8,
            "dict": {
              "format": "label",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "dictType": "MS_E_YESORNO"
            }
          },
          "discount_max_amt": {
            "control": "currency",
            "label": "贴现最大金额",
            "align":"left",
            "span": 8,
            "format":{
              "thousand":",",
              "decimal": 2
            }
          },
          "discount_min_amt": {
            "control": "currency",
            "label": "贴现最小金额",
            "align":"left",
            "span": 8,
            "format":{
              "thousand":",",
              "decimal": 2
            }
          },
          "min_deadline":{
            "control":"text",
            "label":"最小期限",
            "span":8
          },
          "max_deadline":{
            "control":"text",
            "label":"最大期限",
            "span":8
          },
          "highest_rate":{
            "control":"currency",
            "label":"最高利率",
            "align":"left",
            "span":8
          },
          "lowest_rate":{
            "control":"currency",
            "label":"最低利率",
            "align":"left",
            "span":8
          },
          "margin_percent":{
            "control":"currency",
            "label":"保证金比例不低于",
            "align":"left",
            "span":8
          }
        }
      ]
    }
  }
}
