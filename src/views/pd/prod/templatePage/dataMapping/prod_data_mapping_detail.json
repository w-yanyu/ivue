{
  "module": "v-search-editableDataGrid",
  "layout": {
    "datagrid": {
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pd1525",
          "sale_prod_cd": "${{params.sale_prod_cd}}",
          "prod_version": "${{params.prod_version}}",
          "mapping_table_name": "${{params.mapping_table_name}}"
        }
      },
      "thead": [
        {
          "prop": "mapping_field",
          "type": "text",
          "disabled": "true",
          "label": "映射字段",
          "validate": "required"
        },
        {
          "prop": "mapping_field_name",
          "type": "text",
          "disabled": "true",
          "label": "映射字段名称",
          "validate": "required"
        },
        {
          "prop": "default_value",
          "disabled": "true",
          "type": "text",
          "label": "默认值"
        }
      ]
    }
  }
}
