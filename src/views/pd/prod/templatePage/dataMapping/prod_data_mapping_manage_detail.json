{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "pd1538",
          "sale_prod_cd": "${{params.sale_prod_cd}}",
          "prod_version": "${{params.prod_version}}"
        }
      },
      "pagination": {
        "pageSizes": [
          10,
          15,
          20
        ],
        "pageSize": 10
      },
      "thead": [
        {
          "type": "index",
          "align": "center"
        },
        {
          "label": "映射表名",
          "prop": "mapping_table_name",
          "align": "center"
        },
        {
          "label": "映射表注释",
          "prop": "table_name_desc",
          "align": "center"
        },
        {
          "label": "详情",
          "width": "150px",
          "align": "center",
          "fixed": "right",
          "btns": [
            {
              "type": "look",
              "label": "详情",
              "toRequest": {
                "url": "/views/pd/prod/templatePage/dataMapping/prod_data_mapping_detail.json",
                "method": "get",
                "params": {
                  "sale_prod_cd": "${{params.sale_prod_cd}}",
                  "sale_prod_name": "${{params.sale_prod_name}}",
                  "prod_version": "${{params.prod_version}}"
                }
              },
              "props": {
                "title": "数据映射配置详情",
                "width": "80%"
              }
            }
          ]
        }
      ]
    }
  }
}
