{
  "module": "v_search_btn_datagrid",
  "layout": {
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "label": "新增",
          "icon": "el-icon-plus",
          "toRequest": {
            "url": "/views/pd/prod/templatePage/dataMapping/prod_data_mapping_modify_edit.json",
            "method": "GET",
            "params": {
              "sale_prod_cd": "${{params.sale_prod_cd}}",
              "sale_prod_name": "${{params.sale_prod_name}}",
              "prod_version": "${{params.prod_version}}",
              "mapping_table_name": "${{params.mapping_table_name}}",
              "table_name_desc": "${{params.table_name_desc}}"
            }
          }
        }
      ]
    },
    "datagrid": {
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pd1525",
          "sale_prod_cd": "${{params.sale_prod_cd}}",
          "prod_version": "${{params.prod_version}}",
          "mapping_table_name": "${{params.mapping_table_name}}"
        }
      },
      "thead": [
        {
          "prop": "mapping_field",
          "type": "text",
          "label": "映射字段",
          "rules": [
            {
              "required": true
            }
          ]
        },
        {
          "prop": "mapping_field_name",
          "type": "text",
          "label": "映射字段名称"
        },
        {
          "prop": "default_value",
          "type": "text",
          "label": "默认值"
        },
        {
          "label": "操作",
          "fixed": "right",
          "width": 150,
          "align": "center",
          "btns": [
            {
              "type": "add",
              "label": "维护",
              "toRequest": {
                "url": "/views/pd/prod/templatePage/dataMapping/prod_data_mapping_modify_edit.json",
                "method": "GET",
                "params": {

                }
              }
            },
            {
              "type": "delete",
              "label": "删除",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "pd1524"
                }
              },
              "confirm": {
                "title": "确定删除",
                "cancelButtonText": "取消",
                "confirmButtonText": "确定"
              }
            }
          ]
        }
      ]
    }
  }
}
