{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "pd1536"
        }
      },
      "pagination": {
        "show": false
      },
      "thead": [
        {
          "type": "index",
          "align": "center"
        },
        {
          "type": "leaf",
          "label": "映射表名",
          "align": "center",
          "prop": "mapping_table_name"
        },
        {
          "type": "leaf",
          "label": "映射表注释",
          "align": "center",
          "prop": "table_name_desc"
        },
        {
          "label": "操作",
          "width": "150px",
          "align": "center",
          "fixed": "right",
          "btns": [
            {
              "type": "custom",
              "label": "添加",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "post",
                "params": {
                  "servicecode": "pd1537",
                  "sale_prod_cd":"${{params.sale_prod_cd}}",
                  "sale_prod_name":"${{params.sale_prod_name}}",
                  "prod_version":"${{params.prod_version}}"
                }
              },
              "eventAfter": {
                "doParentCallback": true,
                "doLastCallback": true
              }
            }
          ]
        }
      ]
    }
  }
}
