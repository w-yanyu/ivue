{
  "module": "v_search_btn_datagrid",
  "layout": {
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "label": "新增",
          "toRequest": {
            "url": "/views/pd/prod/templatePage/dataMapping/prod_mapping_config_lookup.json",
            "method": "get",
            "params": {
              "sale_prod_cd": "${{params.sale_prod_cd}}",
              "prod_version": "${{params.prod_version}}"
            }
          },
          "props": {
            "title": "数据映射配置新增",
            "width": "65%"
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "pd1538",
          "sale_prod_cd": "${{params.sale_prod_cd}}",
          "prod_version": "${{params.prod_version}}"
        }
      },
      "pagination": {
        "pageSizes": [
          10,
          15,
          20
        ],
        "pageSize": 10
      },
      "thead": [
        {
          "type": "index",
          "align": "center"
        },
        {
          "label": "映射表名",
          "prop": "mapping_table_name",
          "align": "center"
        },
        {
          "label": "映射表注释",
          "prop": "table_name_desc",
          "align": "center"
        },
        {
          "label": "编辑",
          "width": "150px",
          "align": "center",
          "fixed": "right",
          "btns": [
            {
              "type": "look",
              "label": "维护",
              "toRequest": {
                "url": "/views/pd/prod/templatePage/dataMapping/prod_data_mapping_modify.json",
                "method": "get",
                "params": {
                  "sale_prod_cd": "${{params.sale_prod_cd}}",
                  "sale_prod_name": "${{params.sale_prod_name}}",
                  "prod_version": "${{params.prod_version}}"
                }
              },
              "props": {
                "title": "数据映射配置维护",
                "width": "80%"
              }
            },
            {
              "type": "delete",
              "label": "删除",
              "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "pd1539",
                  "sale_prod_cd": "${{params.sale_prod_cd}}",
                  "prod_version": "${{params.prod_version}}"
                }
              },
              "confirm": {
                "title": "提示信息",
                "msg": "该操作将删除当前映射数据。确认删除吗？",
                "confirmButtonText": "确定",
                "cancelButtonText": "取消"
              }
            }
          ]
        }
      ]
    }
  }
}
