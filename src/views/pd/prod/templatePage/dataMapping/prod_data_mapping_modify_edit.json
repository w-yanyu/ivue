{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pd1523",
          "sale_prod_cd": "${{params.sale_prod_cd}}",
          "sale_prod_name": "${{params.sale_prod_name}}",
          "prod_version": "${{params.prod_version}}",
          "mapping_table_name": "${{params.mapping_table_name}}",
          "table_name_desc": "${{params.table_name_desc}}"
        }
      },
      "controls": [
        {
          "join_lend_config_no": {
            "control": "hidden",
            "label": "贴源文件编码"
          },
          "mapping_field": {
            "control": "text",
            "label": "映射字段",
            "width": "120px",
            "span": 8,
            "rules": [
              {
                "required": true
              }
            ]
          },
          "mapping_field_name": {
            "control": "text",
            "label": "映射字段名称",
            "width": "120px",
            "span": 8,
            "rules": [
              {
                "required": true
              }
            ]
          },
          "default_value": {
            "control": "text",
            "label": "默认值",
            "width": "120px",
            "span": 8
          }
        }
      ]
    }
  }
}
