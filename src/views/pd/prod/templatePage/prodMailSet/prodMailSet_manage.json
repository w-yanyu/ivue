{
  "module": "v_search_btn_datagrid",
  "layout": {
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "target": "addlog",
          "label": "新增",
          "toRequest": {
            "method": "GET",
            "url": "/views/pd/prod/templatePage/prodMailSet/prodMailSet_add.json",
            "params": {
              "sale_prod_cd": "${{params.sale_prod_cd}}",
              "prod_version": "${{params.prod_version}}",
              "busi_seq": "${{params.busi_seq}}",
              "apply_type": "${{params.apply_type}}"
            }
          },
          "props": {
            "width": "60%",
            "title": "邮件/短信配置"
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pd0360",
          "sale_prod_cd": "${{params.sale_prod_cd}}",
          "prod_version": "${{params.prod_version}}",
          "apply_type": "${{params.apply_type}}"
        }
      },
      "thead": [
        {
          "type": "string",
          "label": "模板编号",
          "prop": "template_id",
          "align": "center"
        },
        {
          "type": "string",
          "label": "发送类型",
          "prop": "template_type",
          "align": "center",
          "dict": {
            "dictType": "MS_E_TEMPLATE_TYPE",
            "dictKey": [
              "MS_E_TEMPLATE_TYPE"
            ],
            "format": "label-value"
          }
        },
        {
          "type": "string",
          "label": "发送节点",
          "prop": "mail_send_node",
          "align": "center",
          "dict": {
            "dictType": "PD_E_PROTOCOL_NODE",
            "dictKey": [
              "PD_E_PROTOCOL_NODE"
            ],
            "format": "label-value"
          }
        },
        {
          "type": "string",
          "label": "是否生效",
          "prop": "is_effective",
          "align": "center",
          "dict": {
            "dictType": "MS_E_YESORNO",
            "dictKey": [
              "MS_E_YESORNO"
            ],
            "format": "label-value"
          }
        },
        {
          "type": "string",
          "label": "模板内容",
          "prop": "template_content",
          "align": "center",
          "btns": [
            {
              "type": "look",
              "toRequest": {
                "url": "@htmlRender",
                "params": {
                  "template_content": "${{current.template_content}}"
                }
              },
              "props": {
                "width": "45%",
                "title": "预览"
              }
            }
          ]
        },
        {
          "label": "操作",
          "fixed": "right",
          "width": "100px",
          "btns": [
            {
              "type": "edit",
              "label": "维护",
              "toRequest": {
                "url": "/views/pd/prod/templatePage/prodMailSet/prodMailSet_modify.json",
                "method": "GET"
              },
              "props": {
                "width": "65%",
                "title": "邮件/短信配置"
              }
            },
            {
              "type": "delete",
              "label": "删除",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "pd0363"
                }
              },
              "confirm": {
                "title": "确定删除",
                "cancelButtonText": "取消",
                "confirmButtonText": "确定"
              }
            }
          ]
        }
      ]
    }
  }
}
