            "span": 8,
            "disabled": true
          },
          "tran_chinese_name": {
            "control": "text",
            "label": "批量交易中文名称",
            "width": "120px",
            "span": 8,
            "disabled": true
          },
          "execution_code": {
            "control": "select",
            "label": "执行标识",
            "width": "120px",
            "span": 8,
            "disabled": true,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "reconnection_num": {
            "control": "text",
            "label": "重连次数",
            "width": "120px",
            "span": 8,
            "disabled": true
          },
          "fail_interrupt_code": {
            "control": "select",
            "label": "失败中断标识",
            "width": "120px",
            "span": 8,
            "disabled": true,
            "dict": {
              "dictType": "MS_E_YESORNO",
