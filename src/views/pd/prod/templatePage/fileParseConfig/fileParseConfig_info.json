{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "btns": [],
      "controls": [
        {
          "source_file_name": {
            "control": "string",
            "label": "贴源文件名",
            "width": "120px",
            "span": 8
          },
          "source_file_parse_way": {
            "control": "select",
            "label": "贴源文件解析方式",
            "width": "120px",
            "span": 8,
            "disabled": true,
            "dict": {
              "dictType": "PD_E_SOURCE_FILE_PASE_WAY",
              "dictKey": [
                "PD_E_SOURCE_FILE_PASE_WAY"
              ],
              "format": "label"
            }
          },
          "source_file_parse_content": {
            "control": "string",
            "label": "贴源文件解析方式内容",
            "width": "120px",
            "span": 8
          },
          "parse_step_no": {
            "control": "string",
            "label": "解析步骤号",
            "width": "120px",
            "span": 8
          },
          "post_source_file_desc": {
            "control": "textarea",
            "label": "贴源文件描述",
            "width": "120px",
            "span": 24,
            "rows": 4,
            "disabled": true
          },
          "source_file_local_path": {
            "control": "textarea",
            "label": "贴源本地存放路径",
            "width": "120px",
            "span": 24,
            "rows": 4,
            "disabled": true
          },
          "source_file_template": {
            "control": "textarea",
            "label": "贴源文件模板",
            "width": "120px",
            "span": 24,
            "rows": 4,
            "disabled": true
          }
        }
      ]
    }
  }
}
