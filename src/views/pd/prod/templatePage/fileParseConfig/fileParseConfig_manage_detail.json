{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "scope": "searchTable",
      "allData": true,
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "detail,format=pd1560,temp=pd1561",
          "pageTranType": "${{params.pageTranType}}",
          "sale_prod_cd": "${{params.sale_prod_cd}}",
          "prod_version": "${{params.prod_version}}"
        }
      },
      "thead": [
        {
          "type": "string",
          "label": "解析编号",
          "prop": "parse_no",
          "align": "center"
        },
        {
          "type": "string",
          "label": "贴源文件名",
          "prop": "source_file_name",
          "align": "center"
        },
        {
          "type": "string",
          "label": "贴源文件描述",
          "prop": "post_source_file_desc",
          "align": "center"
        },
        {
          "type": "string",
          "label": "贴源文件解析方式",
          "prop": "source_file_parse_way",
          "align": "center",
          "dict": {
            "dictType": "PD_E_SOURCE_FILE_PASE_WAY",
            "dictKey": [
              "PD_E_SOURCE_FILE_PASE_WAY"
            ],
            "format": "label"
          }
        },
        {
          "type": "text",
          "label": "贴源文件解析方式内容",
          "prop": "source_file_parse_content",
          "align": "center"
        },
        {
          "type": "string",
          "label": "贴源本地存放路径",
          "prop": "source_file_local_path",
          "align": "center"
        },
        {
          "type": "string",
          "label": "解析步骤号",
          "prop": "parse_step_no",
          "align": "center"
        },
        {
          "label": "操作",
          "align": "center",
          "fixed": "right",
          "width": "200px",
          "btns": [
            {
              "type": "look",
              "label": "详情",
              "toRequest": {
                "url": "/views/pd/prod/templatePage/fileParseConfig/fileParseConfig_info.json",
                "method": "GET",
                "params": {
                  "sale_prod_cd": "${{params.sale_prod_cd}}",
                  "prod_version": "${{params.prod_version}}",
                  "apply_type": "${{params.apply_type}}"
                }
              },
              "props": {
                "width": "60%",
                "title": "文件解析配置详情"
              }
            }
          ]
        }
      ]
    }
  }
}
