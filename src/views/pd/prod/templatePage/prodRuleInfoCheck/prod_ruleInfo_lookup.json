{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "rule_id": {
            "control": "text",
            "label": "规则编号"
          }
        }
      ]
    },
    "datagrid": {
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "pd0294"
        }
      },
      "pagination": {
        "pageSizes": [
          10,
          15,
          20
        ],
        "pageSize": 10
      },
      "thead": [
        {
          "type": "index",
          "align": "center"
        },
        {
          "type": "leaf",
          "label": "规则编号",
          "align": "center",
          "prop": "rule_id"
        },
        {
          "type": "leaf",
          "label": "规则代码",
          "align": "center",
          "prop": "rule_code"
        },
        {
          "label": "规则名称",
          "prop": "rule_name",
          "align": "center",
          "type": "leaf"
        },
        {
          "label": "规则类型",
          "prop": "rule_type",
          "align": "center",
          "type": "leaf",
          "dict": {
            "dictType": "PD_E_RULE_TYPE",
            "dictKey": [
              "PD_E_RULE_TYPE"
            ],
            "format": "label"
          }
        },
        {
          "label": "操作",
          "align": "center",
          "fixed": "right",
          "btns": [
            {
              "type": "bringback",
              "label": "选择"
            }
          ]
        }
      ]
    }
  }
}
