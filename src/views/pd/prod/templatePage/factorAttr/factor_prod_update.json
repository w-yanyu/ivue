{
    "module": "v_inputDataForm_btn",
    "layout": {
      "form": {
        "scope":"factorupd",
        "btns": [],
        "initRequest": {
          "url": "/SUMP/icmcall/icmRPCCall",
          "method": "POST",
          "params": {
            "servicecode": "pd0705",
            "sale_prod_cd": "${{params.sale_prod_cd}}",
            "prod_version": "${{params.prod_version}}"
          }
        },
       
        "controls": [
          {
          "servicecode": {
            "control":"hidden",
            "label": "交易码",
            "span": 8,
            "value": "pd0708"
          },
      
            "sale_prod_cd": {
              "control":"hidden",
              "label": "可售产品编号",
              "span": 8 
            },
            "prod_version": {
              "control": "hidden",
              "label": "产品版本",
              "span": 8
            },
            "busi_seq": {
              "control": "hidden",
              "label": "业务流水",
              "span": 8
            },
            "financ_type": {
              "control": "select",
              "label": "保理类型",
              "span": 8,
              "dict": {
                "dictType": "PD_E_FACTOR_TYPE",
                "dictKey": [
                  "PD_E_FACTOR_TYPE"
                ], 
                "format": "label"
              }
            },
            "financ_range": {
              "control": "select",
              "label": "保理范围",
              "span": 8,
              "dict": {
                "dictType": "PD_E_FACTOR_RANGE",
                "dictKey": [
                  "PD_E_FACTOR_RANGE"
                ],
                "format": "label"
              }
            },
            "default_rate": {
              "control": "currency",
              "label": "默认费率",
              "span": 8,
              "align":"left",
              "rules": [
                {
                  "required": true,
                  "message": "请输入"
                }
              ]
            },
            "factor_lowest_amt": {
              "control": "currency",
              "label": "保理最低金额",
              "align":"left",
              "span": 8,
              "format":{
                "thousand":",",
                "decimal": 2
              }
            },
            "factor_heighest_amt": {
              "control": "currency",
              "label": "保理最高金额",
              "align":"left",
              "span": 8,
              "format":{
                "thousand":",",
                "decimal": 2
              }
            },
            "max_exposure_amt": {
              "control": "currency",
              "label": "最高敞口金额",
              "align":"left",
              "span": 8,
              "format":{
                "thousand":",",
                "decimal": 2
              }
            },
            "min_deadline": {
              "control": "text",
              "label": "最小期限",
              "span": 8
            },
            "max_deadline": {
              "control": "text",
              "label": "最大期限",
              "span": 8
            },
            "min_grace_period": {
              "control": "text",
              "label": "最小宽期限",
              "span": 8
            },
            "max_grace_period": {
              "control": "text",
              "label": "最大宽期限",
              "span": 8
            },
            "min_rate": {
              "control": "currency",
              "label": "最低费率",
              "align":"left",
              "span": 8
            },
            "max_rate": {
              "control": "currency",
              "label": "最高费率",
              "align":"left",
              "span": 8
            },
            "financ_or_not": {
              "control": "select",
              "label": "是否融资保理",
              "span": 8,
              "dict": {
                "dictType": "MS_E_YESORNO",
                "dictKey": [
                  "MS_E_YESORNO"
                ],
                "format": "label"
              },
              "rules": [
                {
                  "required": true,
                  "message": "请输入"
                }
              ]
            },
            "max_financ_ratio": {
              "control": "currency",
              "label": "最高融资比例",
              "align":"left",
              "span": 8
            },
            "min_factor_financ_ratio": {
              "control": "currency",
              "label": "保理期最低融资利率浮动比例(%)",
              "align":"left",
              "span": 8
            },
            "max_factor_financ_ratio": {
              "control": "currency",
              "label": "保理期最高融资利率浮动比例(%)",
              "align":"left",
              "span": 8
            },
            "min_grace_financ_ratio": {
              "control": "currency",
              "label": "宽限期最低融资利率浮动比例(%)",
              "align":"left",
              "span": 8
            },
            "max_grace_financ_ratio": {
              "control": "currency",
              "label": "宽限期最高融资利率浮动比例(%)",
              "align":"left",
              "span": 8
            },
            "min_ovdue_ratio": {
              "control": "currency",
              "label": "最小逾期利率浮动比例(%)",
              "align":"left",
              "span": 8
            },
            "max_ovdue_ratio": {
              "control": "currency",
              "label": "最大逾期利率浮动比例(%)",
              "align":"left",
              "span": 8
            }
          }
        ]
      }
    }
  }