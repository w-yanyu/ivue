{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pd1516",
          "credit_prod_cd": "${{params.sale_prod_cd}}",
          "prod_version": "${{params.prod_version}}"
        }
      },
      "btns": [],
      "controls": [
        {
          "servicecode": {
            "control": "hidden",
            "label": "新增交易码",
            "value": "pd1515"
          },
          "prod_version": {
            "control": "hidden",
            "label": "授信产品版本"
          },
          "credit_prod_cd": {
            "control": "text",
            "width": "140px",
            "label": "授信产品编号",
            "span": 8,
            "disabled": true
          },
          "pre_credit_min_amt": {
            "label": "预授信最小金额(元)",
            "control": "currency",
            "align": "left",
            "format": {

              "thousand": ",",
              "decimal": 2
            },
            "span": 8,
            "width": "140px",
            "rules": [
              {
                "required": true,
                "message": "请输入预授信最小金额"
              }
            ]
          },
          "pre_credit_max_amt": {
            "label": "预授信最大金额(元)",
            "control": "currency",
            "align": "left",
            "format": {

              "thousand": ",",
              "decimal": 2
            },
            "span": 8,
            "width": "140px",
            "rules": [
              {
                "required": true,
                "message": "请输入预授信最大金额"
              }
            ]
          },
          "pre_credit_min_term": {
            "label": "预授信最小期限(月)",
            "control": "inputNumber",
            "span": 8,
            "width": "140px",
            "rules": [
              {
                "required": true,
                "message": "预授信最小期限(月)"
              }
            ]
          },

          "pre_credit_max_term": {
            "label": "预授信最大期限(月)",
            "control": "inputNumber",
            "span": 8,
            "width": "140px",
            "rules": [
              {
                "required": true,
                "message": "预授信最大期限(月)"
              }
            ]
          },
          "pre_credit_validity_days": {
            "label": "预授信有效期(天)",
            "control": "inputNumber",
            "span": 8,
            "width": "140px",
            "rules": [
              {
                "required": true,
                "message": "预授信有效期(天)"
              }
            ]
          },
          "pre_credit_mode": {
            "control": "select",
            "multiple": true,
            "span": 8,
            "width": "140px",
            "label": "支持预授信模式",
            "dict": {
              "dictType": "PD_E_PRE_CREDIT_MODE",
              "dictKey": [
                "PD_E_PRE_CREDIT_MODE"
              ],
              "format": "label-value"
            },
            "rules": [
              {
                "required": true,
                "message": "必填项"
              }
            ]
          }

        }
      ]
    }
  }
}
