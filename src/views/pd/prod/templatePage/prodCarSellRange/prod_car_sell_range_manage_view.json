{
  "module": "lr-tree-dynamic",
  "layout": {
    "tree": {
      "title": "车辆范围及优惠信息",
      "lazy": false,
      "idField": "tree_id",
      "pIdField": "tree_pid",
      "labelField": "tree_title",
      "isLeafField": "tree_is_left",
      "nodeTypeField": "tree_type",
      "expandId": [
        "-1"
      ],
      "initRequest": {
        "url": "/SUMP/call/RPCCall",
        "method": "POST",
        "params": {
          "sale_prod_cd": "${{params.sale_prod_cd}}",
          "prod_version": "${{params.prod_version}}",
          "servicecode": "detail,format=pd0625,temp=pd0622",
          "pageTranType": "${{params.pageTranType}}"
        }
      },
      "root": {
        "rootNode": {
          "name": "车辆信息",
          "value": "-1"
        }
      },
      "nodeTypes": {
        "carBrand": {
          "children": [
            "carBrand"
          ]
        },
        "carKind": {
          "children": [
            "carKind"
          ]
        },
        "carModel": {
          "children": [
            "carModel"
          ],
          "toRequest": {
            "url": "/views/pd/prod/templatePage/prodCarSellRange/car_sell_discount_view.json",
            "method": "post",
            "params": {
              "tree_id": "${{current.tree_id}}",
              "pageTranType": "${{params.pageTranType}}"
            }
          },
          "eventAfter": {
            "callback": "renderTree"
          }
        }
      }
    }
  }
}
