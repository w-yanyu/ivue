{
  "module": "v-selectableTree",
  "layout": {
    "tree": {
      "span": 24,
      "idField": "tree_id",
      "pIdField": "tree_pid",
      "labelField": "tree_title",
      "checkedField": "checked",
      "isLeafField": "tree_is_left",
      "disabledField": "disabled",
      "showCheckbox": true,
      "showRadio": false,
      "halfCheckedAlias": "halfCheckedList",
      "expandAllLevel": false,
      "expandLevel": 0,
      "expandCheckedLevel": true,
      "checkStrictly": false,
      "showSeach": true,
      "iconField": "icon",
      "submitKeyAlias": "checkedList",
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pd0620",
          "sale_prod_cd": "${{params.sale_prod_cd}}",
          "prod_version": "${{params.prod_version}}"
        }
      },
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pd0621",
          "sale_prod_cd": "${{params.sale_prod_cd}}",
          "prod_version": "${{params.prod_version}}"
        }
      }
    }
  }
}
