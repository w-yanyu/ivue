{
  "module": "lr_transferTable",
  "layout": {
    "transfer": {
      "height": "500px",
      "left": {
        "filterable": false,
        "title": "未关联合作项目",
        "btn": {
          "label": "选择",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "pd0616",
              "sale_prod_cd": "${{params.sale_prod_cd}}",
              "prod_version": "${{params.prod_version}}",
              "busi_seq": "${{params.busi_seq}}"
            }
          },
          "eventAfter": {
            "callback": "restCallBack"
          }
        },
        "datagrid": {
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "pd0618",
              "sale_prod_cd": "${{params.sale_prod_cd}}",
              "prod_version": "${{params.prod_version}}",
              "apply_type": "${{params.apply_type}}"
            }
          },
          "checked": "checked",
          "thead": [
            {
              "label": "序号",
              "type": "index",
              "align": "center"
            },
            {
              "label": "合作项目编号",
              "prop": "part_project_no",
              "align": "center"
            },
            {
              "label": "合作项目名称",
              "prop": "part_project_name",
              "align": "center",
              "width": "180px"
            },
            {
              "label": "合作项目描述",
              "prop": "part_project_describe",
              "align": "center",
              "width": "180px"
            }
          ]
        }
      },
      "right": {
        "filterable": false,
        "title": "已关联合作项目",
        "btn": {
          "label": "取消关联",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "pd0617",
              "sale_prod_cd": "${{params.sale_prod_cd}}",
              "prod_version": "${{params.prod_version}}",
              "busi_seq": "${{params.busi_seq}}"
            }
          },
          "eventAfter": {
            "callback": "restCallBack"
          }
        },
        "datagrid": {
          "scope": "left",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "pd0615",
              "sale_prod_cd": "${{params.sale_prod_cd}}",
              "prod_version": "${{params.prod_version}}"
            }
          },
          "thead": [
            {
              "label": "序号",
              "type": "index",
              "align": "center"
            },
            {
              "label": "合作项目编号",
              "prop": "part_project_no",
              "align": "center"
            },
            {
              "label": "合作项目名称",
              "prop": "part_project_name",
              "align": "center",
              "width": "180px"
            },
            {
              "label": "合作项目版本",
              "prop": "part_project_version",
              "align": "center",
              "width": "180px"
            },
            {
              "label": "合作项目描述",
              "prop": "part_project_describe",
              "align": "center",
              "width": "180px"
            },
            {
              "label": "操作",
              "fixed": "right",
              "width": 100,
              "align": "center",
              "btns": [
                {
                  "type": "custom",
                  "label": "详情",
                  "toRequest": {
                    "url": "@projectAddLayout",
                    "method": "GET",
                    "params": {
                      "tranType": "detail",
                      "part_project_no": "${{current.part_project_no}}",
                      "project_template_no": "${{current.project_template_no}}",
                      "version": "${{current.part_project_version}}"
                    }
                  }
                }
              ]
            }
          ]
        }
      }
    }
  }
}
