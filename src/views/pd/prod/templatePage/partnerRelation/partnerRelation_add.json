{
  "module": "v_form_btn_dynamic",
  "layout": {
    "form": {
      "scope": "formData",
      "formSetTitle": "合作项目关联",
      "controls": [
        {
          "top": {
            "control": "dynamic",
            "span": 24,
            "toRequest": {
              "url": "/views/pd/prod/templatePage/partnerRelation/partnerRelation_group.json",
              "method": "get",
              "params": {
                "sale_prod_cd": "${{params.sale_prod_cd}}",
                "prod_version": "${{params.prod_version}}",
                "busi_seq": "${{params.busi_seq}}",
                "apply_type": "${{params.apply_type}}"
              }
            }
          },
          "bottom": {
            "control": "dynamic",
            "span": 24,
            "toRequest": {
              "url": "/views/pd/prod/templatePage/partnerRelation/bottomDescription.json",
              "method": "get",
              "params": {
                "sale_prod_cd": "${{params.sale_prod_cd}}",
                "prod_version": "${{params.prod_version}}",
                "busi_seq": "${{params.busi_seq}}",
                "apply_type": "${{params.apply_type}}"
              }
            }
          }
        }
      ]
    }
  }
}
