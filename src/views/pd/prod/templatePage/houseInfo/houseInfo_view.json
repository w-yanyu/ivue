{
  "module": "v-form-btn-dynamic",
  "layout": {
    "form": {
      "scope": "detail",
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ar1600",
          "apply_no": "${{params.apply_no}}"
        }
      },
      "controls": [
        {
          "house_cert_no": {
            "control": "text",
            "disabled": true,
            "label": "房产证号码",
            "span": 8
          },
          "house_id": {
            "control": "text",
            "disabled": true,
            "label": "楼盘编号",
            "span": 8
          },
          "area_msg": {
            "control": "text",
            "disabled": true,
            "label": "建筑面积信息",
            "span": 8
          },
          "house_name": {
            "control": "text",
            "disabled": true,
            "label": "楼盘名称",
            "span": 8
          },
          "is_first_hand": {
            "control": "select",
            "disabled": true,
            "label": "是否一手房",
            "span": 8,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "is_refur": {
            "control": "select",
            "disabled": true,
            "label": "是否翻新",
            "span": 8,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "longitude": {
            "disabled": true,
            "control": "text",
            "label": "基准经度",
            "span": 8
          },
          "latitude": {
            "disabled": true,
            "control": "text",
            "label": "基准纬度",
            "span": 8
          },
          "house_addr": {
            "disabled": true,
            "control": "textarea",
            "label": "房屋地址",
            "span": 20
          }
        }
      ]
    },
    "toolbar": {
      "btns": []
    }
  }
}
