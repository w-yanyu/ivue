{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [      
      ],
      "btns": [
      ]
    },
    "toolbar": {
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ap0000",
          "parm_tab_nm": "klnp_extd_rule",
          "dtbs_oprn_flg": "3",
          "data_pkg": "${{current}}"
        }
      },
      "thead": [
        {
          "label": "操作",
          "fixed": "left",
          "width": 150,
          "align": "center",
          "btns": [
            {
              "type": "bringback",
              "label": "选择"
            },
            {
              "type": "look",
              "label": "查看分层参数",
              "toRequest": {
                "url": "/views/pd/prod/templatePage/extendattr/klnp_extd_rule_layer_info.json",
                "method": "POST",
                "params": {
                  "extd_rule_num": "${{params.extd_rule_num}}"
                }
              },
              "props": {
                "width": "50%",
                "title": "分层参数详情"
              }
            }
          ]
        },
        {
          "label": "展期规则编号",
          "prop": "extd_rule_num",
          "align": "center"
        },
        {
          "label": "展期规则描述",
          "prop": "extd_rule_dsc",
          "align": "center"
        },
        {
          "label": "基准利率取值规则",
          "prop": "bchmk_int_rate_val_rule",
          "align": "center",
          "dict": {
            "dictType": "LN_E_BCHMK_INT_RATE_VAL_RULE",
            "dictKey": [
              "LN_E_BCHMK_INT_RATE_VAL_RULE"
            ],
            "format": "label"
          }
        },

        {
          "label": "展期利率浮动控制",
          "prop": "extd_int_rate_flt_ctrl",
          "dict": {
            "dictType": "LN_E_EXTD_INT_RATE_FLT_CTRL",
            "dictKey": [
              "LN_E_EXTD_INT_RATE_FLT_CTRL"
            ],
            "format": "label"
          },
          "align": "center"
        },
        {
          "label": "展期利率浮动值控制",
          "prop": "extd_int_rate_flt_val_ctrl",
          "dict": {
            "dictType": "LN_E_EXTD_INT_RATE_FLT_VAL_CTRL",
            "dictKey": [
              "LN_E_EXTD_INT_RATE_FLT_VAL_CTRL"
            ],
            "format": "label"
          },
          "align": "center"
        },
        {
          "label": "序号",
          "prop": "serl_num_long",
          "align": "center"
        }

      ]
    }
  }
}
