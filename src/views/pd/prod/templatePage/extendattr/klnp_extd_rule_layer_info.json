{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {            
      "initRequest": "${{params}}",
      "controls": [  
            
      ],
      "btns": [
      ]
    },
    "toolbar": {
     
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ap0000",
          "parm_tab_nm": "klnp_extd_rule_layer",
          "dtbs_oprn_flg": "3",
          "data_pkg": "${{params.extd_rule_num}}"
        }
      },
      "thead": [
        {
          "label": "展期规则编号",
          "prop": "extd_rule_num",
          "align": "center"
        },
        {
          "label": "数值序号",
          "prop": "numrcl_val_serl_num",
          "align": "center"
        },
        {
          "label": "原业务贷款期限上限(月)",
          "prop": "extd_rule_matu",
          "align": "center"
        },

        {
          "label": "展期控制方式",
          "prop": "extd_ctrl_way",
          "dict": {
            "dictType": "LN_E_EXTD_CTRL_WAY",
            "dictKey": [
              "LN_E_EXTD_CTRL_WAY"
            ],
            "format": "label"
          },
          "align": "center"
        },
        {
          "label": "期限控制种类",
          "prop": "matu_ctrl_catg",
          "dict": {
            "dictType": "LN_E_MATU_CTRL_CATG",
            "dictKey": [
              "LN_E_MATU_CTRL_CATG"
            ],
            "format": "label"
          },
          "align": "center"
        },
        {
          "label": "期限控制值",
          "prop": "extd_ctrl_matu",
          "align": "center"
        },
        {
          "label": "展期最大期限(月)",
          "prop": "extd_max_matu",
          "align": "center"
        }
      ]
    }
  }
}
