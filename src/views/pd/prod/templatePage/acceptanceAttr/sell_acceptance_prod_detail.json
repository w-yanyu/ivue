{
    "module": "v_inputDataForm_btn",
    "layout": {
      "form": {
        "scope":"acceptancesel",
        "initRequest": {
          "url": "/SUMP/icmcall/icmRPCCall",
          "method": "POST",
          "params": {
            "servicecode": "detail,format=pd0801,temp=pd0805",
            "pageTranType": "${{params.pageTranType}}",
            "sale_prod_cd": "${{params.sale_prod_cd}}",
            "prod_version": "${{params.prod_version}}"
          }
        },
        "btns": [],
        "controls": [
          {
            "sale_prod_cd": {
              "control":"hidden",
              "label": "可售产品编号",
              "span": 8 ,
              "disabled":true
            },
            "prod_version": {
              "control": "hidden",
              "label": "产品版本",
              "span": 8,
              "disabled":true
            },
            "busi_seq": {
              "control": "hidden",
              "label": "业务流水",
              "span": 8,
              "disabled":true
            },
            "acceptance_type": {
              "control": "select",
              "label": "承兑业务种类",
              "span": 8,
              "dict": {
                "dictType": "PD_E_ACCEPTANCE_TYPE",
                "dictKey": [
                  "PD_E_ACCEPTANCE_TYPE"
                ],
                "format": "label"
              },
              "disabled":true
            },
            "is_full_deposit": {
              "control": "select",
              "label": "是否足额保证金",
              "span": 8,
              "dict": {
                "dictType": "MS_E_YESORNO",
                "dictKey": [
                  "MS_E_YESORNO"
                ],
                "format": "label"
              },
              "disabled":true
            },
            "draft_properties": {
                "control": "select",
                "label": "承兑汇票属性",
                "span": 8,
                "dict": {
                  "dictType": "PD_E_DRAFT_PROP",
                  "dictKey": [
                    "PD_E_DRAFT_PROP"
                  ],
                  "format": "label"
                },
                "disabled":true
              },
              "is_need_contract": {
                "control": "select",
                "label": "是否需要交易合同",
                "span": 8,
                "dict": {
                  "dictType": "MS_E_YESORNO",
                  "dictKey": [
                    "MS_E_YESORNO"
                  ],
                  "format": "label"
                },
                "disabled":true
              },
              "appr_valid_date": {
                "control": "text",
                "label": "审批有效期",
                "span": 8,
                "disabled":true
              },
              "is_financial_institutions": {
                "control": "select",
                "label": "是否代其他金融机构签发",
                "span": 8,
                "dict": {
                  "dictType": "MS_E_YESORNO",
                  "dictKey": [
                    "MS_E_YESORNO"
                  ],
                  "format": "label"
                },
                "disabled":true
              },
              "bank_type": {
                "control": "select",
                "label": "金融机构类别",
                "span": 8,
                "dict": {
                  "dictType": "PD_E_BRANCH_TYPE",
                  "dictKey": [
                    "PD_E_BRANCH_TYPE"
                  ],
                  "format": "label"
                },
                "disabled":true
              },
              "ticketing_way": {
                "control": "select",
                "label": "出票方式",
                "span": 8,
                "dict": {
                  "dictType": "PD_E_TICKETING_TYPE",
                  "dictKey": [
                    "PD_E_TICKETING_TYPE"
                  ],
                  "format": "label"
                },
                "disabled":true
              },
              "is_ticket_in_balance": {
                "control": "select",
                "label": "到期未兑付的汇票是否计入余额 ",
                "span": 8,
                "dict": {
                  "dictType": "MS_E_YESORNO",
                  "dictKey": [
                    "MS_E_YESORNO"
                  ],
                  "format": "label"
                },
                "disabled":true
              },
              "is_take_ticket": {
                "control": "select",
                "label": "是否生成票据",
                "span": 8,
                "dict": {
                  "dictType": "MS_E_YESORNO",
                  "dictKey": [
                    "MS_E_YESORNO"
                  ],
                  "format": "label"
                },
                "disabled":true
              },
              "is_print_draft_ticket": {
                "control": "select",
                "label": "是否打印承兑协议",
                "span": 8,
                "dict": {
                  "dictType": "MS_E_YESORNO",
                  "dictKey": [
                    "MS_E_YESORNO"
                  ],
                  "format": "label"
                },
                "disabled":true
              },
              "default_advance_rate": {
                "control": "currency",
                "label": "默认垫款利率",
                "align":"left",
                "span": 8,
                "format":{
                  "thousand":",",
                  "decimal": 2
                },
                "disabled":true
              },
              "default_rate": {
                "control": "currency",
                "label": "默认费率",
                "align":"left",
                "span": 8,
                "format":{
                  "thousand":",",
                  "decimal": 2
                },
                "disabled":true
              },
              "is_colt": {
                "control": "select",
                "label": "是否需要抵质押品",
                "span": 8,
                "dict": {
                  "dictType": "MS_E_YESORNO",
                  "dictKey": [
                    "MS_E_YESORNO"
                  ],
                  "format": "label"
                },
                "disabled":true
              },
            "specify_colt_type": {
              "control": "remoteSelect",
              "label": "指定抵质押品类型",
              "disabled":true,
              "span": 8,
              "align": "center",
              "labelField": "colt_name",
              "valueField": "colt_type",
              "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "co1012"
                }
              }
            },
            "is_vote": {
                "control": "select",
                "label": "折票标志",
                "span": 8,
                "dict": {
                  "dictType": "MS_E_YESORNO",
                  "dictKey": [
                    "MS_E_YESORNO"
                  ],
                  "format": "label"
                },
                "disabled":true
              },
              "is_combined_ticket": {
                "control": "select",
                "label": "合票标准",
                "span": 8,
                "dict": {
                  "dictType": "MS_E_YESORNO",
                  "dictKey": [
                    "MS_E_YESORNO"
                  ],
                  "format": "label"
                },
                "disabled":true
              },
              "is_high_limit": {
                "control": "select",
                "label": "是否支持最高额担保 ",
                "span": 8,
                "dict": {
                  "dictType": "MS_E_YESORNO",
                  "dictKey": [
                    "MS_E_YESORNO"
                  ],
                  "format": "label"
                },
                "disabled":true
              },
            "credit_grace_days": {
              "control": "text",
              "label": "授信宽限天数",
              "span": 8,
              "disabled":true
            },
            "min_appr_amt":{
              "control":"currency",
              "label":"最低申请金额",
              "align":"left",
              "span":8,
              "format":{
                "thousand":",",
                "decimal": 2
              },
              "disabled":true
            },
            "max_appr_amt":{
              "control":"currency",
              "label":"最高申请金额",
              "align":"left",
              "span":8,
              "format":{
                "thousand":",",
                "decimal": 2
              },
              "disabled":true
            },
            "max_exposure_amt":{
              "control":"currency",
              "label":"最高敞口金额",
              "align":"left",
              "span":8,
              "format":{
                "thousand":",",
                "decimal": 2
              },
              "disabled":true
            },
            "margin_percent":{
              "control":"currency",
              "label":"首付保证金比例不低于",
              "align":"left",
              "span":8,
              "format":{
                "thousand":",",
                "decimal": 2
              },
              "disabled":true
            },
            "min_deadline":{
              "control":"text",
              "label":"最小期限",
              "span":8,
              "suffix": "M",
              "disabled":true
            },
            "max_deadline":{
              "control":"text",
              "label":"最大期限",
              "span":8,
              "suffix": "M",
              "disabled":true
            },
            "lowest_advance_rate":{
              "control":"currency",
              "label":"最低垫款利率",
              "align":"left",
              "span":8,
              "format":{
                "thousand":",",
                "decimal": 2
              },
              "disabled":true
            },
            "heighest_advance_rate":{
              "control":"currency",
              "label":"最高垫款利率",
              "align":"left",
              "span":8,
              "format":{
                "thousand":",",
                "decimal": 2
              },
              "disabled":true
            },
            "min_rate":{
              "control":"currency",
              "label":"最低费率",
              "align":"left",
              "span":8,
              "format":{
                "thousand":",",
                "decimal": 2
              },
              "disabled":true
            },
            "max_rate":{
              "control":"currency",
              "label":"最高费率",
              "align":"left",
              "span":8,
              "format":{
                "thousand":",",
                "decimal": 2
              },
              "disabled":true
            }
          }
        ]
      }
    }
  }
