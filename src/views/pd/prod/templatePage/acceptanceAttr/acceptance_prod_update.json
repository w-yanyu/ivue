{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "scope": "acceptanceupd",
      "btns": [],
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pd0805",
          "sale_prod_cd": "${{params.sale_prod_cd}}",
          "prod_version": "${{params.prod_version}}"
        }
      },
      "controls": [
        {
          "servicecode": {
            "control": "hidden",
            "label": "交易码",
            "span": 8,
            "value": "pd0808"
          },
          "sale_prod_cd": {
            "control": "hidden",
            "label": "可售产品编号",
            "span": 8
          },
          "prod_version": {
            "control": "hidden",
            "label": "产品版本",
            "span": 8
          },
          "busi_seq": {
            "control": "hidden",
            "label": "业务流水",
            "span": 8
          },
          "acceptance_type": {
            "control": "select",
            "label": "承兑业务种类",
            "span": 8,
            "dict": {
              "dictType": "PD_E_ACCEPTANCE_TYPE",
              "dictKey": [
                "PD_E_ACCEPTANCE_TYPE"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          },
          "is_full_deposit": {
            "control": "select",
            "label": "是否足额保证金",
            "span": 8,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          },
          "draft_properties": {
            "control": "select",
            "label": "承兑汇票属性",
            "span": 8,
            "dict": {
              "dictType": "PD_E_DRAFT_PROP",
              "dictKey": [
                "PD_E_DRAFT_PROP"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          },
          "is_need_contract": {
            "control": "select",
            "label": "是否需要交易合同",
            "span": 8,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "appr_valid_date": {
            "control": "text",
            "label": "审批有效期",
            "suffix": "M",
            "span": 8
          },
          "is_financial_institutions": {
            "control": "select",
            "label": "是否代其他金融机构签发",
            "span": 8,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          },
          "bank_type": {
            "control": "select",
            "label": "金融机构类别",
            "span": 8,
            "dict": {
              "dictType": "PD_E_BRANCH_TYPE",
              "dictKey": [
                "PD_E_BRANCH_TYPE"
              ],
              "format": "label"
            }
          },
          "ticketing_way": {
            "control": "select",
            "label": "出票方式",
            "span": 8,
            "dict": {
              "dictType": "PD_E_TICKETING_TYPE",
              "dictKey": [
                "PD_E_TICKETING_TYPE"
              ],
              "format": "label"
            }
          },
          "is_ticket_in_balance": {
            "control": "select",
            "label": "到期未兑付的汇票是否计入余额 ",
            "span": 8,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          },
          "is_take_ticket": {
            "control": "select",
            "label": "是否生成票据",
            "span": 8,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          },
          "is_print_draft_ticket": {
            "control": "select",
            "label": "是否打印承兑协议",
            "span": 8,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          },
          "default_advance_rate": {
            "control": "currency",
            "label": "默认垫款利率",
            "align": "left",
            "span": 8,
            "format":{
              "thousand":",",
              "decimal": 2
            }
          },
          "default_rate": {
            "control": "currency",
            "label": "默认费率",
            "align": "left",
            "span": 8,
            "format":{
              "thousand":",",
              "decimal": 2
            }
          },
          "is_colt": {
            "control": "select",
            "label": "是否需要抵质押品",
            "span": 8,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "specify_colt_type": {
            "control": "remoteSelect",
            "label": "指定抵质押品类型",
            "span": 8,
            "align": "center",
            "labelField": "colt_name",
            "valueField": "colt_type",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "servicecode": "co1012"
              }
            }
          },
          "is_vote": {
            "control": "select",
            "label": "折票标志",
            "span": 8,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          },
          "is_combined_ticket": {
            "control": "select",
            "label": "合票标准",
            "span": 8,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          },
          "is_high_limit": {
            "control": "select",
            "label": "是否支持最高额担保 ",
            "span": 8,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "credit_grace_days": {
            "control": "text",
            "label": "授信宽限天数",
            "span": 8,
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          },
          "min_appr_amt": {
            "control": "currency",
            "label": "最低申请金额",
            "span": 8,
            "align": "left",
            "format": {
              "thousand": ",",
              "decimal": 2
            }
          },
          "max_appr_amt": {
            "control": "currency",
            "label": "最高申请金额",
            "span": 8,
            "align": "left",
            "format": {
              "thousand": ",",
              "decimal": 2
            }
          },
          "max_exposure_amt": {
            "control": "currency",
            "label": "最高敞口金额",
            "span": 8,
            "align": "left",
            "format": {
              "thousand": ",",
              "decimal": 2
            }
          },
          "margin_percent": {
            "control": "currency",
            "label": "首付保证金比例不低于",
            "align": "left",
            "span": 8,
            "format":{
              "thousand":",",
              "decimal": 2
            }
          },
          "min_deadline": {
            "control": "text",
            "label": "最小期限",
            "suffix": "M",
            "span": 8
          },
          "max_deadline": {
            "control": "text",
            "label": "最大期限",
            "suffix": "M",
            "span": 8
          },
          "lowest_advance_rate": {
            "control": "currency",
            "label": "最低垫款利率",
            "align": "left",
            "span": 8,
            "format": {
              "thousand": ",",
              "decimal": 2
            }
          },
          "heighest_advance_rate": {
            "control": "currency",
            "label": "最高垫款利率",
            "align": "left",
            "span": 8,
            "format": {
              "thousand": ",",
              "decimal": 2
            }
          },
          "min_rate": {
            "control": "currency",
            "label": "最低费率",
            "align": "left",
            "span": 8,
            "format": {
              "thousand": ",",
              "decimal": 2
            }
          },
          "max_rate": {
            "control": "currency",
            "label": "最高费率",
            "align": "left",
            "span": 8,
            "format": {
              "thousand": ",",
              "decimal": 2
            }
          }
        }
      ]
    }
  }
}
