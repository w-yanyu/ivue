{
    "module": "v_inputDataForm_btn",
    "layout": {
      "form": {
        "initRequest": {
          "url": "/SUMP/icmcall/icmRPCCall",
          "method": "POST",
          "params": {
            "servicecode": "detail,format=pd0434,temp=pd0430",
            "pageTranType": "${{params.pageTranType}}",
            "sale_prod_cd": "${{params.sale_prod_cd}}",
            "prod_version": "${{params.prod_version}}"
          }
        },
        "btns": [],
        "controls": [
          {
            "servicecode": {
              "control": "hidden",
              "label": "临时表交易码",
              "value": "pd0430"
            },
            "template_id": {
              "control": "hidden",
              "label": "模板编号",
              "value": "${{params.template_id}}"
            },
            "prod_version": {
              "control": "hidden",
              "label": "可售产品版本",
              "value": "${{params.prod_version}}"
            },
            "busi_seq": {
              "control": "hidden",
              "label": "业务流水",
              "value": "${{params.busi_seq}}"
            },
            "sale_prod_cd": {
              "control": "hidden",
              "label": "可售产品编号",
              "value": "${{params.sale_prod_cd}}"
            },
            "sale_prod_name": {
              "control": "hidden",
              "label": "可售产品名称",
              "value": "${{params.sale_prod_name}}"
            },
            "default_currency": {
                "control": "select",
                "disabled": true,
                "label": "默认币种",
                "span": 8,
                "dict": {
                    "dictType": "MS_CURRENCY",
                    "dictKey": [
                    "MS_CURRENCY"
                    ],
                    "labelField": "dictName",
                    "valueField": "dictId",
                    "format": "label"
                },
                "rules": [
                    {
                    "required": false,
                    "message": "默认币种不得为空"
                    }
                ]
            },
            "is_colt": {
                "control": "select",
                "disabled": true,
                "label": "是否需要抵质押品",
                "span": 8,
                "dict": {
                  "dictType": "MS_E_YESORNO",
                  "dictKey": [
                    "MS_E_YESORNO"
                  ],
                  "labelField": "dictName",
                  "valueField": "dictId",
                  "format": "label"
                }
            },
            "is_credit_quota": {
              "control": "select",
              "disabled": true,
              "label": "是否需要授信额度",
              "span": 8,
              "dict": {
                "dictType": "MS_E_YESORNO",
                "dictKey": [
                  "MS_E_YESORNO"
                ],
                "labelField": "dictName",
                "valueField": "dictId",
                "format": "label"
              }
            },
            "appr_valid_date": {
                "control": "string",
                "label": "审批有效期",
                "span": 8,
                "value": "${{params.appr_valid_date}}"
            },
            "is_high_limit": {
                "control": "select",
                "disabled": true,
                "label": "是否支持最高额担保",
                "span": 8,
                "dict": {
                  "dictType": "MS_E_YESORNO",
                  "dictKey": [
                    "MS_E_YESORNO"
                  ],
                  "labelField": "dictName",
                  "valueField": "dictId",
                  "format": "label"
                }
              },
              "is_return_repy_info": {
                "control": "select",
                "disabled": true,
                "label": "是否需要回补还款信息",
                "span": 8,
                "dict": {
                  "dictType": "MS_E_YESORNO",
                  "dictKey": [
                    "MS_E_YESORNO"
                  ],
                  "labelField": "dictName",
                  "valueField": "dictId",
                  "format": "label"
                }
              },
              "is_inter_busi": {
                "control": "select",
                "disabled": true,
                "label": "是否连通国际业务接口",
                "span": 8,
                "dict": {
                  "dictType": "MS_E_YESORNO",
                  "dictKey": [
                    "MS_E_YESORNO"
                  ],
                  "labelField": "dictName",
                  "valueField": "dictId",
                  "format": "label"
                }
              },
              "is_trade_contract": {
                "control": "select",
                "disabled": true,
                "label": "是否需要贸易合同",
                "span": 8,
                "dict": {
                  "dictType": "MS_E_YESORNO",
                  "dictKey": [
                    "MS_E_YESORNO"
                  ],
                  "labelField": "dictName",
                  "valueField": "dictId",
                  "format": "label"
                }
              },
              "min_appr_amt": {
                "control": "currency",
                "align": "left",
                "format": {
                  "symbol": "￥",
                  "thousand": ",",
                  "decimal": 2
                },
                "disabled": true,
                "label": "最低申请金额",
                "span": 8
              },
              "max_appr_amt": {
                "control": "currency",
                "align": "left",
                "format": {
                  "symbol": "￥",
                  "thousand": ",",
                  "decimal": 2
                },
                "disabled": true,
                "label": "最高申请金额",
                "span": 8
              },
              "min_exposure_amt": {
                "control": "currency",
                "align": "left",
                "format": {
                  "symbol": "￥",
                  "thousand": ",",
                  "decimal": 2
                },
                "disabled": true,
                "label": "最低敞口金额",
                "span": 8
              },
              "max_exposure_amt": {
                "control": "currency",
                "align": "left",
                "format": {
                  "symbol": "￥",
                  "thousand": ",",
                  "decimal": 2
                },
                "disabled": true,
                "label": "最高敞口金额",
                "span": 8
              },
              "min_interest_rate": {
                "control": "string",
                "label": "最低利率",
                "span": 8,
                "suffix": "年"
              },
              "max_interest_rate": {
                "control": "string",
                "label": "最高利率",
                "span": 8,
                "suffix": "年"
              },
              "margin_percent": {
                "control": "string",
                "label": "首付保证金比例不低于",
                "span": 8
              },
              "min_deadline": {
                "control": "string",
                "label": "最小期限",
                "span": 8,
                "suffix": "月"
              },
              "max_deadline": {
                "control": "string",
                "label": "最大期限",
                "span": 8,
                "suffix": "月"
              }
          }
        ]
      }
    }
  }
