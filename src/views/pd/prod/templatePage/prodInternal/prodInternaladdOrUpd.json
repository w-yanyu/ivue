{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pd0430",
          "sale_prod_cd": "${{params.sale_prod_cd}}",
          "prod_version": "${{params.prod_version}}"
        }
      },
      "btns": [],
      "controls": [
        {
          "servicecode": {
            "control": "hidden",
            "label": "临时表交易码",
            "value": "pd0431"
          },
          "template_id": {
            "control": "hidden",
            "label": "模板编号",
            "value": "${{params.template_id}}"
          },
          "prod_version": {
            "control": "hidden",
            "label": "可售产品版本",
            "value": "${{params.prod_version}}"
          }
        },
        {
          "busi_seq": {
            "control": "hidden",
            "label": "业务流水",
            "value": "${{params.busi_seq}}"
          },
          "sale_prod_cd": {
            "control": "hidden",
            "label": "可售产品编号",
            "value": "${{params.sale_prod_cd}}"
          },
          "sale_prod_name": {
            "control": "hidden",
            "label": "可售产品名称",
            "value": "${{params.sale_prod_name}}"
          }
        },
        {
          "default_currency": {
            "control": "select",
            "value": "156",
            "label": "默认币种",
            "span": 8,
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": [
                "MS_CURRENCY"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": false,
                "message": "默认币种不得为空"
              }
            ]
          },
          "is_colt": {
            "control": "select",
            "value": "${{params.is_colt}}",
            "label": "是否需要抵质押品",
            "span": 8,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "is_credit_quota": {
            "control": "select",
            "value": "${{params.is_credit_quota}}",
            "label": "是否需要授信额度",
            "span": 8,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          }
        },
        {
          "appr_valid_date": {
            "control": "text",
            "label": "审批有效期",
            "span": 8,
            "suffix": "M",
            "value": "${{params.appr_valid_date}}"
          },
          "is_high_limit": {
            "control": "select",
            "value": "${{params.is_high_limit}}",
            "label": "是否支持最高额担保",
            "span": 8,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "is_return_repy_info": {
            "control": "select",
            "value": "${{params.is_return_repy_info}}",
            "label": "是否需要回补还款信息",
            "span": 8,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          }
        },
        {
          "is_inter_busi": {
            "control": "select",
            "value": "${{params.is_inter_busi}}",
            "label": "是否连通国际业务接口",
            "span": 8,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "is_trade_contract": {
            "control": "select",
            "value": "${{params.is_trade_contract}}",
            "label": "是否需要贸易合同",
            "span": 8,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "min_appr_amt": {
            "control": "currency",
            "align": "left",
            "format": {
              "symbol": "￥",
              "thousand": ",",
              "decimal": 2
            },
            "label": "最低申请金额",
            "span": 8
          },
          "max_appr_amt": {
            "control": "currency",
            "align": "left",
            "format": {
              "symbol": "￥",
              "thousand": ",",
              "decimal": 2
            },
            "label": "最高申请金额",
            "span": 8
          },
          "min_exposure_amt": {
            "control": "currency",
            "align": "left",
            "format": {
              "symbol": "￥",
              "thousand": ",",
              "decimal": 2
            },
            "label": "最低敞口金额",
            "span": 8
          },
          "max_exposure_amt": {
            "control": "currency",
            "align": "left",
            "format": {
              "symbol": "￥",
              "thousand": ",",
              "decimal": 2
            },
            "label": "最高敞口金额",
            "span": 8
          },
          "min_interest_rate": {
            "control": "text",
            "label": "最低利率",
            "span": 8,
            "suffix": "年"
          },
          "max_interest_rate": {
            "control": "text",
            "label": "最高利率",
            "span": 8,
            "suffix": "年"
          },
          "margin_percent": {
            "control": "text",
            "label": "首付保证金比例不低于",
            "span": 8
          },
          "min_deadline": {
            "control": "text",
            "label": "最小期限",
            "span": 8,
            "suffix": "月"
          },
          "max_deadline": {
            "control": "text",
            "label": "最大期限",
            "span": 8,
            "suffix": "月"
          }
        }
      ]
    }
  }
}
