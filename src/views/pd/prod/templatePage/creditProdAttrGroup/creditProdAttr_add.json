{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "name": "info",
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pd0313",
          "credit_prod_cd": "${{params.sale_prod_cd}}",
          "prod_version": "${{params.prod_version}}"
        }
      },
      "btns": [],
      "controls": [
        {
          "servicecode": {
            "control": "hidden",
            "label": "新增交易码",
            "value": "pd0314"
          },
          "prod_version": {
            "control": "hidden",
            "label": "授信产品版本"
          },
          "credit_prod_cd": {
            "control": "text",
            "label": "授信产品编号",
            "span": 8,
            "disabled": true
          },
          "is_self_help": {
            "control": "select",
            "label": "是否自助",
            "span": 8,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          },
          "multiple_credit_allow_flag": {
            "control": "select",
            "label": "是否允许多个有效授信",
            "span": 8,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          },
          "credit_way": {
            "control": "select",
            "label": "授信类型",
            "span": 8,
            "dict": {
              "dictType": "PD_E_CREDIT_WAY",
              "dictKey": [
                "PD_E_CREDIT_WAY"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          },
          "cl_allocation_mode": {
            "control": "select",
            "label": "分配方式",
            "span": 8,
            "dict": {
              "dictType": "PD_E_CLALLOCATION_MODE",
              "dictKey": [
                "PD_E_CLALLOCATION_MODE"
              ],
              "format": "label",
              "exclude": [
                "3"
              ]
            },
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          },
          "min_credit_amt": {
            "control": "currency",
            "align": "left",
            "label": "最小授信额度",
            "span": 8,
            "format": {
              "thousand": ",",
              "decimal": 2
            },
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          },
          "max_credit_amt": {
            "control": "currency",
            "align": "left",
            "label": "最大授信额度",
            "span": 8,
            "format": {
              "thousand": ",",
              "decimal": 2
            },
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          },
          "min_credit_exposure": {
            "control": "currency",
            "align": "left",
            "label": "最小授信敞口",
            "span": 8,
            "format": {
              "thousand": ",",
              "decimal": 2
            },
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          },
          "max_credit_exposure": {
            "control": "currency",
            "align": "left",
            "label": "最大授信敞口",
            "span": 8,
            "format": {
              "thousand": ",",
              "decimal": 2
            },
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          },
          "control_credit_flag": {
            "control": "select",
            "label": "是否控制授信额度",
            "span": 8,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          },
          "max_credit_term": {
            "control": "text",
            "label": "最大授信期限(月)",
            "span": 8,
            "rules": [
              {
                "required": true,
                "message": "请输入"
              },
              {
                "type": "number",
                "message": "请输入正确整数"
              }
            ]
          },
          "is_promptly_effect": {
            "control": "select",
            "label": "是否立即生效",
            "span": 8,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          }
        }
      ]
    }
  }
}
