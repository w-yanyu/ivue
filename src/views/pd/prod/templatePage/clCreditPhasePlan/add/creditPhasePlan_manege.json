{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ar0600",
          "apply_no": "${{params.apply_no}}"
        }
      },
      "pagination": {
        "pageSizes": [
          10,
          15,
          20,
          50,
          100
        ],
        "pageSize": 10
      },
      "thead": [
        {
          "label": "计划编号",
          "prop": "plan_id",
          "align": "center"
        },
        {
          "label": "阶段描述",
          "prop": "stage_desc",
          "align": "center"
        },
        {
          "label": "阶段序号",
          "prop": "stage_no",
          "align": "center"
        },
        {
          "label": "阶段比例",
          "prop": "stage_ratio",
          "align": "center"
        },
        {
          "label": "阶段金额",
          "prop": "stage_amt",
          "align": "center"
        }
      ]
    }
  }
}
