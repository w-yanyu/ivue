{
  "module": "lr_transferTable",
  "layout": {
    "transfer": {
      "height":"500px",
      "left": {
        "filterable":false,
        "title": "可选渠道",
        "placeholder": "请输入页面编号",
        "query":["page_template_no"],
        "btn":{
          "label": "关联",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode":"pd0262",
              "sale_prod_cd": "${{params.sale_prod_cd}}",
              "prod_version": "${{params.prod_version}}"
            }
          }
        },
        "datagrid": {
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "pd0261",
              "sale_prod_cd": "${{params.sale_prod_cd}}",
              "prod_version": "${{params.prod_version}}"
            }
          },
          "thead": [
            {
              "label": "渠道编号",
              "prop": "channel_no",
              "control": "text",
              "align": "center"
            },
            {
              "label": "渠道名称",
              "prop": "channel_name",
              "align": "center"
            }
          ]
        }
      },
      "right": {
        "title":"已选渠道",
        "filterable":false,
        "placeholder": "请输入页面编号",
        "query":["page_template_no"],
        "btn":{
          "label":"删除",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode":"pd0264",
              "sale_prod_cd": "${{params.sale_prod_cd}}",
              "prod_version": "${{params.prod_version}}"
            }
          }
        },
        "datagrid": {
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "pd0261",
              "sale_prod_cd": "${{params.sale_prod_cd}}",
              "prod_version": "${{params.prod_version}}"
            }
          },
          "checked":"checked",
          "thead": [
            {
              "label": "渠道编号",
              "prop": "channel_no",
              "control": "text",
              "align": "center"
            },
            {
              "label": "渠道名称",
              "prop": "channel_name",
              "align": "center"
            }
          ]
        }
      }
    }
  }
}
