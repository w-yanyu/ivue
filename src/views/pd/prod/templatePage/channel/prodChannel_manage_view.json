{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "detail,format=pd0260,temp=pd0261",
          "pageTranType": "${{params.pageTranType}}",
          "sale_prod_cd": "${{params.sale_prod_cd}}",
          "prod_version": "${{params.prod_version}}"
        }
      },
      "thead": [
        {
          "label": "渠道编号",
          "prop": "channel_no",
          "control": "text",
          "align": "center"
        },
        {
          "label": "渠道名称",
          "prop": "channel_name",
          "align": "center"
        }
      ]
    }
  }
}
