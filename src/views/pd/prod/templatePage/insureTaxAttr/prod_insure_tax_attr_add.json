{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "name": "info",
      "btns": [],
      "initRequest": {
        "url": "/SUMP/call/RPCCall",
        "method": "post",
        "params": {
          "servicecode": "pd1518",
          "sale_prod_cd": "${{params.sale_prod_cd}}",
          "prod_version": "${{params.prod_version}}"
        }
      },
      "controlsGroup": [
        {
          "title": "投保参数",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "accordion": false,
          "expand": true,
          "controls": [{

            "servicecode": {
              "control": "hidden",
              "label": "保存投保缴税属性",
              "value": "pd1520"
            },
            "sale_prod_cd": {
              "label": "可售产品编号",
              "control": "hidden",
              "value": "${{params.sale_prod_cd}}"
            },
            "prod_version": {
              "label": "产品版本",
              "control": "hidden",
              "value": "${{params.prod_version}}"
            },
            "is_bank_insured": {
              "label": "是否必须银行代投保",
              "control": "select",
              "span": 8,
              "width": "140px",
              "rules": [
                {
                  "required": true,
                  "message": "请选择是否必须银行代投保"
                }
              ],
              "dict": {
                "dictType": "MS_E_YESORNO",
                "dictKey": [
                  "MS_E_YESORNO"
                ]
              },"events": {
                "condition": {
                  "0": {
                    "control": {
                      "source_of_insured_funds": {
                        "disabled": true,
                        "value":""
                      }
                    }
                  },
                  "1": {
                    "control": {
                      "source_of_insured_funds": {
                        "disabled": false
                      }
                    }
                  }
                }
              }
            },
            "source_of_insured_funds": {
              "label": "投保资金来源",
              "control": "select",
              "multiple": true,
              "dict": {
                "dictType": "MS_E_SOURCE_PAY_TYPE",
                "dictKey": [
                  "MS_E_SOURCE_PAY_TYPE"
                ]
              },
              "span": 8,
              "width": "140px",
              "format": "label-value"
            }
            }]
      },
      {
          "title": "缴税参数",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "expand": true,
          "controls": [{
            "is_bank_paid": {
              "control": "select",
              "span": 8,
              "width": "140px",
              "label": "是否必须银行代缴",
              "rules": [
                {
                  "required": true,
                  "message": "请选择是否必须银行代缴"
                }
              ],
              "dict": {
                "dictType": "MS_E_YESORNO",
                "dictKey": [
                  "MS_E_YESORNO"
                ],
                "format": "label"
              },"events": {
                "condition": {
                  "0": {
                    "control": {
                      "source_of_payment": {
                        "disabled": true,
                        "value":""
                      },"tax_rate": {
                        "disabled": true,
                        "value":""
                      }
                    }
                  },
                  "1": {
                    "control": {
                      "source_of_payment": {
                        "disabled": false
                      },"tax_rate": {
                        "disabled": false
                      }
                    }
                  }
                }
              }
            },
            "source_of_payment": {
              "control": "select",
              "multiple": true,
              "span": 8,
              "width": "140px",
              "label": "代缴款来源",
              "dict": {
                "dictType": "MS_E_SOURCE_PAY_TYPE",
                "dictKey": [
                  "MS_E_SOURCE_PAY_TYPE"
                ],
                "format": "label-value"
              }
            },"tax_rate": {
              "label": "缴税税率(%)",
              "control": "currency",
              "span": 8,
              "width": "140px"
            }
          }]
      }
        

      ]
    }
  }
}
