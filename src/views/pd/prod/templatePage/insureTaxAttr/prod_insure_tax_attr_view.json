{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "name": "info",
      "btns": [],
      "initRequest": {
        "url": "/SUMP/call/RPCCall",
        "method": "post",
        "params": {
          "servicecode": "detail,format=pd1521,temp=pd1518",
          "pageTranType": "${{params.pageTranType}}",
          "sale_prod_cd": "${{params.sale_prod_cd}}",
          "prod_version": "${{params.prod_version}}"
        }
      },
      "controlsGroup": [
        {
          "title": "投保参数",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "accordion": false,
          "expand": true,
          "controls": [{
            "sale_prod_cd": {
              "label": "可售产品编号",
              "control": "hidden",
              "value": "${{params.sale_prod_cd}}"
            },
            "prod_version": {
              "label": "产品版本",
              "control": "hidden",
              "value": "${{params.prod_version}}"
            },
            "is_bank_insured": {
              "label": "是否必须银行代投保",
              "disabled":true,
              "control": "select",
              "span": 8,
              "width": "140px",
              "dict": {
                "dictType": "MS_E_YESORNO",
                "dictKey": [
                  "MS_E_YESORNO"
                ]
              }
            },
            "source_of_insured_funds": {
              "label": "投保资金来源",
              "disabled":true,
              "control": "select",
              "multiple": true,
              "dict": {
                "dictType": "MS_E_SOURCE_PAY_TYPE",
                "dictKey": [
                  "MS_E_SOURCE_PAY_TYPE"
                ]
              },
              "span": 8,
              "width": "140px",
              "format": "label-value"
            }
            }]
      },
      {
          "title": "缴税参数",
          "tooltip": "",
          "titleIcon": "",
          "disabled":true,
          "showHint": "",
          "expand": true,
          "controls": [{
            "is_bank_paid": {
              "control": "select",
              "span": 8,
              "disabled":true,
              "width": "140px",
              "label": "是否必须银行代缴",
              "dict": {
                "dictType": "MS_E_YESORNO",
                "dictKey": [
                  "MS_E_YESORNO"
                ],
                "format": "label"
              }
            },
            "source_of_payment": {
              "control": "select",
              "disabled":true,
              "multiple": true,
              "span": 8,
              "width": "140px",
              "label": "代缴款来源",
              "dict": {
                "dictType": "MS_E_SOURCE_PAY_TYPE",
                "dictKey": [
                  "MS_E_SOURCE_PAY_TYPE"
                ],
                "format": "label-value"
              }
            },"tax_rate": {
              "label": "缴税税率(%)",
              "disabled":true,
              "control": "currency",
              "span": 8,
              "width": "140px"
            }
          }]
      }
        

      ]
    }
  }
}
