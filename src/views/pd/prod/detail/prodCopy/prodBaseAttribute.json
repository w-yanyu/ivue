{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "btns": [
        {
          "type": "submit",
          "target": "addlog",
          "label": "复制",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "pd0219"
            }
          }
        }
      ],
      "controls": [
        {
          "sale_prod_cd": {
            "control": "hidden",
            "label": "可售产品编号",
            "value": "${{params.sale_prod_cd}}"
          },
          "prod_version": {
            "control": "hidden",
            "label": "可售产品版本",
            "value": "${{params.prod_version}}"
          },
          "new_sale_prod_cd": {
            "control": "text",
            "align": "center",
            "span": 16,
            "inputWidth": "250px",
            "label": "可售产品编号",
            "rules": [
              {
                "required": true,
                "message": "可售产品编号不得为空"
              },
              {
                "max": 80,
                "message": "可售产品编号长度不能超过80个字符"
              }
            ]
          },
          "new_sale_prod_name": {
            "control": "text",
            "label": "可售产品名称",
            "align": "center",
            "span": 16,
            "inputWidth": "250px",
            "rules": [
              {
                "required": true,
                "message": "可售产品名称不得为空"
              },
              {
                "max": 80,
                "message": "长度不能超过80个字符"
              }
            ]
          }
        }
      ]
    }
  }
}