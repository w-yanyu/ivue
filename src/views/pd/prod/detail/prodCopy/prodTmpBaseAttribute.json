{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "btns": [
        {
          "type": "submit",
          "target": "addlog",
          "label": "复制",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "pd0156"
            }
          }
        }
      ],
      "controls": [
        {
          "prod_version": {
            "control": "hidden",
            "label": "可售产品版本",
            "value": "${{params.prod_version}}"
          },
          "template_id": {
            "control": "hidden",
            "label": "版本编号",
            "value": "${{params.template_id}}"
          },
          "template_name": {
            "control": "text",
            "align": "center",
            "span": 16,
            "inputWidth": "250px",
            "label": "模板名称",
            "rules": [
              {
                "required": true,
                "message": "模板名称不得为空"
              },
              {
                "max": 80,
                "message": "模板名称长度不能超过80个字符"
              }
            ]
          }
        }
      ]
    }
  }
}