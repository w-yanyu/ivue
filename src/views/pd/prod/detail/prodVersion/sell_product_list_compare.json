{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {

      "btns":[]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pd1510",
          "list": "${{params.selectedData}}"
        }
      },
      "pagination": {
        "show": false
      },
      "thead": [
        {
          "type":"string",
          "label": "对比项",
          "prop": "contrast_name",
          "align": "center"
        },
        {
          "type":"textarea",
          "label": "旧版本产品",
          "prop": "small_version_contrast",
          "align": "center"
        },
        {
          "type":"textarea",
          "label": "新版本产品",
          "prop": "major_version_contrast",
          "align": "center"
        }
      ]
    }
  }
}
