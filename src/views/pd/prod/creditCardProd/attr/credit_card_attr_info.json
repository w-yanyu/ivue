{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "name": "info",
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pd1210",
          "sale_prod_cd": "${{params.sale_prod_cd}}",
          "prod_version": "${{params.prod_version}}",
          "apply_type": "${{params.apply_type}}"
        }
      },
      "btns": [],
      "controlsGroup": [
        {
          "title": "放款属性",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "accordion": false,
          "expand": true,
          "controls": [
            {
              "servicecode": {
                "control": "hidden",
                "label": "保存贷款属性",
                "value": "pd1211"
              },
              "busi_seq": {
                "label": "可售产品编号",
                "control": "hidden",
                "value": "${{params.busi_seq}}"
              },
              "sale_prod_cd": {
                "label": "可售产品编号",
                "control": "hidden",
                "value": "${{params.sale_prod_cd}}"
              },
              "prod_version": {
                "label": "产品版本",
                "control": "hidden",
                "value": "${{params.prod_version}}"
              },
              "apply_type": {
                "label": "申请类型",
                "control": "hidden",
                "value": "${{params.apply_type}}"
              },
              "apply_type_name": {
                "label": "申请类型名称",
                "control": "hidden",
                "value": "${{params.apply_type_name}}"
              }
            },
            {
              "min_loan_amt": {
                "label": "最小放款金额",
                "control": "currency",
                "align": "left",
                "format": {
                  "thousand": ",",
                  "decimal": 2
                },
                "span": 8,
                "width": "140px",
                "rules": [
                  {
                    "required": false,
                    "message": "请输入最小放款金额"
                  }
                ]
              },
              "max_loan_amt": {
                "label": "最大放款金额",
                "control": "currency",
                "align": "left",
                "format": {
                  "thousand": ",",
                  "decimal": 2
                },
                "span": 8,
                "width": "140px",
                "rules": [
                  {
                    "required": false,
                    "message": "请输入最大放款金额"
                  }
                ]
              },
              "shortest_loan_term": {
                "label": "最短贷款期限(月)",
                "control": "text",
                "span": 8,
                "width": "140px",
                "rules": [
                  {
                    "required": false,
                    "message": "请输入最短贷款期限(月)"
                  }
                ]
              }
            },
            {
              "longest_loan_term": {
                "label": "最长贷款期限(月)",
                "control": "text",
                "span": 8,
                "width": "140px",
                "rules": [
                  {
                    "required": false,
                    "message": "请输入最长贷款期限(月)"
                  }
                ]
              },
              "support_installment": {
                "label": "支持期限",
                "control": "select",
                "multiple": true,
                "span": 8,
                "width": "140px",
                "rules": [
                  {
                    "required": true,
                    "message": "请输入支持支持期限"
                  }
                ],
                "dict": {
                  "dictType": "PD_E_PERIODS",
                  "dictKey": [
                    "PD_E_PERIODS"
                  ]
                }
              }
            }
          ]
        }
      ]
    }
  }
}