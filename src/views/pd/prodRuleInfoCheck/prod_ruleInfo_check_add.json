{
    "module": "v-inputDataForm-btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "pd0290"
                }
            },
            "controls": [
                {
                    "busi_seq": {
                        "label": "业务流水号",
                        "control": "hidden"
                    },
                    "sale_prod_cd": {
                        "label": "可售产品编号",
                        "control": "hidden"
                    },
                    "prod_version": {
                        "label": "产品版本",
                        "control": "hidden"
                    },
                    "check_control_type": {
                        "label": "检查控制类型",
                        "control": "hidden"
                    },
                    "apply_type": {
                        "label": "申请类型",
                        "control": "hidden"
                    }
                },
                {
                    "rule_id": {
                        "label": "规则编号",
                        "control": "lookup",
                        "span": 8,
                        "width": "140px",
                        "edit": true,
                        "keyField": {
                            "rule_id": "rule_id",
                            "rule_code": "rule_code",
                            "rule_name": "rule_name",
                            "rule_type": "rule_type"
                        },
                        "toRequest": {
                            "url": "/views/pd/prodRuleInfoCheck/prod_ruleInfo_lookup.json",
                            "method": "post"
                        },
                        "props": {
                            "title": "规则信息查询"
                        },
                        "format": "label",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入规则编号"
                            }
                        ]
                    },
                    "rule_code": {
                        "label": "规则代码",
                        "control": "text",
                        "disabled": "true",
                        "span": 8,
                        "width": "140px",
                        "rules": [
                            {
                                "required": false,
                                "message": "请输入规则代码"
                            }
                        ]
                    },
                    "rule_name": {
                        "label": "规则名称",
                        "control": "text",
                        "disabled": "true",
                        "span": 8,
                        "width": "140px",
                        "rules": [
                            {
                                "required": false,
                                "message": "请输入规则名称"
                            }
                        ]
                    },
                    "rule_type": {
                        "label": "规则类型",
                        "control": "select",
                        "disabled": "true",
                        "span": 8,
                        "width": "140px",
                        "rules": [
                            {
                                "required": false,
                                "message": "请选择规则类型"
                            }
                        ],
                        "dict": {
                            "dictType": "PD_E_RULE_TYPE",
                            "dictKey": [
                                "PD_E_RULE_TYPE"
                            ]
                        }
                    },
                    "rule_details": {
                        "label": "规则详情",
                        "control": "textarea",
                        "span": 8,
                        "width": "140px",
                        "rules": [
                            {
                                "required": false,
                                "message": "请输入规则详情"
                            }
                        ]
                    },
                    "appro_type": {
                        "label": "审批类型",
                        "control": "select",
                        "span": 8,
                        "width": "140px",
                        "rules": [
                            {
                                "required": false,
                                "message": "请选择审批类型"
                            }
                        ],
                        "dict": {
                            "dictType": "PD_E_APPRO_ROUTE_TYPE",
                            "dictKey": [
                                "PD_E_APPRO_ROUTE_TYPE"
                            ]
                        }
                    }
                }
            ]
        }
    }
}