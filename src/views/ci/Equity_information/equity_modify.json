{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "btns": [{
                "type": "submit",
                "label": "提交",
                "doRequest": {
                    "url": "/SUMP/icmcall/icmRPCCall",
                    "method": "POST",
                    "params": {
                        "servicecode": "IUMSCUTS0037"

                    }
                }
            }],
            "controls": [{
                    "vetype": {
                        "label": "验资类别",
                        "width": "10px",
                        "control": "select",
                        "rules": [{
                            "required": true,
                            "message": "请选择验资类别"
                        }],
                        "dict": {
                            "dictType": "E_VETYPE",
                            "dictKey": [
                                "E_VETYPE"
                            ],
                            "format": "label"
                        }
                    },
                    "acfirm": {
                        "control": "text",
                        "width": "10px",
                        "label": "会计验资机构名称",
                        "rules": [{
                                "required": false
                            },
                            {
                                "max": 80,
                                "message": "长度不能超过80个字符"
                            }
                        ]
                    }
                },
                {
                    "verycd": {
                        "control": "text",
                        "width": "10px",
                        "label": "会计验资报告号码",
                        "rules": [{
                                "required": false
                            },
                            {
                                "max": 30,
                                "message": "长度不能超过30个字符"
                            }
                        ]
                    },
                    "veridt": {
                        "control": "dateTimePicker",
                        "label": "会计验资时间",
                        "width": "10px",
                        "rules": [{
                            "required": true,
                            "message": "请选择会计验资时间"
                        }]
                    }
                },
                {
                    "regamt": {
                        "control": "text",
                        "width": "10px",
                        "label": "注册资本金额",
                        "rules": [{
                            "required": true,
                            "max": 16,
                            "pattern": "^[0-9]+(\\.[0-9]{1,2})?$",
                            "message": "请输入两位小数以内数字,长度不能超过16个字符"
                        }]
                    },
                    "regcyy": {
                        "label": "注册资本币种",
                        "control": "select",
                        "width": "10px",
                        "dict": {
                            "dictType": "MS_CURRENCY",
                            "dictKey": [
                                "MS_CURRENCY"
                            ],
                            "format": "label"
                        }
                    }
                },
                {
                    "facamt": {
                        "control": "text",
                        "width": "10px",
                        "label": "实际到位资金",
                        "rules": [{
                            "required": false,
                            "max": 16,
                            "pattern": "^[0-9]+(\\.[0-9]{1,2})?$",
                            "message": "请输入两位小数以内数字,长度不能超过16个字符"
                        }]
                    },
                    "liflag": {
                        "label": "是否公众上市公司",
                        "width": "10px",
                        "control": "select",
                        "rules": [{
                            "required": true,
                            "message": "请选择是否公众上市公司"
                        }],
                        "dict": {
                            "dictType": "E_LIFLAG",
                            "dictKey": [
                                "E_LIFLAG"
                            ],
                            "format": "label"
                        }
                    }
                },
                {
                    "stowst": {
                        "control": "text",
                        "width": "10px",
                        "label": "国有股"
                    },
                    "totsto": {
                        "control": "text",
                        "width": "10px",
                        "label": "发起人股"

                    }
                },
                {
                    "prosto": {
                        "control": "text",
                        "width": "10px",
                        "label": "总股本",
                        "rules": [{
                            "required": true,
                            "message": "请输入总股本"
                        }],
                        "decimal": "0"
                    },
                    "innsto": {
                        "control": "text",
                        "width": "10px",
                        "label": "内部职工股",
                        "decimal": "0"
                    }
                },
                {
                    "grosto": {
                        "control": "text",
                        "label": "集体股",
                        "width": "10px"
                    },
                    "consto": {
                        "control": "text",
                        "width": "10px",
                        "label": "转配股",
                        "decimal": "0",
                        "rules": [{
                            "required": false
                        }]
                    }
                },
                {
                    "forsto": {
                        "control": "text",
                        "width": "10px",
                        "label": "外资持股",
                        "rules": [{
                            "required": false
                        }],
                        "decimal": "0"
                    },
                    "bstock": {
                        "control": "text",
                        "width": "10px",
                        "label": "b股",
                        "rules": [{
                            "required": false
                        }],
                        "decimal": "0"
                    }
                },
                {
                    "pristo": {
                        "control": "text",
                        "width": "10px",
                        "label": "优先股",
                        "rules": [{
                            "required": false
                        }],
                        "decimal": "0"
                    },
                    "hstock": {
                        "control": "text",
                        "width": "10px",
                        "label": "h股",
                        "rules": [{
                            "required": false
                        }],
                        "decimal": "0"
                    }
                },
                {
                    "astock": {
                        "control": "text",
                        "width": "10px",
                        "label": "a股",
                        "rules": [{
                            "required": false
                        }],
                        "decimal": "0"
                    },
                    "otcsto": {
                        "control": "text",
                        "width": "10px",
                        "label": "其他流通股",
                        "rules": [{
                            "required": false
                        }],
                        "decimal": "0"
                    }
                },
                {
                    "otnsto": {
                        "control": "text",
                        "width": "10px",
                        "label": "其他未流通股份",
                        "rules": [{
                            "required": false
                        }],
                        "decimal": "0"
                    },
                    "resato": {
                        "control": "text",
                        "width": "10px",
                        "label": "股权分治限售",
                        "rules": [{
                            "required": false
                        }],
                        "decimal": "0"
                    }
                },
                {
                    "lepeto": {
                        "control": "text",
                        "width": "10px",
                        "label": "募集法人股",
                        "rules": [{
                            "required": false
                        }],
                        "decimal": "0"
                    },
                    "csisto": {
                        "control": "text",
                        "width": "10px",
                        "label": "境内法人股",

                        "rules": [{
                            "required": false
                        }]
                    }
                },
                {
                    "issudt": {
                        "control": "dateTimePicker",
                        "width": "10px",
                        "label": "发布日期",
                        "rules": [{
                            "required": true,
                            "message": "请选择发布日期"
                        }]
                    }
                },
                {
                    "remark": {
                        "label": "备注",
                        "width": "10px",
                        "control": "textarea",
                        "span": 24,
                        "autosize": {
                            "minRows": 4
                        },
                        "rules": [{
                            "max": "100",
                            "message": "长度不能超过100个字符"
                        }]
                    }
                },
                {
                    "custid": {
                        "control": "hidden",
                        "width": "10px",
                        "label": "客户编号",
                        "disabled": true
                    },
                    "capsid": {
                        "control": "hidden",
                        "width": "10px",
                        "label": "企业资本结构流水号"
                    }
                }
            ]
        }
    }
}