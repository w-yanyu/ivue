{
  "module": "v_search_btn_datagrid",
  "layout": {
      "toolbar": {
          "btns": [
          ]
      },
      "datagrid": {
          "scope": "searchTable",
          "isInitSearch": false,
          "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                  "servicecode": "qrcusthis",
                  "custid": "${{params.custid}}"
              }
          },
          "thead": [
              {
                  "type": "index",
                  "label":"序号"
              },
            {
              "label": "变更关联业务号",
              "prop": "busino",
              "align":"center"
            },
              {
                  "label": "客户号",
                  "prop": "custid",
                  "align":"center"
              },
              {
                  "label": "客户名称",
                  "prop": "custna",
                  "align":"center"
              },
              {
                  "label": "证件类型",
                  "prop": "certtp",
                  "align":"center",
                  "dict": {
                    "dictType": "E_PUBLIC",
                    "dictKey": [
                      "E_PUBLIC"
                    ],
                  "labelField":"dictName",
                  "valueField":"dictId"
                  }
              },
              {
                  "label": "证件号码",
                  "prop": "certno",
                  "align":"center"
              },
            {
              "label": "证件生效日",
              "prop": "cardsday",
              "align":"center"
            },
            {
              "label": "证件到期日",
              "prop": "cardeday",
              "align":"center"
            },
              {
                  "label": "性别",
                  "prop": "gender",
                  "align":"center",
                  "dict": {
                    "dictType": "MS_E_GENDER",
                    "dictKey": [
                      "MS_E_GENDER"
                    ],
                  "labelField":"dictName",
                  "valueField":"dictId"
                }
              },
              {
               "label": "出生日期",
               "prop":"birthday",
               "align":"center"

              },
            {
              "label": "变更日期",
              "prop":"update_date",
              "align":"center"

            },
            {
              "label": "操作",
              "align":"center",
              "btns": [
                {
                  "type": "look",
                  "label": "查看影像件",
                  "toRequest": {
                    "url": "@UserImageInfo",
                    "method": "get"
                  },
                  "props": {
                    "width": "1000px",
                    "title": "查看影像件"
                  }
                }
              ]
            }
            ]
      }
  }
}
