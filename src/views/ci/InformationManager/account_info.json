{
    "module": "v-inputDataForm-btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "btns": [],
            "controls": [{
                    "custna": {
                        "control": "string",
                        "label": "客户名称",
                        "rules": [{
                            "required": true,
                            "message": "请输入客户名称"
                        }]
                    },
                    "certno": {
                        "control": "string",
                        "label": "证件号码",
                        "rules": [{
                            "required": true,
                            "message": "请输入证件号码"
                        }]
                    }
                },
                {
                    "spllna": {
                        "control": "string",
                        "label": "姓名拼音"
                    },
                    "cuclas": {
                        "control": "string",
                        "label": "客户类别",
                        "dict": {
                            "dictType": "E_CUCLAS",
                            "dictKey": [
                                "E_CUCLAS"
                            ],
                            "format": "label"
                        }
                    }
                },
                {
                    "certtp": {
                        "control": "string",
                        "label": "证件类型",
                        "rules": [{
                            "required": true,
                            "message": "请选择证件类型"
                        }],
                        "dict": {
                            "dictType": "E_PUBLIC",
                            "dictKey": [
                                "E_PUBLIC"
                            ],
                            "labelField": "dictName",
                            "valueField": "dictId"
                        }
                    },
                    "indvfg": {
                        "control": "string",
                        "label": "是否是个人客户",
                        "dict": {
                            "dictType": "E_YES_",
                            "dictKey": [
                                "E_YES_"
                            ],
                            "labelField": "dictName",
                            "valueField": "dictId"
                        }
                    }
                },
                {
                    "lnflag": {
                        "control": "string",
                        "label": "是否是借款客户",
                        "dict": {
                            "dictType": "E_YES_",
                            "dictKey": [
                                "E_YES_"
                            ],
                            "labelField": "dictName",
                            "valueField": "dictId"
                        }
                    },
                    "secflg": {
                        "control": "string",
                        "label": "是否是担保客户",
                        "dict": {
                            "dictType": "E_YES_",
                            "dictKey": [
                                "E_YES_"
                            ],
                            "labelField": "dictName",
                            "valueField": "dictId"
                        }
                    }
                },
                {
                    "grpflg": {
                        "control": "string",
                        "label": "是否集团客户",
                        "dict": {
                            "dictType": "E_YES_",
                            "dictKey": [
                                "E_YES_"
                            ],
                            "labelField": "dictName",
                            "valueField": "dictId"
                        }
                    },
                    "ownflg": {
                        "control": "string",
                        "label": "是否是资产共有人客户",
                        "dict": {
                            "dictType": "E_YES_",
                            "dictKey": [
                                "E_YES_"
                            ],
                            "labelField": "dictName",
                            "valueField": "dictId"
                        }
                    }
                },
                {
                    "comflg": {
                        "control": "string",
                        "label": "是否代办人客户",
                        "dict": {
                            "dictType": "E_YES_",
                            "dictKey": [
                                "E_YES_"
                            ],
                            "labelField": "dictName",
                            "valueField": "dictId"
                        }
                    },
                    "othflg": {
                        "control": "string",
                        "label": "是否其他客户",
                        "dict": {
                            "dictType": "E_YES_",
                            "dictKey": [
                                "E_YES_"
                            ],
                            "labelField": "dictName",
                            "valueField": "dictId"
                        }
                    }
                },
                {
                    "certct": {
                        "control": "string",
                        "label": "证件签发地"
                    },
                    "mannam": {
                        "control": "string",
                        "label": "客户经理名称"
                    }
                },
                {
                    "inctry": {
                        "control": "string",
                        "label": "国籍",
                        "dict": {
                            "dictType": "E_COUNTY",
                            "dictKey": [
                                "E_COUNTY"
                            ]
                        }
                    },
                    "brchcd": {
                        "control": "string",
                        "label": "开户行"
                    }
                }
            ]
        }
    }
}
