{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "scope": "formData",
            "btns": [{
                "type": "submit",
                "label": "提交",
                "icon": "",
                "doRequest": {
                    "url": "/SUMP/icmcall/icmRPCCall",
                    "method": "POST",
                    "params": {
                        "servicecode": "IUMSCUTS0001"
                    }
                }
            }],
            "controls": [{
                    "certtp": {
                        "control": "select",
                        "label": "证件类型",
                        "width": "130px",
                        "dict": {
                            "dictType": "E_PUBLIC",
                            "dictKey": [
                                "E_PUBLIC"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": true,
                            "message": "请选择证件类型"
                        }]
                    },
                    "certno": {
                        "control": "text",
                        "label": "证件号码",
                        "width": "130px",
                        "rules": [{
                                "required": true,
                                "message": "请输入证件号码"
                            },
                            {
                                "max": 50,
                                "message": "长度不能超过50个字符"
                            }
                        ]
                    }
                },
                {
                    "cuclas": {
                        "control": "select",
                        "label": "客户分类",
                        "width": "130px",
                        "dict": {
                            "dictType": "E_CUCLAS",
                            "dictKey": [
                                "E_CUCLAS"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": false,
                            "message": "请选择客户分类"
                        }]
                    },
                    "certct": {
                        "control": "text",
                        "label": "签发地址",
                        "width": "130px",
                        "rules": [{
                                "required": true,
                                "message": "请输入签发地址"
                            },
                            {
                                "max": 80,
                                "message": "长度不能超过80个字符"
                            }
                        ]
                    }
                },
                {
                    "corgcd": {
                        "control": "text",
                        "label": "组织机构代码",
                        "width": "130px",
                        "rules": [
                            {
                                "max": 30,
                                "message": "长度不能超过30个字符"
                            }
                        ]
                    },
                    "culncd": {
                        "control": "text",
                        "label": "贷款卡编号",
                        "width": "130px",
                        "rules": [{
                                "required": false,
                                "message": "请输入贷款卡编号"
                            },
                            {
                                "max": 32,
                                "message": "长度不能超过32个字符"
                            }
                        ]
                    }
                },
                {
                    "orgdit": {
                        "control": "text",
                        "label": "机构信用代码",
                        "width": "130px",
                        "rules": [{
                                "required": true,
                                "message": "请输入机构信用代码"
                            },
                            {
                                "max": 20,
                                "message": "长度不能超过20个字符"
                            }
                        ]
                    },
                    "vipflg": {
                        "control": "select",
                        "label": "会员标志",
                        "width": "130px",
                        "dict": {
                            "dictType": "E_YES_",
                            "dictKey": [
                                "E_YES_"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": true,
                            "message": "请选择是否会员"
                        }]
                    }

                },
                {
                    "cuenna": {
                        "control": "text",
                        "label": "客户英文名称",
                        "width": "130px",
                        "rules": [{
                                "required": false,
                                "message": "请输入客户英文名称"
                            },
                            {
                                "max": 80,
                                "message": "长度不能超过80个字符"
                            }
                        ]
                    },
                    "cuodna": {
                        "control": "text",
                        "label": "客户曾用名",
                        "width": "130px",
                        "rules": [{
                                "required": false,
                                "message": "请输入客户曾用名"
                            },
                            {
                                "max": 80,
                                "message": "长度不能超过80个字符"
                            }
                        ]
                    }
                },
                {
                    "spllna": {
                        "control": "text",
                        "label": "姓名拼音",
                        "width": "130px",
                        "rules": [{
                                "required": false,
                                "message": "请输入姓名拼音"
                            },
                            {
                                "max": 80,
                                "message": "长度不能超过80个字符"
                            }
                        ]
                    },
                    "indvfg": {
                        "control": "select",
                        "label": "是否个人客户",
                        "width": "130px",
                        "dict": {
                            "dictType": "E_YES_",
                            "dictKey": [
                                "E_YES_"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": true,
                            "message": "请选择是否个人客户"
                        }]
                    }
                },

                {
                    "cuprop": {
                        "control": "select",
                        "label": "客户性质",
                        "width": "130px",
                        "dict": {
                            "dictType": "E_CUPROP",
                            "dictKey": [
                                "E_CUPROP"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": true,
                            "message": "请选择客户性质"
                        }]
                    },
                    "inctry": {
                        "control": "select",
                        "label": "国籍",
                        "width": "130px",
                        "dict": {
                            "dictType": "E_COUNTY",
                            "dictKey": [
                                "E_COUNTY"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": true,
                            "message": "请选择国籍"
                        }]
                    }
                },
                {

                    "custty": {
                        "control": "select",
                        "label": "客户类别",
                        "disabled": true,
                        "value": "5",
                        "width": "130px",
                        "dict": {
                            "dictType": "E_CUSTTY",
                            "dictKey": [
                                "E_CUSTTY"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": true,
                            "message": "请选择客户类别"
                        }],
                        "showItem": {
                            "$default": [],
                            "5": [
                                "grpflg"
                            ]
                        },
                        "events": {
                            "condition": {
                                "5": {
                                    "control": {
                                        "custna": {

                                            "label": "集团客户名称",
                                            "width": "130px",
                                            "rules": [{
                                                    "required": true,
                                                    "message": "请输入集团客户名称"
                                                },
                                                {
                                                    "max": 200,
                                                    "message": "长度不能超过200个字符"
                                                }
                                            ]
                                        }
                                    }
                                },
                                "$default": {
                                    "control": {
                                        "custna": {
                                            "label": "客户名称",
                                            "width": "130px",
                                            "rules": [{
                                                    "required": true,
                                                    "message": "请输入客户名称"
                                                },
                                                {
                                                    "max": 200,
                                                    "message": "长度不能超过200个字符"
                                                }
                                            ]
                                        }
                                    }
                                }
                            }
                        }
                    },

                    "custna": {
                        "label": "客户名称",
                        "control": "lookup",
                        "edit": true,
                        "width": "130px",
                        "format": "label",
                        "multiple": false,
                        "props": {
                            "title": "客户名称选择"
                        },
                        "keyField": {
                            "custna": "custna",
                            "certno": "certno",
                            "certtp": "certtp"
                        },
                        "toRequest": {
                            "url": "/views/dh/processStart/customer_list.json",
                            "method": "post"
                        }
                    }
                },
                {
                    "grpflg": {
                        "control": "select",
                        "label": "是否集团虚拟客户",
                        "width": "130px",
                        "dict": {
                            "dictType": "E_YES_",
                            "dictKey": [
                                "E_YES_"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": true,
                            "message": "请选择是否集团虚拟客户"
                        }],
                        "events": {
                            "condition": {
                                "Y": {
                                    "control": {
                                        "custna": {
                                            "label": "集团客户名称(虚拟)",
                                            "width": "130px",
                                            "rules": [{
                                                    "required": true,
                                                    "message": "请输入集团客户名称(虚拟)"
                                                },
                                                {
                                                    "max": 200,
                                                    "message": "长度不能超过200个字符"
                                                }
                                            ]
                                        }
                                    }
                                },
                                "N": {
                                    "control": {
                                        "custna": {
                                            "label": "集团客户名称",
                                            "edit": false,
                                            "props": {
                                                "title": "集团客户名称选择"
                                            },
                                            "rules": [{
                                                "required": true,
                                                "message": "请输入集团客户名称"
                                            }]
                                        }
                                    }
                                }
                            }
                        }
                    }
                },
                {
                    "frozen": {
                        "control": "hidden",
                        "label": "冻结状态",
                        "width": "130px",
                        "dict": {
                            "dictType": "E_YES_",
                            "dictKey": [
                                "E_YES_"
                            ],
                            "format": "label"
                        },
                        "value": "N"
                    },
                    "trantp": {
                        "control": "hidden",
                        "label": "交易类型",
                        "width": "130px",
                        "dict": {
                            "dictType": "E_TRANTP",
                            "dictKey": [
                                "E_TRANTP"
                            ],
                            "format": "label"
                        },
                        "value": "INSERT"
                    }
                },
                {
                    "islpac": {
                        "control": "select",
                        "label": "是否法人关联客户",
                        "width": "130px",
                        "dict": {
                            "dictType": "E_YES_",
                            "dictKey": [
                                "E_YES_"
                            ],
                            "format": "label"
                        },
                        "rules": [{
                            "required": true,
                            "message": "请选择是否法人关联客户"
                        }]
                    }
                }
            ]
        }
    }
}
