{
    "module": "dynamicTabs",
    "layout": {
        "tabs": {
            "scope": "templateScope",
            "initRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "IUMSCUTS0002"

                }
            },
            "tabHeader": [{
                    "id": "accountInfo",
                    "label": "用户信息",
                    "closable": false,
                    "disabled": false,
                    "icon": "el-icon-date",
                    "toRequest": {
                        "url": "/views/ci/InformationManager/user_info.json"
                    }
                },
                {
                    "id": "cardinfo",
                    "label": "绑卡签约信息",
                    "closable": false,
                    "disabled": false,
                    "icon": "el-icon-date",
                    "toRequest": {
                        "url": "/views/ci/InformationManager/card_info.json",
                        "method": "post"
                    }
                }

            ],
            "defaultActiveKey": "accountInfo",
            "position": "top",
            "styleClass": "card"
        }
    }
}