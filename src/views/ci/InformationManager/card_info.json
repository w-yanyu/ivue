{
    "module": "v_search_btn_datagrid",
    "layout": {

        "toolbar": {
            "btns": []
        },
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": true,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "IUMSCUTS0009",
                    "custid": "${{params.custid}}",
                    "certtp": "${{params.certtp}}",
                    "certno": "${{params.certno}}",
                    "channalId": "${{params.channalId}}",
                    "Out_user_id": "${{params.Out_user_id}}"


                }
            },
            "pagination": {
                "show": false
            },
            "thead": [{
                    "type": "index",
                    "label": "序号"
                },
                {
                    "label": "客户号",
                    "prop": "custid",
                    "align": "center"
                },
                {
                    "label": "证件号码",
                    "prop": "certno",
                    "align": "center"
                },

                {
                    "label": "卡号",
                    "prop": "cardNo",
                    "align": "center"
                },

                {
                    "label": "渠道号",
                    "prop": "channalId",
                    "align": "center",
                    "dict": {
                        "dictType": "E_CHANEL_ID",
                        "dictKey": [
                            "E_CHANEL_ID"
                        ],
                        "labelField": "dictName",
                        "valueField": "dictId"
                    }
                },

                {
                    "label": "操作",
                    "align": "center",
                    "btns": [{
                        "type": "look",
                        "label": "详情",
                        "toRequest": {
                            "url": "/views/ci/InformationManager/card_indv_info.json",
                            "method": "POST"
                        },
                        "props": {
                            "width": "85%",
                            "title": "绑卡签约信息详情"
                        }
                    }]
                }
            ]
        }
    }
}