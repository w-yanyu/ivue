{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "name": "info",
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "us0005",
          "cust_no": "${{params.cust_no}}"
        }
      },
      "btns": [
        {
          "type": "custom",
          "label": "修改",
          "icon": "",
          "toRequest": {
            "url": "/views/ci/InformationManager/customer_info_person_update.json",
            "method": "GET",
            "target": "dialog",
            "params": {
              "cust_no": "${{params.cust_no}}"
            }
          },
          "eventAfter": {
            "callback": "resetCallback"
          },
          "props": {
            "width": "80%",
            "title": "客户开户信息维护"
          }
        }
      ],
      "controlsGroup": [
        {
          "title": "客户开户信息",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "accordion": false,
          "expand": true,
          "controls": [
            {
              "cust_name": {
                "control": "string",
                "label": "客户名称",
                "span": "8",
                "width": "25px"
              },
              "cust_befor_name": {
                "control": "text",
                "label": "客户曾用名",
                "span": "8",
                "width": "25px",
                "disabled": "true"
              },
              "cust_english_name": {
                "control": "text",
                "label": "客户英文名称",
                "span": "8",
                "width": "25px",
                "disabled": "true"
              },
              "country_id": {
                "control": "select",
                "label": "国籍",
                "disabled": "true",
                "span": "8",
                "width": "25px",
                "dict": {
                  "dictType": "US_E_INCTRY",
                  "dictKey": [
                    "US_E_INCTRY"
                  ],
                  "format": "label"
                }
              },
              "cust_class": {
                "control": "select",
                "label": "客户类别",
                "disabled": "true",
                "span": "8",
                "width": "25px",
                "dict": {
                  "dictType": "US_E_CUCLAS",
                  "dictKey": [
                    "US_E_CUCLAS"
                  ],
                  "format": "label"
                }
              },
              "cert_type": {
                "control": "select",
                "label": "证件类型(征)",
                "span": "8",
                "width": "25px",
                "disabled": "true",
                "dict": {
                  "dictType": "MS_E_CRET_TYPE",
                  "dictKey": [
                    "MS_E_CRET_TYPE"
                  ],
                  "format": "label"
                }
              },
              "cert_no": {
                "control": "text",
                "label": "证件号码(征)",
                "span": "8",
                "width": "25px",
                "disabled": "true"
              },
              "cert_day": {
                "label": "证件签发日期",
                "width": "25px",
                "control": "dateTimePicker",
                "span": "8",
                "disabled": "true",
                "format": "yyyyMMdd"
              },
              "card_end_day": {
                "label": "证件到期日期",
                "width": "25px",
                "span": "8",
                "control": "dateTimePicker",
                "disabled": "true",
                "format": "yyyyMMdd"
              },
              "certct": {
                "control": "areaCascade",
                "title": "请选择注册地址",
                "label": "注册地址",
                "span": "8",
                "width": "25px",
                "disabled": "true"
              },
              "cust_type": {
                "control": "select",
                "label": "客户类型",
                "span": "8",
                "width": "25px",
                "dict": {
                  "dictType": "US_E_CUST_TYPE",
                  "dictKey": [
                    "US_E_CUST_TYPE"
                  ],
                  "format": "label"
                },
                "disabled": "true"
              }
            },
            {
              "islpac": {
                "control": "hidden",
                "label": "是否法人关联客户",
                "span": "8",
                "width": "25px",
                "dict": {
                  "dictType": "MS_E_YESORNO",
                  "dictKey": [
                    "MS_E_YESORNO"
                  ],
                  "format": "label"
                }
              }
            }
          ]
        },
        {
          "title": "客户号列表",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "expand": true,
          "controls": [
            {
              "cust_no_out": {
                "control": "string",
                "label": "客户编号",
                "span": "8",
                "width": "25px"
              },
              "acctno_type": {
                "control": "string",
                "label": "账户类型",
                "span": "8",
                "width": "25px"
              },
              "remark": {
                "control": "string",
                "label": "备注",
                "span": "8",
                "width": "25px"
              }
            }
          ]
        }
      ]
    }
  }
}
