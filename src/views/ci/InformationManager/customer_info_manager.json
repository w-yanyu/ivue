{
    "module": "v_search_btn_datagrid",
    "pageId":"####",
    "layout": {
        "form": {
            "controls": [{
                "custid": {
                    "control": "text",
                    "label": "客户编号",
                    "rules": [{
                        "max": 30,
                        "message": "长度不能超过30个字符"
                    }]
                },
                "custna": {
                    "control": "text",
                    "label": "客户名称",
                    "rules": [{
                        "max": 80,
                        "message": "长度不能超过80个字符"
                    }]
                },
                "cert_type": {
                    "control": "select",
                    "label": "证件类型",
                    "dict": {
                        "dictType": "US_E_CRET_TYPE",
                        "dictKey": [
                            "US_E_CRET_TYPE"
                        ],
                        "labelField": "dictName",
                        "valueField": "dictId"
                    }
                },
                "cert_no": {
                    "control": "text",
                    "label": "证件号码"
                },
                "is_valid": {
                    "control": "select",
                    "label": "是否有效",
                    "dict": {
                        "dictType": "E_YES_",
                        "dictKey": [
                            "E_YES_"
                        ],
                        "labelField": "dictName",
                        "valueField": "dictId"
                    }
                }
            }]
        },
        "toolbar": {
            "btns": [{
                "type": "add",
                "target": "dialog",
                "label": "新增",
                "toRequest": {
                    "method": "GET",
                    "url": "/views/ci/InformationManager/customer_info_public_add.json"
                },
                "props": {
                    "width": "90%",
                    "title": "对公客户建立"
                }
            }]
        },
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": false,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "us0001",
                    "indvfg": "N",
                    "page_id": "/views/ci/InformationManager/customer_info_manager.json"
                }
            },
            "thead": [{
                    "type": "index",
                    "label": "序号"
                },
                {
                    "label": "客户编号",
                    "prop": "custid",
                    "align": "center"
                },
                {
                    "label": "客户名称",
                    "prop": "custna",
                    "align": "center"
                },
                {
                    "label": "客户性质",
                    "prop": "cust_nature",
                    "align": "center",
                    "dict": {
                        "dictType": "US_E_LEGAL_CUST_PROPERTY",
                        "dictKey": [
                            "US_E_LEGAL_CUST_PROPERTY"
                        ],
                        "labelField": "dictName",
                        "valueField": "dictId"
                    }
                },
                {
                    "label": "证件类型",
                    "prop": "cert_type",
                    "align": "center",
                    "dict": {
                        "dictType": "US_E_CRET_TYPE",
                        "dictKey": [
                            "US_E_CRET_TYPE"
                        ],
                        "labelField": "dictName",
                        "valueField": "dictId"
                    }
                },
                {
                    "label": "证件号码",
                    "prop": "cert_no",
                    "align": "center"
                },
                {
                    "label": "信评模板编号",
                    "prop": "credit_template_no",
                    "align": "center"
                },
                {
                    "label": "信评模板名称",
                    "prop": "credit_template_name",
                    "align": "center"
                },
                {
                    "label": "是否有效",
                    "prop": "is_valid",
                    "align": "center",
                    "dict": {
                        "dictType": "MS_E_YESORNO",
                        "dictKey": [
                            "MS_E_YESORNO"
                        ],
                        "labelField": "dictName",
                        "valueField": "dictId"
                    }
                },
                {
                    "label": "操作",
                    "width": "210px",
                    "align": "center",
                    "btns": [{
                            "type": "custom",
                            "authId":"F0101",
                            "label": "详细信息",
                            "show": {
                                "prop": "indvfg",
                                "is": [
                                    "N"
                                ]
                            },
                            "toRequest": {
                                "url": "/views/us/usManager/user_info.json",
                                "props": {
                                    "width": "95%",
                                    "height": "590px",
                                    "title": "对公客户信息"
                                }
                            }
                        },
                        {
                            "type": "edit",
                            "show": {
                                "prop": "is_valid",
                                "is": [
                                    "N"
                                ]
                            },
                            "label": "信评模板",
                            "toRequest": {
                                "url": "/views/ci/InformationManager/sel_credit_template.json",
                                "props": {
                                    "width": "80%",
                                    "title": "信评模板选择"
                                }
                            }
                        },
                        {
                            "type": "edit",
                            "show": {
                                "prop": "is_valid",
                                "is": [
                                    "N"
                                ]
                            },
                            "label": "状态审核",
                            "doRequest": {
                                "url": "/SUMP/icmcall/icmRPCCall",
                                "method": "POST",
                                "params": {
                                    "servicecode": "IUMSCUTS4011"
                                }
                            }
                        }
                    ]
                }
            ]
        }
    }
}