{
    "module": "v_search_btn_datagrid",
    "layout": {

        "toolbar": {
            "btns": []
        },
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": true,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "IUMSCUTS0002",
                    "custid": "${{params.custid}}"

                }
            },
            "pagination": {
                "show": false
            },
            "thead": [{
                    "type": "index",
                    "label": "序号"
                },
                {
                    "label": "客户号",
                    "prop": "custid",
                    "align": "center"
                },
                {
                    "label": "客户名称",
                    "prop": "custna",
                    "align": "center"
                },

                {
                    "label": "渠道号",
                    "prop": "chanel",
                    "align": "center"
                },

                {
                    "label": "渠道名称",
                    "prop": "chanelName",
                    "align": "center"

                },
                {
                    "label": "操作",
                    "align": "center",
                    "btns": [{
                        "type": "look",
                        "label": "详情",
                        "toRequest": {
                            "url": "/views/ci/InformationManager/user_indv_info.json",
                            "method": "POST"
                        },
                        "props": {
                            "width": "85%",
                            "title": "用户信息详情"
                        }
                    }]
                }
            ]
        }
    }
}