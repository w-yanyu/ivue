{
    "module": "v_search_btn_datagrid",
    "layout": {

        "toolbar": {
            "btns": []
        },
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": true,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "IUMSCUTS0006",
                    "custid": "${{params.custid}}",
                    "pageno": "1",
                    "pagesz": "10",
                    "trantp": "SELECT"

                }
            },
            "thead": [{
                    "type": "index",
                    "label": "序号"
                },
                {
                    "label": "客户编号",
                    "prop": "custid",
                    "align": "center"
                },
                {
                    "label": "联系人姓名",
                    "prop": "connam",
                    "align": "center"
                },
                {
                    "label": "联系人关系",
                    "prop": "conntp",
                    "align": "center",

                    "dict": {
                        "dictType": "E_IDTFTP",
                        "dictKey": [
                            "E_IDTFTP"
                        ],
                        "labelField": "dictName",
                        "valueField": "dictId"
                    }
                },
                {
                    "label": "证件类型",
                    "prop": "certtp",
                    "align": "center",

                    "dict": {
                        "dictType": "E_PUBLIC",
                        "dictKey": [
                            "E_PUBLIC"
                        ],
                        "labelField": "dictName",
                        "valueField": "dictId"
                    }
                },
                {
                    "label": "证件号码",
                    "prop": "certno",
                    "align": "center"
                },
                {
                    "label": "电话",
                    "prop": "phonum",
                    "align": "center"
                },
                {
                    "label": "手机",
                    "prop": "mobino",
                    "align": "center",
                    "dict": {
                        "dictType": "MS_E_GENDER",
                        "dictKey": [
                            "DICT_ID"
                        ],
                        "labelField": "dictName",
                        "valueField": "dictId"
                    }
                },
                {
                    "label": "联系单位",
                    "prop": "conndw",
                    "align": "center"

                },
                {
                    "label": "工作地址",
                    "prop": "addres",
                    "align": "center"

                },
                {
                    "label": "产品号",
                    "prop": "prodcd",
                    "align": "center"

                }
            ]
        }
    }
}