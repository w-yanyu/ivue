{
    "module": "v-inputDataForm-btn",
    "layout": {
        "form": {
          "initDataOnce":true,
          "initRequest":"${{params}}",
            "btns": [],
            "controlsGroup": [
                {
                    "title": "绑卡签约信息",
                    "tooltip": "",
                    "titleIcon": "",
                    "showHint": "",
                    "accordion": false,
                    "expand": true,
                    "controls": [
                        {
                            "custid": {
                                "control": "string",
                                "label": "客户号"
                            },
                            "outUserId": {
                                "control": "string",
                                "label": "外部客户号"
                            }
                        },
                        {
                            "certtp": {
                                "control": "string",
                                "label": "证件类型",
                                "dict": {
                                    "dictType": "E_PUBLIC",
                                    "dictKey": [
                                        "E_PUBLIC"
                                    ],
                                    "labelField": "dictName",
                                    "valueField": "dictId"
                                }
                            },
                            "certno": {
                                "control": "string",
                                "label": "证件号码"
                            }
                        },
                      {
                        
                        "channalId": {
                            "control": "string",
                            "label": "渠道号",
                            "dict": {
                                "dictType": "E_CHANEL_ID",
                                "dictKey": [
                                    "E_CHANEL_ID"
                                ],
                                "labelField": "dictName",
                                "valueField": "dictID"
                            }
                        },
                        "cardNo": {
                          "control": "string",
                          "label": "卡号"
                        }
                      },
                        {
                            "cardBin": {
                                "control": "string",
                                "label": "卡bin"
                            },
                            "cardType": {
                                "control": "string",
                                "label": "银行卡类型",
                                "dict": {
                                    "dictType": "E_BANKTP",
                                    "dictKey": [
                                        "E_BANKTP"
                                    ],
                                    "labelField": "dictName",
                                    "valueField": "dictId"
                                }
                            }
                        },
                       
                        {
                            "bankNo": {
                                "control": "string",
                                "label": "开户行行号"
                            },
                            "bankName": {
                                "control": "string",
                                "label": "开户行名称"
                            }
                        },
                        {
                            "bankProvince": {
                                "control": "string",
                                "label": "开户行所在省份"
                            },
                            "bankCity": {
                                "control": "string",
                                "label": "开户行所在城市"
                            }
                        },
                                            
                        {
                            "mobileNo": {
                                "control": "string",
                                "label": "银行卡预留手机号"
                            }
                        }
                    ]
                }    
            
            ]
        }
    }
}
