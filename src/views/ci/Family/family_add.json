{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "btns": [{
                "type": "submit",
                "label": "提交",
                "doRequest": {
                    "url": "/SUMP/icmcall/icmRPCCall",
                    "method": "POST",
                    "params": {
                        "servicecode": "IUMSCUTS0011"
                    }
                }
            }],
            "controls": [{
                "custid": {
                    "control": "hidden",
                    "label": "客户编号",
                    "width": "10px",
                    "disabled": true
                },
                "falytp": {
                    "label": "关联类型",
                    "control": "select",
                    "width": "10px",
                    "dict": {
                        "dictType": "E_FALYTP",
                        "dictKey": [
                            "E_FALYTP"
                        ],
                        "format": "label"
                    },
                    "rules": [{
                        "required": true,
                        "message": "请选择关联类型"
                    }]
                },
                "custna": {
                    "control": "lookup",
                    "width": "10px",
                    "label": "家族成员姓名",
                    "edit": true,
                    "rules": [{
                        "required": true,
                        "max": 80,
                        "message": "请选择成员"
                    }],
                    "keyField": {
                        "custna": "custna",
                        "cetype": "certtp",
                        "certno": "certno"
                    },
                    "toRequest": {
                        "url": "/views/ci/Family/family_customer_info.json",
                        "method": "get"
                    },
                    "props": {
                        "title": "选择本行客户",
                        "width": "65%"
                    }
                },
                "cetype": {
                    "label": "证件类型",
                    "width": "10px",
                    "control": "select",
                    "disabled": true,
                    "dict": {
                        "dictType": "E_IDTFTP",
                        "dictKey": [
                            "E_IDTFTP"
                        ],
                        "format": "label"
                    },
                    "rules": [{
                        "message": "请选择证件类型"
                    }]
                },
                "certno": {
                    "control": "string",
                    "width": "10px",
                    "label": "证件号码",
                    "rules": [{
                        "required": true,
                        "pattern": "^[A-Za-z0-9]+$",
                        "max": 50,
                        "message": "请输入正确的证件号码,长度50字符以内"
                    }]
                },
                "cuorcd": {
                    "control": "text",
                    "width": "10px",
                    "label": "家族企业组织机构代码",
                    "value": "${{params.corgcd}}",
                    "disabled": true,
                    "rules": [{
                        "required": true,
                        "max": 30,
                        "message": "请输入正确的组织机构代码,长度30字符以内"
                    }]
                },
                "farela": {
                    "label": "家族关系",
                    "width": "10px",
                    "control": "select",
                    "dict": {
                        "dictType": "E_FALATP",
                        "dictKey": [
                            "E_FALATP"
                        ],
                        "format": "label"
                    },
                    "rules": [{
                        "required": true,
                        "message": "请选择家族关系"
                    }]
                },
                "custat": {
                    "label": "是否有效",
                    "width": "10px",
                    "control": "select",
                    "dict": {
                        "dictType": "E_CUSTAT",
                        "dictKey": [
                            "E_CUSTAT"
                        ],
                        "format": "label"
                    },
                    "rules": [{
                        "required": true,
                        "message": "请选择是否有效"
                    }]
                },
                "orgna": {
                    "control": "text",
                    "width": "10px",
                    "label": "家族成员所在机构名称",
                    "rules": [{
                        "required": true,
                        "max": 80,
                        "message": "请输入机构名称,长度80字符以内"
                    }]
                }
            }]
        }
    }
}