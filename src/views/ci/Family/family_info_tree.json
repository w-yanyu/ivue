{
  "module": "v_search_btn_datagrid",
  "layout": {
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "target": "dialog",
          "label": "新增",
          "toRequest": {
            "method": "GET",
            "url": "/views/ci/Family/family_add.json",
            "params": {
              "custid": "${{params.custid}}",
              "corgcd": "${{params.corgcd}}"
            }
          },
          "show": {
            "expression": "'${{params.show_flash}}' == '1'"
          },
          "props": {
            "width": "75%",
            "title": "家族企业成员信息新增"
          }
        }
      ]
    },
    "datagrid": {
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "IUMSCUTS0012",
          "custid": "${{params.custid}}"
        }
      },
      "thead": [
        {
          "type": "index",
          "label": "序号",
          "align": "center"
        },
        {
          "label": "关系",
          "prop": "falytp",
          "align": "center",
          "dict": {
            "dictType": "E_FALYTP",
            "dictKey": [
              "E_FALYTP"
            ],
            "format": "label"
          }
        },
        {
          "label": "家族成员姓名",
          "prop": "custna",
          "align": "center"
        },
        {
          "label": "证件类型",
          "prop": "cetype",
          "align": "center",
          "dict": {
            "dictType": "E_IDTFTP",
            "dictKey": [
              "E_IDTFTP"
            ],
            "format": "label"
          }
        },
        {
          "label": "证件号码",
          "prop": "certno",
          "align": "center"
        },
        {
          "label": "家族关系",
          "prop": "farela",
          "align": "center",
          "dict": {
            "dictType": "E_FALATP",
            "dictKey": [
              "E_FALATP"
            ],
            "format": "label"
          }
        },
        {
          "label": "所在机构名称",
          "prop": "orgna",
          "align": "center"
        },
        {
          "label": "有效状态",
          "prop": "custat",
          "align": "center",
          "dict": {
            "dictType": "E_CUSTAT",
            "dictKey": [
              "E_CUSTAT"
            ],
            "format": "label"
          }
        },
        {
          "label": "操作",
          "fixed": "right",
          "align": "center",
          "width": 160,
          "btns": [
            {
              "type": "look",
              "label": "详情",
              "toRequest": {
                "url": "/views/ci/Family/family__detail.json",
                "method": "POST"
              },
              "props": {
                "width": "75%",
                "title": "家族企业成员信息详情"
              }
            },
            {
              "type": "edit",
              "label": "维护",
              "toRequest": {
                "url": "/views/ci/Family/family_modify.json",
                "method": "GET"
              },
              "show": {
                "expression": "'${{params.show_flash}}' == '1'"
              },
              "props": {
                "width": "75%",
                "title": "家族企业成员信息维护"
              }
            },
            {
              "type": "delete",
              "label": "删除",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "IUMSCUTS0010"
                }
              },
              "show": {
                "expression": "'${{params.show_flash}}' == '1'"
              }
            }
          ]
        }
      ]
    }
  }
}
