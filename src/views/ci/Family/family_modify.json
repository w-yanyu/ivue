{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "btns": [{
                "type": "submit",
                "label": "提交",
                "doRequest": {
                    "url": "/SUMP/icmcall/icmRPCCall",
                    "method": "POST",
                    "params": {
                        "servicecode": "IUMSCUTS0013",
                        "falyid": "${{params.falyid}}"
                    }
                }
            }],
            "controls": [{
                "custid": {
                    "control": "hidden",
                    "width": "10px",
                    "label": "客户编号",
                    "rules": [{
                        "required": true,
                        "max": 19,
                        "message": "请输入客户编号,长度19字符以内"
                    }]
                },
                "falytp": {
                    "label": "关联类型",
                    "width": "10px",
                    "control": "select",
                    "dict": {
                        "dictType": "E_FALYTP",
                        "dictKey": [
                            "E_FALYTP"
                        ],
                        "format": "label"
                    },
                    "rules": [{
                        "required": true,
                        "message": "请选择关联类型"
                    }]
                },
                "custna": {
                    "control": "string",
                    "width": "10px",
                    "label": "家族成员姓名",
                    "rules": [{
                        "required": true,
                        "max": 80,
                        "message": "请输入家族成员姓名,长度80字符以内"
                    }]
                },
                "cetype": {
                    "label": "证件类型",
                    "width": "10px",
                    "control": "string",
                    "dict": {
                        "dictType": "E_IDTFTP",
                        "dictKey": [
                            "E_IDTFTP"
                        ],
                        "format": "label"
                    },
                    "rules": [{
                        "required": true,
                        "message": "请选择证件类型"
                    }]
                },
                "certno": {
                    "control": "string",
                    "width": "10px",
                    "label": "证件号码",
                    "rules": [{
                        "required": true,
                        "pattern": "^[A-Za-z0-9]+$",
                        "max": 50,
                        "message": "请输入证件号码,长度50字符以内"
                    }]
                },
                "cuorcd": {
                    "control": "string",
                    "label": "家族企业组织机构代码",
                    "width": "10px",
                    "rules": [{
                        "required": true,
                        "pattern": "^[0-9]+$",
                        "max": 30,
                        "message": "请输入正确的组织机构代码,长度30字符以内"
                    }]
                },
                "farela": {
                    "label": "家族关系",
                    "width": "10px",
                    "control": "select",
                    "dict": {
                        "dictType": "E_FALATP",
                        "dictKey": [
                            "E_FALATP"
                        ],
                        "format": "label"
                    },
                    "rules": [{
                        "required": true,
                        "message": "请选择家族关系"
                    }],
                    "disabled":"true"
                },
                "custat": {
                    "label": "是否有效",
                    "width": "10px",
                    "control": "select",
                    "dict": {
                        "dictType": "E_CUSTAT",
                        "dictKey": [
                            "E_CUSTAT"
                        ],
                        "format": "label"
                    },
                    "rules": [{
                        "required": true,
                        "message": "请选择是否有效"
                    }]
                },
                "orgna": {
                    "control": "text",
                    "width": "10px",
                    "label": "机构名称",
                    "rules": [{
                        "required": false,
                        "max": 80,
                        "message": "请输入机构名称,长度80字符以内"
                    }]
                }
            }]
        }
    }
}