{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "controls": [{
                "custna": {
                    "span": 8,
                    "control": "text",
                    "label": "客户名称",
                    "placeholder": "请输入客户名称"
                },
                "certno": {
                    "span": 8,
                    "control": "text",
                    "label": "证件号码",
                    "placeholder": "请输入证件号码"
                }
            }]
        },
        "datagrid": {
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "IUMSCUTS0004",
                    "indvfg": "Y"
                }
            },
            "thead": [{
                    "type": "index",
                    "label": "序号",
                    "align": "center"
                },
                {
                    "label": "客户名称",
                    "prop": "custna",
                    "align": "center"
                },
                {
                    "label": "证件类型",
                    "prop": "certtp",
                    "align": "center",
                    "dict": {
                        "dictType": "E_IDTFTP",
                        "dictKey": [
                            "E_IDTFTP"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "证件号码",
                    "prop": "certno",
                    "align": "center"
                },
                {
                    "label": "操作",
                    "fixed": "right",
                    "align": "center",
                    "btns": [{
                        "type": "bringback",
                        "label": "选择"
                    }]
                }
            ]
        }
    }
}