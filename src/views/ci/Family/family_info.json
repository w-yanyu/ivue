{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "initDataOnce": true,
            "controls": [
                {
                    "falyid": {
                        "label": "流水ID",
                        "control": "text",
                        "rules": [
                            {
                                "max": 19,
                                "message": "长度不能超过19个字符"
                            }
                        ]
                    }
                }
            ]
        },
        "toolbar": {
            "btns": [
                {
                    "type": "add",
                    "target": "dialog",
                    "label": "新增",
                    "toRequest": {
                        "method": "GET",
                        "url": "/views/ci/Family/family_add.json"
                    },
                    "props": {
                        "width": "65%",
                        "title": "家族企业成员信息新增"
                    }
                }
            ]
        },
        "datagrid": {
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "post",
                "params": {
                    "servicecode": "IUMSCUTS0012"
                }
            },
            "thead": [
                {
                    "type": "index",
                    "label": "序号"
                },
                {
                    "label": "关系",
                    "prop": "falytp",
                    "dict": {
                        "dictType": "E_FALYTP",
                        "dictKey": [
                            "E_FALYTP"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "家族成员姓名",
                    "prop": "custna"
                },
                {
                    "label": "证件类型",
                    "prop": "cetype",
                    "dict": {
                        "dictType": "E_PUBLIC",
                        "dictKey": [
                            "E_PUBLIC"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "证件号码",
                    "prop": "certno"
                },
                {
                    "label": "家族关系",
                    "prop": "farela",
                    "dict": {
                        "dictType": "E_FALATP",
                        "dictKey": [
                            "E_FALATP"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "所在机构名称",
                    "prop": "orgna"
                },
                {
                    "label": "有效状态",
                    "prop": "custat",
                    "dict": {
                        "dictType": "E_CUSTAT",
                        "dictKey": [
                            "E_CUSTAT"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "操作",
                    "width": "130px",
                    "btns": [
                        {
                            "type": "edit",
                            "label": "维护",
                            "toRequest": {
                                "url": "/views/ci/Family/family_modify.json",
                                "method": "GET"
                            },
                            "props": {
                                "width": "65%",
                                "title": "家族企业成员信息维护"
                            }
                        },
                        {
                            "type": "delete",
                            "label": "删除",
                            "doRequest": {
                                "url": "/SUMP/icmcall/icmRPCCall",
                                "method": "POST",
                                "params": {
                                    "servicecode": "IUMSCUTS0010"
                                }
                            }
                        }
                    ]
                }
            ]
        }
    }
}