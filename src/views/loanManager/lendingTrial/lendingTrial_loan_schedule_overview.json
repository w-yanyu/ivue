{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest":"${{params}}",
            "initDataOnce":true,
            "btns": [],
            "controls": [
                {
                    "stnlno": {
                        "control": "text",
                        "label": "放款流水号",
                        "disabled": true,
                        "width": "130px",
                        "rules": [
                            {
                                "max": 30,
                                "message": "长度不能超过30个字符"
                            }
                        ]
                    },
                    "prinam": {
                        "control": "text",
                        "width": "130px",
                        "label": "还款本金",
                        "disabled": true,
                        "suffix": "元",
                        "format": {
                            "thousand": ",",
                            "decimal": "2",
                            "type": "currency"
                        }
                    },
                    "estint": {
                        "control": "text",
                        "width": "130px",
                        "label": "预估利息",
                        "disabled": true,
                        "suffix": "元",
                        "format": {
                            "thousand": ",",
                            "decimal": "2",
                            "type": "currency"
                        }
                    },
                    "totafe": {
                        "control": "text",
                        "width": "130px",
                        "label": "还款总费用",
                        "disabled": true,
                        "suffix": "元",
                        "format": {
                            "thousand": ",",
                            "decimal": "2",
                            "type": "currency"
                        }
                    },
                    "foremo": {
                        "control": "text",
                        "width": "130px",
                        "label": "预计每月还款额",
                        "disabled": true,
                        "suffix": "元",
                        "format": {
                            "thousand": ",",
                            "decimal": "2",
                            "type": "currency"
                        }
                    },
                    "daintr": {
                        "control": "text",
                        "width": "130px",
                        "label": "日利率",
                        "disabled": true,
                        "suffix": "%",
                        "format": {
                            "thousand": ",",
                            "decimal": "2",
                            "type": "currency"
                        }
                    },
                    "totamo": {
                        "control": "text",
                        "width": "130px",
                        "label": "还款总额",
                        "disabled": true,
                        "suffix": "元",
                        "format": {
                            "thousand": ",",
                            "decimal": "2",
                            "type": "currency"
                        }
                    }
                }
            ]
        }
    }
}