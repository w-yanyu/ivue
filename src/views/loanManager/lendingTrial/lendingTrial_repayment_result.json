{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "initDataOnce": true,
            "btns": [],
            "controlsGroup": [
                {
                    "title": "试算结果",
                    "tooltip": "",
                    "titleIcon": "",
                    "showHint": "",
                    "accordion": true,
                    "expand": true,
                    "controls": [
                        {
                            "cust_nm": {
                                "control": "text",
                                "label": "客户名称",
                                "disabled": true,
                                "width": "130px",
                                "span": 8
                            },
                            "loan_due_bill_num": {
                                "control": "text",
                                "width": "130px",
                                "label": "贷款借据号",
                                "disabled": true,
                                "span": 8
                            },
                            "acct_bal": {
                                "control": "currency",
                                "width": "130px",
                                "label": "还贷前贷款余额",
                                "disabled": true,
                                "align": "left",
                                "suffix": "元",
                                "span": 8
                            },
                            "arag_totl_amt": {
                                "control": "currency",
                                "width": "130px",
                                "label": "欠款总额",
                                "disabled": true,
                                "align": "left",
                                "suffix": "元",
                                "span": 8
                            },
                            "rpymt_way": {
                                "control": "select",
                                "label": "还款方式",
                                "width": "130px",
                                "disabled": true,
                                "dict": {
                                    "dictType": "LN_E_RPYMT_WAY",
                                    "dictKey": [
                                        "LN_E_RPYMT_WAY"
                                    ],
                                    "format": "label"
                                },
                                "span": 8
                            },
                            "rcvbl_acd_int": {
                                "control": "currency",
                                "label": "提前还贷利息",
                                "disabled": true,
                                "width": "130px",
                                "align": "left",
                                "suffix": "元",
                                "span": 8
                            },
                            "rcvbl_fine": {
                                "control": "currency",
                                "label": "提前还贷违约金",
                                "disabled": true,
                                "width": "130px",
                                "align": "left",
                                "suffix": "元",
                                "span": 8
                            },
                            "rpymt_amt": {
                                "control": "currency",
                                "label": "本次还贷总金额",
                                "disabled": true,
                                "width": "130px",
                                "align": "left",
                                "suffix": "元",
                                "span": 8
                            },
                            "due_bill_amt": {
                                "control": "currency",
                                "label": "还贷后剩余金额",
                                "disabled": true,
                                "width": "130px",
                                "align": "left",
                                "suffix": "元",
                                "span": 8
                            },
                            "rsdl_prds": {
                                "control": "text",
                                "label": "剩余期数",
                                "disabled": true,
                                "width": "130px",
                                "span": 8
                            },
                            "nrl_int_rate": {
                                "control": "text",
                                "label": "正常利率",
                                "disabled": true,
                                "width": "130px",
                                "span": 8
                            },
                            "lstSmilrCrCardBillDetl": {
                                "span": 24,
                                "control": "dynamic",
                                "toRequest": {
                                    "url": "/views/loanManager/lendingTrial/apportioned_cost.json",
                                    "params": {
                                        "adv_rpymt_adjust_plan_way": "${{params.adv_rpymt_adjust_plan_way}}",
                                        "adv_rpymt_catg":  "${{params.adv_rpymt_catg}}",
                                        "adv_rpymt_int_rpymt_way":  "${{params.adv_rpymt_int_rpymt_way}}",
                                        "aptmt_rpymt_dt":  "${{params.aptmt_rpymt_dt}}",
                                        "ccy_code_num":  "${{params.ccy_code_num}}",
                                        "ctrct_num":  "${{params.ctrct_num}}",
                                        "cust_num":  "${{params.cust_num}}",
                                        "int_smtn":  "${{params.int_smtn}}",
                                        "loan_acct_num":  "${{params.loan_acct_num}}",
                                        "loan_due_bill_num":  "${{params.loan_due_bill_num}}",
                                        "prcpl_smtn":  "${{params.prcpl_smtn}}",
                                        "ret_totl_amt":  "${{params.ret_totl_amt}}",
                                        "rpymt_amt":  "${{params.rpymt_amt}}",
                                        "rpymt_catg":  "${{params.rpymt_catg}}"
                                      }
                                }
                            }
                        }
                    ]
                }
            ]
        }
    }
}