{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "loan_amt": {
            "control": "text",
            "label": "贷款金额",
            "suffix": "元",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入贷款金额"
              }
            ]
          },
          "int_caln_rule": {
            "control": "select",
            "label": "计息规则",
            "width": "130px",
            "selectFirstValue": true,
            "dict": {
              "dictType": "LN_E_INT_CALN_DYS_WAY",
              "dictKey": [
                "LN_E_INT_CALN_DYS_WAY"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择计息规则"
              }
            ]
          },
          "actl_anul_moly_int_rate": {
            "control": "select",
            "label": "实际年月利率",
            "selectLastValue": true,
            "dict": {
              "dictType": "MS_E_INST_FLAG",
              "dictKey": [
                "MS_E_INST_FLAG"
              ],
              "format": "label"
            },
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请选择实际年月利率"
              }
            ]
          },
          "nrl_int_rate": {
            "control": "text",
            "label": "正常利率",
            "width": "130px",
            "suffix": "%",
            "rules": [
              {
                "required": true,
                "message": "请输入正常利率"
              }
            ]
          },
          "rpymt_matu_t": {
            "label": "还款期限",
            "control": "text",
            "width": "130px",
            "span": "6",
            "rules": [
              {
                "required": true,
                "message": "请输入还款期限"
              }
            ]
          },
          "rpymt_matu_s": {
            "control": "select",
            "multiple": false,
            "value": "M",
            "span": "6",
            "options": [
              {
                "label": "日",
                "value": "D"
              },
              {
                "label": "周",
                "value": "W"
              },
              {
                "label": "旬",
                "value": "X"
              },
              {
                "label": "月",
                "value": "M"
              },
              {
                "label": "季",
                "value": "Q"
              },
              {
                "label": "年",
                "value": "Y"
              }
            ],
            "format": "label"
          },
          "strt_int_caln_dt": {
            "control": "dateTimePicker",
            "label": "起息日期",
            "format": "yyyyMMdd",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入起息日期"
              }
            ]
          },
          "rpymt_way": {
            "control": "select",
            "label": "还款方式",
            "dict": {
              "dictType": "LN_E_RPYMT_WAY",
              "dictKey": [
                "LN_E_RPYMT_WAY"
              ],
              "format": "label"
            },
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请选择还款方式"
              }
            ]
          },
          "eqvlt_deal_rule": {
            "control": "select",
            "label": "等额处理规则",
            "dict": {
              "dictType": "LN_E_EQVLT_DEAL_RULE",
              "dictKey": [
                "LN_E_EQVLT_DEAL_RULE"
              ],
              "format": "label"
            },
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请选择等额处理规则"
              }
            ]
          },
          "first_rpymt_dt_mode": {
            "control": "select",
            "label": "首次还款日确定模式",
            "value": "2",
            "dict": {
              "dictType": "LN_E_FIRST_RPYMT_DT_CFRM_MODE",
              "dictKey": [
                "LN_E_FIRST_RPYMT_DT_CFRM_MODE"
              ],
              "format": "label"
            },
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请选择首次还款日确定模式"
              }
            ]
          },
          "final_prd_rpymt_way": {
            "control": "select",
            "label": "末期还款方式",
            "value": "1",
            "dict": {
              "dictType": "LN_E_FINAL_PRD_RPYMT_WAY",
              "dictKey": [
                "LN_E_FINAL_PRD_RPYMT_WAY"
              ],
              "format": "label"
            },
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请选择末期还款方式"
              }
            ]
          },
          "prcpl_in_sects_flg": {
            "control": "select",
            "label": "本金分段标志",
            "selectFirstValue": true,
            "dict": {
              "dictType": "LN_E_PRCPL_IN_SECTS",
              "dictKey": [
                "LN_E_PRCPL_IN_SECTS"
              ],
              "format": "label"
            },
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请选择本金分段标志"
              }
            ]
          },
          "custzn_rpymt_plan_flg": {
            "control": "select",
            "label": "定制还款计划",
            "selectLastValue": true,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            },
            "width": "130px"
          },
          "rpymt_prd": {
            "control": "periodGet",
            "label": "还款周期",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入还款周期"
              }
            ]
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ln3110",
          "rpymt_matu": "${{current.rpymt_matu_t}}${{current.rpymt_matu_s}}",
          "outlistid": "lnAcctRpymtType"
        }
      },
      "thead": [
        {
          "label": "本期期数",
          "align": "center",
          "type": "text",
          "prop": "this_prd_prds"
        },
        {
          "label": "起息日期",
          "align": "center",
          "type": "dateTimePicker",
          "format": "yyyyMMdd",
          "prop": "strt_int_caln_dt"
        },
        {
          "label": "还款日期",
          "align": "center",
          "type": "dateTimePicker",
          "format": "yyyyMMdd",
          "prop": "rpymt_dt"
        },
        {
          "label": "宽限期到期日",
          "align": "center",
          "type": "dateTimePicker",
          "format": "yyyyMMdd",
          "prop": "grace_prd_matu_dt"
        },
        {
          "label": "计息本金",
          "align": "center",
          "type": "text",
          "prop": "int_caln_prcpl"
        },
        {
          "label": "累计产生利息",
          "align": "center",
          "type": "text",
          "prop": "acmld_gen_int"
        },
        {
          "label": "每期还款总额",
          "align": "center",
          "type": "text",
          "prop": "evry_term_rpymt_totl_amt"
        },
        {
          "label": "账户余额",
          "align": "center",
          "type": "text",
          "prop": "acct_bal"
        }
      ]
    }
  }
}
