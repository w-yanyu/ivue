{
    "module": "dynamicTabs",
    "layout": {
        "tabs": {
            "scope": "templateScope",
            "submitBtnPlace": "right",
            "initDataOnce": true,
            "initRequest": "${{params}}",
            "tabHeader": [
                {
                    "id": "baseInfo",
                    "label": "供期总览",
                    "closable": false,
                    "disabled": false,
                    "icon": "el-icon-date",
                    "toRequest": {
                        "url": "views/loanManager/lendingTrial/lendingTrial_universal_schedule_overview.json",
                        "method": "post"
                    }
                },
                {
                    "id": "identityInfo",
                    "label": "期供列表",
                    "icon": "el-icon-date",
                    "closable": false,
                    "disabled": false,
                    "toRequest": {
                        "url": "views/loanManager/lendingTrial/lendingTrial_universal_schedule_list.json",
                        "method": "post"
                    }
                }
               
            ],
            "defaultActiveKey": "baseInfo",
            "position": "top",
            "styleClass": "card"
        }
    }
}