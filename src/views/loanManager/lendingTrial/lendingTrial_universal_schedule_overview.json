{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest":"${{params}}",
            "initDataOnce":true,
            "btns": [],
            "controls": [
                {
                    "tophno": {
                        "control": "text",
                        "label": "总期数",
                        "disabled": true,
                        "width": "130px",
                        "rules": [
                            {
                                "max": 30,
                                "message": "长度不能超过30个字符"
                            }
                        ]
                    },
                    "amount": {
                        "control": "text",
                        "width": "130px",
                        "label": "分期总金额",
                        "disabled": true,
                        "suffix": "元",
                        "format": {
                            "thousand": ",",
                            "decimal": "2",
                            "type": "currency"
                        }
                    },
                    "ljinst": {
                        "control": "text",
                        "width": "130px",
                        "label": "累计产生利息",
                        "disabled": true,
                        "suffix": "元",
                        "format": {
                            "thousand": ",",
                            "decimal": "2",
                            "type": "currency"
                        }
                    },
                    "totafe": {
                        "control": "text",
                        "width": "130px",
                        "label": "累计产生费用",
                        "disabled": true,
                        "suffix": "元",
                        "format": {
                            "thousand": ",",
                            "decimal": "2",
                            "type": "currency"
                        }
                    }
                }
            ]
        }
    }
}