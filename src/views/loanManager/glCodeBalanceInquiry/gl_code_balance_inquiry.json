{
    "module": "v_search_btn_datagrid",
    "layout": {
      "form": {
        "scope": "formData",
        "controls": [
          {
            "trxn_date": {
              "control": "dateTimePicker",
              "label": "交易日期",
              "rules": [
                  {
                      "required": true,
                      "message":"请选择交易日期"
                  }
              ]
            },
            "branch_id": {
              "control": "text",
              "label": "机构号",
              "rules": [
                  {
                      "required": true,
                      "message":"请选择机构号"
                  }
              ]
            }
          },
          {
            "ccy_code": {
              "control": "select",
              "label": "币种",
              "dict": {
                "dictType": "E_CRCYCD_HS",
                "dictKey": [
                  "E_CRCYCD_HS"
                ],
                "format": "label"
              },
              "rules": [
                  {
                      "required": true,
                      "message":"请选择币种"
                  }
              ]
            },
            "gl_code": {
              "control": "text",
              "label": "科目号"
            }
          }
        ]
      },
      "toolbar": {},
      "datagrid": {
        "isInitSearch": false,
        "doRequest": {
          "url": "/SUMP/icmcall/icmRPCCall",
          "method": "POST",
          "params": {
            "servicecode": "IGLSFATS3013",
            "report_type": "1"
          }
        },
        "thead": [
          {
            "type": "index",
            "align": "center"
          },
          {
            "type":"string",
            "label": "交易日期",
            "prop": "trxn_date",
            "align": "center"
          },
          {
            "type":"string",
            "label": "机构号",
            "prop": "branch_id",
            "align": "center"
          },
          {
            "type":"string",
            "label": "币种",
            "prop": "ccy_code",
            "align": "center",
            "dict": {
              "dictType": "E_CRCYCD_HS",
              "dictKey": [
                "E_CRCYCD_HS"
              ],
              "format": "label"
            }
          },
          {
            "type":"string",
            "label": "科目号",
            "prop": "gl_code",
            "align": "center"
          },
          {
            "type":"string",
            "label": "表内标志",
            "prop": "on_bal_sheet_ind",
            "align": "center",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          {
            "type":"string",
            "label": "上期借方余额",
            "prop": "prev_debit_bal",
            "align": "center"
          },
          {
            "type":"string",
            "label": "上期贷方余额",
            "prop": "prev_credit_bal",
            "align": "center"
          },
          {
            "type":"string",
            "label": "本期借方发生额",
            "prop": "current_debit_amt",
            "align": "center"
          },
          {
            "type":"string",
            "label": "本期贷方发生额",
            "prop": "current_credit_amt",
            "align": "center"
          },
          {
            "type":"string",
            "label": "本期借方余额",
            "prop": "current_debit_bal",
            "align": "center"
          },
          {
            "type":"string",
            "label": "本期贷方余额",
            "prop": "current_credit_bal",
            "align": "center"
          }
        ]
      }
    }
  }