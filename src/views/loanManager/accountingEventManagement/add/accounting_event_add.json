{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "scope":"formData",
            "initRequest": "${{params}}",
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "IGLSFATS9003",
                    "acct_seq":"0001"
                }
            },
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": ""
                }
            ],
            "controls": [
                {
                    "sys_no": {
                        "control": "text",
                        "label": "系统编号",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入系统编号"
                            }
                        ]
                    },
                    "accounting_subject": {
                        "control": "select",
                        "label": "会计主体",
                        "width": "130px",
                        "dict": {
                            "dictType": "E_ATOWTP",
                            "dictKey": [
                                "E_ATOWTP"
                            ],
                            "format": "label"
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择会计主体"
                            }
                        ]
                    }
                },
                {
                    "accounting_alias": {
                        "control": "text",
                        "label": "核算别名",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入核算别名"
                            }
                        ]
                    },
                    "remark": {
                        "control": "text",
                        "label": "描述",
                        "width": "130px"
                    }
                },
                {
                    "bal_attributes": {
                        "control": "select",
                        "label": "余额属性",
                        "width": "130px",
                        "name": "bal_attributes",
                        "dict": {
                            "dictType": "E_BLTYPE",
                            "dictKey": [
                                "E_BLTYPE"
                            ],
                            "format": "label"
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择余额属性"
                            }
                        ]
                    },
                    "gl_code": {
                        "control": "text",
                        "label": "科目号",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入科目号"
                            }
                        ]
                    }
                },
                {
                    "offset_gl_code": {
                        "control": "text",
                        "label": "对方科目",
                        "width": "130px"
                    }
                }
            ]
        }
    }
}
