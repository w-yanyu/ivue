{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "caozuolx": "0",
          "servicecode": "edlaye"
        }
      },
      "controls": [
        {
          "epcgcd": {
            "control": "text",
            "label": "罚金编号",
            "rules": [
              {
                "max": 10,
                "message": "不能超过10个字符"
              },
              {
                "required": true,
                "message": "请输入编号"
              },
              {
                "pattern": "^[0-9a-zA-Z]+$",
                "message": "字符不合法"
              }
            ]
          },
          "numbsq": {
            "control": "text",
            "label": "数值序号",
            "rules": [
              {
                "max": 19,
                "message": "不能超过19个字符"
              },
              {
                "required": true,
                "message": "请输入数值序号"
              },
              {
                "pattern": "^\\d+(\\.{0,1}[\\d]{2}){0,1}$",
                "message": "请输入大于或等于零的数值"
              }
            ]
          }
        },
        {
          "termrt": {
            "control": "text",
            "label": "期限/百分比",
            "rules": [
              {
                "max": 19,
                "message": "不能超过19个字符"
              },
              {
                "required": true,
                "message": "请输入期限/百分比"
              },
              {
                "pattern": "^\\d+(\\.{0,1}[\\d]{2}){0,1}$",
                "message": "请输入大于或等于零的数值"
              }
            ]
          },
          "baprpe": {
            "control": "select",
            "label": "罚金基数",
            "rules": [
              {
                "required": true,
                "message": "请输入罚金基数"
              }
            ],
            "dict": {
              "dictType": "E_BAPRPE",
              "dictKey": [
                "E_BAPRPE"
              ],
              "format": "label",
              "labelField": "dictName",
              "valueField": "dictId"
            }
          }
        },
        {
          "ficame": {
            "control": "select",
            "label": "计算方法",
            "rules": [
              {
                "required": true,
                "message": "请输入计算方法"
              }
            ],
            "dict": {
              "dictType": "E_FICAME",
              "dictKey": [
                "E_FICAME"
              ],
              "format": "label",
              "labelField": "dictName",
              "valueField": "dictId"
            }
          },
          "ficart": {
            "control": "text",
            "label": "罚金金额/百分比",
            "rules": [
              {
                "required": true,
                "message": "请输入罚金金额/百分比"
              },
              {
                "pattern": "^\\d+(\\.{0,1}[\\d]{1,2}){0,1}$",
                "message": "请输入大于或等于零的数值"
              }
            ]
          }
        }
      ]
    }
  }
}