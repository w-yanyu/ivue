{
    "module": "v_search_editableDataGrid",
    "layout": {
        "toolbar": {
            "btns": [{
                    "type": "add",
                    "icon": "el-icon-plus",
                    "label": "新增"
                },
                {
                    "type": "delete",
                    "label": "批量删除",
                    "icon": "el-icon-close"
                }
            ]
        },
        "datagrid": {
            "name": "pspyls",
            "pagination": {
                "show": false
            },
            "thead": [{
                    "label": "序号",
                    "type": "index",
                    "align": "center",
                    "prop": "sortno"
                },
                {
                    "label": "还款方式",
                    "align": "center",
                    "type": "select",
                    "prop": "schdtp",
                    "width": "170px",
                    "rules": [{
                        "required": true,
                        "message": "请选择还款方式"
                    }],
                    "dict": {
                        "dictType": "E_SCHDTP",
                        "dictKey": [
                            "E_SCHDTP"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "还款周期",
                    "align": "center",
                    "type": "text",
                    "prop": "repyfm"
                },
                {
                    "label": "起始日期",
                    "align": "center",
                    "type": "dateTimePicker",
                    "format": "yyyyMMdd",
                    "prop": "bgindt",
                    "rules": [{
                        "required": true,
                        "message": "请选择起始日期"
                    }]
                },
                {
                    "label": "到期日期",
                    "align": "center",
                    "type": "dateTimePicker",
                    "format": "yyyyMMdd",
                    "prop": "matudt",
                    "rules": [{
                        "required": true,
                        "message": "请选择起始日期"
                    }]
                },
                {
                    "label": "本阶段还本金额",
                    "align": "center",
                    "type": "text",
                    "prop": "crrpam",
                    "format": {
                        "thousand": ",",
                        "decimal": "2",
                        "type": "currency"
                    }
                },
                {
                    "label": "本阶段附加费用金额",
                    "align": "center",
                    "type": "text",
                    "prop": "adcgam",
                    "format": {
                        "thousand": ",",
                        "decimal": "2",
                        "type": "currency"
                    }
                },
                {
                    "label": "本阶段附加收费代码",
                    "align": "center",
                    "type": "text",
                    "prop": "adcgcd"
                },
                {
                    "label": "本阶段附加收费计算基础",
                    "align": "center",
                    "type": "select",
                    "prop": "adcgbs",
                    "rules": [{
                        "required": true,
                        "message": "请选择本阶段附加收费计算基础"
                    }],
                    "dict": {
                        "dictType": "E_ADCGBS",
                        "dictKey": [
                            "E_ADCGBS"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "操作",
                    "btns": [{
                        "type": "delete",
                        "label": "删除",
                        "props": {
                            "width": "65%",
                            "title": "删除"
                        }
                    }],
                    "align": "center"
                }
            ]
        }

    }
}