{
    "module": "dynamicTabs",
    "layout": {
        "tabs": {
            "scope": "templateScope",
            "submitBtnPlace": "right",
            "initDataOnce": true,
            "initRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "post",
                "params": {
                    "servicecode": "nxschd",
                    "custac": "${{params.custac}}",
                    "crcycd": "${{params.crcycd}}",
                    "tranam": "${{params.tranam}}",
                    "loandt": "${{params.loandt}}",
                    "period": "${{params.period}}",
                    "matudt": "${{params.matudt}}",
                    "repyfm": "${{params.repyfm}}",
                    "schdtp": "${{params.schdtp}}",
                    "prodcd": "${{params.prodcd}}",
                    "irdyfg": "${{params.irdyfg}}",
                    "lnrtir": "${{params.lnrtir}}",
                    "sprdid": "${{params.sprdid}}",
                    "incftp": "${{params.incftp}}",
                    "frstmd": "${{params.frstmd}}",
                    "prisub": "${{params.prisub}}",
                    "pspyls": "${{params.pspyls}}"
                }
            },
            "tabHeader": [
                {
                    "id": "baseInfo",
                    "label": "期供计划总览",
                    "closable": false,
                    "disabled": false,
                    "icon": "el-icon-date",
                    "toRequest": {
                        "url": "views/loanManager/testLendingTrial/lendingTrial_repayment_schedule_overview.json",
                        "method": "post"
                    }
                },
                {
                    "id": "identityInfo",
                    "label": "期供计划列表",
                    "icon": "el-icon-date",
                    "closable": false,
                    "disabled": false,
                    "toRequest": {
                        "url": "views/loanManager/testLendingTrial/lendingTrial_repayment_schedule_list.json",
                        "method": "post"
                    }
                }
               
            ],
            "defaultActiveKey": "baseInfo",
            "position": "top",
            "styleClass": "card"
        }
    }
}