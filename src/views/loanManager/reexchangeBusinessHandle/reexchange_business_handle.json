{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "scope": "formData",
            "controls": [
                {
                    "custna": {
                        "control": "text",
                        "label": "客户姓名"
                    },
                    "idcardno": {
                        "control": "text",
                        "label": "证件号码"
                    }
                },
                {
                    "isthcl": {
                        "control": "select",
                        "label": "退汇是否已处理",
                        "dict": {
                            "dictType": "E_YES___",
                            "dictKey": [
                                "E_YES___"
                            ],
                            "format": "label"
                        }
                    }
                }
            ]
        },
        "toolbar": {},
        "datagrid": {
            "isInitSearch": false,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "AS_ICMS_JK_SELTHF",
                    "cztype": "select"
                }
            },
            "thead": [
                {
                    "type":"string",
                    "label": "客户姓名",
                    "prop": "custna",
                    "align": "center"
                },
                {
                    "type":"string",
                    "label": "证件类型",
                    "prop": "idtype",
                    "align": "center"
                },
                {
                    "type":"string",
                    "label": "证件号码",
                    "prop": "idcardno",
                    "align": "center"
                },
                {
                    "type":"string",
                    "label": "交易流水",
                    "prop": "channelseq",
                    "align": "center"
                },
                {
                    "type":"string",
                    "label": "交易日期",
                    "prop": "transdate",
                    "align": "center"
                },
                {
                    "type":"string",
                    "label": "退汇金额",
                    "prop": "thamt",
                    "align": "center"
                },
                {
                    "type":"string",
                    "label": "退汇账户",
                    "prop": "thaccno",
                    "align": "center"
                },
                {
                    "type":"string",
                    "label": "借据号",
                    "prop": "lncfno",
                    "align": "center"
                },
                {
                    "type":"string",
                    "label": "退汇原因",
                    "prop": "reason",
                    "align": "center"
                },
                {
                    "type":"string",
                    "label": "核心流水号",
                    "prop": "hexseq",
                    "align": "center"
                },
                {
                    "type":"string",
                    "label": "退汇是否已处理",
                    "prop": "isthcl",
                    "align": "center",
                    "dict": {
                        "dictType": "E_YES___",
                        "dictKey": [
                            "E_YES___"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "操作",
                    "fixed": "right",
                    "align": "center",
                    "btns": [
                        {
                            "type": "edit",
                            "label": "维护",
                            "toRequest": {
                                "url": "/views/loanManager/reexchangeBusinessHandle/reexcahange_information_maintain.json",
                                "method": "POST"
                            },
                            "props": {
                                "title": "退汇信息维护"
                            }
                        }
                    ]
                }
            ]
        }
    }
}