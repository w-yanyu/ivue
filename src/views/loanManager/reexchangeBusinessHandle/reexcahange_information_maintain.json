{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "AS_ICMS_JK_SELTHF",
                    "cztype": "modify"
                }
            },
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": ""
                }
            ],
            "controls": [
                {
                    "custna": {
                        "control": "string",
                        "label": "客户姓名"
                    },
                    "idtype": {
                        "control": "string",
                        "label": "证件类型"
                    }
                },
                {
                    "idcardno": {
                        "control": "string",
                        "label": "证件号码"
                    },
                    "channelseq": {
                        "control": "string",
                        "label": "交易流水"
                    }
                },
                {
                    "transdate": {
                        "control": "dateTimePicker",
                        "label": "交易日期"
                    },
                    "thamt": {
                        "control": "string",
                        "label": "退汇金额"
                    }
                },
                {
                    "hexseq": {
                        "control": "string",
                        "label": "核心流水号"
                    },
                    "lncfno": {
                        "control": "string",
                        "label": "借据号"
                    }
                },
                {
                    "reason": {
                        "control": "string",
                        "label": "退汇原因"
                    },
                    "isthcl": {
                        "control": "select",
                        "label": "退汇是否已处理",
                        "dict": {
                            "dictType": "E_YES___",
                            "dictKey": [
                                "E_YES___"
                            ],
                            "format": "label"
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择退汇是否已处理"
                            }
                        ]
                    }
                }
            ]
        }
    }
}
