{
    "module": "dynamicTabs",
    "layout": {
        "tabs": {
            "initDataOnce": true,
            "btns": [],
            "tabHeader": [
                {
                    "id": "inrChgHist",
                    "label": "利率变更历史记录",
                    "closable": false,
                    "disabled": false,
                    "name":"inrChgHist",
                    "icon": "el-icon-date",
                    "toRequest": {
                        "url": "/views/loanManager/intetestRateAdjustment/interest_rate_adjustment_history.json",
                        "method": "post",
                        "params": {
                           "inrChgHist":"${{params.inrChgHist}}"
                          
                        }
                    }
                },
                {
                    "id": "oldPlans",
                    "label": "调整前还款计划",
                    "icon": "el-icon-date",
                    "closable": false,
                    "disabled": false,
                    "toRequest": {
                        "url": "/views/loanManager/intetestRateAdjustment/interest_rate_adjustment_oldplans.json",
                        "method": "post"
                    }
                },
                {
                    "id": "newPlans",
                    "label": "调整后还款计划",
                    "icon": "el-icon-date",
                    "closable": false,
                    "disabled": false,
                    "toRequest": {
                        "url": "/views/loanManager/intetestRateAdjustment/interest_rate_adjustment_newplans.json",
                        "method": "post"
                    }
                }
            ],
            "defaultActiveKey": "inrChgHist",
            "position": "top",
            "styleClass": "card"
        }
    }
}