{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": "",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "post",
                        "params": {
                            "servicecode": "IGLSFATS9001",
                            "list01": "[${{form}}]"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "gl_code": {
                        "control": "text",
                        "label": "科目号",
                        "disabled": true
                    },
                    "gl_code_desc": {
                        "control": "text",
                        "label": "科目名称",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入科目名称"
                            }
                        ]
                    }
                },
                {
                    "upper_lvl_gl_code": {
                        "control": "text",
                        "label": "上级科目",
                        "disabled": true
                    },
                    "gl_code_type": {
                        "control": "select",
                        "label": "科目类别",
                        "dict": {
                            "dictType": "E_SUBJECTTYPE",
                            "dictKey": [
                                "E_SUBJECTTYPE"
                            ],
                            "format": "label"
                        },
                        "disabled": true
                    }
                },
                {
                    "gl_code_level": {
                        "control": "select",
                        "label": "科目级别",
                        "dict": {
                            "dictType": "E_GLCODELEVEL",
                            "dictKey": [
                                "E_GLCODELEVEL"
                            ],
                            "format": "label"
                        },
                        "disabled": true
                    },
                    "end_gl_code_ind": {
                        "control": "select",
                        "label": "末层科目标志",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        },
                        "disabled": true
                    }
                },
                {
                    "bal_prop": {
                        "control": "select",
                        "label": "余额性质",
                        "dict": {
                            "dictType": "E_BALPROP",
                            "dictKey": [
                                "E_BALPROP"
                            ],
                            "format": "label"
                        },
                        "disabled": true
                    },
                    "on_bal_sheet_ind": {
                        "control": "select",
                        "label": "表内标志",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        },
                        "disabled": true
                    }
                },
                {
                    "simple_list_display_ind": {
                        "control": "select",
                        "label": "简表展示标识",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        },
                        "disabled": true
                    },
                    "bal_check_ind": {
                        "control": "select",
                        "label": "余额检查标志",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择余额检查标志"
                            }
                        ]
                    }
                },
                {
                    "debit_manual_allow": {
                        "control": "select",
                        "label": "借方手工记账许可",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择借方手工记账许可"
                            }
                        ]
                    },
                    "credit_manual_allow": {
                        "control": "select",
                        "label": "贷方手工记账许可",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择贷方手工记账许可"
                            }
                        ]
                    }
                },
                {
                    "manual_open_acct_mode": {
                        "control": "select",
                        "label": "手工开户受理模式",
                        "dict": {
                            "dictType": "E_MANUALOPENACCTMODE",
                            "dictKey": [
                                "E_MANUALOPENACCTMODE"
                            ],
                            "format": "label"
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择手工开户受理模式"
                            }
                        ]
                    },
                    "valid_ind": {
                        "control": "select",
                        "label": "有效标志",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        },
                        "disabled": true
                    }
                },
                {
                    "create_date": {
                        "control": "hidden",
                        "label": "建立日期",
                        "name": "create_date",
                        "disabled": true
                    },
                    "identifier_code": {
                        "control": "hidden",
                        "label": "识别码",
                        "name": "identifier_code",
                        "disabled": true
                    },
                    "recdver": {
                        "control": "hidden",
                        "label": "数据版本号",
                        "name": "recdver",
                        "disabled": true
                    }
                }
            ]
        }
    }
}
