{
    "module": "v-form-editableDataGrid-btn",
    "layout": {
        "form": {},
        "toolbar": {
            "btns": [
                {
                    "type": "add",
                    "target": "dialog",
                    "label": "新增",
                    "icon": "el-icon-plus",
                    "toRequest": {
                        "url": "/views/loanManager/glCodeManagement/add/gl_code_detail_add.json",
                        "method": "get",
                        "params": {}
                    },
                    "props": {
                        "title": "科目信息新增"
                    }
                }
            ]
        },
        "datagrid": {
            "allData":"true",
            "pagination": {
                "show": false
            },
            "name": "list01",
            "thead": [
                {
                  "type":"string",
                  "label": "科目号",
                  "prop": "gl_code",
                  "align": "center"
                },
                {
                  "type":"string",
                  "label": "科目名称",
                  "prop": "gl_code_desc",
                  "align": "center"
                },
                {
                  "type":"string",
                  "label": "上级科目",
                  "prop": "upper_lvl_gl_code",
                  "align": "center"
                },
                {
                  "type":"string",
                  "label": "科目类别",
                  "prop": "gl_code_type",
                  "align": "center",
                  "dict": {
                    "dictType": "E_SUBJECTTYPE",
                    "dictKey": [
                      "E_SUBJECTTYPE"
                    ],
                    "format": "label"
                  }
                },
                {
                  "type":"string",
                  "label": "科目级别",
                  "prop": "gl_code_level",
                  "align": "center",
                  "dict": {
                      "dictType": "E_GLCODELEVEL",
                      "dictKey": [
                          "E_GLCODELEVEL"
                      ],
                      "format": "label"
                  }
                },
                {
                  "type":"string",
                  "label": "末层科目标志",
                  "prop": "end_gl_code_ind",
                  "align": "center",
                  "dict": {
                    "dictType": "MS_E_YESORNO",
                    "dictKey": [
                      "MS_E_YESORNO"
                    ],
                    "format": "label"
                  }
                },
                {
                  "type":"string",
                  "label": "余额性质",
                  "prop": "bal_prop",
                  "align": "center",
                  "dict": {
                    "dictType": "E_BALPROP",
                    "dictKey": [
                      "E_BALPROP"
                    ],
                    "format": "label"
                  }
                },
                {
                  "type":"string",
                  "label": "表内标志",
                  "prop": "on_bal_sheet_ind",
                  "align": "center",
                  "dict": {
                    "dictType": "MS_E_YESORNO",
                    "dictKey": [
                      "MS_E_YESORNO"
                    ],
                    "format": "label"
                  }
                },
                {
                  "type":"string",
                  "label": "简表展示标识",
                  "prop": "simple_list_display_ind",
                  "align": "center",
                  "dict": {
                    "dictType": "MS_E_YESORNO",
                    "dictKey": [
                      "MS_E_YESORNO"
                    ],
                    "format": "label"
                  }
                },
                {
                  "type":"string",
                  "label": "余额检查标志",
                  "prop": "bal_check_ind",
                  "align": "center",
                  "dict": {
                    "dictType": "MS_E_YESORNO",
                    "dictKey": [
                      "MS_E_YESORNO"
                    ],
                    "format": "label"
                  }
                },
                {
                  "type":"string",
                  "label": "借方手工记账许可",
                  "prop": "debit_manual_allow",
                  "align": "center",
                  "dict": {
                    "dictType": "MS_E_YESORNO",
                    "dictKey": [
                      "MS_E_YESORNO"
                    ],
                    "format": "label"
                  }
                },
                {
                  "type":"string",
                  "label": "贷方手工记账许可",
                  "prop": "credit_manual_allow",
                  "align": "center",
                  "dict": {
                    "dictType": "MS_E_YESORNO",
                    "dictKey": [
                      "MS_E_YESORNO"
                    ],
                    "format": "label"
                  }
                },
                {
                  "type":"string",
                  "label": "手工开户受理模式",
                  "prop": "manual_open_acct_mode",
                  "align": "center",
                  "dict": {
                    "dictType": "E_MANUALOPENACCTMODE",
                    "dictKey": [
                      "E_MANUALOPENACCTMODE"
                    ],
                    "format": "label"
                  }
                },
                {
                  "type":"string",
                  "label": "有效标志",
                  "prop": "valid_ind",
                  "align": "center",
                  "dict": {
                    "dictType": "MS_E_YESORNO",
                    "dictKey": [
                      "MS_E_YESORNO"
                    ],
                    "format": "label"
                  }
                },
                {
                    "label": "操作",
                    "fixed": "right",
                    "align": "center",
                    "btns": [
                        {
                            "type": "etEdit",
                            "label": "维护",
                            "toRequest": {
                                "url": "/views/loanManager/glCodeManagement/add/gl_code_detail_add.json",
                                "method": "POST",
                                "params": {}
                            },
                            "props": {
                                "title": "科目信息维护"
                            }
                        },
                        {
                            "type": "delete",
                            "label": "删除",
                            "icon": "el-icon-close"
                        }
                    ]
                }
            ]
        },
        "footer": {
            "position": "right",
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": "",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "post",
                        "params": {
                            "servicecode": "IGLSFATS3010",
                            "list01":"${{current.allData}}"
                        }
                    }
                }
            ]
        }
    }
}