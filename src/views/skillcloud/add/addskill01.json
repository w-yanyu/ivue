{
  "module": "lr_tree_dynamic",
  "layout": {
    "initReqeust": "${{params}}",
    "tree": {
      "scope": "productParam",
      "title": "敞口树形展示",
      "lazy": false,
      "idField": "prod_cd",
      "pIdField": "parent_prod",
      "labelField": "prod_name",
      "nodeTypeField": "prod_level",
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "parent_prod":"${{params.parent_prod}}",
          "audit_status":"${{params.audit_status}}",
          "prod_cd":"${{params.prod_cd}}",
          "servicecode": "FindTree"
        }
      },
      "root": {
        "rootNode": {
          "name": "产品谱系敞口树形展示",
          "value": "1",
          "icon": "xitongguanli",
          "disabled": true
        }
      },
      "nodeTypes": {
        "1": {
          "icon": "el-icon-news",
          "children": [
            "prod_cd",
            "prod_name"
          ],
          "toRequest": {
            "method": "get",
            "url": "/views/skillcloud/add/tree01.json",
            "params": {
              "operater_ind": "M",
              "prod_cd": "${{productParam.prod_cd}}",
              "parent_prod": "${{productParam.parent_prod}}",
              "audit_status":"${{params.audit_status}}"
            }
          },
          "props": {
            "title": "敞口信息详情"
          }
        },
        "2": {
          "icon": "el-icon-news",
          "children": [
            "prod_cd",
            "prod_name"
          ],
          "toRequest": {
            "method": "get",
            "url": "/views/skillcloud/save/tree01.json",
            "params": {
              "operater_ind": "M",
              "prod_cd": "${{productParam.prod_cd}}",
              "parent_prod": "${{productParam.parent_prod}}",
              "audit_status":"${{params.audit_status}}"
            }
          },
          "props": {
            "title": "敞口信息详情"
          }
        },
        "3": {
          "icon": "el-icon-news",
          "children": [
            "prod_cd",
