{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initReqeust": "${{params}}",
      "initDataOnce":true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ADD_NAME"
        }
      },
      "controls": [
        {
          "prod_cd": {
            "label": "节点编码",
            "control": "string",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "节点编码"
              }
            ]
          },
          "link_name": {
            "label": "联系人姓名",
            "control": "text",
            "width": "130px",
            "rules": [
              {
                "message": "联系人姓名"
              }
            ]
          }
        },
        {
          "e_mail": {
            "label": "联系人邮箱",
            "control": "text",
            "width": "130px",
            "rules": [
              {
                "message": "联系人邮箱"
              }
            ]
          },
          "phone_no": {
            "label": "联系人手机号",
            "control": "text",
            "width": "130px",
            "rules": [
              {
                "message": "联系人手机号"
              }
            ]
          }
        }
      ]
    }
  }
}
