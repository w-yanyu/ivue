{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest":"${{params}}",
      "initDataOnce":true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode":"UP_SKILL"
        }
      },
      "btns":[
        {
          "type":"submit",
          "label":"提交"
        }
      ],
      "controls": [
        {
          "audit_status": {
            "control": "select",
            "label": "审核结果",
            "width": "130px",
            "dict": {
              "dictType": "A_SKILL_STATUS",
              "dictKey": [
                  "A_SKILL_STATUS"
              ],
              "labelField":"dictName",
              "valueField":"dictId"
            },
            "rules": [
              {
                "required": true,
                "message": "请先选择审核结果"
              }
            ],
            "format": "label"
          },
          "prod_cd": {
            "control": "string",
            "disable": true,
            "label": "节点编码",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请先选择审核结果"
              }
            ]
          }
        }
      ]
    }
  }
}
