{
    "module": "v-form-btn-dynamic",
    "layout": {
        "form": {
            "scope": "A",
            "name": "detail",
            "initRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "wa0033",
                    "cust_no": "${{params.cust_id}}"
                }
            },
            "controls": [{
                "risk_rate_code": {
                    "label": "预警级别",
                    "width": "140px",
                    "control": "text",
                    "disabled":"true"
                },
                "task_id": {
                    "label": "预警批次",
                    "width": "140px",
                    "control": "text",
                    "disabled":"true"
                },
                "case_id": {
                    "label": "案件编号",
                    "width": "140px",
                    "control": "text",
                    "disabled":"true"
                },
                "task_name": {
                    "label": "案件编号",
                    "width": "140px",
                    "control": "text",
                    "disabled":"true"
                },
                "warn_source": {
                    "label": "预警来源",
                    "width": "140px",
                    "control": "text",
                    "disabled":"true"
                },
                "warn_date": {
                    "label": "预警日期",
                    "width": "140px",
                    "control": "dateTimePicker",
                    "format":"yyyyMMdd",
                    "disabled":"true"
                },
                "case_state_ind": {
                    "label": "工单状态",
                    "width": "140px",
                    "control": "select",
                    "multiple": false,
                    "disabled":"true",
                    "dict": {
                        "dictType": "E_CASE_STATE",
                        "dictKey": [
                            "E_CASE_STATE"
                        ],
                        "format": "label"
                    }
                },
                "new_ind": {
                    "label": "是否首次预警",
                    "width": "140px",
                    "control": "select",
                    "multiple": false,
                    "disabled":"true",
                    "dict": {
                        "dictType": "E_YES___",
                        "dictKey": [
                            "E_YES___"
                        ],
                        "format": "label"
                    }
                },
                "warn_cnt": {
                    "label": "预警次数",
                    "width": "140px",
                    "control": "text",
                    "disabled":"true"
                },
                "last_warn_date": {
                    "label": "上次预警日期",
                    "width": "140px",
                    "control": "dateTimePicker",
                    "format":"yyyyMMdd",
                    "disabled":"true"
                },
                "warn_user_id": {
                    "label": "经办产品经理",
                    "width": "140px",
                    "control": "text",
                    "disabled":"true"
                },
                "prod_name": {
                    "label": "产品名称",
                    "width": "140px",
                    "control": "text",
                    "disabled":"true"
                }

            }]


        }
    }
}
