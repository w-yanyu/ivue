{
    "module": "v-form-btn-dynamic",
    "layout": {
        "form": {
            "scope": "A",
            "name": "detail",
            "initRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "wa0032",
                    "cust_no":"${{params.cust_id}}"

                }
            },
            "controls": [{
                "cust_no": {
                    "label": "客户号",
                    "width": "140px",
                    "control": "text",
                    "disabled":"true"
                },
                "cust_name": {
                    "label": "客户名称",
                    "width": "140px",
                    "control": "text",
                    "disabled":"true"
                },
                "cert_type": {
                    "label": "证件类型",
                    "width": "140px",
                    "control": "select",
                    "multiple": false,
                    "disabled":"true",
                    "dict": {
                        "dictType": "MS_E_CRET_TYPE",
                        "dictKey": [
                            "MS_E_CRET_TYPE"
                        ],
                        "format": "label"
                    },
                    "rules": [{
                        "required": true,
                        "message": "证件类型"
                    }]
                },
                "cert_no": {
                    "label": "证件号码",
                    "width": "140px",
                    "control": "text",
                    "disabled":"true"
                },
                "sex_tp": {
                    "label": "性别",
                    "width": "140px",
                    "control": "select",
                    "multiple": false,
                    "disabled":"true",
                    "dict": {
                        "dictType": "E_SEXTYP",
                        "dictKey": [
                            "E_SEXTYP"
                        ],
                        "format": "label"
                    },
                    "rules": [{
                        "required": true,
                        "message": "性别"
                    }]
                },
                "age": {
                    "label": "年龄",
                    "width": "140px",
                    "control": "text",
                    "disabled":"true"
                },
                "marr_state": {
                    "label": "婚姻状况",
                    "width": "140px",
                    "control": "select",
                    "multiple": false,
                    "disabled":"true",
                    "dict": {
                        "dictType": "E_MARRYS",
                        "dictKey": [
                            "E_MARRYS"
                        ],
                        "format": "label"
                    },
                    "rules": [{
                        "required": true,
                        "message": "婚姻状况"
                    }]
                },
                "max_educat_backg": {
                    "label": "最高学历",
                    "width": "140px",
                    "control": "select",
                    "multiple": false,
                    "disabled":"true",
                    "dict": {
                        "dictType": "E_EDUCATION_EXPERIENCE",
                        "dictKey": [
                            "E_EDUCATION_EXPERIENCE"
                        ],
                        "format": "label"
                    },
                    "rules": [{
                        "required": true,
                        "message": "最高学历"
                    }]
                },
                "live_addr": {
                    "label": "居住地址",
                    "width": "140px",
                    "control": "text",
                    "disabled":"true"
                }

            }]


        }
    }
}
