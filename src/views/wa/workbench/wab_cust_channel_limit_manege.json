{
    "module": "v_search_btn_datagrid",
    "layout": {

      "toolbar": {
        "btns": [

        ]
      },
      "datagrid": {
        "scope": "searchTable",
        "isInitSearch": true,
        "doRequest": {
          "url": "/SUMP/icmcall/icmRPCCall",
          "method": "POST",
          "params": {
            "servicecode": "wa0037",
            "cust_no": "${{params.cust_id}}"
          }
        },
        "thead": [
            {
              "label": "流量合作方",
              "width": "200px",
              "prop": "chan_no",
              "align": "center"
            },
            {
              "label": "额度级别",
              "width": "200px",
              "prop": "limit_level",
              "align": "center"
            },
            {
              "label": "已用额度",
              "width": "200px",
              "prop": "use_limit",
              "align": "center",
              "control": "currency"
            },
            {
              "label": "冻结额度",
              "width": "200px",
              "prop": "block_amt",
              "align": "center",
              "control": "currency"
            },
            {
              "label": "可用额度",
              "width": "200px",
              "prop": "avai_limit",
              "align": "center",
              "control": "currency"
            },
            {
              "label": "额度状态",
              "width": "200px",
              "prop": "limit_state",
              "align": "center"
            }
        ]
      }
    }
  }
