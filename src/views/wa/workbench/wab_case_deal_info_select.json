{
    "module": "v-form-btn-dynamic",
    "layout": {
        "form": {
            "scope": "A",
            "name": "detail",
            "initRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "wa0038",
                  "wf_inst_id":"${{params.proc_inst_id}}",
                  "cust_no":"${{params.cust_id}}"
                }
            },
            "controls": [{
                "case_rec_id": {
                    "label": "案件处置编号",
                    "control": "text",
                    "width": "140px",
                    "disabled":"true"
                },
                "method_code": {
                    "label": "处置措施代码",
                    "width": "140px",
                    "control": "select",
                    "disabled":"true",
                    "multiple": false,
                    "dict": {
                        "dictType": "WA_E_METHOD_CD",
                        "dictKey": [
                            "WA_E_METHOD_CD"
                        ],
                        "format": "label"
                    },
                    "rules": [{
                        "required": true,
                        "message": "处置措施代码"
                    }]
                },
                "ahead_pl_ind": {
                    "label": "提前入催标识",
                    "width": "140px",
                    "control": "select",
                    "disabled":"true",
                    "multiple": false,
                    "dict": {
                        "dictType": "MS_E_YESORNO",
                        "dictKey": [
                            "MS_E_YESORNO"
                        ],
                        "format": "label"
                    },
                    "rules": [{
                        "required": true,
                        "message": "提前入催标识"
                    }]
                }


            }]

        }
    }
}
