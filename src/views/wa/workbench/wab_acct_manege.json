{
    "module": "v_search_btn_datagrid",
    "layout": {

      "toolbar": {
        "btns": [

        ]
      },
      "datagrid": {
        "scope": "searchTable",
        "isInitSearch": true,
        "doRequest": {
          "url": "/SUMP/icmcall/icmRPCCall",
          "method": "POST",
          "params": {
            "servicecode": "wa0034",
            "cust_no": "${{params.cust_id}}"
          }
        },
        "thead": [
            {
              "label": "贷款借据号",
              "width": "160px",
              "prop": "loan_no",
              "align": "center"
            },
            {
              "label": "产品名称",
              "width": "160px",
              "prop": "prod_name",
              "align": "center"
            },
            {
              "label": "贷款金额",
              "width": "160px",
              "prop": "loan_amt",
              "align": "center",
              "control": "currency"
            },
            {
              "label": "呆账本金",
              "width": "160px",
              "prop": "uncollect_acct_princi",
              "align": "center",
              "control": "currency"
            },
            {
              "label": "呆滞本金",
              "width": "160px",
              "prop": "stagnant_princi",
              "align": "center",
              "control": "currency"
            },
            {
              "label": "贷款余额",
              "width": "160px",
              "prop": "loan_bal",
              "align": "center",
              "control": "currency"
            },
            {
              "label": "贷款期限",
              "width": "160px",
              "prop": "loan_term",
              "align": "center",
              "control": "dateTimePicker",
              "format":"yyyyMMdd"
            },
            {
              "label": "开户日期",
              "width": "160px",
              "prop": "acct_open_date",
              "align": "center",
              "control": "dateTimePicker",
              "format":"yyyyMMdd"
            },
            {
              "label": "到期日期",
              "width": "160px",
              "prop": "matu_date",
              "align": "center",
              "control": "dateTimePicker",
              "format":"yyyyMMdd"
            },
            {
              "label": "贷款形态",
              "align": "center",
              "prop": "loan_form",
              "dict": {
                "dictType": "E_CLSSST",
                "dictKey": [
                  "E_CLSSST"
                ],
                "format": "label"
              }
            },
            {
              "label": "应收欠息",
              "width": "160px",
              "prop": "recv_debit_inst",
              "align": "center",
              "control": "currency"
            },
            {
              "label": "催收欠息",
              "width": "160px",
              "prop": "collt_debit_inst",
              "align": "center",
              "control": "currency"
            },
            {
              "label": "应收应计罚息",
              "width": "160px",
              "prop": "recv_accrued_penalt_inst",
              "align": "center",
              "control": "currency"
            },
            {
              "label": "催收应计罚息",
              "width": "160px",
              "prop": "collt_acrued_penalt_inst",
              "align": "center",
              "control": "currency"
            },
            {
              "label": "应收罚息",
              "width": "160px",
              "prop": "recv_penalt_inst",
              "align": "center",
              "control": "currency"
            },
            {
              "label": "催收罚息",
              "width": "160px",
              "prop": "recv_penalt_inst",
              "align": "center",
              "control": "currency"
            },
            {
              "label": "应计复息",
              "width": "160px",
              "prop": "accrued_comp_inst",
              "align": "center",
              "control": "currency"
            },
            {
              "label": "复息",
              "width": "160px",
              "prop": "compd_inst",
              "align": "center",
              "control": "currency"
            },
            {
              "label": "上月分类结果",
              "width": "160px",
              "prop": "last_mon_class_result",
              "align": "center"
            }
        ]
      }
    }
  }
