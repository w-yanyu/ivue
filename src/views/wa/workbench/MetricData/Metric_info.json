
{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "isInitSearch":false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "wa0028",
          "cust_no":"${{params.cust_id}}",
          "model_id": "${{params.model_id}}"
        }
      },
      "thead": [

        {
          "label": "指标信息",
          "prop": "metric_desc",
          "align": "center"
        },
        {
          "label": "指标值",
          "prop": "metric_data_value",
          "align": "center"
        },
        {
          "label": "指标得分",
          "prop": "metric_score",
          "align": "center"
        }
      ]
    }
  }
}
