{
  "module": "dynamicTabs",
  "layout": {
    "tabs": {
      "scope": "templateScope",
      "#initDataOnce":true,
      "tabHeader": [
        {
          "id": "dd1",
          "scope": "baseInfo",
          "clickInit": true,
          "label": "客户基本信息",
          "closable": false,
          "disabled": false,
          "icon": "el-icon-date",
          "toRequest":{
            "url":"/views/wa/workbench/customer_info.json",
            "method": "post",
            "cust_id":"${{params.cust_id}}"
          }

        },
        {
          "id": "dd2",
          "clickInit": true,
          "label": "预警信息",
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "toRequest":{
            "url": "/views/wa/workbench/wab_case_info.json",
            "method": "post",
            "params": {
              "cust_id":"${{params.cust_id}}"
            }
          }
        },
        {
          "id": "dd3",
          "clickInit": true,
          "label": "预警历史",
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "toRequest":{
            "url": "/views/wa/workbench/was_case_history.json",
            "method": "post",
            "params": {
              "cust_id":"${{params.cust_id}}"
            }
          }
        },
        {
          "id": "dd4",
          "clickInit": true,
          "label": "借据信息",
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "toRequest":{
            "url": "/views/wa/workbench/wab_acct_manege.json",
            "method": "post",
            "params": {
              "cust_id":"${{params.cust_id}}"
            }
          }
        },
        {
          "id": "dd5",
          "clickInit": true,
          "label": "渠道额度信息",
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "toRequest":{
            "url": "/views/wa/workbench/wab_cust_channel_limit_manege.json",
            "method": "post",
            "params": {
              "cust_id":"${{params.cust_id}}"
            }
          }
        } ,
        {
          "id": "dd6",
          "clickInit": true,
          "label": "产品额度信息",
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "toRequest":{
            "url": "/views/wa/workbench/wab_cust_channel_p_limit_manege.json",
            "method": "post",
            "params": {
              "cust_id":"${{params.cust_id}}"
            }
          }
        } ,
        {
          "id": "dd7",
          "clickInit": true,
          "label": "审批记录",
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "toRequest":{
            "url": "/views/wa/workbench/approval_record.json",
            "method": "post",
            "params": {
              "appl_id":"${{params.appl_id}}",
              "wf_inst_id":"${{params.wf_inst_id}}"
            }
          }
        } ,
        {
          "id": "dd8",
          "clickInit": true,
          "label": "工单处置信息",
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "toRequest":{
            "url": "/views/wa/workbench/wab_case_deal_info_select.json",
            "method": "post",
            "params": {
              "cust_id":"${{params.cust_id}}",
              "proc_inst_id":"${{params.wf_inst_id}}"

            }
          }
        }
      ],
      "defaultActiveKey": "dd1",
      "position": "top",
      "styleClass": "card"
    }
  }
}
