{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "scope": "formData",
            "controls": [
                {
                    "risk_rate_code": {
                        "control": "text",
                        "label": "风险级别代码"
                    },
                    "risk_rate_name": {
                        "control": "text",
                        "label": "风险级别名称"
                    }
                }
            ]
        },
        "toolbar": {
            "btns": [
                {
                    "type": "custom",
                    "icon": "el-icon-plus",
                    "target": "dialog",
                    "label": "新增",
                    "toRequest": {
                        "method": "GET",
                        "url": "/views/wa/param/risk/customer/customer_add.json"
                    },
                    "props": {
                        "title": "借款人风险级别定义新增"
                    }
                }
            ]
        },
        "datagrid": {
            "isInitSearch": false,
            "doRequest": {
                "url": "/SUMP/iccsCall/warnCall",
                "method": "POST",
                "params": {
                    "servicecode": "wa3021",
                    "risk_type": "C"
                }
            },
            "thead": [
                {
                    "type": "string",
                    "align": "center",
                    "label": "风险级别代码",
                    "width": "100",
                    "prop": "risk_rate_code"
                },
                {
                    "type": "string",
                    "align": "center",
                    "label": "风险级别名称",
                    "width": "150",
                    "prop": "risk_rate_name"
                },
                {
                    "type": "string",
                    "align": "center",
                    "label": "风险分值区间(初值)",
                    "width": "140",
                    "prop": "first_operator",
                    "dict": {
                      "dictType": "WA_E_RELATIONOPERATOR",
                      "dictKey": [
                        "WA_E_RELATIONOPERATOR"
                      ],
                      "format": "label ${{current.first_number}}"
                    }
                },
                {
                  "type": "string",
                  "align": "center",
                  "label": "风险分值区间(末值)",
                  "width": "140",
                  "prop": "second_operator",
                  "dict": {
                    "dictType": "WA_E_RELATIONOPERATOR",
                    "dictKey": [
                      "WA_E_RELATIONOPERATOR"
                    ],
                    "format": "label ${{current.second_number}}"
                  }
                },
                {
                  "type": "string",
                  "align": "center",
                  "width": "130",
                  "label": "风险信号",
                  "prop": "risk_rate_signal",
                  "dict": {
                    "dictType": "WA_E_RISKSIGNAL",
                    "dictKey": [
                      "WA_E_RISKSIGNAL"
                    ],
                    "format": "value-label"
                  }
                },
                {
                    "type": "string",
                    "align": "center",
                    "width": "150",
                    "label": "操作日期",
                    "prop": "data_update_time"
                },
                {
                    "label": "操作",
                    "align":"center",
                    "fixed": "right",
                    "width": "150px",
                    "btns": [
                      {
                        "type": "look",
                        "label": "维护",
                        "toRequest": {
                          "url": "/views/wa/param/risk/customer/customer_modify.json",
                          "method": "GET"
                        },
                        "props": {
                          "title": "策略维护"
                        }
                      },
                      {
                        "type": "delete",
                        "label": "删除",
                        "doRequest": {
                          "url": "/SUMP/iccsCall/warnCall",
                          "method": "POST",
                          "params": {
                            "servicecode": "wa3025"
                          }
                        }
                      }
                    ]
                }
            ]
        }
    }
}
