{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "doRequest": {
        "url": "/SUMP/iccsCall/warnCall",
        "method": "POST",
        "params": {
          "servicecode": "wa4012",
          "warning_cust": "C"
        }
      },
      "controls": [
        {
          "method_code": {
            "control": "select",
            "label": "处理措施代码",
            "dict": {
              "dictType": "E_RISKPMETHOD",
              "dictKey": [
                "E_RISKPMETHOD"
              ],
              "format": "value-label"
            },
            "rules": [
              {
                "required": true,
                "message": "请输入"
              },
              {
                "pattern": "^[\\w\\u4E00-\\u9FA5]{1,20}$",
                "message": "请输入长度在1至20由汉字，字母，数字或下划线组成的字符串"
              }
            ]
          },
          "method_name": {
            "control": "text",
            "label": "处理措施名称",
            "rules": [
              {
                "required": true,
                "message": "请输入"
              },
              {
                "pattern": "^[\\w\\u4E00-\\u9FA5]{1,20}$",
                "message": "请输入长度在1至20由汉字，字母，数字或下划线组成的字符串"
              }
            ]
          }
        },
        {
          "flow_ind": {
            "control": "select",
            "label": "是否启用流程",
            "placeholder":"请选择",
            "dict": {
              "dictType": "E_ISORNO",
              "dictKey": [
                "E_ISORNO"
              ],
              "format": "value-label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择"
              }
            ]
          }
        },
        {
          "label": {
            "control": "link",
            "label": "描述:",
            "span": 5
          },
          "method_desc": {
            "control": "textarea",
            "label": "",
            "span": 19,
            "maxLength": 200,
            "autosize": false,
            "rows": 4
          }
        }
      ]
    }
  }
}
