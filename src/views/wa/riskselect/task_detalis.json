{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "scope": "formData",
            "btns": [],
            "controls": [
                {
                    "task_id": {
                        "control": "text",
                        "width": "130px",
                        "disabled": "true",
                        "span": "11",
                        "label": "预警任务编号"
                    },
                    "cust_no": {
                        "control": "text",
                        "label": "客户号",
                        "disabled": "true",
                        "width": "130px",
                        "span": "11"
                    },
                    "warning_cust": {
                        "control": "select",
                        "label": "预警对象",
                        "disabled": "true",
                        "width": "130px",
                        "span": "11",
                        "dict": {
                            "dictType": "E_WARN_TARGET",
                            "dictKey": [
                                "E_WARN_TARGET"
                            ],
                            "format": "label"
                        }
                    },
                    "warn_ind": {
                        "control": "select",
                        "label": "预警标志",
                        "disabled": "true",
                        "width": "130px",
                        "span": "11",
                        "dict": {
                            "dictType": "E_WA_WARN_IND",
                            "dictKey": [
                                "E_WA_WARN_IND"
                            ],
                            "format": "label"
                        }
                    },
                    "case_cust_name": {
                        "control": "text",
                        "label": "客户姓名",
                        "disabled": "true",
                        "width": "130px",
                        "span": "11"
                    },
                    "warn_user_id": {
                        "control": "text",
                        "label": "预警处理人",
                        "disabled": "true",
                        "width": "130px",
                        "span": "11"
                    },
                    "prod_no": {
                        "control": "text",
                        "label": "产品编码",
                        "disabled": "true",
                        "width": "130px",
                        "span": "11"
                    },
                    "risk_rate_code": {
                        "control": "text",
                        "label": "风险级别代码",
                        "disabled": "true",
                        "width": "130px",
                        "span": "11"
                    },
                    "result_score": {
                        "control": "text",
                        "label": "得分",
                        "disabled": "true",
                        "width": "130px",
                        "span": "11"
                    },
                    "credit_granting": {
                        "control": "text",
                        "label": "授信金额(元)",
                        "disabled": "true",
                        "width": "130px",
                        "span": "11"
                    },
                    "loan_amount_total": {
                        "control": "text",
                        "label": "贷款总额(元)",
                        "disabled": "true",
                        "width": "130px",
                        "span": "11"
                    },
                    "loan_balance_total": {
                        "control": "text",
                        "label": "贷款余额总额(元)",
                        "disabled": "true",
                        "width": "130px",
                        "span": "11"
                    },
                    "loan_period": {
                        "control": "text",
                        "label": "贷款期限",
                        "disabled": "true",
                        "width": "130px",
                        "span": "11"
                    },
                    "termfm_type": {
                        "control": "select",
                        "label": "贷款期数类型",
                        "disabled": "true",
                        "width": "130px",
                        "span": "11",
                        "dict": {
                            "dictType": "E_TERMFM_TYPE",
                            "dictKey": [
                                "E_TERMFM_TYPE"
                            ],
                            "format": "label"
                        }
                    },
                    "overdue_amount_total": {
                        "control": "text",
                        "label": "贷款逾期总额(元)",
                        "disabled": "true",
                        "width": "130px",
                        "span": "11"
                    },
                    "overdue_day": {
                        "control": "text",
                        "label": "逾期天数",
                        "disabled": "true",
                        "width": "130px",
                        "span": "11"
                    },
                    "screen_style": {
                        "control": "select",
                        "label": "预警对象筛选方式",
                        "disabled": "true",
                        "width": "130px",
                        "span": "11",
                        "dict": {
                            "dictType": "WA_E_SCREEN_STYLE",
                            "dictKey": [
                                "WA_E_SCREEN_STYLE"
                            ],
                            "format": "label"
                        }
                    },
                    "method_code": {
                        "control": "select",
                        "label": "处置措施代码",
                        "disabled": "true",
                        "width": "130px",
                        "span": "11",
                        "dict": {
                            "dictType": "WA_E_METHOD_CD",
                            "dictKey": [
                                "WA_E_METHOD_CD"
                            ],
                            "format": "label"
                        }
                    },
                    "warn_date": {
                        "control": "dateTimePicker",
                        "label": "预警日期",
                        "disabled": "true",
                        "width": "130px",
                        "span": "11",
                        "format": "yyyyMMdd"
                    },
                    "first_warn_date": {
                        "control": "dateTimePicker",
                        "label": "首次预警日期",
                        "disabled": "true",
                        "width": "130px",
                        "span": "11",
                        "format": "yyyyMMdd"
                    }

                }
            ]


        }
    }
}
