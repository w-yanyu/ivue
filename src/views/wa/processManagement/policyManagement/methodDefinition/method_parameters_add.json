{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "scope": "templateScope",
            "init":{
                "initRequest":"${{params}}"
            },
            "initDataOnce":true,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "post",
                "params": {
                  "servicecode": "bpm0029",
                  "appId":"${{params.appId}}"
                }
              },
              "btns":[
				{
					"type":"submit",
					"label":"提交"
				}
			],
            "controls": [
                {

                    "method_id": {
                        "label": "方法编号",
                        "control": "hidden",
                        "value":"${{params.method_id}}",
                        "width": "130px"
                    },

                    "sort_no": {
                        "label": "参数序号",
                        "control": "text",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入参数序号"
                            }
                        ]
                    },
                    "param_clazz": {
                        "control": "select",
                        "label": "参数分类",
                        "dict": {
                            "dictType": "BPM_E_CANSFL",
                            "dictKey": [
                                "BPM_E_CANSFL"
                            ],
                            "format": "label"
                        },
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择参数分类"
                            }
                        ]
                    },
                   
                    "param_type": {
                        "control": "select",
                        "label": "参数类型",
                        "dict": {
                            "dictType": "BPM_E_CANSLX",
                            "dictKey": [
                                "BPM_E_CANSLX"
                            ],
                            "format": "label"
                        },
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择参数类型"
                            }
                        ]
                    },
                    "fixed_param": {
                        "control": "select",
                        "label": "固定参数名",
                        "dict": {
                            "dictType": "BPM_E_INCSXX",
                            "dictKey": [
                                "BPM_E_INCSXX"
                            ],
                            "format": "label"
                        },
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择固定参数名"
                            }
                        ]
                    }
                }
            ]
        }
    }
}