{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "post",
                "params": {
                  "servicecode": "bpm0025",
                  "appId":"${{params.appId}}"
                }
              },
              "btns":[
				{
					"type":"submit",
					"label":"提交"
				}
			],
            "controls": [
                {
                    "method_id": {
                        "label": "方法编号",
                        "control": "text",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入方法编号"
                            }
                        ]
                    },
                    "method_name": {
                        "label": "方法名称",
                        "control": "text",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入方法名称"
                            }
                        ]
                    },
                    "method_full_path": {
                        "label": "全路径类名",
                        "control": "text",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入全路径类名"
                            }
                        ]
                    },
                    "valid_flag": {
                        "control": "select",
                        "label": "有效标志",
                        "dict": {
                            "dictType": "BPM_E_ISFLAG",
                            "dictKey": [
                                "BPM_E_ISFLAG"
                            ],
                            "format": "label"
                        },
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择有效标志"
                            }
                        ]
                    }
                }
            ]
        }
    }
}