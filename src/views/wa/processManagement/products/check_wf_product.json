{
    "module": "v-inputDataForm-btn",
    "layout": {
        "form": {
            "name":"check_wf_product",
            "initRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "bpm0057",
                    "wfprod_cd": "${{params.wfprod_cd}}"
                }
            },
            "btns": [],
            "controls": [
                {
                    "wfprod_cd": {
                        "control": "text",
                        "label": "流程产品编号",
                        "disabled":true,
                        "span": 22,
                        "value":"${{params.wfprod_cd}}"
                    }
                },
                {
                    "warn_msg": {
                        "control": "textarea",
                        "label": "校验结果",
                        "span": 22,
                        "rows":10
                    }
                }
            ]
        }
    }
}