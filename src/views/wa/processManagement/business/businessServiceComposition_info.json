{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "appId": {
              "control": "select",
              "label": "业务模块",
              "selectFirstValue":true,
              "dict": {
                  "dictType": "MS_APPID",
                  "dictKey": [
                      "MS_APPID"
                  ],
                  "format": "value-label"
              },
              "rules": [
                  {
                      "required": true,
                      "message": "请选择业务模块"
                  }]
          }
        },
        {
          "busi_type": {
            "control": "select",
            "label": "业务类型",
            "dict": {
              "dictType": "BPM_E_BUSI_TYPE",
              "dictKey": [
                "BPM_E_BUSI_TYPE"
              ]

            }
          },
          "route_type": {
            "control": "select",
            "label": "申请类型",
            "dict": {
              "dictType": "BPM_E_APPL_TYPE",
              "dictKey": [
                "BPM_E_APPL_TYPE"
              ]

            }
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "target": "dialog",
          "label": "新增",
          "toRequest": {
            "method": "GET",
            "url": "/views/msap/bpm/processManagement/business/businessServiceComposition_add.json",
            "params": {
              "appId":"${{form.appId}}"
            }
          },
          "props": {
            "width": "75%",
            "title": "业务场景服务组合信息明细"
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "bpm0036"
        }
      },
      "thead": [
        {
          "label": "业务类型",
          "prop": "busi_type",
          "align": "center",
          "dict": {
            "dictType": "BPM_E_BUSI_TYPE",
            "dictKey": [
              "BPM_E_BUSI_TYPE"
            ]
          },
          "format": "label"
        },
        {
          "label": "申请类型",
          "prop": "appl_type",
          "align": "center",
          "dict": {
            "dictType": "BPM_E_APPL_TYPE",
            "dictKey": [
              "BPM_E_APPL_TYPE"
            ]

          }
        },
        {
          "label": "是否可用",
          "prop": "avaiable_ind",
          "align": "center",
          "dict": {
            "dictType": "MS_E_YESORNO",
            "dictKey": [
              "MS_E_YESORNO"
            ]

          }
        },
        {
          "label": "适应流程",
          "prop": "adapt_process",
          "align": "center"
        },
        {
          "label": "业务方法调用类型",
          "prop": "busi_method_call_type",
          "align": "center",
          "dict": {
            "dictType": "BPM_E_CALL_BUSI_TYPE",
            "dictKey": [
              "BPM_E_CALL_BUSI_TYPE"
            ]
          }
        },
        {
          "label": "服务名称(绝对路径)",
          "prop": "service_name",
          "align": "center"
        },
        {
          "label": "排序序号",
          "prop": "sort_no",
          "align": "center"
        },
        {
          "label": "操作",
          "align":"center",
          "fixed": "right",
          "btns": [

            {
              "type": "look",
              "label": "详情",
              "toRequest": {
                "url": "/views/msap/bpm/processManagement/business/businessServiceComposition_details.json",
                "method": "GET",
                "params": {
                  "appId":"${{form.appId}}"
                }
              },
              "props": {
                "width": "75%",
                "title": "业务场景服务组合信息明细"
              }
            },{
              "type": "edit",
              "label": "修改",
              "toRequest": {
                "url": "/views/msap/bpm/processManagement/business/businessServiceComposition_modify.json",
                "method": "GET",
                "params": {
                  "appId":"${{form.appId}}"
                }
              },
              "props": {
                "width": "75%",
                "title": "业务场景服务组合信息明细"
              }
            },
            {
              "type": "delete",
              "label": "删除",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "bpm0035",
                  "appId":"${{form.appId}}"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
