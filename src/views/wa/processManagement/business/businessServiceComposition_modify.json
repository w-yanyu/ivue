{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "scope": "formData",
      "initRequest":"${{params}}",
      "btns": [
        {
          "type": "submit",
          "label": "保存",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "bpm0034",
              "busi_id": "${{params.busi_id}}",
              "appId":"${{params.appId}}"
            }
          }
        }
      ],
      "controls": [

        {
          "busi_type": {
            "label": "业务产品",
            "control": "select",
            "width": "130px",
            "span": 7,
            "dict": {
              "dictType": "BPM_E_BUSI_TYPE",
              "dictKey": [
                "BPM_E_BUSI_TYPE"
              ]

            },
            "rules": [
              {
                "required": true,
                "message": "请选择业务产品"
              }
            ]
          },
          "appl_type": {
            "control": "select",
            "label": "申请类型",
            "width": "130px",
            "span": 7,
            "dict": {
              "dictType": "BPM_E_APPL_TYPE",
              "dictKey": [
                "BPM_E_APPL_TYPE"
              ]

            },
            "rules": [
              {
                "required": true,
                "message": "请选择申请类型"
              }
            ]
          },
          "route_type": {
            "control": "text",
            "label": "路线类型",
            "width": "130px",
            "disabled": false,
            "span": 7,
            "rules": [
              {
                "required": true,
                "message": "请输入流程状态",
                "max": "10"
              }
            ]
          }
        },
        {

          "avaiable_ind": {
            "control": "select",
            "label": "是否可用",
            "width": "130px",
            "span": 7,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择是否可用"
              }
            ]
          },
          "adapt_process": {
            "control": "text",
            "label": "适应流程",
            "width": "130px",
            "span": 14,
            "rules": [
              {
                "required": true,
                "max": "200",
                "message": "请输入适应流程"
              }
            ]
          }
        },
        {
          "busi_method_call_type": {
            "control": "select",
            "label": "业务方法调用类型",
            "width": "130px",
            "span": 7,
            "dict": {
              "dictType": "BPM_E_CALL_BUSI_TYPE",
              "dictKey": [
                "BPM_E_CALL_BUSI_TYPE"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择业务方法调用类型"
              }
            ]
          },
          "service_name": {
            "control": "text",
            "label": "服务名称(绝对路径)",
            "width": "130px",
            "span": 7,
            "rules": [
              {
                "required": true,
                "message": "请输入服务名称(绝对路径)",
                "max": "200"
              }
            ]
          },
          "sort_no": {
            "control": "text",
            "label": "排序序号",
            "width": "130px",
            "span": 7,
            "rules": [
              {
                "required": true,
                "message": "请输入排序序号",
                "max": "8"
              }
            ]
          },
          "remark": {
            "label": "备注",
            "control": "textarea",
            "width": "130px",
            "span":21,
            "autosize":{
              "minRows": 4
            },
            "rules": [
              {
                "required": true,
                "max": "200",
                "message": "请输入备注"
              }
            ]
          }
        }
      ]
    }
  }
}

