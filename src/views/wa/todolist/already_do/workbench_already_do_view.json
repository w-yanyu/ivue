{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [

        {

          "appl_id": {
            "control": "text",
            "label": "申请编号"
          }
        },
        {
          "cert_no": {
            "control": "text",
            "label": "证件号码",
            "rules": [
              {
                "max": 20,
                "message": "长度不能超过20个字符"
              }
            ]
          },
          "mobile_no": {
            "control": "text",
            "label": "手机号码"
          }
        },
        {
          "appl_start_date": {
            "control": "dateTimePicker",
            "label": "进件起始日期"
          },
          "appl_end_date": {
            "control": "dateTimePicker",
            "label": "进件截止日期"
          }
        },
        {
          "cust_name": {
            "control": "text",
            "label": "申请人姓名"
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "bpm1013",
          "sizePage": 10,
          "appId":"waOnl",
          "qryInfo":{
            "user_id": "$g{{user_info.userCode}}"
          }
        }
      },
      "thead": [
        {
          "label": "序号",
          "type": "index"
        },
        {
          "label": "申请编号",
          "prop": "appl_id",
          "width": 200,
          "align": "center"
        },
        {
          "label": "申请人姓名",
          "prop": "cust_name",
          "align": "center"
        },
        {
          "label": "证件号码",
          "prop": "cert_no",
          "width": 160,
          "align": "center"
        },
        {
          "label": "产品名称",
          "prop": "prod_name",
          "align": "center"
        },
        {
          "label": "进件日期",
          "prop": "system_date",
          "align": "center"
        },

        {
          "label": "当前节点",
          "prop": "task_define_name",
          "align": "center"
        },
        {
          "label": "当前处理人",
          "prop": "cur_user_id",
          "align": "center"
        },
        {
          "label": "操作",
          "fixed": "right",
          "width": 130,
          "align": "center",
          "btns": [
            {
              "type": "edit",
              "label": "详情",
              "toRequest": {
                "url": "@CommAlreadyDoDetail",
                "method": "GET",
                "target": "navTab",
                "params": {
                  "frame_cd": "${{current.task_url2}}",
                  "cust_name": "${{current.cust_name}}",
                  "appl_state": "${{current.appl_state}}",
                  "appl_date": "${{current.system_date}}",
                  "task_inst_id":"${{current.task_inst_id}}",
                  "appId":"waOnl",
                  "wf_inst_id": "${{current.wf_inst_id}}",
                  "cust_no": "${{current.cust_id}}"
                }
              },
              "props": {
                "title": "已办事项详情"
              }
            },
            {
              "type": "edit",
              "label": "流程跟踪",
              "toRequest": {
                "url": "@ProcessImgView",
                "method": "GET",
                "params": {
                  "appId":"waOnl",
                  "from": "doneList"
                }
              },
              "props": {
                "width": "1200px",
                "title": "流程跟踪图"
              }
            }
          ]
        }
      ]
    }
  }
}
