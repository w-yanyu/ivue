{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "controls": [{
                    "task_no": {
                        "control": "text",
                        "rules": [{
                                "required": true,
                                "message": "请输入任务编号"
                            },
                            {
                                "max": 32,
                                "message": "长度不能超过32个字符"
                            }
                        ],
                        "disabled": "true",
                        "label": "任务编号",
                        "width": "125px"
                    },
                    "task_name": {
                        "control": "text",
                        "rules": [{
                            "required": true,
                            "message": "请输入任务名称"
                        }],
                        "label": "任务名称",
                        "width": "125px"
                    }

                },
                {
                    "cert_type": {
                        "control": "select",
                        "label": "证件类型",
                        "width": "125px",
                        "dict": {
                            "format": "label",
                            "dictKey": [
                                "US_E_PRI_CRET_TYPE"
                            ],
                            "dictType": "US_E_PRI_CRET_TYPE"
                        },
                        "rules": [{
                            "required": true,
                            "message": "请输入证件类型"
                        }]
                    },
                    "cert_no": {
                        "control": "text",
                        "label": "证件号码",
                        "width": "125px",
                        "rules": [{
                            "required": true,
                            "message": "请输入证件号码"
                        }]
                    }
                },
                {
                    "cust_review_type": {
                        "control": "select",
                        "rules": [{
                            "required": true,
                            "message": "请选择回访类型"
                        }],
                        "label": "回访类型",
                        "width": "125px",
                        "dict": {
                            "format": "label",
                            "dictKey": [
                                "PL_E_CUST_REVIWE_TYPE"
                            ],
                            "dictType": "PL_E_CUST_REVIWE_TYPE"
                        }
                    },
                    "check_prod": {
                        "control": "text",
                        "label": "检查产品",
                        "width": "125px",
                        "rules": [{
                            "required": true,
                            "message": "请输入检查产品"
                        }]
                    }
                },
                {
                    "check_time": {
                        "control": "dateTimePicker",
                        "label": "检查时间",
                        "width": "125px",
                        "disabledDate": "dateAfter",
                        "rules": [{
                            "required": true,
                            "message": "请输入授信编号"
                        }, {
                            "max": 100,
                            "message": "长度不能超过100个字符"
                        }]
                    },
                    "remark": {
                        "control": "text",
                        "label": "备注",
                        "width": "125px",
                        "rules": [{
                            "required": true,
                            "message": "请输入备注"
                        }, {
                            "max": 100,
                            "message": "长度不能超过100个字符"
                        }]
                    }
                },
                {
                    "cust_name": {
                        "control": "text",
                        "rules": [{
                                "required": true,
                                "message": "请输入客户姓名"
                            },
                            {
                                "max": 100,
                                "message": "长度不能超过100个字符"
                            }
                        ],
                        "label": "客户姓名",
                        "width": "125px"
                    },
                    "cr_grntg_num": {
                        "control": "text",
                        "label": "授信编号",
                        "width": "125px",
                        "rules": [{
                                "required": true,
                                "message": "请输入授信编号"
                            },
                            {
                                "max": 100,
                                "message": "长度不能超过100个字符"
                            }
                        ]
                    }
                }
            ]
        }
    }
}