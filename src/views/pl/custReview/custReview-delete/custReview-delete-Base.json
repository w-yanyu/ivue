{
    "module": "v_inputDataForm_btn",
    "layout": {
        "initReqeust": "${{params}}",
        "form": {
            "initDataOnce": true,
            "controls": [
                {
                    "auth_id": {
                        "control": "string",
                        "disabled": true,
                        "label": "授权ID"
                    },
                    "auth_name": {
                        "control": "string",
                        "disabled": true,
                        "label": "授权名称"
                    }
                },
                {
                    "amount_exchange_ind": {
                        "control": "string",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        },
                        "label": "折算标志"
                    },
                    "auth_ccy": {
                        "control": "string",
                        "label": "折算币种",
                        "prop": "auth_ccy",
                        "dict": {
                            "dictType": "MS_CURRENCY",
                            "dictKey": [
                                "MS_CURRENCY"
                            ],
                            "format": "label"
                        }
                    }
                },
                {
                    "remark": {
                        "control": "string",
                        "label": "备注",
                        "rules": [
                            {
                                "max": 200,
                                "message": "长度不能超过200个字符"
                            }
                        ]
                    }
                }
            ]
        }
    }
}