{
    "module": "v-form-editableDataGrid-btn",
    "layout": {
        "initReqeust": "${{params}}",
        "datagrid": {
            "name": "ctAuthInfo",
            "pagination": {
                "show": false
            },
            "thead": [
                {
                    "type": "index"
                },
                {
                    "type": "string",
                    "label": "授权ID",
                    "prop": "auth_id"
                },
                {
                    "type": "string",
                    "label": "角色ID",
                    "prop": "role_id"
                },
                {
                    "type": "string",
                    "label": "折算币种",
                    "prop": "auth_ccy",
                    "dict": {
                        "dictType": "MS_CURRENCY",
                        "dictKey": [
                            "MS_CURRENCY"
                        ],
                        "format": "label"
                    }
                },
                {
                    "type": "string",
                    "label": "现金最大授权金额",
                    "prop": "cash_max_limit_amt"
                },
                {
                    "type": "string",
                    "label": "转账最大授权金额",
                    "prop": "trxn_max_limit_amt"
                }
            ]
        }
    }
}