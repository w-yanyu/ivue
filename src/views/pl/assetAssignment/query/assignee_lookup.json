{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "asge_no": {
            "label": "受让人编号",
            "control": "text"
          },
          "asge": {
            "label": "受让人名称",
            "control": "text"
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {}
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pl0044"
        }
      },
      "thead": [
        {
          "type": "index"
        },
        {
          "prop": "asge_no",
          "label": "受让人编号",
          "align": "center"
        },
        {
          "prop": "asge",
          "label": "受让人名称",
          "align": "center"
        },
        {
          "prop": "assignee_type",
          "label": "受让人类型",
          "align": "center",
          "dict": {
            "dictType": "PL_E_ASSIGNEE_TYPE",
            "dictKey": [
              "PL_E_ASSIGNEE_TYPE"
            ],
            "format": "label"
          }
        },
        {
          "prop": "assignee_status",
          "label": "受让人状态",
          "align": "center",
          "dict": {
            "dictType": "PL_E_ASSIGNEE_STATUS",
            "dictKey": [
              "PL_E_ASSIGNEE_STATUS"
            ],
            "format": "label"
          }
        },
        {
          "label": "开户名称",
          "align": "center",
          "prop": "open_acct_name"
        },
        {
          "label": "开户行名称",
          "align": "center",
          "prop": "bank_name"
        },
        {
          "label": "开户行行号",
          "align": "center",
          "prop": "bank_no"
        },
        {
          "prop": "assignee_cycle",
          "label": "回收转付周期",
          "align": "center",
          "dict": {
            "dictType": "PL_E_E_ASSIGNEE_CYCLE",
            "dictKey": [
              "PL_E_E_ASSIGNEE_CYCLE"
            ],
            "format": "label"
          }
        },
        {
          "label": "备注",
          "align": "center",
          "prop": "remark"
        },
        {
          "label": "操作",
          "fixed": "right",
          "align": "center",
          "btns": [
            {
              "type": "bringback",
              "label": "选择"
            }
          ]
        }
      ]
    }
  }
}