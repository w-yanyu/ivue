{
  "module": "v-form-btn-dynamic",
  "layout": {
    "form": {
      "name":"detail",
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pl0028",
          "asmt_code": "${{params.asmt_code}}"
        }
      },
      
      "controls": [
        
        {
          "apply_no": {
            "control": "textarea",
            "disabled": true,
            "label": "申请编号",
            "width": "130px",
            "span":"10"
          },
          "asmt_code": {
            "control": "textarea",
            "disabled": true,
            "label": "转让编码",
            "width": "130px",
            "span":"10"
          },
          "busi_num": {
            "control": "textarea",
            "disabled": true,
            "label": "业务编号",
            "width": "130px",
            "span":"10"
          },
          "asge": {
            "control": "textarea",
            "disabled": true,
            "label": "受让方",
            "width": "130px",
            "span":"10"
          },
          "asmt_asset_orig_prc": {
            "control": "currency",
            "disabled": true,
            "label": "转让资产原价(元)",
            "width": "130px",
            "span":"10"
          },
          "evlan_way": {
            "control": "select",
            "disabled": true,
            "label": "评估方式",
            "width": "130px",
            "span":"10",
            "dict": {
              "dictType": "E_EVALUATION_WAY",
              "dictKey": [
                "E_EVALUATION_WAY"
              ]
            }
          },
          "asmt_prc": {
            "control": "currency",
            "disabled": true,
            "label": "转让价格(元)",
            "width": "130px",
            "span":"10"
          },
          "asmt_tm": {
            "control": "dateTimePicker",
            "disabled": true,
            "label": "转让时间",
            "width": "130px",
            "span":"10",
            "format": "yyyyMMdd"
          },
          "appt": {
            "control": "textarea",
            "disabled": true,
            "label": "申请人",
            "width": "130px",
            "span":"10"
          },
          "aply_dt": {
            "control": "dateTimePicker",
            "disabled": true,
            "label": "申请时间",
            "width": "130px",
            "span":"10",
            "format": "yyyyMMdd"
          },
          "cust_no": {
            "control": "textarea",
            "disabled": true,
            "label": "客户编码",
            "width": "130px",
            "span":"10"
          }
        }
      ]
    }
  }
}

