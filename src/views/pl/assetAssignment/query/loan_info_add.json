{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "foldLineNumber": 2,
      "controls": [{
          "cntrno": {
            "span": 8,
            "control": "text",
            "label": "合同编号",
            "rules": [{
              "max": 80,
              "message": "长度不能超过80个字符"
            }]
          },
          "loan_due_bill_num": {
            "span": 8,
            "control": "text",
            "label": "借据号",
            "rules": [{
              "max": 80,
              "message": "长度不能超过80个字符"
            }]
          },
          "cust_nm": {
            "span": 8,
            "control": "text",
            "label": "客户姓名"
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "syln0002"  ,
          "cust_no": "${{params.cust_no}}",
          "outlistid":"lnCpstnQryType"
        }
      },
      "thead": [{
        "label": "操作",
        "fixed": "left",
        "align": "center",
        "width": "160px",
        "btns": [
          {
            "type": "bringback",
            "label": "选择"
          }
        ]
      },
        {
          "label": "借据号",
          "prop": "loan_due_bill_num",
          "width": "200px"
        },
        {
          "label": "合同编号",
          "prop": "ctrct_num",
          "width": "80px"
        },
        {
          "label": "客户姓名",
          "prop": "cust_nm"
        },
        {
          "label": "起息日期",
          "prop": "strt_int_caln_dt"
        },
        {
          "label": "到期日期",
          "prop": "matu_dt"
        },
        {
          "label": "借据金额(元)",
          "prop": "due_bill_amt"
        }
      ]
    }
  }
}
