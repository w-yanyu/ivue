{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "btns": [{
          "type": "submit",
          "label": "提交",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "pl0031",
              "apply_no": "${{params.apply_no}}",
              "appt": "$g{{user_info.userCode}}"

            }
          }
        },
        {
          "type": "reset",
          "label": "重置",
          "confirm": {
            "title": "提示信息",
            "msg": "确定重置已填写的信息吗？",
            "confirmButtonText": "确定",
            "cancelButtonText": "取消"
          }
        }
      ],
      "controls": [{
        "cust_no": {
          "control": "hidden"
        },
        "transfer_type": {
          "control": "select",
          "span": 8,
          "label": "转让类型",
          "disabled": false,
          "rules": [
            {
              "required": true
            }
          ],
          "width": "25px",
          "dict": {
            "dictType": "PL_E_TRANSFER_TYPE",
            "dictKey": [
              "PL_E_TRANSFER_TYPE"
            ],
            "format": "label"
          },
          "events": {
            "showItem": {
              "0": [
                "busi_num"
              ],
              "1": [
                "busi_num1"
             
              ]
            }
          }
        },

        "busi_num": {
          "control": "lookup",
          "label": "借据号",
          "width": "25px",
          "name":"busi_num",
          "span": 8,
          "edit": true,
          "rules": [{
            "required": true,
            "message": "借据号不能为空!"
          }],
          "keyField": {
            "busi_num": "loan_due_bill_num",
            "asmt_asset_orig_prc": "due_bill_amt",
            "cust_no": "cust_num",
            "apply_date": "strt_int_caln_dt"
          },
          "toRequest": {
            "url": "/views/pl/assetAssignment/query/loan_info_add.json",
            "method": "get"
          },
          "props": {
            "title": "选择借据",
            "width": "80%"
          }
        },
        "busi_num1": {
          "control": "lookup",
          "label": "资产包",
          "width": "25px",
          "span": 8,
          "edit": true,
          "rules": [{
            "required": true,
            "message": "资产包不能为空!"
          }],
          "keyField": {
            "busi_num1": "package_no",
            "asmt_asset_orig_prc": "package_amt",
            "cust_no": "cust_num",
            "apply_date": "package_dt"
          },
          "toRequest": {
            "url": "/views/pl/assetPackage/packageAsset/packageAssetListLookup.json",
            "method": "get"
          },
          "props": {
            "title": "选择资产包",
            "width": "80%"
          }
        },

      

        "asmt_asset_orig_prc": {
          "control": "string",
          "span": 8,
          "label": "转让资产原价",
          "width": "25px",
          "disabled": true,
          "rules": [{
            "required": true,
            "message": "转让资产原价不能为空!"
          }]
        },
        "apply_date": {
          "span": 8,
          "control": "dateTimePicker",
          "label": "申请日期",
          "width": "25px",
          "disabled": true,
          "rules": [{
            "required": true,
            "message": "申请日期不能为空!"
          }]
        },
        "asmt_prc": {
          "span": 8,
          "control": "currency",
          "width": "25px",
          "label": "转让价格",
          "rules": [{
              "required": true,
              "message": "请输入数字"
            }
          ]
        },

        "asge": {
          "control": "lookup",
          "label": "受让人",
          "width": "25px",
          "span": 8,
          "edit": true,
          "rules": [{
            "required": true,
            "message": "受让人不能为空!"
          }],
          "keyField": {
            "asge_no": "asge_no",
            "asge": "asge"
          },
          "toRequest": {
            "url": "/views/pl/assetAssignment/query/assignee_lookup.json",
            "method": "get"
          },
          "props": {
            "title": "选择受让人",
            "width": "80%"
          }
        },
        "asge_no": {
          "span": 8,
          "control": "hidden",
          "label": "受让人编号"
        },
        "msg_template": {
          "control": "lookup",
          "width": "25px",
          "label": "对客短信公告模板",
          "span": 8,
          "edit": true,
          "rules": [{
            "required": true,
            "message": "对客短信公告模板不能为空!"
          }],
          "keyField": {
            "template_id": "template_id",
            "msg_template": "template_name"
          },
          "toRequest": {
            "url": "/views/msap/smsTemplate/smsTemplate_management_lookup.json",
            "method": "get"
          },
          "props": {
            "title": "选择短信模板",
            "width": "80%"
          }
        },
        "template_id": {
          "span": 8,
          "control": "hidden",
          "label": "短信模板id"
        },
        "evlan_way": {
          "control": "select",
          "label": "评估方式",
          "width": "25px",
          "span": 8,
          "dict": {
            "dictType": "PL_E_EVALUATION_WAY",
            "dictKey": [
              "PL_E_EVALUATION_WAY"
            ],
            "format": "label"
          },
          "rules": [{
            "required": true,
            "message": "请选择评估方式"
          }]
        },
        "asmt_date": {
          "span": 8,
          "control": "dateTimePicker",
          "label": "转让日期",
          "width": "25px",
          "rules": [{
            "required": false,
            "message": "请选择日期!"
          }]
        },
        "is_reports": {
          "label": "是否继续征信报送",
          "width": "25px",
          "span": 8,
          "control": "select",
          "dict": {
            "dictType": "MS_E_YESORNO",
            "dictKey": [
              "MS_E_YESORNO"
            ],
            "format": "value-label"
          }
        }        
      }]
    }
  }
}
