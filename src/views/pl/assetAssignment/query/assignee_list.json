{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "asge_no": {
            "label": "受让人编号",
            "control": "text"
          },
          "asge": {
            "label": "受让人名称",
            "control": "text"
          },
          "assignee_type": {
            "label": "受让人类型",
            "control": "select",
            "multiple": false,
            "dict": {
              "dictKey": [
                "PL_E_ASSIGNEE_TYPE"
              ],
              "dictType": "PL_E_ASSIGNEE_TYPE",
              "format": "label"
            }
          },
          "assignee_status": {
            "label": "受让人状态",
            "control": "select",
            "multiple": false,
            "dict": {
              "dictKey": [
                "PL_E_ASSIGNEE_STATUS"
              ],
              "dictType": "PL_E_ASSIGNEE_STATUS",
              "format": "label"
            }
          }
        }
      ]     
    },
    "toolbar": {
      "btns": [{
        "type": "add",
        "target": "dialog",
        "label": "新增",
        "toRequest": {
          "method": "GET",
          "url": "/views/pl/assetAssignment/query/assignee_add.json"
        },
        "props": {
          "width": "65%",
          "title": "资产转让信息录入"
        }
      }]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pl0044"
        }
      },
      "thead": [
        {
          "type": "index"
        },
        {
          "prop": "asge_no",
          "label": "受让人编号",
          "align": "center"
        },
        {
          "prop": "asge",
          "label": "受让人名称",
          "align": "center"
        },    
        {
          "prop": "assignee_type",
          "label": "受让人类型",
          "align": "center",
          "dict": {
            "dictType": "PL_E_ASSIGNEE_TYPE",
            "dictKey": [
              "PL_E_ASSIGNEE_TYPE"
            ],
            "format": "label"
          }
        },
        {
          "prop": "assignee_status",
          "label": "受让人状态",
          "align": "center",
          "dict": {
            "dictType": "PL_E_ASSIGNEE_STATUS",
            "dictKey": [
              "PL_E_ASSIGNEE_STATUS"
            ],
            "format": "label"
          }
        },

        {
          "label": "开户名称",
          "align": "center",
          "prop": "open_acct_name"
        },
        {
          "label": "开户行名称",
          "align": "center",
          "prop": "bank_name"
        },
        {
          "label": "开户行行号",
          "align": "center",
          "prop": "bank_no"
        },
        {
          "prop": "assignee_cycle",
          "label": "回收转付周期",
          "align": "center",
          "dict": {
            "dictType": "PL_E_E_ASSIGNEE_CYCLE",
            "dictKey": [
              "PL_E_E_ASSIGNEE_CYCLE"
            ],
            "format": "label"
          }
        },
        {
          "label": "备注",
          "align": "center",
          "prop": "remark"
        },
        {
          "label": "操作",
          "fixed": "right",
          "width": "200",
          "align": "center",
          "btns": [
            {
              "toRequest": {
                "method": "POST",
                "url": "/views/pl/assetAssignment/query/assignee_update.json",
                "params": {
                  "package_no": "${{current.package_no}}"
                }
              },
              "label": "维护",
              "type": "look",
              "props": {
                "width": "80%",
                "height": "80%",
                "title": "维护"
              }
            },
            {
              "type": "delete",
              "label": "删除",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "delete",
                "params": {
                  "servicecode": "pl0045"

                }
              },
              "confirm": {
                "title": "提示信息",
                "msg": "确定删除该信息吗？",
                "confirmButtonText": "确定",
                "cancelButtonText": "取消"
              }
            }
          ]
        }
      ]
    }
  }
}