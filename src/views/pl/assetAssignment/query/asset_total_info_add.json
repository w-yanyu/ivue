{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "btns": [{
          "type": "submit",
          "label": "提交",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "pl0032",
              "apply_no": "${{params.apply_no}}"
            }
          }
        },
        {
          "type": "reset",
          "label": "重置",
          "confirm": {
            "title": "提示信息",
            "msg": "确定重置已填写的信息吗？",
            "confirmButtonText": "确定",
            "cancelButtonText": "取消"
          }
        }
      ],
      "controls": [{
        "acctg_org": {
          "control": "lookup",
          "label": "账务机构",
          "span": 7,
          "edit": true,
          "rules": [{
            "required": true,
            "message": "账务机构不能为空!"
          }],
          "keyField": {
            "acctg_org": "branch_id",
            "corpno": "corpno",
            "branch_name": "branch_name"
          },
          "toRequest": {
            "url": "@UserBranchTreeManager",
            "method": "post",
            "params": {}
          },
          "props": {
            "title": "机构信息查询"
          }
        },
        "sell_amt": {
          "control": "currency",
          "span": 7,
          "label": "出售金额(元)",
          "rules": [{
              "required": true,
              "message": "请输入出售金额"
            }
          ]
        },
        "amt_acct_num": {
          "span": 7,
          "control": "text",
          "label": "存款账号",
          "rules": [{
              "required": true,
              "message": "请输入存款账号"
            },
            {
              "pattern": "^[0-9]{15,19}$",
              "message": "请输入15-19位的存款账号"
            }
          ]
        },
        "dept_acct_nm": {
          "span": 7,
          "control": "text",
          "label": "存款户名",
          "rules": [{
              "required": true,
              "message": "请输入存款户名"
            },
            {
              "max": 100,
              "message": "长度不能超过100个字符"
            }
          ]
        },
        "open_acct_bank": {
          "span": 7,
          "control": "text",
          "label": "开户银行",
          "rules": [{
              "required": true,
              "message": "请输入开户银行"
            },
            {
              "max": 100,
              "message": "长度不能超过100个字符"
            }
          ]
        }
      }]
    }
  }
}
