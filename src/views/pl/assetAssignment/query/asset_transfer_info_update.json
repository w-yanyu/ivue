{
    "module": "v-inputDataForm-btn",
    "layout": {
        "form": {
            "scope": "templateScope",
            "initRequest":"${{params}}",
            "initDataOnce":true,

            "btns": [{
                "type": "submit",
                "label": "保存",
                "doRequest": {
                    "url": "/SUMP/icmcall/icmRPCCall",
                    "method": "POST",
                    "params": {
                        "servicecode": "pl0040",
                        "apply_no": "${{params.apply_no}}",
                        "prim_key": "${{params.prim_key}}",
                        "busi_num": "${{params.busi_num}}",
                        "busi_num1": "${{params.busi_num1}}"
                    }
                }
            }],

            "controls": [{
                "cust_no": {
                    "control": "hidden"
                },
                "apply_no": {
                    "control": "hidden"
                },
                "prim_key": {
                    "control": "hidden"
                },
                "asmt_code": {
                    "control": "hidden"
                },
                "transfer_type": {
                  "control": "select",
                  "span": 8,
                  "label": "转让类型",
                  "disabled": true,
                  "rules": [
                    {
                      "required": true
                    }
                  ],
                  "width": "25px",
                  "dict": {
                    "dictType": "PL_E_TRANSFER_TYPE",
                    "dictKey": [
                      "PL_E_TRANSFER_TYPE"
                    ],
                    "format": "label"
                  },
                  "events": {
                    "showItem": {
                      "0": [
                        "busi_num"
                      ],
                      "1": [
                        "busi_num1"
                     
                      ]
                    }
                  }
                },
        
                "busi_num": {
                  "control": "lookup",
                  "label": "借据号",
                  "width": "25px",
                  "name":"busi_num",
                  "span": 8,
                  "edit": true,
                  "disabled":true,
                  "rules": [{
                    "required": true,
                    "message": "借据号不能为空!"
                  }],
                  "keyField": {
                    "busi_num": "loan_due_bill_num",
                    "asmt_asset_orig_prc": "due_bill_amt",
                    "cust_no": "cust_num",
                    "apply_date": "strt_int_caln_dt"
                  },
                  "toRequest": {
                    "url": "/views/pl/assetAssignment/query/loan_info_add.json",
                    "method": "get"
                  },
                  "props": {
                    "title": "选择借据",
                    "width": "80%"
                  }
                },
                "busi_num1": {
                  "control": "lookup",
                  "label": "资产包",
                  "width": "25px",
                  "disabled":true,
                  "span": 8,
                  "edit": true,
                  "rules": [{
                    "required": true,
                    "message": "资产包不能为空!"
                  }],
                  "keyField": {
                    "busi_num1": "package_no",
                    "asmt_asset_orig_prc": "package_amt",
                    "cust_no": "cust_num",
                    "apply_date": "package_dt"
                  },
                  "toRequest": {
                    "url": "/views/pl/assetPackage/packageAsset/packageAssetListLookup.json",
                    "method": "get"
                  },
                  "props": {
                    "title": "选择资产包",
                    "width": "80%"
                  }
                },
                "asmt_asset_orig_prc": {
                    "control": "string",
                    "width": "25px",
                    "span": 8,
                    "label": "转让资产原价"
                },
   
                "asmt_prc": {
                    "span": 8,
                    "width": "25px",
                    "control": "text",
                    "label": "转让价格",
                    "rules": [{
                            "required": true,
                            "pattern": "^[0-9]+(\\.[0-9]{2})?$",
                            "message": "请输入两位小数或整数数字"
                        },
                        {
                            "max": 21,
                            "message": "长度不能超过21个字符"
                        }
                    ]
                },
                "asge": {
                    "control": "lookup",
                    "label": "受让人",
                    "width": "25px",
                    "span": 8,
                    "edit": true,
                    "rules": [{
                      "required": true,
                      "message": "受让人不能为空!"
                    }],
                    "keyField": {
                      "asge_no": "asge_no",
                      "asge": "asge"
                    },
                    "toRequest": {
                      "url": "/views/pl/assetAssignment/query/assignee_lookup.json",
                      "method": "get"
                    },
                    "props": {
                      "title": "选择受让人",
                      "width": "80%"
                    }
                  },
                  "asge_no": {
                    "span": 8,
                    "control": "hidden",
                    "label": "受让人编号"
                  },
                  "msg_template": {
                    "control": "lookup",
                    "width": "25px",
                    "label": "对客短信公告模板",
                    "span": 8,
                    "edit": true,
                    "rules": [{
                      "required": true,
                      "message": "对客短信公告模板不能为空!"
                    }],
                    "keyField": {
                      "template_id": "template_id",
                      "msg_template": "template_name"
                    },
                    "toRequest": {
                      "url": "/views/msap/smsTemplate/smsTemplate_management_lookup.json",
                      "method": "get"
                    },
                    "props": {
                      "title": "选择短信模板",
                      "width": "80%"
                    }
                  },
                  "template_id": {
                    "span": 8,
                    "control": "hidden",
                    "label": "短信模板id"
                  },
                  "evlan_way": {
                    "control": "select",
                    "label": "评估方式",
                    "width": "25px",
                    "span": 8,
                    "dict": {
                      "dictType": "PL_E_EVALUATION_WAY",
                      "dictKey": [
                        "PL_E_EVALUATION_WAY"
                      ],
                      "format": "label"
                    },
                    "rules": [{
                      "required": true,
                      "message": "请选择评估方式"
                    }]
                  },
                "asmt_date": {
                    "value": "${{current.asmt_tm}}",
                    "span": 8,
                    "control": "dateTimePicker",
                    "label": "转让日期",
                    "width": "25px",
                    "rules": [{
                        "required": false,
                        "message": "请选择日期!"
                    }]
                },
                "is_reports": {
                    "label": "是否继续征信报送",
                    "width": "25px",
                    "span": 8,
                    "control": "select",
                    "dict": {
                      "dictType": "MS_E_YESORNO",
                      "dictKey": [
                        "MS_E_YESORNO"
                      ],
                      "format": "value-label"
                    }
                  } 
            }]
        }
    }
}