{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "btns": [{
          "type": "submit",
          "label": "提交",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "pl0042"
            }
          }
        }
      ],
      "controls": [{
        "asge": {
          "label": "受让人名称",
          "control": "text"
        },
        "assignee_type": {
          "label": "受让人类型",
          "control": "select",
          "multiple": false,
          "dict": {
            "dictKey": [
              "PL_E_ASSIGNEE_TYPE"
            ],
            "dictType": "PL_E_ASSIGNEE_TYPE",
            "format": "label"
          }
        },
        "assignee_status": {
          "label": "受让人状态",
          "control": "select",
          "multiple": false,
          "dict": {
            "dictKey": [
              "PL_E_ASSIGNEE_STATUS"
            ],
            "dictType": "PL_E_ASSIGNEE_STATUS",
            "format": "label"
          }
        },
        "open_acct_name": {
          "label": "开户名称",
          "control": "text"
        },
        "bank_name": {
          "label": "开户行名称",
          "control": "text"
        },
        "bank_no": {
          "label": "开户行行号",
          "control": "text"
        },
        "assignee_cycle": {
          "label": "回收转付周期",
          "control": "select",
          "multiple": false,
          "dict": {
            "dictKey": [
              "PL_E_E_ASSIGNEE_CYCLE"
            ],
            "dictType": "PL_E_E_ASSIGNEE_CYCLE",
            "format": "label"
          }
        },
        "remark": {
          "control": "textarea",
          "label": "备注"
        }
      }]
    }
  }
}
