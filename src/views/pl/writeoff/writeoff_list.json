{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "foldLineNumber": 3,
      "controls": [
        {
          "apply_no": {
            "control": "text",
            "label": "申请编号"
          },
          "cust_name": {
            "control": "text",
            "label": "客户名称"
          },
          "loan_no": {
            "control": "text",
            "label": "借据号"
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pl0313",
          "sizePage": 10
        }
      },
      "thead": [
        {
          "label": "申请人",
          "align": "center",
          "prop": "cust_name"
        },
        {
          "label": "申请号",
          "align": "center",
          "prop": "apply_no"
        },
        {
          "label": "借据号",
          "align": "center",
          "prop": "loan_no"
        },
        {
          "label": "本金合计",
          "align": "center",
          "prop": "prcpl_smtn"
        },
        {
          "label": "利息合计",
          "align": "center",
          "prop": "int_smtn"
        },
        {
          "label": "核销本金",
          "align": "center",
          "prop": "write_off_princi"
        },
        {
          "label": "核销利息",
          "align": "center",
          "prop": "write_off_inst"
        },
        {
          "label": "备注",
          "align": "center",
          "prop": "remark"
        },
        {
          "label": "操作",
          "align":"center",
          "fixed": "right",
          "width": 200,
          "btns": [
           
            {
              "type": "edit",
              "label": "详情",
              
              "toRequest": {
                "url": "@PlWriteOffApplyDetail",
                "method": "GET"
              },
              "props": {
                "width": "80%",
                "title": "核销详情"
              }
            }
           
          ]
        }

      ]
    }
  }
}