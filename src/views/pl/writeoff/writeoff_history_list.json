{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pl0311",
          "loan_no": "${{params.loan_no}}"
        }
      },
      "thead": [
        {
          "label": "申请人",
          "align": "center",
          "prop": "cust_name"
        },
        {
          "label": "申请号",
          "align": "center",
          "prop": "apply_no"
        },
        {
          "label": "本金合计",
          "align": "center",
          "prop": "prcpl_smtn"
        },
        {
          "label": "利息合计",
          "align": "center",
          "prop": "int_smtn"
        },
        {
          "label": "核销本金",
          "align": "center",
          "prop": "write_off_princi"
        },
        {
          "label": "核销利息",
          "align": "center",
          "prop": "write_off_inst"
        },
        {
          "label": "备注",
          "align": "center",
          "prop": "remark"
        }

      ]
    }
  }
}
