{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "cost_type": {
            "label": "资金成本类型",
            "control": "select",
            "dict": {
              "dictType": "PL_E_FUND_COST_TYPE",
              "dictKey": [
                "PL_E_FUND_COST_TYPE"
              ],
              "format": "label"
            }           
        },              
          "offer_dt": {
            "label": "报价日期",
            "control": "dateTimePicker",
            "type": "date",
            "format":"yyyyMMdd"   
          },
          "channel_id": {
            "label": "渠道",
            "control": "select",
            "multiple": false,
            "dict": {
              "dictKey": [
                "PL_E_FUND_CHANNEL"
              ],
              "dictType": "PL_E_FUND_CHANNEL",
              "format": "label"
            }
          },    
          "term_day": {
            "label": "期限(天)",
            "control": "inputNumber",
            "precision":0      
        }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "target": "dialog",
          "label": "新增",
          "toRequest": {
            "method": "GET",
            "url": "/views/pl/assetPackage/fundCost/addFundCost.json"
          }
        }

      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pl0712"
        }
      },
      "thead": [
        {
          "type": "index"
        },
        {
          "prop": "fund_cost_no",
          "label": "资金成本编号",
          "align": "center"
        },
        {
          "prop": "cost_type",
          "label": "资金成本类型",
          "align": "center",
          "dict": {
            "dictType": "PL_E_FUND_COST_TYPE",
            "dictKey": [
              "PL_E_FUND_COST_TYPE"
            ],
            "format": "label"
          }
        },
        {
          "label": "报价日期",        
          "align": "center",
          "prop": "offer_dt"

        },
        {
          "prop": "channel_id",
          "label": "渠道",
          "align": "center",
          "dict": {
            "dictType": "PL_E_FUND_CHANNEL",
            "dictKey": [
              "PL_E_FUND_CHANNEL"
            ],
            "format": "label"
          }
        },
        {
          "label": "产品名称",        
          "align": "center",
          "prop": "prod_name"

        },
        {
          "label": "期限(天)",        
          "align": "center",
          "prop": "term_day"

        },
        {
          "label": "资金成本",        
          "align": "center",
          "prop": "fund_cost"

        },
        {
          "label": "操作",
          "fixed": "right",
          "width": "200",
          "align": "center",
          "btns": [
            {
              "toRequest": {
                "method": "POST",
                "url": "/views/pl/assetPackage/fundCost/updFundCost.json"
              },
              "label": "维护",
              "type": "edit"
            },
            {
              "doRequest": {
                "method": "POST",
                "url": "/SUMP/icmcall/icmRPCCall",
                "params": {
                  "servicecode": "pl0711",
                  "method": "del"
                }
              },
              "label": "删除",
              "type": "delete"
            }                      
          ]
        }
      ]
    }
  }
}
