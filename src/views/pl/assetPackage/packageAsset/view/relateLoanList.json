{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "btns": [],
      "controls": []
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pl0719",
          "package_no": "${{params.package_no}}"
        }
      },
      "thead": [
        {
          "prop": "loan_no",
          "label": "借据号",
          "align": "center"
        },
        {
          "prop": "cust_name",
          "label": "客户姓名",
          "align": "center"
        },
        {
          "label": "放款日期",
          "align": "center",
          "prop": "open_acct_dt"
        },
        {
          "label": "借据金额",
          "align": "center",
          "prop": "due_bill_amt"
        },
        {
          "label": "到期日期",
          "align": "center",
          "prop": "matu_date"
        },
        {
          "prop": "repay_type",
          "label": "还款方式",
          "align": "center",
          "dict": {
            "dictType": "LN_E_RPYMT_WAY",
            "dictKey": [
              "LN_E_RPYMT_WAY"
            ],
            "format": "label"
          }
        },
        {
          "prop": "loan_form",
          "label": "贷款形态",
          "align": "center",
          "dict": {
            "dictType": "LN_E_LOAN_FORM",
            "dictKey": [
              "LN_E_LOAN_FORM"
            ],
            "format": "label"
          }
        },
        {
          "prop": "loan_acct_type",
          "label": "贷款账户状态",
          "align": "center",
          "dict": {
            "dictType": "LN_E_LOAN_ACCT_STE",
            "dictKey": [
              "LN_E_LOAN_ACCT_STE"
            ],
            "format": "label"
          }
        }
       
      ]
    }
  }
}