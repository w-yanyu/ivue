{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "btns": [],
      "controls": []
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pl0724",
          "package_no": "${{params.package_no}}"
        }
      },
      "thead": [
        {
          "prop": "operation_date",
          "label": "操作日期",
          "align": "center"
        },
        {
          "prop": "name_yield",
          "label": "名义收益率(%)",
          "align": "center"
        },
        {
          "label": "剩余利息之和",
          "align": "center",
          "prop": "rest_inst_sum"
        },
        {
          "label": "剩余本金之和",
          "align": "center",
          "prop": "rest_prin_sum"
        },
        {
          "label": "剩余本息之和",
          "align": "center",
          "prop": "rest_prin_inst_sum"
        },
        {
          "prop": "cash_sum",
          "label": "现金流现值之和",
          "align": "center"
        },    
        {
          "prop": "accrual_yield",
          "label": "实际收益率(%)",
          "align": "center"
        }
      ]
    }
  }
}