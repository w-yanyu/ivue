{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "btns": [],
      "controls": []
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pl0046",
          "package_no": "${{params.package_no}}"
        }
      },
      "thead": [
        {
          "prop": "loan_no",
          "label": "借据号",
          "align": "center"
        },
        {
          "prop": "cust_nm",
          "label": "客户姓名",
          "align": "center"
        },
        {
          "label": "放款日期",
          "align": "center",
          "prop": "lend_date"
        },
        {
          "label": "到期日期",
          "align": "center",
          "prop": "matu_date"
        },
        {
          "label": "回收金额",
          "align": "center",
          "prop": "recovery_amount",
          "format":{
						"decimal":2,
						"decimalPoint":".",
						"thousandsPoint":","
					}
        },
        
        {
          "label": "转付批次号",
          "align": "center",
          "prop": "transfer_num"
        },
                
        {
          "label": "转付日期",
          "align": "center",
          "prop": "transfer_date"
        }
       
      ]
    }
  }
}