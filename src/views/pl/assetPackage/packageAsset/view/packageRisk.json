{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "btns": [],
      "controls": []
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pl0723",
          "package_no": "${{params.package_no}}"
        }
      },
      "thead": [
        {
          "prop": "operation_date",
          "label": "数据日期",
          "align": "center"
        },
        {
          "prop": "pd_rate",
          "label": "PD数据指标",
          "align": "center"
        },
        {
          "prop": "lgd_rate",
          "label": "LGD数据指标",
          "align": "center"
        },
        {
          "label": "EAD指数",
          "align": "center",
          "prop": "ead_rate"
        },
        {
          "label": "EL指数",
          "align": "center",
          "prop": "el_rate"
        },
        {
          "label": "风险准备计提测算",
          "align": "center",
          "prop": "risk_accrual_ready"
        },
        {
          "prop": "special_accrual_ready",
          "label": "专项计提准备",
          "align": "center"
        },
        {
          "prop": "nomal_accrual_ready",
          "label": "正常计提准备",
          "align": "center"
        }
      ]
    }
  }
}