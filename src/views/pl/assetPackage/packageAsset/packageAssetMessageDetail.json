{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "controls": [],
      "btns": []
    },
    "toolbar": {
      "btns": []
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pl0735",
          "package_no":"${{params.package_no}}",
          "template_id":"${{params.template_id}}"
        }
      },
      "pagination": {
        "pageSizes": [
          5,
          10,
          15,
          20
        ],
        "pageSize": 5,
        "show": false
      },
      "thead": [
        {
          "type": "index"
        },
        {
          "prop": "loan_no",
          "label": "借据号",
          "align": "center"
        },
        {
          "prop": "cust_name",
          "label": "客户姓名",
          "align": "center"
        },
        {
          "prop": "loan_status",
          "label": "贷款账户状态",
          "align": "center",
          "dict": {
            "dictType": "LN_E_LOAN_ACCT_STE",
            "dictKey": [
              "LN_E_LOAN_ACCT_STE"
            ],
            "format": "label"
          }
        },
        {
          "prop": "template_id",
          "label": "短信模板编号",
          "align": "center"
        },
        {
          "prop": "template_name",
          "label": "短信模板名称",
          "align": "center"
        },
        {
          "prop": "sms_content",
          "label": "短信内容",
          "align": "center"
        },
        {
          "prop": "mobile_no",
          "label": "手机号",
          "align": "center"
        },
        {
          "prop": "send_status",
          "label": "发送状态",
          "align": "center",
          "dict":{
            "dictType": "MS_E_PROCESS_STATUS",
            "dictKey": [
              "MS_E_PROCESS_STATUS"
            ],
            "format": "value-label"
          }
        }
      ]
    }
  }
}