{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pl0721",
          "package_no": "${{params.package_no}}"
        }
      },
      "controls": [
        {
          "package_no": {
            "label": "资产包编号",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入资产包名称"
              }
            ]
          },
          "package_name": {
            "label": "资产包名称",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入资产包名称"
              }
            ]
          },
          "channel_id": {
            "label": "渠道",
            "control": "select",
            "multiple": false,
            "dict": {
              "dictKey": [
                "PL_E_FUND_CHANNEL"
              ],
              "dictType": "PL_E_FUND_CHANNEL",
              "format": "label"
            }
          },
          "loan_use": {
            "label": "贷款用途",
            "control": "text"
          },
          "loan_form": {
            "label": "贷款形态",
            "control": "select",
            "multiple": false,
            "dict": {
              "dictKey": [
                "LN_E_LOAN_FORM"
              ],
              "dictType": "LN_E_LOAN_FORM",
              "format": "label"
            }
          },
          "lend_start_dt": {
            "label": "贷款发放起始日期",
            "control": "dateTimePicker",
            "type": "date",
            "format": "yyyyMMdd",
            "rules": [
              {
                "max": 8,
                "message": "长度不能超过8个字符"
              }
            ]
          },
          "lend_end_dt": {
            "label": "贷款发放截止日期",
            "control": "dateTimePicker",
            "type": "date",
            "format": "yyyyMMdd",
            "rules": [
              {
                "max": 8,
                "message": "长度不能超过8个字符"
              }
            ]
          },
          "loan_acct_type": {
            "label": "贷款账户状态",
            "control": "select",
            "multiple": false,
            "dict": {
              "dictKey": [
                "LN_E_LOAN_ACCT_STE"
              ],
              "dictType": "LN_E_LOAN_ACCT_STE",
              "format": "label"
            }
          },
          "repay_type": {
            "label": "还款方式",
            "control": "select",
            "multiple": false,
            "dict": {
              "dictKey": [
                "LN_E_RPYMT_WAY"
              ],
              "dictType": "LN_E_RPYMT_WAY",
              "format": "label"
            }
          },
          "inst_rate_min": {
            "label": "执行利率最小值",
            "control": "inputNumber",
            "precision": 6
          },
          "inst_rate_max": {
            "label": "执行利率最大值",
            "control": "inputNumber",
            "precision": 6
          },
          "cust_name": {
            "label": "客户名称",
            "control": "text"
          },
          "new_industry_invest": {
            "label": "新行业投向",
            "control": "text"
          },
          "guar_type": {
            "label": "担保方式",
            "control": "select",
            "multiple": false,
            "dict": {
              "dictKey": [
                "MS_E_GUAR_TYPE"
              ],
              "dictType": "MS_E_GUAR_TYPE",
              "format": "label"
            }
          },
          "surplus_min_day": {
            "label": "剩余期限（天）最小值",
            "control": "inputNumber",
            "precision": 0,
            "rules": [
              {
                "pattern": "^[1-9]\\d*|0$",
                "message": "请输入正整数！"
              }
            ]
          },
          "surplus_max_day": {
            "label": "剩余期限（天）最大值",
            "control": "inputNumber",
            "precision": 0,
            "rules": [
              {
                "pattern": "^[1-9]\\d*|0$",
                "message": "请输入正整数！"
              }
            ]
          },
          "manage_cost": {
            "label": "管理成本",
            "control": "inputNumber",
            "precision": 2
          }
        }
      ]
    }
  }
}