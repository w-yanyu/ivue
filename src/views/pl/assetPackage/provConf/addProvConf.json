{
  "module": "v_inputDataForm_btn",
  "layout": {
      "form": {
          "btns": [
              {
                  "type": "submit",
                  "label": "提交",
                  "icon": "",
                  "doRequest": {
                      "url": "/SUMP/icmcall/icmRPCCall",
                      "method": "POST",
                      "params": {
                          "servicecode": "pl0701"
                      }
                  }
              }
          ],
          "controls": [
              {
                  "prvn_acrl_code": {
                      "label": "拨备计提代码",
                      "control": "text",
                      "rules": [
                          {
                              "required": true,
                              "message":"拨备计提代码"
                          },
                          {
                            "max": 32,
                            "message": "长度不能超过32个字符"
                          }
                      ]
                  },
                  "prvn_acrl_nm": {
                      "label": "拨备计提名称",
                      "control": "text",
                      "rules": [
                          {
                              "required": true,
                              "message":"拨备计提名称"
                          },
                          {
                            "max": 80,
                            "message": "长度不能超过80个字符"
                          }
                      ]
                  },
                  "five_class_classification": {
                    "label": "五级分类级别",
                    "control": "select",
                    "multiple": false,
                    "dict": {
                      "dictKey": [
                        "PL_E_CLEVEL"
                      ],
                      "dictType": "PL_E_CLEVEL",
                      "format": "label"
                    },
                    "rules": [
                      {
                        "required": true,
                        "message":"请选择五级分类级别"  
                      }
                    ]
                  },
                  "nrl_prvn_acrl_pcnt": {
                      "label": "拨备计提比例",
                      "control": "inputNumber",
                      "precision":0,
                      "rules": [
                          {
                              "required": true,
                              "message":"正常拨备计提比例"
                          },
                          {
                            "pattern": "^[1-9]\\d*|0$",
                            "message": "请输入正整数！"
                          }
                      ]
                  },
                  "prvn_acrl_pcnt_upr_lmt": {
                      "label": "拨备计提比例上限",
                      "control": "inputNumber",
                      "rules": [
                        {
                          "pattern": "^[1-9]\\d*|0$",
                          "message": "请输入正整数！"
                        }
                      ]
                  },
                  "ovdue_dys_down_lmt": {
                    "label": "逾期天数下限",
                    "control": "inputNumber",
                    "rules": [
                      {
                        "pattern": "^[1-9]\\d*|0$",
                        "message": "请输入正整数！"
                      }
                    ]
                },
                  "ovdue_dys_upr_lmt": {
                      "label": "逾期天数上限",
                      "control": "inputNumber",
                      "rules": [
                        {
                          "pattern": "^[1-9]\\d*|0$",
                          "message": "请输入正整数！"
                        }
                      ]
                  }                        
               }
          ]
      }
  }
}
