{
  "module": "v_inputDataForm_btn",
  "layout": {
      "form": {
          "btns": [
              {
                  "type": "submit",
                  "label": "提交",
                  "icon": "",
                  "doRequest": {
                      "url": "/SUMP/icmcall/icmRPCCall",
                      "method": "POST",
                      "params": {
                          "servicecode": "pl0705"
                      }
                  }
              }
          ],
          "controls": [
              {
                  "reg_date": {
                      "label": "登记日期",
                      "control": "dateTimePicker",
                      "type": "date",
            
                      "format":"yyyyMMdd",                   
                      "rules": [
                          {
                              "required": true,
                              "message":"登记日期"
                          },
                          {
                            "max": 8,
                            "message": "长度不能超过8个字符"
                          }
                      ]
                  },                  
                  "term_day": {
                      "label": "期限(天)",
                      "control": "inputNumber",
                      "precision":0,
                      "rules": [
                          {
                              "required": true,
                              "message":"期限(天)"
                          },
                          {
                            "pattern": "^[1-9]\\d*|0$",
                            "message": "请输入正整数！"
                          }
                      ]
                  },
                  "bchmk_int_rate": {
                      "label": "利率(%)",
                      "control": "inputNumber",
                      "precision":6,
                      "rules": [
                        {
                          "required": true,
                          "message":"利率"
                      }
                      ]
                  }                      
               }
          ]
      }
  }
}
