{
  "module": "v_inputDataForm_btn",
  "layout": {
      "form": {
        "initRequest": "${{params}}",
          "btns": [
              {
                  "type": "submit",
                  "label": "保存",
                  "icon": "",
                  "doRequest": {
                      "url": "/SUMP/icmcall/icmRPCCall",
                      "method": "POST",
                      "params": {
                          "servicecode": "pl0706"
                      }
                  }
              }
          ],
          "controls": [
              {
                "reg_date": {
                  "label": "登记日期",
                  "control": "dateTimePicker",
                  "type": "date",
                  "format":"yyyyMMdd",         
                  "disabled":true                
                 
              },                        
              "term_day": {
                  "label": "期限(天)",
                  "control": "inputNumber",
                  "disabled":true,
                  "precision":0
              },
              "bchmk_int_rate": {
                  "label": "利率(%)",
                  "control": "inputNumber",
                  "precision":6,
                  "rules": [
                    {
                      "required": true,
                      "message":"利率(%)"
                  }
                  ]
              }                      
           }
          ]
      }
  }
}
