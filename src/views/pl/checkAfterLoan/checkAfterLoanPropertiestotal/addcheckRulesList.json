{
  "module": "v_inputDataForm_btn",
  "layout": {
      "form": {
          "btns": [
              {
                  "type": "submit",
                  "label": "提交",
                  "icon": "",
                  "doRequest": {
                      "url": "/SUMP/icmcall/icmRPCCall",
                      "method": "POST",
                      "params": {
                          "servicecode": "pl0156"
                      }
                  }
              }
          ],
          "controls": [
              {
                  "check_rule_no": {
                      "label": "检查规则编号",
                      "control": "text",
                      "rules": [
                          {
                              "required": true,
                              "message":"请输入检查规则编号"
                          }
                      ]
                  },
                  "check_rule_desc": {
                      "label": "检查规则描述",
                      "control": "text",
                      "rules": [
                          {
                              "required": true,
                              "message":"请输入检查规则描述"
                          }
                      ]
                  },
                  "first_check_date": {
                      "label": "首次检查宽限期",
                      "control": "dateTimePicker",
                      "rules": [
                          {
                              "required": true,
                              "message":"请输入首次检查宽限期"
                          }
                      ]
                  },
                  "check_frequency": {
                      "label": "默认检查频率",
                      "control": "text",
                      "rules": [
                          {
                              "required": true,
                              "message":"请输入默认检查频率"
                          }
                      ]
                  },
                  "is_need_check": {
                    "label": "是否首次检查",
                    "control": "select",
                    "multiple": false,
                    "dict": {
                      "dictKey": [
                        "MS_E_YESORNO"
                      ],
                      "dictType": "MS_E_YESORNO",
                      "format": "label"
                    },
                    "rules": [
                      {
                        "required": true,
                        "message":"请输入是否首次检查"
                      }
                    ]
                  },
                  "check_flow": {
                      "label": "检查流程",
                      "control": "text",
                      "rules": [
                          {
                              "required": true,
                              "message":"请输入检查流程"
                          }
                      ]
                  },
                  "rule_state": {
                    "label": "规则状态",
                    "control": "select",
                    "multiple": false,
                    "dict": {
                      "dictKey": [
                        "PL_E_ISFLAG"
                      ],
                      "dictType": "PL_E_ISFLAG",
                      "format": "label"
                    },
                    "rules": [
                      {
                        "required": true,
                        "message":"请输入规则状态"
                      }
                    ]
                  }
               }
          ]
      }
  }
}
