{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": "",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "pl0152",
                            "check_rule_no": "${{params.check_rule_no}}"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "condition_no": {
                        "label": "条件编号",
                        "control": "text",
                        "rules": [
                            {
                                "required": true,
                                "message":"请输入条件编号"
                            }
                        ]
                    },
                    "condition_desc": {
                        "label": "条件描述",
                        "control": "text",
                        "rules": [
                            {
                                "required": true,
                                "message":"请输入条件描述"
                            }
                        ]
                    },
                    "condition_table_name": {
                        "label": "表名",
                        "control": "text",
                        "rules": [
                            {
                                "required": true,
                                "message":"请输入表名"
                            }
                        ]
                    },
                    "condition_field": {
                        "label": "表字段",
                        "control": "text",
                        "rules": [
                            {
                                "required": true,
                                "message":"请输入表字段"
                            }
                        ]
                    },
                    "related_with_field": {
                        "label": "同主表关联字段",
                        "control": "text",
                        "rules": [
                            {
                            }
                        ]
                    } ,
                    "operator": {
                        "label": "操作符",
                        "control": "select",
                        "multiple": false,
                        "dict": {
                          "dictKey": [
                            "PL_E_OPERATION"
                          ],
                          "dictType": "PL_E_OPERATION",
                          "format": "label"
                        },
                        "rules": [
                          {
                            "required": true,
                            "message":"请输入操作符"
                          }
                        ]
                      },
                      "value_category": {
                        "label": "值类别",
                        "control": "select",
                        "multiple": false,
                        "dict": {
                          "dictKey": [
                            "PL_E_VALUES"
                          ],
                          "dictType": "PL_E_VALUES",
                          "format": "label"
                        },
                        "rules": [
                          {
                            "required": true,
                            "message":"请输入值类别"
                          }
                        ]
                      },
                    "field_value": {
                        "label": "值",
                        "control": "text",
                        "rules": [
                            {
                                "required": true,
                                "message":"请输入值"
                            }
                        ]
                    },
                    "rule_busi_type": {
                        "label": "业务类型",
                        "control": "select",
                        "multiple": false,
                        "dict": {
                          "dictKey": [
                            "PL_E_LNCATP2"
                          ],
                          "dictType": "PL_E_LNCATP2",
                          "format": "label"
                        },
                        "rules": [
                          {
                            "required": true,
                            "message":"请输入业务类型"
                          }
                        ]
                      },
                    "check_frequency": {
                        "label": "检查频率",
                        "control": "text",
                        "rules": [
                            {
                                "required": true,
                                "message":"请输入检查频率"
                            }
                        ]
                    }
                }
            ]
        }
    }
}
