{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "scope": "templateScope",
            "initRequest":"${{params}}",
            "initDataOnce":true,


          "btns": [
              {
                  "type": "submit",
                  "label": "保存",
                  "icon": "",
                  "doRequest": {
                      "url": "/SUMP/icmcall/icmRPCCall",
                      "method": "POST",
                      "params": {
                          "servicecode": "pl0154",
                          "check_rule_no": "${{params.check_rule_no}}"
                      }
                  }
              }
          ],
          "controls": [
            {
                "condition_no": {
                    "label": "条件编号",
                    "control": "text",
                    "disabled": true,
                    "rules": [
                        {
                            "required": true
                        }
                    ]
                },
                "condition_desc": {
                    "label": "条件描述",
                    "control": "text",
                    "rules": [
                        {
                            "required": true
                        }
                    ]
                },
                "condition_table_name": {
                    "label": "表名",
                    "control": "text",
                    "rules": [
                        {
                            "required": true
                        }
                    ]
                },
                "condition_field": {
                    "label": "表字段",
                    "control": "text",
                    "rules": [
                        {
                            "required": true
                        }
                    ]
                },
                "related_with_field": {
                    "label": "同主表关联字段",
                    "control": "text",
                    "rules": [
                        {
                        }
                    ]
                } ,
                "operator": {
                    "label": "操作符",
                    "control": "select",
                    "multiple": false,
                    "dict": {
                      "dictKey": [
                        "PL_E_OPERATION"
                      ],
                      "dictType": "PL_E_OPERATION",
                      "format": "label"
                    },
                    "rules": [
                      {
                        "required": true
                      }
                    ]
                  },
                  "value_category": {
                    "label": "值类别",
                    "control": "select",
                    "multiple": false,
                    "dict": {
                      "dictKey": [
                        "PL_E_VALUES"
                      ],
                      "dictType": "PL_E_VALUES",
                      "format": "label"
                    },
                    "rules": [
                      {
                        "required": true
                      }
                    ]
                  },
                "field_value": {
                    "label": "值",
                    "control": "text",
                    "rules": [
                        {
                            "required": true
                        }
                    ]
                },
                "rule_busi_type": {
                    "label": "业务类型",
                    "control": "select",
                    "multiple": false,
                    "dict": {
                      "dictKey": [
                        "PL_E_LNCATP2"
                      ],
                      "dictType": "PL_E_LNCATP2",
                      "format": "label"
                    },
                    "rules": [
                      {
                        "required": true
                      }
                    ]
                  },
                "check_frequency": {
                    "label": "检查频率",
                    "control": "text",
                    "rules": [
                        {
                            "required": true
                        }
                    ]
                }
            }
        ]
    }
}
}
