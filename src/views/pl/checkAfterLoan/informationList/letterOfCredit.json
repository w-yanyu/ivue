{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "uccp3080",
          "quota_type": "03",
          "source_system": "001",
          "cust_no":"${{params.cust_id}}"
        }
      },
      "pagination": {
        "show": false
      },
      "thead": [
        {
          "type": "index"
        },
        {
          "label": "额度编号",
          "prop": "prod_credit_seq",
          "align": "center",
          "width": "200px"
        },
        {
            "label": "产品编号",
            "prop": "prod_code",
            "align": "center"
        },
        {
            "label": "产品名称",
            "prop": "prod_name",
            "align": "center"
        },
        {
            "label": "业务币种",
            "prop": "base_ccy_code",
            "align": "center",
            "dict": {
                "dictType": "MS_CURRENCY",
                "dictKey": [
                    "MS_CURRENCY"
                ],
                "format": "label"
            }
        },
        {
            "label": "生效日期",
            "prop": "effect_date",
            "align": "center"
        },
        {
            "label": "失效日期",
            "prop": "expiry_date",
            "align": "center"
        },
        {
            "label": "额度状态",
            "prop": "quota_status",
            "align": "center",
            "dict": {
                "dictType": "UCCP_E_QUOTA_STATUS",
                "dictKey": [
                    "UCCP_E_QUOTA_STATUS"
                ],
                "format": "label"
            }
        },
        {
            "label": {
                "default": "渠道来源",
                "i18n": "T302.Channel-Source"
                        },
            "prop": "source_system",
            "align": "center",
            "dict": {
                "dictType": "UCCP_E_SOURCE_SYSTEM",
                "dictKey": [
                    "UCCP_E_SOURCE_SYSTEM"
                ],
                "format": "label"
            }
        },
        {
            "label": {
                "default": "授信总额",
                "i18n": "T302.Total-credit-amount"
                        },
            "prop": "prod_total_credit",
            "align": "center",
            "format": {
                "thousand": ","
              }
        },
        {
            "label": {
                "default": "授信可用额度",
                "i18n": "T302.Available-credit-limit"
                        },
            "prop": "available_use_limit",
            "align": "center",
            "format": {
                "thousand": ","
              }
        }
        
      ]
    }
  }
}
