{
  "module": "v_search_btn_datagrid",
  "layout": {

    "toolbar": {
      "btns": [
        {
          "type": "add",
          "target": "dialog",
          "label": "新增",
          "toRequest": {
            "method": "GET",
            "url": "/views/pl/checkAfterLoan/informationList/adduseOfCreditFunds.json",
            "params": {
                "contract_no":"${{params.ctrct_num}}"
            }
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pl0180",
          "contract_no":"${{params.ctrct_num}}"
        }
      },
      "thead": [
        {
          "type": "index"
        },
        {
          "prop": "payment_date",
          "label": "付款日",
          "align": "center"
        },
        {
          "prop": "payer",
          "label": "收款人",
          "align": "center"
        },
        {
          "prop": "acct_amt",
          "label": "金额（万元）",
          "align": "center"
        },
        {
          "label": "贷款用途",
          "width": 120,
          "prop": "loan_purp",
          "dict": {
            "dictType": "PL_E_LOAN_PURP",
            "dictKey": [
              "PL_E_LOAN_PURP"
            ],
            "format": "value-label"
          }
        },
        {
          "label": "是否挪用，违规",
          "width": 120,
          "prop": "is_appropriate",
          "dict": {
            "dictType": "MS_E_YESORNO",
            "dictKey": [
              "MS_E_YESORNO"
            ],
            "format": "value-label"
          }
        },
        {
          "label": "付款方式",
          "width": 120,
          "prop": "pay_type",
          "dict": {
            "dictType": "PL_E_PAY",
            "dictKey": [
              "PL_E_PAY"
            ],
            "format": "value-label"
          }
        },
        {
          "label": "操作",
          "fixed": "right",
          "width": "200",
          "align": "center",
          "btns": [
            {
              "type": "edit",
              "label": "修改",
              "toRequest": {
                  "url": "/views/pl/checkAfterLoan/informationList/upduseOfCreditFunds.json",
                  "method": "GET"
              },
              "props": {
                  "width": "75%",
                  "title": "修改"
              }
            },
            {
              "doRequest": {
                "method": "POST",
                "url": "/SUMP/icmcall/icmRPCCall",
                "params": {
                  "servicecode": "pl0182",
                  "method": "del"
                }
              },
              "label": "删除",
              "type": "delete"
            }

          ]
        }

      ]
    }
  }
}
