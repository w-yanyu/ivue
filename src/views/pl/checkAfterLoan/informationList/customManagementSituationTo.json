{
  "module": "v-form-btn-dynamic",
  "layout": {
      "form": {
          "scope": "A",
          "name": "detail",
          "initRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "pl0200",
                "cust_no":"${{params.cust_no}}",
                "cust_name":"${{params.cust_name}}"
              }
          },
          "controls": [{

              "customer_operation": {
                  "label": "客户经营情况",
                  "control": "text",
                  "width": "140px",
                  "disabled":"false",
                  "rules": [
                    {
                      "required": true,
                      "max": "250",
                      "message": "长度不能超过250个字符"
                    }
                  ]
              }



          }]

      },
      "toolbar": {
          "btns": [
              {
                  "type": "submit",
                  "label": "保存",
                  "doRequest": {
                      "url": "/SUMP/icmcall/icmRPCCall",
                      "method": "post",
                      "params": {
                          "servicecode": "pl0201",
                          "cust_no":"${{params.cust_id}}"
                      }
                  }
              }
          ]
      }
  }
}
