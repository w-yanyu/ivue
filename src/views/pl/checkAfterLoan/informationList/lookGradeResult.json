{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "cr1023",
          "loan_due_bill_num": "${{params.loan_no}}",
          "cust_no": "${{params.cust_id}}"

        }
      },
      "thead": [
        {
          "type": "index"
        },
        {
          "prop": "cust_no",
          "label": "客户号",
          "align": "center"
        },

        {
          "prop": "eval_dt",
          "label": "评级日期",
          "align": "center"
        },
        {
          "prop": "fl_rank",
          "label": "认定等级",
          "align": "center"
        },
        {
          "prop": "user_cd",
          "label": "评级人",
          "align": "center"
        },
        {
          "prop": "eval_sc",
          "label": "评级总分",
          "align": "center"
        },
        {
          "prop": "flag",
          "label": "评级状态",
          "align": "center",
          "dict": {
            "dictType": "PL_E_STATUS",
            "dictKey": [
              "PL_E_STATUS"
            ],
            "format": "label"
          }
        }
      ]
    }
  }
}
