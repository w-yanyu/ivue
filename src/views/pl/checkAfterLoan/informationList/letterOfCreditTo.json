{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "cl0037",
          "cust_no":"${{params.cust_no}}"
        }
      },
      "thead": [
        {
          "type": "index"
        },
        {
          "prop": "cust_name",
          "label": "客户名称",
          "align": "center"
        },
        {
          "prop": "limit_no",
          "label": "授信额度代码",
          "align": "center"
        },
        {
          "prop": "cl_amt",
          "label": "业务额度",
          "align": "center"
        },
        {
          "label": "额度级别",
          "prop": "cl_level",
          "align": "center",
          "dict": {
            "dictType": "CL_E_CL_LEVEL",
            "dictKey": [
              "CL_E_CL_LEVEL"
            ],
            "format": "label",
            "labelField": "dictName",
            "valueField": "dictId"
          }
        },
        {
          "prop": "ccy_code",
          "label": "币种",
          "align": "center",
          "dict": {
            "dictType": "MS_CURRENCY",
            "dictKey": [
              "MS_CURRENCY"
            ],
            "format": "label"
          }
        },
        {
          "label": "产品名称",
          "prop": "cl_prod_name",
          "align": "center"
        },
        {
          "label": "额度类型名称",
          "prop": "cl_type_name",
          "align": "center"
        },
        {
          "prop": "effect_date",
          "label": "生效日期",
          "align": "center"
        },
        {
          "prop": "expiry_date",
          "label": "失效日期",
          "align": "center"
        },
        {
          "label": "状态",
          "width": 120,
          "prop": "status",
          "dict": {
            "dictType": "CL_E_STATUS",
            "dictKey": [
              "CL_E_STATUS"
            ],
            "format": "label"
          }
        }
        
      ]
    }
  }
}
