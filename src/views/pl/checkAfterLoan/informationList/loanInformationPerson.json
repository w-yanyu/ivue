{
  "module": "v-inputDataForm-btn",
  "layout": {
  "form": {
    "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
            "servicecode": "pl0187",
            "contract_no": "${{params.ctrct_num}}"
        }
    },
    "btns": [
      {
        "type": "submit",
        "label": "保存",
        "icon": "",
        "doRequest": {
          "url": "/SUMP/icmcall/icmRPCCall",
          "method": "POST",
          "params": {
            "servicecode": "pl0185",
            "contract_no": "${{params.ctrct_num}}"
          }
        }
      }
    ],
    "controlsGroup": [
      {
        "title": "个人贷后检查（消费类）",
        "tooltip": "",
        "titleIcon": "",
        "showHint": "",
        "accordion": false,
        "expand": true,
        "controls": [
          {
            "check_time": {
              "control": "dateTimePicker",
              "label": "检查时间"
            },
            "cust_num": {
              "value": "${{params.cust_num}}",
              "control": "text",
              "label": "客户代码",
              "disabled":"true"
            },
            "cust_nm": {
              "value": "${{params.cust_nm}}",
              "control": "text",
              "label": "借款人姓名",
              "disabled":"true"
            },
            "nrl_prcpl": {
              "value": "${{params.nrl_prcpl}}",
              "control": "text",
              "label": "贷款余额",
              "disabled":"true"
            },
            "strt_int_caln_dt": {
              "value": "${{params.strt_int_caln_dt}}",
              "control": "text",
              "label": "起息日期",
              "disabled":"true"
            },
            "matu_dt": {
              "value": "${{params.matu_dt}}",
              "control": "text",
              "label": "到期日期",
              "disabled":"true"
            },
            "ctrct_amt": {
              "value": "${{params.ctrct_amt}}",
              "control": "text",
              "label": "合同金额",
              "disabled":"true"
            },
            "ask_tm": {
              "control": "dateTimePicker",
              "label": "打听时间",
              "disabled": "false"
            },
            "side_ask_object": {
              "control": "text",
              "label": "侧面打听对象",
              "disabled": "false"
            },
            "borrower_relation": {
               "label": "与借款人关系",
                "control": "select",
                "multiple": false,
                "dict": {
                  "dictKey": [
                    "PL_E_RELATION"
                  ],
                  "dictType": "PL_E_RELATION",
                  "format": "label"
                },
                "rules": [
                  {
                    "required": true,
                    "message": "请选择与借款人关系"
                  }
                ]
            },
            "side_ask_way": {
              "label": "侧面打听方式",
                "control": "select",
                "multiple": false,
                "dict": {
                  "dictKey": [
                    "PL_E_WAY"
                  ],
                  "dictType": "PL_E_WAY",
                  "format": "label"
                },
                "rules": [
                  {
                    "required": true,
                    "message": "请选择侧面打听方式"
                  }
                ]
            },
            "survey_boject_phone": {
              "control": "text",
              "label": "调查对象电话",
              "disabled": "false"
            }
          }
        ]
      },
      {
        "title": "",
        "tooltip": "",
        "titleIcon": "",
        "showHint": "",
        "accordion": false,
        "expand": true,
        "controls": [
          {
            "loan_purp_check": {
              "control": "text",
              "label": "贷款用途检查",
              "span": 24,
              "width":12,
              "rules": [
                {
                    "required": true,
                    "message": "请输入贷款用途检查"
                }
            ]
            },
            "repay_princi_inst_check": {
              "control": "text",
              "label": "还本付息情况检查",
              "span": 24,
              "width":12,
              "rules": [
                {
                    "required": true,
                    "message": "请输入还本付息情况检查"
                }
            ]
            },
            "family_check": {
              "control": "text",
              "label": "家庭重大变化情况检查",
              "span": 24,
              "width":12,
              "rules": [
                {
                    "required": true,
                    "message": "请输入家庭重大变化情况检查"
                }
            ]
            },
            "repay_src_check": {
              "control": "text",
              "label": "还款来源检查",
              "span": 24,
              "width":12,
              "rules": [
                {
                    "required": true,
                    "message": "请输入还款来源检查"
                }
            ]
            },
            "impact_borrower_analy": {
              "control": "text",
              "label": "影响借款人还款因素分析",
              "span": 24,
              "width":12,
              "rules": [
                {
                    "required": true,
                    "message": "请输入影响借款人还款因素分析"
                }
            ]
            },
            "guar_power_analy": {
              "control": "text",
              "label": "担保能力分析（含抵（质）押物状况、保证人情况）",
              "span": 24,
              "width":12,
              "rules": [
                {
                    "required": true,
                    "message": "请输入担保能力分析（含抵（质）押物状况、保证人情况）"
                }
            ]
            },
            "risk_synth_eval": {
              "control": "text",
              "label": "风险综合评价、主要问题及处理意见",
              "span": 24,
              "width":12,
              "rules": [
                {
                    "required": true,
                    "message": "请输入风险综合评价、主要问题及处理意见"
                }
            ]
            },
            "deal_opinion": {
              "control": "text",
              "disabled": false,
              "label": "处理意见",
              "span": 24,
              "width":12
            }
          }
        ]
      }

    ]
  }
  }
}
