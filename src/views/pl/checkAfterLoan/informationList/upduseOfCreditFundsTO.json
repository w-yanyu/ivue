{
  "module": "v_inputDataForm_btn",
  "layout": {
      "form": {
        "scope": "templateScope",
        "initRequest":"${{params}}",
        "initDataOnce":true,


      "btns": [

      ],
      "controls": [
        {
          "payment_date": {
            "control": "dateTimePicker",
            "label": "付款日",
            "disabled": true,

            "rules": [{
              "required": true
            }]
          },
            "payer": {
                "label": "收款人",
                "control": "text",
                "disabled": true,

                "rules": [
                    {
                        "required": true
                    }
                ]
            },
            "acct_amt": {
                "label": "金额（万元）",
                "control": "text",
                "disabled": true,

                "rules": [
                    {
                        "required": true
                    }
                ]
            },
            "loan_purp": {
              "label": "贷款用途",
              "disabled": true,

              "control": "select",
              "multiple": false,
              "dict": {
                "dictKey": [
                  "PL_E_LOAN_PURP"
                ],
                "dictType": "PL_E_LOAN_PURP",
                "format": "label"
              },
              "rules": [
                {
                  "required": true
                }
              ]
            },
            "is_appropriate": {
              "label": "是否挪用，违规",
              "disabled": true,

              "control": "select",
              "multiple": false,
              "dict": {
                "dictKey": [
                  "MS_E_YESORNO"
                ],
                "dictType": "MS_E_YESORNO",
                "format": "label"
              },
              "rules": [
                {
                  "required": true
                }
              ]
            },
            "pay_type": {
              "label": "付款方式",
              "disabled": true,

              "control": "select",
              "multiple": false,
              "dict": {
                "dictKey": [
                  "PL_E_PAY"
                ],
                "dictType": "PL_E_PAY",
                "format": "label"
              },
              "rules": [
                {
                  "required": true
                }
              ]
            }
        }
    ]
}
}
}
