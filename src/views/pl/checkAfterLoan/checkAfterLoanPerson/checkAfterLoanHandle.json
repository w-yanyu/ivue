{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "matrix": {
            "label": "客户名称",
            "control": "text",
            "rules": [
              {
                "max": "5",
                "message": "长度不能超过5个字符"
              }
            ]
          },
          "hndov_ste": {
            "control": "select",
            "label": "检查性质",
            "dict": {
              "dictType": "PL_E_ALTER_STATUS",
              "dictKey": [
                "PL_E_ALTER_STATUS"
              ]
            },
            "format": "label"
          },
          "date_range": {
              "control": "dateTimePicker",
              "label": "创建日期",
              "type": "daterange",
              "startPlaceholder": "起始日期",
              "endPlaceholder": "截止日期",
              "startField": "start_date",
              "endField": "end_date",
              "width": 150,
              "span": "24",
              "rules": [
                  {
                      "message": "请选择日期区间"
                  }
              ],
              "valueFormat": "yyyyMMdd",
              "unlinkPanels": true
          }
      }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "AS_PLMS_KG_MACURD",
          "method": "queryAll"
        }
      },
      "thead": [
        {
          "type": "index"
        },
        {
          "prop": "matrix",
          "label": "客户名称",
          "align": "center"
        },
        {
          "prop": "groupi",
          "label": "业务类型",
          "align": "center"
        },
        {
          "prop": "matmin",
          "label": "检查性质",
          "align": "center"
        },
        {
          "prop": "matmax",
          "label": "检查频率",
          "align": "center"
        },
        {
          "prop": "matmax",
          "label": "检查月份",
          "align": "center"
        },
        {
          "prop": "matmax",
          "label": "检查季度",
          "align": "center"
        },
        {
          "prop": "matmax",
          "label": "检查人员",
          "align": "center"
        },
        {
          "prop": "matmax",
          "label": "检查地点",
          "align": "center"
        },
        {
          "prop": "matmax",
          "label": "检查时间",
          "align": "center"
        },
        {
          "prop": "matmax",
          "label": "上一处理人",
          "align": "center"
        },
        {
          "prop": "matmax",
          "label": "流程启动者",
          "align": "center"
        },
        {
          "prop": "codesc",
          "label": "发起时间",
          "align": "center"
        },
        {
          "prop": "codesc",
          "label": "最后处理时间",
          "align": "center"
        }
      ]
    }
  }
}
