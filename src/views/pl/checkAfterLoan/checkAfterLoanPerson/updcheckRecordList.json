{
  "module": "v_inputDataForm_btn",
  "layout": {
      "form": {
        "scope": "templateScope",
        "initRequest":"${{params}}",
        "initDataOnce":true,
        
          "btns": [
              {
                  "type": "submit",
                  "label": "保存",
                  "icon": "",
                  "doRequest": {
                      "url": "/SUMP/icmcall/icmRPCCall",
                      "method": "POST",
                      "params": {
                          "servicecode": "AS_PLMS_KG_MACURD"
                      }
                  }
              }
          ],
          "controls": [
              {
                
                  "wthr_hndov_cust": {
                    "control": "select",
                    "label": "检查性质",
                    "width": "130px",
                    "disabled": true,
                    "span":"10",
                    "dict": {
                      "dictType": "MS_E_YESORNO",
                      "dictKey": [
                        "MS_E_YESORNO"
                      ],
                      "format": "label",
                      "rules": [
                        {
                            "required": true
                        }
                    ]
                    }
                  },
                  "codeid": {
                      "label": "检查时间",
                      "control": "text",
                      "rules": [
                          {
                              "required": true,
                              "max": "5",
                              "message": "长度不能超过5个字符"
                          }
                      ]
                  },
                  "matmax": {
                      "label": "检查年份",
                      "control": "text",
                      "placeholder": "最大天数填99999",
                      "rules": [
                          {
                              "required": true,
                              "max": "5",
                              "message": "长度不能超过5个字符"
                          }
                      ]
                  },
                  "matmin": {
                      "label": "检查月份",
                      "control": "text",
                      "placeholder": "未到期输入-9999",
                      "rules": [
                          {
                              "required": true,
                              "max": "5",
                              "message": "长度不能超过5个字符"
                          }
                      ]
                  },
                  "wt_hndov_cust": {
                    "control": "select",
                    "label": "检查季度",
                    "width": "130px",
                    "disabled": true,
                    "span":"10",
                    "dict": {
                      "dictType": "MS_E_YESORNO",
                      "dictKey": [
                        "MS_E_YESORNO"
                      ],
                      "format": "label",
                      "rules": [
                        {
                            "required": true
                        }
                    ]
                    }
                  },
                  "mat": {
                      "label": "检查人员",
                      "control": "text",
                      "placeholder": "未到期输入-9999",
                      "rules": [
                          {
                              "required": true,
                              "max": "5",
                              "message": "长度不能超过5个字符"
                          }
                      ]
                  },
                  "matmin4": {
                      "label": "检查地点",
                      "control": "text",
                      "placeholder": "未到期输入-9999",
                      "rules": [
                          {
                              "required": true,
                              "max": "5",
                              "message": "长度不能超过5个字符"
                          }
                      ]
                  },
                  "matmi4": {
                    "label": "检查内容",
                    "control": "text",
                    "placeholder": "未到期输入-9999",
                    "rules": [
                        {
                            
                            "max": "5",
                            "message": "长度不能超过5个字符"
                        }
                    ]
                },
                "mtmi4": {
                  "label": "检查过程",
                  "control": "text",
                  "placeholder": "未到期输入-9999",
                  "rules": [
                      {
                          
                          "max": "5",
                          "message": "长度不能超过5个字符"
                      }
                  ]
              }
              }
              

          ]
      }
  }
}