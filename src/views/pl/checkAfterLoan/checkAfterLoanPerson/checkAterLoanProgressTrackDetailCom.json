{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "btns": [
      ],
      "controls": [
        {
          "check_no": {
            "control": "hidden",
            "label": "检查编号",
            "disabled": true
          },
          "cust_name": {
            "control": "hidden",
            "label": "客户姓名",
            "disabled": true
          },
          "cust_no": {
            "label": "客户编号",
            "control": "text",
            "disabled": true
          },
          "cust_type": {
            "label": "客户类型",
            "control": "select",
            "disabled": true,
            "dict": {
              "dictKey": [
                "US_E_APPL_CUST_TYPE"
              ],
              "dictType": "US_E_APPL_CUST_TYPE",
              "format": "label"
            }
          },
          "busi_type": {
            "value":"${{params.busi_type}}",
            "label": "业务类型",
            "control": "select",
            "disabled": true
          },
          "contract_no": {
            "label": "合同号",
            "control": "text",
            "disabled": true
          },
          "cert_type": {
            "label": "证件类型",
            "control": "select",
            "disabled": true,
            "dict": {
              "dictKey": [
                "MS_E_CRET_TYPE"
              ],
              "dictType": "MS_E_CRET_TYPE",
              "format": "label"
            }
          },
          "cert_no": {
            "label": "证件号码",
            "control": "text",
            "disabled": true
          },
          "is_first_check": {
            "label": "是否首次检查",
            "control": "select",
            "disabled": true,
            "dict": {
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "dictType": "MS_E_YESORNO",
              "format": "label"
            }
          },
          "run_date": {
            "label": "跑批日期",
            "control": "text",
            "disabled": true
          },
          "change_ste": {
            "label": "变更状态",
            "control": "select",
            "disabled": true,
            "dict": {
              "dictKey": [
                "PL_E_ALTER_STATUS"
              ],
              "dictType": "PL_E_ALTER_STATUS",
              "format": "label"
            }
          },
          "pl_method_no": {
            "label": "贷后措施编号",
            "control": "text",
            "disabled": true
          },
          "customer_operation": {
            "label": "客户经营情况",
            "control": "text",
            "disabled": true
          },
          "check_character": {
            "label": "检查性质",
            "control": "select",
            "disabled": true
          },
          "check_frequency": {
            "label": "默认检查频率",
            "control": "text",
            "disabled": true
          },
          "check_quarter": {
            "label": "检查季度",
            "control": "text",
            "disabled": true
          },
          "check_man": {
            "label": "检查人员",
            "control": "text",
            "disabled": true
          },
          "check_area": {
            "label": "检查地点",
            "control": "text",
            "disabled": true
          },
          "check_tm": {
            "label": "检查时间",
            "control": "text",
            "disabled": true
          },
          "cust_manager_name": {
            "label": "客户经理名",
            "control": "text",
            "disabled": true
          },
          "toDbsx": {
            "span": 24,
            "unFieldList": [
                "apply_type",
                "busi_type"
            ],
            "control": "dynamic",
            "#isShowChildrenForm": false,
            "toRequest": {
                "method": "GET",
                "url": "@FlowSubmitBtn",
                "params": {
                    "appId": "plOnl",
                    "servicecode": "pl0170",
                    "change_num": "${{params.check_no}}",
                    "cust_name": "${{params.cust_name}}",
                    "cust_no": "${{params.cust_no}}",
                    "busi_type": "${{params.busi_type}}",
                    "contract_no": "${{params.contract_no}}",
                    "cert_type": "${{params.cert_type}}",
                    "cert_no": "${{params.cert_no}}",
                    "appr_status": "${{params.appr_status}}",
                    "finalUrl": "views/pl/workbench/workbench_wait_todo_view.json",
                    "trans_type": "ADD"
                }
            }
        }

        }
      ]
    }
  }
}
