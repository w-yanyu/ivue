{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pl0171",
          "apply_no":"${{params.applid}}"
        }
      },
      "thead": [

        {
          "label": "客户号",
          "prop": "cust_no",
          "align": "center"
        },
        {
          "label": "客户名称",
          "prop": "cust_name",
          "align": "center"
        },
        {
          "label": "证件类型",
          "prop": "cert_type",
          "align": "center",
          "dict": {
            "dictType": "MS_E_CRET_TYPE",
            "dictKey": [
              "MS_E_CRET_TYPE"
            ],
            "labelField": "dictName",
            "valueField": "dictId"
          }
        },
        {
          "label": "证件号码",
          "prop": "cert_no",
          "align": "center"
        },
        {
          "label": "操作",
          "align": "center",
          "btns": [
            {
              "type": "custom",
              "label": "详细信息",
              "toRequest": {
                "url": "/views/us/entManager/ent_cust_tree.json",
                "props": {
                  "width": "95%",
                  "height": "590px",
                  "title": "查看"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
