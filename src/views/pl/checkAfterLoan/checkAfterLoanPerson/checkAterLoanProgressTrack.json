{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "cust_name": {
            "label": "客户名称",
            "control": "text",
            "rules": [
              {
              }
            ]
          },
          "change_ste": {
            "control": "select",
            "label": "检查状态",
            "dict": {
              "dictType": "PL_E_ALTER_STATUS",
              "dictKey": [
                "PL_E_ALTER_STATUS"
              ]
            },
            "format": "label"

          }
      }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pl0166"
        }
      },
      "thead": [
        {
          "type": "index"
        },
        {
          "prop": "cust_name",
          "label": "客户名称",
          "align": "center"
        },
        {
          "label": "检查性质",
          "width": 120,
          "prop": "check_character",
          "dict": {
            "dictType": "PL_E_CHECK_CHARACTER",
            "dictKey": [
              "PL_E_CHECK_CHARACTER"
            ],
            "format": "label"
          }
        },
        {
          "prop": "check_frequency",
          "label": "检查频率",
          "align": "center"
        },
        {
          "prop": "check_quarter",
          "label": "检查季度",
          "align": "center",
          "value":"第四季度"
        },
        {
          "prop": "check_man",
          "label": "检查人员",
          "align": "center"
        },
        {
          "prop": "check_tm",
          "label": "检查时间",
          "align": "center"
        },
        {
          "label": "检查状态",
          "width": 120,
          "prop": "change_ste",
          "dict": {
            "dictType": "PL_E_ALTER_STATUS",
            "dictKey": [
              "PL_E_ALTER_STATUS"
            ],
            "format": "label"
          }
        },
        {
          "label": "操作",
          "fixed": "right",
          "width": "200",
          "align": "center",
          "btns": [
            {
              "type": "edit",
              "label": "详情",
              "toRequest": {
                  "url": "@ZManualCheck",
                  "method": "GET"
              },
              "props": {
                  "width": "75%",
                  "title": "详情"
              }
          }
          ]
        }
      ]
    }
  }
}
