{
    "module": "v_form_btn_dynamic",
    "layout": {
        "form": {
            "scope": "formData",
            "formSetTitle": "五级分类批量调整",
            "controls": [
                {
                    "fiveTypeFile": {
                        "control": "dynamic",
                        "isShowChildForm": true,
                        "span": 12,
                        "toRequest": {
                            "url": "@fiveTypeFile",
                            "method": "get",
                            "params": {
                                "formKey": "fiveTypeFile",
                                "formLabel": "上传五级分类批量调整文件",
                                "required": true
                            }
                        }
                    },
                    "fileList": {
                      "control": "dynamic",
                      "isShowChildForm": true,
                      "span": 12,
                      "toRequest": {
                        "url": "@fiveTypeUploadFileView",
                        "method": "get",
                        "params": {
                          "formKey": "fileList",
                          "formLabel": "附件上传"
                        }
                      }
                    }
                }
            ]
        },
        "toolbar": {
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "unplain":true,
                    "icon": "",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "pl0107"
                        }
                    }
                }
            ]
        }
    }
}
