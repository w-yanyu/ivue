{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "loan_no": {
            "control": "text",
            "label": "借据号",
            "span": 12,
            "rules": [
              {
                "max": 80,
                "message": "长度不能超过80个字符"
              }
            ]
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pl0100"
        }
      },
      "thead": [
        {
          "label": "申请号",
          "prop": "apply_no",
          "width":240,
          "align": "center"
        },
        {
          "label": "借据号",
          "prop": "loan_no",
          "width":200,
          "align": "center"
        },
        {
          "label": "人工五级分类(调整后)",
          "prop": "artificial_five_clasf_tp",
          "width":200,
          "align": "center",
          "dict": {
            "dictType": "PL_E_CLEVEL",
            "dictKey": [
              "PL_E_CLEVEL"
            ],
            "format": "label"
          }
        },
        {
          "label": "申请状态",
          "prop": "aply_ste",
          "align": "center",
          "dict": {
            "dictType": "PL_E_KGAPPS",
            "dictKey": [
              "PL_E_KGAPPS"
            ],
            "format": "label"
          }
        },
        {
          "label": "申请调整原因",
          "prop": "aply_adjust_reason",
          "align": "center"
        },
        {
          "label": "备注",
          "prop": "remark",
          "align": "center"
        }
      ]
    }
  }
}