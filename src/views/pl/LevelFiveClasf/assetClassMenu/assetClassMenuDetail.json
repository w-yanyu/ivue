{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "loan_no": {
            "control": "text",
            "label": "借据号",
            "span": 12,
            "rules": [
              {
                "max": 80,
                "message": "长度不能超过80个字符"
              }
            ]
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pl0211"
        }
      },
      "thead": [

        {
          "label": "借据号",
          "prop": "loan_no",
          "width":200,
          "align": "center"
        },
        {
          "label": "合同号",
          "prop": "contract_no",
          "width":200,
          "align": "center"
        },
        {
          "label": "可售产品名称",
          "prop": "sale_prod_name",
          "width":200,
          "align": "center"
        },
        {
          "label": "借据金额",
          "prop": "due_bill_amt",
          "align": "center"
        },
        {
          "label": "分类代码",
          "prop": "classify_code",
          "align": "center",
          "dict": {
            "dictType": "PL_E_CLEVEL",
            "dictKey": [
              "PL_E_CLEVEL"
            ],
            "format": "label"
          }
        },
        {
          "label": "修改日期",
          "prop": "mod_date",
          "align": "center"
        },
        {
          "label": "逾期天数",
          "prop": "overdue_days",
          "align": "center" 
        },
        {
          "label": "分类类型",
          "prop": "is_system",
          "align": "center",
          "dict": {
            "dictType": "PL_E_CHANGE_TYPE",
            "dictKey": [
              "PL_E_CHANGE_TYPE"
            ],
            "format": "label"
          }
        }
        
      ]
    }
  }
}