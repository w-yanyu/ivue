{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": "",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "pl0112",
                            "matrix_number": "${{params.matrix_number}}",
                            "matrix_name": "${{params.matrix_name}}"

                        }
                    }
                }
            ],
            "controls": [
                {
                    "ovdue_dys_down_lmt": {
                        "label": "逾期天数下限",
                        "control": "text",
                        "rules": [
                            {
                                "required": true,
                                "message":"请输入逾期天数下限"
                            },
                            {
                                "pattern": "^[0-9]*$",
                                "message": "请输入整数"
                            }
                        ]
                    },
                    "ovdue_dys_upr_lmt": {
                        "label": "逾期天数上限",
                        "control": "text",
                        "rules": [
                            {
                                "required": true,
                                "message":"请输入逾期天数上限"
                            },
                            {
                                "pattern": "^[0-9]*$",
                                "message": "请输入整数"
                            }
                        ]
                    },
                    "classify_code":  {
                        "label": "五级分类级别",
                        "control": "select",
                        "multiple": false,
                        "dict": {
                          "dictKey": [
                            "PL_E_CLEVEL"
                          ],
                          "dictType": "PL_E_CLEVEL",
                          "format": "label"
                        },
                        "rules": [
                          {
                            "required": true,
                            "message":"请选择五级分类级别"  
                          }
                        ]
                      }
                }
            ]
        }
    }
}
