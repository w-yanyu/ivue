{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "name": "info",
      "btns": [
        {
          "type": "submit",
          "label": "保存",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "wa0009"
            }
          }
        }
      ],
      "controlsGroup": [
        {
          "title": "筛选策略基本信息",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "accordion": false,
          "expand": true,
          "controls": [
            {
              "matrix_name": {
                "label": "矩阵名称",
                "control": "text",
                "span": "8",
                "width": "25px",
                "format": "label",
                "rules": [
                  {
                    "required": true,
                    "message": "请输入矩阵名称"
                  }
                ]
              },
              "matrix_number": {
                "label": "矩阵编号",
                "control": "text",
                "span": "8",
                "width": "25px",
                "format": "label",
                "rules": [
                  {
                    "required": true,
                    "message": "请输入矩阵编号"
                  }
                ]
              },
              "matrix_describe": {
                "label": "矩阵描述",
                "control": "text",
                "span": "8",
                "width": "25px",
                "format": "label",
                "rules": [
                  {
                    "required": true,
                    "message": "请输入矩阵描述"
                  }
                ]
              },
              "is_need_flow": {
                "label": "是否启用流程",
                "control": "select",
                "span": "8",
                "width": "25px",
                "multiple": false,
                "dict": {
                  "dictType": "MS_E_YESORNO",
                  "dictKey": [
                    "MS_E_YESORNO"
                  ],
                  "format": "label"
                },
                "rules": [
                  {
                    "required": true,
                    "message": "请选择是否启用"
                  }
                ]
              },
              "prod_no": {
                "label": "矩阵编号",
                "span": "8",
                "width": "25px",
                "control": "lookup",
                "edit": true,
                "disable": "true",
                "keyField": {
                  "prod_no": "prod_no"
                },
                "toRequest": {
                  "url": "/views/wa/param/filter/tranferProduct.json",
                  "method": "post"
                },
                "props": {
                  "title": "矩阵参数配置"
                },
                "format": "label"
              },
              "product_binding": {
                "label": "产品绑定",
                "control": "text",
                "span": "8",
                "width": "25px",
                "format": "label",
                "rules": [
                  {
                    "required": true,
                    "message": "请输入矩阵描述"
                  }
                ]
              }
              
            }

          ]
        }

      ]
    }
  }
}
