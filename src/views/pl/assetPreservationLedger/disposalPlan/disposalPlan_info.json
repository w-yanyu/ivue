{
  "module": "v_search_btn_datagrid",
  "layout": {
    "toolbar": {
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pl0297",
          "loan_no": "${{params.loan_no}}"
        }
      },
      "thead": [
        {
          "label": "序号",
          "type": "index",
          "align": "center"
        },
        {
          "label": "处置方式",
          "prop": "disposal_plan",
          "align": "center",
          "dict": {
            "dictType": "PL_E_DISPAL_PLAN",
            "dictKey": [
              "PL_E_DISPAL_PLAN"
            ]
          },
          "format": "label"
        },
        {
          "label": "借据号",
          "prop": "loan_no",
          "align": "center"
        },
        {
          "label": "客户编号",
          "prop": "cust_no",
          "align": "center"
        },
        {
          "label": "客户名称",
          "prop": "cust_nm",
          "align": "center"
        },
        {
          "label": "贷款期限",
          "prop": "loan_matu",
          "align": "center"
        },
        {
          "label": "贷款形态",
          "prop": "loan_form",
          "align": "center",
          "dict": {
            "dictType": "PL_E_CLSSST",
            "dictKey": [
              "PL_E_CLSSST"
            ],
            "format": "label"
          }
        },
        {
          "label": "贷款账户状态",
          "prop": "loan_acct_ste",
          "align": "center",
          "dict": {
            "dictType": "PL_E_ACCTST",
            "dictKey": [
              "PL_E_ACCTST"
            ],
            "format": "label"
          }
        },
        {
          "label": "币种",
          "prop": "ccy_code",
          "align": "center",
          "dict": {
            "dictType": "MS_CURRENCY",
            "dictKey": [
              "MS_CURRENCY"
            ],
            "format": "label"
          }
        },
        {
          "label": "还款账号",
          "prop": "rpymt_acct_num",
          "align": "center"
        },
        {
          "label": "资产负责人编号",
          "prop": "new_usr_num",
          "align": "center"
        },
        {
          "label": "资产负责人名称",
          "prop": "new_usr_nm",
          "align": "center"
        },
        {
          "label": "原机构",
          "prop": "old_org_num",
          "align": "center"
        },
        {
          "label": "新机构",
          "prop": "new_org_num",
          "align": "center"
        },
        {
          "label": "备注",
          "prop": "remark",
          "align": "center"
        }
      ]
    }
  }
}
