{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "post",
        "params": {
          "servicecode": "pl0295",
          "loan_no": "${{params.loan_no}}"
        }
      },
      "btns": [],
      "controls": [
        {
          "hndov_code": {
            "control": "string",
            "label": "移交编码",
            "width": "120px",
            "span": 8
          },
          "new_usr_num": {
            "control": "string",
            "label": "新客户经理编号",
            "width": "120px",
            "span": 8
          },
          "new_usr_nm": {
            "control": "string",
            "label": "新客户经理名称",
            "width": "120px",
            "span": 8
          },
          "old_org_num": {
            "control": "string",
            "label": "原机构号",
            "width": "120px",
            "span": 8
          },
          "disposal_code": {
            "control": "string",
            "label": "处置编码",
            "width": "120px",
            "span": 8
          },
          "new_org_num": {
            "control": "string",
            "label": "新机构号",
            "width": "120px",
            "span": 8
          },
          "loan_no": {
            "control": "string",
            "label": "借据号",
            "width": "120px",
            "span": 8
          },
          "cust_no": {
            "control": "string",
            "label": "客户编号",
            "width": "120px",
            "span": 8
          },
          "cust_nm": {
            "control": "string",
            "label": "客户名称",
            "width": "120px",
            "span": 8
          },
          "loan_acct_ste": {
            "control": "select",
            "label": "贷款账户状态",
            "disabled":true,
            "width": "120px",
            "span": 8,
            "dict": {
              "dictType": "PL_E_ACCTST",
              "dictKey": [
                "PL_E_ACCTST"
              ],
              "labelField": "dictName",
              "valueField": "dictId"
            }
          },
          "loan_matu": {
            "control": "string",
            "label": "贷款期限",
            "width": "120px",
            "span": 8
          },
          "loan_form": {
            "control": "select",
            "label": "贷款形态",
            "width": "120px",
            "span": 8,
            "disabled": true,
            "dict": {
              "dictType": "PL_E_CLSSST",
              "dictKey": [
                "PL_E_CLSSST"
              ],
              "format": "label"
            }
          },
          "remark": {
            "control": "textarea",
            "label": "备注",
            "disabled": true,
            "width": "120px",
            "span": 24,
            "autosize": {
              "minRows": 3,
              "maxRows": 5
            }
          },
          "rpymt_acct_num": {
            "control": "string",
            "label": "还款账号",
            "width": "120px",
            "span": 8
          },
          "disposal_plan": {
            "control": "select",
            "label": "处置方式",
            "width": "120px",
            "span": 8,
            "disabled": true,
            "dict": {
              "dictType": "PL_E_DISPAL_PLAN",
              "dictKey": [
                "PL_E_DISPAL_PLAN"
              ],
              "format": "label"
            }
          },
          "ccy_code": {
            "control": "string",
            "label": "币种",
            "disabled": true,
            "width": "120px",
            "span": 8,
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": [
                "MS_CURRENCY"
              ],
              "format": "label"
            }
          },
          "prcpl_smtn": {
            "control": "currency",
            "label": "本金合计",
            "disabled": true,
            "width": "120px",
            "span": 8,
            "format":{
              "thousand": ",",
              "decimal": "2"
            }
          },
          "int_smtn": {
            "control": "currency",
            "label": "利息合计",
            "disabled": true,
            "width": "120px",
            "span": 8,
            "format":{
              "thousand": ",",
              "decimal": "2"
            }
          },
          "due_bill_amt": {
            "control": "currency",
            "label": "借据金额",
            "disabled": true,
            "width": "120px",
            "span": 8,
            "format":{
              "thousand": ",",
              "decimal": "2"
            }
          }
        }
      ]
    }
  }
}
