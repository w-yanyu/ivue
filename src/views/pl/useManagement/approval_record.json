{
    "module": "v_search_btn_datagrid",
    "layout": {
        "datagrid": {
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "pl0058",
                    "apply_no": "${{params.apply_no}}"
                }
            },
            "pagination": {
                "pageSizes": [10, 15, 20, 50, 100],
                "pageSize": 10
            },
            "thead": [
                
                {
                    "label": "任务名称",
                    "align": "center",
                    "prop": "task_inst_name"
                },
                {
                    "label": "处理时间",
                    "align": "center",
                    "prop": "submit_time"
                },
                {
                    "label": "处理用户",
                    "align": "center",
                    "prop": "user_id"
                },
                {
                    "label": "处理结果",
                    "align": "center",
                    "prop": "appv_result"
                },
                {
                    "label": "下一节点名称",
                    "align": "center",
                    "prop": "route_cd"
                },
                {
                    "label": "目标处理人",
                    "align": "center",
                    "prop": "agent_cd"
                }
            ]
        }
    }
}
