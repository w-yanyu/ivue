{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "ln3100",
                    "loan_due_bill_num": "${{params.loan_due_bill_num}}"
                }
            },
            "btns": [{
                "type": "submit",
                "label": "提交",
                "icon": "",
                "doRequest": {
                    "url": "/SUMP/icmcall/icmRPCCall",
                    "method": "POST",
                    "params": {
                        "servicecode": "ln3030",
                        "open_acct_oprn_flg": "4"    
                    }
                }
            }],
            "controlsGroup": [
                {
                  "title": "基本信息",
                  "tooltip": "",
                  "titleIcon": "",
                  "showHint": "",
                  "accordion": false,
                  "expand": true,
                  "controls": [
                    {
                        "loan_due_bill_num": {
                            "control": "text",
                            "label": "借据号",
                            "span": "8",
                            "width": "25px",
                            "disabled": true
                        },"cust_nm": {
                            "control": "text",
                            "label": "客户名称",
                            "span": "8",
                            "width": "25px",
                            "disabled": true
                        },"cust_num": {
                            "control": "text",
                            "label": "客户号",
                            "span": "8",
                            "width": "25px",
                            "disabled": true
                        }
                   },
                   {
                        "prod_nm": {
                            "control": "text",
                            "label": "产品名称",
                            "span": "8",
                            "width": "25px",
                            "disabled": true
                        },"prod_code": {
                            "control": "text",
                            "label": "产品代码",
                            "span": "8",
                            "width": "25px",
                            "disabled": true
                        },"ccy_code_num": {
                            "control": "select",
                            "label": "货币代号",
                            "span": "8",
                            "width": "25px",
                            "disabled": true,
                            "dict": {
                                "dictType": "MS_CURRENCY",
                                "dictKey": [
                                  "MS_CURRENCY"
                                ],
                                "format": "label"
                              }
                        }
                   }
                  ]
                },{
                    "title": "欠款信息",
                    "tooltip": "",
                    "titleIcon": "",
                    "showHint": "",
                    "accordion": false,
                    "expand": true,
                    "controls": [
                      {
                          "prcpl_smtn": {
                              "control": "text",
                              "label": "本金合计",
                              "span": "8",
                              "width": "25px",
                              "disabled": true
                          },"int_smtn": {
                              "control": "text",
                              "label": "利息合计",
                              "span": "8",
                              "width": "25px",
                              "disabled": true
                          },"nrl_prcpl": {
                              "control": "text",
                              "label": "正常本金",
                              "span": "8",
                              "width": "25px",
                              "disabled": true
                          }
                     },
                     {
                          "ovdue_prcpl": {
                              "control": "text",
                              "label": "逾期本金",
                              "span": "8",
                              "width": "25px",
                              "disabled": true
                          },"stgnt_prcpl": {
                              "control": "text",
                              "label": "呆滞本金",
                              "span": "8",
                              "width": "25px",
                              "disabled": true
                          },"chk_write_off_prcpl": {
                              "control": "text",
                              "label": "核销本金",
                              "span": "8",
                              "width": "25px",
                              "disabled": true
                          }
                     },{
                        "rcvbl_db_int": {
                            "control": "text",
                            "label": "应收欠息",
                            "span": "8",
                            "width": "25px",
                            "disabled": true
                        },"rcvbl_pnly_int": {
                            "control": "text",
                            "label": "应收罚息",
                            "span": "8",
                            "width": "25px",
                            "disabled": true
                        },"rcvbl_subsd_int": {
                            "control": "text",
                            "label": "应收贴息",
                            "span": "8",
                            "width": "25px",
                            "disabled": true
                        }
                   }
                    ]
                  },
                {
                    "title": "变更信息",
                    "tooltip": "",
                    "titleIcon": "",
                    "showHint": "",
                    "accordion": false,
                    "expand": true,
                    "controls": [
                      {
                        "nrl_int_rate": {
                            "control": "text",
                            "label": "正常利率",
                            "span": "8",
                            "width": "25px"
                        }
                     }
                    ]
                  }
            ]
        }
    }
}
