            "width": "130px"
          }

        },
        {
          "use_loan_cmtmt_flg": {
            "control": "select",
            "label": "使用贷款承诺标志",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            },
            "width": "130px"
          },
          "cmtmt_loan": {
            "control": "select",
            "label": "承诺贷款",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            },
            "width": "130px"
          }
        },
        {
          "orig_rfndd_way": {
            "label": "还款方式",
            "control": "select",
            "disabled": true,
            "dict": {
              "dictType": "MS_E_RPYMT_WAY",
              "dictKey": [
                "MS_E_RPYMT_WAY"
              ],
              "format": "value-label"
            },
            "rules": [
              {

                "required": true
              }
            ],
            "width": "130px"
          },
          "rvlg_loan_flg": {
            "control": "select",
            "label": "循环贷款标志",
            "dict": {
              "dictType": "MS_E_YESORNO",
