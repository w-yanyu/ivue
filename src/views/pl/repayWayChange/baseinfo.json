{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {

      "btns": [

      ],
      "controls": [
        {
          "cust_no": {
            "label": "cust_no",
            "control": "hidden"
          },
          "cust_name": {
            "label": "cust_name",
            "control": "hidden"
          },
