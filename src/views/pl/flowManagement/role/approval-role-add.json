{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "bpm0050",
          "appId":"plOnl"
        }
      },
      "controls": [
        {
          "appr_role_id": {
            "control": "text",
            "label": "审批角色编号",
            "rules": [
              {
                "required": true,
                "message": "审批角色编号"
              }
            ]
          },
          "appr_role_name": {
            "control": "text",
            "label": "审批角色名称",
            "rules": [
              {
                "required": true,
                "message": "审批角色名称"
              }
            ]
          }
        },
        {
          "appr_role_desc": {
            "control": "text",
            "label": "审批角色描述"
          },
            "valid_flag": {
                "control": "select",
                "label": "是否有效",
                "selectFirstValue":true,
                "dict": {
                    "dictType": "BPM_E_ISFLAG",
                    "dictKey": [
                        "BPM_E_ISFLAG"
                    ],
                    "format": "value-label"
                },
                "rules": [
                    {
                        "required": true,
                        "message": "请选择是否有效"
                    }]
            }
          
         
        },{
          "remark": {
            "control": "text",
            "label": "备注"
          }
        }

      ]
    }
  }
}
