{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "bpm0008",
              "route_id": "${{params.route_id}}",
              "trans_type": "UPDATE",
              "appId":"plOnl"
            }
          }
        }
      ],
      "controls": [
        {
          "route_cd": {
            "control": "string",
            "label": "路线编码",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入路线编码"
              }
            ]
          },
          "route_name": {
            "control": "string",
            "label": "路线名称",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入路线名称"
              }
            ]
          },
          "route_type": {
            "control": "select",
            "label": "路线类型",
            "width": "130px",
            "dict": {
              "dictType": "BPM_E_ROUTE_TYPE",
              "dictKey": [
                "BPM_E_ROUTE_TYPE"
              ],
              "labelField": "drop_list_desc",
              "valueField": "drop_list_value",
              "format": "value-label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择路线类型"
              }
            ],
            "events": {
              "condition": {
                  "REJT": {
                      "control": {
                          "goback_ind": {
                              "value":"1"
                          }
                      }
                  },
                  "BACK": {
                    "control": {
                        "goback_ind": {
                            "value":"1"
                        }
                    }
                  },
                  "$default": {
                      "control": {
                        "goback_ind": {
                          "value":"0"
                      }
                      }
                  }
                }
              }
          },
          "seq_no": {
            "control": "text",
            "label": "显示排序",
            "width": "130px",
            "rules": [
              {

                "message": "请输入显示排序"
              }
            ]
          },
          "wf_prod_no": {
            "control": "string",
            "label": "工作流产品编号",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入工作流产品编号"
              }
            ]
          },
          "wf_prod_name": {
            "control": "string",
            "label": "工作流产品名称",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入工作流产品名称"
              }
            ]
          },
          "task_define_id": {
            "control": "string",
            "label": "起始节点号",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入起始节点号"
              }
            ]
          },
          "task_define_name": {
            "control": "string",
            "label": "起始节点名称",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入起始节点名称"
              }
            ]
          },
          "next_task_define_id": {
            "control": "string",
            "label": "目标节点号",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入目标节点号"
              }
            ]
          },
          "next_task_define_name": {
            "control": "string",
            "label": "目标节点名称",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入目标节点名称"
              }
            ]
          },
          "goback_ind": {
            "disabled":true,
            "control": "select",
            "label": "是否退回原处理人",
            "width": "130px",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择是否退回原处理人"
              }
            ]
          },
          "route_action": {
            "control": "string",
            "label": "路线action",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入路线action"
              }
            ]
          },
          "remark": {
            "control": "textarea",
            "label": "流程描述",
            "width": "130px",
            "maxlength": 500,
            "showWordLimit": true
          }
        }
      ]
    }
  }
}
