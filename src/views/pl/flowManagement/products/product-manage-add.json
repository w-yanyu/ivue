{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
   
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/call/aOrUProcess",
            "method": "POST",
            "params": {
              "servicecode": "bpm0004",
              "appId":"plOnl"
            }
          }
        }
      ],
      "controls": [
        
        {
          "wf_prod_no": {
            "control": "text",
            "label": "流程产品编号",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入机构号"
              },
              {
                "max": 64,
                "message": "长度不能超过64个字符"
              }
            ]
          },
          "wf_prod_name": {
            "control": "text",
            "label": "流程产品名称",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入流程产品名称"
              },
              {
                "max": 64,
                "message": "长度不能超过64个字符"
              }
            ]
          },
          "wf_prod_tp": {
            "control": "select",
            "label": "流程产品类型",
            "width": "130px",
            "dict": {
              "dictType": "BPM_E_WFPDTP",
              "dictKey": [
                "BPM_E_WFPDTP"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择流程产品类型"
              }
            ]
          },
          "wf_define_id": {
            "control": "lookup",
            "label": "流程定义ID",
            "width": "130px",
            "prop": "actidd",
            "keyField":{
              "wf_define_id":"id_",
              "wf_define_name":"name_"
            },
            "toRequest": {
              "url": "/views/pl/flowManagement/products/process-product-list.json",
              "method": "post",
              "params": {
                "appId":"plOnl"
              }
            },
            "props": {
              "title": "流程模板查询"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择流程定义ID"
              }
            ]
          },
          "wf_define_name": {
            "control": "string",
            "label": "流程定义名称",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入流程定义名称"
              }
            ]
          }
        }
      ]
    }
  }
}
