{
  "module": "lr_transferTable",
  "layout": {
    "transfer": {
      "height":"500px",
      "left": {
        "filterable":false,
        "title": "可选策略",
        "btn":{
          "label": "授权",
          "doRequest": {
            "url":"/SUMP/call/playStrategy",
            "method":"post",
            "params":{
              "trans_type": "INSERT",
              "wf_task_no": "${{params.wf_task_no}}",
              "servicecode": "bpm0012",
              "appId":"plOnl"
            }
          }
        },
        "datagrid": {
          "doRequest": {
            "url":"/SUMP/icmcall/icmRPCCall",
            "method":"POST",
            "params": {
              "wf_task_no": "${{params.wf_task_no}}",
              "servicecode": "bpm0013",
              "appId":"plOnl"
            }
          },
          "checked":"checked",
          "pagination": {
            "show":true,
            "pageSizes": [5, 10, 15, 20],
            "pageSize": 5
          },
          "thead": [
            {
              "prop": "policy_name",
              "label":"策略名称"
            },
            {
              "prop": "policy_id",
              "label": "策略编号"

            }
          ]
        }
      },
      "right": {
        "filterable":false,
        "title":"已选策略",
        "placeholder": "请输入角色名称",
        "btn":{
          "label":"取消授权",
          "doRequest": {
            "url":"/SUMP/icmcall/icmRPCCall",
            "method":"post",
            "params":{
              "trans_type": "DELETE",
              "wf_task_no": "${{params.wf_task_no}}",
              "servicecode": "bpm0059",
              "appId":"plOnl"
            }
          }
        },
        "datagrid": {
          "doRequest": {
            "url":"/SUMP/icmcall/icmRPCCall",
            "method":"post",
            "params": {
              "wf_task_no": "${{params.wf_task_no}}",
              "servicecode": "bpm0011",
              "appId":"plOnl"
            }
          },
          "thead": [
            {
              "prop": "policy_name",
              "label":"策略名称"
            },
            {
              "prop": "policy_id",
              "label": "策略编号"
            }
          ]
        }
      }
    }
  }
}
