{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest":"${{params}}",
      "scope": "searchTable",
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/call/nodeModify",
            "method": "POST",
            "params": {
              "servicecode": "bpm0006",
              "wf_task_no": "${{params.wf_task_no}}",
              "trans_type": "UPDATE",
              "appId":"plOnl"
            }
          }
        }
      ],
      "controls": [
        {
          "wf_prod_no": {
            "control": "string",
            "label": "流程编号",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入流程编号"
              }
            ]
          },
          "wf_prod_name": {
            "control": "string",
            "label": "流程名称",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入流程名称"
              }
            ]
          },
          "task_define_id": {
            "control": "string",
            "label": "活动id",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入活动id"
              }
            ]
          },
          "task_define_name": {
            "control": "string",
            "label": "活动名称",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入活动名称"
              }
            ]
          },
          "task_type": {
            "control": "string",
            "label": "节点类型",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入节点类型"
              }
            ]
          },
          "task_url1": {
            "control": "lookup",
            "label": "编辑框架",
            "width": "130px",
            "disabled": false,
            "edit": true,
            "value": "",
            "keyField": {
              "task_url1": "frame_cd"
            },
            "toRequest": {
              "url": "/views/pl/flowManagement/products/node/menu_list.json",
              "method": "get",
              "params": {
                "appId":"plOnl"
              }
            },
            "props": {
              "title": "框架选择",
              "width": "80%"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择框架编号"
              }
            ]
          },
          "task_url2": {
            "control": "lookup",
            "label": "查看框架",
            "width": "130px",
            "disabled": false,
            "edit": true,
            "value": "",
            "keyField": {
              "task_url2": "frame_cd"
            },
            "toRequest": {
              "url": "/views/pl/flowManagement/products/node/menu_list.json",
              "method": "get",
              "params": {
                "appId":"plOnl"
              }
            },
            "props": {
              "title": "框架选择",
              "width": "80%"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择框架编号"
              }
            ]
          },
          "data_version": {
            "control": "string",
            "label": "版本号",
            "width": "130px",
            "rules": [
              {
                "required": true,
                "message": "请输入版本号"
              }
            ]
          },
          "remark": {
            "control": "textarea",
            "label": "流程备注",
            "width": "130px"
          }
        }
      ]
    }
  }
}
