{
  "module": "dynamicTabs",
  "layout": {
    "tabs": {
      "tabHeader": [
        {
          "id": "tabOne",
          "label": "策略配置",
          "closable": false,
          "disabled": false,
          "params": {
            "idField": "idField"
          },
          "icon": "el-icon-date",
          "toRequest": {
            "url": "/views/pl/flowManagement/products/node/node-rule.json",
            "method": "post",
            "params": {
              "appId":"plOnl"
            }
          }
        },
        {
          "id": "tabTwo",
          "label": "策略规则配置",
          "closable": false,
          "disabled": false,
          "params": {
            "idField": "idField"
          },
          "icon": "el-icon-date",
          "toRequest": {
            "url": "/views/pl/flowManagement/products/node/config/strategy-rule-config.json",
            "method": "post",
            "params": {
              "appId":"plOnl"
            }
          }
        }
      ],
      "defaultActiveKey": "tabOne",
      "position": "top",
      "styleClass": "card"
    }
  }
}
