{
    "module": "v_search_btn_datagrid",
    "layout": {
      "form": {
        "controls": [
          {
            "hypth_debt_goods_mterls_class": {
              "label": "抵债物资类别",
              "control": "select",
              "span": 12,
              "dict": {
                "dictType": "Pl_E_HYPTH_DEBT_GOODS_MTERLS_CLASS",
                "dictKey": [
                  "Pl_E_HYPTH_DEBT_GOODS_MTERLS_CLASS"
                ],
                "format": "label"
              }
            },
            "debt_asset_displ_catg": {
              "label": "抵债资产处置种类",
              "control": "select",
              "span": 12,
              "dict": {
                "dictType": "PL_E_DISPL_CATG",
                "dictKey": [
                  "PL_E_DISPL_CATG"
                ],
                "format": "value-label"
              }
            }
          },
          {
            "start_date": {
              "label": "起始日期",
              "control": "dateTimePicker",
              "span": 12,
              "format": "yyyyMMdd"
            },
            "end_date": {
              "label": "结束日期",
              "control": "dateTimePicker",
              "span": 12,
              "format": "yyyyMMdd"
            }
          },
          {
            "report_freq": {
              "label": "报表频率",
              "control": "select",
              "span": 12,"value":"1",

              "dict": {
                "dictType": "PL_E_REP_FREQ",
                "dictKey": [
                  "PL_E_REP_FREQ"
                ],
                "format": "label"
              }
            }
          }
        ]
      },
      "toolbar": {
        "btns": []
      },
      "datagrid": {
        "scope": "searchTable",
        "isInitSearch": true,
        "doRequest": {
          "url": "/SUMP/icmcall/icmRPCCall",
          "method": "POST",
          "params": {
            "servicecode": "pl0803"
          }
        },
        "thead": [
            {
              "label": "报表日期",
              "prop": "rept_dt",
              "align":"center"
            },
            {
              "label": "报表频率",
              "prop": "report_freq",
              "align":"center",
              "dict": {
                "dictType": "PL_E_REP_FREQ",
                "dictKey": [
                  "PL_E_REP_FREQ"
                ],
                "format": "label"
              }
            },
            {
              "label": "抵债物资类别",
              "prop": "hypth_debt_goods_mterls_class",
              "align":"center",
              "dict": {
                "dictType": "Pl_E_HYPTH_DEBT_GOODS_MTERLS_CLASS",
                "dictKey": [
                  "Pl_E_HYPTH_DEBT_GOODS_MTERLS_CLASS"
                ],
                "format": "label"
              }
            },
            {
              "label": "抵债资产处置种类",
              "prop": "debt_asset_displ_catg",
              "align":"center",
              "dict": {
                "dictType": "PL_E_DISPL_CATG",
                "dictKey": [
                  "PL_E_DISPL_CATG"
                ],
                "format": "value-label"
              }
            },
            {
              "label": "抵债资产处置笔数",
              "prop": "assets_deal_number",
              "align":"center"
            },
            {
              "label": "抵债资产处置金额",
              "prop": "assets_deal_mount",
              "align":"center"
            },
            {
              "label": "回收金额",
              "prop": "recovery_amount",
              "align":"center"
            },
            {
              "label": "处置费用",
              "prop": "disposal_costs",
              "align":"center"
            }   
        ]
      }
    }
  }
  