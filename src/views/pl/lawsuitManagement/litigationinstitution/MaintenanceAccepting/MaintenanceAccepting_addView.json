{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
           
            "initDataOnce": true,
            "initRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "pl0542",
                  "ma_org_no": "${{params.ma_org_no}}"
                }
            },
            "controls": [
                {
                    "case_acceptance": {
                        "control": "select",
                        "label": "机构类型",
                        
                        "dict": {
                            "dictType": "PL_E_ACCOUNT_TYPE",
                            "dictKey": [
                                "PL_E_ACCOUNT_TYPE"
                            ],
                            "format": "label"
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择机构类型"
                            }
                        ]
                    }, "branch_name": {
                        "control": "text",
                        "label": "机构名称",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入机构名称"
                            }
                        ]
                    }},
                        {
                        "acct_type": {
                            "control": "text",
                            "label": "申请类型",
                            "value":"新增",
                            "rules": [{
                                    "required": false,
                                    "message": "请输入申请类型"
                                }
                                
                            ],
                            "disabled":true
                        },
                         "remark":{
                        "control":"textarea",
                         "label":"备注",
                         "span":"24",
                         "rules": [
                            {
                                "required": true,
                                "message": "请输入备注"
                            }
                        ]
                         
                        }}
            ]
        }
    }
}