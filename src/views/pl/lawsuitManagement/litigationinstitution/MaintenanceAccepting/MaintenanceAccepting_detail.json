{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
          "scope": "templateScope",
          "initRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "pl0542",
                "ma_org_no": "${{params.ma_org_no}}"
                
              }
          },
          "initDataOnce":true,
  
  
            "btns": [
 
            ],
            "controls": [
                {
                    "ma_org_no": {
                        "control": "hidden",
                        "label": "受理机构编号",
                        "width": "130px",
                        "disabled":true
                    }

                },
                {  "case_acceptance": {
                    "control": "select",
                    "label": "机构类型",
                    "dict": {
                        "dictType": "PL_E_ACCOUNT_TYPE",
                        "dictKey": [
                            "PL_E_ACCOUNT_TYPE"
                        ],
                        "format": "label"
                    },
                    "disabled":true
                },
                    "branch_name": {
                        "control": "text",
                        "label": "机构名称",
                       
                        "disabled":true
                    }},{
                    "acceptance_org_type": {
                        "control": "select",
                        "label": "机构状态",
                       
                        "dict": {
                            "dictType": "PL_E_BRANCH_STATUS",
                            "dictKey": [
                                "PL_E_BRANCH_STATUS"
                            ],
                            "format": "label"
                        },
                        "disabled":true
                    }
                }
                
    
             ]
        }
    }
  }
  