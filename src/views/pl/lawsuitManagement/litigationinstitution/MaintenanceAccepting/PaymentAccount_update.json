{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "scope": "formData",
            "initRequest": "${{params}}",
            "btns": [{
                    "type": "submit",
                    "icon": "el-icon-check",
                    "label": "保存",
                    "doRequest": {
                        "method": "POST",
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "params": {
                            "servicecode": "pl0545"
                        }
                    }
                },
                {
                    "type": "reset",
                    "icon": "el-icon-close",
                    "label": "重置"
                }
            ],
            "controls": [
                {
                    "ma_org_no": {

                        "label": "受理机构编号",
                        "control": "hidden",
                        "placeholder": "请输入",
                        "rules": [{
                            "required": true,
                            "message": "请输入"
                        }]},
                        "account_id": {

                            "label": "账户ID",
                            "control": "hidden",
                            "placeholder": "请输入",
                            "rules": [{
                                "required": true,
                                "message": "请输入"
                            }]}
                    },{
                    "acct_type": {
                        "control": "select",
                        "label": "收款账户类型",
                        "placeholder": "请选择",
                        "dict": {
                            "dictType": "Pl_E_ACCOUNTYPE",
                            "dictKey": [
                                "Pl_E_ACCOUNTYPE"
                            ],
                            "format": "label"
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入收款账户类型"
                            }
                        ]
                    },
                    "acct_name": {
                        "control": "text",
                        "label": "收款账户名称",
                        "placeholder": "请输入",
                        "rules": [{
                            "required": true,
                            "message": "请输入账户名称"
                        }]
                    }
                    },{
                        "acct_no": {
                        "control": "text",
                        "label": "收款账户账号",
                        "placeholder": "请输入",
                        
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入收款账户账号"
                            }
                        ]
                    }, 
                    "bank_name": {
                        "control": "text",
                        "label": "开户行",
                        "placeholder": "请输入",
                        
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入开户行"
                            }
                        ]
                    }},{
                        "bank_no": {
                        "control": "text",
                        "label": "开户行行号",
                        "placeholder": "请输入",
                        
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入开户行行号"
                            }
                        ]
                    }
                }

               
                
            ]
        }
    }
}
