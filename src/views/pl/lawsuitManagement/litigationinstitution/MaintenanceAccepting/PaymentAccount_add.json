{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "scope": "formData",
            "btns": [{
                    "type": "submit",
                    "label": "保存",
                    "doRequest": {
                        "method": "POST",
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "params": {
                            "servicecode": "pl0544",
                            "ma_org_no": "${{params.ma_org_no}}"
                        }
                    }
                },
                {
                    "type": "reset",
                    "icon": "el-icon-close",
                    "label": "重置"
                }
            ],
            "controls": [
                {   "acct_type": {
                    "control": "select",
                    "label": "收款账户类型",
                    "placeholder": "请选择",
                    "dict": {
                        "dictType": "Pl_E_ACCOUNTYPE",
                        "dictKey": [
                            "Pl_E_ACCOUNTYPE"
                        ],
                        "format": "label"
                    },
                    "rules": [{
                        "required": true,
                        "message": "请选择"
                    }]},
                    "acct_name": {
                        "control": "text",
                        "label": "收款账户名称",
                        "placeholder": "请输入",
                        "rules": [{
                            "required": true,
                            "message": "请输入"
                        }]
                    }
                },{
                    "acct_no": {
                    "control": "text",
                    "label": "收款账户账号",
                    "placeholder": "请输入",
                    "rules": [{
                        "required": true,
                        "message": "请输入"
                    }]
                    },

                        "bank_name": {
                        "control": "text",
                        "label": "开户行",
                        "placeholder": "请输入",
                        "rules": [{
                            "required": true,
                            "message": "请输入"
                        }]
                    }
                },{
                        "bank_no": {
                        "control": "text",
                        "label": "开户行行号",
                        "placeholder": "请输入",
                        "rules": [{
                            "required": true,
                            "message": "请输入"
                        }]
                    }
                }
                


            ]
        }
    }
}
