                    "message": "请输入"
                }]
                 }},
                {
                "acct_no": {
                "control": "string",
                "label": "收款账户账号",
                "placeholder": "请输入",
                "rules": [{
                    "message": "请输入"
                }]
                }, 
               "bank_name": {
                "control": "string",
                "label": "开户行",
                "placeholder": "请输入",
                "rules": [{
                    "message": "请输入"
                }]
                }},
                 {
                "bank_no": {
                "control": "string",
                "label": "开户行行号",
                "placeholder": "请输入",
                "rules": [{
                    "message": "请输入"
                }]
            }
            }
                


            ]
        }
    }
}
