{
    "module": "v-realonlyForm-btn",
    "layout": {
        "form": {
            "scope": "formData",
            "initRequest": "${{params}}",
            "controls": [{
                "acct_type": {
                "control": "string",
                "label": "收款账户类型",
                "dict": {
                    "dictType": "Pl_E_ACCOUNTYPE",
                    "dictKey": [
                        "Pl_E_ACCOUNTYPE"
                    ],
                    "format": "label"
                }
                },
                "acct_name": {
                "control": "string",
                "label": "收款账户名称",
                "placeholder": "请输入",
