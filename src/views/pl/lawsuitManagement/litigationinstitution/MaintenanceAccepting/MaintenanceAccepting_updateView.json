{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
          "scope": "templateScope",
          "initRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "pl0542",
                "ma_org_no": "${{params.ma_org_no}}"
                
              }
          },
          "initDataOnce":true,
  
  
            "btns": [
 
            ],
            "controls": [
                {
                    "ma_org_no": {
                        "control": "hidden",
                        "label": "受理机构编号",
                        "rules": [{
                                "required": true,
                                "message": "请输入受理机构编号"
                            }
                        ],
                        "disabled":true
                    }

                },
                {
                    "case_acceptance": {
                    "control": "select",
                    "label": "机构类型",
                    "placeholder": "请选择",
                    "dict": {
                        "dictType": "PL_E_ACCOUNT_TYPE",
                        "dictKey": [
                            "PL_E_ACCOUNT_TYPE"
                        ],
                        "format": "label"
                    },
                    "rules": [
                        {
                            "required": true,
                            "message": "请选择机构类型"
                        }
                    ]
                },
                    "branch_name": {
                        "control": "text",
                        "label": "机构名称",
                        "rules": [{
                                "required": true,
                                "message": "请输入客户编号"
                            }
                        ]
                        
                    }},{
                        "acceptance_org_type": {
                        "control": "select",
                        "label": "机构状态",
                        "placeholder": "请选择",
                        "dict": {
                            "dictType": "PL_E_BRANCH_STATUS",
                            "dictKey": [
                                "PL_E_BRANCH_STATUS"
                            ],
                            "format": "label"
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择机构状态"
                            }
                        ]
                    },"acct_type": {
                        "control": "text",
                        "label": "申请类型",
                        "value":"修改",
                        "rules": [{
                                "required": false,
                                "message": "请输入申请类型"
                            }
                            
                        ],
                        "disabled":true
                    }},{
                        "remark":{
                            "control":"textarea",
                             "label":"备注",
                             "span":"24",
                             "rules": [
                                {
                                    "required": true,
                                    "message": "请输入备注"
                                }
                            ]
                             
                            }
                    }
                    
    
             ]
        }
    }
  }
  