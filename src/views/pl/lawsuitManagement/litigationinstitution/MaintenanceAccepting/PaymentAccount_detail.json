{
    "module": "v_search_btn_datagrid",
    "layout": {
        "toolbar": {
            "btns": []
        },
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": true,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "pl0543",
                    "ma_org_no": "${{params.ma_org_no}}"
                }
            },
            "thead": [{
                    "type": "index",
                    "width": 50,
                    "label": "序号"
                },
                {
                    "label": "收款账户类型",
                    "prop": "acct_type",
                    "align":"center",
                    "dict": {
                    "dictType": "Pl_E_ACCOUNTYPE",
                    "dictKey": [
                      "Pl_E_ACCOUNTYPE"
                   ]
                }
                
                },
                {
                    "label": "收款账户名称",
                    "prop": "acct_name",
                    "align": "center"
                },
                {
                    "label": "收款账户账号",
                    "prop": "acct_no",
                    "align": "center"
                },
                {
                    "label": "开户行",
                    "prop": "bank_name",
                    "align": "center"
                },
                {
                    "label": "开户行行号",
                    "prop": "bank_no",
                    "align": "center"
                },
                {
                    "label": "操作",
                    "fixed": "right",
                    "width": 160,
                    "align": "center",
                    "btns": [{
                            "type": "look",
                            "label": "详情",
                            "toRequest": {
                                "url": "/views/pl/lawsuitManagement/litigationinstitution/MaintenanceAccepting/PaymentAccount_info.json",
                                "method": "POST"
                            },
                            "props": {
                                "width": "75%",
                                "title": "受理机构收款账户详情"
                            }
                        }
                    ]
                }
            ]
        }
    }
}
