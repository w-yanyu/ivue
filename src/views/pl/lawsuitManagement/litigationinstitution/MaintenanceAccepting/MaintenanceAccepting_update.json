{
    "module": "d-steps",
    "layout": {
      "steps": {
        "scope": "formObj",
        "direction": "horizontal",
        "alignCenter": true,
        "finishStatus": "success",
        "simpleType": false,
        "nextStepType": "submit",
        "list": [
          {
            "###forbidBack":true,
            "scope": "step1",
            "title": "受理机构信息维护",
            "icon": "el-icon-edit",
            "toRequest": {
              "url": "/views/pl/lawsuitManagement/litigationinstitution/MaintenanceAccepting/MaintenanceAccepting_updateView.json",
              "method": "get",
              "params": {
                "ma_org_no": "${{params.ma_org_no}}"
              }
            },
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "post",
              "params":{
                "servicecode":"pl0540"
               
              },
              "resultSet": {
  
              }
            }
          },
          {
            "###forbidBack":true,
            "scope": "step2",
            "title": "受理机构收款账户",
            "icon": "el-icon-edit",
            "toRequest": {
              "url": "/views/pl/lawsuitManagement/litigationinstitution/MaintenanceAccepting/PaymentAccount.json",
              "method": "get",
              "params": {
                "ma_org_no": "${{formObj.step1.ma_org_no}}"
  
              }
            },
            "doRequest": {
              "url": "/pte/json/layout/success.json",
              "method": "post",
              "eventAfter": {
                "doParentCallback": true
              }
            }
          }
          
          
        ]
      }
    }
  }
  