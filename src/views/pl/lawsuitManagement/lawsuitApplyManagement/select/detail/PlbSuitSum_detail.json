{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
          "scope": "templateScope",
          "initRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "pl0517",
                "loan_no": "${{params.loan_no}}"
                
              }
          },
          "initDataOnce":true,
  
  
            "btns": [
 
            ],
            "controls": [
                {
                    "loan_no": {
                        "control": "text",
                        "label": "借据号",
                        "width": "130px",
                        "rules": [{
                                "required": true,
                                "message": "请输入合作方账号"
                            },
                            {
                                "max": 200,
                                "message": "长度不能超过200个字符"
                            }
                        ],
                        "disabled":true
                    },
                    "cust_name": {
                        "control": "text",
                        "label": "客户姓名",
                        "width": "130px",
                        "rules": [{
                                "required": true,
                                "message": "请输入合作方账号"
                            },
                            {
                                "max": 200,
                                "message": "长度不能超过200个字符"
                            }
                        ],
                        "disabled":true
                    }
                    
                },
                {
                    "cust_no": {
                        "control": "text",
                        "label": "客户编号",
                        "width": "130px",
                        "rules": [{
                                "required": false,
                                "message": "请输入客户编号"
                            },
                            {
                                "max": 80,
                                "message": "长度不能超过80个字符"
                            }
                        ],
                        "disabled":true
                    },
                    "prod_no": {
                        "control": "text",
                        "label": "产品号",
                        "width": "130px",
                        "rules": [{
                                "required": false,
                                "message": "请输入客户编号"
                            },
                            {
                                "max": 80,
                                "message": "长度不能超过80个字符"
                            }
                        ],
                        "disabled":true
                    }
                },
                {
                  "prod_name": {
                      "control": "text",
                      "label": "产品名",
                      "width": "130px",
                      "rules": [{
                              "required": true,
                              "message": "请输入合作方证件号码"
                          },
                          {
                              "max": 30,
                              "message": "长度不能超过30个字符"
                          }
                      ],
                      "disabled":true
                  },
                  "loan_form": {
                    "control": "select",
                    "label": "贷款形态",
                    "disabled":true,
                    "width": "130px",
                    "dict": {
                        "dictType": "MS_E_LEVEL5_TYPE",
                        "dictKey": [
                            "MS_E_LEVEL5_TYPE"
                        ],
                        "format": "label"
                    }

                    }
    
                },
                {
                    "entrust_status": {
                        "control": "select",
                        "label": "当前案件委案状态",
                        "disabled":true,
                        "width": "130px",
                        "dict": {
                            "dictType": "PL_E_ENTRUSTSTUS",
                            "dictKey": [
                                "PL_E_ENTRUSTSTUS"
                            ],
                            "format": "label"
                        }
    
                        },
                    "suit_status": {
                        "control": "select",
                        "label": "当前案件诉讼状态",
                        "disabled":true,
                        "width": "130px",
                        "dict": {
                            "dictType": "PL_E_SUITSTUS",
                            "dictKey": [
                                "PL_E_SUITSTUS"
                            ],
                            "format": "label"
                        }
    
                    }
                },
                {
                    "is_closed": {
                        "control": "select",
                        "label": "是否关闭",
                        "disabled":true,
                        "width": "130px",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        }
    
                    },
                    "is_finished": {
                        "control": "select",
                        "label": "是否结案",
                        "disabled":true,
                        "width": "130px",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        }
    
                    }
    
                },
                {
                    "is_lgfe": {
                        "control": "select",
                        "label": "是否法诉",
                        "disabled":true,
                        "width": "130px",
                        "dict": {
                            "dictType": "MS_E_YESORNO",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "format": "label"
                        }
    
                    },
                    "entrorg_name": {
                        "control": "text",
                        "label": "委案机构名称",
                        "width": "130px",
                        "rules": [{
                                "required": false,
                                "message": "请输入客户编号"
                            },
                            {
                                "max": 80,
                                "message": "长度不能超过80个字符"
                            }
                        ],
                        "disabled":true
                    }
    
                }
             ]
        }
    }
  }
  