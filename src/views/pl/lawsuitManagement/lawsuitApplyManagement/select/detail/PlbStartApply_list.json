{
    "module": "v_search_btn_datagrid",
    "layout": {
        "toolbar": {
            "btns": []
        },
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": true,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "pl0518",
                    "loan_no": "${{params.loan_no}}"
                }
            },
            "thead": [{
                    "type": "index",
                    "width": 50,
                    "label": "序号"
                },
                {
                    "label": "申请号",
                    "prop": "apply_no",
                    "align": "center",
                    "disabled": true
                },
                {
                    "label": "借据号",
                    "prop": "loan_no",
                    "align": "center"
                },
                {
                    "label": "诉讼主因",
                    "prop": "suitReason",
                    "align": "center",
                    "dict": {
                        "dictType": "PL_E_SUITREASON",
                        "dictKey": [
                            "PL_E_SUITREASON"
                        ],
                        "labelField": "dictName",
                        "valueField": "dictId"
                    }
                },
                {
                    "label": "诉讼审批状态",
                    "prop": "apply_status",
                    "align": "center",
                    "dict": {
                        "dictType": "PL_E_SUITAPLYSTATUS",
                        "dictKey": [
                            "PL_E_SUITAPLYSTATUS"
                        ],
                        "labelField": "dictName",
                        "valueField": "dictId"
                    }
                }
            ]
        }
    }
}
