{
    "module": "v_search_btn_datagrid",
    "layout": {
        "toolbar": {
            "btns": []
        },
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": true,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "pl0519",
                    "loan_no": "${{params.loan_no}}"
                }
            },
            "thead": [{
                    "type": "index",
                    "width": 50,
                    "label": "序号"
                },
                {
                    "label": "申请号",
                    "prop": "apply_no",
                    "align": "center",
                    "disabled": true
                },
                {
                    "label": "借据号",
                    "prop": "loan_no",
                    "align": "center"
                },
                {
                    "label": "委案机构名称",
                    "prop": "entrorg_name",
                    "align": "center"
                },
                {
                    "label": "诉讼地",
                    "prop": "forum_willmigerl",
                    "align": "center",
                    "dict": {
                        "dictType": "PL_E_SUITSITE",
                        "dictKey": [
                            "PL_E_SUITSITE"
                        ],
                        "labelField": "dictName",
                        "valueField": "dictId"
                    }
                },
                {
                    "label": "机构类型",
                    "prop": "entrorg_type",
                    "align": "center",
                    "dict": {
                        "dictType": "PL_E_ORGTYPES",
                        "dictKey": [
                            "PL_E_ORGTYPES"
                        ],
                        "labelField": "dictName",
                        "valueField": "dictId"
                    }
                },
                {
                    "label": "费用结算模式",
                    "prop": "clen_mode",
                    "align": "center",
                    "dict": {
                        "dictType": "PL_E_CLENMODE",
                        "dictKey": [
                            "PL_E_CLENMODE"
                        ],
                        "labelField": "dictName",
                        "valueField": "dictId"
                    }
                },
                {
                    "label": "诉讼审批状态",
                    "prop": "apply_status",
                    "align": "center",
                    "dict": {
                        "dictType": "PL_E_SUITAPLYSTATUS",
                        "dictKey": [
                            "PL_E_SUITAPLYSTATUS"
                        ],
                        "labelField": "dictName",
                        "valueField": "dictId"
                    }
                }
            ]
        }
    }
}
