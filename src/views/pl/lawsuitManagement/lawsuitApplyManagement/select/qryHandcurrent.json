{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "loan_no": {
            "control": "text",
            "label": "借据号"

          },
          "cust_name": {
            "control": "text",
            "label": "客户姓名"
          }
        },
        {
          "entrorg": {
            "control": "text",
            "label": "委案机构"
          }
         
        }

      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "export",
          "label": "导出",
          "icon": "iconfont icon-daochu",
          "doRequest": {
            "url": "/SUMP/rest/file/commExport",
            "method": "post",
            "params": {
              "servicecode": "pl0523",
              "exportname": "委案信息表"

            }
          },
          "confirm":{
            "title":"提示信息",
            "msg":"确认导出吗？",
            "confirmButtonText":"确定",
            "cancelButtonText":"取消"
          }
        }

      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pl0523",

          "page_id": "/views/pt/partnerManage/ptPartner_list.json"
        }
      },
      "thead": [
        {
          "type": "index"
        },
        {
          "label": "申请编号",
          "prop": "apply_no",
          "align":"center"
        },
        {
          "label": "客户名称",
          "prop": "cust_name",
          "align":"center"
        },
        {
          "label": "客户编号",
          "prop": "cust_no",
          "align":"center"
        },
        {
          "label": "借据号",
          "prop": "loan_no",
          "align":"center"
        },
        {
          "label": "委案时间",
          "prop": "entr_time",
          "align":"center"
        },
        {
          "label": "委案时间",
          "prop": "entr_time",
          "align":"center"
        },
        {
          "label": "委案机构",
          "prop": "entrorg_name",
          "align":"center"
        }
      ]
    }
  }
}
