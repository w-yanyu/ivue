{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "scope": "formData",
            "initRequest": "${{params}}",
            "btns": [{
                    "type": "submit",
                    "icon": "el-icon-check",
                    "label": "保存",
                    "doRequest": {
                        "method": "POST",
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "params": {
                            "servicecode": "pl0536"
                        }
                    }
                },
                {
                    "type": "reset",
                    "icon": "el-icon-close",
                    "label": "重置"
                }
            ],
            "controls": [
                {
                    "attn_name": {
                        "control": "text",
                        "label": "收件人名称",
                        
                        "placeholder": "请输入",
                        "rules": [{
                            "required": true,
                            "message": "请输入"
                        }]
                    },"attn_address": {
                        "control": "text",
                        "label": "收件人地址",
                       
                        "placeholder": "请输入",
                        "rules": [{
                            "required": true,
                            "message": "请输入"
                        }]
                    }
                },
                {
                    "city_name": {
                        "control": "text",
                        "label": "机构名称",
                        "placeholder": "请输入",
                        "rules": [{
                            "required": true,
                            "message": "请输入"
                        }]
                    },
                    "attn_phone": {
                        "control": "text",
                        "label": "收件人电话",
                      
                        "placeholder": "请输入",
                        "rules": [{
                            "required": true,
                            "message": "请输入"
                        }]
                    }
                },{
                    "attn_id": {
                        "control": "hidden",
                        "label": "用印材料收件人管理编号",
                        "placeholder": "请输入",
                        "rules": [{
                            "required": true,
                            "message": "请输入"
                        }]
                    }},
{
                    "remark":{
                        "control":"textarea",
                         "label":"备注",
                         "span":"24"
                        }
                        }


            ]
        }
    }
}
