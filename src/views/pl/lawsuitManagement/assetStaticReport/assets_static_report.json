{
    "module": "v_search_btn_datagrid",
    "layout": {
      "form": {
        "controls": [
          {
            "oprtg_org": {
              "label": "营业机构",
              "control": "text",
              "span": 12
            },
            "report_freq": {
              "label": "报表频率",
              "control": "select",
              "span": 12,"value":"1",
              "dict": {
                "dictType": "PL_E_REP_FREQ",
                "dictKey": [
                  "PL_E_REP_FREQ"
                ],
                "format": "label"
              }
            }
          },
          {
            "start_date": {
              "label": "起始日期",
              "control": "dateTimePicker",
              "span": 12,
              "format": "yyyyMMdd"
            },
            "end_date": {
              "label": "结束日期",
              "control": "dateTimePicker",
              "span": 12,
              "format": "yyyyMMdd"
            }
          },
          {
            "hypth_debt_goods_mterls_class": {
              "label": "抵债物资类别",
              "control": "select",
              "span": 12,
              "dict": {
                "dictType": "Pl_E_HYPTH_DEBT_GOODS_MTERLS_CLASS",
                "dictKey": [
                  "Pl_E_HYPTH_DEBT_GOODS_MTERLS_CLASS"
                ],
                "format": "label"
              }
            }
          }
        ]
      },
      "toolbar": {
        "btns": []
      },
      "datagrid": {
        "scope": "searchTable",
        "isInitSearch": true,
        "doRequest": {
          "url": "/SUMP/icmcall/icmRPCCall",
          "method": "POST",
          "params": {
            "servicecode": "pl0802"
          }
        },
        "thead": [
            {
              "label": "报表日期",
              "prop": "rept_dt",
              "align":"center"
            },
            {
              "label": "报表频率",
              "prop": "report_freq",
              "align":"center",
              "dict": {
                "dictType": "PL_E_REP_FREQ",
                "dictKey": [
                  "PL_E_REP_FREQ"
                ],
                "format": "label"
              }
            },
            {
              "label": "抵债物资类别",
              "prop": "hypth_debt_goods_mterls_class",
              "align":"center",
              "dict": {
                "dictType": "Pl_E_HYPTH_DEBT_GOODS_MTERLS_CLASS",
                "dictKey": [
                  "Pl_E_HYPTH_DEBT_GOODS_MTERLS_CLASS"
                ],
                "format": "label"
              }
            },
            {
              "label": "抵债资产新增数量",
              "prop": "assets_add_number",
              "align":"center"
            },
            {
              "label": "抵债资产新增金额",
              "prop": "assets_add_mount",
              "align":"center"
            }    
        ]
      }
    }
  }
  