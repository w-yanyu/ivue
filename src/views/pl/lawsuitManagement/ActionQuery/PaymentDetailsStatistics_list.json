{
    "module": "v_search_btn_datagrid",
    "layout": {
      "form": {
        "controls": [
          {
            "loan_no": {
              "control": "text",
              "label": "借据号"
  
            },
            "cust_name": {
              "control": "text",
              "label": "客户名称"
            }
          },
          {
            "expense_type": {
              "control": "select",
              "label": "费用类型",
               "dict": {
                 "dictType": "PL_E_EXPENSE_TYPE",
                 "dictKey": [
                   "PL_E_EXPENSE_TYPE"
                 ],
                 "format": "label"
               }
            },
            "acct_name": {
                "control": "text",
                "label": "收款账户名称"
              }
          }
        ]
      },
      "toolbar": {
        "btns": []
      },
      "datagrid": {
        "scope": "searchTable",
        "isInitSearch": true,
        "doRequest": {
          "url": "/SUMP/icmcall/icmRPCCall",
          "method": "POST",
          "params": {
            "servicecode": "pl0549"
  
            
          }
        },
        "thead": [
            {
              "label": "缴费申请单号",
              "prop": "apply_no",
              "align":"center"
            },
            {
              "label": "借据号",
              "prop": "loan_no",
              "align":"center"
            },
            {
              "label": "客户名称",
              "prop": "cust_name",
              "align":"center"
            },
            {
              "label": "费用类型",
              "prop": "expense_type",
              "align":"center",
              "dict": {
                "dictType": "PL_E_EXPENSE_TYPE",
                "dictKey": [
                  "PL_E_EXPENSE_TYPE"
                ],
                "format": "label"
              }
            },
            {
              "label": "费用金额",
              "prop": "pay_amount",
              "align":"center"
            },
            {
              "label": "收款账号类型",
              "prop": "acctno_type",
              "align":"center",
              "dict": {
                "dictType": "PL_E_ACCOUNT_TYPE",
                "dictKey": [
                  "PL_E_ACCOUNT_TYPE"
                ],
                "format": "label"
              }
            },
            {
              "label": "收款账户账号",
              "prop": "rece_accno",
              "align":"center"
            },
            {
              "label": "收款账户名称",
              "prop": "acct_name",
              "align":"center"
            },
            {
              "label": "缴费发起人",
              "prop": "data_create_user",
              "align":"center"
            },
            {
              "label": "发起时间",
              "prop": "data_create_time",
              "align":"center"
            },
            {
              "label": "结案时间",
              "prop": "check_post_date",
              "align":"center"
            }         
        ]
      }
    }
  }
  