{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "btns": [
        {
          "type": "submit",
          "icon": "el-icon-check",
          "label": "保存",
          "doRequest": {
            "method": "POST",
            "url": "/SUMP/icmcall/icmRPCCall",
            "params": {
              "servicecode": "pl0002",
              "change_num": "${{params.change_num}}",
              "txn_type": "UPDATE"
            }
          }
        },
        {
          "type": "reset",
          "icon": "el-icon-close",
          "label": "重置"
        }
      ],
      "controls": [
        {
          "cust_no": {
            "label": "cust_id",
            "control": "hidden"
          },
          "cust_name": {
            "label": "custna",
            "control": "hidden"
          },
          "cert_no": {
            "label": "证件号码",
            "control": "hidden"
          },
          "contract_no": {
            "label": "合同号",
            "control": "hidden"
          }
        },
        {
          "loan_no": {
            "label": "借据号",
            "control": "string",
            "rules": [
              {
                "required": true,
                "message": "请输入借据号"
              }
            ]
          },
          "prod_no": {
            "label": "贷款产品",
            "control": "string",
            "rules": [
              {
                "required": true,
                "message": "请输入贷款产品"
              }
            ]
          },
          "change_befr_acct": {
            "label": "账号(原)",
            "control": "string",
            "rules": [
              {
                "required": true,
                "message": "请输入原账号"
              }
            ]
          },
          "change_aft_acct": {
            "label": "账号(新)",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入变更后账号"
              }
            ]
          },
          "change_aft_acct_nm": {
            "label": "账户名称(新)",
            "control": "hidden"
          },
          "change_befr_acct_tp": {
            "label": "账户类型(原)",
            "control": "select",
            "disabled":true,
            "dict": {
              "dictType": "PL_E_PAYACT",
              "dictKey": [
                "PL_E_PAYACT"
              ],
              "format": "label"
            }
          },
          "change_aft_acct_tp": {
            "label": "账户类型(新)",
            "control": "select",
            "disabled":true,
            "value": "0",
            "dict": {
              "dictType": "PL_E_PAYACT",
              "dictKey": [
                "PL_E_PAYACT"
              ],
              "format": "label"
            }
          },
          "ccy_code": {
            "label": "币种",
            "control": "select",
            "disabled":true,
"selectFirstValue":true,
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": [
                "MS_CURRENCY"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请输入币种"
              }
            ]
          },
          "auto_pymt_tp": {
            "label": "自动付款类型",
            "control": "select",
            "dict": {
              "dictType": "PL_E_AUTO_PAY_TYPE",
              "dictKey": [
                "PL_E_AUTO_PAY_TYPE"
              ],
              "format": "label"
            }
          },
          "deal_dt": {
            "label": "处理日期",
            "control": "dateTimePicker",
            "format": "yyyyMMdd",
            "rules": [
              {
                "required": true,
                "message": "请输入处理日期"
              }
            ]
          },
          "matu_dt": {
            "label": "到期日",
            "control": "dateTimePicker",
            "format": "yyyyMMdd",
            "rules": [
              {
                "required": true,
                "message": "请输入到期日"
              }
            ]
          },
          "loan_matu": {

            "label": "贷款期限",
            "control": "hidden"
          },
          "ctrct_int_rate": {
            "label": "合同利率",
            "control": "hidden"
          },
          "nrl_int_rate": {
            "label": "正常利率",
            "control": "hidden"
          },
          "pay_prty": {
            "label": "支付优先级",
            "control": "select",
            "dict": {
              "dictType": "PL_E_PAY_PRIORITY",
              "dictKey": [
                "PL_E_PAY_PRIORITY"
              ],
              "format": "label"
            }
          },
          "wthr_sign": {
            "label": "是否签约",
            "control": "select",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "remark": {
            "label": "备注",
            "control": "textarea",
            "span": 24,
            "autosize": {
              "minRows": 3,
              "maxRows": 5
            },
            "maxlength": 100,
            "showWordLimit": true
          }
        }
      ]
    }
  }
}
