{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "loan_due_bill_num": {
            "control": "text",
            "label": "借据号"
          },
          "cust_num": {
            "control": "hidden",
            "label": "客户号",
            "value":"${{params.cust_num}}"
        }
      }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "syln0002"  ,
          "outlistid":"lnCpstnQryType"
        }
      },
      "thead": [
        {
          "label": "操作",
          "fixed": "left",
          "btns": [
            {
              "type": "bringback",
              "label": "选择"
            }
          ]
        },
        {
          "label": "贷款借据号",
          "prop": "loan_due_bill_num"
        },
        {
          "label": "还款账号",
          "prop": "rpymt_acct_num"
        },
        {
          "label": "贷款产品",
          "prop": "prod_nm"
        },
        {
          "label": "客户姓名",
          "prop": "cust_nm"
        },
        {
          "label": "借据金额",
          "prop": "due_bill_amt"
        }
      ]
    }
  }
}
