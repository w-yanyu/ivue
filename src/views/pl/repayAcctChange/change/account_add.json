{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {

      "btns": [

      ],
      "controls": [
        {
          "cust_no": {
            "label": "cust_no",
            "control": "hidden"
          },
          "cust_name": {
            "label": "cust_name",
            "control": "hidden"
          },
          "cert_no": {
            "label": "证件号码",
            "control": "hidden"
          },
          "contract_no": {
            "label": "合同号",
            "control": "hidden"
          }
        },
        {
          "loan_no": {
            "label": "借据号",
            "control": "lookup",
            "align":"center",
            "edit": false,
            "keyField": {
              "loan_no": "loan_due_bill_num",
              "cust_no": "cust_no",
              "prod_no": "prod_nm",
              "change_befr_acct": "rpymt_acct_num",
              "change_befr_acct_nm": "cust_nm",
              "change_befr_acct_tp": "change_befr_acct_tp",
              "change_aft_acct_nm": "cust_nm",
              "contract_no": "contract_no",
              "matu_dt": "matu_dt",
              "loan_matu": "loan_matu",
              "ctrct_int_rate": "ctrct_int_rate",
              "nrl_int_rate": "nrl_int_rate",
              "cert_no": "cert_no",
              "cust_name": "cust_nm"
            },
            "toRequest": {
              "url": "/views/pl/repayAcctChange/change/duebill_no_lookup.json",
              "method": "post"
            },
            "rules": [
              {
                "required": true,
                "message": "请输入借据号"
              }
            ]
          },
          "prod_no": {
            "label": "贷款产品",
            "control": "text",
            "rules": [
              {
                "required": true,
                "message": "请输入贷款产品"
              }
            ]
          },
          "change_befr_acct": {
            "label": "账号(原)",
            "control": "text",
            "disabled": true,
            "rules": [
              {
                "required": false
              }
            ]
          },
          "change_aft_acct": {
            "label": "账号(新)",
            "control": "text",
            "rules": [
              {

                "required": true,
                "message": "请输入变更后账号"
              }
            ]
          },
          "change_befr_acct_nm": {
            "label": "账户名称(原)",
            "control": "string"
          },
          "change_aft_acct_nm": {
            "label": "账户名称(新)",
            "control": "string",
            "rules": [
              {
                "required": false,
                "message": "请输入账户名称(新)"
              }
            ]
          },
          "change_befr_acct_tp": {
            "label": "账户类型(原)",
            "control": "hidden",
            "value": "0",
            "disabled":true,
            "dict": {
              "dictType": "PL_E_PAYACT",
              "dictKey": [
                "PL_E_PAYACT"
              ],
              "format": "label"
            }
          },
          "change_aft_acct_tp": {
            "label": "账户类型(新)",
            "control": "hidden",
            "value": "0",
            "disabled":true,
            "dict": {
              "dictType": "PL_E_PAYACT",
              "dictKey": [
                "PL_E_PAYACT"
              ],
              "format": "label"
            }
          },
          "ccy_code": {
            "label": "币种",
            "control": "hidden",
            "disabled":true,
            "value": "156",
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": [
                "MS_CURRENCY"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择币种"
              }
            ]
          },
          "auto_pymt_tp": {
            "label": "自动付款类型",
            "control": "select",
            "dict": {
              "dictType": "PL_E_AUTO_PAY_TYPE",
              "dictKey": [
                "PL_E_AUTO_PAY_TYPE"
              ],
              "format": "label"
            }
          },
          "deal_dt": {
            "label": "处理日期",
            "control": "dateTimePicker",
            "format": "yyyyMMdd",
            "rules": [
              {
                "required": true,
                "message": "请输入处理日期"
              }
            ]
          },
          "matu_dt": {
            "label": "到期日",
            "control": "hidden",
            "format": "yyyyMMdd",
            "rules": [
              {

                "message": "请输入到期日"
              }
            ]
          },
          "pay_prty": {
            "label": "支付优先级",
            "control": "select",
            "dict": {
              "dictType": "PL_E_PAY_PRIORITY",
              "dictKey": [
                "PL_E_PAY_PRIORITY"
              ],
              "format": "label"
            }
          },
          "wthr_sign": {
            "label": "是否签约",
            "control": "select",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          },
          "remark": {
            "label": "备注",
            "control": "textarea",
            "span": 24,
            "autosize": {
              "minRows": 3,
              "maxRows": 5
            },
            "maxlength": 100,
            "showWordLimit": true
          },
          "toDbsx": {
            "span": 24,
            "unFieldList": [
                "apply_type",
                "busi_type"
            ],
            "control": "dynamic",
            "#isShowChildrenForm": false,
            "toRequest": {
                "method": "GET",
                "url": "@FlowSubmitBtn",
                "params": {
                    "appId": "plOnl",
                    "servicecode": "pl0002",
                    "txn_type": "INSERT",
                    "finalUrl": "views/pl/workbench/workbench_wait_todo_view.json",
                    "trans_type": "ADD"
                }
            }
          }
        }
      ]
    }
  }
}
