{
    "module": "v-realonlyForm-btn",
    "layout": {
        "form": {
            "name": "detail",
            "initRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "pl0005",
                    "change_num": "${{params.change_num}}"
                }
            },
            "controls": [{
                "loan_no": {
                    "label": "借据号",
                    "control": "string",
                    "rules": [{
                        "required": true,
                        "message": "请输入借据号"
                    }]
                },
                "prod_no": {
                    "label": "贷款产品",
                    "control": "string",
                    "rules": [{
                        "required": true,
                        "message": "请输入贷款产品"
                    }]
                },
                "change_befr_acct": {
                    "label": "账号(原)",
                    "control": "string",
                    "rules": [{
                        "required": true,
                        "message": "请输入原账号"
                    }]
                },
                "change_aft_acct": {
                    "label": "账号(新)",
                    "control": "string",
                    "rules": [{
                        "required": true,
                        "message": "请输入变更后账号"
                    }]
                },
                "change_befr_sbbr": {
                    "label": "所在支行(原)",
                    "control": "hidden"
                },
                "change_aft_sbbr": {
                    "label": "所在支行(新)",
                    "control": "hidden"
                },
                "change_befr_acct_nm": {
                    "label": "账户名称(原)",
                    "control": "string",
                    "rules": [{
                        "required": true,
                        "message": "请输入原账户名称"
                    }]
                },
                "change_aft_acct_nm": {
                    "label": "账户名称(新)",
                    "control": "string",
                    "rules": [{
                        "required": true,
                        "message": "请输入变更后账户名称"
                    }]
                },
                "change_befr_acct_tp": {
                    "label": "账户类型(原)",
                    "control": "string",
                    "dict": {
                        "dictType": "PL_E_PAYACT",
                        "dictKey": [
                            "PL_E_PAYACT"
                        ],
                        "format": "label"
                    }
                },
                "change_aft_acct_tp": {
                    "label": "账户类型(新)",
                    "control": "string",
                    "dict": {
                        "dictType": "PL_E_PAYACT",
                        "dictKey": [
                            "PL_E_PAYACT"
                        ],
                        "format": "label"
                    }
                },
                "ccy_code": {
                    "label": "币种",
                    "control": "string",
                    "dict": {
                        "dictType": "MS_CURRENCY",
                        "dictKey": [
                            "MS_CURRENCY"
                        ],
                        "format": "label"
                    }
                },
                "auto_pymt_tp": {
                    "label": "自动付款类型",
                    "control": "string",
                    "dict": {
                        "dictType": "PL_E_AUTO_PAY_TYPE",
                        "dictKey": [
                            "PL_E_AUTO_PAY_TYPE"
                        ],
                        "format": "label"
                    }
                },
                "deal_dt": {
                    "label": "处理日期",
                    "control": "string",
                    "rules": [{
                        "required": true,
                        "message": "请输入处理日期"
                    }]
                },
                "matu_dt": {
                    "label": "到期日",
                    "control": "string",
                    "rules": [{
                        "required": true,
                        "message": "请输入到期日"
                    }]
                },
                "pay_prty": {
                    "label": "支付优先级",
                    "control": "string",
                    "dict": {
                        "dictType": "PL_E_PAY_PRIORITY",
                        "dictKey": [
                            "PL_E_PAY_PRIORITY"
                        ],
                        "format": "label"
                    }
                },
                "wthr_sign": {
                    "label": "是否签约",
                    "control": "string",
                    "dict": {
                        "dictType": "MS_E_YESORNO",
                        "dictKey": [
                            "MS_E_YESORNO"
                        ],
                        "format": "label"
                    }
                },
                "remark": {
                    "label": "备注",
                    "disabled": true,
                    "control": "textarea",
                    "span": 24,
                    "autosize": {
                        "minRows": 3,
                        "maxRows": 5
                    },
                    "maxlength": 100,
                    "showWordLimit": true
                }
            }]
        }
    }
}
