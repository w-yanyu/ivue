{
  "module": "v-form-btn-dynamic",
  "layout": {
    "form": {
      "name":"detail",
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pl0021",
          "transfer_id": "${{params.transfer_id}}"
        }
      },
      
      "controls": [
        {
          "transfer_id": {
            "control": "textarea",
            "disabled": true,
            "label": "移交编码",
            "width": "130px",
            "span":"10"
          },
          "operate_date": {
            "control": "textarea",
            "disabled": true,
            "label": "操作日期",
            "width": "130px",
            "span":"10"
          },
          "user_id": {
            "control": "textarea",
            "disabled": true,
            "label": "操作员编号",
            "width": "130px",
            "span":"10"
          },
          "transfer_type": {
            "control": "select",
            "disabled": true,
            "label": "移交类型",
            "width": "130px",
            "span":"10",
            "dict": {
              "dictType": "BPM_E_TRANS_TYPE",
              "dictKey": [
                "BPM_E_TRANS_TYPE"
              ]
            }
          },
          "transfer_no": {
            "control": "textarea",
            "disabled": true,
            "label": "业务编号",
            "width": "130px",
            "span":"10"
          },
          "old_usr_cd": {
            "control": "textarea",
            "disabled": true,
            "label": "原客户经理编号",
            "width": "130px",
            "span":"10"
          },
          "old_usr_name": {
            "control": "textarea",
            "disabled": true,
            "label": "原客户经理名称",
            "width": "130px",
            "span":"10"
          },
          "new_usr_cd": {
            "control": "textarea",
            "disabled": true,
            "label": "新客户经理编号",
            "width": "130px",
            "span":"10"
          },
          "new_usr_name": {
            "control": "textarea",
            "disabled": true,
            "label": "新客户经理名称",
            "width": "130px",
            "span":"10"
          },
          "old_brch_cd": {
            "control": "textarea",
            "disabled": true,
            "label": "原机构号",
            "width": "130px",
            "span":"10"
          },
          "new_brch_cd": {
            "control": "textarea",
            "disabled": true,
            "label": "新机构号",
            "width": "130px",
            "span":"10"
          }
         
        }
      ]
    }
  }
}

