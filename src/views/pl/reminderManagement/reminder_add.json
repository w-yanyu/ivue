{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
          "scope":"strForm",
          "initRequest": "${{params}}",
          "doRequest": {
            "url": "/SUMP/call/RPCCall",
            "method": "POST",
            "params": {
              "servicecode": "pl0300",
              "operater_ind": "M"
            }
          },
            "btns": [
                {
                    "type": "submit",
                    "label": "确认",
                    "icon": "",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "pl0300"
                        }
                    }
                }
            ],
            "controls": [
                {
                  "lmt_num": {
                    "control": "text",
                    "span": 10,
                    "label": "额度编号",
                    "rules": [{
                      "required": true,
                      "message": "请选择"
                    }]
                  },
                  "cust_name": {
                    "control": "text",
                    "span": 10,
                    "label": "客户名称",
                    "rules": [{
                      "required": true,
                      "message": "请选择"
                    }]
                  }
                },
              {
                "cert_no": {
                  "control": "text",
                  "span": 10,
                  "label": "证件号码",
                  "rules": [{
                    "required": true,
                    "message": "请选择"
                  }]
                },
                "cert_type": {
                  "control": "select",
                  "span": 10,
                  "label": "证件类型",
                  "dict": {
                    "dictType": "MS_E_CRET_TYPE",
                    "dictKey": [
                      "MS_E_CRET_TYPE"
                    ],
                    "format": "label"
                  },
                  "rules": [{
                    "required": true,
                    "message": "请选择"
                  }]
                }
              },
              {
                "commission_company": {
                  "control": "text",
                  "span": 10,
                  "label": "委托公司名称",
                  "rules": [{
                    "required": true,
                    "message": "请选择"
                  }]
                },
                "urge_collt_time": {
                  "control": "dateTimePicker",
                  "span": 10,
                  "label": "催收时间",
                  "placeholder": "请选择",
                  "format": "yyyyMMdd",
                  "rules": [{
                    "required": true,
                    "message": "请选择"
                  }]
                }
              },
              {
                "urge_collt_type": {
                  "control": "select",
                  "span": 10,
                  "label": "催收方式",
                  "dict": {
                    "dictType": "PL_E_URG_COLLT",
                    "dictKey": [
                      "PL_E_URG_COLLT"
                    ],
                    "format": "label"
                  },
                  "rules": [{
                    "required": true,
                    "message": "请选择"
                  }]
                },
                "contacter": {
                  "control": "text",
                  "span": 10,
                  "label": "接洽人",
                  "rules": [{
                    "required": true,
                    "message": "请选择"
                  }]
                }
              },
              {
                "contact_info": {
                  "control": "select",
                  "span": 10,
                  "label": "联系状态",
                  "dict": {
                    "dictType": "PL_E_CONTACT_INFO",
                    "dictKey": [
                      "PL_E_CONTACT_INFO"
                    ],
                    "format": "label"
                  },
                  "rules": [{
                    "required": true,
                    "message": "请选择"
                  }]
                },
                "repay_aspir": {
                  "control": "select",
                  "span": 10,
                  "label": "还款意愿",
                  "dict": {
                    "dictType": "PL_E_REPAY_ASPIR",
                    "dictKey": [
                      "PL_E_REPAY_ASPIR"
                    ],
                    "format": "label"
                  },
                  "rules": [{
                    "required": true,
                    "message": "请选择"
                  }]
                }
              },
              {
                "repay_capacity": {
                  "control": "select",
                  "span": 10,
                  "label": "还款能力",
                  "dict": {
                    "dictType": "PL_E_REPAY_CAPACITY",
                    "dictKey": [
                      "PL_E_REPAY_CAPACITY"
                    ],
                    "format": "label"
                  },
                  "rules": [{
                    "required": true,
                    "message": "请选择"
                  }]
                },
                "cust_feat": {
                  "control": "select",
                  "span": 10,
                  "label": "客群特征",
                  "dict": {
                    "dictType": "PL_E_CUST_FEAT",
                    "dictKey": [
                      "PL_E_CUST_FEAT"
                    ],
                    "format": "label"
                  },
                  "rules": [{
                    "required": true,
                    "message": "请选择"
                  }]
                }
              },
              {
                "remark": {
                  "control": "textarea",
                  "span": 10,
                  "label": "备注",
                  "rules": [{
                    "required": true,
                    "message": "请选择"
                  }]
                }
              }
            ]
        }
    }
}
