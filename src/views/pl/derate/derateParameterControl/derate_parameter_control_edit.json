{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "controls": [
        {
          "acct_age": {
            "control": "select",
            "label": "账龄",
            "width": "130px",
            "dict": {
                "dictType": "PL_E_REDUPARA",
                "dictKey": [
                    "PL_E_REDUPARA"
                ],
                "format": "label"
            },
            "rules": [
                {
                    "required": true,
                    "message": "请选择账龄"
                }
            ]
          },
          "prod_broad_heading": {
            "control": "select",
            "label": "产品大类",
            "width": "130px",
            "dict": {
                "dictType": "PL_E_PRODDP",
                "dictKey": [
                    "PL_E_PRODDP"
                ],
                "format": "label"
            },
            "rules": [
                {
                    "required": true,
                    "message": "请选择产品大类"
                }
            ]
          }
        },
        {
          "dert_int_upr_lmt": {
            "label": "减免利息上限(%)",
            "control": "inputNumber",
            "value": "${{params.redu_Inte_max}}",
            "min": 0,
            "max": 100,
            "controlsPosition": "right",
            "rules": [
              {
                "required": true,
                "message": "请输入减免利息上限"
              }
            ]
          },
          "dert_pnly_int_upr_lmt": {
            "label": "减免罚息上限(%)",
            "control": "inputNumber",
            "value": "${{params.redu_pena_Inte_max}}",
            "min": 0,
            "max": 100,
            "controlsPosition": "right",
            "rules": [
              {
                "required": true,
                "message": "请输入减免罚息上限"
              }
            ]
          },
          "dert_cntgy_upr_lmt": {
            "label": "减免费用上限(%)",
            "control": "inputNumber",
            "min": 0,
            "max": 100,
            "controlsPosition": "right",
            "rules": [
              {
                "required": true,
                "message": "请输入减免费用上限"
              }
            ]
          }
        }
      ],
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "pl0069",
              "parm_id": "${{params.parm_id}}"
            }
          }
        }
      ]
    }
  }
}