{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pl0070",
          "loan_no": "${{params.loan_no}}"
        }
      },
      "thead": [
        {
          "label": "申请人",
          "align": "center",
          "prop": "cust_name"
        },
        {
          "label": "申请时间",
          "align": "center",
          "prop": "appldt"
        },
        {
          "label": "客户申请减免时间",
          "align": "center",
          "prop": "cust_apply_dert_tm"
        },
        {
          "label": "减免类型",
          "align": "center",
          "prop": "dert_way",
          "dict": {
            "format": "label",
            "dictKey": [
              "PL_E_REMTTP"
            ],
            "dictType": "PL_E_REMTTP"
          }
        },
        {
          "label": "申请原因",
          "align": "center",
          "prop": "aply_adjust_reason",
          "width": 130,
          "dict": {
            "format": "label",
            "dictKey": [
              "E_ADJRS"
            ],
            "dictType": "E_ADJRS"
          }
        },
        {
          "label": "减免总金额",
          "align": "center",
          "prop": "zs_dert_totl_amt"
        },
        {
          "label": "减免违约金",
          "align": "center",
          "prop": "derate_overdue_fine"
        },
        {
          "label": "减免罚息",
          "align": "center",
          "prop": "dert_penalt_inst"
        },
        {
          "label": "减免利息",
          "align": "center",
          "prop": "dert_inst"
        },
        {
          "label": "实际减免金额",
          "align": "center",
          "prop": "zsd_dert_totl_amt"
        }

      ]
    }
  }
}
