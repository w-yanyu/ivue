{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "scope":"strForm",
      "initRequest": "${{params}}",
      "doRequest": {
        "url": "/SUMP/call/RPCCall",
        "method": "POST"
      },
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "pl0355"
            }
          }
        }
      ],
      "controls": [
        {
          "prim_key": {
            "control": "hidden",
            "span": 10,
            "label": "主键",
            "rules": [{
              "required": false,
              "message": "请选择"
            }]
          },
          "archives_level": {
            "control": "select",
            "span": 10,
            "label": "档案等级",
            "dict": {
              "dictType": "PL_E_ARCHIVES_LEVEL",
              "dictKey": [
                "PL_E_ARCHIVES_LEVEL"
              ],
              "format": "label"
            },
            "rules": [{
              "required": true,
              "message": "请选择"
            }]
          },
          "archives_directory": {
            "control": "text",
            "span": 10,
            "label": "档案目录名称",
            "rules": [{
              "required": true,
              "message": "请选择"
            }]
          }
        },
        {
          "registration": {
            "control": "text",
            "span": 10,
            "label": "登记人",
            "rules": [{
              "required": true,
              "message": "请选择"
            }]
          },
          "reg_date": {
            "control": "dateTimePicker",
            "span": 10,
            "label": "登记日期",
            "placeholder": "请选择",
            "format": "yyyyMMdd",
            "rules": [{
              "required": true,
              "message": "请选择"
            }]
          }
        },
        {
          "reg_organization": {
            "control": "text",
            "span": 10,
            "label": "登记机构",
            "rules": [{
              "required": true,
              "message": "请选择"
            }]
          }
        }
      ]
    }
  }
}
