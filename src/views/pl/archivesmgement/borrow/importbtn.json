{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "controls": [
                { "archive_no": {
                    "control": "text",
                    "label": "归档编号",
                    "rules": [
                        {
                            "max": 10,
                            "message": "长度不能超过10个字符"
                        }
                    ]
                },
                "cust_name": {
                    "control": "text",
                    "label": "客户名称",
                    "rules": [
                        {
                            "max": 32,
                            "message": "长度不能超过32个字符"
                        }
                    ]
                }
            }
            ]
        },
        "toolbar": {
            "btns": [
                {
                    "type": "add",
                    "target": "dialog",
                    "label": "确定",
                    "doRequest": {
                        "url": "/SUMP/call/RPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "",
                            "bind_default_menu_ind": "1",
                            "system_id": ""
                        }
                    },
                    "props": {
                        "title": "",
                        "width": "80%"
                    }
                }
            ]
        },
        "datagrid": {
            "isInitSearch": false,
            "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "POST",
                "params": {
                    "servicecode": ""
                }
            },
            "pagination": {
                "show": true
            },
            "thead": [
                {
                    "type": "index"
                },
                {
                    "label": "客户名称",
                    "prop": "cust_name"
                },
                {
                    "label": "归档编号",
                    "prop": "archive_no"
                },
                {
                    "label": "上架人员",
                    "prop": "shelf_person"
                },
                {
                    "label": "上架时间",
                    "prop": "shelf_date"
                },
                {
                    "label": "上架机构",
                    "prop": "shelf_org"
                }
            ]
        }
    }
}
