                  "loan_no": {
                    "control": "text",
                    "label": "借据编号",
                    "width": "100px",
                    "rules": [
                        {
                            "required": true,
                            "message": "借据编号不能为空"
                        },
                        {
                            "max": 64,
                            "message": "长度不能超过64个字符"
                        }
                    ]
