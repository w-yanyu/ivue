                        {
                            "max": 64,
                            "message": "长度不能超过64个字符"
                        }
                    ]
                  },
                  "archive_org": {
                    "control": "text",
                    "label": "归档机构",
                    "width": "100px"
                  },
                  "enrollment_type": {
                    "control": "text",
                    "label": "入册类型",
                    "width": "100px",
                    "rules": [
                        {
                            "required": true,
                            "message": "入册类型不能为空"
                        },
                        {
                            "max": 64,
                            "message": "长度不能超过64个字符"
