                  "archive_date": {
                    "control": "text",
                    "label": "归档日期",
                    "width": "100px"
                  },
                  "archive_no": {
                    "control": "text",
                    "label": "归档编号",
                    "width": "100px",
                    "rules": [
                        {
                            "required": true,
                            "message": "归档编号不能为空"
