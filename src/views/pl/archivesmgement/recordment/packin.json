                            }
                        ]
                    },
                  "busi_amt": {
                    "control": "text",
                    "label": "业务金额",
                    "width": "100px",
                    "rules": [
                        {
                            "required": true,
                            "message": "业务金额不能为空"
                        },
                        {
                            "max": 64,
                            "message": "长度不能超过64个字符"
                        }
                    ]
                  },
                  "busi_start": {
                    "control": "text",
                    "label": "业务起始日",
                    "width": "100px",
                    "rules": [
                        {
                            "required": true,
                            "message": "业务起始日不能为空"
                        },
                        {
                            "max": 64,
                            "message": "长度不能超过64个字符"
                        }
                    ]
