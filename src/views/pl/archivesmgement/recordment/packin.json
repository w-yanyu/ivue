{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
          "initRequest": "${{params}}",
            "btns": [
                {
                    "type": "submit",
                    "label": "保存",
                    "icon": "",
                    "doRequest": {
                        "url": "/SUMP/call/RPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "",
                            "bind_default_menu_ind": "1",
                            "system_id": ""
                        }
                    }
                },
                {
                    "type": "submit",
                    "label": "保存并提交",
                    "icon": "",
                    "doRequest": {
                        "url": "/SUMP/call/RPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "",
                            "bind_default_menu_ind": "1",
                            "system_id": ""
                        }
                    }
                }
            ],
            "controls": [
                {
                    "cust_name": {
                        "control": "text",
                        "label": "客户名称",
                        "width": "100px",
                        "prop":"",
                        "rules": [
                            {
                                "required": true,
                                "message": "客户名称不能为空"
                            },
                            {
                                "max": 64,
                                "message": "长度不能超过64个字符"
                            }
                        ]
                    },
                    "busi_type": {
                        "control": "text",
                        "label": "业务品种",
                        "width": "100px",
                        "rules": [
                            {
                                "required": true,
                                "message": "业务品种不能为空"
