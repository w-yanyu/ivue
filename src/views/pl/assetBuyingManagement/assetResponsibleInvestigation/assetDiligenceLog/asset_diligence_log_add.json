{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "pl0428",
              "diligence_no": "${{params.diligence_no}}",
              "page_id": "/views/pl/assetBuyingManagement/assetResponsibleInvestigation/assetDiligenceLog/asset_diligence_log_add.json"
            }
          }
        }
      ],
      "controls": [
        {
          "diligence_no": {
            "control": "hidden",
            "label": "尽调编号",
            "disabled": true
          },
          "start_date": {
            "label": "开始日期",
            "control": "dateTimePicker",
            "rules": [
              {
                "max": "10",
                "message": "长度不能超过10个字符"
              },
              {
                "required": true,
                "message": "请输入开始日期"
              }
            ]
          },
          "end_date": {
            "label": "结束日期",
            "control": "dateTimePicker",
            "rules": [
              {
                "max": "10",
                "message": "长度不能超过10个字符"
              },
              {
                "required": true,
                "message": "请输入结束日期"
              }
            ]
          },
          "work_summarize": {
            "control": "textarea",
            "label": "工作概述",
            "align": "center",
            "span": "20",
            "rows": 1,
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          },
          "work_content": {
            "control": "textarea",
            "label": "工作内容",
            "align": "center",
            "span": "20",
            "rows": 10,
            "rules": [
              {
                "required": true,
                "message": "请输入"
              }
            ]
          }
        }
      ]
    }
  }
}