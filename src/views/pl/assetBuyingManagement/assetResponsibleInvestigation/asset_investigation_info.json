{
  "module": "v-realonlyForm-btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "controls": [
        {
          "package_no": {
            "control": "remoteSelect",
            "label": "资产包名称",
            "disabled": true,
            "labelField": "package_name",
            "valueField": "package_no",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "length": 999,
                "start": 1,
                "servicecode": "pl0423"
              }
            },
            "format": "label"
          },
          "diligence_no": {
            "control": "string",
            "label": "尽调编号"
          },
          "diligence_mechanism": {
            "control": "string",
            "label": "尽调机构"
          },
          "project_manager": {
            "control": "string",
            "label": "项目经理"
          },
          "department": {
            "control": "remoteSelect",
            "label": "部门名称",
            "disabled": true,
            "labelField": "branch_name",
            "valueField": "branch_id",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2173"
              }
            },
            "format": "label"
          },
          "diligence_base_date": {
            "label": "尽调基准日",
            "control": "string"
        },
          "diligence_conclusion": {
            "control": "textarea",
            "label": "尽调结论",
            "align": "center",
            "disabled": true,
            "span": "20",
            "rows": 6
          },
          "remark": {
            "control": "textarea",
            "label": "备注信息",
            "align": "center",
            "disabled": true,
            "span": "20",
            "rows": 6
          } 
        }
      ]
    }
  }
}