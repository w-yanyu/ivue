{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "isInitSearch": true,
        "doRequest": {
          "url": "/SUMP/icmcall/icmRPCCall",
          "method": "POST",
          "params": {
            "length": 999,
            "asset_package_type": "1",
            "servicecode": "pl0440",
            "package_no": "${{params.package_no}}",
            "package_name": "${{params.package_name}}",
            "asset_no": "${{params.asset_no}}",
            "out_part_asset_no": "${{params.out_part_asset_no}}",
            "contract_no": "${{params.contract_no}}",
            "cust_no": "${{params.cust_no}}",
            "loan_no": "${{params.loan_no}}",
            "cust_name": "${{params.cust_name}}"
          }
        },
      "btns": [
        {
          "type": "close",
          "label": "取消"
        },
        {
          "type": "submit",
          "icon": "el-icon-check",
          "label": "确定",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "length": 999,
              "asset_package_type": "1",
              "servicecode": "pl0442",
              "package_no": "${{params.package_no}}",
              "package_name": "${{params.package_name}}",
              "asset_no": "${{params.asset_no}}",
              "out_part_asset_no": "${{params.out_part_asset_no}}",
              "contract_no": "${{params.contract_no}}",
              "cust_no": "${{params.cust_no}}",
              "loan_no": "${{params.loan_no}}",
              "cust_name": "${{params.cust_name}}"
            }
          }
        }
      ],
      "controls": [
        {
          "risk_model": {
            "control": "select",
            "label": "风控模型",
            "rules": [
              {
                "required": true,
                "message": "请选择"
              }
            ],
            "dict": {
              "dictType": "PL_E_RISK_MODEL",
              "dictKey": [
                "PL_E_RISK_MODEL"
              ],
              "format": "label"
            }
          }
        }
      ]
    }
  }
}