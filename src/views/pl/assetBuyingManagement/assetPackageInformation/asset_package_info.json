{
  "module": "v-realonlyForm-btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "controls": [
        {
          "package_name": {
            "control": "string",
            "label": "资产包名称"
          },
          "package_no": {
            "control": "string",
            "label": "资产包编码"
          }
        },
        {
          "bench_mark_date": {
            "control": "string",
            "label": "债权基准日"
          },
          "asset_package_cost": {
            "control": "string",
            "label": "资产包成本",
            "format": {
              "thousand": ","
            }
          }
        },
        {
          "department": {
            "control": "remoteSelect",
            "disabled": true,
            "label": "所在部门",
            "labelField": "branch_name",
            "valueField": "branch_id",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ct2173"
              }
            },
            "format": "label"
          },
          "asset_source": {
            "control": "string",
            "label": "资产来源",
            "dict": {
              "dictType": "PL_E_ASSET_SOURCE",
              "dictKey": [
                "PL_E_ASSET_SOURCE"
              ],
              "format": "label"
            }
          }
        },
        {
          "source_asset_package_no": {
            "control": "string",
            "label": "来源方资产包编号"
          },
          "asset_package_type": {
            "control": "string",
            "label": "资产包类型",
            "dict": {
              "dictType": "PL_E_ASSET_PACKAGE_TYPE",
              "dictKey": [
                "PL_E_ASSET_PACKAGE_TYPE"
              ],
              "format": "label"
            }
          }
        },
        {
          "remark": {
            "control": "textarea",
            "label": "备注",
            "disabled": true,
            "align": "center",
            "span": "20",
            "rows": 6
          } 
        }
      ]
    }
  }
}