{
  "module": "v_search_btn_datagrid",
  "layout": {

    "toolbar": {
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pl0015",
          "apply_no":"${{params.apply_no}}"
        }
      },
      "thead": [
        {
          "type": "index",
          "align": "center"
        },
        {
          "label": "客户编号",
          "prop": "busi_num",
          "align": "center"
        },
        {
          "label": "客户经理名称",
          "prop": "old_usr_nm",
          "align": "center"
        },
        {
          "label": "客户经理编号",
          "prop": "old_usr_num",
          "align": "center"
        },
        {
          "label": "移交后客户经理名称",
          "prop": "new_usr_nm",
          "align": "center"
        },
        {
          "label": "移交后客户经理编号",
          "prop": "new_usr_num",
          "align": "center"
        },
        {
          "label": "原机构",
          "prop": "old_org_num",
          "align": "center"
        },
        {
          "label": "新机构",
          "prop": "new_org_num",
          "align": "center"
        },
        {
          "label": "操作",
          "align":"center",
          "fixed": "right",
          "btns": [
            {
              "type": "delete",
              "label": "删除",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "pl0011"
                }
              }
            }
          ]
        }

      ]
    }
  }
}
