{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "brch_relation_code": {
            "control": "select",
            "label": "机构关系",
            "value": "01",
            "rules": [
              {
                "required": "true",
                "message": "请选择机构关系"
              }
            ],
            "dict": {
              "dictType": "CT_E_BRANCH_RELATION",
              "dictKey": [
                "CT_E_BRANCH_RELATION"
              ],
              "format": "label"
            }
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/rest/auth/userManager",
        "method": "POST",
        "params": {
          "servicecode": "ct2501",
            "role_id": "custmage",
            "appl_id": "${{params.appl_id}}",
            "branch_within":"${{params.branch_within}}"
        }
      },
      "thead": [
        {
          "label": "客户经理编码",
          "prop": "user_id",
          "align": "center"
        },
        {
          "label": "客户经理",
          "prop": "user_name",
          "align": "center"
        },
        {
          "label": "机构编号",
          "prop": "branch_id",
          "align": "center"
        },

        {
          "label": "操作",
          "align": "center",
          "fixed": "right",
          "btns": [{
            "type": "bringback",
            "label": "选择"
          }]
        }
      ]
    }
  }
}
