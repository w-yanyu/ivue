{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "controls": [{
                "cust_name": {
                    "control": "lookup",
                    "edit": true,
                    "label": "客户名称",
                    "span": 10,
                    "keyField": {
                        "cust_name": "cust_name"
                    },
                    "toRequest": {
                        "url": "/views/pl/assetPreserve/assetHndov/useinfo.json",
                        "method": "GET",
                        "params": {
                            "indvfg": "Y",
                            "statss": "Y"
                        }
                    },
                    "events": {
                        "set": {
                            "temp_name": ""
                        }
                    },
                    "format": "label",
                    "rules": [{
                        "required": false,
                        "message": "请选择"
                    }]
                },
                "loan_due_bill_num": {
                    "control": "text",
                    "label": "借据号",
                    "rules": [{
                        "max": 80,
                        "message": "长度不能超过80个字符"
                    }]
                }
            }]
        },
        "toolbar": {
            "btns": [{
                "type": "custom",
                "icon": "el-icon-edit",
                "label": "移出",
               "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "pl0251",
                  "list": "${{current.selectedData}}",
                  "apply_no": "${{params.apply_no}}",
                  "hndov_way":"0"
                }
              },
              "eventAfter": {
                "doParentCallback": true,
                "callback": "addCallback"
              }
            }]
        },
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": true,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "pl0298"
                }
              },
            "thead": [
              {
                "type": "selection",
                "width": "55"
              },
              {
                "type": "index",
                "align": "center"
              },
              {
                "label": "移交编码",
                "prop": "hndov_code",
                "align": "center"
              },
              {
                "label": "借据号",
                "prop": "loan_no",
                "align": "center"
              },
              {
                "label": "币种",
                "prop": "ccy_code",
                "align": "center",
                "dict": {
                  "dictType": "MS_CURRENCY",
                  "dictKey": [
                    "MS_CURRENCY"
                  ],
                  "format": "label"
                }
              },
              {
                "label": "客户名称",
                "prop": "cust_nm",
                "align": "center"
              },
              {
                "label": "还款账号",
                "prop": "rpymt_acct_num",
                "align": "center"
              },
              {
                "label": "贷款期限",
                "prop": "loan_matu",
                "align": "center"
      
              },
              {
                "label": "贷款形态",
                "prop": "loan_form",
                "align": "center",
                "dict": {
                  "dictType": "PL_E_CLSSST",
                  "dictKey": [
                    "PL_E_CLSSST"
                  ],
                  "format": "label"
                }
              },
              {
                "label": "贷款账户状态",
                "prop": "loan_acct_ste",
                "align": "center",
                "dict": {
                  "dictType": "LN_E_LOAN_ACCT_STE",
                  "dictKey": [
                    "LN_E_LOAN_ACCT_STE"
                  ],
                  "format": "label"
                }
              },
              {
                "label": "新机构",
                "prop": "new_org_num",
                "align": "center"
              }
      
            ]
        }
    }
}
