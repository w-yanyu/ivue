{
    "module": "v_search_btn_datagrid",
    "layout": {
      "form": {
        "controls": [
          {
            "cust_no": {
              "control": "text",
              "label": "客户编号",
              "rules": [
                {
                  "max": 80,
                  "message": "长度不能超过80个字符"
                }
              ]
            },
            "cust_name": {
              "control": "text",
              "label": "客户名称",
              "rules": [
                {
                  "max": 80,
                  "message": "长度不能超过80个字符"
                }
              ]
            },
            "cert_type": {
              "control": "select",
              "label": "证件类型",
              "dict": {
                "dictType": "MS_E_CRET_TYPE",
                "dictKey": [
                  "MS_E_CRET_TYPE"
                ],
                "labelField": "dictName",
                "valueField": "dictId"
              }
            },
            "cert_no": {
              "control": "text",
              "label": "证件号码"
            }
          }
        ]
      },
      "datagrid": {
        "scope": "searchTable",
        "isInitSearch": false,
        "doRequest": {
          "url": "/SUMP/icmcall/icmRPCCall",
          "method": "POST",
          "params": {
            "servicecode": "us0008",
            "valid_state":"1"
          }
        },
        "thead": [
          {
            "type": "index",
            "label": "序号"
          },
          {
            "label": "客户号",
            "prop": "cust_no",
            "align": "center"
          },
          {
            "label": "客户名称",
            "prop": "cust_name",
            "align": "center"
          },
          {
            "label": "证件类型",
            "prop": "cert_type",
            "align": "center",
            "dict": {
              "dictType": "MS_E_CRET_TYPE",
              "dictKey": [
                "MS_E_CRET_TYPE"
              ],
              "labelField": "dictName",
              "valueField": "dictId"
            }
          },
          {
            "label": "证件号码",
            "prop": "cert_no",
            "align": "center"
          },
          {
            "label": "操作",
            "align": "center",
            "fixed": "right",
            "btns": [
              {
                "type": "bringback",
                "label": "选择"
              }
            ]
          }
        ]
      }
    }
  }