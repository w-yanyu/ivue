{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "scope": "form",
      "initRequest": "${{params}}",
      "btns": [

      ],
      "controls": [
        {
          "start_oprtr": {
            "label": "申请人",
            "control": "string",
            "value": "$g{{user_info.userCode}}",
            "rules": [
              {
                "required": true,
                "message": "申请人不能为空"
              }
            ]
          },
          "actvn_org": {
            "label": "申请机构",
            "control": "string",
            "value": "$g{{user_info.branch_id}}",
            "rules": [
              {
                "required": true,
                "message": "申请机构不能为空"
              }
            ]
          }
        },
        {
          "cust_no": {
            "label": "抵债人编号",
            "control": "lookup",
            "keyField": {
              "cust_no": "cust_no",
              "cust_nm": "cust_nm"
            },
            "toRequest": {
              "url": "/views/pl/assetPreserve/assetReceive/getUserInfo.json",
              "method": "get",
              "params": {
                "treeTitle": "客户选择"
              }
            },
            "props": {
              "title": "客户选择"
            },
            "rules": [
              {
                "required": true,
                "message": "客户号不得为空"
              }
            ]
          },
          "cust_nm": {
            "label": "抵债人名称",
            "control": "text",
            "disabled": true
          }
        },
        {
          "toDbsx": {
            "span": 24,
            "unFieldList": [
              "apply_type",
              "busi_type"
            ],
            "control": "dynamic",
            "#isShowChildrenForm": false,
            "toRequest": {
              "method": "GET",
              "url": "@FlowSubmitBtn",
              "params": {
                "appId": "plOnl",
                "servicecode": "pl0262",
                "start_oprtr": "$g{{user_info.userCode}}",
                "actvn_org": "$g{{user_info.branch_id}}",
                "cust_no": "${{form.cust_no}}",
                "cust_name": "${{form.cust_nm}}",
                "finalUrl": "views/pl/workbench/workbench_wait_todo_view.json",
                "trans_type": "ADD"
              }
            }
          }
        }

      ]
    }
  }
}
