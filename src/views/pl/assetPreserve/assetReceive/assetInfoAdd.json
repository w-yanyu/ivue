{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "scope": "form",
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pl0284",
          "apply_no":"${{params.apply_no}}"
        }
      },
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pl0269",
          "apply_no":"${{params.apply_no}}"
        }
      },
      "controls": [
        {
          "debt_asset_num": {
            "control": "lookup",
            "label": "抵债资产编号",
            "keyField": {
              "debt_asset_num": "debt_asset_num",
              "cust_nm": "cust_nm",
              "hypth_debt_goods_mterls_class": "hypth_debt_goods_mterls_class",
              "debt_asset_qty": "debt_asset_qty",
              "cstd_debt_asset_amt": "cstd_debt_asset_amt"
            },
            "toRequest": {
              "url": "/views/pl/assetPreserve/assetDeal/dealAssetChoose.json",
              "method": "post",
              "params": {}
            },
            "format": "label",
            "rules": [
              {
                "required": true,
                "message": "请输入资产编号"
              }
            ]
          },
          "cust_nm": {
            "control": "text",
            "label": "客户名称",
            "disabled": true
          }
        },
        {
          "hypth_debt_goods_mterls_class": {
            "control": "select",
            "label": "抵债资产类别",
            "disabled": true,
            "dict": {
              "dictType": "PL_HYPTH_DEBT_GOODS_MTERLS_CLASS",
              "dictKey": [
                "PL_HYPTH_DEBT_GOODS_MTERLS_CLASS"
              ],
              "format": "value-label"
            }

          },
          "debt_asset_qty": {
            "control": "text",
            "label": "抵债资产数量"
          }
        },
        {
          "cstd_debt_asset_amt": {
            "control": "text",
            "label": "代保管抵债资产金额",
            "disabled": true,
            "format":{
              "thousand": ",",
              "decimal": 3
            }
          },
          "wait_deal_debt_asset_amt": {
            "disabled": true,
            "control": "text",
            "label": "待处理抵债资产金额",
            "format":{
              "thousand": ",",
              "decimal": 3
            }
          }
        },
        {
          "fee_rpymt_amt": {
            "control": "text",
            "label": "还费金额",
            "disabled": true,
            "format":{
              "thousand": ",",
              "decimal": 3
            }
          },
          "prcpl_rpymt_amt": {
            "disabled": true,
            "control": "text",
            "label": "还本金额",
            "format":{
              "thousand": ",",
              "decimal": 3
            }
          }
        },
        {
          "int_rpymt_amt": {
            "control": "text",
            "label": "还息金额",
            "disabled": true,
            "format":{
              "thousand": ",",
              "decimal": 3
            }
          },
          "rpymt_amt": {
            "disabled": true,
            "control": "text",
            "label": "还款金额",
            "format":{
              "thousand": ",",
              "decimal": 3
            }
          }
        }
      ]
    }
  }
}
