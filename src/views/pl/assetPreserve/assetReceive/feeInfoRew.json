{
  "module": "v_search_btn_datagrid",
  "layout": {

    "toolbar": {
      "btns": [
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pl0275",
          "apply_no":"${{params.apply_no}}"
        }
      },
      "thead": [
        {
          "label": "费用类别",
          "prop": "fee_tp",
          "align": "center",
          "dict": {
            "dictType": "PL_E_FEE_TP",
            "dictKey": [
              "PL_E_FEE_TP"
            ],
            "format": "value-label"
          }
        },
        {
          "label": "处理日期",
          "prop": "deal_dt",
          "align": "center"
        },
        {
          "label": "费用合计",
          "prop": "sum_fee",
          "align": "center",
          "format":{
            "thousand": ",",
            "decimal": 3
          }
        },
        {
          "label": "费用合计",
          "prop": "sum_fee",
          "align": "center",
          "format":{
            "thousand": ",",
            "decimal": 3
          }
        },
        {
          "label": "费用挂账账号",
          "prop": "fee_susps_acct_num",
          "align": "center"
        },
        {
          "label": "费用挂账账号子序号",
          "prop": "fee_susps_sub_acct_serl_num",
          "align": "center"
        },
        {
          "label": "操作",
          "align":"center",
          "fixed": "right",
          "width": 200,
          "btns": [
            {
              "type": "look",
              "label": "详情",
              "toRequest": {
                "url": "/views/pl/assetPreserve/assetReceive/feeInfoDetail.json",
                "method": "Get"
              },
              "props": {
                "width": "65%",
                "title":  "详情"
              }
            }
          ]
        }

      ]
    }
  }
}
