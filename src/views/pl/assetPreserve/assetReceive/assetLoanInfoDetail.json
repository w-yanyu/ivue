{
  "module": "v_realonlyForm_btn",
  "layout": {
    "form": {
      "scope": "form",
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pl0289",
          "apply_no":"${{params.apply_no}}"
        }
      },
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pl0266"
        }
      },
      "controls": [
        {
          "loan_no": {
            "control": "string",
            "label": "借据号"
            },
          "contract_no": {
            "control": "string",
            "label": "合同编号",
            "disabled":true
          }
        },
        {
          "acctst": {
            "control": "currency",
            "label": "抵债时贷款余额",
            "disabled":true,
            "format":{
              "thousand": ",",
              "decimal": 3
            }
          },
          "paydb_prcpl": {
            "control": "currency",
            "disabled":true,
            "label": "抵债本金",
            "format":{
              "thousand": ",",
              "decimal": 3
            }
          }
        },
        {
          "no_pay_prcpl": {
            "control": "currency",
            "disabled":true,
            "label": "未抵偿本金",
            "format":{
              "thousand": ",",
              "decimal": 3
            }
          },
          "db_int": {
            "control": "currency",
            "label": "欠息",
            "disabled":true
          }
        },
        {
          "paydb_int": {
            "control": "currency",
            "label": "抵债利息",
            "disabled":true,

            "format":{
              "thousand": ",",
              "decimal": 3
            }
          },
          "no_pay_int": {
            "control": "currency",
            "disabled":true,
            "label": "未抵偿贷款利息",
            "format":{
              "thousand": ",",
              "decimal": 3
            }
          }
        },
        {
          "ccy_code": {
            "control": "select",
            "label": "币种",
            "disabled": true,
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": [
                "MS_CURRENCY"
              ],
              "format": "label"
            },
            "value": "156"
          }
        },
        {
          "remks": {
            "disabled":true,
            "control": "textarea",
            "label": "备注",
            "span": 24
          }
        }
      ]
    }
  }
}
