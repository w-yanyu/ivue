{
  "module": "v_realonlyForm_btn",
  "layout": {
    "form": {
      "scope": "templateScope",
      "initRequest":  {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pl0289",
          "debt_asset_num": "${{params.asset_no}}"
        }
      },
      "controls": [
        {
          "apply_no": {
            "control": "string",
            "span": 8,
            "label": "申请号"
          },
          "debt_asset_num": {
            "control": "string",
            "span": 8,
            "label": "抵债资产编号"
          },
          "debt_asset_nm": {
            "control": "string",
            "span": 8,
            "label": "抵债资产名称"
          },
          "cust_num": {
            "control": "string",
            "span": 8,
            "label": "客户编号"
          },
          "cust_nm": {
            "control": "string",
            "span": 8,
            "label": "客户名称"
          },
          "hypth_debt_goods_mterls_class": {
            "control": "string",
            "span": 8,
            "label": "抵债物资类别",
            "dict": {
              "dictType": "Pl_E_HYPTH_DEBT_GOODS_MTERLS_CLASS",
              "dictKey": [
                "Pl_E_HYPTH_DEBT_GOODS_MTERLS_CLASS"
              ],
              "format": "value-label"
            }
          },
          "debt_asset_ste": {
            "control": "string",
            "span": 8,
            "label": "抵债资产状态",
            "dict": {
              "dictType": "PL_E_DEBT_ASSET_STE",
              "dictKey": [
                "PL_E_DEBT_ASSET_STE"
              ],
              "format": "value-label"

            }
          },
          "eqty_right_ctfct_catg": {
            "control": "string",
            "span": 8,
            "label": "产权证明种类",
            "dict": {
              "dictType": "Pl_E_EQTY_RIGHT_CTFCT_CATG",
              "dictKey": [
                "Pl_E_EQTY_RIGHT_CTFCT_CATG"
              ],
              "format": "value-label"

            }
          },
          "ccy_code_num": {
            "control": "string",
            "span": 8,
            "label": "货币代号",
            "dict": {
              "dictType": "MS_CURRENCY",
              "dictKey": [
                "MS_CURRENCY"
              ],
              "format": "label"
            }
          },
          "wait_trfr_debt_asset": {
            "control": "string",
            "span": 8,
            "label": "待转抵债资产"
          },
          "alrdy_dsps_debt_asset": {
            "control": "string",
            "span": 8,
            "label": "已处置抵债资产"
          },
          "wait_deal_debt_asset_amt": {
            "control": "string",
            "span": 8,
            "label": "待处理抵债资产金额"
          },
          "debt_asset_unit": {
            "control": "string",
            "span": 8,
            "label": "抵债资产单位"
          },
          "debt_asset_qty": {
            "control": "string",
            "span": 8,
            "label": "抵债资产数量"
          },
          "evlan_val": {
            "control": "string",
            "span": 8,
            "label": "评估价值"
          },
          "debt_asset_pstg_val": {
            "control": "string",
            "span": 8,
            "label": "抵债资产入账价值"
          },
          "abst_dsc": {
            "control": "string",
            "span": 8,
            "label": "摘要描述"
          },
          "remks": {
            "control": "string",
            "span": 8,
            "label": "备注"
          }


        }
      ]
    }
  }
}
