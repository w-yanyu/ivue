{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "scope": "form",
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pl0272",
          "apply_no":"${{params.apply_no}}"
        }
      },
      "controls": [
        {
          "fee_tp": {
            "control": "select",
            "span": 12,
            "label": "费用类型",
            "dict": {
              "dictType": "PL_E_FEE_TP",
              "dictKey": [
                "PL_E_FEE_TP"
              ],
              "format": "value-label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择费用类型"
              }
            ]
          },
          "deal_dt": {
            "control": "dateTimePicker",
            "span": 12,
            "label": "处理日期",
            "rules": [
              {
                "required": true,
                "message": "请输入处理时间"
              }
            ]
          }
        },
        {
          "sum_fee": {
            "control": "currency",
            "label": "费用总和",
            "span": 12,
            "format": {
              "thousand": ",",
              "decimal": 3
            },
            "rules": [
              {
                "required": true,
                "message": "请输入费用总和"
              }
            ]
          },
          "fee_susps_acct_num": {
            "control": "text",
            "span": 12,
            "label": "费用挂账账号",
            "rules": [
              {
                "required": true,
                "message": "请输入费用挂账账号"
              }
            ]
          }
        },
        {
          "fee_susps_sub_acct_serl_num": {
            "control": "text",
            "span": 12,
            "label": "费用挂账账号子序号",
            "rules": [
              {
                "required": true,
                "message": "请输入费用挂账账号子序号"
              }
            ]
          }
        },
        {
          "remark": {
            "control": "textarea",
            "label": "备注",
            "span": 24
          }
        }
      ]
    }
  }
}
