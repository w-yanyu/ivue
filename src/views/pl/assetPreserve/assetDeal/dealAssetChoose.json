{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "debt_asset_num": {
            "control": "text",
            "label": "资产编号",
            "rules": [
              {
                "max": 80,
                "message": "长度不能超过80个字符"
              }
            ]
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pl0294",
          "deal_flag":"0"
        }
      },
      "thead": [
        {
          "label": "资产编号",
          "prop": "debt_asset_num",
          "align": "center"
        },
        {
          "label": "资产名称",
          "prop": "debt_asset_nm",
          "align": "center"
        },
        {
          "label": "客户名称",
          "prop": "cust_nm",
          "align": "center"
        },
        {
          "label": "抵债物资类别",
          "prop": "hypth_debt_goods_mterls_class",
          "align": "center",
          "dict": {
            "dictType": "PL_E_HYPTH_DEBT_GOODS_MTERLS_CLASS",
            "dictKey": [
              "PL_E_HYPTH_DEBT_GOODS_MTERLS_CLASS"
            ],
            "format": "value-label"
          }
        },
        {
          "label": "抵债资产数量",
          "prop": "debt_asset_qty",
          "align": "center"
        },
        {
          "label": "代保管抵债资产金额",
          "prop": "wait_trfr_debt_asset",
          "align": "center",
          "format":{
            "thousand": ",",
            "decimal": 3
          }

        },
        {
          "label": "待处理抵债资产",
          "prop": "wait_deal_debt_asset_amt",
          "align": "center",
          "format":{
            "thousand": ",",
            "decimal": 3
          }

        },
        {
          "label": "货币代号",
          "prop": "ccy_code_num",
          "align": "center",
          "dict": {
            "dictType": "MS_CURRENCY",
            "dictKey": [
              "MS_CURRENCY"
            ],
            "format": "value-label"
          }
        },
        {
          "label": "操作",
          "align":"center",
          "fixed": "right",
          "width": 200,
          "btns": [
            {
              "type": "bringback",
              "label": "选择"
            }
          ]
        }

      ]
    }
  }
}
