{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "scope": "form",
      "initRequest": "",
      "btns": [],
      "controls": [
        {
          "start_oprtr": {
            "label": "申请人",
            "control": "string",
            "value": "$g{{user_info.userCode}}",
            "rules": [
              {
                "required": true,
                "message": "申请人不能为空"
              }
            ]
          },
          "actvn_org": {
            "label": "申请机构",
            "control": "string",
            "value": "$g{{user_info.branch_id}}",
            "rules": [
              {
                "required": true,
                "message": "申请机构不能为空"
              }
            ]
          }
        },
        {
          "debt_asset_num": {
            "control": "lookup",
            "label": "抵债资产编号",
            "keyField": {
              "debt_asset_num": "debt_asset_num",
              "debt_asset_nm": "debt_asset_nm",
              "cust_nm": "cust_nm",
              "cust_num":"cust_num",
              "hypth_debt_goods_mterls_class": "hypth_debt_goods_mterls_class",
              "debt_asset_qty": "debt_asset_qty",
              "wait_trfr_debt_asset": "wait_trfr_debt_asset",
              "sum_deal_debt_asset_amt": "wait_deal_debt_asset_amt"
            },
            "toRequest": {
              "url": "/views/pl/assetPreserve/assetDeal/dealAssetChoose.json",
              "method": "post",
              "params": {}
            },
            "format": "label",
            "rules": [
              {
                "required": true,
                "message": "请输入资产编号"
              }
            ]
          },
          "cust_nm": {
            "control": "text",
            "label": "资产归属客户",
            "disabled": true
          }
        },
        {

          "hypth_debt_goods_mterls_class": {
            "control": "select",
            "label": "抵债资产类别",
            "disabled": true,
            "dict": {
              "dictType": "Pl_E_HYPTH_DEBT_GOODS_MTERLS_CLASS",
              "dictKey": [
                "Pl_E_HYPTH_DEBT_GOODS_MTERLS_CLASS"
              ],
              "format": "value-label"
            }

          },
          "cust_num": {
            "control": "hidden",
            "label": "资产归属客户编号",
            "disabled": true
          },
          "debt_asset_nm": {
            "control": "hidden",
            "label": "抵债资产名称",
            "disabled": true
          },
          "wait_trfr_debt_asset": {
            "control": "text",
            "label": "代保管抵债资产金额",
            "disabled": true,
            "format":{
              "thousand": ",",
              "decimal": 3
            }
          }
        },
        {
          "debt_asset_displ_catg": {
            "control": "select",
            "label": "抵债资产处置种类",
            "dict": {
              "dictType": "PL_E_DISPL_CATG",
              "dictKey": [
                "PL_E_DISPL_CATG"
              ],
              "format": "value-label"
            }

          },
          "sum_deal_debt_asset_amt": {
            "control": "text",
            "label": "待处理抵债资产金额",
            "format":{
              "thousand": ",",
              "decimal": 3
            }
          }
        },
        {

          "wait_deal_debt_asset_amt": {
            "control": "text",
            "label": "本次处置抵债资产金额",
            "format":{
              "thousand": ",",
              "decimal": 3
            },
            "rules": [
              {
                "required": true,
                "message": "请输入处置变现金额"
              }]
          },
          "wait_rlzn_debt_asset_amt": {
            "control": "text",
            "label": "待变现金额",
            "format":{
              "thousand": ",",
              "decimal": 3
            },
            "rules": [
              {
                "required": true,
                "message": "请输入处置变现金额"
              }]
          }
        },
        {
          "debt_asset_rlzn_acct_num": {
            "control": "text",
            "label": "抵债资产变现账号",
            "rules": [
              {
                "required": true,
                "message": "请输入抵债资产变现账号"
              }]
          },
          "da_rlzn_sub_acct_serl_num": {
            "control": "text",
            "label": "抵债资产变现子账户序号",
            "rules": [
              {
                "required": true,
                "message": "请输入抵债资产变现子账户序号"
              }]
          }
        },
        {
          "debt_asset_displ_cmplty_flg": {
            "control": "select",
            "label": "抵债资产处置完毕标志",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "value-label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择抵债资产处置完毕标志"
              }]
          },
          "atchmt_file": {
            "control": "hidden",
            "label": "file_path"
          },
          "file": {
            "doRequest": {
              "url": "/SUMP/minioCall/uploadAndCheck",
              "method": "post",
              "params": {
              },
              "resultSet": {
                "atchmt_file": "path",
                "file_name": "oldName",
                "file_size": "size"
              }
            },
            "width": "240px",
            "control": "file",
            "limit": 1,
            "limitSize": 10,
            "label": "上传文件",
            "btnLable": "请选择文件"
          }
        },
        {
          "remark": {
            "control": "textarea",
            "span": "24",
            "label": "备注"
          }
        },
        {
          "toDbsx": {
            "span": 24,
            "unFieldList": [
              "apply_type",
              "busi_type"
            ],
            "control": "dynamic",

            "#isShowChildrenForm": false,
            "toRequest": {
              "method": "GET",
              "url": "@FlowSubmitBtn",
              "params": {
                "appId": "plOnl",
                "servicecode": "pl0278",
                "start_oprtr": "$g{{user_info.userCode}}",
                "actvn_org": "$g{{user_info.branch_id}}",
                "cust_no": "${{form.cust_no}}",
                "cust_name": "${{form.cust_nm}}",
                "finalUrl": "views/pl/workbench/workbench_wait_todo_view.json",
                "trans_type": "ADD"
              }
            }
          }
        }
      ]
    }
  }
}
