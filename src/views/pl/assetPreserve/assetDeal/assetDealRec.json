{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "asset_no": {
            "control": "text",
            "label": "资产编号"
          },
          "apply_no": {
            "control": "text",
            "label": "申请编号"
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pl0282"
        }
      },
      "thead": [
        {
          "label": "申请编号",
          "prop": "apply_no",
          "align": "center"
        },
        {
          "label": "资产编号",
          "prop": "asset_no",
          "align": "center"
        },
        {
          "label": "抵债评估价",
          "prop": "asse_amt",
          "align": "center",
          "format":{
            "thousand": ",",
            "decimal": 3
          }
        },
        {
          "label": "抵债金额",
          "prop": "paydb_amt",
          "align": "center",
          "format":{
            "thousand": ",",
            "decimal": 3
          }
        },
        {
          "label": "抵债资产处置种类",
          "prop": "debt_asset_displ_catg",
          "align": "center",
          "dict": {
            "dictType": "PL_E_DISPL_CATG",
            "dictKey": [
              "PL_E_DISPL_CATG"
            ],
            "format": "value-label"
          }
        },
        {
          "label": "币种",
          "prop": "ccy_code",
          "align": "center",
          "dict": {
            "dictType": "MS_CURRENCY",
            "dictKey": [
              "MS_CURRENCY"
            ],
            "format": "value-label"
          }
        },
        {
          "label": "操作",
          "align":"center",
          "fixed": "right",
          "width": 200,
          "btns": [
            {
              "type": "look",
              "label": "资产详情",
              "toRequest": {
                "url": "/views/pl/assetPreserve/assetReceive/recAssetInfo.json",
                "method": "Get"
              },
              "props": {
                "width": "65%",
                "title":  "信贷业务信息维护"
              }
            }
          ]
        }

      ]
    }
  }
}
