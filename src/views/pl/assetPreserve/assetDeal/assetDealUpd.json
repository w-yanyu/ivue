{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "scope": "form",
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pl0281",
          "apply_no":"${{params.apply_no}}"
        }
      },
      "btns": [{
        "type": "submit",
        "label": "保存",
        "doRequest": {
          "url": "/SUMP/icmcall/icmRPCCall",
          "method": "post",
          "params": {
            "servicecode": "pl0280",
            "apply_no":"${{params.apply_no}}"
          }
        },
        "eventAfter": {
          "doLastCallback": false
        }
      }],
      "controls": [
        {
          "start_oprtr": {
            "label": "申请人",
            "control": "string",
            "value": "$g{{user_info.userCode}}",
            "rules": [
              {
                "required": true,
                "message": "申请人不能为空"
              }
            ]
          },
          "actvn_org": {
            "label": "申请机构",
            "control": "string",
            "value": "$g{{user_info.branch_id}}",
            "rules": [
              {
                "required": true,
                "message": "申请机构不能为空"
              }
            ]
          }
        },
        {
          "debt_asset_num": {
            "control": "lookup",
            "label": "抵债资产编号",
            "keyField": {
              "debt_asset_num": "debt_asset_num",
              "asset_cust_nm": "cust_nm",
              "hypth_debt_goods_mterls_class": "hypth_debt_goods_mterls_class",
              "debt_asset_qty": "debt_asset_qty",
              "wait_trfr_debt_asset": "wait_trfr_debt_asset",
              "sum_deal_debt_asset_amt": "wait_deal_debt_asset_amt"
            },
            "toRequest": {
              "url": "/views/pl/assetPreserve/assetDeal/dealAssetChoose.json",
              "method": "post",
              "params": {}
            },
            "format": "label",
            "rules": [
              {
                "required": true,
                "message": "请输入资产编号"
              }
            ]
          },
          "cust_nm": {
            "control": "text",
            "label": "资产归属客户",
            "disabled": true
          }
        },
        {
          "wait_trfr_debt_asset": {
            "control": "text",
            "label": "代保管抵债资产金额",
            "disabled": true,
            "format":{
              "thousand": ",",
              "decimal": 3
            }
          },
          "debt_asset_displ_catg": {
            "control": "select",
            "label": "抵债资产处置种类",
            "dict": {
              "dictType": "PL_E_DISPL_CATG",
              "dictKey": [
                "PL_E_DISPL_CATG"
              ],
              "format": "value-label"
            }

          }
        },
        {

          "wait_deal_debt_asset_amt": {
            "control": "text",
            "label": "本次处置抵债资产金额",
            "format":{
              "thousand": ",",
              "decimal": 3
            },
            "rules": [
              {
                "required": true,
                "message": "请输入处置变现金额"
              }]
          },
          "wait_rlzn_debt_asset_amt": {
            "control": "text",
            "label": "待变现金额",
            "format":{
              "thousand": ",",
              "decimal": 3
            },
            "rules": [
              {
                "required": true,
                "message": "请输入处置变现金额"
              }]
          }
        },
        {
          "debt_asset_rlzn_acct_num": {
            "control": "text",
            "label": "抵债资产变现账号",
            "rules": [
              {
                "required": true,
                "message": "请输入抵债资产变现账号"
              }]
          },
          "da_rlzn_sub_acct_serl_num": {
            "control": "text",
            "label": "抵债资产变现子账户序号",
            "rules": [
              {
                "required": true,
                "message": "请输入抵债资产变现子账户序号"
              }]
          }
        },
        {
          "debt_asset_displ_cmplty_flg": {
            "control": "select",
            "label": "抵债资产处置完毕标志",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "value-label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择抵债资产处置完毕标志"
              }]
          },
          "atchmt_file": {
            "control": "hidden",
            "label": "file_path"
          },
          "file": {
            "doRequest": {
              "url": "/SUMP/minioCall/upload",
              "method": "post",
              "params": {
              },
              "resultSet": {
                "atchmt_file": "path",
                "file_name": "oldName",
                "file_size": "size"
              }
            },
            "width": "240px",
            "control": "file",
            "limit": 1,
            "limitSize": 10,
            "label": "更换附件",
            "btnLable": "请选择文件"
          }
        },
        {
          "btn1": {
            "control": "button",
            "text": "点击下载",
            "label": "附件下载",
            "type": "export",
            "doRequest": {
              "url": "/SUMP/minioCall/download",
              "method": "GET",
              "params": {
                "path": "${{form.atchmt_file}}",
                "fileName": "文件"
              }
            }
          }
        },
        {
          "remark": {
            "control": "textarea",
            "span": "24",
            "label": "备注"
          }
        }
      ]
    }
  }
}
