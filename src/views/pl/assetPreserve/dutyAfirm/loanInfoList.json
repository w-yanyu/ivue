{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "foldLineNumber": 2,
      "controls": [
        {

          "loan_due_bill_num": {
            "span": 8,
            "control": "text",
            "label": "借据号",
            "rules": [

            ]
          },
          "ctrct_num": {
            "span": 8,
            "control": "text",
            "label": "合同编号",
            "format": "label"
          }
        },
        {
          "cust_num": {
            "span": 8,
            "control": "text",
            "label": "客户号",
            "format": "label"
          }

        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "syln0002",
          "outlistid":"lnCpstnQryType"
        }
      },
      "thead": [
        {
          "label": "序号",
          "type": "index",
          "align": "center"
        },
        {
          "align": "center",
          "label": "客户姓名",
          "width":"100",
          "prop": "cust_nm"
        },
        {
          "align": "center",
          "width":"220",
          "label": "借据号",
          "prop": "loan_due_bill_num"
        },{
          "align": "center",
          "width":"220",
          "label": "合同编号",
          "prop": "ctrct_num"
        },{
          "align": "center",
          "width":"220",
          "label": "客户号",
          "prop": "cust_num"
        },
        {
          "align": "center",
          "label": "可售产品编号",
          "prop": "slbl_prod_code"
        },
        {
          "align": "center",
          "label": "产品名称",
          "width":"120",
          "prop": "prod_nm"
        },
        {
          "align": "center",
          "label": "放款日期",
          "prop": "open_acct_dt"
        },
        {
          "align": "center",
          "label": "到期日期",
          "prop": "matu_dt"
        },
        {
          "align": "center",
          "label": "借据金额(元)",
          "prop": "due_bill_amt",
          "format":{
            "thousand":","
          }
        },
        {
          "align": "center",
          "label": "合同金额(元)",
          "prop": "ctrct_amt",
          "format":{
            "thousand":","
          }
        },
        {
          "align": "center",
          "label": "贷款形态",
          "prop": "loan_form",
          "dict": {
            "dictType": "LN_E_LOAN_FORM",
            "dictKey": [
              "LN_E_LOAN_FORM"
            ],
            "format": "label"
          }
        },
        {
          "align": "center",
          "label": "贷款账户状态",
          "prop": "loan_acct_ste",
          "dict": {
            "dictType": "LN_E_LOAN_ACCT_STE",
            "dictKey": [
              "LN_E_LOAN_ACCT_STE"
            ],
            "format": "label"
          }
        },
        {
          "label": "操作",
          "fixed": "right",
          "align": "center",
          "btns": [
            {
              "type": "look",
              "label": "详情",
              "toRequest": {
                "url": "/views/integrated/loanInfo/loan_info_tabs.json",
                "method": "POST",
                "params": {
                  "loan_due_bill_num":"${{current.loan_due_bill_num}}"
                }
              },
              "props": {
                "width": "80%",
                "title": "借据信息详情"
              }
            }
          ]
        }
      ]
    }
  }
}
