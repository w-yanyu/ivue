{
  "module": "v-form-editableDataGrid-btn",
  "layout": {
    "form": {
      "controls": [
        {
          "id": {
            "control": "text",
            "label": "编号"
          },
          "name": {
            "control": "text",
            "label": "姓名"
          }
        },
        {
          "age": {
            "control": "text",
            "label": "年龄"
          },
          "address": {
            "control": "text",
            "label": "地址"
          }
        }
      ]
    },
    "toolbar": {
      "btns": [
        {
          "type": "add",
          "label": "新增",
          "icon": "el-icon-plus"
        },
        {
          "type": "batchDelete",
          "label": "批量删除",
          "icon": "el-icon-close"
        }
      ]
    },
    "datagrid": {
      "pagination": {
        "show": false
      },
      "thead": [
        {
          "type": "index",
          "label": "序号"
        },
        {
          "prop": "name",
          "type": "text",
          "label": "姓名"
        },
        {
          "prop": "rel",
          "type": "text",
          "label": "关系"
        },
        {
          "prop": "workType",
          "type": "text",
          "label": "工作"
        },
        {
          "prop": "educ",
          "type": "select",
          "label": "学历",
          "options": [
            {
              "value": "1",
              "label": "高中"
            },
            {
              "value": "2",
              "label": "本科"
            },
            {
              "value": "3",
              "label": "硕士"
            },
            {
              "value": "4",
              "label": "博士"
            }
          ]
        },
        {
          "label": "操作",
          "width": "280px",
          "btns": [
            {
              "type": "delete",
              "label": "删除",
              "icon": "el-icon-close"
            }
          ]
        }
      ]
    },
    "footer": {
      "btns": [
        {
          "type": "submit",
          "label": "保存",
          "icon": "el-icon-tickets",
          "doRequest": {
            "url": "/demo/submitData",
            "method": "get"
          }
        }
      ]
    }
  }
}
