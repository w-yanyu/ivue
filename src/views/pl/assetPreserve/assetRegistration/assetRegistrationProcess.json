{
    "module": "v_inputDataForm_btn",
    "layout": {
      "form": {
        "scope": "form",
        "initRequest": {
          "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pl0292",
          "apply_no":"${{params.apply_no}}"
        }
      },
        "btns": [],
        "controls": [
            {
                "cust_num": {
                "label": "客户号",
                "control": "text",
                "disabled": true,
                "rules": [
                  {
                    "required": true,
                    "message": "客户号不能为空"
                  }
                ]
              },
              "cust_nm": {
                "label": "客户名称",
                "control": "text",
                "disabled": true,
                "rules": [
                  {
                    "required": true,
                    "message": "客户姓名不能为空"
                  }
                ]
              }},{
                "debt_asset_num": {
                  "span": 12,
                  "label": "抵债资产编号",
                  "disabled": true,
                  "control": "text"
                  ,
                  "rules": [
                    {
                      "required": true,
                      "message": "资产编号不得为空"
                    }
                  ]
                }
            ,
            "debt_asset_nm": {
              "label": "抵债资产名称",
              "disabled": true,
              "control": "text",
              "span": 12,

            "rules": [
                {
                  "required": true,
                  "message": "资产名称不得为空"
                }
              ]
          }},
          {
            "hypth_debt_goods_mterls_class": {
              "control": "select",
              "label": "抵债资产类型",
              "disabled": true,
              "span": 12,
              "dict": {
                "dictType": "Pl_E_HYPTH_DEBT_GOODS_MTERLS_CLASS",
                "dictKey": [
                  "Pl_E_HYPTH_DEBT_GOODS_MTERLS_CLASS"
                ],
                "format": "value-label"

              },
              "rules": [
                {
                  "required": true,
                  "message": "资产类型不得为空"
                }
              ]
            },
              "eqty_right_ctfct_catg": {
                "control": "select",
                "label": "产权证明种类",
                "disabled": true,
                "span": 12,
                "dict": {
                  "dictType": "Pl_E_EQTY_RIGHT_CTFCT_CATG",
                  "dictKey": [
                    "Pl_E_EQTY_RIGHT_CTFCT_CATG"
                  ],
                  "format": "value-label"

                },
                "rules": [
                    {
                      "required": true,
                      "message": "产权证明种类不得为空"
                    }
                  ]
            }
            },
            {
                "ccy_code_num": {
                  "control": "select",
                  "disabled": true,
                  "label": "币种",
                  "span": 12,
                  "dict": {
                    "dictType": "MS_CURRENCY",
                    "dictKey": [
                      "MS_CURRENCY"
                    ],
                    "format": "value-label"
                    },
                  "rules": [
                    {
                      "required": true,
                      "message": "请选择币种"
                    }
                  ]
                },

            "wait_trfr_debt_asset": {
              "control": "currency",
              "disabled": true,
              "label": "代保管抵债资产金额",
              "span": 12,
              "format":{
                "thousand": ",",
                "decimal": 3
              }
            }},{


          "debt_asset_unit": {
            "label": "抵债资产单位",
            "control": "text",
            "disabled": true,
            "span": 12,

          "rules": [
              {
                "required": true,
                "message": "资产单位不得为空"
              }
            ]
        }
        ,
        "debt_asset_qty": {
            "label": "抵债资产数量",
            "control": "text",
            "disabled": true,
            "span": 12,

          "rules": [
              {
                "required": true,
                "message": "资产单位不得为空"
              }
            ]
        }},{
        "evlan_val": {
            "control": "currency",
            "label": "评估价值",
            "disabled": true,
            "span": 12,
            "format":{
              "thousand": ",",
              "decimal": 3
            },
            "rules": [
                {
                  "required": true,
                  "message": "评估价值不得为空"
                }
              ]
          },
            "atchmt": {
              "control": "hidden"
            },
            "atchmt_file": {
              "control": "button",
              "text": "点击下载",
              "label": "附件下载",
              "type": "export",
              "doRequest": {
                "url": "/SUMP/minioCall/download",
                "method": "GET",
                "params": {
                  "path": "${{form.atchmt}}",
                  "fileName": "文件"
                }
              }
            }
    },{
      "remks": {
        "control": "textarea",
        "disabled": true,
        "span": 12,
        "label": "备注"
      }}




        ]
      }
    }
  }
