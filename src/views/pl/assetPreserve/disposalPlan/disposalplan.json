            "dict": {
                "dictKey": [
                    "PL_E_DISPAL_PLAN"
                ],
              "dictType": "PL_E_DISPAL_PLAN",
              "format": "label"
            }
           }
          }
          
        
  
        ]
      }
    }
  }
  
  