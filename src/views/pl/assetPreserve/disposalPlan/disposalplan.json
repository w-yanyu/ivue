{
    "module": "v_inputDataForm_btn",
    "layout": {
      "form": {
        "scope": "formData",
        "btns": [
          {
            "type": "submit",
            "label": "保存",
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "POST",
              "params": {
                "servicecode": "pl0257",
                "list": "${{params.selectedData}}",
                "apply_no": "${{params.apply_no}}"
              }
            }
          }
        ],
        "controls": [
          {
            "disposal_plan": {
