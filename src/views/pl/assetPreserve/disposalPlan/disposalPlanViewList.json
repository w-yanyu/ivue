{
  "module": "v_search_btn_datagrid",
  "layout": {

    "toolbar": {
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "pl0258",
          "apply_no":"${{params.apply_no}}"
        }
      },
      "thead": [
        {
          "type": "index",
          "align": "center"
        },
        {
          "label": "处置方式",
          "prop": "disposal_plan",
          "align": "center",
            "dict": {
              "dictType": "PL_E_DISPAL_PLAN",
              "dictKey": [
                "PL_E_DISPAL_PLAN"
              ]
            },
            "format": "label"
        },

        {
          "label": "借据号",
          "prop": "loan_no",
          "align": "center"
        },
        {
          "label": "币种",
          "prop": "ccy_code",
          "align": "center",
          "dict": {
            "dictType": "MS_CURRENCY",
            "dictKey": [
                "MS_CURRENCY"
            ]
        }
        },
        {
          "label": "客户编号",
          "prop": "cust_no",
          "align": "center"
        },
        {
          "label": "客户名称",
          "prop": "cust_nm",
          "align": "center"
        },
        {
          "label": "贷款期限",
          "prop": "loan_matu",
          "align": "center"

        },
        {
          "label": "贷款形态",
          "prop": "loan_form",
          "align": "center",
          "dict": {
            "dictType": "PL_E_CLSSST",
            "dictKey": [
              "PL_E_CLSSST"
            ],
            "format": "label"
          }
        },
        {
          "label": "贷款账户状态",
          "prop": "loan_acct_ste",
          "align": "center",
          "dict": {
            "dictType": "LN_E_LOAN_ACCT_STE",
            "dictKey": [
              "LN_E_LOAN_ACCT_STE"
            ],
            "format": "label"
          }
        },
        {
          "label": "新机构",
          "prop": "new_org_num",
          "align": "center"
        },
       {
          "label": "操作",
          "align":"center",
          "fixed": "right",
          "btns": [
            {
              "type": "delete",
              "label": "撤回",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                  "servicecode": "pl0259",
                  "apply_no":"${{params.apply_no}}"
                }
              }
            }
          ]
        }

      ]
    }
  }
}
