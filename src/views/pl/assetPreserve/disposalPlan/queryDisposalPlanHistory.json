{
    "module": "v_search_btn_datagrid",
    "layout": {
      "form": {
        "controls": [
          {
            "apply_no": {
              "control": "text",
              "label": "申请号"
            }
          }
        ]
      },
      "datagrid": {
        "scope": "searchTable",
        "isInitSearch": true,
        "doRequest": {
          "url": "/SUMP/icmcall/icmRPCCall",
          "method": "POST",
          "params": {
            "servicecode": "pl0260"
          }
        },
        "thead": [
          {
            "type": "index",
            "align": "center"
          },
          {
            "label": "申请编号",
            "prop": "apply_no",
            "align": "center"
          },
          {
            "label": "启动操作员",
            "prop": "start_oprtr",
            "align": "center"
          },
          {
            "label": "启动机构",
            "prop": "actvn_org",
            "align": "center"
          },
          {
            "label": "当前处理人",
            "prop": "cur_agnt",
            "align": "center"
          },        {
            "label": "当前处理步骤",
            "prop": "cur_deal_step",
            "align": "center"
          },
          {
            "label": "操作",
            "align":"center",
            "fixed": "right",
            "btns": [
              {
                "type": "look",
                "label": "查看详情",
                "toRequest": {
                  "url": "/views/pl/assetPreserve/disposalPlan/disposalPlanViewListDetail.json",
                  "method": "GET",
                  "params": {
                    "apply_no": "${{current.apply_no}}"
                  }
                },
                "props": {
                  "width": "65%",
                  "title": "处置信息"
                }
              }
            ]
          }
        ]
      }
    }
  }
