{
  "module": "v-form-btn-dynamic",
  "layout": {
    "form": {
      "initRequest":"${{params}}",
      "scope": "formData",
      "controls": [
        {
          "hndov_prod_nm": {
            "label": "移交产品",
            "control": "lookup",
            "width": "130px",
            "span": "10",
            "disabled": true,
            "edit": true,
            "keyField": {
              "hndov_prod_nm": "simwf_name",
              "hand_prod_code": "simwf_cd",
              "wthr_hndov_cust": "cust_tx_flag",
              "wthr_hndov_acctg": "accting_flag",
              "wthr_hndov_busi": "trans_busi_flag",
              "wthr_cross_org_hndov": "trans_brch_flag"
            },
            "toRequest": {
              "url": "/views/pl/assetHndov/handover/tranferProduct.json",
              "method": "post"
            },
            "props": {
              "title": "选择移交产品"
            },
            "format": "label"
            
          }
        },
        {
          "hand_prod_code": {
            "control": "hidden",
            "label": "移交产品编码",
            "width": "130px",
            "span":"10"
          },
          "wthr_hndov_acctg": {
            "control": "select",
            "label": "是否移交账务",
            "width": "130px",
            "disabled": true,
            "span":"10",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          }
        },
        {
          "wthr_hndov_cust": {
            "control": "select",
            "label": "是否移交客户",
            "width": "130px",
            "disabled": true,
            "span":"10",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          }
        },
        {
          "wthr_hndov_busi": {
            "control": "select",
            "label": "是否移交业务",
            "width": "130px",
            "span": "10",
            "disabled": true,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          }
        },
        {
          "wthr_cross_org_hndov": {
            "control": "select",
            "label": "是否跨机构移交",
            "width": "130px",
            "span":"10",
            "disabled": true,
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "label"
            }
          }
        },
        {
          "remark": {
            "label": "备注",
            "control": "textarea",
            "width": "130px",
            "span":"10",
            "disabled": true,
            "autosize":{
              "minRows": 2
            },
            "rules": [
              {
                "max": "200",
                "message": "长度不能超过200个字符"
              }
            ]
          }
        }
      ]
    }
  }
}

