{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/rest/auth/userManager",
        "method": "POST",
        "params": {
          "servicecode": "ct2002",
            "appl_id": "${{params.appl_id}}"
        }
      },
      "thead": [
        {
          "label": "客户经理编码",
          "prop": "user_id",
          "align": "center"
        },
        {
          "label": "客户经理",
          "prop": "user_name",
          "align": "center"
        },
        {
          "label": "机构编号",
          "prop": "branch_id",
          "align": "center"
        },

        {
          "label": "操作",
          "align": "center",
          "fixed": "right",
          "btns": [{
            "type": "bringback",
            "label": "选择"
          }]
        }
      ]
    }
  }
}
