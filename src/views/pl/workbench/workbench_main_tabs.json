{
  "module": "dynamicTabs",
  "layout": {
    "form": {
      "controls": [
          {
              "appId": {
                  "control": "select",
                  "label": "业务模块",
                  "selectFirstValue":true,
                  "dict": {
                      "dictType": "MS_APPID",
                      "dictKey": [
                          "MS_APPID"
                      ],
                      "format": "value-label"
                  },
                  "rules": [
                      {
                          "required": true,
                          "message": "请选择业务模块"
                      }]
              }
            }]
          },
    "tabs": {
      "tabHeader": [
        {
          "id": "tab1",
          "label": "待办事项",
          "closable": false,
          "disabled": false,
          "icon": "el-icon-date",
          "toRequest": {
            "url": "/views/pl/workbench/workbench_wait_todo_view.json",
            "method": "post"
          }
        },
        {
          "id": "tab2",
          "label": "已办事项",
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "toRequest": {
            "url": "/views/pl/workbench/workbench_already_do_view.json",
            "method": "post"
          }
        },

        {
          "id": "tab5",
          "label": "进件池",
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "toRequest": {
            "url": "/views/pl/workbench/workbench_into_pool_view.json",
            "method": "post"
          }
        }
      ],
      "defaultActiveKey": "tab1",
      "position": "top",
      "styleClass": "card"
    }
  }
}
