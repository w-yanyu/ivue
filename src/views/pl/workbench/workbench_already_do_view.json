{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [

        
        {
          
          "appl_id": {
            "control": "text",
            "label": "申请编号"
          },"cust_name": {
            "control": "text",
            "label": "申请人姓名"
          }
        },
        {
          "appl_start_date": {
            "control": "dateTimePicker",
            "label": "进件起始日期"
          },
          "appl_end_date": {
            "control": "dateTimePicker",
            "label": "进件截止日期"
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "bpm1013",
          "sizePage": 10,
          "appId":"plOnl",
          "qryInfo":{
            "user_id": "$g{{user_info.userCode}}"
          }
        }
      },
      "thead": [
        {
          "label": "序号",
          "type": "index"
        },
        {
          "label": "申请编号",
          "prop": "appl_id",
          "width": 200,
          "align": "center"
        },
        {
          "label": "申请人姓名",
          "prop": "cust_name",
          "width": 140,
          "align": "center"
        },{
          "label": "流程产品名称",
          "width": 180,   
          "prop": "wf_prod_name"
        },
        {
          "label": "申请日期",
          "prop": "system_date",
          "align": "center"
        },
        {
          "label": "当前节点",
          "prop": "task_define_name",
          "align": "center"
        },
        {
          "label": "处理状态",
          "prop": "appl_state",
          "dict": {
              "dictType": "CT_E_APPLST",
              "dictKey": [
                "CT_E_APPLST"
              ],
              "format": "label"
          }
        },
        {
          "label": "当前处理用户名称",
          "prop": "user_name",
          "width": 125,
          "value": "${{current.cur_user_id}}-${{current.user_name}}"
        },
        {
          "label": "当前处理角色名称",
          "prop": "role_name",
          "width": 125,
          "value": "${{current.cur_wfrl_id}}-${{current.role_name}}"
        },
        {
          "label": "操作",
          "fixed": "right",
          "width": 160,
          "align": "center",
          "btns": [
            {
              "type": "edit",
              "label": "详情",
              "toRequest": {
                "url": "@CommAlreadyDoDetail",
                "method": "GET",
                "target": "navTab",
                "params": {
                  "frame_cd": "${{current.task_url2}}",
                  "cust_name": "${{current.cust_name}}",
                  "appl_state": "${{current.appl_state}}",
                  "appl_date": "${{current.appl_date}}",
                  "task_inst_id":"${{current.task_inst_id}}",
                  "appId":"plOnl",
                  "wf_inst_id": "${{current.wf_inst_id}}",
                  "cust_no": "${{current.cust_id}}"
                }
              },
              "props": {
                "title": "已办事项详情"
              }
            },
            {
              "type": "edit",
              "label": "流程跟踪",
              "toRequest": {
                "url": "@ProcessImgView",
                "method": "GET",
                "params": {
                  "appId":"plOnl",
                  "from": "doneList"
                }
              },
              "props": {
                "width": "1200px",
                "title": "流程跟踪图"
              }
            },
            {
              "type": "edit",
              "label": "撤回",
              "show": {
                "prop": "bpm_recallable",
                "is": [
                  "1"
                ]
              },
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "bpm1017",
                    "appId":"plOnl",
                    "wf_inst_id": "${{current.wf_inst_id}}"
                }
              },
              "confirm":{
                  "title":"提示信息",
                  "msg":"确认要撤回吗？",
                  "confirmButtonText":"确定",
                  "cancelButtonText":"取消"
              }
            }
          ]
        }
      ]
    }
  }
}
