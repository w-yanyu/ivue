{
  "module": "dynamicTabs",
  "layout": {
    "tabs": {
      "tabHeader": [
        {
          "id": "01",
          "label": "预警详情",
          "closable": false,
          "disabled": false,
          "icon": "el-icon-date",
          "toRequest": {
            "url": "/views/ew/alertDetailsInfo.json",
            "method": "post",
            "params": {
              "case_id": "${{params.case_id}}"
            },
            "props": {
              "title": "预警信息详情"
            }
          }
        },
        {
          "id": "02",
          "label": "授信记录信息",
          "closable": false,
          "disabled": false,
          "icon": "el-icon-date",
          "toRequest": {
            "url": "/views/ew/creditProdDetail.json",
            "method": "post",
            "params": {
              "cust_no": "${{params.cust_id}}"
            }
          }
        },
        {
          "id": "03",
          "label": "合同借据信息",
          "closable": false,
          "disabled": false,
          "icon": "el-icon-date",
          "toRequest": {
            "url": "/views/ew/contractinfo.json",
            "method": "post",
            "params": {
              "case_id": "${{params.case_id}}"
            }
          }
        }
      ],
      "defaultActiveKey": "01",
      "position": "top",
      "styleClass": "card"
    }


  }
}
