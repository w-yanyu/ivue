{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "isInitSearch": true,
      "scope": "searchTable",
      "doRequest": {
        "url": "/SUMP/call/RPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ew3176",
          "contno": "${{params.contno}}",
          "case_id": "${{params.case_id}}",
          "method": "queryAll"
        }
      },
      "thead": [
        {
          "label": "担保合同编号",
          "align": "center",
          "width": "220px",
          "prop": "assucontno"
        },
        {
          "label": "担保人/抵质人名称",
          "align": "center",
          "prop": "assucustname"
        },
        {
          "label": "担保合同类型",
          "align": "center",
          "width": "180px",
          "prop": "assuconttype",
          "dict": {
            "dictType": "AR_PL_GUAR_RULE",
            "dictKey": [
              "AR_PL_GUAR_RULE"
            ],
            "format": "label"
          }
        },
        {
          "label": "担保起始日期",
          "align": "center",
          "prop": "begindate"
        },
        {
          "label": "担保到期日期",
          "align": "center",
          "prop": "enddate"
        },
        {
          "label": "担保合同币种",
          "align": "center",
          "prop": "assusign",
          "dict": {
            "dictType": "E_CURRENCY",
            "dictKey": [
              "E_CURRENCY"
            ],
            "format": "label"
          }
        },
        {
          "type": "string",
          "align": "center",
          "width": "180px",
          "label": "担保合同金额（元）",
          "prop": "assuamt"
        },
        {
          "label": "本次使用担保金额（元）",
          "align": "center",
          "prop": "usedassuamt"
        },
        {
          "label": "担保合同签订时间点",
          "align": "center",
          "prop": "contdate"
        },
        {
          "label": "担保合同状态",
          "align": "center",
          "prop": "assucontstatus",
          "dict": {
            "dictType": "AR_E_VALID_STATE",
            "dictKey": [
              "AR_E_VALID_STATE"
            ],
            "format": "label"
          }
        }
      ],
      "pagination": {
        "show": false
      }
    }
  }
}
