{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "scope": "formData",
      "controls": [
        {
          "case_id": {
            "control": "text",
            "span": 8,
            "width": "120px",
            "maxlength":50,
            "label": "预警任务编号"
            
          },
          "cust_id": {
            "control": "text",
            "span": 8,
            "width": "120px",
            "label": "客户编号",
            "maxlength":50
          },
          "custna": {
            "control": "text",
            "span": 8,
            "width": "120px",
            "label": "客户名称"
          },
          "idtfno": {
            "control": "text",
            "span": 8,
            "width": "120px",
            "label": "证件号码",
            "maxlength":18
          },
          "level": {
            "control": "select",
            "label": "预警级别",
            "span": 8,
            "width": "120px",
            "default": "",
            "dict": {
              "dictType": "css_level",
              "dictKey": [
                "css_level"
              ],
              "format": "label"
            }
          },
          "csstatus": {
            "control": "select",
            "label": "超时状态",
            "width": "120px",
            "span": 8,
            "default": "",
            "dict": {
              "dictType": "css_tatus",
              "dictKey": [
                "css_tatus"
              ],
              "format": "label"
            }
          }
        }
      ]
    },
    "datagrid": {
      "doRequest": {
        "url": "/SUMP/call/RPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ew3161"
        }
      },
      "thead": [
        {
          "type": "string",
          "align": "center",
          "label": "预警任务编号",
          "prop": "case_id"                                                                               
        },
        {
          "type": "string",
          "width": "150px",
          "align": "center",
          "label": "客户编号",
          "prop": "cust_id"
        },
        {
          "type": "string",
          "align": "center",
          "label": "客户名称",
          "prop": "custna"
        },
        {
          "type": "string",
          "width": "180px",
          "align": "center",
          "label": "证件号码",
          "prop": "idtfno"
        },
    
        {
          "type": "string",
          "align": "center",
          "label": "预警内容",
          "prop": "contentx"
        },
        {
          "type": "string",
          "align": "center",
          "width":"180px",
          "label": "预警时间",
          "prop": "yjdate",
          "format": "yyyy-MM-dd HH:mm:ss"
        },
        {
          "type": "string",
          "align": "center",
          "label": "预警级别",
          "prop": "level",
          "dict": {
            "dictType": "css_level",
            "dictKey": [
              "css_level"
            ],
            "format": "label"
          }
        },        
        {
          "type": "string",
          "align": "center",
          "label": "超时状态",
          "prop": "csstatus",
          "dict": {
            "dictType": "css_tatus",
            "dictKey": [
              "css_tatus"
            ],
            "format": "label"
          }
        },
        {
          "type": "string",
          "align": "center",
          "width": "180px",
          "label": "预警超时时长",
          "prop": "yjcssj"
        },
        {
          "label": "操作",
          "fixed": "right",
          "width": 120,
          "align": "center",
          "btns": [
            {
              "type": "look",
              "label": "详情",
              "toRequest": {
                "url": "/views/ew/alertQryTab.json",
                "method": "GET",
                "params": {
                  "case_id": "${{current.case_id}}"
                }
              },
              "props": {
                "width": "85%",
                "height": "85%",
                "title": "预警信息详情"
              }
            },
            {
              "type": "look",
              "label": "处理",
              "toRequest": {
                "url": "@earlyProcess",
                "method": "GET",
                "params": {
                  "url_query":"/views/ew/earlyProcess.json",
                  "case_id": "${{current.case_id}}",
                  "rule_id": "${{current.rule_id}}",
                  "cust_id": "${{current.cust_id}}"
                }
              },
              "props": {
                "width": "80%",
                "height": "85%",
                "title": "预警处理"
              }
            }
          ]
        }
      ]
    }
  }
}
