{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "controls": [],
            "btns": []
          },
        "toolbar": {},
        "datagrid": {
            "isInitSearch": true,
            "scope": "searchTable",
            "doRequest": {
              "url": "/SUMP/call/RPCCall",
              "method": "POST",
              "params": {
                "servicecode": "ew3163",
                "case_id": "${{params.case_id}}"
              }
            },
            "pagination": {
              "show": false
            },
            "thead": [
              {
                "type": "string",
                "align": "center",
                "label": "合同编号",
                "prop": "contno"
              },
              {
                "type": "string",
                "align": "center",
                "label": "产品名称",
                "prop": "prodname"
              },
              {
                "type": "string",
                "align": "center",
                "label": "合同金额（元）",
                "prop": "conttotalamt"
              },
              {
                "type": "string",
                "align": "center",
                "label": "合同可用余额（元）",
                "prop": "conttouseamt"
              },
              {
                "type": "string",
                "align": "center",
                "width": "180px",
                "label": "担保方式",
                "prop": "assukind",
                "dict": {
                  "dictType": "PL_E_GUARANTEE_WAY",
                  "dictKey": [
                    "PL_E_GUARANTEE_WAY"
                  ],
                  "format": "label"
                }
              },
              {
                "type": "string",
                "align": "center",
                "label": "起始日期",
                "prop": "begindate"
              },
              {
                "type": "string",
                "align": "center",
                "width": "120",
                "label": "到期日期",
                "prop": "enddate"
              },
              {
                "type": "string",
                "align": "center",
                "label": "贷款用途",
                "prop": "loanuseremark",
                "dict": {
                  "dictType": "PL_LOAN_USE",
                  "dictKey": [
                    "PL_LOAN_USE"
                  ],
                  "format": "label"
                }
              },
              {
                "type": "string",
                "align": "center",
                "width": "180px",
                "label": "还款方式",
                "prop": "retukind",
                "dict": {
                  "dictType": "AR_E_RETUKIND",
                  "dictKey": [
                    "AR_E_RETUKIND"
                  ],
                  "format": "label"
                }
              },
              {
                "type": "string",
                "align": "center",
                "width": "180px",
                "label": "合同状态",
                "prop": "contstatus",
                "dict": {
                  "dictType": "AR_PL_CONTSTATUS",
                  "dictKey": [
                    "AR_PL_CONTSTATUS"
                  ],
                  "format": "label"
                }
              },
              {
                "label": "操作",
                "width": "180PX",
                "align": "center",
                "btns": [
                  {
                    "type": "custom",
                    "label": "冻结",
                    "doRequest": {
                      "url": "/SUMP/call/RPCCall",
                      "params": {
                        "servicecode": "ew3178",
                        "contno": "${{current.contno}}",
                        "contstatus": "A5"
                      }
                    },
                    "confirm": {
                      "title": "提示信息",
                      "msg": "确定冻结吗？",
                      "confirmButtonText": "确定",
                      "cancelButtonText": "取消"
                    },
                    "show": {
                      "expression": "'${{current.contstatus}}' == 'A4'"
                    }  
      
                  }
                ]
              }
            ]
          }
    }
}
