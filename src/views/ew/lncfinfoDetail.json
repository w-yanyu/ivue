{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "height": "80%",
      "scope": "formData",
      "btns": [],
      "initRequest": {
        "url": "/SUMP/call/RPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ew3175",
          "loanacno": "${{params.loanacno}}",
          "case_id": "${{params.case_id}}"
        }
      },
      "controlsGroup": [
        {
          "title": "借据详细信息",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "accordion": false,
          "expand": true,
          "controls": [
            {
              "loanacno": {
                "control": "text",
                "disabled": true,
                "span": 8,
                "label": "借据编号"
              },
              "contno": {
                "control": "text",
                "disabled": true,
                "span": 8,
                "label": "合同编号"
              },
              "custid": {
                "control": "text",
                "disabled": true,
                "span": 8,
                "label": "客户编号"
              },
              "custname": {
                "control": "text",
                "disabled": true,
                "span": 8,
                "label": "客户名称"
              },
              "prodname": {
                "control": "text",
                "disabled": true,
                "span": 8,
                "label": "产品名称"
              },
              "assukind": {
                "control": "select",
                "label": "担保方式",
                "disabled": true,
                "span": 8,
                "dict": {
                  "dictType": "EW_E_GUAR_TYPE",
                  "dictKey": [
                    "EW_E_GUAR_TYPE"
                  ],
                  "format": "label"
                }
              },
              "paymode": {
                "control": "select",
                "label": "支付方式",
                "disabled": true,
                "span": 8,
                "dict": {
                  "dictType": "PD_E_PAYMENT_METHOD",
                  "dictKey": [
                    "PD_E_PAYMENT_METHOD"
                  ],
                  "format": "label"
                }
              },
              "sumtcapi": {
                "control": "text",
                "label": "发放金额(元)",
                "span": 8,
                "disabled": true
              },
              "bal": {
                "control": "text",
                "disabled": true,
                "span": 8,
                "label": "借据余额(元)"
              },
              "begindate": {
                "control": "text",
                "disabled": true,
                "span": 8,
                "label": "发放日期"
              },
              "enddate": {
                "control": "text",
                "span": 8,
                "disabled": true,
                "label": "到期日期"
              },
              "overbal": {
                "control": "text",
                "disabled": true,
                "span": 8,
                "label": "拖欠本金（元）"
              },
              "overinte": {
                "control": "text",
                "disabled": true,
                "span": 8,
                "label": "拖欠利息（元）"
              },
              "overbegindate_tcapi": {
                "control": "text",
                "disabled": true,
                "span": 8,
                "label": "本金拖欠开始日期"
              },
              "overbegindate_inte": {
                "control": "text",
                "disabled": true,
                "span": 8,
                "label": "利息拖欠开始日期"
              },
              "inteadjukind": {
                "control": "select",
                "label": "利率调整方式",
                "disabled": true,
                "span": 8,
                "dict": {
                  "dictType": "EW_E_ARRANGE_MODE",
                  "dictKey": [
                    "EW_E_ARRANGE_MODE"
                  ],
                  "format": "label"
                }
              },
              "lprinteratetz": {
                "control": "text",
                "disabled": true,
                "span": 8,
                "label": "基点调整数"
              },
              "interate": {
                "control": "text",
                "span": 8,
                "disabled": true,
                "label": "贷款利率(%)"
              },
              "floatfinerate": {
                "control": "text",
                "disabled": true,
                "span": 8,
                "label": "逾期利率浮动比%"
              },
              "finerate": {
                "control": "text",
                "disabled": true,
                "span": 8,
                "label": "逾期利率%"
              },
              "riskflag": {
                "control": "select",
                "disabled": true,
                "span": 8,
                "label": "风险分类",
                "dict": {
                  "dictType": "AR_PL_RISK_FLAG",
                  "dictKey": [
                    "AR_PL_RISK_FLAG"
                  ],
                  "format": "label"
                }
              },
              "overflag": {
                "control": "select",
                "label": "拖欠标志",
                "disabled": true,
                "span": 8,
                "dict": {
                  "dictType": "AR_E_OVERFLAG",
                  "dictKey": [
                    "AR_E_OVERFLAG"
                  ],
                  "format": "label"
                }
              },
              "payoffflag": {
                "control": "select",
                "disabled": true,
                "span": 8,
                "label": "结清标志",
                "dict": {
                  "dictType": "AR_PL_LOAN_STATE",
                  "dictKey": [
                    "AR_PL_LOAN_STATE"
                  ],
                  "format": "label"
                }
              },
              "operid": {
                "control": "text",
                "disabled": true,
                "span": 8,
                "label": "客户经理"
              },
              "bbal": {
                "control": "text",
                "disabled": true,
                "span": 8,
                "width": "220px",
                "label": "逾期本金"
              },
              "yqlx": {
                "control": "text",
                "disabled": true,
                "span": 8,
                "width": "220px",
                "label": "逾期利息"
              },
              "yqts": {
                "control": "text",
                "disabled": true,
                "span": 8,
                "width": "220px",
                "label": "逾期天数"
              },
              "yqqs": {
                "control": "text",
                "disabled": true,
                "span": 8,
                "width": "220px",
                "label": "逾期期数"
              },
              "zwdqr": {
                "control": "text",
                "disabled": true,
                "span": 8,
                "width": "220px",
                "label": "债务到期日"
              },
              "sfyqs": {
                "control": "select",
                "label": "是否已起诉",
                "disabled": true,
                "span": 8,
                "options": [
                  {
                    "label": "是",
                    "value": "1"
                  },
                  {
                    "label": "否",
                    "value": "0"
                  }
                ]
              },
              "pjsxr": {
                "control": "text",
                "disabled": true,
                "span": 8,
                "width": "220px",
                "label": "判决生效日"
              },
              "ccbqrq": {
                "control": "text",
                "disabled": true,
                "span": 8,
                "width": "220px",
                "label": "财产保全起始日期"
              },
              "sfyzxla": {
                "control": "select",
                "disabled": true,
                "span": 8,
                "width": "220px",
                "label": "是否已执行立案",
                "options": [
                  {
                    "label": "是",
                    "value": "1"
                  },
                  {
                    "label": "否",
                    "value": "0"
                  }
                ]
              },
              "zxlarq": {
                "control": "text",
                "disabled": true,
                "span": 8,
                "width": "220px",
                "label": "执行立案日期"
              },
              "bankid": {
                "control": "text",
                "disabled": true,
                "span": 16,
                "label": "机构名称"
              }
              
            }
          ]
        }

      ]
    }
  }
}
