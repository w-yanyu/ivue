{
  "module": "v-inputDataForm-btn",
  "layout": {
    "scope": "templateScope",
    "form": {
      "btns":[],
      "controls": [
          {
            "top": {
              "control":"dynamic",
              "span":24,
              "toRequest":{
                "url":"/views/ew/earlyProcess.json",
                "method": "post",
                "params": {
                  "case_id": "${{params.case_id}}",
                  "cust_id": "${{params.cust_id}}"
                }
              }
            },
            "middle": {
              "control":"dynamic",
              "span":24,
              "toRequest":{
                "url":"/views/ew/creditProdInfo.json",
                "method": "post",
                "params": {
                  "cust_no": "${{params.cust_id}}"
                }
              }
            },
            "bottom": {
              "control":"dynamic",
              "span":24,
              "toRequest":{
                "url":"/views/ew/contractDataInfo.json",
                "method": "post",
                "params": {
                  "case_id": "${{params.case_id}}",
                  "cust_no": "${{params.cust_id}}"
                }
              }
            }
          } 
      ]
    }
  }
}
