{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "isInitSearch": true,
      "scope": "searchTable",
      "doRequest": {
        "url": "/SUMP/call/RPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ew3164",
          "case_id": "${{params.case_id}}",
          "method": "queryAll"
        }
      },
      "thead": [
        {
          "label": "合同编号",
          "align": "center",
          "prop": "contno"
        },
        {
          "label": "借据编号",
          "align": "center",
          "prop": "loanacno"
        },
        {
          "label": "贷款金额(元)",
          "align": "center",
          "prop": "tcapi"
        },
        {
          "label": "贷款余额(元)",
          "align": "center",
          "prop": "bal"
        },
        {
          "label": "发放日期",
          "align": "center",
          "prop": "begindate"
        },
        {
          "label": "到期日期",
          "align": "center",
          "prop": "enddate"
        },
        {
          "type": "string",
          "align": "center",
          "width": "180px",
          "label": "拖欠标志",
          "prop": "overflag",
          "dict": {
            "dictType": "AR_E_OVERFLAG",
            "dictKey": [
              "AR_E_OVERFLAG"
            ],
            "format": "label"
          }
        },
        {
          "label": "贷款利率(%)",
          "align": "center",
          "prop": "interate"
        },
        {
          "label": "操作",
          "fixed": "right",
          "width": 120,
          "align": "center",
          "btns": [
            {
              "type": "look",
              "label": "详情",
              "toRequest": {
                "url": "/views/ew/lncfinfoDetail.json",
                "method": "POST",
                "params": {
                  "loanacno": "${{current.loanacno}}",
                  "case_id": "${{params.case_id}}"
                }
              },
              "props": {
                "width": "80%",
                "height": "85%",
                "title": "借据信息详情"
              }
            }
          ]
        }
      ],
      "pagination": {
        "show": false
      }
    }
  }
}
