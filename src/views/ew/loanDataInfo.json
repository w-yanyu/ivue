{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "isInitSearch": true,
      "scope": "searchTable",
      "doRequest": {
        "url": "/SUMP/call/RPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ew3164",
          "case_id": "${{params.case_id}}",
          "method": "queryAll"
        }
      },
      "thead": [
        {
          "label": "借据编号",
          "align": "center",
          "prop": "loanacno"
        },
        {
          "label": "合同编号",
          "align": "center",
          "prop": "contno"
        },
        {
          "label": "客户编号",
          "align": "center",
          "prop": "custid"
        },
        {
          "label": "客户名称",
          "align": "center",
          "prop": "custname"
        },
        {
          "label": "产品名称",
          "align": "center",
          "prop": "prodname"
        },
        {
          "label": "担保方式",
          "align": "center",
          "prop": "assukind",
          "dict": {
            "dictType": "EW_E_GUAR_TYPE",
            "dictKey": [
              "EW_E_GUAR_TYPE"
            ],
            "format": "label"
          }
        },
        {
          "label": "发放日期",
          "align": "center",
          "prop": "begindate"
        },
        {
          "label": "到期日期",
          "align": "center",
          "prop": "enddate"
        },
        {
          "label": "贷款金额(元)",
          "align": "center",
          "prop": "tcapi"
        },
        {
          "label": "贷款余额(元)",
          "align": "center",
          "prop": "bal"
        },
        {
          "label": "贷款利率(%)",
          "align": "center",
          "prop": "interate"
        },
        {
          "align": "center",
          "label": "还款方式",
          "prop": "retukind",
          "dict": {
            "dictType": "AR_E_REPAY_WAY",
            "dictKey": [
              "AR_E_REPAY_WAY"
            ],
            "format": "label"
          }
        },
        {
          "align": "center",
          "label": "还款周期",
          "prop": "retuPeri"
        },
        {
          "label": "操作",
          "fixed": "right",
          "width": 120,
          "align": "center",
          "btns": [
            {
              "type": "delete",
              "label": "转催收",
              "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "params": {
                  "servicecode": "ew3164"
                }
              },
              "confirm": {
                "title": "提示信息",
                "msg": "确定转催收吗？",
                "confirmButtonText": "确定",
                "cancelButtonText": "取消"
              }
            }
          ]
        }
      ],
      "pagination": {
        "show": false
      }
    }
  }
}
