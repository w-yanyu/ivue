{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "height": "80%",
      "scope": "formData",
      "initRequest": {
        "url": "/SUMP/call/RPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ew3162",
          "case_id": "${{params.case_id}}"
        }
      },
      "btns": [],
      "controlsGroup": [
        {
          "title": "客户基本信息",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "accordion": false,
          "expand": true,
          "controls": [
            {
              "cust_id": {
                "control": "text",
                "disabled": true,
                "span": 6,
                "label": "客户编号"
              },
              "custna": {
                "control": "text",
                "disabled": true,
                "span": 6,
                "label": "客户名称"
              },
              "idtfno": {
                "control": "text",
                "label": "证件号码",
                "span": 6,
                "disabled": true
              },
              "contact_no": {
                "control": "text",
                "disabled": true,
                "span": 6,
                "label": "联系方式"
              },
              "mar_status": {
                "control": "select",
                "label": "婚姻状况",
                "disabled": true,
                "span": 6,
                "default": "",
                "dict": {
                  "dictType": "MS_E_MARITA",
                  "dictKey": [
                    "MS_E_MARITA"
                  ],
                  "format": "label"
                }
              },
              "borrower_nature": {
                "label": "借款人性质",
                "control": "select",
                "disabled": true,
                "span": 6,
                "default": "",
                "dict": {
                  "dictType": "US_E_CUST_TYPE",
                  "dictKey": [
                    "US_E_CUST_TYPE"
                  ],
                  "format": "label"
                }
              },
              "occupation": {
                "control": "select",
                "span": 6,
                "disabled": true,
                "label": "职业",
                "default": "",
                "dict": {
                  "dictType": "PL_WA_JOB_TYPE",
                  "dictKey": [
                    "PL_WA_JOB_TYPE"
                  ],
                  "format": "label"
                }
              },
              "commadd": {
                "control": "text",
                "span": 6,
                "disabled": true,
                "label": "通讯地址"
              }
            }
          ]
        },
        {
          "title": "预警信息",
          "tooltip": "",
          "titleIcon": "",
          "showHint": "",
          "accordion": false,
          "expand": true,
          "controls": [
            {
              "case_id": {
                "control": "text",
                "disabled": true,
                "span": 6,
                "label": "任务预警编号"
              },
              "level": {
                "control": "select",
                "label": "预警级别",
                "disabled": true,
                "span": 6,
                "dict": {
                  "dictType": "css_level",
                  "dictKey": [
                    "css_level"
                  ],
                  "format": "label"
                }
              },
              "yjdate": {
                "control": "text",
                "disabled": true,
                "span": 6,
                "label": "预警时间",
                "format": "yyyy-MM-dd HH:mm:ss"
              },
              "category": {
                "control": "select",
                "label": "预警分类",
                "disabled": true,
                "span": 6,
                "default": "",
                "format": "value",
                "options":[
                  {
                    "label": "信用状况异常",
                    "value": "信用状况异常"
                  },
                  {
                    "label": "资金流向异常",
                    "value": "资金流向异常"
                  },
                  {
                    "label": "触发法律风险",
                    "value": "触发法律风险"
                  },
                  {
                    "label": "当事人信息变化",
                    "value": "当事人信息变化"
                  },
                  {
                    "label": "家庭状况变化",
                    "value": "家庭状况变化"
                  },
                  {
                    "label": "我行预警名单",
                    "value": "我行预警名单"
                  },
                  {
                    "label": "经营状况变化",
                    "value": "经营状况变化"
                  }
                ]
               },

              "csstatus": {
                "control": "select",
                "label": "超时状态",
                "disabled": true,
                "span": 6,
                "dict": {
                  "dictType": "css_tatus",
                  "dictKey": [
                    "css_tatus"
                  ],
                  "format": "label"
                }
              },
              "yjcssj": {
                "control": "text",
                "span": 6,
                "disabled": true,
                "label": "预警超时时长"
              },
              "contentx": {
                "control": "textarea",
                "disabled": true,
                "span": 12,
                "label": "预警内容"
              }
            }
          ]
        }
      ]
    }
  }
}