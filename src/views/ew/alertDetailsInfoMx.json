{
    "module": "v-inputDataForm-btn",
    "layout": {
        "form": {
            "height": "80%",
            "scope": "formData",
            "initRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "ew3162",
                    "case_id": "${{params.case_id}}"
                }
            },
            "btns": [

                {
                    "###type": "add",
                    "target": "查看影像",
                    "label": "查看影像",
                    "icon": "iconfont icon-daochu",
                    "toRequest": {
                      "method": "POST",
                      "url": "@ViewListComm",
                      "params": {
                        "bid": "${{params.case_id}}"
                      }
                    },
                    "props": {
                      "width": "60%",
                      "height": "65%",
                      "title": "查看影像"
                    }
                  }
                
            ],
            "controlsGroup": [
                {
                    "title": "客户基本信息",
                    "tooltip": "",
                    "titleIcon": "",
                    "showHint": "",
                    "accordion": false,
                    "expand": true,
                    "controls": [
                        {
                            "cust_id": {
                                "control": "text",
                                "disabled": true,
                                "span": 6,
                                "label": "客户编号"
                            },
                            "custna": {
                                "control": "text",
                                "disabled": true,
                                "span": 6,
                                "label": "客户名称"
                            },
                            "idtfno": {
                                "control": "select",
                                "label": "证件号码",
                                "span": 6,
                                "disabled": true
                            },
                            "contact_no": {
                                "control": "select",
                                "disabled": true,
                                "span": 6,
                                "label": "联系方式"
                            },
                            "mar_status": {
                                "control": "select",
                                "label": "婚姻状况",
                                "disabled": true,
                                "span": 6,
                                "default": "",
                                "dict": {
                                    "dictType": "US_MARITAL_STATUS",
                                    "dictKey": [
                                        "US_MARITAL_STATUS"
                                    ],
                                    "format": "label"
                                }
                            },
                            "borrower_nature": {
                                "control": "select",
                                "span": 6,
                                "disabled": true,
                                "label": "借款人性质",
                                "default": "",
                                "dict": {
                                    "dictType": "US_E_NATURE_BORROWER",
                                    "dictKey": [
                                    "US_E_NATURE_BORROWER"
                                    ],
                                "format": "label"
                                }
                            },
                            "occupation": {
                                "control": "select",
                                "span": 6,
                                "disabled": true,
                                "label": "职业",
                                "default": "",
                                "dict": {
                                    "dictType": "PL_WA_JOB_TYPE",
                                    "dictKey": [
                                    "PL_WA_JOB_TYPE"
                                    ],
                                    "format": "label"
                                }
                            },
                            "commadd": {
                                "control": "text",
                                "span": 6,
                                "disabled": true,
                                "label": "通讯地址"
                            }
                        }
                    ]
                },
                {
                    "title": "预警信息",
                    "tooltip": "",
                    "titleIcon": "",
                    "showHint": "",
                    "accordion": false,
                    "expand": true,
                    "controls": [
                        {
                            "case_id": {
                                "control": "text",
                                "disabled": true,
                                "span": 6,
                                "label": "任务预警编号"
                            },
                            "level": {
                                "control": "select",
                                "label": "预警级别",
                                "disabled": true,
                                "span": 6,
                                "dict": {
                                    "dictType": "css_level",
                                    "dictKey": [
                                        "css_level"
                                    ],
                                    "format": "label"
                                }
                            },
                            "yjdate": {
                                "control": "text",
                                "disabled": true,
                                "span": 6,
                                "label": "预警时间"
                            },
                            "category": {
                                "control": "select",
                                "disabled": true,
                                "span": 6,
                                "label": "预警分类",
                                "dict": {
                                    "dictType": "category",
                                    "dictKey": [
                                        "category"
                                    ],
                                    "format": "label"
                                }
                            },
                            "csstatus": {
                                "control": "select",
                                "label": "超时状态",
                                "disabled": true,
                                "span": 6,
                                "dict": {
                                    "dictType": "css_tatus",
                                    "dictKey": [
                                        "css_tatus"
                                    ],
                                    "format": "label"
                                }
                            },
                            "yjcssj": {
                                "control": "text",
                                "span": 6,
                                "disabled": true,
                                "label": "预警超时时长"
                            },
                            "cmanager": {
                                "control": "text",
                                "span": 6,
                                "disabled": true,
                                "label": "客户经理"
                            },
                            "jgname": {
                                "control": "text",
                                "span": 6,
                                "disabled": true,
                                "label": "机构名称"
                            },
                            "contentx": {
                                "control": "textarea",
                                "disabled": true,
                                "span": 18,
                                "label": "预警内容"
                            }
                        }
                    ]
                },
                {
                    "title": "处理结果",
                    "tooltip": "",
                    "titleIcon": "",
                    "showHint": "",
                    "accordion": false,
                    "expand": true,
                    "controls": [
                        {
                            "status": {
                                "control": "select",
                                "label": "处理状态",
                                "disabled": true,
                                "span": 6,
                                "dict": {
                                    "dictType": "ew_status",
                                    "dictKey": [
                                        "ew_status"
                                    ],
                                    "format": "label"
                                }
                            },
                            "cldate": {
                                "control": "text",
                                "disabled": true,
                                "span": 6,
                                "label": "预警处理时间"
                            },
                            "handle_results": {
                                "control": "select",
                                "label": "处理结果",
                                "disabled": true,
                                "span": 6,
                                "dict": {
                                    "dictType": "result_code",
                                    "dictKey": [
                                        "result_code"
                                    ],
                                    "format": "label"
                                }
                            },
                            "risk_point": {
                                "control": "select",
                                "span": 6,
                                "disabled": true,
                                "label": "风险点",
                                "format":"label",
                                "options": [
                  {
                    "label": "未能联系到当事人",
                    "value": "A"
                  },
                  {
                    "label": "疑似当事人家庭发生变故",
                    "value": "B"
                  },
                  {
                    "label": "疑似当事人经营发生变故",
                    "value": "C"
                  },
                  {
                    "label": "疑似当事人工作发生变故",
                    "value": "D"
                  },
                  {
                    "label": "疑似当事人产生法律风险",
                    "value": "E"
                  },
                  {
                    "label": "疑似当事人遭遇疾病",
                    "value": "F"
                  },
                  {
                    "label": "其他",
                    "value": "G"
                  }
                ]
                            },
                            "remark": {
                                "control": "textarea",
                                "span": 18,
                                "disabled": true,
                                "label": "备注"
                            }
                        }
                    ]
                }
            ]
        }
    }
}