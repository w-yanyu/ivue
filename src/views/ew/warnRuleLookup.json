{
    "module": "v_search_btn_datagrid",
    "layout": {
        
        "form": {
            "doRequest": {
                "url": "",
                "method": "POST",
                "params": {}
            },
            "controls": [],
            "btns":[]
        },
        "toolbar":{
            "btns":[]
        },

        "datagrid": {
            "isInitSearch": true,
            "doRequest": {
                "url": "/SUMP/riskController/riskCall",
                "method": "post",
                "params": {
                  "servicecode": "risk-management-service/risk-decision-rule/page-select",
                  "appId": "risk-management-service",
                  "ruleStatus"  :"Y",
                  "isList": true
                }
              },
    
            "thead": [
                {
                    "label": "序号",
                    "type": "index",
                    "align": "center"
                },
                {
                    "type": "string",
                    "label": "规则id",
                    "prop": "ruleId",
                    "align": "center"
                },
                {
                    "type": "string",
                    "label": "规则代码",
                    "align": "center",
                    "prop": "ruleCode"
                },
                {
                    "type": "string",
                    "label": "规则名称",
                    "prop": "ruleName",
                    "align": "center"
                },
                {
                    "label": "规则类型",
                    "prop": "ruleType",
                    "align": "center",
                    "type": "string",
                    "dict": {
                    "dictType": "PD_E_RULE_TYPE",
                    "dictKey": [
                        "PD_E_RULE_TYPE"
                    ],
                    "format": "label"
                    }
                },  
                {
                    "label": "操作",
                    "align": "center",
                    "btns": [
                        {
                            "type": "bringback",
