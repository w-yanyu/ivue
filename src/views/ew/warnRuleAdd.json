{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "scope": "formData",
            "initRequest": "${{params}}",
            "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "ew3166"
                }
            },
            "btns": [
                {
                    "type": "submit",
                    "label": "提交"
                }
            ],
            "controls": [
                {
                    "rule_name": {
                        "control": "text",
                        "label": "规则名称",
                        "span": 16,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            },
                            {
                                "max": 50,
                                "message": "最多可以输入 50 个字节"
                            }
                        ]
                    },
                    "rule_condition": {
                        "control": "textarea",
                        "label": "规则条件",
                        "resize": "both",
                        "span": 16,
                        "rows": 3,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            },
                            {
                                "max": 100,
                                "message": "最多可以输入 100 个字节"
                            }
                        ]
                    },
                    "guarantees_number": {
                        "control": "text",
                        "label": "担保增加次数大于等于(次)",
                        "span": 16,
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入"
                            },
                            {
                                "type": "number",
                                "message": "请输入数字"
                            },
                            {
                                "max": 2,
                                "message": "最多可以输入 2 位数字"
                            }
                        ]
                    },
                    "early_warning_frequency": {
                        "control": "select",
                        "label": "预警频率",
                        "span": 16,
                        "default": "",
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择"
                            }
                        ],
                        "dict": {
                            "dictType": "early_warning_frequency",
                            "dictKey": [
                                "early_warning_frequency"
                            ],   
                            "format": "label"
                        }
                    },
                    "early_warning_level": {
                        "control": "select",
                        "label": "预警级别",
                        "span": 16,
                        "default": "",
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择"
                            }
                        ],
                        "dict": {
                            "dictType": "early_warning_level",
                            "dictKey": [
                                "early_warning_level"
                            ],
                            
                            "format": "label"
                        }
                    },
                    "remark": {
                        "control": "textarea",
                        "label": "备注",
                        "resize": "both",
                        "span": 16,
                        "rows": 3,
                        "rules": [
                            {
                                "max": 200,
                                "message": "最多可以输入 200 个字节"
                            }
                        ]
                    },
                    
                    "effective_status": {
                        "control": "select",
                        "label": "启用状态",
                        "span": 16,
                        "default": "",
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择"
                            }
                        ],
                        "dict": {
                            "dictType": "effective_status",
                            "dictKey": [
                                "effective_status"
                            ],   
                            "format": "label"
                        }
                    }
                    
                }
            ]
        }
    }
}