{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "post",
                "params":{
                  "servicecode": "adjucl"
                }
            },
              "btns":[
                  {
                      "type":"submit",
                      "label":"提交"
                  }
              ],
            "controls": [
                {
                    "busino": {
                        "label": "产品号",
                        "control": "string",
                        "disabled":true,
                        "required":true,
                        "align":"center",
                        "value":"${{params.prodcd}}"
                    },
                    "custno":{
                        "control": "string",
                        "label": "客户号",
                        "align":"center",
                        "value":"${{params.custno}}",
                        "required":true,
                        "disabled":true
                    }
                },
                {
                    
                    "crdlno": {
                        "control": "string",
                        "label": "额度编号",
                        "disabled":true,
                        "required":true,
                        "value":"${{params.crdlno}}"
                    },
                    "crcycd": {
                        "control": "string",
                        "label": "币种",
                        "disabled":true,
                        "required":true,
                        "value":"${{params.crcycd}}"
                    }
                },
                {
                    "cllevl": {
                        "label": "额度级别",
                        "control": "select",
                        "required":true,
                        "value":"${{params.cllevl}}",
                        "dict": {
                            "dictType": "E_CLLEVL",
                            "dictKey": [
                                "E_CLLEVL"
                            ],
                            "format": "label",
                            "labelField": "dictName",
                            "valueField": "dictId"
                        },
                        "disabled":true
                    },
                    "crdlam": {
                        "control": "currency",
                        "label": "额度金额",
                        "required":true,
                        "value":"${{params.crdlam}}"
                    }
                },
                {
                    "status":{
                        "control": "select",
                        "label": "有效状态",
                        "align":"center",
                        "required":true,
                        "value":"${{params.status}}",
                        "dict": {
                            "dictType": "E_STATUS",
                            "dictKey": [
                              "E_STATUS"
                            ],
                          "format": "label",
                          "labelField":"dictName",
                          "valueField":"dictId"
                        }
                    },
                    "dateam":{
                        "control": "text",
                        "label": "期限",
                        "value":"${{params.dateam}}",
                        "required":true
                    }
                },
                {
                    "stardt": {
                        "control": "dateTimePicker",
                        "value":"${{params.stardt}}",
                        "label": "生效日期",
                        "required":true,
                        "disabled":true
                    },
                    "effedt": {
                        "control": "dateTimePicker",
                        "label": "失效日期",
                        "value":"${{params.effedt}}",
                        "required":true
                    }
                },
                {
                    "intrvl": {
                        "control": "text",
                        "label": "利率",
                        "value":"${{params.intrvl}}",
                        "required":false
                    },
                    "cardno": {
                        "control": "text",
                        "label": "交易卡号",
                        "value":"${{params.cardno}}",
                        "required":false
                    }
                }
            ]
        }
    }
}