{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "doRequest": {
                "url": "/SUMP/call/RPCCall",
                "method": "post",
                "params":{
                  "servicecode": "frzecl"
                }
            },
              "btns":[
                  {
                      "type":"submit",
                      "label":"提交"
                  }
              ],
            "controls": [
                {
                    "busino": {
                        "label": "产品号",
                        "control": "string",
                        "disabled":true,
                        "align":"center",
                        "value":"${{params.prodcd}}"
                    },
                    "custna": {
                        "control": "string",
                        "label": "客户名称",
                        "disabled":true,
                        "value":"${{params.custna}}"
                    }
                },
                {
                    
                    "crdlno": {
                        "control": "string",
                        "label": "额度编号",
                        "disabled":true,
                        "value":"${{params.crdlno}}"
                    },
                    "crcycd": {
                        "control": "string",
                        "label": "币种",
                        "disabled":true,
                        "value":"${{params.crcycd}}"
                    }
                },
                {
                    "cllevl": {
                        "label": "额度级别",
                        "control": "string",
                        "value":"${{params.cllevl}}",
                        "dict": {
                            "dictType": "E_CLLEVL",
                            "dictKey": [
                                "E_CLLEVL"
                            ],
                            "format": "label",
                            "labelField": "dictName",
                            "valueField": "dictId"
                        },
                        "disabled":true
                    },
                    "crdlam": {
                        "control": "string",
                        "label": "额度金额",
                        "value":"${{params.crdlam}}",
                        "disabled":true
                    }
                },
                {
                    "stardt": {
                        "control": "dateTimePicker",
                        "label": "生效日期",
                        "required":true
                    },
                    "effedt": {
                        "control": "dateTimePicker",
                        "label": "失效日期",
                        "controll":"dateTimePicker",
                        "required":true
                    }
                },
                {
                    "amount": {
                        "control": "currency",
                        "label": "冻结金额",
                        "required":true
                    },
                    "custno":{
                        "control": "hidden",
                        "label": "客户id",
                        "align":"center",
                        "value":"${{params.custno}}"
                    },
                    "status":{
                        "control": "hidden",
                        "label": "有效状态",
                        "align":"center",
                        "value":"${{params.status}}"
                    }
                }
            ]
        }
    }
}