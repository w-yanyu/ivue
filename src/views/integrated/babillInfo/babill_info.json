{
  "module": "v-realonlyForm-btn",
  "layout": {
    "form": {
      "scope": "templateScope",
      "initRequest":"${{params}}",
      "initDataOnce":true,

      "controls": [
        {
          "this_tm_bill_rdrd_num": {
            "control": "string",
            "label": "本次出票编号"
          },
          "par_amt": {
            "control": "string",
            "label": "票面金额"
          },
          "format":{
            "thousand": ",",
            "decimal": 3
          }
        },
        {
          "draft_amt": {
            "control": "string",
            "label": "汇票金额",
            "format":{
              "thousand": ",",
              "decimal": 3
            }
          },
          "prep_mny_amt": {
            "control": "string",
            "label": "备款金额",
            "format":{
              "thousand": ",",
              "decimal": 3
            }
          }

        },
        {

          "issue_dt": {
            "control": "string",
            "label": "签发日期"
          },
          "matu_dt": {
            "control": "string",
            "label": "到期日期"
          }
        },
        {
          "paye_cust_num": {
            "control": "string",
            "label": "收款人客户号"
          },
          "paye_acct_num": {
            "control": "string",
            "label": "收款人账号"
          }
        },
        {
          "paye_sub_acct_serl_num": {
            "control": "string",
            "label": "收款人账号子序号"
          },
          "paye_acct_nm": {
            "control": "string",
            "label": "收款人户名"
          }
        },
        {
          "rcev_mny_bank_num": {
            "control": "string",
            "label": "收款行行号"
          },
          "rcev_mny_bank_nm": {
            "control": "string",
            "label": "收款行行名"
          }
        },
        {
          "brr_acct_num": {
            "control": "string",
            "label": "持票人账号"
          },
          "brr_sub_acct_serl_num": {
            "control": "string",
            "label": "持票人账号子序号"
          }
        },
        {
          "brr_nm": {
            "control": "string",
            "label": "持票人名称"
          },
          "brr_open_acct_bank_num": {
            "control": "string",
            "label": "持票人开户行行号"
          }
        },
        {
          "brr_open_acct_bank_nm": {
            "control": "string",
            "label": "持票人开户行行名"
          },
          "adv_flg": {
            "control": "string",
            "label": "垫款标志",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "value-label"
            }
          }
        },
        {
          "adv_due_bill_num": {
            "control": "string",
            "label": "垫款借据编号"
          },
          "trfr_adv_amt": {
            "control": "string",
            "label": "转垫款金额"
          }
        },
        {
          "bill_ste": {
            "control": "string",
            "label": "票据状态",
            "dict": {
              "dictType": "LN_E_BANK_ACPT_BILL_STE",
              "dictKey": [
                "LN_E_BANK_ACPT_BILL_STE"
              ],
              "format": "value-label"
            }
          },
          "elec_bill_flg": {
            "control": "string",
            "label": "是否电子票",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "value-label"
            }
          }
        },
        {
          "acpt_dt": {
            "control": "string",
            "label": "承兑日期"
          },
          "loss_dt": {
            "control": "string",
            "label": "挂失日期"
          }
        },
        {
          "cncl_loss_dt": {
            "control": "string",
            "label": "解挂日期"
          },
          "unused_ret_dt": {
            "control": "string",
            "label": "未用退回日期"
          }
        }
      ]
    }
  }
}
