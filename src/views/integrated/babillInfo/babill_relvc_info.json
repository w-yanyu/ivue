{
  "module": "v-realonlyForm-btn",
  "layout": {
    "form": {
      "scope": "templateScope",
      "initRequest":"${{params}}",
      "controls": [
        {
          "bank_acpt_agrmt_num": {
            "control": "string",
            "label": "银承协议编号"
          },
          "this_tm_bill_rdrd_btch": {
            "control": "string",
            "label": "批次号"
          }
        },
        {
          "ccy_code_num": {
            "control": "string",
            "label": "货币代号",
            "dict": {
              "dictType": "MS_E_CURRENCY",
              "dictKey": [
                "MS_E_CURRENCY"
              ],
              "format": "value-label"
            }
          },
          "agrmt_totl_par_amt": {
            "control": "string",
            "label": "协议总票面金额",
            "format":{
              "thousand": ",",
              "decimal": 3
            }
          }

        },
        {

          "this_tm_bill_rdrd_totl_amt": {
            "control": "string",
            "label": "本次出票总金额",
            "format":{
              "thousand": ",",
              "decimal": 3
            }
          },
          "aval_amt": {
            "control": "string",
            "label": "可用金额",
            "format":{
              "thousand": ",",
              "decimal": 3
            }
          }
        },
        {
          "eff_dt": {
            "control": "string",
            "label": "生效日期"
          },
          "invld_dt": {
            "control": "string",
            "label": "失效日期"
          }
        },
        {
          "dawer_cust_num": {
            "control": "string",
            "label": "出票人客户号"
          },
          "dawer_acct_num": {
            "control": "string",
            "label": "出票人账号"
          }
        },
        {
          "dawer_sub_acct_serl_num": {
            "control": "string",
            "label": "出票人账号子序号"
          },
          "dawer_full_nm": {
            "control": "string",
            "label": "出票人全称"
          }
        },
        {
          "prep_mny_hang_up_flg": {
            "control": "string",
            "label": "备款是否允许挂账",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "value-label"
            }
          },
          "mrgn_acct_num": {
            "control": "string",
            "label": "保证金账号"
          }
        },
        {
          "mrgn_sub_acct_serl_num": {
            "control": "string",
            "label": "保证金账号子序号"
          },
          "mrgn_acct_nm": {
            "control": "string",
            "label": "保证金账号名称"
          }
        },
        {
          "full_amt_mrgn_flg": {
            "control": "string",
            "label": "是否全额保证金",
            "dict": {
              "dictType": "MS_E_YESORNO",
              "dictKey": [
                "MS_E_YESORNO"
              ],
              "format": "value-label"
            }
          },
          "mrgn_gntd_amt": {
            "control": "string",
            "label": "保证金担保金额",
            "format":{
              "thousand": ",",
              "decimal": 3
            }
          }
        }

      ]
    }
  }
}
