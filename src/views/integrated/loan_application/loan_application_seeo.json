{
  "module": "v-realonlyForm-btn",
  "layout": {
    "scope": "templateScope",
    "initDataOnce":true,
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ICMSLNTS0084"
        }
      },
      "controls": [
        {
          "thircustid": {
            "control": "string",
            "label": "第三方客户号",
            "value": "${{form.loanInFormation.thircustid}}",
            "disabled": true,
            "width": "160px"
          },
          "signpl": {
            "control": "string",
            "label": "证件签发地",
            "value": "${{form.loanInFormation.signpl}}",
            "disabled": true,
            "width": "160px"
          }
        },

        {
          "custna": {
            "control": "string",
            "label": "客户名称",
            "value": "${{params.custna}}",
            "disabled": true,
            "width": "160px"
          },
          "gender": {
            "control": "string",
            "label": "性别",
            "value": "${{form.loanInFormation.gender}}",
            "disabled": true,
            "width": "160px",
            "dict": {
              "dictType": "MS_E_GENDER",
              "dictKey": [
                "MS_E_GENDER"
              ],
              "format": "label"
            }
          }
        },


        {
          "idtype": {
            "control": "select",
            "label": "证件类型",
            "disabled": true,
            "value": "${{form.loanInFormation.idtype}}",
            "width": "160px",
            "dict": {
              "dictType": "E_IDTFTR",
              "dictKey": [
                "E_IDTFTR"
              ],
              "format": "label"
            }
          },
          "cardno": {
            "control": "string",
            "label": "证件号码",
            "value": "${{form.loanInFormation.cardno}}",
            "disabled": true,
            "width": "160px"
          }
        },

        {
          "cardeday": {
            "control": "string",
            "label": "证件到期日",
            "value": "${{form.loanInFormation.cardeday}}",
            "disabled": true,
            "width": "160px"
          }

        }
      ]
    }
  }
}
