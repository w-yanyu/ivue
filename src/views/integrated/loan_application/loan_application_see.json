{
  "module": "v-realonlyForm-btn",
  "layout": {
    "scope": "templateScope",
    "initDataOnce":true,
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ICMSLNTS0084"
        }
      },
      "controls": [
        {
          "applid": {
            "control": "string",
            "label": "申请单号",
            "value": "${{form.loanInFormation.applid}}",
            "disabled": true,
            "width": "160px"
          },
          "thirdid": {
            "control": "string",
            "label": "第三方申请单号",
            "value": "${{form.loanInFormation.thirdid}}",
            "disabled": true,
            "width": "160px"
          }
        },
        {
          "custid": {
            "control": "string",
            "label": "客户号",
            "disabled": true,
            "value": "${{form.loanInFormation.custid}}",
            "width": "160px"
          },
          "channel": {
            "control": "string",
            "label": "渠道号",
            "value": "${{params.channel}}",
            "disabled": true,
            "width": "160px"
          }
        },

        {
          "prodcd": {
            "control": "string",
            "label": "产品号",
            "value": "${{form.loanInFormation.prodcd}}",
            "disabled": true,
            "width": "160px"
          },
          "prodna": {
            "control": "string",
            "label": "产品名称",
            "value": "${{params.prodna}}",
            "disabled": true,
            "width": "160px"

          }
        },
        {
          "appuam": {
            "control": "string",
            "label": "放款金额",
            "value": "${{form.loanInFormation.appuam}}",
            "disabled": true,
            "width": "160px"

          },
          "ppvtnr": {
            "control": "string",
            "label": "贷款期限",
            "value": "${{form.loanInFormation.ppvtnr}}",
            "disabled": true,
            "width": "160px"

          }
        },
        {
          "schdtp": {
            "control": "string",
            "label": "还款方式",
            "value": "${{form.loanInFormation.schdtp}}",
            "disabled": true,
            "width": "160px",
            "dict": {
              "format": "label",
              "dictKey": [
                "E_SCHDTP"
              ],
              "dictType": "E_SCHDTP"
            }
          },
          "intrat": {
            "control": "string",
            "label": "利率",
            "value": "${{form.loanInFormation.intrat}}",
            "disabled": true,
            "width": "160px"

          }
        },

        {
          "loanus": {
            "control": "string",
            "label": "贷款用途",
            "value": "${{form.loanInFormation.loanus}}",
            "disabled": true,
            "width": "160px",
            "dict": {
              "dictType": "E_LOANYT",
              "dictKey": [
                  "E_LOANYT"
              ],
              "format": "label"
          }
          }

        }
      ]
    }
  }
}
