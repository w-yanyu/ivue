{
  "module": "v-realonlyForm-btn",
  "layout": {
    "scope": "templateScope",
    "initDataOnce":true,
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "ICMSLNTS0084",
          "applid": "${{params.applid}}"
        }
      },
      "controls": [
        {
          "corewh": {
            "control": "string",
            "label": "拒绝或失败原因",
            "value": "${{form.loanInFormation.corewh}}",
            "disabled": true,
            "width": "160px"
          }
        },

        {
          "corecode": {
            "control": "string",
            "label": "拒绝码",
            "value": "${{form.loanInFormation.corecode}}",
            "disabled": true,
            "width": "160px"
          }
        },
        
        {
          "refusecodein": {
            "control": "string",
            "label": "内部拒绝码",
            "disabled": true,               
            "value": "${{form.loanInFormation.refuseCodeIn}}",
            "width": "160px"
          }
        },

        {
          "refusewhyin": {
            "control": "string",
            "label": "内部拒绝原因",
            "value":"${{form.loanInFormation.refuseWhyIn}}",
            "disabled": true,
            "width": "160px"
          }
        }
        

      ]
    }
  }
}
