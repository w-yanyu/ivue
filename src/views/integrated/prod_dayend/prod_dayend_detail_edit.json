{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "scope": "templateScope",
            "initRequest":"${{params}}",
            "initDataOnce":true,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "post",
                "params": {
                  "servicecode": "updateproddayend"
                }
              },
              "btns":[
			],
            "controls": [
                {
                    "corpno": {
                        "label": "法人代码",
                        "control": "string",
                        "initShow": true,
                        "width": "130px"
                    },
                    "sprdid": {
                        "label": "可售产品ID",
                        "control": "string",
                        "width": "130px"
                    },
                    "prodcd": {
                        "label": "产品代码",
                        "control": "string",
                        "width": "130px"
                    },
                    "prodna": {
                        "label": "产品名称",
                        "control": "string",
                        "width": "130px",
                        "span": 12
                    },
                    "prod_date": {
                        "label": "产品日期",
                        "width": "130px",
                        "control": "string",
                        "span": 12
                    },
                    "tran_flow_id": {
                        "label": "批量交易流程ID",
                        "control": "string",
                        "width": "130px",
                        "span": 12
                    },
                    "tran_group_id": {
                        "label": "批量交易组ID",
                        "width": "130px",
                        "control": "string",
                        "span": 12
                    },
                    "tran_id": {
                        "label": "批量交易ID",
                        "control": "string",
                        "width": "130px",
                        "span": 12
                    },
                    "task_exe_mode": {
                        "label": "批量任务执行模式",
                        "control": "string",
                        "width": "130px",
                        "span": 12
                    },
                    "start_time": {
                        "label": "轮询开始时间",
                        "width": "130px",
                        "control": "timePicker",
                        "disabled": true,
                        "format": "HHmm",
                        "rules": [
                          {
                            "required": true,
                            "message": "请输入轮询开始时间"
                          }
                        ]
                    },
                    "end_time": {
                        "label": "轮询结束时间",
                        "width": "130px",
                        "control": "timePicker",
                        "disabled": true,
                        "format": "HHmm",
                        "rules": [
                          {
                            "required": true,
                            "message": "请输入轮询结束时间"
                          }
                        ]
                    }
                }
            ]
        }
    }
}
