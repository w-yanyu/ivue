{
    "module": "dynamicTabs",
    "layout": {
        "tabs": {
            "btns": [],
            "tabHeader": [
                {
                    "id": "baseInfo",
                    "label": "授信申请信息",
                    "closable": false,
                    "disabled": false,
                    "icon": "el-icon-date",
                    "toRequest": {
                      "url": "@CreditApplicationInfo",
                      "method": "get",
                      "params": {
                        "appl_id":"${{params.applid}}"
                      }
                    }
                },
                {
                    "id": "histor",
                    "label": "授信申请人信息",
                    "icon": "el-icon-date",
                    "closable": false,
                    "disabled": false,
                    "toRequest": {
                        "url": "/views/us/usManager/creditInformation/appt_list.json",
                        "method": "get"
                    }
                },
              {
                "id": "relation",
                "label": "申请人联系人信息",
                "icon": "el-icon-date",
                "closable": false,
                "disabled": false,
                "toRequest": {
                  "url": "/views/integrated/credit_tdapplication/user_contact_information_meun.json",
                  "method": "get",
                  "params": {
                    "custid":"${{params.custid}}"
                  }
                }
              },
              
              {
                "id": "view",
                "label": "征信报告信息",
                "icon": "el-icon-date",
                "closable": false,
                "disabled": false,
                "toRequest": {
                  "url": "@creditapplicationview",
                  "method": "get"
                }
              }
            ],
            "defaultActiveKey": "baseInfo",
            "position": "top",
            "styleClass": "card"
        }
    }
}
