{
    "module": "v_search_btn_datagrid",
    "layout": {
        "toolbar": {
            "btns": [
                
            ]
        },
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": true,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "IUMSCUTS1068",
                    "custid": "${{params.custid}}"
                }
            },
            "thead": [
                {
                    "type": "index",
                    "width": 50,
                    "label": "序号"
                },
                {
                    "label": "客户序号",
                    "prop": "custid",
                    "align": "center",
                    "disabled": true
                },
                {
                    "label": "联系人姓名",
                    "prop": "connam",
                    "align": "center"
                },
                {
                    "label": "联系人关系",
                    "prop": "conntp",
                    "align": "center",
                    "dict": {
                        "dictType": "E_CONNTP",
                        "dictKey": [
                            "E_CONNTP"
                        ],
                        "format": "label"
                    }
                },
                {
                    "label": "工作地址",
                    "prop": "addres",
                    "align": "center"
                },
                {
                    "label": "联系单位",
                    "prop": "conndw",
                    "align": "center"
                },
                {
                    "label": "联系单位电话",
                    "prop": "phonum",
                    "align": "center"
                },
                {
                    "label": "操作",
                    "fixed": "right",
                    "width": 160,
                    "align": "center",
                    "btns": [
                        {
                            "type": "look",
                            "label": "详情",
                            "toRequest": {
                                "url": "views/us/usManager/person/userContactInformation_details.json",
                                "method": "GET",
                                "custid": "${{params.custid}}"
                            },
                            "props": {
                                "width": "75%",
                                "title": "客户联系人信息详情"
                            }
                        }
                    ]
                }
            ]
        }
    }
}