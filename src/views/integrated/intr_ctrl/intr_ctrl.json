{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "foldLineNumber":2,
      "controls": [
        {
          "sprdid": {
            "span": 8,
            "label": "可售产品ID",
            "control": "text",
            "rules": [
              {
                "max": 18,
                "message": "长度不能超过18个字符"
              }
            ]
          },
          "prodcd": {
            "span": 8,
            "control": "text",
            "label": "产品代码",
            "rules": [
              {
                "max": 18,
                "message": "长度不能超过18个字符"
              }
            ]
          }
        },
        {
          "lncfno": {
            "span": 8,
            "control": "text",
            "label": "借据号",
            "rules": [
              {
                "max": 80,
                "message": "长度不能超过80个字符"
              }
            ]
          },
          "ctrldt": {
            "span": 8,
            "control": "dateTimePicker",
            "label": "管控日期",
            "format": "yyyyMMdd"
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "qrintrctrl"
        }
      },
      "thead": [
        {
          "label": "序号",
          "type" :  "index"
        },
        {
          "label": "贷款借据号",
          "prop": "lncfno"
        },{
          "label": "可售产品ID",
          "prop": "sprdid"
        },
        {
          "label": "产品代码",
          "prop": "prodcd"
        },
        {
          "label": "上次累计日期",
          "prop": "lastdt"
        },
        {
          "label": "上日累计汇总",
          "prop": "lastam"
        },
        {
          "label": "管控日期",
          "prop": "ctrldt"
        },
        {
          "label": "当日上限金额",
          "prop": "limtam"
        },
        {
          "label": "应收息费汇总",
          "prop": "infesm"
        },
        {
          "label": "减免利息",
          "prop": "derani"
        },
        {
          "label": "减免罚息",
          "prop": "derapi"
        },
        {
          "label": "减免复息",
          "prop": "deraci"
        },
        {
          "label": "减免费用",
          "prop": "derafe"
        }
      ]
    }
  }
}
