{
  "module": "v-realonlyForm-btn",
  "layout": {
    "scope": "templateScope",
    "initDataOnce":true,
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "qrcrde",
          "applid": "${{params.applid}}"
        }
      },
      "controls": [
        {
          "thircustid": {
            "control": "string",
            "label": "第三方客户号",
            "value": "${{form.crdApplicant.thircustid}}"
          },
          "custna": {
            "control": "string",
            "label": "客户名称",
            "value": "${{form.crdApplicant.custna}}"
          }
        },
        {
          "gender": {
            "control": "string",
            "label": "性别",
            "value": "${{form.crdApplicant.gender}}",
            "dict": {
              "dictType": "MS_E_GENDER",
              "dictKey": [
                "MS_E_GENDER"
              ],
              "format": "label"
            }
          },
          "idtype": {
            "control": "string",
            "label": "证件类型",
            "value": "${{form.crdApplicant.idtype}}",
            "dict": {
              "dictType": "E_IDTFTR",
              "dictKey": [
                "E_IDTFTR"
              ],
              "format": "label"
            }
          }
        },
        {
          "cardno": {
            "control": "string",
            "label": "证件号码",
            "value": "${{form.crdApplicant.cardno}}"
          },
          "cardeday": {
            "control": "string",
            "label": "证件到期日",
            "value": "${{form.crdApplicant.cardeday}}"
          }
        },
        {
          "signpl": {
          "control": "string",
          "label": "证件签发地",
          "value": "${{form.crdApplicant.signpl}}"
        },
          "mobino": {
            "control": "string",
            "label": "联系电话",
            "value": "${{form.crdApplicant.mobino}}"
          }
        },
        {
          "house_addr": {
            "control": "string",
            "label": "家庭住址",
            "value": "${{form.crdApplicant.house_addr}}"
          },
          "metier": {
            "control": "string",
            "label": "职业",
            "value": "${{form.crdApplicant.metier}}",
            "dict": {
              "dictType": "E_OCCUPA",
              "dictKey": [
                "E_OCCUPA"
              ],
              "format": "label"
            }
          }
        },
        {
          "emplna": {
            "control": "string",
            "label": "工作单位名称",
            "value": "${{form.crdApplicant.emplna}}"
          },
          "empadd": {
            "control": "string",
            "label": "工作单位地址",
            "value": "${{form.crdApplicant.empadd}}"
          }
        },
        {
          "emptel": {
            "control": "string",
            "label": "工作单位联系电话",
            "value": "${{form.crdApplicant.emptel}}"
          },
          "edulev": {
            "control": "string",
            "label": "学历",
            "value": "${{form.crdApplicant.edulev}}",
            "dict": {
              "dictType": "E_MAXEDU",
              "dictKey": [
                "E_MAXEDU"
              ],
              "format": "label"
            }
          }
        },
        {
          "degree": {
            "control": "string",
            "label": "学位",
            "value": "${{form.crdApplicant.degree}}",
            "dict": {
              "dictType": "E_MAXDEG",
              "dictKey": [
                "E_MAXDEG"
              ],
              "format": "label"
            }
          },
          "school": {
            "control": "string",
            "label": "学校",
            "value": "${{form.crdApplicant.school}}"
          }
        },
        {
          "mthinc": {
            "control": "string",
            "label": "月收入",
            "value": "${{form.crdApplicant.mthinc}}",
            "dict": {
              "dictType": "E_MONTHINCOME",
              "dictKey": [
                "E_MONTHINCOME"
              ],
              "format": "label"
            }
          }
        }
      ]
    }
  }
}
