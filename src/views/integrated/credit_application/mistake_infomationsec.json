{
  "module": "v-realonlyForm-btn",
  "layout": {
    "scope": "templateScope",
    "initDataOnce":true,
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "qrcrde",
          "applid": "${{params.applid}}"
        }
      },
      "controls": [
        {
          "corewh": {
            "control": "string",
            "label": "拒绝或失败原因",
            "value": "${{form.misInfomation.corewh}}",
            "disabled": true
          }
        },

        {
          "corecode": {
            "control": "string",
            "label": "拒绝码",
            "value": "${{form.misInfomation.corecode}}",
            "disabled": true
          }
        },


        {
          "refusecodein": {
            "control": "string",
            "label": "内部拒绝码",
            "disabled": true,               
            "value": "${{form.misInfomation.refuseCodeIn}}"
          }
        },

        {
          "refusewhyin": {
            "control": "string",
            "label": "内部拒绝原因",
            "value":"${{form.misInfomation.refuseWhyIn}}",
            "disabled": true
          }
        }
      ]
    }
  }
}
