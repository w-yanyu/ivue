{
  "module": "v-realonlyForm-btn",
  "layout": {
    "scope": "templateScope",
    "initDataOnce":true,
    "form": {
      "initRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "qrcrde",
          "applid": "${{params.applid}}"
        }
      },
      "controls": [
        {
          "applid": {
            "control": "string",
            "label": "申请单号",
            "value": "${{params.applid}}"
          },
          "thirdid": {
            "control": "string",
            "label": "第三方申请号",
            "value": "${{form.crdApplyInfo.thirdid}}"
          }
        },
        {
          "custid": {
            "control": "string",
            "label": "客户号",
            "value": "${{form.crdApplyInfo.custid}}"
          },
          "crdtcd": {
            "control": "string",
            "label": "申请产品号",
            "value": "${{form.crdApplyInfo.crdtcd}}"
          }
        },
        {

          "chancd": {
            "control": "string",
            "label": "申请渠道",
            "value": "${{form.crdApplyInfo.chancd}}"
          },
          "proname": {
            "control": "string",
            "label": "申请产品名称",
            "value": "${{form.crdApplyInfo.proname}}"
          }
        },
        {
          "ppvtnr": {
            "control": "string",
            "label": "申请期限",
            "value": "${{form.crdApplyInfo.ppvtnr}}"
          },
          "applam": {
            "control": "string",
            "label": "申请金额",
            "value": "${{form.crdApplyInfo.applam}}"
          }
        }

      ]
    }
  }
}
