{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "ofpacd": {
            "label": "合作方编号",
            "control": "text",
            "rules": [
              {
                "max": 18,
                "message": "长度不能超过18个字符"
              }
            ]
          },
          "ofpana": {
            "control": "text",
            "label": "合作方名称",
            "rules": [
              {
                "max": 18,
                "message": "长度不能超过18个字符"
              }
            ]
          }
        },{
          "stardt": {
            "control": "dateTimePicker",
            "label": "生效起始日期"
          },
          "expidt": {
            "control": "dateTimePicker",
            "label": "生效截止日期"
          }

        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "qropsl"
        }
      },
      "thead": [
        {
          "label": "序号",
          "type" :  "index",
          "align":"center"
        },
        {
          "label": "合作方编号",
          "prop": "ofpacd",
          "visible": "true",
          "align":"center"
        },
        {
          "label": "协议编号",
          "prop": "cortno",
          "align":"center"
        },
        {
          "label": "合作方名称",
          "prop": "ofpana",
          "align":"center"
        },
        {
          "label": "产品名称",
          "prop": "prodna",
          "align":"center"
        },
        {
          "label": "生效日期",
          "prop": "effedt",
          "align":"center"
        },
        {
          "label": "失效日期",
          "prop": "exprdt",
          "align":"center"
        },
        {
          "label": "业务规模(元)",
          "prop": "partsz",
          "align":"center",
          "format":{
            "thousand":","
          }
        },
        {
          "label": "已用额度(元)",
          "prop": "useamt",
          "align":"center",
          "format":{
            "thousand":","
          }
        },
        {
          "label": "剩余可用额度(元)",
          "prop": "availb",
          "align":"center",
          "format":{
            "thousand":","
          }
        },
        {
          "label": "合作方状态",
          "prop": "offsta",
          "align":"center",
          "dict": {
            "dictType": "E_OFFSTA",
            "dictKey": [
              "E_OFFSTA"
            ],
            "format": "label"
          }
        }


      ]
    }
  }
}
