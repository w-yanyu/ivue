{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "scope": "templateScope",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "qrkjfl",
          "lncfno": "${{params.lncfno}}",
          "acmode": "${{params.acmode}}",
          "sprdid": "${{params.sprdid}}"
        }
      },
      "thead": [
        {
          "label": "交易日期",
          "align":"center",

          "prop": "trandt"
        },
        {
              "align":"center",
              "label": "交易流水",
          "width":210,
          "prop": "transq"
        },
        {
              "align":"center",
              "label": "借贷标志",
          "prop": "amntcd",
          "dict": {
            "dictType": "E_AMNTCD",
            "dictKey": [
              "E_AMNTCD"
            ],
            "format": "label"
          }
        },
        {
              "align":"center",
              "label": "科目号",
          "prop": "gl_code"
        },
        {
              "align":"center",
              "label": "科目名称",
          "prop": "gl_code_desc"
        },
        {
              "align":"center",
              "label": "交易金额",
          "prop": "tranam"
        },
        {
              "align":"center",
              "label": "记账账号",
          "width":180,
          "prop": "cuacno"
        }
      ]
    }
  }
}
