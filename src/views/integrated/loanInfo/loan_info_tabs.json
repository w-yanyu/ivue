{
  "module": "dynamicTabs",
  "layout": {
    "tabs": {
      "tabHeader": [
        {
          "id": "tab1",
          "label": "借据详细信息",
          "closable": false,
          "disabled": false,
          "icon": "el-icon-date",
          "toRequest": {
            "url": "/views/integrated/loanInfo/loan_detail.json",
            "method": "post"
          }
        },
        {
          "id": "tab2",
          "label": "交易明细信息",
          "closable": false,
          "disabled": false,
          "icon": "el-icon-date",
          "toRequest": {
            "url": "/views/integrated/loanInfo/transaction_detail.json",
            "method": "post"
          }
        },
        {
          "id": "tab4",
          "label": "期供信息",
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "toRequest": {
            "url": "/views/integrated/loanInfo/loan_period_supply.json",
            "method": "post"
          }
        },
        {
          "id": "tab5",
          "label": "还款计划信息",
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "toRequest": {
            "url": "/views/integrated/loanInfo/repayment_schedule.json",
            "method": "post"
          }
        },
        {
          "id": "tab6",
          "label": "还款记录",
          "icon": "el-icon-date",
          "closable": false,
          "disabled": false,
          "toRequest": {
            "url": "/views/integrated/loanInfo/repament_history_list.json",
            "method": "post"
          }
        }
      ],
      "defaultActiveKey": "tab1",
      "position": "top",
      "styleClass": "card"
    }
  }
}

