          "prop": "ccy_code_num",
          "dict": {
            "dictType": "MS_CURRENCY",
            "dictKey": [
              "MS_CURRENCY"
            ],
            "format": "label"
          }
        },
        {
          "align": "center",
          "label": "归还应计贴息",
          "prop": "ret_acd_subsd_int"
        },
        {
          "align": "center",
          "label": "应计贴息",
          "prop": "acd_subsd_int"
        },
        {
          "align": "center",
          "label": "归还应收贴息",
          "prop": "ret_rcvbl_subsd_int"
        },
        {
          "align": "center",
          "label": "结转应收贴息",
          "prop": "carry_over_rcvbl_subsd_int"
        },
        {
          "align": "center",
          "label": "应收贴息",
          "prop": "rcvbl_subsd_int"
        }
      ]
    }
  }
}
