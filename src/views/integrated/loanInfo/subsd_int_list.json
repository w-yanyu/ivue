{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "foldLineNumber": 2,
      "controls": [
        {
          "loan_due_bill_num": {
            "span": 12,
            "control": "text",
            "label": "借据号"
          },
          "subsd_int_acct_num": {
            "control": "text",
            "span": 12,
            "label": "贴息账号",
            "format": "label"
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "syln0023",
          "outlistid": "list01"
        }
      },
      "thead": [
        {
          "label": "序号",
          "width": "50px",
          "type": "index",
          "align": "center"
        },
        {
          "align": "center",
          "label": "贴息账号",
          "prop": "subsd_int_acct_num"
        },{
          "align": "center",
          "label": "借据号",
          "prop": "loan_due_bill_num"
        },{
          "align": "center",
