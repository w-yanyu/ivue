{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form":{
      "initRequest": "${{params}}",
      "controls": [
          {
            "adv_rpymt_fine_num": {
              "span": 12,
              "control": "text",
              "label": "提前还款罚金编号"
            },
            "adv_rpymt_fine_nm": {
              "control": "text",
              "span": 12,
              "label": "提前还款罚金名称",
              "format": "label"
            }
          }
        ]
         
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": false,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "syln0020",
          "outlistid": "list01"
        }
      },
      "thead": [
        {
          "label": "操作",
          "align": "center",
          "fixed": "left",
          "btns": [
            {
              "type": "bringback",
              "label": "选择"
            }
          ]
        },
        {
          "align": "center",
          "label": "提前还款罚金编号",
          "prop": "adv_rpymt_fine_num",
          "btns": [
            {
              "type": "look",
              "toRequest": {
                "url": "/views/integrated/loanInfo/rpyment_fine_layer.json",
                "method": "POST",
                "params": {
                  "adv_rpymt_fine_num": "${{current.adv_rpymt_fine_num}}"
                }
              },
              "props": {
                "width": "80%",
                "title": "提前还款罚金分层信息"
              }
            }
          ]
        },
        {
          "align": "center",
          "label": "提前还款罚金名称",
          "width":220,
          "prop": "adv_rpymt_fine_nm"
        },
        {
          "align": "center",
          "label": "生效日期", 
          "prop": "eff_dt"
        },
        {
          "align": "center",
          "label": "失效日期",
          "prop": "invld_dt"
        },
        {
          "align": "center",
          "label": "最大罚金金额",
          "format": {
            "thousand": ","
          },
          "prop": "max_fine_amt"
        },
        {
          "align": "center",
          "label": "最小罚金金额",
          "format": {
            "thousand": ","
          },
          "prop": "min_fine_amt"
        },
        {
          "align": "center",
          "label": "罚金计算类型",
          "prop": "fine_caln_tp",
          "dict": {
            "dictType": "LN_E_FINE_CALN_TP",
            "dictKey": [
              "LN_E_FINE_CALN_TP"
            ],
            "format": "label"
          }
        },
        {
          "align": "center",
          "label": "利息天数",
          "prop": "int_dys"
        },
        {
          "align": "center",
          "label": "起始日期规则",
          "prop": "strt_dt_rule",
          "dict": {
            "dictType": "LN_E_STRT_DT_RULE",
            "dictKey": [
              "LN_E_STRT_DT_RULE"
            ],
            "format": "label"
          }
        },
        {
          "align": "center",
          "label": "固定金额",
          "format": {
            "thousand": ","
          },
          "prop": "fixd_amt"
        },
        {
          "align": "center",
          "label": "分层取值规则",
          "prop": "layer_val_rule",
          "dict": {
            "dictType": "LN_E_LAYER_VAL_RULE",
            "dictKey": [
              "LN_E_LAYER_VAL_RULE"
            ],
            "format": "label"
          }
        }
      ]
    }
  }
}