{
    "module": "v_form_btn_dynamic",
    "layout": {
        "form": {
            "controls": [
                {
                    "custno": {
                        "control": "text",
                        "label": "客户号",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入客户号"
                            },
                            {
                                "max": 24,
                                "message": "长度不能超过24个字符"
                            }
                        ]
                    },
                    "ordrid": {
                        "control": "text",
                        "label": "订单号",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入订单号"
                            },
                            {
                                "max": 30,
                                "message": "长度不能超过30个字符"
                            }
                        ]
                    }
                },
                {
                    "servtp": {
                        "control": "select",
                        "label": "渠道",
                        "dict": {
                            "dictType": "E_SERVTP",
                            "dictKey": [
                                "E_SERVTP"
                            ],
                            "format": "label"
                        },
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入渠道"
                            }
                        ]
                    },
                    "prodcd": {
                        "control": "text",
                        "label": "产品代码",
                        "rules": [
                            {
                                "max": 18,
                                "message": "长度不能超过18个字符"
                            }
                        ]
                    }
                },
                {
                    "lncfam": {
                        "control": "currency",
                        "label": "借据金额",
                        "decimal": "2",
                        "symbol": "￥",
                        "thousand": ",",
                        "rules": [
                            {
                                "max": 21,
                                "message": "长度不能超过21个字符"
                            }
                        ]
                    },
                    "termfm": {
                        "control": "text",
                        "label": "贷款期限",
                        "rules": [
                            {
                                "max": 6,
                                "message": "长度不能超过6个字符"
                            }
                        ]
                    }
                },
                {
                    "schdtp": {
                        "control": "select",
                        "label": "还款方式",
                        "dict": {
                            "dictType": "E_SCHDTP",
                            "dictKey": [
                                "E_SCHDTP"
                            ],
                            "format": "label"
                        }
                    }
                }
            ]
        },
        "toolbar": {
            "btns": [
                {
                    "type": "submit",
                    "label": "查询",
                    "unplain": true,
                    "icon": "",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "CORELNTS2001"
                        }
                    },
                    "toRequest": {
                        "url": "/views/integrated/repaymentPlan/repayment_plan_info_page.json",
                        "method": "POST"
                    }
                },
                {
                    "type": "reset",
                    "btnStyle": "success",
                    "icon": "",
                    "label": "重置"
                }
            ]
        }
    }
}