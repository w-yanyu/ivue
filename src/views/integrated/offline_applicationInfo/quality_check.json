{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "controls": [
              {
                "crdtcd": {
                  "control": "select",
                  "label": "申请产品",
                  "dict": {
                    "dictType": "E_OFFPROD",
                    "dictKey": [
                      "E_OFFPROD"
                    ],
                    "format": "label"
                  },
                  "rules": [
                    {
                      "required": true,
                      "message": "请选择产品编号"
                    }
                  ]
                },
                "stepCode": {
                  "control": "select",
                  "label": "流程步骤" ,
                  "dict": {
                    "dictType": "E_STEPCODE",
                    "dictKey": [
                      "E_STEPCODE"
                    ],
                    "format": "label"
                  }
                }
              },
              {
                "applid": {
                  "control": "text",
                  "label": "进件编号"
                },
                "custMgrName": {
                  "control": "text",
                  "label": "客户经理姓名"
                }
              },
              {
                  "idtfno": {
                    "control": "text",
                    "label": "身份证号",
                    "rules": [
                      {
                        "max": 20,
                        "message": "长度不能超过20个字符"
                      }
                    ]
                  },
                  "mobino": {
                    "control": "text",
                    "label": "手机号"
                  }
              },
              {
                "stardt": {
                  "control": "dateTimePicker",
                  "label": "进件起始日期",
                  "editable": true,
                  "format": "yyyyMMdd"
                },
                "expidt": {
                  "control": "dateTimePicker",
                  "label": "进件截止日期",
                  "editable": true,
                  "format": "yyyyMMdd"
                }
              },
              {
                "applst": {
                  "control": "select",
                  "label": "案件状态",
                  "dict": {
                    "dictType": "E_SXAPPLST",
                    "dictKey": [
                      "E_SXAPPLST"
                    ],
                    "format": "label"
                  }
                }
              }
            ]
        },
        "toolbar": {
            "btns": [
            ]
        },
        "datagrid": {
            "scope": "searchTable",
            "isInitSearch": false,
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "POST",
                "params": {
                    "servicecode": "qrOfApplyInfo"
                }
            },
            "thead": [
              {
                "label": "序号",
                "type": "index"
              },
              {
                "label": "进件编号",
                "prop": "applid",
                "align":"center",
                "visible": "true"
              },
              {
                "label": "姓名",
                "prop": "custna",
                "align":"center"
              },
              {
                "label": "申请产品",
                "prop": "prodna",
                "align":"center"
              },
              {
                "label": "身份证号",
                "prop": "idtfno",
                "align":"center"
              },
              {
                "label": "手机号",
                "prop": "mobino",
                "align":"center"
              },
              {
                "label": "申请金额(元)",
                "prop": "applam",
                "align":"center",
                "format":{
                  "thousand":","
                }
              },
              {
                "label": "最终审批额度(元)",
                "prop": "cdtamt",
                "align":"center",
                "format":{
                  "thousand":","
                }
              },
              {
                "label": "进件日期",
                "prop": "applDt",
                "align":"center"
              },
              {
                "label": "申请状态",
                "prop": "applst",
                "align":"center",

                "dict": {
                  "dictType": "E_SXAPPLST",
                  "dictKey": [
                    "E_SXAPPLST"
                  ],
                  "format": "label"
                }
              },
                {
                 "label": "详情",
                 "align":"center",
                 "width": 150,
                 "btns": [
                   {
                     "type": "look",
                     "label": "查看",
                     "toRequest": {
                       "url": "@QualityCheck",
                       "target":"navTab",
                       "method": "POST"
                     },
                     "props": {
                       "width": "100%",
                       "height": "100%",
                       "title": "申请信息详情"
                     }
                   },
                   {
                     "type": "edit",
                     "label": "流程跟踪",
                     "toRequest": {
                       "url": "@ProcessImgView",
                       "method": "GET",
                       "params": {
                         "from": "doneList"
                       }
                     },
                     "props": {
                       "width": "1200px",
                       "title": "流程跟踪图"
                     }
                   }
                  ]
                }
              ]
        }
    }
}
