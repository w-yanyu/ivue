{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "scope": "templateScope",
            "initRequest":"${{params}}",
            "initDataOnce":true,
            
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "post",
              "params":{
                "servicecode": "AS_PLMS_KG_COCURD",
                    "method": "edit"
              }
            },
            "btns":[
				{
					"type":"submit",
					"label":"提交"
				}
			],
            "controls": [
                {
                    "codeid": {
                        "label": "五级分类编号",
                        "control": "string",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入五级分类编号"
                            }
                        ]
                    },
                    "codesc": {
                        "label": "五级分类描述",
                        "control": "text",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入五级分类描述"
                            }
                        ]
                    },
                
                    "clevel": {
                        "control": "select",
                        "label": "五级分类级别",
                        "dict": {
                            "dictType": "E_CLEVEL1",
                            "dictKey": [
                                "E_CLEVEL1"
                            ],
                            "format": "label"
                        },
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择五级分类级别"
                            }
                        ]
                    }
                }
            ]
        }
    }
}