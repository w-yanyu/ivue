{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
           
            "initRequest":"${{params}}",
            "initDataOnce":true,
            
            "doRequest": {
              "url": "/SUMP/icmcall/icmRPCCall",
              "method": "post",
              "params":{
                "servicecode": "AS_PLMS_KG_RUCURD",
                    "method": "edit"
              }
            },
            "btns":[
				{
					"type":"submit",
					"label":"提交"
				}
			],
            "controls": [
                {
                    "groupi": {
                        "label": "组合编号",
                        "control": "string",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入组合编号"
                            }
                        ]
                    },
                    "groupd": {
                        "label": "五级组合描述",
                        "control": "text",
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入组合描述"
                            }
                        ]
                    },
                
                    "dbtype": {
                        "control": "select",
                        "label": "担保方式",
                        "dict": {
                            "dictType": "E_DBTYPE",
                            "dictKey": [
                                "E_DBTYPE"
                            ],
                            "format": "label"
                        },
                        "width": "130px",
                        "rules": [
                            {
                                "required": true,
                                "message": "请选择担保方式"
                            }
                        ]
                    }
                }
            ]
        }
    }
}