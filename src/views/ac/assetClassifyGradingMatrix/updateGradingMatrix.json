{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": "",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "AS_PLMS_KG_MACURD",
                            "method": "edit"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "matrix": {
                        "label": "矩阵id",
                        "control": "text",
                        "rules": [
                            {
                                "max": "5",
                                "message": "长度不能超过5个字符"
                            }
                        ]
                    },
                    "codeid": {
                        "label": "五级分类id",
                        "control": "remoteSelect",
                        "labelField": "codesc",
                        "valueField": "codeid",
                        "doRequest": {
                            "url": "/SUMP/icmcall/icmRPCCall",
                            "method": "POST",
                            "params": {
                                "servicecode": "AS_PLMS_KG_COCURD",
                                "method": "queryAll"
                            }
                        },
                        "format": "label"
                    }
                },
                {
                    "matmax": {
                        "label": "逾期天数上限",
                        "control": "text",
                        "placeholder": "最大天数填99999",
                        "rules": [
                            {
                                "max": "5",
                                "message": "长度不能超过5个字符"
                            }
                        ]
                    },
                    "matmin": {
                        "label": "逾期天数下限",
                        "control": "text",
                        "placeholder": "未到期输入-9999",
                        "rules": [
                            {
                                "max": "5",
                                "message": "长度不能超过5个字符"
                            }
                        ]
                    }
                },
                {
                    "groupi": {
                        "label": "组合id",
                        "control": "remoteSelect",
                        "labelField": "groupd",
                        "valueField": "groupi",
                        "doRequest": {
                            "url": "/SUMP/icmcall/icmRPCCall",
                            "method": "POST",
                            "params": {
                                "servicecode": "AS_PLMS_KG_RUCURD",
                                "method": "queryAll"
                            }
                        },
                        "format": "label",
                        "rules": [
                            {
                                "max": "5",
                                "message": "长度不能超过5个字符"
                            }
                        ]
                    }
                }
            ]
        }
    }
}
