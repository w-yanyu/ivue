{
    "module": "v_search_btn_datagrid",
    "layout": {
        "form": {
            "controls": [
                {
                    "loanid": {
                        "label": "借据编号",
                        "control": "text",
                        "rules": [
                            {
                                "max": 32,
                                "message": "长度不能超过32个字符"
                            }
                        ]
                    },
                    "kmonth": {
                        "label": "分类时间",
                        "control": "text",
                        "rules": [
                            {
                                "max": 20,
                                "message": "长度不能超过20个字符"
                            }
                        ]
                    },
                    "custna": {
                        "label": "客户名称",
                        "control": "text",
                        "rules": [
                            {
                                "max": 200,
                                "message": "长度不能超过200个字符"
                            }
                        ]
                    },
                    "certno": {
                        "label": "证件号码",
                        "control": "text",
                        "rules": [
                            {
                                "max": 50,
                                "message": "长度不能超过50个字符"
                            }
                        ]
                    }
                }
            ],
            "foldLineNumber": 3
        },
        "datagrid": {
            "isInitSearch": false,
            "scope": "searchTable",
            "doRequest": {
                "url": "/SUMP/icmcall/icmRPCCall",
                "method": "post",
                "params": {
                    "servicecode": "AS_PLMS_KG_QRZCFL"
                }
            },
            "thead": [
                {
                    "label": "序号",
                    "type": "index"
                },
                {
                    "label": "客户名称",
                    "prop": "custna"
                },
                {
                    "label": "证件号码",
                    "prop": "certno"
                },
                {
                    "label": "借据编号",
                    "prop": "loanid"
                },
                {
                    "label": "分类时间",
                    "prop": "kmonth"
                },
                {
                    "label": "分类标识",
                    "prop": "maaufl"
                },
                {
                    "label": "分类状态",
                    "prop": "lastre"
                },
                {
                    "label": "分类结果",
                    "prop": "firstr"
                },
                {
                    "label": "申请编号",
                    "prop": "applid"
                },
                {
                    "label": "操作",
                    "fixed": "right",
                    "width": 200,
                    "align":"center",
                    "btns": [
                        {
                            "type": "delete",
                            "label": "删除",
                            "doRequest": {
                                "url": "/SUMP/icmcall/icmRPCCall",
                                "method": "post",
                                "params": {
                                    "servicecode": "AS_ICMS_CP_DEMETH",
                                    "trantp":"DELETE"
                                    
                                }
                            }
                        }
                    ]
                }
            ],
            "pagination": {
                "show": true
            }
        }
    }
}