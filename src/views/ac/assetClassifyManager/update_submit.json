{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "initRequest": "${{params}}",
      "btns": [
        {
          "type": "submit",
          "label": "提交",
          "icon": "",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "PLMSKGTS0001"
            }
          }
        }
      ],
      "controls": [
        {
          "lncfno": {
            "control": "hidden",
            "label": "lncfno:",
            "visible": "false",
            "value": "${{params.lncfno}}"
          }
        },
        {
          "applid": {
            "control": "hidden",
            "label": "applid:",
            "visible": "false",
            "value": "${{params.applid}}"
          }
        },
        {
          "fltype": {
            "control": "select",
            "label": "五级分类调整类型:",
            "disabled": false,
            "value": "${{params.fltype}}",
            "options": [
              {
                "label": "影响征信",
                "value": "YX"
              },
              {
                "label": "不影响征信",
                "value": "BX"
              }
            ],
            "rules": [
              {
                "required": true,
                "message": "请输入五级分类调整类型"
              }
            ]
          }
        },
        {
          "rgzxgradcd": {
            "control": "select",
            "label": "人工五级分类:",
            "disabled": false,
            "value": "${{params.rgzxgradcd}}",
            "options": [
              {
                "label": "正常",
                "value": "PF"
              },
              {
                "label": "关注",
                "value": "SM"
              },
              {
                "label": "次级",
                "value": "SS"
              },
              {
                "label": "可疑",
                "value": "LO"
              },
              {
                "label": "损失",
                "value": "TL"
              }
            ],
            "rules": [
              {
                "required": true,
                "message": "请输入五级分类"
              }
            ]
          }
        },
        {
          "resion": {
            "control": "textarea",
            "value": "${{params.resion}}",
            "label": "调整原因:"
          }
        },
        {
          "memo": {
            "control": "textarea",
            "value": "${{params.memo}}",
            "label": "备注:",
            "disabled": false
          }
        }
      ]
    }
  }
}
