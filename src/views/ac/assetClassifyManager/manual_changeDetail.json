{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "lncfno": {
            "control": "text",
            "label": "借据号",
            "span": 8,
            "rules": [
              {
                "max": 80,
                "message": "长度不能超过80个字符"
              }
            ]
          },
          "prodna": {
            "control": "text",
            "label": "产品名称",
            "span": 8,
            "rules": [
              {
                "max": 30,
                "message": "长度不能超过30个字符"
              }
            ]
          },
          "graddtbeg": {
            "control": "dateTimePicker",
            "label": "开始日期:",
            "span": 8,
            "format": "yyyyMMdd"
          },
          "graddtend": {
            "control": "dateTimePicker",
            "label": "结束日期:",
            "span": 8,
            "format": "yyyyMMdd"
          },
          "maaufl": {
            "control": "select",
            "label": "五级分类类型",
            "span": 8,
            "dict": {
              "dictType": "E_FLTYPE",
              "dictKey": [
                "E_FLTYPE"
              ],
              "format": "label"
            }
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "PLMSKGTS0006"
        }
      },
      "thead": [
        {
          "label": "调整日期",
          "prop": "kmonth",
          "align": "center"
        },
        {
          "label": "调整经办人",
          "prop": "foundp",
          "align": "center"
        },
        {
          "label": "调整复核人",
          "prop": "editpe",
          "align": "center"
        },
        {
          "label": "借据号",
          "prop": "lncfno",
          "align": "center"
        },
        {
          "label": "产品名称",
          "prop": "prodna",
          "align": "center"
        },
        {
          "label": "逾期天数",
          "align": "center",
          "prop": "oddays"
        },
        {
          "label": "调整类别",
          "prop": "maaufl",
          "align": "center",
          "dict": {
            "dictType": "E_FLTYPE",
            "dictKey": [
              "E_FLTYPE"
            ],
            "format": "label"
          }
        },
        {
          "label": "自动调整前五级分类",
          "prop": "firstr",
          "align": "center",
          "dict": {
            "dictType": "E_CLEVEL1",
            "dictKey": [
              "E_CLEVEL1"
            ],
            "format": "label"
          }
        },
        {
          "label": "自动调整后五级分类",
          "prop": "endres",
          "align": "center",
          "dict": {
            "dictType": "E_CLEVEL1",
            "dictKey": [
              "E_CLEVEL1"
            ],
            "format": "label"
          }
        },
        {
          "label": "人工影响调整前五级分类",
          "prop": "yxfirstr",
          "align": "center",
          "dict": {
            "dictType": "E_CLEVEL1",
            "dictKey": [
              "E_CLEVEL1"
            ],
            "format": "label"
          }
        },
        {
          "label": "人工影响调整后五级分类",
          "prop": "yxendres",
          "align": "center",
          "dict": {
            "dictType": "E_CLEVEL1",
            "dictKey": [
              "E_CLEVEL1"
            ],
            "format": "label"
          }
        },
        {
          "label": "人工不影响调整前五级分类",
          "prop": "noyxfirstr",
          "align": "center",
          "dict": {
            "dictType": "E_CLEVEL1",
            "dictKey": [
              "E_CLEVEL1"
            ],
            "format": "label"
          }
        },
        {
          "label": "人工不影响调整后五级分类",
          "prop": "noyxendres",
          "align": "center",
          "dict": {
            "dictType": "E_CLEVEL1",
            "dictKey": [
              "E_CLEVEL1"
            ],
            "format": "label"
          }
        },
        {
          "label": "调整时贷余",
          "align": "center",
          "prop": "lendbl"
        },
        {
          "label": "调整原因",
          "align": "center",
          "prop": "resion"
        }
      ]
    }
  }
}