{
  "module": "v_search_btn_datagrid",
  "layout": {
    "form": {
      "controls": [
        {
          "lncfno": {
            "control": "text",
            "label": "借据号",
            "span": 12,
            "rules": [
              {
                "max": 80,
                "message": "长度不能超过80个字符"
              }
            ]
          },
          "graddtbeg": {
            "control": "dateTimePicker",
            "label": "开始日期:",
            "span":12,
            "format": "yyyyMMdd"
          }
        },
        {
          "graddtend": {
            "control": "dateTimePicker",
            "label": "结束日期:",
            "span": 12,
            "format": "yyyyMMdd"
          },
          "prodna": {
            "control": "text",
            "label": "产品名称",
            "span": 12,
            "rules": [
              {
                "max": 30,
                "message": "长度不能超过30个字符"
              }
            ]
          }
        }
      ]
    },
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "PLMSKGTS0005"
        }
      },
      "thead": [
        {
          "label": "借据号",
          "prop": "lncfno",
          "align": "center"
        },
        {
          "label": "产品名称",
          "prop": "prodna",
          "align": "center"
        },
        {
          "label": "贷款余额",
          "prop": "lnhhxbl",
          "align": "center"
        },
        {
          "label": "系统五级分类",
          "prop": "endres",
          "align": "center",
          "dict": {
            "dictType": "E_CLEVEL1",
            "dictKey": [
              "E_CLEVEL1"
            ],
            "format": "label"
          }
        },
        {
          "label": "系统五级分类日期",
          "prop": "graddt",
          "align": "center"
        },
        {
          "label": "人工五级分类(影响征信)",
          "prop": "rgzxgradcd",
          "align": "center",
          "dict": {
            "dictType": "E_CLEVEL1",
            "dictKey": [
              "E_CLEVEL1"
            ],
            "format": "label"
          }
        },
        {
          "label": "人工五级分类日期(影响征信)",
          "prop": "rgzxgraddt",
          "align": "center"
        },
        {
          "label": "人工五级分类(不影响征信)",
          "prop": "rggradcd",
          "align": "center",
          "dict": {
            "dictType": "E_CLEVEL1",
            "dictKey": [
              "E_CLEVEL1"
            ],
            "format": "label"
          }
        },
        {
          "label": "人工五级分类日期(不影响征信)",
          "prop": "rggraddt",
          "align": "center"
        },
        {
          "label": "逾期天数",
          "prop": "modsdy",
          "align": "center"
        }
      ]
    }
  }
}