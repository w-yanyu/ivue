{
    "module": "v_inputDataForm_btn",
    "layout": {
        "form": {
            "initRequest": "${{params}}",
            "btns": [
                {
                    "type": "submit",
                    "label": "提交",
                    "icon": "",
                    "doRequest": {
                        "url": "/SUMP/icmcall/icmRPCCall",
                        "method": "POST",
                        "params": {
                            "servicecode": "AS_ICMS_KG_RECURD",
                            "method":"edit"
                        }
                    }
                }
            ],
            "controls": [
                {
                    "ruleid": {
                        "label": "规则id",
                        "control": "text",
                        "width": "130px",
                        "disabled": true,
                        "rules": [
                            {
                                "max": "5",
                                "message": "长度不能超过5个字符"
                            },
                            {
                                "required": true,
                                "message": "请输入规则id"
                            }
                        ]
                    },
                    "rudesc": {
                        "label": "规则描述",
                        "width": "130px",
                        "control": "text",
                        "rules": [
                            {
                                "max": "30",
                                "message": "长度不能超过30个字符"
                            },
                            {
                                "required": true,
                                "message": "请输入规则描述"
                            }
                        ]
                    }
                },
                {
                    "rudate": {
                        "label": "分类日期",
                        "control": "dateTimePicker",
                        "width": "130px",
                        "rules": [
                            {
                                "max": "10",
                                "message": "长度不能超过10个字符"
                            },
                            {
                                "required": true,
                                "message": "请输入分类日期"
                            }
                        ]
                    },
                    "freque": {
                        "label": "手工分类频率",
                        "control": "select",
                        "width": "130px",
                        "dict": {
                            "format": "label",
                            "dictKey": [
                                "E_FREQUE"
                            ],
                            "dictType": "E_FREQUE"
                        }
                    }
                },
                {
                    "fachio": {
                        "label": "自动分类方式",
                        "control": "select",
                        "width": "130px",
                        "dict": {
                            "format": "label",
                            "dictKey": [
                                "E_FACHIO"
                            ],
                            "dictType": "E_FACHIO"
                        }
                    },
                    "flowpa": {
                        "label": "是否发起流程",
                        "control": "select",
                        "width": "130px",
                        "dict": {
                            "format": "label",
                            "dictKey": [
                                "MS_E_YESORNO"
                            ],
                            "dictType": "MS_E_YESORNO"
                        },
                        "events": {
                            "condition": {
                                "Y": {
                                    "control": {
                                        "pronumber": {
                                            "value": "KGRD_AU_2"
                                        }
                                    }
                                },
                                "N": {
                                    "control": {
                                        "pronumber": {
                                            "value": ""
                                        }
                                    }
                                }
                            } 
                        }
                    }
                },
                {
                    "pronumber": {
                        "label": "流程编号",
                        "control": "text",
                        "width": "130px",
                        "disabled": true,
                        "rules": [
                            {
                                "max": "20",
                                "message": "长度不能超过20个字符"
                            }
                        ]
                    }
                }  
            ]
        }
    }
}
