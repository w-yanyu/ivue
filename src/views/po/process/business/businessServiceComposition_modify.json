{
  "module": "v_inputDataForm_btn",
  "layout": {
    "form": {
      "scope": "formData",
      "initRequest":"${{params}}",
      "btns": [
        {
          "type": "submit",
          "label": "保存",
          "doRequest": {
            "url": "/SUMP/icmcall/icmRPCCall",
            "method": "POST",
            "params": {
              "servicecode": "upbuse",
              "servid": "${{params.servid}}"
            }
          }
        }
      ],
      "controls": [

        {
          "lncatp": {
            "label": "业务产品",
            "control": "select",
            "width": "130px",
            "span": 7,
            "dict": {
              "dictType": "E_LNCATP",
              "dictKey": [
                "E_LNCATP"
              ]

            },
            "rules": [
              {
                "required": true,
                "message": "请选择业务产品"
              }
            ]
          },
          "appltp": {
            "control": "select",
            "label": "申请类型",
            "width": "130px",
            "span": 7,
            "dict": {
              "dictType": "E_APPLTP",
              "dictKey": [
                "E_APPLTP"
              ]

            },
            "rules": [
              {
                "required": true,
                "message": "请选择申请类型"
              }
            ]
          },
          "applst": {
            "control": "text",
            "label": "流程状态",
            "width": "130px",
            "disabled": false,
            "span": 7,
            "rules": [
              {
                "required": true,
                "message": "请输入流程状态",
                "max": "10"
              }
            ]
          }
        },
        {

          "isable": {
            "control": "select",
            "label": "是否可用",
            "width": "130px",
            "span": 7,
            "dict": {
              "dictType": "E_SHIFOUBZ",
              "dictKey": [
                "E_SHIFOUBZ"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择是否可用"
              }
            ]
          },
          "suitwf": {
            "control": "text",
            "label": "适应流程",
            "width": "130px",
            "span": 14,
            "rules": [
              {
                "required": true,
                "max": "200",
                "message": "请输入适应流程"
              }
            ]
          }
        },
        {
          "call_busi_type": {
            "control": "select",
            "label": "业务方法调用类型",
            "width": "130px",
            "span": 7,
            "dict": {
              "dictType": "BPM_E_CALL_BUSI_TYPE",
              "dictKey": [
                "BPM_E_CALL_BUSI_TYPE"
              ],
              "format": "label"
            },
            "rules": [
              {
                "required": true,
                "message": "请选择业务方法调用类型"
              }
            ]
          },
          "servnm": {
            "control": "text",
            "label": "服务名称(绝对路径)",
            "width": "130px",
            "span": 7,
            "rules": [
              {
                "required": true,
                "message": "请输入服务名称(绝对路径)",
                "max": "200"
              }
            ]
          },
          "sortno": {
            "control": "text",
            "label": "排序序号",
            "width": "130px",
            "span": 7,
            "rules": [
              {
                "required": true,
                "message": "请输入排序序号",
                "max": "8"
              }
            ]
          },
          "remark": {
            "label": "备注",
            "control": "textarea",
            "width": "130px",
            "span":21,
            "autosize":{
              "minRows": 4
            },
            "rules": [
              {
                "required": true,
                "max": "200",
                "message": "请输入备注"
              }
            ]
          }
        }
      ]
    }
  }
}

