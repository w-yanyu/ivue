{
  "module": "v-inputDataForm-btn",
  "layout": {
    "form": {
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "bpm0050"
        }
      },
      "controls": [
        {
          "appr_role_id": {
            "control": "text",
            "label": "审批角色编号",
            "rules": [
              {
                "required": true,
                "message": "审批角色编号"
              }
            ]
          },
          "appr_role_name": {
            "control": "text",
            "label": "审批角色名称",
            "rules": [
              {
                "required": true,
                "message": "审批角色名称"
              }
            ]
          }
        },
        {
          "appr_role_desc": {
            "control": "text",
            "label": "审批角色描述"
          },
          "valid_flag": {
            "control": "select",
            "label": "是否有效",
            "options": [
              {
                "label": "有效",
                "value": "1"
              },
              {
                "label": "无效",
                "value": "0"
              }
            ]
          }
        },{
          "remark": {
            "control": "text",
            "label": "备注"
          }
        }

      ]
    }
  }
}
