{
  "module": "v_search_btn_datagrid",
  "layout": {
    "datagrid": {
      "scope": "searchTable",
      "isInitSearch": true,
      "doRequest": {
        "url": "/SUMP/icmcall/icmRPCCall",
        "method": "POST",
        "params": {
          "servicecode": "bpm0040"
        }
      },
      "thead": [
        {
          "label": "流程产品编号",
          "prop": "wfprod_cd"
        },
        {
          "label": "流程产品名称",
          "prop": "wfprod_name"
        },
        {
          "label": "流程定义名称",
          "prop": "proc_def_name"
        },
        {
          "label": "流程产品类型",
          "prop": "wfprod_tp",
          "dict": {
            "dictType": "BPM_E_WFPDTP",
            "dictKey": [
              "BPM_E_WFPDTP"
            ],
            "format": "label"
          }
        },
        {
          "label":"流程定义ID",
          "prop":"proc_def_id",
          "width": 120
        },
        {
          "label":"版本号",
          "prop":"ver",
          "width": 60
        },
        {
          "label": "操作",
          "width": "100px",
          "align":"center",
          "btns": [
          {
            "type": "edit",
            "label": "选择",
            "toRequest": {
                "url": "/views/po/process/products/product-manage-copy.json",
                "method": "post"
            },
            "props": {
                "title": "复制流程"
            }
          }
          ]
        }
      ]
    }
  }
}
